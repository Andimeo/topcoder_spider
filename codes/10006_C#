            using System;<br>using System.Collections.Generic;<br>using System.Text;<br><br>class BaronsAndCoins<br>{<br>    int calc(int dx, int dy, int last)<br>    {<br>        int m = last * dy + dy * (dy + 1) / 2;<br>        if (m &gt; dx)<br>            return 100000;<br>        return (dx - m + dy - 1) / dy + dy + last;<br>    }<br>    public int getMaximum(int[] x, int[] y)<br>  {<br>    int n = x.Length;<br>    for (int i = 0; i &lt; n; ++i)<br>      for (int j = i + 1; j &lt; n; ++j)<br>                if (y[j] &lt; y[i])<br>                {<br>                    int t = x[i];<br>                    x[i] = x[j];<br>                    x[j] = t;<br>                    t = y[i];<br>                    y[i] = y[j];<br>                    y[j] = t;<br>                }<br>        int[,] f = new int[n, n + 1];<br>        for (int i = 0; i &lt; n; i++)<br>            for (int j = 0; j &lt;= n; j++)<br>                f[i, j] = 100000;<br>        for (int i = 0; i &lt; n; i++)<br>            f[i, 1] = calc(x[i], y[i], 0);<br>        int res = 0;<br>        for (int i = 0; i &lt; n; i++)<br>            for (int j = 0; j &lt;= n; j++)<br>                if (f[i, j] &lt; 100000)<br>                {<br>                    for (int k = i + 1; k &lt; n; ++k)<br>                        if (x[i] &lt; x[k] &amp;&amp; y[i] &lt; y[k])<br>                        {<br>                            int t = calc(x[k] - x[i], y[k] - y[i], f[i, j]);<br>                            if (t &lt; f[k, j + 1])<br>                                f[k, j + 1] = t;<br>                        }<br>                    if (j &gt; res)<br>                        res = j;<br>                }<br>        return res;<br>  }<br>}<br></td>