            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class RoyalTreasurer<br>{<br>  public int minimalArrangement(int[] A, int[] B)<br>  {<br>    Arrays.sort(A);<br>    boolean b[]=new boolean[B.length];<br>    Arrays.fill(b,false);<br>    int i=0,j=0,max=0,ret=0,d=0;<br>    for(i=0;i&lt;A.length;i++)<br>    {<br>      max=0;<br>      d=0;<br>      for(j=0;j&lt;B.length;j++)<br>      {<br>        if(!b[j])<br>        {<br>          if(max&lt;B[j])<br>          {<br>            max=B[j];<br>            d=j;<br>          }  <br>        }<br>      }<br>      ret+=A[i]*max;<br>      b[d]=true;<br>    }<br>    return ret;      <br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>