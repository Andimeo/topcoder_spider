            // Cry as challenge phase is passing by<br><br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br><br>#include &lt;cmath&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br><br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br><br>using namespace std;<br><br>#define FORC(it,v) for( __typeof((v).begin()) it = (v).begin(); it != (v).end(); ++it )<br><br>bool eq( string a, string b )<br>{<br>    if( a.size() != b.size() ) return false;<br><br>    int cnt = 0;<br><br>    for( int i = 0; i &lt; ( int )a.size(); ++i )<br>        cnt += ( a[i] != b[i] );<br><br>    return cnt &lt; 2;<br>}<br><br>class StrangeComparator {<br>public:<br>    vector &lt;string&gt; compareString( vector &lt;string&gt; a, vector &lt;string&gt; b ) {<br>        vector&lt; string &gt; sol;<br><br>        for( int i = 0; i &lt; ( int )a.size(); ++i )<br>            sol.push_back( eq( a[i], b[i] ) ? "Yes" : "No" );<br><br>        return sol;<br>    }<br>};<br><br><br><br><br><br><br><br><br><br>// Powered by FileEdit<br>// Powered by moj 3.0 beta 11 [modified TZTester]<br>// Powered by CodeProcessor<br></td>