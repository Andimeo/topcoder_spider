            public class StrangeComparator {<br><br>    public String[] compareString(String[] a, String[] b) {<br>        int n = a.length;<br>        int i, nr;<br>        String[] ret = new String[n];<br>        for (i = 0; i &lt; n; i++) {<br>            nr = 0;<br>            if (a[i].length() == b[i].length()) {<br>                for (int j = 0; j &lt; a[i].length(); j++) {<br>                    if (a[i].charAt(j) != b[i].charAt(j)) {<br>                        nr++;<br>                    }<br>                }<br>                if (nr &gt; 1) {<br>                    ret[i] = "" + "No";<br>                } else {<br>                    ret[i] = "" + "Yes";<br>                }<br>            } else {<br>                ret[i] = "" + "No";<br>            }<br>        }<br>        return ret;<br>    }<br><br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by moj 4.11 [modified TZTester]<br>// Powered by CodeProcessor<br></td>