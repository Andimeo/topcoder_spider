            using System;<br><br>public class PrimeSoccer {<br>  public double getProbability(int skillOfTeamA, int skillOfTeamB) {<br>        return 1 - (1 - getP(skillOfTeamA / 100.0)) * (1 - getP(skillOfTeamB / 100.0));<br>  }<br><br>    double getP(double s)<br>    {<br>        int n = 90 / 5;<br>        double[] p = new double[n + 1];<br>        p[0] = 1;<br>        for (int i = 0; i &lt; n; ++i)<br>        {<br>            double[] np = new double[n + 1];<br>            for (int j = 0; j &lt; n; ++j) {<br>                np[j] += p[j] * (1 - s);<br>                np[j + 1] += p[j] * s;<br>            }<br>            p = np;<br>        }<br>        double res = 0;<br>        for (int i = 2; i &lt;= n; ++i)<br>        {<br>            bool ok = true;<br>            for (int j = 2; j &lt; i - 1; ++j)<br>                if (i % j == 0)<br>                    ok = false;<br>            if (ok)<br>                res += p[i];<br>        }<br>        return res;<br>    }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>