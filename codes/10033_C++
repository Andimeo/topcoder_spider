            #include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;queue&gt;<br>#include &lt;map&gt;<br>#include &lt;math.h&gt;<br>#include &lt;cmath&gt;<br>#include &lt;algorithm&gt;<br><br>using namespace std;<br><br>class PrimeSoccer<br>{<br>  public:<br>  double getProbability(int skillOfTeamA, int skillOfTeamB)<br>  {<br>    double a[19] = {0};<br>    double b[19] = {0};<br>    a[0] = b[0] = 1;<br>    for (int i=0; i&lt;18; i++) {<br>      for (int j=18; j&gt;=0; j--)<br>        a[j] = ((a[j-1])*skillOfTeamA + a[j]*(100-skillOfTeamA))/100.0;<br>      for (int j=18; j&gt;=0; j--)<br>        b[j] = ((b[j-1])*skillOfTeamB + b[j]*(100-skillOfTeamB))/100.0;<br>    }<br>    double res = 0.0;<br>    for (int i=0; i&lt;=18; i++) for (int j=0; j&lt;=18; j++)<br>    {<br>      int pp = 0;<br>      if (i==2 || i==3 || i==5 || i==7 || i==11 || i==13 || i==17) pp ++;<br>      if (j==2 || j==3 || j==5 || j==7 || j==11 || j==13 || j==17) pp ++;<br>      if (pp) res += a[i]*b[j];<br>    }<br>    return res;<br>  };<br>};<br></td>