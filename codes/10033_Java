            import static java.lang.Math.*;<br>import static java.math.BigInteger.*;<br>import static java.util.Arrays.*;<br>import static java.util.Collections.*;<br>import java.math.*;<br>import java.util.*;<br><br>public class PrimeSoccer {<br>  public double getProbability(int skillOfTeamA, int skillOfTeamB) {<br>    double p1 = prob(skillOfTeamA / 100.0);<br>    double p2 = prob(skillOfTeamB / 100.0);<br>    return p1 + p2 - p1 * p2;<br>  }<br>  double prob(double prob) {<br>    double[] dp = new double[19];<br>    dp[0] = 1;<br>    for (int i = 0; i &lt; 18; i++) {<br>      for (int j = 18; j &gt; 0; j--) {<br>        dp[j] = dp[j] * (1 - prob) + dp[j - 1] * prob;<br>      }<br>      dp[0] = dp[0] * (1 - prob);<br>    }<br>    double res = 0;<br>    for (int i = 0; i &lt; 19; i++) if (isPrime(i)) res += dp[i];<br>    return res;<br>  }<br>  boolean isPrime(int p) {<br>    if (p &lt;= 1) return false;<br>    for (int i = 2; i &lt; p; i++) if (p % i == 0) return false;<br>    return true;<br>  }<br>  void debug(Object...os) {<br>    System.err.println(Arrays.deepToString(os));<br>  }<br>}<br></td>