            public class BirthdayCake {<br>    public static int howManyFriends(String[] availableFruits, String[] friendsDislikings, int K) {<br>        int m = availableFruits.length, n = friendsDislikings.length;<br>        //System.out.println(m + " " + n);<br>        int p2 = 1;<br>        for (int i = 0; i &lt; n; i++) {<br>            p2 *= 2;<br>        }<br>        <br>        int[] an = new int[p2];<br>        <br>        boolean[][] li = new boolean[m][n];<br>        for (int i = 0; i &lt; m; i++) {<br>            for (int j = 0; j &lt; n; j++) {<br>                li[i][j] = true;<br>            }<br>        }<br>        <br>        for (int i = 0; i &lt; n; i++) {<br>            String[] k = friendsDislikings[i].split(" ");<br>            for (int j = 0; j &lt; k.length; j++) {<br>                for (int l = 0; l &lt; m; l++) {<br>                    if (k[j].equals(availableFruits[l])) {<br>                        li[l][i] = false;<br>                    }<br>                }<br>            }<br>        }<br>        <br>        <br>        /*for (int i = 0; i &lt; m; i++) {<br>            for (int j = 0; j &lt; n; j++) {<br>                System.out.print(li[i][j] + " ");<br>            }<br>            System.out.println();<br>        }*/<br>        <br>        int pp2, res;<br>        for (int i = 0; i &lt; m; i++) {<br>            pp2 = 1; res = 0;<br>            for (int j = 0; j &lt; n; j++) {<br>                if (!li[i][j]) {<br>                    res += pp2;<br>                }<br>                pp2 *= 2;<br>            }<br>            //System.out.println(res);<br>            for (int j = 0; j &lt; p2; j++) {<br>                if ((j ^ res) == (j + res)) {<br>                    an[j]++;<br>                }<br>            }<br>        }<br>        <br>        /*for (int j = 0; j &lt; p2; j++) {<br>            System.out.println(an[j]);<br>        }*/<br>        <br>        int ans = 0;<br>        for (int j = 0; j &lt; p2; j++) {<br>            if ((an[j] &gt;= K) &amp;&amp; (Integer.bitCount(j) &gt; ans)) {<br>                ans = Integer.bitCount(j);<br>            }<br>        }<br>        return ans;<br>    }<br>}<br></td>