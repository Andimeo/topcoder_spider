            class Teaching{<br>  public int count(string[] words, int K){<br>    int res = 0;<br>    int[] letters = new int[words.Length];<br>    for (int i = 0; i&lt; words.Length; ++i){<br>      letters[i] = 0;<br>      for (int j = 0; j&lt;words[i].Length; ++j)<br>        letters[i] = letters[i] | 1 &lt;&lt; (words[i][j] - 'a');<br>    }<br>    for (int T = 0; T&lt; 1 &lt;&lt; 26; ++T){<br>      if ((T &amp; (1 &lt;&lt; ('a' - 'a'))) == 0) continue;<br>      if ((T &amp; (1 &lt;&lt; ('n' - 'a'))) == 0) continue;<br>      if ((T &amp; (1 &lt;&lt; ('t' - 'a'))) == 0) continue;<br>      if ((T &amp; (1 &lt;&lt; ('a' - 'a'))) == 0) continue;<br>      <br>      if ((T &amp; (1 &lt;&lt; ('t' - 'a'))) == 0) continue;<br>      if ((T &amp; (1 &lt;&lt; ('i' - 'a'))) == 0) continue;<br>      if ((T &amp; (1 &lt;&lt; ('c' - 'a'))) == 0) continue;<br>      if ((T &amp; (1 &lt;&lt; ('a' - 'a'))) == 0) continue;<br>      <br>      int units = 0;<br>      for (int i=0;i&lt;26;i++)<br>        if ((T &amp; (1 &lt;&lt; i)) != 0) units++;<br>      if (units &gt; K) continue;<br>      <br>      int count = 0; <br>      for (int j = 0; j&lt;words.Length; ++j)<br>        if ((letters[j] | T) == T) count++;<br>      if (count &gt; res)<br>        res = count;<br>    }<br>    return res;<br>  }<br>}<br></td>