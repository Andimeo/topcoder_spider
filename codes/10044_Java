<td class="problemText" colspan="8" valign="middle" align="left">
            public class Teaching {<br>  public int count(String[] words, int K) {<br>    int ans = 0;<br>    if (K &lt; 5) return 0;<br>    K -= 5;<br>    <br>    String letters = "bdefghjklmopqrsuvwxyz";<br>    int[] mask = new int[words.length];<br>    <br>    for (int i = 0; i &lt; words.length; i++) {<br>      int bits = 0;<br>      for (int j = 0; j &lt; words[i].length(); j++) {<br>        char c = words[i].charAt(j);<br>        int k = letters.indexOf(c);<br>        if (k &gt;= 0) bits |= (1 &lt;&lt; k);<br>      }<br>      mask[i] = bits;<br>    }<br>    <br>    for (int bits = 0; bits &lt; 2097152; bits++) {<br>      if (count(bits) != K) continue;<br>      int total = 0;<br>      for (int i = 0; i &lt; words.length; i++) {<br>        if ((mask[i] &amp; bits) == mask[i]) total++;<br>      }<br>      if (total &gt; ans) ans = total;<br>    }<br>    return ans;<br>  }<br>  int count(int bits) {<br>    int total = 0;<br>    while (bits &gt; 0) {<br>      total += bits &amp; 1;<br>      bits &gt;&gt;= 1;<br>    }<br>    return total;<br>  }<br>}<br></td>