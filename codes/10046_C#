            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class LoveCalculator <br>{<br>    private int szl(string s1, string s2)<br>    {<br>        s1 += s2;<br>        int L, O, V, E;<br>        L = O = V = E = 0;<br>        for (int i = 0; i &lt; s1.Length; i++)<br>        {<br>            if (s1[i] == 'L')<br>                L++;<br>            else if (s1[i] == 'O')<br>                O++;<br>            else if (s1[i] == 'V')<br>                V++;<br>            else if (s1[i] == 'E')<br>                E++;<br>        }<br>        return ((L + O) * (L + V) * (L + E) * (O + V) * (O + E) * (V + E)) % 100;<br>    }<br>    public string findBoy(string girl, string[] boys) <br>  {<br>        Array.Sort(boys);<br>        //((L+O)*(L+V)*(L+E)*(O+V)*(O+E)*(V+E))%100<br>        int max = -1;<br>        int helye = -1;<br>        for (int i = 0; i &lt; boys.Length; i++)<br>        {<br>            if (szl(girl,boys[i])&gt;max)<br>            {<br>                max = szl(girl, boys[i]);<br>                helye = i;<br>            }<br>        }<br>        return boys[helye];<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by r1cs1]<br>// Powered by CodeProcessor<br></td>