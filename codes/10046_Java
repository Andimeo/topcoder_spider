            import java.util.Arrays;<br><br>public class LoveCalculator {<br>    public String findBoy(String girl, String[] boys) {<br>        Arrays.sort(boys);<br>        int max = Integer.MIN_VALUE;<br>        String minb = null;<br>        for (int i = 0; i &lt; boys.length; i++) {<br>            int score = calc(girl, boys[i]);<br>            if(score &gt; max){<br>                max = score;<br>                minb = boys[i];<br>            }<br>        }<br>        return minb;<br>    }<br><br>    int calc(String girl, String boy) {<br>        int L,O,V,E;<br>        L = count(girl,'L') + count(boy,'L');<br>        O = count(girl,'O') + count(boy,'O');<br>        V = count(girl,'V') + count(boy,'V');<br>        E = count(girl,'E') + count(boy,'E');<br>        return ((L+O)*(L+V)*(L+E)*(O+V)*(O+E)*(V+E))%100;<br>    }<br><br>    int count(String s, char c) {<br>        int count = 0;<br>        for (int i = 0; i &lt; s.length(); i++) {<br>            if(s.charAt(i) == c){<br>                count++;<br>            }<br>        }<br>        return count;<br>    }<br>}<br></td>