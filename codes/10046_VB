            Public Class LoveCalculator<br><br>    Public Function findBoy(ByVal girl As String, ByVal boys As String()) As String<br>        Dim i As Integer<br>        Dim j As Integer<br>        Dim result As String<br>        Dim mp As Double<br>        Dim mt As Double<br>        Dim f As String<br>        mp = -1<br><br>        Dim l As Integer<br>        Dim o As Integer<br>        Dim v As Integer<br>        Dim e As Integer<br><br>        Array.Sort(boys)<br>        result = boys(0)<br>        For i = 0 To boys.Length - 1<br>            l = 0<br>            o = 0<br>            v = 0<br>            e = 0<br>            f = boys(i) &amp; girl<br>            For j = 0 To f.Length - 1<br>                If (f(j) = "L") Then<br>                    l = l + 1<br>                End If<br>                If (f(j) = "O") Then<br>                    o = o + 1<br>                End If<br>                If (f(j) = "V") Then<br>                    v = v + 1<br>                End If<br>                If (f(j) = "E") Then<br>                    e = e + 1<br>                End If<br>            Next j<br>            mt = ((l + o) * (l + v) * (l + e) * (o + v) * (o + e) * (v + e)) Mod 100<br>            If (mt &gt; mp) Then<br>                mp = mt<br>                result = boys(i)<br>            End If<br>        Next i<br>        Return result<br>    End Function<br><br>End Class<br></td>