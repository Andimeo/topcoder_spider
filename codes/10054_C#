            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class Undo<br>{<br>  public string getText(string[] commands, int[] time)<br>  {<br>    int N = commands.Length;<br>    bool[] done = new bool[ N ];<br>    <br>    for ( int i = N - 1; i &gt;= 0; i-- ) {<br>      string cmd = commands[i];<br>      <br>      if ( !done[i] &amp;&amp; cmd[0] == 'u' ) {<br>        long k = long.Parse( cmd.Split( ' ' )[1] );<br>        <br>        for ( int j = i - 1; j &gt;= 0; j-- ) {<br>          if ( time[i] - time[j] &lt;= k ) {<br>            done[j] = true;<br>          }<br>        }<br>      }<br>    }<br>    <br>    string r = "";<br>    for ( int i = 0; i &lt; N; i++ ) {<br>      string cmd = commands[i];<br>      if ( !done[i] &amp;&amp; cmd[0] == 't' ) {<br>        char d = cmd.Split(' ')[1][0];<br>        <br>        r += d;<br>      }<br>    }<br>    return r;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>