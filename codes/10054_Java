            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class Undo<br>{<br>  public String getText(String[] c, int[] t)<br>  {<br>    int n = c.length;<br>    String[] res = new String[n + 1];<br>    res[0] = "";<br>    for (int i = 0; i &lt; n; ++i) {<br>      String[] tmp = c[i].split(" ");<br>      if (tmp[0].equals("type")) {<br>        res[i + 1] = res[i] + tmp[1];<br>      } else {<br>        int j = i - 1;<br>        int delta = Integer.parseInt(tmp[1]);<br>        while (j &gt;= 0 &amp;&amp; t[j] + delta &gt;= t[i]) {<br>          --j;<br>        }<br>        res[i + 1] = res[j + 1];<br>      }<br>    }<br>    return res[n];<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>