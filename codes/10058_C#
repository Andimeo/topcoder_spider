            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class YearProgressbar {<br>    public double percentage(string currentDate) {<br>    DateTime dt = DateTime.Parse(currentDate);<br>    DateTime start = new DateTime(dt.Year, 1, 1);<br>    DateTime end = new DateTime(dt.Year + 1, 1, 1);<br><br>    return 100.0 * (dt - start).Ticks / (end - start).Ticks;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>