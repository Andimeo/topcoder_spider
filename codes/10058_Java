            public class YearProgressbar {<br>  public static double percentage(String d) {<br>    <br>    int[] days = {31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31};<br>    int total = 365;<br>    <br>    int monthInt = 0;<br>    <br>    String[] sp = d.split("[ ,:]+");<br>    for (String s: sp) System.out.println(s);<br>    <br>    if (sp[0].equals("January")) {<br>      monthInt = 0;<br>    } else if (sp[0].equals("February")) {<br>      monthInt = 1;<br>    } else if (sp[0].equals("March")) {<br>      monthInt = 2;<br>    } else if (sp[0].equals("April")) {<br>      monthInt = 3;<br>    } else if (sp[0].equals("May")) {<br>      monthInt = 4;<br>    } else if (sp[0].equals("June")) {<br>      monthInt = 5;<br>    } else if (sp[0].equals("July")) {<br>      monthInt = 6;<br>    } else if (sp[0].equals("August")) {<br>      monthInt = 7;<br>    } else if (sp[0].equals("September")) {<br>      monthInt = 8;<br>    } else if (sp[0].equals("October")) {<br>      monthInt = 9;<br>    } else if (sp[0].equals("November")) {<br>      monthInt = 10;<br>    } else if (sp[0].equals("December")) {<br>      monthInt = 11;<br>    }<br>    <br>    int year = Integer.parseInt(sp[2]);<br>    <br>    if ((year % 400 == 0) || ((year % 4 == 0) &amp;&amp; (year % 100 != 0))) {<br>      System.out.println("Leap year!");<br>      days[1] = 29;<br>      total = 366;<br>    }<br>    <br>    double elapsed = 0;<br>    <br>    for (int i = 0; i &lt; monthInt; i++)<br>      elapsed += days[i];<br>    elapsed += Integer.parseInt(sp[1]) - 1;<br>    <br>    elapsed *= 24;<br>    elapsed += Integer.parseInt(sp[3]);<br>    elapsed *= 60;<br>    elapsed += Integer.parseInt(sp[4]);<br>    elapsed *= 100;<br>    <br>    double length = total * 24 * 60;<br>    <br>    return elapsed / length;<br>  }<br>}<br></td>