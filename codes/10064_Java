            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class LostSortingAlgorithm<br>{<br>  int n;<br>  int m;<br>  int ansk=0;<br>  int[] a=new int[100];<br>  int[] b=new int[100];<br>  int[] wk=new int[100];<br>  String[] ww=new String[100];<br>  String[] ff=new String[100];<br>  String[] w=new String[100];<br>  int[][] g=new int[256][256];<br>  String an="";<br>  void sort(int r){<br>    for(int i=0;i&lt;m;i++){<br>      g[(int)ww[i].charAt(r)][0]++;<br>      g[(int)ww[i].charAt(r)][g[(int)ww[i].charAt(r)][0]]=i;<br>    }<br>    int klg=0;<br>    for (int i=0;i&lt;256;i++){<br>      for (int j=1;j&lt;=g[i][0];j++){<br>        ff[klg]=ww[g[i][j]];<br>        klg++;<br>      }<br>    }<br>    for(int i=0;i&lt;m;i++){<br>      g[(int)ww[i].charAt(r)][g[(int)ww[i].charAt(r)][0]]=0;<br>      g[(int)ww[i].charAt(r)][0]--;<br>    }<br>    for(int i=0;i&lt;m;i++){<br>      ww[i]=ff[i];<br>    }<br>    return;<br>  }<br>  void bct(int p){<br>    if (p==n){<br>      for (int i=n-1;i&gt;=0;i--){<br>        sort(a[i]);    <br>      }<br>      boolean b=true;<br>      for (int i=0;i&lt;m;i++){<br>        if (ww[i].compareTo(w[i])!=0) b=false;<br>      }<br>      if (b==true){<br>        ansk++;<br>        an="";<br>        for (int i=0;i&lt;n;i++){<br>          an+=a[i];<br>        }  <br>      }<br>      return;<br>    }<br>    for(int i=0;i&lt;n;i++) if (b[i]==0){<br>      b[i]=1;<br>      a[p]=i;<br>      bct(p+1);<br>      b[i]=0;<br>    }<br>    return;<br>  }<br>  public String recoverSortingAlgorithm(String[] wx)<br>  {<br>    n=wx[0].length();<br>    m=wx.length;<br>    for (int i=0;i&lt;m;i++){<br>      w[i]=wx[i];<br>      ww[i]=w[i];<br>    }    <br>    Arrays.fill(a,0);<br>    Arrays.fill(b,0);<br>    bct(0);<br>    if (ansk==0) return("IMPOSSIBLE");<br>    if (ansk&gt;1) return("TOO MANY");<br>    return(an);<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>