            using System;<br><br>public class TwoLotteryGames {<br>    int count(int a)<br>    {<br>        int res = 0;<br>        while (a &gt; 0)<br>        {<br>            ++res;<br>            a = a &amp; (a - 1);<br>        }<br>        return res;<br>    }<br><br>  public double getHigherChanceGame(int n, int m, int k) {<br>        int p = 0;<br>        int q = 0;<br>        for (int s = 0; s &lt; (1 &lt;&lt; n) - 1; ++s)<br>            if (count(s) == m)<br>            {<br>                ++q;<br>                if (count(s &amp; ((1 &lt;&lt; m) - 1)) &gt;= k)<br>                    ++p;<br>            }<br>        return (double)p / q;<br>  }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>