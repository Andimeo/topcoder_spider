            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class TwoLotteryGames<br>{<br>  public double getHigherChanceGame(int n, int m, int k)<br>  {<br>    int total = 0;<br>    int good = 0;<br>    int pow=1;<br>    for (int i=1; i&lt;=n; i++) pow*=2;<br>    <br>    for (int mine=0; mine&lt;pow; mine++)<br>      {<br>        int[] a = new int[9];<br>        int m2=mine;<br>        int ac=0;<br>        while(m2!=0)<br>          {<br>            a[ac]=m2%2;<br>            m2/=2;<br>            ac++;<br>          }<br>        int mm=0;<br>        for (int w=0;w&lt;9;w++) if (a[w]==1) mm++;<br>        if (mm!=m) continue;<br>        <br>        for (int his=0; his&lt;pow; his++)<br>          {<br>            int[] b = new int[9];              <br>            m2=his;<br>            int bc=0;<br>            while(m2!=0)<br>              {<br>                b[bc]=m2%2;<br>                m2/=2;<br>                bc++;<br>              }<br>        int mm2=0;<br>        for (int w=0;w&lt;9;w++) if (b[w]==1) mm2++;<br>        if (mm2!=m) continue;<br>                              <br>          total++;<br>          int f=0;<br>          for (int w=0;w&lt;9;w++) if ((a[w]==1)&amp;&amp;(a[w]==b[w])) f++;  <br>          <br>          if (f&gt;=k) good++; <br> <br>          }          <br>      }<br>    <br>    return 0.0+((double)good/total);<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>