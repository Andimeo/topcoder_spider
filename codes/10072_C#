            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class MultiNumber<br>{<br>  public string check(int number)<br>  {<br>    string s = number.ToString();<br>    int n = s.Length;<br>    for (int i=0; i&lt;n-1; i++)<br>    {<br>      int p1 = 1;<br>      for (int j=0; j&lt;=i; j++)<br>        p1 *= int.Parse(s[j].ToString());<br>      int p2 = 1;<br>      for (int j=i+1; j&lt;n; j++)<br>        p2 *= int.Parse(s[j].ToString());  <br>      if (p1 == p2)<br>        return "YES";<br>    }<br>    return "NO";<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>