            import java.util.*;<br>import java.math.*;<br>import static java.lang.Math.*;<br>import static java.util.Arrays.*;<br>import static java.lang.Integer.*;<br> <br>public class MultiNumber<br>{<br>  public String check(int x)<br>  {<br>    String s = ""+x;<br>    for(int i=1;i&lt;s.length();++i){<br>      String a = s.substring(0, i);<br>      String b = s.substring(i);<br>      int t = 1, y = t;<br>      for(char c:a.toCharArray())  t*=(c-'0');<br>      for(char c:b.toCharArray())  y*=(c-'0');<br>      if(t==y)  return "YES";<br>    }<br>    return "NO";<br>  }<br>  <br>  <br>}<br> <br> <br> <br> <br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>