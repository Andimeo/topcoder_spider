            import java.util.*;<br>public class BarracksEasy {<br>    public int attack(int myUnits, int barHp, int unitsPerRound) {<br>        int res;<br>        int p[][][] = new int [51][51][105];<br>        int a[] = new int[300000];<br>        int b[] = new int[300000];<br>        int c[] = new int[300000];<br>        int cur = 0;<br>        for(int i = 0; i &lt;= myUnits; i ++) <br>          for(int j = 0; j &lt;= barHp; j ++)<br>            for(int v = 0; v &lt;= 2 * myUnits + 1; v ++)<br>              p[i][j][v] =  1000000;<br>        p[myUnits][barHp][0] = 0;<br>        a[0] = myUnits;<br>        b[0] = barHp;<br>        c[0] = 0;<br>        int zz = 1;<br>        while(zz &gt; cur) {<br>          int ii = a[cur];<br>          int jj = b[cur];<br>          int vv = c[cur];<br>          int pp = p[ii][jj][vv];<br>          for(int k = 0; k &lt;= ii; k ++) {<br>            int j = jj - k;<br>            int v = vv - ii + k;<br>            if(j &lt;= 0)j = 0;<br>            if(v &lt;= 0)v = 0;<br>            <br>            int i = ii - v;<br>            if(j  &gt; 0) {<br>              v += unitsPerRound;<br>            }<br>            if(i &gt; 0 &amp;&amp; p[i][j][v] &gt; pp + 1) {<br>              p[i][j][v] = pp + 1;<br>              a[zz] = i;<br>              b[zz] = j;<br>              c[zz] = v;<br>              zz ++;<br>            }<br>          }<br>          cur ++;<br>        }<br>        int xx = p[1][0][0];<br>        for(int x = 2; x &lt;= myUnits; x ++) {<br>          if(p[x][0][0] &lt; xx) xx = p[x][0][0];<br>        }<br>        if(xx &gt;= 1000000) return -1;<br>        return xx;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>