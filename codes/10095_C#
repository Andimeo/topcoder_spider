            using System;<br><br>public class CrazyBot {<br>    bool[,] v;<br>  public double getProbability(int n, int east, int west, int south, int north) {<br>        v = new bool[2 * n + 1, 2 * n + 1];<br>        return get(n, n, n, east / 100.0, west / 100.0, south / 100.0, north / 100.0); <br>  }<br><br>    private double get(int am, int x, int y, double e, double w, double s, double n)<br>    {<br>        if (am == 0)<br>            return 1.0;<br>        v[x, y] = true;<br>        double res = 0;<br>        if (!v[x + 1, y])<br>            res += e * get(am - 1, x + 1, y, e, w, s, n);<br>        if (!v[x - 1, y])<br>            res += w * get(am - 1, x - 1, y, e, w, s, n);<br>        if (!v[x, y + 1])<br>            res += s * get(am - 1, x, y + 1, e, w, s, n);<br>        if (!v[x, y - 1])<br>            res += n * get(am - 1, x, y - 1, e, w, s, n);<br>        v[x, y] = false;<br>        return res;<br>    }<br><br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>