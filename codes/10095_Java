            import static java.lang.Math.*;<br>import static java.math.BigInteger.*;<br>import static java.util.Arrays.*;<br>import static java.util.Collections.*;<br>import java.math.*;<br>import java.util.*;<br><br>public class CrazyBot {<br>  double east, west, south, north;<br>  public double getProbability(int n, int east, int west, int south, int north) {<br>    this.east = east / 100.0;<br>    this.west = west / 100.0;<br>    this.south = south / 100.0;<br>    this.north = north / 100.0;<br>    return rec(new boolean[100][100], 50, 50, 1, n);<br>  }<br>  double rec(boolean[][] used, int x, int y, double prob, int n) {<br>    if (used[x][y]) return 0;<br>    if (n == 0) return prob;<br>    double sum = 0;<br>    used[x][y] = true;<br>    sum += rec(used, x + 1, y, prob * east, n - 1);<br>    sum += rec(used, x - 1, y, prob * west, n - 1);<br>    sum += rec(used, x, y + 1, prob * north, n - 1);<br>    sum += rec(used, x, y - 1, prob * south, n - 1);<br>    used[x][y] = false;<br>    return sum;<br>  }<br>  void debug(Object...os) {<br>    System.err.println(Arrays.deepToString(os));<br>  }<br>}<br></td>