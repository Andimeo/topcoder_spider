            import java.util.*;<br> <br>public class EquilibriumPoints {<br>    public double[] getPoints(int[] x, int[] m) {<br>        int n = x.length;<br>        double[] res = new double[n - 1];<br>        for (int i = 0; i &lt; (n - 1); i++) {<br>            double l = x[i];<br>            double r = x[i + 1];<br>            double mm = (l + r) * 0.5;<br>            while (mm != l &amp;&amp; mm != r) {<br>                double f = 0;<br>                for (int j = 0; j &lt;= i; j++) {<br>                    f += m[j] / ((mm - x[j]) * (mm - x[j]));<br>                }<br>                for (int j = i + 1; j &lt; n; j++) {<br>                    f -= m[j] / ((mm - x[j]) * (mm - x[j]));<br>                }<br>                if (f &gt; 0) {<br>                    l = mm;<br>                } else {<br>                    r = mm;<br>                }<br>                mm = (l + r) * 0.5;<br>            }<br>            res[i] = l;<br>        }<br>        return res;<br>    }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>