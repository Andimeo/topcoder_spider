            #include &lt;stdio.h&gt;<br>#include &lt;stdlib.h&gt;<br>#include &lt;ctype.h&gt;<br>#include &lt;string.h&gt;<br>#include &lt;math.h&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;iostream&gt;<br>#include &lt;map&gt;<br>#include &lt;queue&gt;<br>#include &lt;set&gt;<br>#include &lt;sstream&gt;<br>using namespace std;<br><br>typedef vector&lt;int&gt; vi ;<br>typedef vector&lt; vi &gt; vii ;<br>typedef vector&lt;string&gt; vs ;<br>typedef vector&lt;double&gt; vd ;<br><br>#define min(a,b) a&lt;b?a:b<br>#define max(a,b) a&gt;b?a:b<br>#define fo(i,n) for(i=0;i&lt;(n);++i)<br>#define clr(a,b) memset(a,b,sizeof(a))<br>#define all(a) a.begin(),a.end()<br>#define sorta( n ) sort( all( n ) )<br><br>class InverseFactoring {<br>public:<br>int getTheNumber(vector &lt;int&gt; f)<br>{<br>    sorta( f );<br>    return f[0] * f[f.size() - 1];<br>}<br><br><br><br><br><br><br><br>};<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>