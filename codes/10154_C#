            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br> <br>public class LampsGrid<br>{<br>  public int mostLit (string[] initial, int K)<br>  {<br>    int r = initial.Length;<br>    int c = initial[0].Length;<br> <br>    int max = 0;<br>    <br>    for (int i = 0; i &lt; r; ++i)<br>    {<br>      int need = 0;<br>      for (int j = 0; j &lt; c; ++j)<br>        if (initial[i][j] == '0')<br>          ++need;<br> <br>      if (need &gt; K || (K - need) % 2 != 0)<br>        continue;<br> <br>      int mam = 0;<br>      for (int k = 0; k &lt; r; ++k)<br>        if (initial[i] == initial[k])<br>          ++mam;<br> <br>      max = Math.Max (max, mam);<br>    }<br> <br>    return max;<br>  }<br>  <br>  <br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>