            import java.util.*;<br> <br>public class LampsGrid {<br>  public int mostLit(String[] initial, int K) {<br>    int res = 0;<br>        for (int i = 0; i &lt; initial.length; ++i) {<br>            int am = 0;<br>            for (int j = 0; j &lt; initial[i].length(); ++j)<br>                if (initial[i].charAt(j) == '0')<br>                    ++am;<br>            if (am &gt; K || am % 2 != K % 2)<br>                continue;<br>            int cur = 0;<br>            for (String s : initial)<br>                if (s.equals(initial[i]))<br>                    ++cur;<br>            res = Math.max(res, cur);<br>        }<br>        return res;<br>  }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>