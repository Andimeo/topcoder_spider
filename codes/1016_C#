            using System;<br>using System.Collections;<br>using System.Text.RegularExpressions;                                                    <br> <br>public class Decimal<br>{<br>  int calc(int n)<br>  {<br>    int  l= 0, r = 1;<br>    while (l!= n+1)<br>    {<br>      r = (10 * r)%n;<br>      l++;<br>    }<br>    int c = r;<br>    r = (10 * r)%n;<br>    int k = 0;<br>    while (r!=c)<br>    {<br>      r= (10 * r)%n;<br>      k++;<br>    }<br>    return k+1;<br>  }<br>  public int[] find(int lower, int upper, int lowerLength, int upperLength)<br>  {<br>    ArrayList a = new ArrayList();<br>    for (int i=lower;i&lt;=upper;i++)<br>    {<br>      int k = calc(i);<br>      if (k&gt;=lowerLength&amp;&amp;k&lt;=upperLength) a.Add(i);<br>    }<br>    return (int[])a.ToArray(typeof(System.Int32));<br>  }<br>}<br></td>