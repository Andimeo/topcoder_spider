            #include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cmath&gt;<br>#include &lt;queue&gt;<br>using namespace std;<br>typedef vector&lt;int&gt; VI;  typedef vector&lt;vector&lt;int&gt; &gt; VVI;<br>typedef vector&lt;string&gt; VS;  typedef vector&lt;vector&lt;string&gt; &gt; VVS;<br>typedef signed long long i64;  typedef unsigned long long u64;<br><br>class Decimal {<br>public:<br>vector &lt;int&gt; find(int lower, int upper, int lowerLength, int upperLength) {<br>  int i, j, k, x, y, z, n;<br>  vector &lt;int&gt; ret;<br><br>  for( x = lower; x &lt;= upper; x++ ) {<br>    VI u(x, -1);<br>    y = (10*y)%x;<br>    for( i = 0, y = 1; u[y] == -1; i++ ) {<br>      u[y] = i;<br>      y = (10*y)%x;<br>    }<br>    z = i-u[y];<br>    if( x == 1 ) z = 1;<br>    if( z &gt;= lowerLength &amp;&amp; z &lt;= upperLength ) ret.push_back(x);<br>  }<br>  return ret;<br>}<br>};<br><br><br>// Powered by FileEdit<br></td>