            import java.util.*;<br>public class Decimal {<br>  public int[] find(int lower, int upper, int lowerLength, int upperLength) {<br>    int[] pos = new int[upper + 1];<br>    int[] result = new int[200];<br>    int ct = 0;<br>    for (int i = lower; i &lt;= upper; ++i) {<br>      int len = repLength(i, pos);<br>      if (len &gt;= lowerLength &amp;&amp; len &lt;= upperLength) {<br>        result[ct++] = i;<br>      }<br>    }<br>    int[] rr = new int[ct];<br>    System.arraycopy(result, 0, rr, 0, ct);<br>    return rr;<br>  }<br>  <br>  int repLength(int n, int[] pos)<br>  {<br>    for (int i = pos.length - 1; i &gt;= 0; --i) pos[i] = -1;<br>    <br>    int rem = 1;<br>    int ct = 0;<br>    while (pos[rem] &lt; 0) {<br>      pos[rem] = ct;<br>      rem = (10 * rem) % n;<br>      ++ct;<br>    }<br>    System.out.println("repLength(" + n + ") = " + (ct - pos[rem]));<br>    return ct - pos[rem];<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>