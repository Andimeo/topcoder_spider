<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br><br><br>public class MagicSpell<br>{<br>  public static string fixTheSpell(string spell)<br>  {<br>        Stack&lt;char&gt; c = new Stack&lt;char&gt;();<br>        foreach (char ch in spell)<br>            if (ch == 'A' || ch == 'Z') c.Push(ch);<br>        char[] chars = new char[spell.Length];<br>        for (int i = 0; i &lt; chars.Length; ++i)<br>            if (spell[i] == 'A' || spell[i] == 'Z')<br>                chars[i] = c.Pop();<br>            else<br>                chars[i] = spell[i];<br>        return new string(chars);<br>  }<br><br>}<br></td>