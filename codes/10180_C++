            #include&lt;ctime&gt;<br>#include&lt;cstdlib&gt;<br>#include&lt;iostream&gt;<br>#include&lt;algorithm&gt;<br>#include&lt;sstream&gt;<br>#include&lt;string&gt;<br>#include&lt;vector&gt;<br>#include&lt;cmath&gt;<br>using namespace std;<br><br>#define FOR(i,a,b) for(int i = (a); i &lt; (b); ++i)<br>#define REP(i,n) FOR(i,0,n)<br>#define FORE(it,x) for(typeof(x.begin()) it=x.begin();it!=x.end();++it)<br>#define pb push_back<br>#define all(x) (x).begin(),(x).end()<br>#define CLEAR(x,with) memset(x,with,sizeof(x))<br>#define sz size()<br>typedef long long ll;<br><br>struct TheLuckyString <br>{<br>  int count(string s) <br>  {<br>    int ret = 0;<br>    sort(all(s));<br>    do<br>    {<br>      bool ok = true;<br>      REP(i,s.sz-1)<br>        if(s[i] == s[i+1])<br>        {<br>          ok = false; break;<br>        }<br>      if(ok) ++ret;<br>    } while(next_permutation(all(s)));<br>    return ret;<br>  }<br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by moj 4.11 [modified TZTester]<br>// Powered by CodeProcessor<br></td>