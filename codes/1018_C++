            #include &lt;vector&gt;<br>#include &lt;string&gt;<br>using namespace std;<br>#define ll long long<br>#define vs vector&lt;string&gt;<br>#define vi vector&lt;int&gt;<br> <br>class Comment { public: vs strip(vs line) {<br>int i,j,m,n;<br>n=line.size();<br>string t;<br>vs ret;<br>for(i=0;i&lt;n;i++) ret.push_back(t);<br>int st=0;<br>for(i=0;i&lt;n;i++) {<br>j=0;<br>m=line[i].size();<br>for(;j&lt;m;j++) {<br>  if (st==0) { <br>    if (line[i][j]=='/' &amp;&amp; j+1&lt;m &amp;&amp; line[i][j+1]=='/') goto l1;<br>    if (line[i][j]=='/' &amp;&amp; j+1&lt;m &amp;&amp; line[i][j+1]=='*') { j++; st=1; continue; }<br>    if (line[i][j]=='"') { st=2; ret[i]+=line[i][j]; continue; }<br>    ret[i]+=line[i][j];<br>  } else if (st==1) {<br>    if (line[i][j]=='*' &amp;&amp; j+1&lt;m &amp;&amp; line[i][j+1]=='/') { j++; st=0; continue; }<br>  } else if (st==2) {<br>    if (line[i][j]=='\\' &amp;&amp; j+1&lt;m) { ret[i]+=line[i][j];    ret[i]+=line[i][j+1]; j++; continue; }<br>    if (line[i][j]=='\"') { st=0;     ret[i]+=line[i][j];continue; }<br>    ret[i]+=line[i][j];<br>  }<br>}<br>l1:;<br>}<br>vs ret2;<br>for(i=0;i&lt;n;i++) if (ret[i]!="") ret2.push_back(ret[i]);<br>return ret2;<br> <br>}};<br></td>