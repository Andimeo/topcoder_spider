            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>class OrderedNim<br>{<br>    public string winner(int[] layout)<br>    {<br>        bool alice = true;<br>        for (int i = layout.Length - 2; i &gt;= 0; --i)<br>        {<br>            if (layout[i] == 1)<br>            {<br>                alice = !alice;<br>            }<br>            else<br>            {<br>                alice = true;<br>            }<br>        }<br>        return alice ? "Alice" : "Bob";<br>    }<br>}<br></td>