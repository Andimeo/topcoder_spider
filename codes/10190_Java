            import static java.lang.Math.*;<br>import static java.math.BigInteger.*;<br>import static java.util.Arrays.*;<br>import static java.util.Collections.*;<br>import java.math.*;<br>import java.util.*;<br><br>public class OrderedNim {<br>  public String winner(int[] layout) {<br>    int n = layout.length;<br>    boolean win = false;<br>    for (int i = n - 1; i &gt;= 0; i--) {<br>      if (layout[i] &gt; 1) win = true;<br>      else win = !win;<br>    }<br>    return win ? "Alice" : "Bob";<br>  }<br>  void debug(Object...os) {<br>    System.err.println(deepToString(os));<br>  }<br>}<br></td>