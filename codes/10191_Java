            import static java.lang.Math.*;<br>import static java.math.BigInteger.*;<br>import static java.util.Arrays.*;<br>import static java.util.Collections.*;<br>import java.math.*;<br>import java.util.*;<br><br>public class SettingTents {<br>  public int countSites(int N, int M) {<br>    int n = max(N, M);<br>    int res = 0;<br>    for (int w = 1; w &lt;= n; w++) {<br>      for (int i = 0; i &lt; w; i++) {<br>        res += max(0, (N - w + 1)) * max(0, (M - w + 1));<br>        for (int k = 1; k * 2 &lt; w; k++) if ((w - 2 * i) * k % w == 0) {<br>          int x = max(max(i, w - i), w - k) - min(min(i, w - i), k);<br>          res += max(0, (N - x + 1)) * max(0, (M - w + 1));<br>          res += max(0, (N - w + 1)) * max(0, (M - x + 1));<br>        }<br>      }<br>    }<br>    return res;<br>  }<br>  void debug(Object...os) {<br>    System.err.println(deepToString(os));<br>  }<br>}<br></td>