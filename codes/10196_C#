            using System;<br><br>public class ShufflingMachine {<br>    public double stackDeck(int[] shuffle, int maxShuffles, int[] cardsReceived, int K) {<br>        int n = shuffle.Length;<br>        int[] p = new int[n];<br>        for (int i = 0; i &lt; n; ++i)<br>            p[i] = i;<br>        int[] cnt = new int[n];<br>        for (int i = 0; i &lt; maxShuffles; ++i)<br>        {<br>            int[] np = new int[n];<br>            for (int j = 0; j &lt; n; ++j)<br>                np[shuffle[j]] = p[j];<br>            p = np;<br>            foreach (int x in cardsReceived)<br>                ++cnt[p[x]];<br>        }<br>        Array.Sort(cnt);<br>        double res = 0;<br>        for (int i = 0; i &lt; K; ++i)<br>            res += cnt[n - 1 - i];<br>        return res / maxShuffles;<br>    }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>