            import java.util.*;<br> <br>public class ShufflingMachine {<br>    public double stackDeck(int[] shuffle, int maxShuffles, int[] cardsReceived, int K) {<br>        int[] a = shuffle;<br>        int n = a.length;<br>        int[] z = new int[n];<br>        for (int i = 1; i &lt;= maxShuffles; i++) {<br>            int[] b = new int[n];<br>            for (int j = 0; j &lt; n; j++) {<br>                b[j] = j;<br>            }<br>            for (int j = 0; j &lt; i; j++) {<br>                int[] nb = new int[n];<br>                for (int k = 0; k &lt; n; k++) {<br>                    nb[k] = a[b[k]];<br>                }<br>                b = nb;<br>            }<br>            int[] br = new int[n];<br>            for (int j = 0; j &lt; n; j++) {<br>                br[b[j]] = j;<br>            }<br>            for (int j = 0; j &lt; cardsReceived.length; j++) {<br>                z[br[cardsReceived[j]]]++;<br>            }<br>        }<br> <br>        Arrays.sort(z);<br>        double v = 0;<br>        for (int i = z.length - K; i &lt; z.length; i++) {<br>            v += z[i];<br>        }<br>        return v * 1.0 / maxShuffles;<br>    }<br> <br> <br>}<br> <br> <br>// Powered by PopsEdit<br> <br>// Powered by CodeProcessor<br></td>