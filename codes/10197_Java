            public class CommanderIdol{<br>  public static int getMaxPopularity(String[]roads){<br>    int N = roads.length;<br>    int[][] grid = new int[N][N];<br>    for (int i = 0;i &lt; N;i++)<br>      for (int j = 0;j &lt; N;j++)<br>        if (roads[i].charAt(j) == '-')<br>          grid[i][j] = 1000000;<br>        else<br>          grid[i][j] = roads[i].charAt(j) - '0';<br>    <br>    for (int k = 0;k &lt; N;k++)<br>      for (int i = 0;i &lt; N;i++)<br>        for (int j = 0;j &lt; N;j++)<br>          if (i != j &amp;&amp; j != k &amp;&amp; k != j)<br>            if (grid[i][k] + grid[k][j] &lt; grid[i][j]){<br>              grid[i][j] = grid[i][k] + grid[k][j];<br>              grid[j][i] = grid[i][j];<br>            }<br>            <br>    int max = 0;<br>    for (int i = 1;i &lt; N;i++){<br>      if (grid[0][i] &gt; max)<br>        max = grid[0][i];<br>      if (grid[0][i] &gt;= 1000000)<br>        return -1;<br>    }<br>    <br>    <br>    int pop = 0;<br>    for (int i = 1;i &lt; N;i++)<br>      pop += max - grid[0][i];<br>    return pop;  <br>  }<br>}<br></td>