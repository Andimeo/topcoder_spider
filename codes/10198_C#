            using System;<br>using System.Collections.Generic;<br>using System.IO;<br>using System.Text;<br><br>public class PaternityScandal<br>{<br>    public PaternityScandal()<br>    {<br>    }<br><br>    public int getFamily(string[] femaleGeneticTest, string[] maleGeneticTest, string[] personalityTest)<br>    {<br>        int f = femaleGeneticTest.Length;<br>        int m = maleGeneticTest.Length;<br>        int c = femaleGeneticTest[0].Length;<br>        int maxFamilyCount = 0;<br>        int familyCount = 0;<br><br>        for (int i = 0; i &lt; m; i++)<br>        {<br>            for (int j = 0; j &lt; f; j++)<br>            {<br>                if (personalityTest[i][j] == 'Y')<br>                {<br>                    familyCount = 0;<br>                    for (int k = 0; k &lt; c; k++)<br>                    {<br>                        if (maleGeneticTest[i][k] == 'Y' &amp;&amp; femaleGeneticTest[j][k] == 'Y')<br>                            familyCount++;<br>                    }<br>                    maxFamilyCount = Math.Max(maxFamilyCount, familyCount + 2);<br>                }<br>            }<br>        }<br><br>        return maxFamilyCount;<br>    }<br>}<br></td>