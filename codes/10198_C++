            // Cry as challenge phase is passing by<br><br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br><br>#include &lt;cmath&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br><br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br><br>using namespace std;<br><br>#define FORC(it,v) for( __typeof((v).begin()) it = (v).begin(); it != (v).end(); ++it )<br><br>class PaternityScandal<br>{ <br>public: <br>    int getFamily(vector &lt;string&gt; femaleGeneticTest, vector &lt;string&gt; maleGeneticTest, vector &lt;string&gt; personalityTest) <br>    { <br>        int male = maleGeneticTest.size();<br>        int fem = femaleGeneticTest.size();<br>        int ch = maleGeneticTest[0].size();<br><br>        int maks = 0;<br><br>        for( int a = 0; a &lt; male; ++a )<br>            for( int b = 0; b &lt; fem; ++b ) {<br>                if( personalityTest[a][b] != 'Y' ) continue;<br>                <br>                int cnt = 2;<br><br>                for( int i = 0; i &lt; ch; ++i )<br>                    if( maleGeneticTest[a][i] == 'Y' )<br>                        if( femaleGeneticTest[b][i] == 'Y' )<br>                            ++cnt;<br><br>                maks = max( maks, cnt );<br>            }<br><br>        return maks;<br>    }<br><br><br><br>// ------------<br>    <br> <br>}; <br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>