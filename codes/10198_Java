            public class PaternityScandal{<br>  public static int getFamily(String[] f,String[]m,String[]p){<br>    int best = 0;<br>    int count;<br>    for (int i = 0;i &lt; f.length;i++)<br>      for (int j = 0;j &lt; m.length;j++)<br>        if (p[j].charAt(i) == 'Y'){<br>          count = 2;<br>          for (int k = 0;k &lt; f[0].length();k++)<br>            if (f[i].charAt(k) == 'Y' &amp;&amp; m[j].charAt(k) == 'Y')<br>              count++;<br>          if (count &gt; best)<br>            best = count;<br>        }<br>    return best;<br>  }<br>}<br></td>