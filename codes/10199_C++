            // Cry as challenge phase is passing by<br><br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br><br>#include &lt;cmath&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br><br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br><br>using namespace std;<br><br>#define FORC(it,v) for( __typeof((v).begin()) it = (v).begin(); it != (v).end(); ++it )<br><br>class ThirtyRocks<br>{ <br>public: <br>    vector &lt;int&gt; assign(vector &lt;int&gt; boxMinimum, vector &lt;int&gt; boxMaximum) <br>    { <br>        vector&lt; int &gt; V = boxMinimum;<br>        int sum = 0; FORC( it, V ) sum += *it;<br><br>        if( sum &gt; 30 ) return vector&lt; int &gt;();<br><br>        for( int i = V.size() - 1; sum &lt; 30 &amp;&amp; i &gt;= 0; --i ) {<br>            while( V[i] &lt; boxMaximum[i] &amp;&amp; sum &lt; 30 ) {<br>                ++V[i];<br>                ++sum;<br>            }<br>        }<br><br>        if( sum != 30 ) return vector&lt; int &gt;();<br>        return V;<br>    }<br><br><br><br>// ------------<br>    <br> <br>}; <br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>