            public<br><br>class ThirtyRocks { <br><br>private int minTot=0; <br><br>private int maxTot=0; <br><br>private int numLeft=30; <br><br>public int[] flip(int[] arr){ <br><br>int[] ret=new int[arr.length]; <br><br>for(int c=0;c&lt;arr.length;c++){ <br>ret[c]=arr[arr.<br><br>length-(1+c)]; <br>}<br><br><br>return ret; <br>}<br><br><br>public int[] assign(int[] boxMinimum, int[] boxMaximum){ <br>boxMinimum=flip(boxMinimum);<br><br>boxMaximum=flip(boxMaximum);<br><br><br>int[] fin=new int[boxMinimum.length]; <br><br>for(int c=0;c&lt;boxMinimum.length;c++){ <br><br>minTot+=boxMinimum[c]; <br><br>maxTot+=boxMaximum[c]; <br>}<br><br><br>if(maxTot-minTot&lt;30-minTot||maxTot&lt;30||minTot&gt;30){ <br><br>return new int[0]; <br>}<br><br><br>for(int c=0;c&lt;boxMinimum.length;c++){ <br>fin[c]=boxMinimum[c];<br><br><br>numLeft-=fin[c]; <br>}<br><br><br>for(int c=0;c&lt;fin.length;c++){ <br><br>if(fin[c]&lt;boxMaximum[c]){ <br><br>int dif=boxMaximum[c]-fin[c]; <br><br>if(dif&lt;numLeft){ <br>fin[c]+=dif;<br><br><br>numLeft-=dif; <br>}<br><br>else{ <br>fin[c]+=<br><br>numLeft; <br><br>return flip(fin); <br>}<br><br>}<br><br>}<br><br><br>return flip(fin); <br>}<br><br>}<br></td>