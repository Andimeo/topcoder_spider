            #include&lt;iostream&gt;<br>#include&lt;cstdio&gt;<br>#include&lt;cctype&gt;<br>#include&lt;cmath&gt;<br>#include&lt;cstdlib&gt;<br>#include&lt;algorithm&gt;<br>#include&lt;vector&gt;<br>#include&lt;string&gt;<br>#include&lt;list&gt;<br>#include&lt;deque&gt;<br>#include&lt;map&gt;<br>#include&lt;set&gt;<br>#include&lt;queue&gt;<br>#include&lt;stack&gt;<br>#include&lt;utility&gt;<br>#include&lt;sstream&gt;<br>#include&lt;cstring&gt;<br>using namespace std;<br> <br>#define FOR(I,A,B) for(int I=(A);I&lt;=(B);I++)<br>#define REP(I,N) for(int I=0;I&lt;(N);I++)<br>#define ALL(X) (X).begin(),(X).end()<br>#define PB push_back<br>#define MP make_pair<br>#define ST first<br>#define ND second<br>typedef vector&lt;int&gt; VI;<br>typedef pair&lt;int,int&gt; PII;<br>typedef long long LL;<br>typedef vector&lt;string&gt; VS;<br>VS parse(string s)<br>{<br>  string a;<br>  VS wyn;<br>  REP(i,s.size())<br>  if (s[i]!=' ') a+=s[i];<br>  else if (!a.empty()) { wyn.PB(a); a=""; }<br>  if (!a.empty()) wyn.PB(a);<br>  return wyn;<br>}<br> <br>bool tr[55];<br>bool kn[55];<br> <br>class Telltale <br>{<br>  public:<br>  int doNotTellTheTruth(int n, vector &lt;int&gt; a, vector &lt;string&gt; parties){<br>    REP(i,a.size()) kn[a[i]]=1;<br>    bool md=!a.empty();<br>    while(md){<br>      md=false;<br>      REP(i,parties.size()){<br>        if(tr[i]) continue;<br>        VS cr=parse(parties[i]);<br>        REP(j,cr.size()){<br>          int a=0;<br>          REP(i,cr[j].length()) a=10*a+int(cr[j][i]-'0');<br>          if(kn[a]){<br>            tr[i]=1;<br>            break;<br>          }<br>        }<br>        if(tr[i]){<br>          REP(j,cr.size()){<br>            int a=0;<br>            REP(i,cr[j].length()) a=10*a+int(cr[j][i]-'0');<br>            kn[a]=1;<br>          }<br>          md=true;<br>        }<br>      }<br>    }<br>    int w=0;<br>    REP(i,parties.size()) if(!tr[i]) ++w;<br>    return w;<br>  }<br>};<br> <br> <br> <br>// Powered by FileEdit<br>// Powered by TZT