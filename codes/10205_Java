            import java.util.*;<br>public class Telltale {<br>  public int doNotTellTheTruth(int n, int [] a, String [] parties) {<br>    boolean [] cantell = new boolean [parties.length];<br>    for (int i = 0; i &lt; parties.length; i ++) {<br>      cantell [i] = true;<br>    }<br>    ArrayList &lt;Integer&gt; notell = new ArrayList &lt;Integer&gt; ();<br>    boolean [] beento = new boolean [n + 1];<br>    for (int i : a) {<br>      notell.add(i);<br>    }<br>    for (int x = 0; x &lt; notell.size(); x ++) {<br>      int i = notell.get(x);<br>      if (beento [i]) continue;<br>      for (int s = 0; s &lt; parties.length; s ++) {<br>        String [] buffer = parties[s].split(" ");<br>        int [] buff = new int [buffer.length];<br>        for (int j = 0; j &lt; buffer.length; j ++) {<br>          buff [j] = Integer.valueOf(buffer [j]).intValue();<br>        }<br>        boolean good = true;<br>        for (int t : buff) {<br>          if (t == i) {<br>            good = false;<br>          }<br>        }<br>        if (!good) {<br>          cantell [s] = false;<br>          for (int j : buff) {<br>            notell.add(j);<br>          }<br>          beento [i] = true;<br>        }<br>      }<br>    }<br>    int count = 0;<br>    for (boolean i : cantell) {<br>      if (i == true) {<br>        count ++;<br>      }<br>    }<br>    return count;<br>  }<br>}<br></td>