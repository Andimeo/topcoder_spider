            // Headers {{{<br>#include &lt;algorithm&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;iostream&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;queue&gt;<br>#include &lt;set&gt;<br>#include &lt;sstream&gt;<br>#include &lt;stack&gt;<br>#include &lt;string&gt;<br>#include &lt;utility&gt;<br>#include &lt;vector&gt;<br>using namespace std;<br>#define REP(i,n) for(int i=0; i&lt;(n); ++i)<br>#define debug(x) cerr &lt;&lt; #x &lt;&lt; " = " &lt;&lt; x &lt;&lt; '\n'<br>template&lt;typename Q&gt; inline int size(Q a) { return a.size(); }<br>// }}}<br> <br>struct match {<br>  int A,B,x,y;<br>  match() {}<br>  match(int _A,int _B,int _x,int _y):A(_A),B(_B),x(_x),y(_y) {}<br>};<br> <br>map&lt;string,int&gt; m;<br>int n;<br> <br>int score[100];<br> <br>int calc(vector&lt;match&gt; v) {<br>  REP(i,n) score[i]=0;<br>  REP(i,size(v)) {<br>    if(v[i].x&gt;v[i].y) score[v[i].A]+=3;<br>    if(v[i].x&lt;v[i].y) score[v[i].B]+=3;<br>    if(v[i].x==v[i].y) score[v[i].A]++,score[v[i].B]++;<br>  }<br>  int res,mx=-1;<br>  REP(i,n) if(score[i]==mx) res=-1; else if(score[i]&gt;mx) { mx=score[i]; res=i; }<br>  return res;<br>}<br> <br>class SoccerFan {<br>  public:<br>  int whereIsMistake(vector &lt;string&gt; results, string winner) {<br>    vector&lt;match&gt; v;<br>    m.clear();<br>    n=0;<br>    REP(i,size(results)) {<br>      char a[50],b[50];<br>      int x,y;<br>      sscanf(results[i].c_str(),"%s %s %d:%d",a,b,&amp;x,&amp;y);<br>      if(m.find(a)==m.end()) m[a]=n++;<br>      if(m.find(b)==m.end()) m[b]=n++;<br>      v.push_back(match(m[a],m[b],x,y));<br>    }<br>    if(m.find(winner)==m.end()) return -1;<br>    if(calc(v)==m[winner]) return -2;<br>    REP(i,size(v)) {<br>      int _x=v[i].x,_y=v[i].y;<br>      v[i].x=0,v[i].y=1;<br>      if(calc(v)==m[winner]) return i;<br>      v[i].x=0,v[i].y=0;<br>      if(calc(v)==m[winner]) return i;<br>      v[i].x=1,v[i].y=0;<br>      if(calc(v)==m[winner]) return i;<br>      v[i].x=_x; v[i].y=_y;<br>    }<br>    return -1;<br>  }<br>  <br> <br>};<br> <br> <br> <br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>