            import java.util.*;<br><br>public class SimpleCardGame {<br><br>  public int whoIsTheWinner(int[] points) {<br>    int ret = -1;<br>    int temp = 0;<br>    int c = 0;<br>    int x = 0;<br>    for (int i = 0; i &lt; points.length; i++) {<br>      if (points[i] &gt; temp &amp;&amp; points[i] &lt; 22) {<br>        temp = points[i];<br>        ret = i;<br>      }<br>    }<br>    for (int i = 0; i &lt; points.length; i++) {<br>      if (points[i] == temp) x++;<br>    }<br>    if (x &gt; 1) return -1;<br>    return ret;<br>  }<br><br>}<br></td>