            using System;<br> <br>public class TheBeauty {<br>  public int find(int n) {<br>        bool[] was = new bool[10];<br>        while (n != 0)<br>        {<br>            int r = n % 10;<br>            was[r] = true;<br>            n /= 10;<br>        }<br>        int cc = 0;<br>        for (int i = 0; i &lt; 10; i++)<br>        {<br>            if (was[i]) cc++;<br>        }<br>        return cc;<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>