            using System;<br>using System.Collections.Generic; <br><br>public class SuperSum {<br>    private long md = 1000000007;<br><br>    long inv(long k)<br>    {<br><br>        return pw(k, md - 2);<br>    }<br><br>    long pw(long n,long p)<br>    {<br>        if (p==0)<br>        {<br>            return 1;<br>        }<br>        long t1 = pw(n, p/2);<br>        t1 = (t1*t1)%md;<br>        if (p%2==1)<br>        {<br>            t1 = (t1*n)%md;<br>        }<br>        return t1;<br>    }<br><br>  public int calculate(int k, int n) {<br>      long res = 1;<br>      for (int i = 0; i &lt; k+1; i++)<br>      {<br>          res = (res*(n + k - i))%md;<br>          res = (res*inv(i + 1))%md;<br>      }<br>      return (int)res;<br><br>  }<br>  <br><br>}<br><br><br><br>// Powered by FileEdit<br></td>