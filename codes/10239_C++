            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;queue&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>const int mod=1000000007;<br> <br>long long power(int n,int m)<br>{<br>  long long res=1;<br>  for (;m;m/=2)<br>  {<br>    if (m&amp;1) res=res*n%mod;<br>    n=(long long)n*n%mod;<br>  }<br>  return res;<br>}<br> <br>class SuperSum {<br>public:<br>  int calculate(int k, int n) <br>  {<br>    long long res=1;<br>    for (int i=n+k;i&gt;=n;i--)<br>      res=res*i%mod;<br>    cout &lt;&lt; res &lt;&lt; endl;<br>    <br>    for (int i=1;i&lt;=k+1;i++)<br>      res=res*power(i,mod-2)%mod;<br>    return (int)res;<br> <br>  }<br>};<br> <br> <br> <br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>