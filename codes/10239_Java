            import java.math.BigInteger;<br><br>public class SuperSum {<br>  public int calculate(int k, int n) {<br><br>        BigInteger x = BigInteger.ONE;<br><br>        for (int i = 0; i &lt;= k; i++) {<br>            x = x.multiply(BigInteger.valueOf(n + i));<br>        }<br>        for (int i = 0; i &lt;= k; i++) {<br>            x = x.divide(BigInteger.valueOf(i + 1));<br>        }<br><br>        return x.mod(new BigInteger("1000000007")).intValue();<br>    }<br>}<br><br><br>// Powered by FileEdit<br>// Powered by moj 4.11 [modified TZTester]<br>// Powered by CodeProcessor<br></td>