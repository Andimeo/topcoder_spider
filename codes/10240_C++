            #include &lt;sstream&gt;<br>#include &lt;queue&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cmath&gt;<br>#include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br><br>using namespace std;<br><br>class ShorterSuperSum<br>{<br>public:<br>  int calculate(int k, int n)<br>  {<br>      int i,j,a[100][19],sum;<br>      memset(a,0,sizeof(a));<br><br>      for(i=1;i&lt;=n;i++)<br>            a[0][i] = i;<br><br>        for(i=1;i&lt;=k;i++)<br>        {<br>            sum = 0;<br>            for(j=1;j&lt;=n;j++)<br>            {<br>                sum += a[i-1][j];<br>                a[i][j] = sum;<br>            }<br>        }<br><br>        return a[k][n];<br>  }<br>};<br></td>