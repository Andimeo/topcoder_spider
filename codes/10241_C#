            using System;<br><br>public class SequenceSums {<br>  public int[] sequence(int _N, int _L) {<br>        long n = _N;<br>        for (long l = _L; l &lt;= 100; ++l)<br>        {<br>            long cur = l * (l - 1) / 2;<br>            if (n &gt;= cur &amp;&amp; (n - cur) % l == 0)<br>            {<br>                int[] res = new int[l];<br>                for (int i = 0; i &lt; l; ++i)<br>                    res[i] = (int) ((n - cur) / l + i);<br>                return res;<br>            }<br>        }<br>        return new int[0];<br>  }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>