            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;queue&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cctype&gt;<br>#include &lt;string&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br> <br>class SequenceSums<br>{<br>public:<br>  vector &lt;int&gt; sequence(int S, int L0)<br>  {<br>    vector&lt;int&gt; R;<br>    for (int L=L0;L&lt;=100;L++)<br>      if (S-L*(L-1)/2&gt;=0 &amp;&amp; (S-L*(L-1)/2)%L==0)<br>      {<br>        int A=(S-L*(L-1)/2)/L;<br>        for (int i=0;i&lt;L;i++) R.push_back(A+i);<br>        return R;<br>      }<br>    return R;<br>  }<br>};<br> <br> <br> <br>// Powered by PopsEdit<br></td>