            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class SequenceSums<br>{<br>  public int[] sequence(int n, int l)<br>  {<br>        for (int i = l; i &lt;= 100; ++i)<br>        {<br>            if ((n - (i) * (i - 1) / 2) % i != 0)<br>            {<br>                continue;<br>            }<br>            if ((n - (i) * (i - 1) / 2) / i &lt; 0)<br>            {<br>                continue;<br>            }<br>            int q = (n - (i) * (i - 1) / 2) / i;<br>            int[] ans = new int[i];<br>            for (int j = 0; j &lt; i; ++j)<br>            {<br>                ans[j] = q + j;<br>            }<br>            return ans;<br>        }<br>        return new int[0];<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>