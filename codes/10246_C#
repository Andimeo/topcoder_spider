            using System;<br><br>public class SubrectanglesOfTable {<br>  public long[] getQuantity(string[] table) {<br>        long[] res = new long[26];<br>        int height = table.Length;<br>        int width = table[0].Length;<br>        for (int row = 0; row &lt; height; ++row)<br>            for (int col = 0; col &lt; width; ++col)<br>            {<br>                res[table[row][col] - 'A'] += (eval(row, col, 2 * height, 2 * width) + eval(row + height, col, 2 * height, 2 * width) + eval(row, col + width, 2 * height, 2 * width) + eval(row + height, col + width, 2 * height, 2 * width));<br>            }<br>        return res;<br>  }<br><br>    private long eval(long row, long col, long height, long width)<br>    {<br>        return (row + 1) * (col + 1) * (height - row) * (width - col);<br>    }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>