            #include &lt;sstream&gt;<br>#include &lt;vector&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;string.h&gt;<br>#include &lt;ctype.h&gt;<br>#include &lt;math.h&gt;<br>using namespace std;<br>#define foreach(i,c)for(__typeof((c).end())i=(c).begin();i!=(c).end();++i)<br> <br>class SubrectanglesOfTable {<br>  public:<br>  vector&lt;long long&gt; getQuantity(vector &lt;string&gt; table) {<br>    int m = table.size();<br>    int n = table[0].size();<br>    vector&lt; long long &gt; ans( 26, 0 );<br>    for( int i = 0; i &lt; m + m; i++ ) for( int j = 0; j &lt; n + n; j++ )<br>      ans[table[i % m][j % n] - 'A'] += ( i + 1 ) * ( j + 1 ) * ( m + m - i ) * ( n + n - j );<br>    return ans;<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>