            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class SubwayTrip {<br>    public double earlierTrain(int Ve, int Vy, int L, int T) {<br>        double d = (L / (double)Ve) - (L / (double)(Ve + Vy));<br>        return Math.Min(d / T, 1.0);<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>