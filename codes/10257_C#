            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br><br>public class SumAndProduct {<br>    public int smallestSet(int S, int P)<br>    {<br>        int n = P &lt; S ? 2 : 1;<br>        while (true)<br>        {<br>            double f = 1.0 * S / n;<br>            if (f &lt; 1)<br>                return -1;<br>            double p = Math.Pow(f, n);<br>            if (p &lt; P)<br>                n++;<br>            else<br>                return n;<br>        }<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>