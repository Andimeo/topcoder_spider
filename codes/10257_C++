            #include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;numeric&gt;<br>#include &lt;iostream&gt;<br>#include &lt;limits&gt;<br>#include &lt;functional&gt;<br>#include &lt;math.h&gt;<br>#include &lt;stdio.h&gt;<br>#include &lt;stdlib.h&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br><br>using namespace std;<br><br>class SumAndProduct<br>{<br>public:<br>  int smallestSet(int S, int P)<br>  {<br>    if (S == P)<br>      return 1;<br><br>    for (int i = 2; i &lt;= S; i++)<br>    {<br>      double d = i;<br>      if ((S / d) &gt;= pow(P, 1.0 / d))<br>      {<br>        cout &lt;&lt; S / d &lt;&lt; endl;<br>        return i;<br>      }<br>    }<br><br>    return - 1;<br>  }<br>};<br></td>