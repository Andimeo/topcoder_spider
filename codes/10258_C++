            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br>#include &lt;complex&gt;<br>#include &lt;setjmp.h&gt;<br>#include &lt;ext/hash_map&gt;<br>#include &lt;queue&gt;<br>using namespace std;<br> <br>#define FR(i,a,b) for(int i=a;i&lt;b;i++)<br>#define FOR(i,n) FR(i,0,n)<br>#define FORI(i,v) FOR(i,(int)v.size())<br>#define FORALL(i,v) for(typeof(v.end())i=v.begin();i!=v.end();++i)<br>#define BEND(v) v.begin(),v.end()<br>#define dump(x) cerr &lt;&lt; #x &lt;&lt; " = " &lt;&lt; (x) &lt;&lt; endl;<br>typedef long long ll; typedef long double ld;<br> <br>struct LaserShooting {<br>double numberOfHits(vector &lt;int&gt; x, vector &lt;int&gt; y1, vector &lt;int&gt; y2) {<br>  double ret = 0;<br>  FORI(i,x) {<br>    double theta = atan2(y1[i], x[i]);<br>    double phi = atan2(y2[i], x[i]);<br>    ret += fabs(theta - phi);<br>  }<br>  return ret / M_PI;<br>}<br> <br> <br>};<br> <br> <br> <br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>