            import java.util.*;<br> <br>public class LaserShooting {<br>    public double numberOfHits(int[] x, int[] y1, int[] y2) {<br>        double res = 0;<br>        for (int i = 0; i &lt; x.length; i++) {<br>            double a1 = Math.atan2(y1[i], x[i]);<br>            double a2 = Math.atan2(y2[i], x[i]);<br>            res += Math.abs(a2 - a1) / Math.PI;<br>        }<br>        return res;<br>    }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>