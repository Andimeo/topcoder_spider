            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class FallingPoints<br>{<br>   public double[] getHeights(int[] X, int R)<br>   {<br>       double[] ret = new double[X.Length];<br>       double R2 = R * R;<br>       ret[0] = 0.0;<br><br>       for (int i = 1; i &lt; ret.Length; i++)<br>           if (Math.Abs(X[i] - X[i - 1]) &gt; R)<br>               ret[i] = 0.0;<br>           else<br>           {<br>               ret[i] = ret[i - 1] + Math.Sqrt(R2 - ((X[i] - X[i - 1]) * (X[i] - X[i - 1])));<br>           }<br><br>       return ret;<br>   }<br><br><br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>