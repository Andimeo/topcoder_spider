<td class="problemText" colspan="8" valign="middle" align="left">
            public class FallingPoints {<br>  public static double[] getHeights(int[] x, int r) {<br>    final int rSq = r*r;<br>    final double[] y = new double[x.length];<br>    <br>    for (int i = 1; i &lt; x.length; i++) {<br>      final int dx = x[i - 1] - x[i];<br>      final int dxSq = dx*dx;<br>      final int dySq = rSq - dxSq;<br>      if (dySq &lt; 0)<br>        y[i] = 0.0;<br>      else {<br>        final double dy = Math.sqrt((double) dySq);<br>        y[i] = y[i - 1] + dy;<br>      }<br>    }<br>    <br>    return y;<br>  }<br>}<br></td>