            import java.util.*;<br>import java.math.*;<br>import java.io.*;<br>import java.text.*;<br>import java.util.regex.*;<br>import static java.util.Arrays.*;<br>import static java.lang.Math.*;<br>import static java.lang.Integer.parseInt;<br>public class HexatridecimalSum {<br>  public String maximizeSum(String[] numbers, int k) {<br>    BigInteger base = BigInteger.valueOf(36);<br>    BigInteger[] inc = new BigInteger[36];<br>    fill(inc, BigInteger.ZERO);<br>    BigInteger ret = BigInteger.ZERO;<br>    for(int i = 0; i &lt; numbers.length; i++){<br>      for(int j = 0; j &lt; numbers[i].length(); j++){<br>        int dig = conv(numbers[i].charAt(numbers[i].length()-1-j));<br>        inc[dig] = inc[dig].add(base.pow(j).multiply(BigInteger.valueOf(35-dig)));<br>      }<br>      ret = ret.add(new BigInteger(numbers[i], 36));<br>    }<br>    sort(inc);<br>    for(int i = 0; i &lt; k; i++){<br>      ret = ret.add(inc[35-i]);<br>    }<br>    return ret.toString(36).toUpperCase();<br>  }<br>  int conv(char c){<br>    if(c &gt;= '0' &amp;&amp; c &lt;= '9') return c-'0';<br>    return 10+c-'A';<br>  }<br> <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>