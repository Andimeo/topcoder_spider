            using System;<br>using System.Collections.Generic;<br> <br>public class LeastMajorityMultiple <br>{<br>  public int leastMajorityMultiple(int a, int b, int c, int d, int e)<br>  {<br>        for (int i = 1; true; i++)<br>        {<br>            int count = 0;<br>            if (i % a == 0) count++;<br>            if (i % b == 0) count++;<br>            if (i % c == 0) count++;<br>            if (i % d == 0) count++;<br>            if (i % e == 0) count++;<br>            if (count &gt;= 3) return i;<br>        }<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>