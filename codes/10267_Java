            import java.util.*;<br>import java.math.*;<br>import static java.lang.Math.*;<br>import static java.util.Arrays.*;<br>import static java.lang.Integer.*;<br> <br>public class LeastMajorityMultiple<br>{<br>  public int leastMajorityMultiple(int a, int b, int c, int d, int e)<br>  {<br>    int[] x = new int[]{a, b, c, d, e};<br>    for(int i=1;;++i){<br>      int cnt = 0;<br>      for(int j:x)<br>        if(i%j==0)  cnt++;<br>      if(cnt&gt;=3)  return i;<br>    }<br>    <br>  }<br>  <br>  <br>}<br> <br> <br> <br> <br> <br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>