            Public Class LeastMajorityMultiple<br>Public Function leastMajorityMultiple(a As Integer, b As Integer, c As Integer, d As Integer, e As Integer) As Integer<br>Dim xx(5) As Integer<br>xx(1) = a<br>xx(2) = b<br>xx(3) = c<br>xx(4) = d<br>xx(5) = e<br>Dim n As Integer<br>n = 1<br>Dim i As Integer<br>Dim j As Integer<br>While n &lt; 1000005<br>j = 0<br>For i = 1 To 5<br>If n Mod xx(i) = 0 Then<br>j = j + 1<br>End If<br>Next<br>If j &gt;= 3 Then<br>leastMajorityMultiple = n<br>n = 2000000 + 99<br>End If<br>n = n + 1<br>End While<br>End Function<br>End Class<br></td>