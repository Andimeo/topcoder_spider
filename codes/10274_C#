            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class Underprimes<br>{<br> <br>  bool isPrime(int n)<br>  {<br>    if (n==1)<br>      return false;<br>    for(int i=2;i*i&lt;=n;++i)<br>    {<br>      if (n%i==0)return false;<br>    }<br>    return true;<br>  }<br>  <br>  public int howMany(int A, int B)<br>  {<br>    int ans = 0;<br>    for(int i=A;i&lt;=B;++i)<br>    {<br>      int j = i;<br>      int q = 0;<br>      for(int k=2;k*k&lt;=j;++k)<br>      {<br>        while (j%k==0)<br>        {<br>          ++q;<br>          j/=k;<br>        }<br>      }<br>      if (j!=1)++q;<br>      if (isPrime(q))++ans;<br>    }<br>    return ans;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>