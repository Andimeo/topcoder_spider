            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;queue&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br>#include &lt;complex&gt;<br>using namespace std;<br> <br>class Underprimes<br>{<br>public:<br>  int howMany(int, int);<br>};<br>int getFactors(int n)<br>{<br>  int ret=0;<br>  int d=1;<br>  for(int i=2;i*i&lt;=n;i+=d,d=2)<br>    if(n%i==0)<br>      while(n%i==0)<br>      {<br>        n/=i;<br>        ret++;<br>      }<br>  if(n!=1)<br>    ret++;<br>  return ret;<br>}<br>bool isPrime(int n)<br>{<br>    if(n&lt;=1 || (n!=2 &amp;&amp; n%2==0))<br>      return false;<br>    for(int i=3;i*i&lt;=n ;i+=2)<br>      if(n%i==0)<br>        return false;<br>    return true;<br>}<br>int Underprimes::howMany(int A, int B)<br>{<br>  int ret=0;<br>  for(int i=A;i&lt;=B;i++)<br>     if(isPrime(getFactors(i)))<br>         ret++;<br>  return ret;<br>}<br> <br> <br> <br> <br>//Powered by KawigiEdit 2.1.7 (beta) modified by pivanof!<br></td>