            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class CellRemoval<br>{<br>  public int cellsLeft(int[] parent, int deletedCell)<br>  {<br>    int n = parent.length;<br>    boolean[] dead = new boolean[n];<br>    dead[deletedCell] = true;<br>    int res = n - 1;<br>    for (int i = 0; i &lt; n; i++)<br>    {<br>      for (int j = 0; j &lt; n; j++)<br>      {<br>        if (parent[j] == -1) continue;<br>        if (dead[j]) continue;<br>        if (dead[parent[j]])<br>        {<br>          dead[j] = true;<br>          res--;<br>        }<br>      }<br>    }<br>    return res / 2;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>