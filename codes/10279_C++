            #include &lt;vector&gt;<br>#include &lt;deque&gt;<br>#include &lt;string&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;cmath&gt;<br> <br>using namespace std;<br> <br>class ThePresents {<br> public:<br>  double find(int n, int length, int width, int height) {<br>    double lo = 0;<br>    double hi = min(length, min(width, height));<br>    <br>    for (int i = 0; i &lt; 1000; i++) {<br>      double m = (lo + hi) / 2;<br>      <br>      long long c = floor(length / m) * floor(width / m) * floor(height / m);<br>      if (c &lt; n) {<br>        hi = m;<br>      } else {<br>        lo = m;<br>      }<br>    }<br> <br>    return lo;<br>  }<br>  <br> <br>}; <br> <br> <br> <br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>