            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class ThePresents<br>{<br>  public double find(int n, int length, int width, int height)<br>  {<br>    if(n==1)  {<br>      return Math.min(length, Math.min(width, height));<br>    }<br>    int[] min = new int[3];<br>    min[0] = length;<br>    min[1] = width;<br>    min[2] = height;<br>    Arrays.sort(min);<br>    length=min[0];<br>    width=min[1];<br>    height=min[2];<br>    double minSize = 0;<br>    double maxSize = 1e18;<br>    for(int i = 0; i &lt; 1000; i++)  {<br>      double curr = (minSize+maxSize)/2;<br>      long maxLength = (long) (length/curr);<br>      long maxWidth = (long) (width/curr);<br>      long maxHeight = (long) (height/curr);<br>      if(maxLength*maxWidth*maxHeight&lt;n)  {<br>        maxSize = curr;<br>      }<br>      else  {<br>        minSize = curr;<br>      }<br>    }<br>    return (minSize+maxSize)/2;<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>