            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class MegaCoolNumbersEasy<br>{<br>  public int count(int N)<br>  {<br>    int t = 0;<br>    for (int i = 1; i &lt;= N; i++)<br>      if (isCool(i))<br>        t++;<br>    return t;    <br>  }<br>  <br>  bool isCool(int n)<br>  {<br>    string s = n.ToString();<br>    if (s.Length &lt;= 2)<br>      return true;<br>    <br>    int d = int.Parse(s[1].ToString()) - int.Parse(s[0].ToString());<br>    bool f = true;<br>    for (int i = 0; i&lt;s.Length-1; i++)<br>      if (int.Parse(s[i+1].ToString()) - int.Parse(s[i].ToString()) != d)<br>        f = false;<br>    return f;                  <br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>