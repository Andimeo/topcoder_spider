            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class MegaCoolNumbersEasy<br>{<br>  boolean isCool (int n) {<br>    if (n &lt;= 99) return true;<br>    String s = n + "";<br>    int diff = s.charAt (1) - s.charAt (0);<br>    for (int i = 0; i &lt; s.length() - 1; i++) <br>      if (s.charAt (i+1) - s.charAt (i) != diff) return false;<br>    return true;<br>  }<br>  <br>  public int count(int N)<br>  {<br>    int cnt = 0;<br>    for (int i = 1; i &lt;= N; i++) <br>      if (isCool (i)) cnt++;<br>    return cnt;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>