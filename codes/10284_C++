            #include&lt;iostream&gt;<br>#include&lt;cstdio&gt;<br>#include&lt;cctype&gt;<br>#include&lt;cmath&gt;<br>#include&lt;cstdlib&gt;<br>#include&lt;algorithm&gt;<br>#include&lt;vector&gt;<br>#include&lt;string&gt;<br>#include&lt;list&gt;<br>#include&lt;deque&gt;<br>#include&lt;map&gt;<br>#include&lt;set&gt;<br>#include&lt;queue&gt;<br>#include&lt;stack&gt;<br>#include&lt;utility&gt;<br>#include&lt;sstream&gt;<br>#include&lt;cstring&gt;<br>using namespace std;<br>#define MP make_pair<br>#define x first<br>#define y second<br>int sqr(int a) {return a*a;}<br>class Parallelograms <br>{<br>  public:<br>  double getDifference(int xA, int yA, int xB, int yB, int xC, int yC)<br>  {<br>   pair&lt;double,double&gt;a=MP(double(xA),double(yA));<br>   pair&lt;double,double&gt;b=MP(double(xB),double(yB));<br>   pair&lt;double,double&gt;c=MP(double(xC),double(yC));<br>   if((a.x-b.x)*(c.y-b.y)==(a.y-b.y)*(c.x-b.x)) return -1;<br>   double ab=sqrt(sqr(a.x-b.x)+sqr(a.y-b.y));<br>   double bc=sqrt(sqr(b.x-c.x)+sqr(b.y-c.y));<br>   double ca=sqrt(sqr(c.x-a.x)+sqr(c.y-a.y));<br>   return 2*(max(ab, max(bc, ca))-min(ab, min(bc, ca))); <br>  }<br>};<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>