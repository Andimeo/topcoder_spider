            import java.util.*;<br>import java.math.*;<br><br>public class Parallelograms {<br>  double dist(double x, double y) {<br>    return Math.sqrt(x*x+y*y);<br>  }<br>  int kc(int x1, int y1, int x2, int y2) {<br>    return x1*y2-x2*y1;<br>  }<br>  public double getDifference(int xA, int yA, int xB, int yB, int xC, int yC) {<br>    double res = 0.0;<br>    double p1 = dist(xA-xB,yA-yB)+dist(xC-xB,yC-yB);<br>    double p2 = dist(xB-xA,yB-yA)+dist(xC-xA,yC-yA);<br>    double p3 = dist(xA-xC,yA-yC)+dist(xB-xC,yB-yC);<br>    res = Math.max(p1, Math.max(p2,p3))-Math.min(p1, Math.min(p2,p3));<br>    res *= 2;<br>    if (kc(xA-xC,yA-yC,xB-xC,yB-yC)==0) res = -1;<br>        <br>    System.err.println(res);<br>    return res;<br>  }<br><br>}<br></td>