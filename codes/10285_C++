            #include&lt;iostream&gt;<br>#include&lt;cstdio&gt;<br>#include&lt;cctype&gt;<br>#include&lt;cmath&gt;<br>#include&lt;cstdlib&gt;<br>#include&lt;algorithm&gt;<br>#include&lt;vector&gt;<br>#include&lt;string&gt;<br>#include&lt;list&gt;<br>#include&lt;deque&gt;<br>#include&lt;map&gt;<br>#include&lt;set&gt;<br>#include&lt;queue&gt;<br>#include&lt;stack&gt;<br>#include&lt;utility&gt;<br>#include&lt;sstream&gt;<br>#include&lt;cstring&gt;<br>using namespace std;<br>int howmany[2000];<br>class AnotherCoinProblem <br>{<br>  public:<br>  int minimumCoins(long long cost)<br>  {<br>    int res=0;<br>    for(int i=0;i&lt;=150;i++)howmany[i]=1000;<br>    howmany[0]=0;<br>    for(int i=1;i&lt;=100;i++)<br>    howmany[i]=min(howmany[i-1]+1,howmany[i]);<br>    for(int i=10;i&lt;=100;i++)<br>    howmany[i]=min(howmany[i-10]+1,howmany[i]);<br>    for(int i=25;i&lt;=100;i++)<br>    howmany[i]=min(howmany[i-25]+1,howmany[i]);<br>    while(cost)<br>    {<br>     int a=cost%100;<br>     cost/=100;<br>     res+=howmany[a];           <br>    }<br>    return res;<br>  }<br>};<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>