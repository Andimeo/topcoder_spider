            import java.util.*;<br>public class AnotherCoinProblem{<br>  public int minimumCoins(long cost){<br>    int ret = 0;<br>    int t = 0;<br>    <br>    <br>    int[] d = new int[16];//digit<br>    Arrays.fill(d,0);<br>    String tmp = ""+cost;<br>    int plus = 16-tmp.length();<br>    for(int i = 0; i &lt; tmp.length(); i++){<br>      d[i+plus] = (int)tmp.charAt(i) - (int)'0';<br>    }<br>    //for(int i = 0; i &lt; 16; i++) System.out.println(d[i]);<br>    System.out.println(" ");<br>    for(int i = 0; i &lt; 8; i++){<br>      //looking at 2*i and 2*i+1<br>      t = d[2*i] + d[2*i + 1];<br>      for(int n = 1; n &lt; 5; n++){<br>        if((d[2*i] == 2 &amp;&amp; d[2*i + 1]&gt;=5) || d[2*i] &gt; 2){<br>          d[2*i]-=2;<br>          d[2*i + 1]-=5;<br>          if(d[2*i + 1] &lt; 0){<br>            d[2*i + 1]+=10;<br>            d[2*i]--;<br>          }<br>          //System.out.println(n);<br>          t=Math.min(t,d[2*i] + d[2*i + 1] + n);<br>          System.out.println(d[2*i] + " " + d[2*i + 1] + " " + n);<br>        }<br>      }<br>      System.out.println(i + " " + t);<br>      ret+=t;<br>    }<br>    <br>    return ret;<br>    <br>  }<br>}<br></td>