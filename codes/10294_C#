            using System;<br>using System.Collections.Generic;<br>using System.Text;<br><br>class SkiFriction<br>{<br>    public int bestPosition(string skiFriction, string pathFriction)<br>    {<br>        int totalTime = 0;<br>        for (int i = 0; i &lt; pathFriction.Length - skiFriction.Length; i++)<br>        {<br>            int maxFric = 0;<br>            for (int j = 0; j &lt; skiFriction.Length; j++)<br>            {<br>                int friction = skiFriction[j] + pathFriction[i + j] - '0' - '0';<br>                if (friction &gt; maxFric)<br>                {<br>                    maxFric = friction;<br>                }<br>            }<br>            totalTime += maxFric;<br>        }<br>        return totalTime;<br>    }<br>}<br></td>