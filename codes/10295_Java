            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class GroupedWordChecker<br>{<br>  boolean isG(String s)<br>  {<br>    boolean ret=true;<br>    boolean [] used = new boolean[27];<br>    <br>    int N=s.length();<br>    for(int i=0; i&lt;N; i++)<br>    {<br>      if(used[s.charAt(i)-'a']&amp;&amp;i&gt;0&amp;&amp;s.charAt(i)!=s.charAt(i-1))return false;<br>      used[s.charAt(i)-'a']=true;<br>    }<br>    return ret;<br>  }<br>  <br>  public int howMany(String[] words)<br>  {<br>    int N=words.length;<br>    int ret=0;<br>    for(int i=0; i&lt;N; i++)<br>    {<br>      if(isG(words[i]))ret++;<br>    }<br>    return ret;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>