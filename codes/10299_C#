<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br><br>class TheSquareDivTwo<br>{<br>  public string[] solve(string[] board)<br>  {<br>    int[] counts = new int[board.Length];<br>    for (int i = 0; i &lt; board.Length; ++i)<br>      for (int j = 0; j &lt; board.Length; ++j)<br>        if (board[i][j] == 'C') counts[i]++;<br>        <br>    string[] output = new string[board.Length];<br>    for (int i = 0; i &lt; board.Length; ++i)<br>      for (int j = 0; j &lt; board.Length; ++j)<br>        if (i &lt; board.Length - counts[j])<br>          output[i] += '.';<br>        else output[i] += 'C';<br>        <br>    return output;<br>  }<br>}<br></td>