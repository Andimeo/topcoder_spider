            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class TheSquareDivTwo {<br>public:<br>  vector &lt;string&gt; solve(vector &lt;string&gt;);<br>};<br> <br>int arr[51];<br> <br>vector &lt;string&gt; TheSquareDivTwo::solve(vector &lt;string&gt; board) {<br>  int len = board.size();<br>  for (int i = 0; i != len; i++)<br>    for (int j = 0; j != len; j++)<br>    {<br>      if (board[i][j] == 'C')<br>        arr[i] ++;<br>    }  <br>  <br>  vector&lt;string&gt; ret(len, "");<br>  for (int i = 0; i != len; i++)<br>  {<br>    for (int j = 0; j != len - arr[i]; j++)<br>    {<br>      ret[j] += '.';<br>    }<br>    for (int j = len - arr[i]; j != len; j++)<br>    {<br>      ret[j] += 'C';<br>    }<br>  }<br>  return ret;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>