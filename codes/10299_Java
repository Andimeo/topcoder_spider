            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class TheSquareDivTwo<br>{<br>  public String[] solve(String[] b)<br>  {<br>    int n = b.length;<br>    int[] r = new int[n];<br>    int count = 0;<br>    for (int i = 0; i &lt; n; i++) {<br>      for (int j = 0; j &lt; n; j++) {<br>        if (b[i].charAt(j) == 'C') {<br>          r[i]++;<br>          count++;<br>        }<br>      }<br>    }<br>    String[] res = new String[n];<br>    for (int i = n - 1; i &gt;= 0; i--) {<br>      res[i] = "";<br>      for (int j = n - 1; j &gt;= 0; j--)<br>        if (r[j] &gt; 0) {<br>          r[j]--;<br>          res[i] = 'C' + res[i];<br>        } else {<br>          res[i] = '.' + res[i];<br>        }<br>    }<br>    return res;<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>