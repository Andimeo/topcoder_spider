            import java.util.*;<br>import java.text.*;<br>import static java.util.Arrays.*;<br>public class IncredibleMachineEasy {<br>    public double gravitationalAcceleration(int[] height, int T) {<br>        double res = 0.0;<br>        for(int i = 0; i &lt; height.length; i++) {<br>          res += Math.sqrt(2 * height[i]);<br>        }<br>        return res * res / T / T;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>