            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class IncredibleMachine<br>{<br>    public double gravitationalAcceleration(int[] x, int[] y, int T)<br>    {<br>        double t = 0, v = 0;<br>        for (int i = 1; i &lt; x.Length; ++i)<br>        {<br>            double v_ = Math.Sqrt(v * v + (y[i-1] - y[i]) * 2);<br>            t += Math.Sqrt((x[i] - x[i - 1]) * (x[i] - x[i - 1]) + (y[i] - y[i - 1]) * (y[i] - y[i - 1])) / (v_ + v) * 2;<br>            v = v_;<br>        }<br>        return t * t / T / T;<br>    }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>