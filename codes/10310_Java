            import static java.lang.Math.*;<br>import static java.math.BigInteger.*;<br>import static java.util.Arrays.*;<br>import static java.util.Collections.*;<br>import java.math.*;<br>import java.util.*;<br><br>public class IncredibleMachine {<br>  public double gravitationalAcceleration(int[] x, int[] y, int T) {<br>    double left = 0, right = 1e20;<br>    int n = x.length;<br>    for (int i = 0; i &lt; 200; i++) {<br>      double mid = (left + right) / 2;<br>      double t = 0, v = 0;<br>      for (int j = 0; j + 1 &lt; n; j++) {<br>        double d = hypot(x[j + 1] - x[j], y[j + 1] - y[j]);<br>        double g = mid * (y[j] - y[j + 1]) / d;<br>        double t2 = (-v + sqrt(v * v + 2 * g * d)) / g;<br>        t += t2;<br>        v += g * t2;<br>      }<br>      if (t &lt; T) right = mid;<br>      else left = mid;<br>    }<br>    return left;<br>  }<br>  void debug(Object...os) {<br>    System.err.println(deepToString(os));<br>  }<br>}<br></td>