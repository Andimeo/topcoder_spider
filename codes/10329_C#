            using System;<br>using System.Collections.Generic; <br><br>public class PageNumbers {<br>    int get(int n,int d)<br>    {<br>        if (d==0)<br>        {<br>            return n/10;<br>        }<br>        else<br>        {<br>            int ret = n/10;<br>            if((n%10)&gt;=d)<br>            {<br>                ret++;<br>            }<br>            return ret;<br>        }<br>    }<br><br>  public int[] getCounts(int N) {<br>        int[] ret = new int[10];<br>      int mul = 1;<br>      int N1 = N;<br>      while (N&gt;0)<br>      {<br>          for (int i = 0; i &lt; 10; i++)<br>          {<br>              ret[i] = ret[i] + get(N-1, i)*mul;<br>          }<br>          int ml1 = N1%mul + 1;<br>          ret[N%10] = ret[N%10] + ml1;<br>          N /= 10;<br>          mul *= 10;<br>      }<br>      return ret;<br>  }<br>  <br><br>}<br><br><br><br>// Powered by FileEdit<br></td>