            #include&lt;iostream&gt;<br>#include&lt;vector&gt;<br>#include&lt;string&gt;<br>#include&lt;map&gt;<br>#include&lt;set&gt;<br>#include&lt;algorithm&gt;<br>#include&lt;sstream&gt;<br>#include&lt;cstdio&gt;<br>#include&lt;cmath&gt;<br>#include&lt;cstdlib&gt;<br>#include&lt;ctime&gt;<br>using namespace std;<br> <br>template&lt;typename T&gt; inline int SZ(const T&amp; c) { return (int)c.size(); }<br> <br>#include&lt;stdio.h&gt;<br>#include&lt;string.h&gt;<br> <br>int ans[10],sign;<br> <br>void solve(int num)<br>{<br>  int i,t=num%10;<br>  if(num&lt;=0) return;<br>  for(i=0;i&lt;=t;i++)<br>    ans[i]+=sign;<br>  num/=10;<br>  int tt=num;<br>  while(tt!=0)<br>  {<br>    ans[tt%10]+=(t+1)*sign;<br>    tt/=10;<br>  }<br>  for(i=0;i&lt;10;i++)<br>    ans[i]+=num*sign;<br>  ans[0]-=sign;<br>  sign*=10;<br>  solve(num-1);<br>}<br> <br>struct PageNumbers<br>{<br>  vector &lt;int&gt; getCounts(int N)<br>  {<br>    int a=1,b=N;<br>    memset(ans,0,sizeof(ans));<br>    sign=1;solve(b);<br>    sign=-1;solve(a-1);<br>    vector&lt;int&gt; ret;<br>    for(int i=0;i&lt;10;i++)<br>      ret.push_back(ans[i]);<br>    return ret;<br>  }<br>};<br></td>