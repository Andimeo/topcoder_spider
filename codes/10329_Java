            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class PageNumbers<br>{<br>  public int[] getCounts(int a)<br>  {<br>    int [] cd = new int[10];<br>    int d = 0;<br>    long last = 0;<br>    long n = a;<br>    long pow = 1;<br>    //long res = 0;<br>    cd[0] = 0;<br>    while (a &gt; 0) {<br>      d ++;<br>      last = a % 10;<br>      a /= 10;<br> <br>    //  long ra = n / pow;<br>      long rl = n % pow;<br> <br>    //  printf("last = %ld; a = %ld ; rl = %ld; pow = %ld\n", last, a, rl, pow);<br> <br>      for (int i = 0; i &lt; 10; i ++) {<br> <br>        if (i &lt; last) {<br>          cd[i] += pow * a;<br>          if (i &gt; 0)<br>            cd[i] += pow;<br>        }<br>        else if (last == i) {<br>          if (i &gt; 0) {<br>            cd[i] += pow * a  + (rl + 1);<br>          }<br>          else if (a &gt; 0) {<br>            cd[i] += pow * (a -1)  + (rl + 1);<br>          }<br>        }<br>        else {<br>          cd[i] += a * pow;<br>        }<br>      }<br>      pow *= 10;<br>//      for (int j = 0; j &lt; 10;  j ++) {<br>//        printf("%ld ",  cd[j]);<br>//      }<br>//      printf("%ld \n", a);<br>    }<br>    return cd;<br>  }<br>  <br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>