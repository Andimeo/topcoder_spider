            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class RepaintTheChessboard<br>{<br>  public int minimumChanges(String[] a  )<br>  {<br>        int ans = 64;<br>        int n = a.length;<br>        int m = a[0].length();<br>        for (int i = 0; i &lt;= n - 8; ++i)<br>        {<br>            for (int j = 0; j &lt;= m - 8; ++j)<br>            {<br>                int cur = 0;<br>                for (int dx = 0; dx &lt;= 7; ++dx)<br>                {<br>                    for (int dy = 0; dy &lt;= 7; ++dy)<br>                    {<br>                        if ((dx + dy) % 2 == 0 &amp;&amp; a[i + dx].charAt(j + dy) == 'B')<br>                        {<br>                            ++cur;<br>                        }<br>                        if ((dx + dy) % 2 == 1 &amp;&amp; a[i + dx].charAt(j + dy) == 'W')<br>                        {<br>                            ++cur;<br>                        }<br>                    }<br>                }<br>                cur = Math.min(cur, 64 - cur);<br>                ans = Math.min(ans, cur);<br>            }<br>        }<br> <br>        return ans;<br>    <br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>