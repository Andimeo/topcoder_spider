            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br> <br>public class BidirectionalQueue {<br>    public int extractElements(int N, int[] indices) {<br>        int n = N;<br>    int current = 0;<br>    int t = 0;<br>    <br>    for (int i = 0; i &lt; indices.Length; i++)<br>    {<br>      int x = indices[i] - 1;<br>      int a = (x - current + n) %n;<br>      int b = (current -x +n) %n;<br>      t += Math.Min(a,b);<br>      n--;<br>      current = x;<br>      for (int j = i + 1; j &lt; indices.Length; j++)<br>      {<br>        if(indices[j] &gt; x)<br>          indices[j]--;<br>      }<br>    }<br>    <br>        return t;<br>    }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>