            import java.util.*;<br>import java.math.*;<br>public class BidirectionalQueue {<br>  public int extractElements(int N, int[] indices) {<br>    int[] q = new int[N];<br>    int shift = 0;<br>    for (int i = 0; i &lt; N; ++i) q[i] = i+1;<br>    for (int i = 0, k; i &lt; indices.length; ++i) {<br>      int want = indices[i];<br>      for (k = 0; k &lt; q.length; ++k) if (q[k] == want) break;<br>      shift += Math.min(k, q.length - k);<br>      int[] qq = new int[q.length - 1];<br>      for (int j = 0; j &lt; qq.length; ++j) qq[j] = q[(k+1+j)%q.length];<br>      q = qq;<br>    }<br>    return shift;<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>