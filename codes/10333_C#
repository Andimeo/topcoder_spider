            using System;<br><br>public class PlaneFractal {<br>  public string[] getPattern(int s, int N, int K, int r1, int r2, int c1, int c2) {<br>        string[] res = new string[r2 - r1 + 1];<br>        for (int r = r1; r &lt;= r2; ++r)<br>            for (int c = c1; c &lt;= c2; ++c)<br>                res[r - r1] += isBlack(N, K, s, r, c) ? "1" : "0";<br>        return res;<br>  }<br><br>    private bool isBlack(int N, int K, int s, int r, int c)<br>    {<br>        if (s == 0)<br>            return false;<br>        int mid = (N - K) / 2;<br>        if (r % N &gt;= mid &amp;&amp; r % N &lt; mid + K &amp;&amp; c % N &gt;= mid &amp;&amp; c % N &lt; mid + K)<br>            return true;<br>        return isBlack(N, K, s - 1, r / N, c / N);<br>    }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>