            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;cmath&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;set&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;cstdio&gt;<br><br>using namespace std;<br><br>vector&lt;string&gt; res;<br><br>char get_ch(int x, int y, int N, int K) {<br>  int nx, ny, now;<br>  for (;x || y; x /= N, y /= N) {<br>    nx = x % N, ny = y % N;<br>    now = (N - K) / 2;<br>    if (nx &gt;= now &amp;&amp; nx &lt;= now + K - 1 &amp;&amp; ny &gt;= now &amp;&amp; ny &lt;= now + K - 1) return '1';<br>  }<br>  return '0';<br>}<br><br><br>class PlaneFractal{<br>public:<br>  <br>  vector &lt;string&gt; getPattern(int s, int N, int K, int r1, int r2, int c1, int c2) {<br>    res.clear();<br>    string tmp;    <br>    for (int i = r1; i &lt;= r2; i++) {<br>      tmp = "";<br>      for (int j = c1; j &lt;= c2; j++) tmp = tmp + get_ch(i, j, N, K);<br>      res.push_back(tmp);<br>    }<br>    return res;<br>  }<br>  <br><br>};<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>