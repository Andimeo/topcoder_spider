            import java.util.*;<br>import static java.lang.Math.*;<br>public class PlaneFractal {<br>    // s shouldn't be needed<br>    public String[] getPattern(int s, int N, int K, int r1, int r2, int c1, int c2) {<br>        int B = (N-K)/2;<br>        String[] res = new String[r2-r1+1];<br>        Arrays.fill(res, "");<br>        for (int r0=r1;r0&lt;=r2;r0++)<br>            for(int c0=c1;c0&lt;=c2;c0++) {<br>                int r = r0, c = c0;<br>                char ch = '0';<br>                for (int i = s; i--&gt;0; ) {<br>                    int t1 = r%N, t2 = c%N;<br>                    if (t1 &gt;= B &amp;&amp; t1 &lt; N-B &amp;&amp; t2 &gt;= B &amp;&amp; t2 &lt; N-B) ch = '1'; <br>                    r /= N; c /= N;<br>                }<br>                res[r0-r1] += ch;<br>            }<br>        return res;<br>    }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>