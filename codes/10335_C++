            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;queue&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cctype&gt;<br>#include &lt;string&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br> <br>class KthProbableElement<br>{<br>public:<br>  double C[105][105];<br>  double f[1005][105];<br>  double probability(int m, int L, int U, int n, int d)<br>  {<br>    memset(C,0,sizeof(C));<br>    for (int i=0;i&lt;=m;i++) for (int j=0;j&lt;=i;j++)<br>      C[i][j]=(j==0)?1:(C[i-1][j-1]+C[i-1][j]);<br>    double S=pow((double)(U-L+1),(double)m);<br>    memset(f,0,sizeof(f));<br>    f[L-1][0]=1;<br>    for (int i=L;i&lt;=U;i++)<br>    {<br>      for (int k=0;k&lt;=m;k++)<br>        for (int c=0;c&lt;=k;c++)<br>          f[i][k]+=f[i-1][k-c]*C[k][c];<br>      if (i&lt;n)<br>        for (int k=d;k&lt;=m;k++) f[i][k]=0;<br>      else<br>        for (int k=0;k&lt;d;k++) f[i][k]=0;<br>    }<br>    double R=f[U][m];<br>    return R/S;<br>  }<br>};<br></td>