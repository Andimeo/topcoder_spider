            import java.util.*;<br>import java.io.*;<br>import static java.lang.Math.*;<br>import static java.util.Arrays.*;<br>import java.math.*;<br>public class KthProbableElement {<br>  public double probability(int M, int lowerBound, int upperBound, int N, int K) {<br>    int n = upperBound-lowerBound+1;<br>    hChance = 1.0/n;<br>    lChance = (N-lowerBound)/(double)n;<br>    goal = K;<br>    return go(M,0,0);<br>  }<br>  double lChance = 0;<br>  double hChance = 0;<br>  int goal;<br>  double[][][] memo = new double[101][101][101];<br>  double go(int left, int lower, int eq){<br>    if(memo[left][lower][eq] != 0) return memo[left][lower][eq]-1;<br>    if(left == 0) return (lower &lt; goal &amp;&amp; lower+eq &gt;= goal) ? 1 : 0;<br>    double ret = lChance*go(left-1, lower+1, eq);<br>    ret += hChance*go(left-1, lower, eq+1);<br>    ret += (1-lChance-hChance)*go(left-1, lower, eq);<br>    memo[left][lower][eq] = ret+1;<br>    return ret;<br>  }<br> <br> <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>