            using System;<br>using System.Collections.Generic;<br> <br>public class FriendScore <br>{<br>  public int highestScore(string[] friends)<br>  {<br>    int best = 0;<br>    for (int i = 0; i &lt; friends.Length; i++)<br>    {<br>      int count = 0;<br>      for (int j = 0; j &lt; friends[i].Length; j++)<br>      {<br>        if (i == j) continue;<br>        if (friends[i][j] == 'Y')<br>          count++;<br>        else<br>        {<br>          for (int k = 0; k &lt; friends.Length; k++)<br>          {<br>            if (k == i || k == j) continue;<br>            if (friends[i][k] == 'Y' &amp;&amp; friends[j][k] == 'Y')<br>            {<br>              count++;<br>              break;<br>            }<br>          }<br>        }<br>      }<br>      best = Math.Max(best, count);<br>    }<br>    return best;<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>