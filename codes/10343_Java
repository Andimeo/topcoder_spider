            public class FriendScore {<br>  public int highestScore(String[] friends) {<br>    int max = 0;<br>    int len = friends.length;<br>    for(int i=0; i&lt;len; i++) {<br>      int cnt = 0;<br>      for(int j=0; j&lt;len; j++) if(i!=j) {<br>        if(friends[i].charAt(j)=='Y') {<br>          cnt++;<br>        } else {<br>          boolean found = false;<br>          for(int k=0; k&lt;len; k++) if(k!=i &amp;&amp; k!=j) {<br>            if(friends[k].charAt(i)=='Y' &amp;&amp; friends[k].charAt(j)=='Y') {<br>              found = true;<br>              break;<br>            }<br>          }<br>          if(found) cnt++;<br>        }<br>      }<br>      if(cnt&gt;max) max=cnt;<br>    }<br>    return max;<br>  }<br>}<br></td>