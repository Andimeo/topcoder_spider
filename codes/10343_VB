            Class FriendScore<br>    Public Function highestScore(ByVal friends As String()) As Integer<br>        Dim i As Integer<br>        Dim j As Integer<br>        Dim k As Integer<br><br><br>        Dim iMax As Integer<br><br>        Dim iq As New System.Collections.Queue<br>        iMax = 0<br>        For i = 0 To friends.Length - 1<br>            iq.Clear()<br>            iq.Enqueue(i)<br>            For j = 0 To friends(i).Length - 1<br>                If (Mid(friends(i), j + 1, 1) = "Y") Then<br>                    If iq.Contains(j) = False Then<br>                        iq.Enqueue(j)<br>                    End If<br><br>                    For k = 0 To friends(i).Length - 1<br>                        If (Mid(friends(j), k + 1, 1) = "Y") Then<br>                            If iq.Contains(k) = False Then<br>                                iq.Enqueue(k)<br>                            End If<br>                        End If<br>                    Next<br>                End If<br>            Next<br><br>            If iMax &lt; iq.Count - 1 Then<br>                iMax = iq.Count - 1<br>            End If<br><br>        Next<br><br>        Return iMax<br>    End Function<br>End Class<br></td>