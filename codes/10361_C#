            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br> <br>public class SaveTheTrees<br>{<br>  public int minimumCut (int[] x, int[] y, int[] h)<br>  {<br>    int min = int.MaxValue;<br>    int n = x.Length;<br>    int[] qq = new int[n];<br>    <br>    foreach (int xa in x)<br>      foreach (int xb in x)<br>        if (xb &gt;= xa)<br>          foreach (int ya in y)<br>            foreach (int yb in y)<br>              if (yb &gt;= ya)<br>            {<br>              int cut = 0;<br>              int cutl = -2 * (xb - xa + yb - ya);<br> <br>              int qqn = 0;<br> <br>              for (int i = 0; i &lt; n; ++i)<br>                if (x[i] &lt; xa || x[i] &gt; xb || y[i] &lt; ya || y[i] &gt; yb)<br>              {<br>                ++cut;<br>                cutl += h[i];<br>              }<br>              else<br>                qq[qqn++] = h[i];<br> <br>              Array.Sort (qq, 0, qqn);<br> <br>              while (cutl &lt; 0 &amp;&amp; qqn &gt; 0)<br>              {<br>                ++cut;<br>                cutl += qq[--qqn];<br>              }<br>              <br>              if (cutl &gt;= 0)<br>                min = Math.Min (min, cut);<br>            }<br>    <br>    return min;<br>  }<br> <br> <br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>