            import java.util.*;<br><br>public class SaveTheTrees {<br>    public int minimumCut(int[] x, int[] y, int[] h){<br>      int n = x.length;<br>      for (int i = 0; i &lt; n; i++) {<br>        for (int j = 0; j &lt; n - 1; j++) {<br>          if (h[j] &lt; h[j + 1]) {<br>            int tmp = x[j];<br>            x[j] = x[j + 1];<br>            x[j + 1] = tmp;<br>            <br>            tmp = y[j];<br>            y[j] = y[j + 1];<br>            y[j + 1] = tmp;<br>            <br>            tmp = h[j];<br>            h[j] = h[j + 1];<br>            h[j + 1] = tmp;<br>          }<br>        }<br>      }<br>      <br>      int answer = x.length - 1;<br>      for (int i = 0; i &lt; x.length; i++) {<br>        for (int j = 0; j &lt; x.length; j++) {<br>          if (x[i] &gt;= x[j]) {<br>            continue;<br>          }<br>            for (int u = 0; u &lt; y.length; u++) {<br>              for (int v = 0; v &lt; y.length; v++) {<br>                if (y[u] &gt;= y[v]) {<br>                  continue;<br>                }<br>                int total = 0;<br>                int count = 0;<br>                for (int w = 0; w &lt; x.length; w++) {<br>                  if (x[w] &lt; x[i] || x[w] &gt; x[j] || y[w] &lt; y[u] || y[w] &gt; y[v]) {<br>                    total += h[w];<br>                    count++;<br>                  }<br>                }<br>                int size = 2 * (x[j] - x[i] + y[v] - y[u]) - total;<br>                for (int w = 0; w &lt; x.length; w++) {<br>                  if (x[w] &lt; x[i] || x[w] &gt; x[j] || y[w] &lt; y[u] || y[w] &gt; y[v]) {<br>                    continue;<br>                  }<br>                  if (size &lt;= 0) {<br>                    break;<br>                  }<br>                  size -= h[w];<br>                  count++;<br>                }<br>                answer = Math.min(answer, count);<br>              }<br>            }<br>        }<br>      }<br>      return answer;<br>    }<br>}<br></td>