            import java.util.*;<br> <br>public class TheSwap<br>{<br>  HashSet &lt;String&gt; [] h;<br> <br>  void go (String x, int k) {<br>    if (h[k].contains (x)) return;<br>    h[k].add (x);<br>    if (k == 0) return;<br>    char [] z = x.toCharArray ();<br>    for (int i = 0; i &lt; z.length; i++)<br>      for (int j = i + 1; j &lt; z.length; j++)<br>        if (i != 0 || z[j] != '0') {<br>          char t = z[i]; z[i] = z[j]; z[j] = t;<br>          go (new String (z), k - 1);<br>               t = z[i]; z[i] = z[j]; z[j] = t;<br>        }<br>  }<br> <br> <br>  public int findMax(int n, int k)<br>  {<br>    String s = "" + n;<br>    h = new HashSet [k + 1];<br>    for (int i = 0; i &lt;= k; i++)<br>      h[i] = new HashSet &lt;String&gt; ();<br> <br>    go (s, k);<br> <br>    String mx = "";<br> <br>    for (String t : h[0])<br>      if (t.compareTo (mx) &gt; 0) mx = t;<br>    <br>    if (mx.length () == 0) return -1; else return Integer.parseInt (mx);<br>  }<br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>