            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cmath&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;utility&gt;<br> <br>using namespace std;<br> <br>class StrangeCountry {<br>  public:<br>  <br>  int flag[100];<br>  int n, m;<br>  vector&lt;string&gt; g;<br>  <br>  void dfs(int x) {<br>    if (flag[x]) return;<br>    flag[x] = 1;<br>    m++;<br>    int i;<br>    for (i = 0; i &lt; n; i++) {<br>      if (g[x][i] == 'Y') {<br>        dfs(i);<br>      }<br>    }<br>  }<br>  <br>  int transform(vector &lt;string&gt; gg) {<br>    int i, j, k = 0, t = 0;<br>    g = gg;<br>    n = g.size();<br>    for (i = 0; i &lt; n; i++) {<br>      for (j = 0; j &lt; n; j++) if (g[i][j] == 'Y') {<br>        k++;<br>      }<br>    }<br>    k /= 2;<br>    if (k &lt; n - 1) return -1;<br>    for (i = 0; i &lt; n; i++) if (!flag[i]) {<br>      m = 0;<br>      dfs(i);<br>      if (m == 1) return -1;<br>      t++;<br>    }<br>    return t - 1;<br>  }<br>};<br> <br> <br>// Powered by PopsEdit<br></td>