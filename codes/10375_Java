            import java.util.*;<br> <br>public class StrangeCountry {<br>    boolean[][] a;<br>    int n;<br>    int m;<br>    boolean[] u;<br> <br>    int dfs(int i) {<br>        u[i] = true;<br>        int r = 1;<br>        for (int j = 0; j &lt; n; j++) {<br>            if (a[i][j] &amp;&amp; !u[j]) {<br>                r += dfs(j);<br>            }<br>        }<br>        return r;<br>    }<br> <br>    public int transform(String[] g) {<br>        n = g.length;<br>        a = new boolean[n][n];<br>        m = 0;<br>        for (int i = 0; i &lt; n; i++) {<br>            for (int j = 0; j &lt; n; j++) {<br>                a[i][j] = g[i].charAt(j) == 'Y';<br>                if (a[i][j] &amp;&amp; j &gt; i) {<br>                    m++;<br>                }<br>            }<br>        }<br>        if (m &lt; n - 1) {<br>            return -1;<br>        }<br>        int r = 0;<br>        u = new boolean[n];<br>        for (int i = 0; i &lt; n; i++) {<br>            if (!u[i]) {<br>                r++;<br>                int s = dfs(i);<br>                if (s == 1) {<br>                    return -1;<br>                }<br>            }<br>        }<br>        return r - 1;<br>    }<br> <br> <br>}<br> <br> <br>// Powered by PopsEdit<br> <br>// Powered by CodeProcessor<br></td>