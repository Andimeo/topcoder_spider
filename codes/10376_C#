            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class DifferentStrings<br>{<br>   public int minimize(string A, string B)<br>   {<br>       int ret = 51;<br>       int l = B.Length - A.Length;<br><br>       for(int i = 0;i &lt;= l;i++)<br>       {<br>           int nret = 0;<br><br>           for(int j = 0;j &lt; A.Length;j++)<br>               nret += A[j] == B[j + i] ? 0 : 1;<br><br>           ret = Math.Min(ret, nret);<br>           }<br><br>       return ret;<br>   }<br><br><br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>