            #include&lt;stdio.h&gt;<br>#include&lt;stdlib.h&gt;<br>#include &lt;algorithm&gt; <br>#include &lt;cmath&gt; <br>#include &lt;functional&gt; <br>#include &lt;iostream&gt; <br>#include &lt;list&gt; <br>#include &lt;map&gt; <br>#include &lt;queue&gt; <br>#include &lt;stack&gt; <br>#include &lt;string&gt; <br>#include &lt;vector&gt; <br><br>using namespace std;<br><br>class DifferentStrings<br>{<br>public: <br>  int minimize(string a, string b)<br>  {<br>    int len1=a.size();<br>    int len2=b.size();<br>    int i,j;<br>    int Min=INT_MAX;<br>    for (i=0;i+len1&lt;=len2;i++)<br>    {<br>      int m=0;<br>      for (j=0;j&lt;len1;j++)<br>      {<br>        if(a[j]!=b[i+j])<br>          m++;<br>      }<br>      if (m&lt;Min)<br>      {<br>        Min=m;<br>      }<br><br>    }<br>    return Min;<br>  }<br>};<br></td>