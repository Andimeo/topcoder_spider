            import java.util.*;<br>public class DifferentStrings<br>{<br>  static int r=10000;<br>  public int minimize(String A, String B)<br>  { <br>    int d=0;<br>      for(int k=0;k&lt;B.length()-A.length()+1;k++)<br>    {<br>        d=difference(A,B.substring(k,k+A.length()));<br>        r=Math.min(r,d);<br>    }<br>    return r;<br>  }  <br>  int difference(String A,String B)<br>  {<br>    int d=0;<br>    for(int i=0;i&lt;A.length();i++)<br>    if(A.charAt(i)!=B.charAt(i))<br>    d++;<br>    return d;    <br>  }<br>}<br></td>