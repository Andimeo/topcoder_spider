            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Diagnostics;<br><br>public class SquareOfDigits<br>{<br>    public int getMax(string[] data)<br>    {<br>        int max = int.MinValue;<br>        for (int r = 0; r &lt; data.Length; r++)<br>            for (int c = 0; c &lt; data[0].Length; c++)<br>            {<br>                for (int i = 0; i + r &lt; data.Length &amp;&amp; i + c &lt; data[0].Length; i++)<br>                {<br>                    if (data[r][c] == data[r + i][c] &amp;&amp; data[r][c] == data[r + i][c + i] &amp;&amp; data[r][c] == data[r][c + i])<br>                    {<br>                        max = Math.Max(max, (i + 1)*(i+1));<br><br>                    }<br>                }<br><br>            }<br>        return max;<br><br>    }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>