            import java.util.*;<br>public class SquareOfDigits {<br>    public int getMax(String[] data) {<br>        int res=1;<br>        for (int i = 0; i &lt; data.length; i++) {<br>      for (int j = 0; j &lt; data[i].length(); j++) {<br>        for (int k = 0; k &lt; Math.min(data.length-i,data[i].length()-j); k++) {<br>          if(data[i+k].charAt(j+k)==data[i+k].charAt(j)&amp;&amp;<br>          data[i].charAt(j)==data[i].charAt(j+k)&amp;&amp;<br>          data[i].charAt(j+k)==data[i+k].charAt(j))<br>            res=Math.max(res,(k+1)*(k+1));<br>        }<br>      }<br>    }<br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>