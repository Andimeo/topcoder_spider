            using System;<br> <br>public class UnluckyNumbers {<br>  public int getCount(int[] luckySet, int n) {<br>        int m = luckySet.Length;<br>        int cc = 0;<br>        bool[] was = new bool[10001];<br>        for (int i = 1; i &lt;= 1000; i++)<br>        {<br>            was[i] = false;<br>        }<br><br>        for (int i = 0; i &lt; luckySet.Length; i++)<br>        {<br>            was[luckySet[i]] = true;<br>        }<br>        for (int i = 1; i &lt;= 1000; i++)<br>        {<br>            for (int j = i + 1; j &lt;= 1000; j++)<br>            {<br>                if (n &gt;= i &amp;&amp; n &lt;= j)<br>                {<br>                    bool good = true;<br>                    for (int k = i; k &lt;= j; k++)<br>                    {<br>                        if (was[k]) good = false;<br>                    }<br>                    if (good) cc++;<br>                }<br>            }<br>        }<br>        return cc;<br><br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>