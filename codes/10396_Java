            public class UnluckyNumbers {<br> <br>  public int getCount(int[] luckySet, int n) {<br>    int max = 0;<br>    for (int i = 0; i &lt; luckySet.length; i++) {<br>      if (luckySet[i] &gt; max)<br>        max = luckySet[i];<br>    }<br>    <br>    int count = 0;<br>    for (int i = 1; i &lt;= max - 1; i++) {<br>      for (int j = i + 1; j &lt;= max; j++) {<br>        if (n &gt;= i &amp;&amp; n &lt;= j) {<br>          boolean isOk = true;<br>          for (int k = 0; k &lt; luckySet.length; k++) {<br>            if (luckySet[k] &gt;= i &amp;&amp; luckySet[k] &lt;= j)<br>              isOk = false;<br>          }<br>          <br>          if (isOk)<br>            count++;<br>        }<br>      }<br>    }<br>    <br>    return count;<br>  }<br> <br>}<br></td>