            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class PouringWater<br>{<br>  public int getMinBottles(int N, int K)<br>  {<br>    int a = N;<br>    while(Integer.bitCount(a) &gt; K)<br>      a++;<br>    return a-N;<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>