            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class PlanarGraphShop<br>{<br>  public int bestCount(int n)<br>  {<br>    int p = 36;<br>    Dictionary&lt;int, int&gt; l = new Dictionary&lt;int, int&gt;();<br>    //l.Add(1, 0);<br>    l.Add(8, 0);<br>    l.Add(9, 0);    <br>    for(int i=1; i&lt;=p; i++)<br>    {<br>      int s = 3 * i - 6;<br>      for(int j=0; j&lt;=s; j++)<br>      {<br>        int x = i * i * i + j * j;<br>        l[x] = 0;<br>      }<br>    }<br>    int[] a = new int[n + 1];<br>    for(int i=0; i&lt;=n; i++)<br>      a[i] = i;<br>    int[] b = new int[l.Count];<br>    int idx = 0;<br>    foreach(int q in l.Keys)<br>    {<br>      b[idx] = q;<br>      idx ++;<br>    }<br>    //Console.WriteLine(b.Length);<br>    for(int i=0; i&lt;b.Length; i++)<br>    {<br>      for(int j=0; j+b[i]&lt;=n; j++)<br>      {<br>        a[j + b[i]] = Math.Min(a[j + b[i]], a[j] + 1);<br>      }<br>    }    <br>    return a[n];<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>