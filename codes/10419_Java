<td class="problemText" colspan="8" valign="middle" align="left">
            import java.util.*;<br><br>public class PrefixFreeCode {<br>  public int minCost( int n, int[] c ) {<br>    int k = c.length;<br>    Arrays.sort( c );<br>    int[][] r = new int[n + 1][k];<br>    for ( int i = 3; i &lt;= n; i ++ ) {<br>      Arrays.fill( r[i], 1000000 );<br>    }<br>    Arrays.fill( r[2], c[0] + c[1] );<br>    r[2][0] = 3 * c[0] + c[1];<br>    for ( int i = 3; i &lt;= n; i ++ ) {<br>      for ( int j = 1; j &lt; k; j ++ ) {<br>        r[i][j] = r[i][j - 1];<br>        r[i][j] = Math.min( r[i][j], c[0] + ( i - 1 ) * c[j] + r[i - 1][k - 1] );<br>        r[i][j] = Math.min( r[i][j], r[i - 1][j - 1] + c[j] );<br>        for ( int i1 = 2; i1 &lt; i - 1; i1 ++ ) {<br>          r[i][j] = Math.min( r[i][j], r[i1][j - 1] + ( i - i1 ) * c[j] + r[i - i1][k - 1] );<br>        }<br>      }<br>      r[i][0] = i * c[0] + r[i][k - 1]; <br>    }<br>    return r[n][k - 1];<br>  }<br>}<br></td>