            /*<br>  Program   :   1000<br>  Source    :   Topcoder Single Round 465 Division 2<br>  Author    :   czshs_yk<br>  Date      :   26/03/10<br>*/<br><br>#include &lt;iostream&gt;<br>#include &lt;cstdio&gt; <br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br>#include &lt;vector&gt;<br>#include &lt;queue&gt;<br>#include &lt;string&gt;<br>#include &lt;map&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;cmath&gt;<br>using namespace std;<br><br>struct WeirdTimes {<br>       vector &lt;int&gt; hourValues (vector &lt;int&gt; v, int k) {<br>              int mins[100], n = v.size ();<br>              mins[0] = 0;<br>              <br>              vector &lt;int&gt; w;<br>              w.clear (); w.push_back (-1);<br>              <br>              for (int i = 1; i &lt; n; i ++)<br>                  if (v[i] &gt; v[i - 1]) mins[i] = mins[i - 1]; <br>                  else mins[i] = mins[i - 1] + 1;<br>              if (mins[n - 1] &gt; 23) return w;<br>              long long s[100][30];<br>              memset (s, 0, sizeof (s));<br>              for (int i = mins[n - 1]; i &lt;= 23; i ++)<br>                  s[n - 1][i] = 1;<br>              for (int i = n - 1; i &gt;= 0; i --) {<br>                  for (int j = mins[i]; j &lt;= 23; j ++) {<br>                      int d = (v[i + 1] &gt; v[i] ? 0 : 1);<br>                      for (int k = j + d; k &lt;= 23; k ++)<br>                          s[i][j] += s[i + 1][k];<br>                  }<br>              }<br>              <br>              vector &lt;int&gt; h;<br>              h.clear ();<br>              long long cnt = 0, K = k;<br>              for (int i = 0; i &lt; n; i ++) {<br>                  int j = mins[i];<br>                  if (i) {<br>                     int d = (v[i] &gt; v[i - 1] ? 0 : 1);<br>                     j = h[i - 1] + d;<br>                  }<br>                  for (; j &lt;= 23; j ++) {<br>                      cnt += s[i][j];<br>                      if (cnt &gt;= K) break;<br>                  }<br>                  if (cnt &lt; K) return w;<br>                  cnt -= s[i][j];<br>                  h.push_back (j);<br>              }<br>              return h;<br>       }<br>};<br></td>