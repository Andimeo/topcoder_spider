<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>using System.Text.RegularExpressions;<br>public class WordSpaces {<br>public int[] find(string sentence,string[] words) {<br>int[] ret=new int[words.Length];<br>for(int i=0;i&lt;ret.Length;i++)ret[i]=sentence.Length+1;<br>for(int i=0;i&lt;words.Length;i++) <br> for(int grb=0;grb&lt;sentence.Length;grb++)<br> {<br>  string pattern="";<br>  for(int l=0;l&lt;words[i].Length-1;l++)<br>   pattern+=words[i][l]+".{"+grb+"}";<br>  pattern+=words[i][words[i].Length-1];<br>  Regex regx=new Regex(pattern);<br>  if(regx.IsMatch(sentence)) {ret[i]=Math.Min(ret[i],regx.Match(sentence).Index);}<br> }<br>for(int i=0;i&lt;ret.Length;i++)<br> if(ret[i]&gt;sentence.Length)ret[i]=-1;<br>return ret;<br>}  <br>}<br></td>