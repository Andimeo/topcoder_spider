            using System; <br>using System.Collections; <br>using System.Collections.Generic; <br>using System.Collections.Specialized; <br>using System.Text; <br>using System.Text.RegularExpressions; <br> <br>public class BoredPhilosophers {<br>    public int[] simulate(string[] text, int N) <br>    {<br>        string str = string.Join("", text);<br>        string[] c = str.Split(" ".ToCharArray(), StringSplitOptions.RemoveEmptyEntries);<br>        int[] ret = new int[N];<br>        for (int i = 0; i &lt; N; i++)<br>        {<br>            Dictionary&lt;string, bool&gt; d = new Dictionary&lt;string, bool&gt;();<br>            for (int j = 0; j + i &lt; c.Length; j++)<br>            {<br>                str = string.Join(" ", c, j, i + 1);<br>                if (!d.ContainsKey(str))<br>                    d[str] = true;<br>            }<br>            ret[i] = d.Keys.Count;<br>        }<br>        return ret;<br>    }<br><br><br>} <br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>