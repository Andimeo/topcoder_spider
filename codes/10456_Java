            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class BoredPhilosophers<br>{<br>  public int[] simulate(String[] text, int N)<br>  {<br>    HashSet&lt;String&gt; set = new HashSet&lt;String&gt;();<br>    <br>    int[] ret = new  int[N];<br>    <br>    String tt = "";<br>    <br>    for(int i = 0; i &lt; text.length; i++)<br>      tt += text[i];<br>      <br>    String[] spl = tt.trim().split(" ");<br>    <br>    for(int i = 1; i &lt;= N; i++)<br>    {<br>      set.clear();<br>    <br>      for(int j = 0; j + i -1 &lt; spl.length; j++)<br>      {<br>        String curr = "";<br>        for(int k = j; k &lt; j+ i; k++)<br>        {<br>          curr += spl[k] + " ";<br>        }<br>        <br>        set.add(curr);<br>      }  <br>    <br>      ret[i-1] = set.size();<br>    }<br>    <br>    return ret;<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>