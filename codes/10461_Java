            import java.util.*;<br>import java.util.regex.*;<br>import static java.lang.Math.*;<br>import static java.util.Arrays.*;<br>import static java.lang.Integer.*;<br>import static java.lang.Double.*;<br>import static java.util.Collections.*;<br><br>public class ToolsBox {<br>    public int countTools(String[] need) {<br>        int res;<br>        HashSet&lt;String&gt; set = new HashSet&lt;String&gt;();<br>        for (int i = 0; i &lt; need.length; i++) {<br>            String[]a = need[i].split(" ");<br>            for (int j = 0; j &lt; a.length; j++) {<br>                set.add(a[j]);<br>            }<br><br>        }<br>        return set.size();<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>