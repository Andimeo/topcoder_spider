            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>public class SimpleWordGame {<br>  public int points(string[] player, string[] dictionary) {<br>    int ret=0;<br>        List&lt;string&gt; d = new List&lt;string&gt;(dictionary);<br>        List&lt;string&gt; done = new List&lt;string&gt;();<br>        foreach (string s in player)<br>        {<br>            if (done.Contains(s)) continue;<br>            done.Add(s);<br>            if (d.Contains(s)) ret += s.Length * s.Length;<br>        }<br>        return ret;<br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>