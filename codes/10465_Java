            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class SimpleWordGame<br>{<br>  public int points(String[] player, String[] dictionary)<br>  {<br>    Set&lt;String&gt; aaa = new TreeSet&lt;String&gt;();<br>    for(String s:player) aaa.add(s);<br>    int ans=0;<br>    for(String s:dictionary)<br>    {<br>      if (aaa.contains(s))ans+=s.length()*s.length();<br>    }    <br>    return ans;<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>