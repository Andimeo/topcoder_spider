<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class PaperAndPaintEasy<br>{<br>    public long computeArea(int width, int height, int xfold, int cnt, int x1, int y1, int x2, int y2)<br>    {<br>        int layed = Math.Min(xfold, width - xfold);<br>        int rectWidth = Math.Max(xfold, width - xfold);<br>        int rectHeight = height / (cnt + 1);<br>        x2 -= 1;<br>        y2 -= 1;<br>        if (x2 &gt;= rectWidth) x2 = rectWidth - 1;<br>        if (y2 &gt;= rectHeight) y2 = rectHeight - 1;<br>        int hcount = y2 - y1 + 1;<br>        long sum = 0;<br>        if (x1 &lt; layed &amp;&amp; x2&lt;layed)<br>        {<br>            sum += (long)((long)cnt + 1) * hcount * 2 * ((long)x2 - x1 + 1);<br>        }<br>        else if (x1 &lt; layed &amp;&amp; x2 &gt;= layed)<br>        {<br>            sum += (long)((long)cnt + 1) * hcount * 2 * ((long)layed - x1) + (long)((long)cnt + 1) * hcount * ((long)x2 - layed + 1);<br>        }<br>        else<br>        {<br>            sum += (long)((long)cnt + 1) * hcount * ((long)x2 - x1 + 1);<br>        }<br>        return (long)width*height- sum;<br>    }<br>}<br></td>