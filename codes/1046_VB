            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class StringMult <br>{<br>    public string times(string sFactor, int iFactor) <br>  {<br>    string ret = "";<br>    if (sFactor == "" || iFactor == 0)<br>      return "";<br>    if (iFactor &gt; 0)<br>    {<br>      for (int i = 0; i &lt; iFactor; i++)<br>        ret += sFactor;<br>    }<br>    else<br>    {<br>      string rev = "";<br>      for (int i = sFactor.Length - 1; i &gt;= 0; i--)<br>        rev += sFactor[i];<br>      for (int i = 0; i &lt; -iFactor; i++)<br>        ret += rev;<br>    }<br>    return ret;<br>    }<br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>