            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;bitset&gt;<br>using namespace std;<br>#include &lt;string.h&gt;<br>#include &lt;stdio.h&gt;<br><br>#ifdef _WIN32<br>#define longlong __int64<br>#else<br>#define longlong long long<br>#endif<br><br>class Lexer<br>{<br>public:<br>  vector&lt;string&gt; tokenize(vector&lt;string&gt; tokens, string input)<br>  {<br>    vector&lt;string&gt; res;<br><br>    for(int i = 0; i &lt; input.size(); )<br>    {<br>      int longest = 0;<br>      for(int j = 0; j &lt; tokens.size(); j++)<br>      {<br>        if(input.compare(i, tokens[j].size(), tokens[j]) == 0)<br>        {<br>          if(tokens[j].size() &gt; longest) longest = tokens[j].size();<br>        }<br>      }<br><br>      if(longest == 0)<br>      {<br>        i++;<br>      }<br>      else<br>      {<br>        res.push_back(input.substr(i, longest));<br>        i += longest;<br>      }<br>    }<br><br>    return res;<br>  }<br>};<br><br><br><br><br>// Powered by FileEdit<br></td>