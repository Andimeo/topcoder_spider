            /*/*///*/*/ Powered by radedit!            /*/*/*/*/*/ ;<br>/*/*//*/*/  import java . util .           /*/*/*/*/*/ ;<br>/*/*//*/*/  import java . lang . reflect . /*/*/*/*/*/ ;<br>public class Lexer {<br>   boolean ddd = false ;<br>  public String[] tokenize(String[] tokens, String input) {<br>    int i, j, k ;<br>    i = 0 ;<br>    ArrayList r = new ArrayList() ;<br>    while (i &lt; input.length()) {<br>      int bs = 0 ;<br>      int bt = -1 ;<br>      String ss = input.substring(i) ;<br>      for (j=0; j&lt;tokens.length; j++) {<br>  if (ss.startsWith(tokens[j]) &amp;&amp; tokens[j].length() &gt; bs) {<br>    bs = tokens[j].length() ;<br>    bt = j ;<br>  }<br>      }<br>      if (bt &lt; 0) {<br>  i++ ;<br>      } else {<br>  r.add(tokens[bt]) ;<br>  i += bs ;<br>      }<br>    }<br>    return (String[])r.toArray(new String[0]) ;<br>  }<br>}<br></td>