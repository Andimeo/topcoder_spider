            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class FourBlocksEasy<br>{<br>  public int maxScore(string[] grid)<br>  {<br>        int score = 0; int s = grid[0].Length;<br>        for (int i = 0; i &lt; s; i++)<br>        {<br>            if ((i != s - 1) &amp;&amp; (grid[0][i] == '.') &amp;&amp; (grid[0][i + 1] == '.') &amp;&amp; (grid[1][i] == '.') &amp;&amp; (grid[1][i + 1] == '.'))<br>            {<br>                score += 16;<br>                i++;<br>            }<br>            else score += 2;<br>        }<br>        return score;<br>  }<br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>