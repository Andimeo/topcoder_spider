            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class FourBlocksEasy {<br>public:<br>  int maxScore(vector &lt;string&gt;);<br>};<br> <br>int FourBlocksEasy::maxScore(vector &lt;string&gt; g) {<br>  int ans=0;<br>  for(int i=0;i&lt;g[0].size();i++)<br>  {<br>    if(g[0][i]=='.' &amp;&amp; g[0][i+1]=='.' &amp;&amp; g[1][i]=='.' &amp;&amp; g[1][i+1]=='.')<br>    {ans+=16;i++;}<br>    else<br>    ans+=2;<br>  }<br>  return ans;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>