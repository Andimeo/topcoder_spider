            import java.util.*;<br>import java.io.*;<br>import static java.lang.Math.*;<br>import static java.util.Arrays.*;<br> <br>public class FourBlocksEasy {<br>  public int maxScore(String[] grid) {<br>    char[][] c = new char[grid.length][grid[0].length()];<br>    for (int i = 0; i &lt; grid.length; ++i) {<br>      c[i] = grid[i].toCharArray();<br>    }<br>    for (int i = 0; i &lt; c[0].length; ++i) {<br>      if (c[0][i] == '.') {<br>        c[0][i] = 'Z';<br>      }<br>      if (c[1][i] == '.') {<br>        c[1][i] = 'Z';<br>      }<br>      if (c[0][i] == '1' || c[1][i] == '1') {<br>        c[0][i] = '1';<br>        c[1][i] = '1';<br>      }<br>    }<br>    String t = new String(c[0]);<br> <br>    t = t.replaceAll("ZZ", "8");<br>    t = t.replaceAll("Z", "1");<br> <br>    int res = 0;<br>    for (int i = 0; i &lt; t.length(); ++i) {<br>      res += t.charAt(i) - '0';<br>    }<br>    return res * 2;<br>  }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>