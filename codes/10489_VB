            Public Class FourBlocksEasy<br>    Public Function maxScore(ByVal grid As String()) As Integer<br>        Dim i As Integer<br>        Dim iMax As Integer<br>        Dim iRet As Integer<br><br>        iRet = 0<br>        iMax = grid(0).Length<br>        For i = 1 To iMax<br>            If Mid(grid(0), i, 1) = "." And Mid(grid(1), i, 1) = "." Then<br>                If i &lt;&gt; iMax Then<br>                    If Mid(grid(0), i + 1, 1) = "." And Mid(grid(1), i + 1, 1) = "." Then<br>                        iRet = iRet + 16<br>                        i = i + 1<br>                    Else<br>                        iRet = iRet + 2<br>                    End If<br><br>                Else<br>                    iRet = iRet + 2<br><br>                End If<br>            Else<br>                iRet = iRet + 2<br>            End If<br>        Next<br><br>        Return iRet<br><br>    End Function<br>End Class<br></td>