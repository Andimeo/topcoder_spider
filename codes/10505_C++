            #include &lt;vector&gt;<br>#include &lt;fstream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;set&gt;<br>#include &lt;string&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;iostream&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;map&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cmath&gt;<br>#include &lt;queue&gt;<br>#include &lt;numeric&gt;<br>#include &lt;cstdio&gt;<br> <br>using namespace std;<br> <br>#define fe(i,a,n) for(int i = a, _n = n; i &lt; _n; i++)<br>#define fi(i,a,n) for(int i = a, _n = n; i &lt;= _n; i++)<br>#define LL long long<br> <br>class SoccerLeagues {<br>  public:<br>    vector &lt;int&gt; points(vector &lt;string&gt; matches) {<br>      int n = matches.size();<br>      vector&lt;int&gt; res(n, 0);<br>      fe(i,0,n) {<br>        fe(j,0,n) { if(i == j) continue;<br>          if(matches[i][j] == 'W')<br>            res[i] += 3;<br>          else if(matches[i][j] == 'D') {<br>            res[j]++;<br>            res[i]++;<br>          }<br>          else {<br>            res[j] += 3;<br>          }<br>        }<br>      }<br>      return res;<br>    }<br>       <br> <br>};<br> <br> <br> <br> <br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>