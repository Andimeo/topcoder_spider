            import java.util.*;<br>public class SoccerLeagues {<br>  public int[] points(String[] matches) {<br>    int[] res = new int[matches.length];<br>    <br>    for (int i = 0; i &lt; matches.length; i++) {<br>      <br>      for (int j = 0; j &lt; matches[i].length(); j++) {<br>        <br>        int ch = matches[i].charAt(j);<br>        if (ch == 'W') {<br>          res[i] += 3;<br>        } else if (ch == 'D') {<br>          res[i]++;<br>          res[j]++;<br>        } else if (ch == 'L') {<br>          res[j] += 3;<br>        }<br>        <br>      }<br>    }<br>    <br>    return res;<br>  }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>