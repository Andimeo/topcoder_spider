            Class SoccerLeagues<br>    Public Function points(ByVal matches As String()) As Integer()<br>        Dim n As Integer = matches.Length<br>        Dim res As Integer() = New Integer(n - 1) {}<br>        For j As Integer = 0 To n - 1<br>            For i As Integer = 0 To n - 1<br>                If i &lt;&gt; j Then<br>                    Select Case matches(i)(j)<br>                        Case "W" : res(i) += 3<br>                        Case "D" : res(i) += 1 : res(j) += 1<br>                        Case "L" : res(j) += 3<br>                    End Select<br>                End If<br>            Next<br>        Next<br>        Return res<br>    End Function<br>End Class<br></td>