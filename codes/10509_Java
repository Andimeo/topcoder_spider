            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class TheEncryptionDivTwo<br>{<br>  public String encrypt(String m)<br>  {<br>    HashMap&lt;Character, Character&gt; t = new HashMap&lt;Character, Character&gt;();<br>    char c = 'a';<br>    String ret = "";<br>    for(int i = 0; i &lt; m.length(); i++) {<br>      char p = m.charAt(i);<br>      if(t.get(p) != null) {<br>        ret += t.get(p);<br>      } else {<br>        ret += c;<br>        t.put(p, c);<br>        c = (char)(c+1);<br>      }<br>    }<br>    return ret;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>