<td class="problemText" colspan="8" valign="middle" align="left">
            Class TheEncryptionDivTwo<br><br><br>    Public Function encrypt(ByVal message As String) As String<br>        Dim i As Integer<br>        Dim s As String<br>        Dim iCnt As Integer<br><br>        Dim k As New System.Collections.ArrayList<br>        iCnt = 0<br>        Dim strRet As String<br>        strRet = ""<br><br>        For i = 1 To message.Length<br>            s = Mid(message, i, 1)<br>            If k.Contains(s) = False Then<br>                k.Add(s)<br>                strRet = strRet + Chr(Asc("a") + iCnt)<br>                iCnt += 1<br>            Else<br>                strRet = strRet + Chr(Asc("a") + k.IndexOf(s))<br>            End If<br>        Next<br>        Return strRet<br>    End Function<br>End Class<br></td>