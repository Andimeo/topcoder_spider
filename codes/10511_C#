            using System; <br>using System.Collections; <br>using System.Collections.Generic; <br>using System.Collections.Specialized; <br>using System.Text; <br>using System.Text.RegularExpressions; <br> <br>public class TheNewHouseDivTwo {<br><br>    public int count(int[] x, int[] y) <br>    {<br>        int ret = 0;<br>        int n = x.Length;<br><br>        for (int i = -100; i &lt;= 100; i++)<br>        {<br>            for (int j = -100; j &lt;= 100; j++)<br>            {<br>                bool f1, f2, f3, f4;<br>                f1 = f2 = f3 = f4 = false;  <br>                for (int k = 0; k &lt; n; k++)<br>                {<br>                   // if (x[k] == i &amp;&amp; y[k] == j)<br>                   //     break;<br>                    if (x[k] == i &amp;&amp; y[k] &lt; j)<br>                        f1 = true;<br>                    if (x[k] == i &amp;&amp; y[k] &gt; j)<br>                        f2 = true;<br>                    if (y[k] == j &amp;&amp; x[k] &lt; i)<br>                        f3 = true;<br>                    if (y[k] == j &amp;&amp; x[k] &gt; i)<br>                        f4 = true;<br>                }<br>                if (f1 &amp;&amp; f2 &amp;&amp; f3 &amp;&amp; f4)<br>                    ret++;<br>            }<br>        }<br>        return ret;<br>    }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>