            public class TheNewHouseDivTwo {<br>  public boolean in(int x, int y){<br>    boolean a=false, b=false, c= false, d=false; <br>    for(int i=0; i&lt;X.length; i++){<br>      if(x==X[i]&amp;&amp;y&lt;Y[i])a=true;<br>      if(x==X[i]&amp;&amp;y&gt;Y[i])b=true;<br>      if(y==Y[i]&amp;&amp;x&lt;X[i])c=true;<br>      if(y==Y[i]&amp;&amp;x&gt;X[i])d=true;<br>    }<br>    return a&amp;b&amp;c&amp;d;<br>  }<br>  public int X[], Y[];<br>  public int count(int[] xx, int[] yy) {<br>    int res=0;<br>    X=xx; Y=yy;<br>    for(int x=-101; x&lt;102; x++){<br>      for(int y=-101; y&lt;102; y++){<br>        if(in(x, y))res++;<br>      }<br>    }<br>    return res;<br>  }<br> <br>}<br></td>