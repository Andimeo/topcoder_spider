            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br> <br>public class TheNewHouseDivOne<br>{<br>  public double distance (int[] x, int[] y, int k)<br>  {<br>    int n = x.Length;<br>    <br>    int[] dist = new int[n];<br>    <br>    int min = int.MaxValue;<br>    <br>    for (int i = -200; i &lt;= 200; ++i)<br>      for (int j = -200; j &lt;= 200; ++j)<br>    {<br>      for (int q = 0; q &lt; n; ++q)<br>        dist[q] = Math.Abs (2 * x[q] - i) + Math.Abs (2 * y[q] - j);<br>      Array.Sort (dist);<br>      <br>      min = Math.Min (min, dist[k - 1]);<br>    }<br>    <br>    return min / 2.0;<br>  }<br>  <br>  <br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>