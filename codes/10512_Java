            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class TheNewHouseDivOne<br>{<br>  public double distance(int[] x, int[] y, int k)<br>  {<br>    Double ans = Double.POSITIVE_INFINITY;<br>    double[] dist = new double[x.length];<br>    for (int x0 = -200; x0 &lt;= 200; ++x0) {<br>      for (int y0 = -200; y0 &lt;= 200; ++y0) {<br>        for (int i = 0; i &lt; x.length; ++i) {<br>          dist[i] = Math.abs(x0 / 4. - x[i]) + Math.abs(y0 / 4. - y[i]);<br>        }<br>        Arrays.sort(dist);<br>        ans = Math.min(ans, dist[k - 1]);<br>      }<br>    }<br>    return ans;<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>