            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br> <br>public class TheLockDivOne<br>{<br>  string max (string s1, string s2)<br>  {<br>    if (string.Compare (s1, s2) &lt; 0)<br>      return s2;<br>    <br>    return s1;<br>  }<br>  <br>  string g (int n, long k)<br>  {<br>    if (n == 1)<br>      return k.ToString ();<br>    <br>    long nn2 = 1L &lt;&lt; (n - 1);<br>    <br>    if (k &lt; nn2)<br>      return "0" + g (n - 1, k);<br>    <br>    if (k == nn2)<br>      return "1" + g (n - 1, nn2 - 1);<br>    <br>    return "1" + g (n - 1, k - nn2 - 1);<br>  }<br>  <br>  string f (int n, long k)<br>  {<br>    if (n == 1)<br>      return k.ToString ();<br>    <br>    long nn2 = 1L &lt;&lt; (n - 1);<br>    <br>    if (k &lt; nn2)<br>      return "0" + f (n - 1, k);<br>    <br>    if (k == nn2)<br>      return "1" + g (n - 1, nn2 - 1);<br>    <br>    return "1" + max (g (n - 1, nn2 - 1), f (n - 1, k - nn2 - 1));<br>  }<br>  <br>  public string password (int n, long k)<br>  {<br>    return f (n, k - 1);<br>  }<br>  <br>  <br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>