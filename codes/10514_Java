            public class TheLockDivOne {<br>    private String go(int n, long k) {<br>        if (n == 0)<br>            return "";<br>        if (k &lt;= 1L &lt;&lt; (n - 1))<br>            return "0" + go(n - 1, k);<br>        if (k == (1L &lt;&lt; (n - 1)) + 1)<br>            return "1" + go(n - 1, 1L &lt;&lt; (n - 1));<br>        return "1" + go(n - 1, k - 1 - (1L &lt;&lt; (n - 1)));<br>    }<br><br>  public String password(int n, long k) {<br>    String ans = "";<br>        if (n == 0)<br>            return "";<br>        if (k &lt;= 1L &lt;&lt; (n - 1))<br>            return "0" + password(n - 1, k);<br>        ans = go(n, (1L &lt;&lt; (n - 1)) + 1);<br>        if (k != (1L &lt;&lt; (n - 1)) + 1) {<br>            String cand = "1" + password(n - 1, k - 1 - (1L &lt;&lt; (n - 1)));<br>            if (cand.compareTo(ans) &gt; 0)<br>                ans = cand;<br>        }<br>        return ans;<br>  }<br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by moj 4.12 [modified TZTester]<br>// Powered by CodeProcessor<br></td>