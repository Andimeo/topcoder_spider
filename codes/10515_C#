            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class CubeWalking<br>{<br>  public string finalPosition(string movement)<br>  {<br>        int[][] a = new int[3][];<br>        for (int i = 0; i &lt; 3; i++)<br>            a[i] = new int[3];<br>        a[0][0] = a[0][2] = a[2][0] = a[2][2] = 1;<br>        a[1][1] = 2;<br>        int[] dx = { 1, 0, -1, 0 };<br>        int[] dy = { 0, -1, 0, 1 };<br>        int x, y;<br>        x = 1;<br>        y = 1;<br>        int z = 0;<br>        for (int i = 0; i &lt; movement.Length; i++)<br>        {<br>            char what = movement[i];<br>            if (what == 'L')<br>            {<br>                z++;<br>                if (z == 4)<br>                    z = 0;<br>            }<br>            else if (what == 'R')<br>            {<br>                z--;<br>                if (z == -1)<br>                    z = 3;<br>            }<br>            else<br>            {<br>                x += dx[z];<br>                y += dy[z];<br>                if (x == -1)<br>                    x = 2;<br>                if (x == 3)<br>                    x = 0;<br>                if (y == -1)<br>                    y = 2;<br>                if (y == 3)<br>                    y = 0;<br>            }<br>        }<br>        int ans = a[x][y];<br>        if (ans == 0)<br>            return "BLUE";<br>        else if (ans == 1)<br>            return "RED";<br>        else<br>            return "GREEN";<br> <br>  }<br> <br>    <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>