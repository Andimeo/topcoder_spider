            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class CubeWalking<br>{<br>  public String finalPosition(String movement)<br>  {<br>    int currX = 1, currY = 1, dir = 0;<br>    for(int i = 0; i &lt; movement.length(); i++)  {<br>      if(movement.charAt(i) == 'L')  {<br>        dir--;<br>        dir += 4;<br>        dir %= 4;<br>      }<br>      else if(movement.charAt(i) == 'R')  {<br>        dir++;<br>        dir %= 4;<br>      }<br>      else  {<br>        if(dir == 0)  {<br>          currY--;<br>          currY += 3;<br>          currY %= 3;<br>        }<br>        else if(dir == 1)  {<br>          currX++;<br>          currX %= 3;<br>        }<br>        else if(dir==2)  {<br>          currY++;<br>          currY %= 3;<br>        }<br>        else if(dir==3)  {<br>          currX--;<br>          currX+=3;<br>          currX%=3;<br>        }<br>      }<br>    }<br>    if(currX == 1 &amp;&amp; currY == 1)  {<br>      return "GREEN";<br>    }<br>    else if(Math.abs(currX-currY) == 1)  {<br>      return "BLUE";<br>    }<br>    else  {<br>      return "RED";<br>    }<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>