            using System;<br>using System.Collections.Generic;<br> <br>public class StrangeComputer {<br>  public int setMemory(string mem) {<br>    int res = 0;<br>    char bit = '0';<br>    for (int i = 0; i &lt; mem.Length; ++i)<br>    {<br>      if (mem[i] != bit)<br>      {<br>        ++res;<br>        bit = mem[i];<br>      }<br>    }<br>    return res;<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>