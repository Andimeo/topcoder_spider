            public class StrangeComputer<br>{<br>  int ret;<br><br>  public int setMemory(String mem) {<br>    String machine = "";<br>    char[] cad = new char[mem.length()];<br>    <br>    for (int i = 0; i &lt; mem.length(); i++) {<br>      machine += '0';<br>    }<br>  <br>    <br>    while (!machine.equals(mem)) {<br>      int p = 0;<br>      while (mem.charAt(p) == machine.charAt(p)) {<br>        cad[p] = mem.charAt(p);<br>        p++;<br>      }<br>      <br>      char c = mem.charAt(p);<br>      for (int i = p; i &lt; machine.length();i++) {<br>        cad[i] = c; <br>      }<br>      machine = new String(cad);<br>      <br>      ret++;<br>    }<br>    <br>    return ret;<br><br>  }<br><br><br>} // Powered by ExampleBuilder<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>