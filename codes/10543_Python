            #import math,string,itertools,fractions,heapq,collections,re,array,bisect<br> <br> <br> <br>INF = 2147483647<br> <br>class EmoticonsDiv1:<br>    def printSmiles(self, smiles):<br>        quickest = [INF] * 1010<br>        quickest[1] = 0<br>        for i in range(20):<br>            for k in range(1, len(quickest)):<br>                quickest[k-1] = min(quickest[k-1], quickest[k]+1)<br>                for m in range(2,20):<br>                    if k*m &lt; len(quickest):<br>                        quickest[k*m] = min(quickest[k*m], quickest[k]+1+m-1)<br> <br>        return quickest[smiles]<br></td>