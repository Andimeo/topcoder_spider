<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>  using System.Collections.Generic;<br>  using System.Text;<br>  public class MountainRoad<br>  {<br>    public double findDistance(int[] start, int[] finish)<br>    {<br>      int min = 100000;<br>      foreach (int v in start)<br>      {<br>        if (v &lt; min) min = v;<br>      }<br>      int max = -100000;<br>      foreach (int v in finish)<br>      {<br>        if (v &gt; max) max = v;<br>      }<br><br>      double m = min;<br>      double x = max;<br>      return (x - m) * Math.Sqrt(2);<br>    }}<br></td>