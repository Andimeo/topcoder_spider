            import java.util.*;<br><br>public class MountainRoad {<br>  public double findDistance(int[] start, int[] finish) {<br>        int min, max;<br>        min = start[0];<br>        max = finish[0];<br>        for (int i = 0; i &lt; start.length; i++) {<br>            min = Math.min(min, start[i]);<br>            max = Math.max(max, finish[i]);<br>        }<br><br>    return (max - min) * Math.sqrt(2);<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>