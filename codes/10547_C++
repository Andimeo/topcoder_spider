cond                 second<br><br>typedef vector&lt;int&gt;            VI;<br>typedef vector&lt; vector&lt;int&gt; &gt;  VII;<br>typedef vector&lt;string&gt;         VS;<br>typedef unsigned long long     ULL;<br>typedef long long              LL;<br>typedef pair&lt;int,int&gt;          PI;<br>typedef unsigned long          UL;<br><br>class OddDivisors<br>{<br>  public : long long findSum(int N)<br>  {<br>    long long Sum=0LL;<br>    while(N) {<br>      Sum=Sum+Odd(N);<br>      N/=2;<br>    }<br>    return Sum;<br>  }<br>  public : long long Odd(int N)<br>  {<br>    if(N%2==1) {<br>      return (long long)((N+1)/2)*((N+1)/2);<br>    }<br>    else {<br>      return (long long)(N/2)*(N/2);<br>    }<br>  }<br>};<br></td>