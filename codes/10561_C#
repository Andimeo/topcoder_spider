            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class NotTwo<br>{<br>  public int maxStones(int w, int h)<br>  {<br>    int ret = 0;<br>    for(int i=0; i&lt;w; i++)<br>    {<br>      for(int j=0; j&lt;h; j++)<br>      {<br>        int v = i / 2 + j / 2;<br>        if(v % 2 == 0)<br>          ret ++;<br>      }<br>    }<br>    return ret;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>