            import static java.lang.Math.*;<br>import java.util.*;<br><br>public class CutSticks {<br><br>  public double maxKth(int[] sticks, int C, int K) {<br>    double low = 0.0;<br>    double high = Long.MAX_VALUE;<br>    for(int i = 0; i &lt; 100;i++)<br>    {<br>      double mid = (low+high)/2;<br>      if(pos(mid,sticks,C,K))<br>      {<br>        low = mid;<br>      }else{<br>        high = mid;<br>      }<br>    }<br>    return low;<br>  }<br>  private boolean pos(double mid, int[] S, long c, long k) {<br>    long num = 0;<br>    long used = 0;<br>    for(int a:S)<br>    {<br>      long u = (long)(a/mid);<br>      if(u &gt; 0)<br>      {<br>        if(used + u-1 &gt; c)<br>          u = c-used+1;<br>        used += u-1;<br>        num += u;<br>      }<br>    }<br>    <br>    return num &gt;=k;<br>  }<br>  public void p(Object s){System.out.println(s);}<br>}<br></td>