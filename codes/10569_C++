            #include &lt;cmath&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cassert&gt;<br> <br>#include &lt;algorithm&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;functional&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br> <br>using namespace std;<br> <br>#define forn(i, n) for (int i = 0; i &lt; (int)(n); i++)<br>#define forit(i, a) for (__typeof((a).begin()) i = (a).begin(); i != (a).end(); i++)<br>#define sz(a) (int)(a).size()<br>#define all(a) (a).begin(), (a).end()<br>#define pb push_back<br>#define mp make_pair<br> <br>typedef long long ll;<br>typedef vector &lt;int&gt; vi;<br>typedef pair &lt;int, int&gt; pii;<br> <br>const int maxn = 200010;<br>const int maxk = 53;<br> <br>ll f[maxn];<br>int b[maxk];<br> <br>struct NewCoins <br>{<br>  int minCoins(vector &lt;int&gt; a)<br>  {<br>    int n = sz(a);<br>    <br>    ll res = (ll)1e18;<br>    memset(f, 0x30, sizeof(f));<br>    f[1] = 0;<br>    for (int x = 1; x &lt; maxn; x++)<br>    {<br>      if (x &gt; (int)1e5)<br>        res = min(res, f[x]);<br>      forn(i, n)<br>        b[i] = a[i] / x;<br>      for (int y = 2; x * y &lt; maxn; y++)<br>      {<br>        int z = x * y;<br>        ll sum = 0;<br>        forn(i, n)<br>          sum += b[i] % y;<br>        f[z] = min(f[z], f[x] + sum);<br>      }<br>    }<br>    return res;<br>  }<br>};<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>