            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class NewCoins<br>{<br>  public int minCoins(int[] a)<br>  {<br>    int n = a.length;<br>    int[] din = new int[100001];<br>    for (int i = 100000; i &gt;= 1; --i) {<br>      for (int j = 0; j &lt; n; ++j) {<br>        din[i] += a[j] / i;<br>      }<br>      for (int t = 2 * i; t &lt;= 100000; t += i) {<br>        int cur = din[t];<br>        for (int j = 0; j &lt; n; ++j) {<br>          cur += (a[j] % t) / i;<br>        }<br>        din[i] = Math.min(din[i], cur);<br>      }<br>    }<br>    return din[1];<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>