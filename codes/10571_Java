            import java.util.*;<br> <br>public class ModuloFourDivisor {<br>    boolean contains(long[] x, long y) {<br>        for (long z : x)<br>            if (z == y)<br>                return true;<br>        return false;<br>    }<br> <br>    public int countQuadruplets(long[] A, long[] B, long[] C, long[] D) {<br>        int res = 0;<br>        for (long b : B) {<br>            if (b == 0)<br>                continue;<br>            for (long d: D) {<br>                long g = gcd(b, d);<br>                long b1 = b / g;<br>                long d1 = d / g;<br>                if (b1 != d1 &amp;&amp; b1 != d1 + 1)<br>                    continue;<br>                long z = g * (b1 + d1);<br>                if (contains(A, 0) &amp;&amp; contains(C, 0)) {<br>                    ++res;<br>                }<br>                if (contains(C, z)) {<br>                    for (long a : A)<br>                        if (a % z == 0)<br>                            ++res;<br>                }<br>            }<br>        }<br>        return res;<br>    }<br> <br>    private long gcd(long x, long y) {<br>        while (y &gt; 0) {<br>            long t = x % y;<br>            x = y;<br>            y = t;<br>        }<br>        return x;<br>    }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>