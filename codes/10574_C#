            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class SoldierLabeling <br>{<br>  public int count(int n, int lowerBound, int upperBound) <br>  {<br>        int a = (int)Math.Pow(10, lowerBound - 1);<br>        int b = (int)Math.Min(Math.Pow(10, upperBound) - 1,n);<br>        if ((b - a + 1) &lt; 0)<br>            return 0;<br>        return b - a + 1;<br>  }<br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by r1cs1]<br>// Powered by CodeProcessor<br></td>