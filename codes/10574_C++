            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;queue&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt; <br> <br>using namespace std;<br> <br>class SoldierLabeling { <br>        public: <br>        int count(int n, int l, int u) { <br>      int ret = 0;<br>      for (int i=1; i&lt;=n; i++) {<br>        int length = 0, k = i;<br>        while (k&gt;0) {<br>          length++;<br>          k /= 10;<br>        }<br>        if (length&gt;=l &amp;&amp; length&lt;=u) ret++;<br>      }<br>      return ret;          <br>        } <br>        <br> <br>}; <br> <br> <br> <br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>