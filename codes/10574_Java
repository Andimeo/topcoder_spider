            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class SoldierLabeling<br>{<br>  public int count(int n, int lowerBound, int upperBound)<br>  {<br>    int c = 0;<br>    for(int i = 1; i &lt;= n; i++) {<br>      String s = Integer.toString(i);<br>      if(s.length() &gt;= lowerBound &amp;&amp; s.length() &lt;= upperBound)<br>        c++;<br>    }<br>    return c;<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>