            #include &lt;map&gt;<br>#include &lt;deque&gt;<br>#include &lt;queue&gt;<br>#include &lt;stack&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;vector&gt;<br>#include &lt;set&gt;<br>#include &lt;complex&gt;<br>using namespace std;<br>#define rep(i,m) for(int i=0;i&lt;m;i++)<br>#define mem(a,b) memset(a,b,sizeof(a))<br>#define mp make_pair<br>typedef pair&lt;int,int&gt; pii;<br>int oo=(int)1e9;<br>int di[]={-2,-2,-1,1,2,2,1,-1};<br>int dj[]={-1,1,2,2,1,-1,-2,-2};<br>bool vis[8][8];<br> <br>class KnightsTour {<br>public:<br>  int visitedPositions(vector &lt;string&gt; b) {<br>        int i=0,j=0;<br>        rep(ii,8)rep(jj,8)if(b[ii][jj]=='K')i=ii,j=jj;<br>    mem(vis,0);<br>    int ret=0;<br>    while(1){<br>            ret++;<br>            vis[i][j]=1;<br>            pair&lt;int,pii&gt; mn=mp(oo,mp(oo,oo));<br>            rep(k,8){<br>                int ni=i+di[k];<br>                int nj=j+dj[k];<br>                if(ni&gt;=0 &amp;&amp; nj&gt;=0 &amp;&amp; ni&lt;8 &amp;&amp; nj&lt;8 &amp;&amp; b[ni][nj]!='*' &amp;&amp; !vis[ni][nj]){<br>                    pair&lt;int,pii&gt; p=mp(-1,mp(ni,nj));<br>                    rep(k2,8){<br>                        int ni2=ni+di[k2];<br>                        int nj2=nj+dj[k2];<br>                        if(ni2&gt;=0 &amp;&amp; nj2&gt;=0 &amp;&amp; ni2&lt;8 &amp;&amp; nj2&lt;8 &amp;&amp; b[ni2][nj2]!='*' &amp;&amp; !vis[ni2][nj2])<br>                            p.first++;<br>                    }<br>                    mn=min(mn,p);<br>                }<br>            }<br>            if(mn.first==oo)<br>                break;<br>            i=mn.second.first;<br>            j=mn.second.second;<br>        }<br>        return ret;<br>  }<br>};<br> <br> <br> <br> <br>//Powered by KawigiEdit 2.1.8 (beta) modified by pivanof!<br></td>