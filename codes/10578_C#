            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class ImportantTasks<br>{<br>  public int maximalCost(int[] complexity, int[] computers)<br>  {<br>        Array.Sort(complexity);<br>        Array.Sort(computers);<br> <br>        int Cnt = 0;<br>        int i = 0;<br>        int index = 0;<br>        for (; i &lt; computers.Length &amp;&amp; index&lt;complexity.Length;)<br>        {<br>            if (computers[i] &gt;= complexity[index])<br>            {<br>                Cnt++;<br>                i++;<br>                index++;<br>            }<br>            else<br>                i++;<br>        }<br> <br>        return Cnt;<br> <br>  }<br> <br>    //<br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>