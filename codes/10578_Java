            import java.util.*;<br>import static java.lang.Math.*;<br> <br>public class ImportantTasks {<br>    public int maximalCost(int[] cost, int[] comp) {<br>      Arrays.sort(cost);<br>      Arrays.sort(comp);<br>      int r = 0;<br>      for (int i = 0, j = 0; i &lt; cost.length &amp;&amp; j &lt; comp.length;i++, j++) {<br>        if(cost[i] &lt;= comp[j])<br>          r++;<br>        else<br>          i--;<br>    }<br>      return r;<br>    }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>