            #include &lt;stdio.h&gt;<br>#include &lt;string.h&gt;<br>#include &lt;stdlib.h&gt;<br>#include &lt;stdbool.h&gt;<br>#include &lt;stdint.h&gt;<br>#include &lt;limits.h&gt;<br> <br>#include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;algorithm&gt;<br>using namespace std;<br> <br>struct ImportsList {<br>  vector &lt;int&gt; importsCount(vector &lt;string&gt; R) {<br>    vector&lt;int&gt; ans;<br>    int i, j, k, n = R.size(), c;<br> <br>    for (i = 0; i != n; ++i) {<br>      c = 0;<br>      for (j = 0; j != n; ++j) <br>        if (R[i][j] == 'Y'){<br>          for (k = 0; k != n; ++k)<br>            if (R[i][k] == 'Y' &amp;&amp; R[k][j] == 'Y')<br>              break;<br>          if (k == n)<br>            ++c;<br>        }<br>      ans.push_back(c);<br>    }<br> <br>    return ans;<br>  }<br>};<br> <br> <br> <br> <br>// Powered by FileEdit<br></td>