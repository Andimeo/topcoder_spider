<td class="problemText" colspan="8" valign="middle" align="left">
            public class ImportsList {<br>  public int[] importsCount(String[] requires) {<br>    int N = requires.length;<br>    int[] r = new int[N];<br><br>    boolean[][] reqs = new boolean[N][N];<br>    for (int i = 0; i &lt; N; i++) {<br>      String a = requires[i];<br>      for (int j = 0; j &lt; N; j++) {<br>        if (a.charAt(j) == 'Y') {<br>          reqs[i][j] = true;<br>        }<br>      }<br>    }<br>    for (int k = 0; k &lt; N; k++) {<br>      for (int i = 0; i &lt; N; ++i) {<br>        for (int j = 0; j &lt; N; ++j) {<br>          if (reqs[i][j] &amp;&amp; reqs[i][k] &amp;&amp; reqs[k][j]) {<br>            reqs[i][j] = false;<br>          }<br>        }<br>      }<br>    }<br><br>    for (int i = 0; i &lt; N; i++) {<br>      int c = 0;<br>      for (int j = 0; j &lt; N; ++j) {<br>        if (reqs[i][j])<br>          c++;<br>      }<br>      r[i] = c;<br>    }<br><br>    return r;<br>  }<br>}<br></td>