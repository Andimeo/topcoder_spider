            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br> <br>using namespace std;<br> <br>class EggCartons {<br>  public:<br>  int minCartons(int n) {<br>    int ans = 1000000;<br>    for (int f = 0; f &lt; 100; f++) {<br>      for (int s = 0; s &lt; 100; s++) {<br>        if (f * 8 + s * 6 == n) {<br>          ans = min(ans, f + s);<br>        }<br>      }<br>    }<br>    if (ans == 1000000) return -1;<br>    return ans;<br>  }<br> <br>  <br> <br>};<br> <br> <br> <br> <br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>