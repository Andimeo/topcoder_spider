            import java.util.*;<br>public class EggCartons {<br>  public int minCartons(int n) {<br>    int res = 1000000;<br>    <br>    <br>    for (int i = 0; i &lt; 100; i++) {<br>      for (int j = 0; j &lt; 100; j++) {<br>        int m = i*6 + j * 8;<br>        if (m == n) {<br>          res = Math.min(res, i + j);<br>        }<br>      }<br>    }<br>    <br>    if (res == 1000000) {<br>      return -1;<br>    }<br>    <br>    return res;<br>  }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>