<td class="problemText" colspan="8" valign="middle" align="left">
            Public Class EggCartons<br>    Public Function minCartons(ByVal n As Integer) As Integer<br>        Dim a As Integer<br>        Dim b As Integer<br>        If n / 2 &lt;&gt; Int(n / 2) Then<br>            Return -1<br>        ElseIf n / 8 = Int(n / 8) Then<br>            Return n / 8<br>        Else<br>            a = -1<br>            Do<br>                n = n - 6<br>                b += 1<br>                If (n Mod 8) = 0 Then<br>                    a = (n \ 8) + b<br>                    Exit Do<br>                End If<br>            Loop Until n &lt; 6<br>            Return a<br>        End If<br>    End Function<br>End Class<br></td>