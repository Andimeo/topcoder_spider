            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class TextFrames {<br>public:<br>  vector &lt;string&gt; generateFrame(int, int, string, int, int, int, int);<br>};<br> <br>vector &lt;string&gt; TextFrames::generateFrame(int w, int h, string p, int x1, int y1, int x2, int y2) {<br>  int i, j, q;<br>  vector &lt;string&gt; ret;<br>  ret.clear();<br>  for (i=y1;i&lt;y2;i++) {<br>    string s = "";<br>    for (j=x1;j&lt;x2;j++) {<br>      if (i == 0) q = j; else<br>      if (j == w-1) q = w+i-1; else<br>      if (i == h-1) q = w+h-2+(w-j-1); else<br>      if (j == 0) q = w+h+w-3+(h-i-1); else {<br>        s += ".";<br>        continue;<br>      }<br>      q %= p.length()+1;<br>      if (q == p.length()) s += ".";<br>      else s += p[q];<br>    }<br>    ret.push_back(s);<br>  }<br>  return ret;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>