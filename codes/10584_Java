            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br>public class TextFrames {<br>  public String[] generateFrame(int width, int height, String phrase, int x1, int y1, int x2, int y2) {<br>    --y2;<br>    --x2;<br>    int tmp = x1;<br>    x1 = y1;<br>    y1 = tmp;<br>    tmp = x2;<br>    x2 = y2;<br>    y2 = tmp;<br>    char[][] grid = new char[x2 - x1 + 1][y2 - y1 + 1];<br>    for (int r = x1; r &lt;= x2; ++r) {<br>      for (int c = y1; c &lt;= y2; ++c) {<br>        grid[r - x1][c - y1] = getChar(r, c, height, width, phrase);<br>      }<br>    }<br>    String[] res = new String[grid.length];<br>    for (int i = 0; i &lt; grid.length; ++i) {<br>      res[i] = new String(grid[i]);<br>    }<br>    return res;<br>  }<br> <br>  private char getChar(int r, int c, int height, int width, String s) {<br>    s += ".";<br>    if ((r &gt; 0 &amp;&amp; r &lt; height - 1) &amp;&amp; (c &gt; 0 &amp;&amp; c &lt; width - 1)) {<br>      return '.';<br>    }<br>    if (r == 0 &amp;&amp; c &lt; width - 1) {<br>      return s.charAt(c % s.length());<br>    } else if (c == width - 1 &amp;&amp; r &lt; height - 1) {<br>      return s.charAt((width - 1 + r) % s.length());<br>    } else if (r == height - 1 &amp;&amp; c &gt; 0) {<br>      return s.charAt(((width - 1 + height - 1) + (width - 1 - c)) % s.length());<br>    } else if (c == 0 &amp;&amp; r &gt; 0) {<br>      return s.charAt(((width - 1 + height - 1 + width - 1) + (height - 1 - r)) % s.length());<br>    } else {<br>      throw null;<br>    }<br>  }<br> <br> <br>}<br>// Powered by KawigiEdit 2.1.8 (beta) modified by pivanof!<br></td>