            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class Archery<br>{<br>    public double expectedPoints(int N, int[] ringPoints)<br>    {<br>        double res = 0.0;<br><br>        int total = (N + 1) * (N + 1);<br><br>        for (int i = 0; i &lt; N + 1; i++)<br>        {<br>            res += ringPoints[i] * (i * 2 + 1);<br>        }<br><br>        res /= (double)total;<br>        <br>        return res;<br>    }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>