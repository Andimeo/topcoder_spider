            #include&lt;iostream&gt;<br>#include&lt;vector&gt;<br>#include&lt;list&gt;<br>#include&lt;map&gt;<br>#include&lt;set&gt;<br>#include&lt;algorithm&gt;<br>#include&lt;deque&gt;<br>#include&lt;stack&gt;<br>#include&lt;bitset&gt;<br>#include&lt;functional&gt;<br>#include&lt;numeric&gt;<br>#include&lt;utility&gt;<br>#include&lt;iomanip&gt;<br>#include&lt;cstdlib&gt;<br>#include&lt;ctime&gt;<br>#include&lt;queue&gt;<br>#include&lt;cmath&gt;<br>#include&lt;sstream&gt;<br>#include&lt;string&gt;<br>#include&lt;cstdio&gt;<br><br>using namespace std;<br><br>class Archery<br>{<br>  public:<br>  double expectedPoints(int n, vector &lt;int&gt; r)<br>  {<br>    double x=0;<br>    for(int i=0;i&lt;=n;i++)<br>    {<br>      double a=((i+1)*(i+1)-i*i)*1.0/((n+1)*(n+1));<br>      x+=a*r[i];<br>    }<br>    return x;<br>  }<br>};<br></td>