<td class="problemText" colspan="8" valign="middle" align="left">
            public class TriFibonacci<br>{<br>  public int complete(int[] A)<br>  {<br>    int i, x=-1, t;<br>    <br>    for(i=0; i&lt;A.length; i++)<br>    {<br>      if(A[i] == -1)<br>      {<br>        x = i;<br>        break;<br>      }<br>    }<br>    <br>    if(x == -1)<br>      return -1;<br>      <br>    if(x &lt; 3)<br>    {<br>      for(i=0, t=0; i&lt;3; i++)<br>        t += A[i];<br>      t += 1;<br>      A[x] = A[3]-t;<br>    }<br>    else<br>      A[x] = A[x-1]+A[x-2]+A[x-3];<br>    <br>    if(A[x] &lt;= 0)<br>      return -1;<br>    <br>    for(i=3; i&lt;A.length; i++)<br>    {<br>      if(A[i] != A[i-1]+A[i-2]+A[i-3])<br>        return -1;<br>    }<br>    <br>    return A[x];<br>  }<br>}<br></td>