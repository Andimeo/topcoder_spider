            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class TheBlackJackDivOne<br>{<br>  int getValue(String s){<br>    char ch = s.charAt(0);<br>    if(ch == 'A') return 11;<br>    if(ch &gt;= '2' &amp;&amp; ch &lt;= '9') return ch - '0';<br>    return 10;<br>  }<br>  int total = 0;<br>  int[] cnt = new int[12];<br>  double go(int v){<br>    if(v &lt;= 0) return 0;<br>    double ans = 1;<br>    for(int i = 2; i &lt; 12; ++i){<br>      if(cnt[i] &gt; 0 &amp;&amp; v &gt; i){<br>        --cnt[i];<br>        --total;<br>        ans += go(v - i) * (1 + cnt[i]) / (1 + total);<br>        ++cnt[i];<br>        ++total;<br>      }<br>    }<br>    return ans;<br>  }<br>  public double expected(String[] c)<br>  {  <br>    total = 52 - c.length;<br>    Arrays.fill(cnt, 4);<br>    cnt[10] = 16; cnt[1] = 0; cnt[0] = 0;<br>    int num = 21;<br>    for(int i = 0; i &lt; c.length; ++i){<br>      cnt[getValue(c[i])]--;<br>      num -= getValue(c[i]);<br>    }<br>    return go(num);<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>