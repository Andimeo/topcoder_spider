            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections.Generic;<br><br>public class TheBlackJackDivTwo {<br>    public int score(string[] cards) {<br>        int res = 0;<br>        <br>        for(int i=0;i&lt;cards.Length;i++) {<br>            if (cards[i][0] == 'T') res += 10;<br>            else if (cards[i][0] == 'J') res += 10;<br>            else if (cards[i][0] == 'Q') res += 10;<br>            else if (cards[i][0] == 'K') res += 10;<br>            else if (cards[i][0] == 'A') res += 11;<br>            else {<br>                res += (cards[i][0] - '0');<br>            }<br>        }<br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>