            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;queue&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br> <br>class TheBlackJackDivTwo {<br>public:<br>  int score(vector &lt;string&gt; cards) {<br>    char c;<br>    int sum=0;<br>    for(int i=0;i&lt;cards.size();i++){<br>      c=cards[i][0];<br>      if(c=='A') sum+=11;<br>      else if(c=='J'||c=='K'||c=='Q'||c=='T') sum+=10;<br>      else sum+=int(c)-48;<br>    }<br>    return sum;  <br>    <br>  }<br>};<br> <br> <br> <br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>