            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class Inequalities {<br>    public int maximumSubset(string[] inequalities) {<br>        int best = 0;<br>        int N = inequalities.Length;<br>        for (double dx = -1; dx &lt; 1002; dx+=0.5) {<br>            int count=0;<br>            for (int i = 0; i &lt; N; i++) {<br>                string[] s = inequalities[i].Split(' ');<br>                int c = int.Parse(s[2]);<br>                if (s[1] == "&gt;" &amp;&amp; dx&gt;c) count++;<br>                if (s[1] == "=" &amp;&amp; dx == c) count++;<br>                if (s[1] == "&gt;=" &amp;&amp; dx &gt;= c) count++;<br>                if (s[1] == "&lt;" &amp;&amp; dx &lt; c) count++;<br>                if (s[1] == "&lt;=" &amp;&amp; dx &lt;= c) count++;<br>            }<br>            best = Math.Max(best, count);<br>        }<br>        return best;<br><br>    }<br>}<br></td>