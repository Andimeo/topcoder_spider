            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class Inequalities<br>{<br>  public int maximumSubset(String[] inequalities)<br>  {<br>    int ans = 0;<br>    for (int x = -5; x &lt;= 2005; ++x) {<br>      int cur = 0;<br>      for (String s : inequalities) {<br>        String[] tmp = s.split(" ");<br>        int v = Integer.parseInt(tmp[2]);<br>        if ("=".equals(tmp[1])) {<br>          if (x == 2 * v) {<br>            cur++;<br>          }<br>        }<br>        if ("&lt;".equals(tmp[1])) {<br>          if (x &lt; 2 * v) {<br>            cur++;<br>          }<br>        }<br>        if ("&lt;=".equals(tmp[1])) {<br>          if (x &lt;= 2 * v) {<br>            cur++;<br>          }<br>        }<br>        if ("&gt;".equals(tmp[1])) {<br>          if (x &gt; 2 * v) {<br>            cur++;<br>          }<br>        }<br>        if ("&gt;=".equals(tmp[1])) {<br>          if (x &gt;= 2 * v) {<br>            cur++;<br>          }<br>        }<br>      }<br>      ans = Math.max(ans, cur);<br>    }<br>    return ans;<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>