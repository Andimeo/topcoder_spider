            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class TheTriangleBothDivs<br>{<br>    string a, b, c;<br>    bool eq(string aa, string bb)<br>    {<br>        for (int i = 0; i &lt; aa.Length; i++)<br>            if (aa[i] != '?' &amp;&amp; aa[i] != bb[i])<br>                return false;<br>        return true;<br>    }<br>    public string fix(string time)<br>    {<br>        a = time.Substring(0, 2);<br>        b = time.Substring(3, 2);<br>        c = time.Substring(9, 2);<br>        string ret = "";<br>        for(int h=0;h&lt;=23;h++)<br>            for(int m=0;m&lt;=59;m++)<br>                for (int g = -9; g &lt;= 9; g++)<br>                {<br>                    string aa = h.ToString();<br>                    string bb = m.ToString();<br>                    string cc = g.ToString();<br>                    if (aa.Length &lt; 2)<br>                        aa = "0" + aa;<br>                    if (bb.Length &lt; 2)<br>                        bb = "0" + bb;<br>                    if (cc.Length &lt; 2)<br>                        cc = "+" + cc;<br>                    if (eq(a, aa) &amp;&amp; eq(b, bb) &amp;&amp; eq(c, cc))<br>                    {<br>                        int h2 = h - g;<br>                        if (h2 &lt; 0)<br>                            h2 += 24;<br>                        h2 %= 24;<br>                        string aa2 = h2.ToString();<br>                        if (aa2.Length &lt; 2)<br>                            aa2 = "0" + aa2;<br>                        string temp = aa2 + ":" + bb;<br>                        if (ret.Length==0 || temp.CompareTo(ret)&lt;0)<br>                            ret = temp;<br>                    }<br>                }<br>        return ret;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.8 (beta) modified by pivanof!<br></td>