<td class="problemText" colspan="8" valign="middle" align="left">
            #include &lt;math.h&gt;<br>#include &lt;string.h&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;map&gt;<br>#include &lt;queue&gt;<br>#include &lt;algorithm&gt;<br>using namespace std;<br><br>const long long mmod = 1000000007;<br>class RabbitNumbering<br>{<br>  public:<br>  int theCount(vector &lt;int&gt; maxNumber)<br>  {<br>    int n = maxNumber.size();<br>    sort(maxNumber.begin(), maxNumber.end());<br>    long long res = 1;<br>    for (int i=0; i&lt;n; i++) {<br>      res = res * (maxNumber[i]-i);<br>      res %= mmod;<br>    }<br>    return res;<br>  };<br>};<br></td>