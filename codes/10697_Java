            import java.util.*;<br> <br>public class RabbitNumbering {<br>    static final long MOD = 1000000007;<br> <br>    public int theCount(int[] maxNumber) {<br>        Arrays.sort(maxNumber);<br>        long res = 1;<br>        for (int i = 0; i &lt; maxNumber.length; ++i) {<br>            long cur = maxNumber[i] - i;<br>            if (cur &lt; 0)<br>                cur = 0;<br>            res = (res * cur) % MOD;<br>        }<br>        return (int) res;<br>    }<br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>