<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>using System.Collections.Generic;<br>using System.Text;<br> <br>class ProductTriplet<br>{<br>    long count(long x, long y, long z)<br>    {<br>        if (x == 0 || y == 0 || z == 0)<br>        {<br>            return 0;<br>        }<br>        long res = 0;<br>        for (long i = 1; i &lt;= x; ++i)<br>        {<br>            long length = Math.Min(y, z / i);<br>            if (length == 0)<br>            {<br>                break;<br>            }<br>            long low = i;<br>            long high = x + 1;<br>            while (low + 1 &lt; high)<br>            {<br>                long mid = (low + high) / 2;<br>                long next_length = Math.Min(y, z / mid);<br>                if (length == next_length)<br>                {<br>                    low = mid;<br>                }<br>                else<br>                {<br>                    high = mid;<br>                }<br>            }<br>            res += length * (low - i + 1);<br>            i = low;<br>        }<br>        return res;<br>    }<br>    public long countTriplets(int minx, int maxx, int miny, int maxy, int minz, int maxz)<br>    {<br>        return count(maxx, maxy, maxz) - count(minx - 1, maxy, maxz) - count(maxx, miny - 1, maxz) - count(maxx, maxy, minz - 1)<br>             + count(minx - 1, miny - 1, maxz) + count(minx - 1, maxy, minz - 1) + count(maxx, miny - 1, minz - 1) - count(minx - 1, miny - 1, minz - 1);<br>    }<br>}<br></td>