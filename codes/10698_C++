<td class="problemText" colspan="8" valign="middle" align="left">
            #include &lt;algorithm&gt;<br> <br>using namespace std;<br> <br>class ProductTriplet<br>{<br>public:<br>  long long countTriplets(int mx, int my, int mz)<br>  {<br>    long long results = 0;<br>    for (int x = 1; x &lt;= mx &amp;&amp; x &lt;= 50000; x++)<br>    {<br>      results += min(my, mz / x);<br>    }<br>    for (int y = 1; y &lt;= my &amp;&amp; y &lt;= 50000; y++)<br>    {<br>      results += max(0, min(mx, mz / y) - 50000);      <br>    }<br>    return results;<br>  }<br>  long long countTriplets(int minx, int maxx, int miny, int maxy, int minz, int maxz)<br>  {<br>    int x[2], y[2], z[2];<br>    x[0] = minx - 1;<br>    x[1] = maxx;<br>    y[0] = miny - 1;<br>    y[1] = maxy;<br>    z[0] = minz - 1;<br>    z[1] = maxz;<br>    long long results = 0;<br>    for (int i = 0; i &lt; 2; i++)<br>      for (int j = 0; j &lt; 2; j++)<br>        for (int k = 0; k &lt; 2; k++)<br>        {<br>          long long l = countTriplets(x[i], y[j], z[k]);<br>          if ((i + j + k) % 2 == 0)<br>            results -= l;<br>          else<br>            results += l;<br>        }<br>    return results;<br>  }<br>};<br></td>