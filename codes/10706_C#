            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class DoubleXor<br>{<br>  public int calculate(int N)<br>  {<br>        int[] a = new int[10];<br>        int[] b = new int[10];<br>    for (int i=N; i&gt;0; i--)<br>    {<br>        int j = i;<br>            for (int k=0; k&lt;10; k++)<br>            {<br>                a[k] = j%10;<br>                j /= 10;<br>            }<br>            for (int k=0; k&lt;10; k++)<br>            {<br>                b[k] ^= a[k];<br>                b[k] %= 10;<br>            }<br>    }<br>      int d = 1;<br>      int ans = 0;<br>        for (int i=0; i&lt;10; i++)<br>        {<br>            ans = ans + d*b[i];<br>            d *= 10;<br>        }<br>      return ans;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>