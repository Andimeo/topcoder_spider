            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class DoubleXor {<br>public:<br>  int calculate(int);<br>};<br> <br>int DoubleXor::calculate(int N) {<br>  int i, x, a[100], j, ans;<br>  memset(a,0,sizeof(a));<br>  for (i=N;i&gt;=1;i--) {<br>    x = i;<br>    for (j=1;j&lt;=7;j++) {<br>      a[j] = (a[j]^(x%10)) % 10;<br>      x /= 10;<br>    }<br>  }<br>  ans = 0;<br>  for (i=7;i&gt;=1;i--) ans = ans*10+a[i];<br>  return ans;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>