            import java.util.*;<br> <br>public class DoubleXor {<br>    public int calculate(int N) {<br>        int[] digits = new int[7];<br>        for (int i = N; i &gt;= 1; --i) {<br>            int t = i;<br>            for (int j = 0; j &lt; 7; ++j) {<br>                digits[j] = (digits[j] ^ (t % 10)) % 10;<br>                t /= 10;<br>            }<br>        }<br>        int res = 0;<br>        for (int j = 0; j &lt; 7; ++j)<br>            res = res * 10 + digits[6 - j];<br>        return res;<br>    }<br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>