            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;string&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br>#include &lt;algorithm&gt;<br>using namespace std;<br><br>class WordsGame<br>{<br>public:<br>  int minimumSwaps(vector &lt;string&gt; grid, string word)<br>  {<br>    vector&lt;char&gt;vv;<br>        int m=word.length(),i,j;<br>        int minn=0x7fffffff;<br>    for(i=0;i&lt;m;i++){vv.clear();<br>             for(j=0;j&lt;m;j++)<br>             vv.push_back(grid[i][j]);<br><br>             int sum=0;<br>             bool flag=1;<br>             for(j=0;j&lt;m;j++)<br>             {<br>                if(word[j]==vv[j])<br>                continue;int k;<br>                for(k=j+1;k&lt;m;k++)<br>                if(vv[k]==word[j])<br>                 break;<br>                 if(k==m)<br>                 {flag=0;break;}<br>                 else<br>                 {<br>                     vv[k]=vv[j];<br>                     sum++;    <br>                 }               <br>             } <br>             if(flag&amp;&amp;sum&lt;minn)minn=sum;                <br>        }<br>        for(i=0;i&lt;m;i++){vv.clear();<br>             for(j=0;j&lt;m;j++)<br>             vv.push_back(grid[j][i]);<br><br>             int sum=0;<br>             bool flag=1;<br>             for(j=0;j&lt;m;j++)<br>             {<br>                if(word[j]==vv[j])<br>                continue;int k;<br>                for(k=j+1;k&lt;m;k++)<br>                if(vv[k]==word[j])<br>                 break;<br>                 if(k==m)<br>                 {flag=0;break;}<br>                 else<br>                 {<br>                     vv[k]=vv[j];<br>                     sum++;    <br>                 }               <br>             } <br>             if(flag&amp;&amp;sum&lt;minn)minn=sum;                <br>        }if(minn==0x7fffffff)return -1;<br>        return minn;<br>  }<br>};<br></td>