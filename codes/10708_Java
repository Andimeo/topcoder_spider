            import java.util.*;<br>import static java.lang.Math.*;<br> <br>public class WordsGame {<br>    public int minimumSwaps(String[] ss, String s) {<br>      int r = 1&lt;&lt;30;<br>      for (int i = 0; i &lt; ss.length; i++) {<br>      String t = ss[i];<br>      r = min(r, cost(s,t));<br>    }<br>      <br>      for (int i = 0; i &lt; ss.length; i++) {<br>        String t = "";<br>        for (int row = 0; row &lt; ss.length; row++) {<br>        t += ss[row].charAt(i);<br>      }<br>      r = min(r, cost(s,t));<br>    }<br>      return r == 1&lt;&lt;30 ? -1 : r;<br>    }<br>    int cost(String s, String t)<br>    {<br>      for(char c: s.toCharArray())<br>        if(!t.contains(""+c))<br>          return 1&lt;&lt;30;<br>      char[] a = t.toCharArray();<br>      int r = 0;<br>      for (int i = 0; i &lt; s.length(); i++) {<br>      if(s.charAt(i) == a[i])<br>        continue;<br>      for(int j = i+1; j &lt; s.length(); j++)<br>        if(a[j] == s.charAt(i))<br>        {<br>          char temp = a[i];<br>          a[i] = a[j];<br>          a[j] = temp;<br>          break;<br>        }<br>      r++;<br>    }<br>      return r;<br>    }<br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>