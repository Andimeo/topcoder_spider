            const int dis[10][7] =<br>{<br>  {1, 1, 1, 0, 1, 1, 1},<br>  {0, 0, 1, 0, 0, 1, 1},<br>  {1, 0, 1, 1, 1, 0, 1},<br>  {1, 0, 1, 1, 0, 1, 1},<br>  {0, 1, 1, 1, 0, 1, 0},<br>  {1, 1, 0, 1, 0, 1, 1},<br>  {1, 1, 0, 1, 1, 1, 1},<br>  {1, 0, 1, 0, 0, 1, 0},<br>  {1, 1, 1, 1, 1, 1, 1},<br>  {1, 1, 1, 1, 0, 1, 1},<br>};<br><br>class NumbersAndMatches<br>{<br>  public:<br>  <br>  long long dp[127][127];<br>  <br>  long long differentNumbers(long long N, int K)<br>  {<br>    const int m = 126;<br>    <br>    for (int i = 0; i &lt;= m; i ++)<br>      for (int j = 0; j &lt;= m; j ++)<br>        dp[i][j] = !i &amp;&amp; !j;<br>    <br>    int inc[10][10], dec[10][10];<br>    <br>    for (int i = 0; i &lt; 10; i ++)<br>      for (int j = 0; j &lt; 10; j ++)<br>      {<br>        inc[i][j] = dec[i][j] = 0;<br>        for (int k = 0; k &lt; 7; k ++)<br>        {<br>          if (dis[i][k] &lt; dis[j][k])<br>            inc[i][j] ++;<br>          if (dis[i][k] &gt; dis[j][k])<br>            dec[i][j] ++;<br>        }<br>      }<br>    <br>    while (N &gt; 0)<br>    {<br>      int n = N % 10;<br>      N /= 10;<br>      <br>      for (int i = m; i &gt;= 0; i --)<br>        for (int j = m; j &gt;= 0; j --)<br>          if (dp[i][j])<br>          {<br>            long long me = dp[i][j];<br><br>            dp[i][j] -= me;<br>            for (int k = 0; k &lt; 10; k ++)<br>            {<br>              dp[i + inc[n][k]][j + dec[n][k]] += me;<br>            }<br>          }<br>    }<br>    <br>    long long re = 0;<br>    for (int i = 0; i &lt;= K; i ++)<br>      re += dp[i][i];<br>    return re;<br>  }<br>};<br></td>