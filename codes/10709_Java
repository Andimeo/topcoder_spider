            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class NumbersAndMatches<br>{<br>  int[][] f = {<br>    {1,1,1,0,1,1,1},//0<br>    {0,0,1,0,0,1,1},//1<br>    {1,0,1,1,1,0,1},//2<br>    {1,0,1,1,0,1,1},//3<br>    {0,1,1,1,0,1,0},//4<br>    {1,1,0,1,0,1,1},//5<br>    {1,1,0,1,1,1,1},//6<br>    {1,0,1,0,0,1,0},//7<br>    {1,1,1,1,1,1,1},//8<br>    {1,1,1,1,0,1,1},//9<br>  };<br>  <br>  int add(int i, int j) {<br>    int r = 0;<br>    for (int it = 0; it &lt; 7; ++it) {<br>      if (f[i][it] == 0 &amp;&amp; f[j][it] == 1) {<br>        ++r;<br>      }<br>    }<br>    return r;<br>  }<br>  <br>  int del(int i, int j) {<br>    int r = 0;<br>    for (int it = 0; it &lt; 7; ++it) {<br>      if (f[i][it] == 1 &amp;&amp; f[j][it] == 0) {<br>        ++r;<br>      }<br>    }<br>    return r;<br>  }<br>  <br>  public long differentNumbers(long num, int k)<br>  {<br>    ArrayList&lt;Integer&gt; a = new ArrayList&lt;Integer&gt;();<br>    while (num &gt; 0) {<br>      a.add((int)(num % 10));<br>      num /= 10;<br>    }<br>    int n = a.size();<br>    long[][][] d = new long[n + 1][k + 1][k + 1];<br>    d[0][0][0] = 1;<br>    for (int i = 0; i &lt; n; ++i) {<br>      for (int u = 0; u &lt;= k; ++u) {<br>        for (int v = 0; v &lt;= k; ++v) {<br>          if (d[i][u][v] == 0) {<br>            continue;<br>          }<br>          for (int dig = 0; dig &lt; 10; ++dig) {<br>            int uu = u + add(a.get(i), dig);<br>            int vv = v + del(a.get(i), dig);<br>            if (uu &gt;= 0 &amp;&amp; uu &lt;= k &amp;&amp; vv &gt;= 0 &amp;&amp; vv &lt;= k) {<br>              d[i + 1][uu][vv] += d[i][u][v];<br>            }<br>          }<br>        }<br>      }<br>    }<br>    long ans = 0;<br>    for (int i = 0; i &lt;= k; ++i) {<br>      ans += d[n][i][i];<br>    }<br>    return ans;<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>