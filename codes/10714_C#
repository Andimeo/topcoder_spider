            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class SpidersOnTheGrid<br>{<br>  public int find(string[] A)<br>  {<br>        int n = A.Length;<br>        int m = A[0].Length;<br>        int[,] d = new int[n, m];<br>        for (int i = 0; i &lt; n; i++)<br>        {<br>            for (int j = 0; j &lt; m; j++)<br>            {<br>                d[i, j] = 1;<br>            }<br>        }<br>        for (int i = 0; i &lt; n; i++)<br>        {<br>            for (int j = 0; j &lt; m; j++)<br>            {<br>                int dx;<br>                int dy;<br>                dx = 0; dy = 0;<br>                if (A[i][j] == 'N')<br>                {<br>                    dx = -1;<br>                }<br>                else if (A[i][j] == 'W')<br>                {<br>                    dy = -1;<br>                }<br>                else if (A[i][j] == 'S')<br>                {<br>                    dx = 1;<br>                }<br>                else if (A[i][j] == 'E')<br>                {<br>                    dy = 1;<br>                }<br>                int x = i;<br>                int y = j;<br>                if (x + dx &gt;= 0 &amp;&amp; x + dx &lt; n &amp;&amp; y + dy &gt;= 0 &amp;&amp; y + dy &lt; m)<br>                {<br>                    d[i, j]--;<br>                    d[x + dx, y+dy]++;<br>                }<br>                else<br>                {<br>                    d[i, j]--;<br>                }<br>            }<br>        }<br>        int ans = 0;<br>        for (int i = 0; i &lt; n; i++)<br>            for (int j = 0; j &lt; m; j++)<br>                if (d[i, j] == 0)<br>                    ans++;<br>        return ans;<br>  }<br> <br>    <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>