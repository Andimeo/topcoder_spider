            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;queue&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br> <br>class SpidersOnTheGrid {<br>public:<br>  int find(vector &lt;string&gt; A) {<br>    vector &lt; string &gt; novi = A;<br>    for ( int i = 0; i &lt; A.size(); ++i )<br>      for ( int j = 0; j &lt; A[0].size(); ++j )<br>        novi[i][j] = '5';<br>    <br>    for ( int i = 0; i &lt; A.size(); ++i )<br>      for ( int j = 0; j &lt; A[0].size(); ++j ){<br>        if ( A[i][j] == 'N' &amp;&amp; i - 1 &gt;= 0 ) novi[i-1][j] = 'x';<br>        if ( A[i][j] == 'S' &amp;&amp; i + 1 &lt; A.size() ) novi[i+1][j] = 'x';<br>        if ( A[i][j] == 'E' &amp;&amp; j + 1 &lt; A[0].size() ) novi[i][j+1] = 'x';<br>        if ( A[i][j] == 'W' &amp;&amp; j - 1 &gt;= 0 ) novi[i][j-1] = 'x';<br>      }<br>      <br>    int res = 0;<br>      <br>    for ( int i = 0; i &lt; A.size(); ++i )<br>      for ( int j = 0; j &lt; A[0].size(); ++j )<br>        if ( novi[i][j] != 'x' )<br>          ++res;<br>  <br>    return res;  <br>  }<br>};<br> <br> <br> <br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>