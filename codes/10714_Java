            import java.util.*;<br>public class SpidersOnTheGrid <br>{<br>    public int find(String[] A) <br>    {<br>        int res = 0;<br>        <br>        boolean[][] cells = new boolean[A.length][A[0].length()];<br>        <br>        for(int i = 0; i &lt; A.length; i++)<br>          for(int j = 0; j &lt; A[0].length(); j++)<br>          {<br>            if(A[i].charAt(j) == 'N' &amp;&amp; i-1 &gt;= 0)<br>              cells[i-1][j] = true;<br>            else if(A[i].charAt(j) == 'S' &amp;&amp; i+1 &lt; A.length)<br>              cells[i+1][j] = true;<br>            else if(A[i].charAt(j) == 'E' &amp;&amp; j+1 &lt; A[0].length())<br>              cells[i][j+1] = true;<br>            else if(A[i].charAt(j) == 'W' &amp;&amp; j-1 &gt;= 0)<br>              cells[i][j-1] = true;<br>          }<br>        <br>        for(int i = 0; i &lt; A.length; i++)<br>          for(int j = 0; j &lt; A[0].length(); j++)<br>            if(cells[i][j] == false)<br>              res++;<br>        <br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>