            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class EasySequence {<br>  public int find(int[] A, int[] B) {<br>    List&lt;int&gt; p = new List&lt;int&gt;(A);<br>    for (int i = p.Count; i &lt; 100000; i++)<br>    {<br>      int sum = 0;<br>      for (int j = i - A.Length; j &lt; i; j++)<br>      {<br>        sum += p[j];<br>      }<br>      sum %= 10;<br>      p.Add(sum);<br>    }<br><br>    StringBuilder sb = new StringBuilder();<br>    for (int i = 0; i &lt; p.Count; i++)<br>    {<br>      sb.Append(p[i]);<br>    }<br>    StringBuilder sb2 = new StringBuilder();<br>    for (int i = 0; i &lt; B.Length; i++)<br>    {<br>      sb2.Append(B[i]);<br>    }<br><br><br>    return sb.ToString().IndexOf(sb2.ToString()) ;<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>