            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;queue&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>int a[1000010],n,m;<br>bool fl;<br> <br>class EasySequence {<br>public:<br>  int find(vector &lt;int&gt; A, vector &lt;int&gt; B) {<br>    n=A.size();m=B.size();<br>    for(int i=0;i&lt;n;i++)<br>      a[i]=A[i];<br>    for(int i=n;i&lt;1000000;i++){<br>      for(int j=1;j&lt;=n;j++)<br>        a[i]+=a[i-j];<br>      a[i]%=10;<br>    }<br>    for(int i=0;i&lt;1000000-m;i++){<br>      fl=true;<br>      for(int j=0;j&lt;m;j++)<br>        fl=fl&amp;&amp;(a[i+j]==B[j]);<br>      if(fl)return i;<br>    }<br>    return -1;<br>  }<br>};<br> <br> <br> <br>//Powered by KawigiEdit 2.1.5 (beta) modified by pivanof!<br></td>