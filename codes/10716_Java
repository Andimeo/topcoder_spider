            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br><br>public class EasySequence<br>{<br>  public int find(int[] A, int[] B)<br>  {<br>    char[] ch = new char[1 &lt;&lt; 22];<br>    int n = A.length;<br>    for(int i = 0; i &lt; n; ++i) ch[i] = (char)(A[i]);<br>    for(int i = n; i &lt; ch.length; ++i){<br>      int sum = 0;<br>      for(int j = 1; j &lt;= n; ++j){<br>        sum += ch[i - j];<br>      }<br>      ch[i] = (char)(sum % 10);<br>    }<br>    for(int i = 0; i &lt; ch.length; ++i) ch[i] += '0';<br>    String s = new String(ch);<br>    String t = "";<br>    for(int i : B) t += i;<br>    return s.indexOf(t);<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br><br><br>// Powered by FileEdit<br></td>