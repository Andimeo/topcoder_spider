            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class ColorfulStrings {<br>public:<br>  string getKth(int, int);<br>};<br> <br>string s, ans;<br>int was[11111111];<br>int n,k;<br> <br>void rec(int v) {<br>  if (v == n) {<br>    k--;<br>    if (k == 0) ans = s;<br>    return;<br>  }<br>  char ch;<br>  int ok, t, i;<br>  for (ch='0';ch&lt;='9';ch++) {<br>    s += ch;<br>    ok = 1; t = 1;<br>    for (i=v;i&gt;=0;i--) {<br>      t *= s[i]-48;<br>      if (was[t]) ok = 0;<br>      was[t]++;<br>    }<br>    if (ok) rec(v+1);<br>    t = 1;<br>    for (i=v;i&gt;=0;i--) {<br>      t *= s[i]-48;<br>      was[t]--;<br>    }<br>    s.erase(s.length()-1);<br>  }<br>}<br> <br>string ColorfulStrings::getKth(int nn, int kk) {<br>  n = nn; k = kk;<br>  if (n &gt; 10) return "";<br>  ans = ""; s = "";<br>  memset(was,0,sizeof(was));<br>  rec(0);<br>  return ans;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>