            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class ColorfulStrings<br>{<br>  ArrayList&lt;String&gt; res = new ArrayList&lt;String&gt;();<br>  int[] a;<br>  int re = 0;<br>  int n;<br>  public String getKth(int n, int k)<br>  {<br>    this.n = n;<br>    if (n &gt;= 10) return "";<br>    a = new int[n];<br>    doit(n, 0);<br>    Collections.sort(res);<br>    if (k &gt; res.size()) return "";<br>    return res.get(k - 1);<br>  }<br>  <br>  void doit(int n, int used)<br>  {<br>    if (n == 0)<br>    {<br>      check();<br>      return;<br>    }<br>    for (int i = 0; i &lt; 10; i++)<br>    {<br>      if (((used &gt;&gt; i) &amp; 1) == 1) continue;<br>      a[n - 1] = i;<br>      doit(n - 1 , used | (1 &lt;&lt; i));<br>    }<br>  }<br>  <br>  int[] p = new int[400000];<br>  void check()<br>  {<br>    re++;<br>    for (int i = 0; i &lt; n; i++)<br>    {<br>      for (int j = i + 1; j &lt;= n; j++)<br>      {<br>        int r = 1;<br>        for (int k = i; k &lt; j; k++)<br>        {<br>          r *= a[k];<br>        }<br>        if (p[r] == re) return;<br>        p[r] = re;<br>      }<br>    }<br>    String s = "";<br>    for (int i = 0; i &lt; n; i++)<br>    {<br>      s += a[i];<br>    }<br>    res.add(s);<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>