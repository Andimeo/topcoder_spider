            #include&lt;vector&gt;<br>#include&lt;string&gt;<br>#include&lt;algorithm&gt;<br>#include&lt;cmath&gt;<br>#include&lt;utility&gt;<br>#include&lt;set&gt;<br>#include&lt;queue&gt;<br>#include&lt;map&gt;<br>#include&lt;cstdlib&gt;<br>#include&lt;cstdio&gt;<br>#include&lt;cstring&gt;<br>#include&lt;iostream&gt;<br>#include&lt;sstream&gt;<br>#include&lt;climits&gt;<br>#include&lt;sys/time.h&gt;<br>using namespace std;<br><br>#define REP(i,a,b) for(i=a;i&lt;b;i++)<br>#define rep(i,n) REP(i,0,n)<br><br>#define INF 10000000<br>#define EPS 1e-7<br><br>class ColoringRectangle {<br>public:<br>int chooseDisks(int width, int height, vector &lt;int&gt; a, vector &lt;int&gt; b) {<br>  int i,j,k,loop;<br>  int res = INF, tmp;<br>  double rest, gain;<br>  vector&lt;int&gt; sw;<br><br>  sort(a.rbegin(),a.rend());<br>  sort(b.rbegin(),b.rend());<br><br>  rep(loop,2){<br>    rest = width;<br>    tmp  = INF;<br>    for(i=0;;i++){<br>      if(i&gt;=a.size()) break;<br>      if(a[i] &lt; height) break;<br>      gain = sqrt(a[i]*a[i]-height*height);<br>      rest -= gain;<br>      if(rest &lt; EPS){tmp=i*2+1; break;}<br>      <br>      if(i&gt;=b.size()) break;<br>      if(b[i] &lt; height) break;<br>      gain = sqrt(b[i]*b[i]-height*height);<br>      rest -= gain;<br>      if(rest &lt; EPS){tmp=i*2+2; break;}<br>    }<br><br>    if(res&gt;tmp) res=tmp;<br>    sw=a; a=b; b=sw;<br>  }<br><br>  if(res==INF) res=-1;<br>  return res;<br>}<br><br><br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>