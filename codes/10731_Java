            import java.util.*;<br> <br>public class ColoringRectangle {<br>    public int solve(int width, int height, int[] red, int[] blue) {<br>        int rp = red.length - 1;<br>        int bp = blue.length - 1;<br> <br>        double l = 0;<br>        int c = 0;<br> <br>        while (rp &gt;= 0) {<br>            if (red[rp] &lt; height) {<br>                return Integer.MAX_VALUE;<br>            }<br>            l += Math.sqrt(red[rp] * red[rp] - height * height);<br>            c++;<br>            rp--;<br>            if (l &gt;= width) {<br>                return c;<br>            }<br> <br>            if (bp &lt; 0) {<br>                return Integer.MAX_VALUE;<br>            }<br>            if (blue[bp] &lt; height) {<br>                return Integer.MAX_VALUE;<br>            }<br>            l += Math.sqrt(blue[bp] * blue[bp] - height * height);<br>            c++;<br>            bp--;<br>            if (l &gt;= width) {<br>                return c;<br>            }<br>        }<br>        return Integer.MAX_VALUE;<br>    }<br> <br>    public int chooseDisks(int width, int height, int[] red, int[] blue) {<br>        Arrays.sort(red);<br>        Arrays.sort(blue);<br> <br>        int r = Math.min(solve(width, height, red, blue), solve(width, height, blue, red));<br>        if (r == Integer.MAX_VALUE) {<br>            r = -1;<br>        }<br>        return r;<br>    }<br> <br> <br>}<br> <br> <br>// Powered by PopsEdit<br> <br>// Powered by CodeProcessor<br></td>