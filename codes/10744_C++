<td class="problemText" colspan="8" valign="middle" align="left">
            #include &lt;math.h&gt;<br>#include &lt;string.h&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;map&gt;<br>#include &lt;queue&gt;<br>#include &lt;algorithm&gt;<br>using namespace std;<br><br>class CandyBox<br>{<br>  public:<br>  vector &lt;double&gt; expectedScore(int C, vector &lt;int&gt; score, int S)<br>  {<br>    int sz = score.size();<br>    vector&lt;double&gt; res(sz);<br>    for (int i=0; i&lt;sz; i++) res[i] = score[i];<br>    for  (int i=0; i&lt;S; i++) {<br>      double diff[100] = {0};<br>      for (int j=0; j&lt;sz; j++)<br>        for  (int k=j+1; k&lt;sz; k++) {<br>          double pp = 2.0/sz*C/(C*sz-1);<br>          diff[j] += (res[k]-res[j])*pp/C;<br>          diff[k] -= (res[k]-res[j])*pp/C;<br>        }<br>      for  (int j=0; j&lt;sz; j++) res[j] += diff[j];<br>    }<br>    return res;<br>  };<br>};<br></td>