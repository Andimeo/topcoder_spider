            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class Nisoku {<br>    public double theMax(double[] cards) {<br>        Array.Sort(cards);<br>        int N = cards.Length;<br>        double max = 0;<br>        for (int i = 0; 2*i &lt;= N; i++) {<br>            double tot = 1;<br>            for (int j = 0; j &lt; i; j++) {<br>                tot *= cards[j] + cards[2 * i - j-1];<br>            }<br>            for (int j = 2*i; j &lt; N; j++) {<br>                tot *= cards[j];<br>            }<br>            max = Math.Max(max, tot);<br>        }<br>        return max;<br>    }<br>}<br></td>