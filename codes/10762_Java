            import java.util.*;<br><br>import javax.transaction.TransactionRequiredException;<br>public class T9 {<br>    public String message(String[] part, String[] dict, String[] keystr) {<br>      Arrays.sort(dict);<br>      Map&lt;String, ArrayList&lt;String&gt;&gt; ma = new TreeMap&lt;String, ArrayList&lt;String&gt;&gt;();<br>      for(int i=0;i&lt;dict.length;++i) {<br>        String r = "";<br>        for(int j=0;j&lt;dict[i].length();++j) {<br>          for(int t=0;t&lt;9;++t) {<br>            if(part[t].indexOf(dict[i].charAt(j)) &gt;= 0)<br>              r += (char)('1' + t);<br>          }<br>        }<br>        if(ma.get(r) == null) {<br>          ma.put(r, new ArrayList&lt;String&gt;());<br>        }<br>        ma.get(r).add(dict[i]);<br>      }<br>      String s = "";<br>      for(String x : keystr)<br>        s += x;<br>      String res = "";<br>      for(int i=0;i&lt;s.length();++i) {<br>        if(s.charAt(i) == '0') {<br>          res += " ";<br>        }<br>        else {<br>          int j = i;<br>          for(;j&lt;s.length() &amp;&amp; s.charAt(j) != '0';++j);<br>          String z = "";<br>          for(int t=i;t&lt;j;++t) if(s.charAt(t) &gt;= '1' &amp;&amp; s.charAt(t) &lt;= '9') z += s.charAt(t);<br>          int id = 0;<br>          for(int t=i;t&lt;j;++t)<br>            if(s.charAt(t) == '#') ++id;<br>            else if(s.charAt(t) == '*') id += 5;<br>          res += ma.get(z).get(id);<br>          i = j - 1;<br>        }<br>      }<br>      return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>