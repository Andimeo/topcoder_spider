            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class SimpleGuess<br>{<br>  public int getMaximum(int[] hints)<br>  {<br>    int ret = -1;<br>    for (int i = 0; i &lt; hints.Length; ++i)<br>      for (int j = 0; j &lt; hints.Length; ++j)<br>        if (i != j &amp;&amp; hints[i] &gt;= hints[j] &amp;&amp; (hints[i] - hints[j]) % 2 == 0)<br>        {<br>          int x = (hints[i] + hints[j]) / 2;<br>          int y = (hints[i] - hints[j]) / 2;<br>          if (x * y &gt; ret)<br>            ret = x * y;<br>        }<br>    return ret;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>