            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;set&gt;<br>using namespace std;<br>class SimpleGuess {<br>  public:<br>  int getMaximum(vector&lt;int&gt; tmp) {<br>  set&lt;int&gt; voc,ans;<br>  set&lt;int&gt;::iterator it;<br>  for(int i=0;i&lt;tmp.size();i++)<br>    voc.insert(tmp[i]);<br> <br> <br>  for(int i=0;i&lt;=100;i++){<br>    for(int j=0;j&lt;=100;j++){<br>      if(voc.find(i+j) != voc.end() &amp;&amp; voc.find(i-j) != voc.end())<br>        ans.insert(i*j);<br>    }<br>  }<br>  it = ans.end();<br>  it-- ;<br>  return *it;<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>