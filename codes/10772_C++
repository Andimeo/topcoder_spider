            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;queue&gt;<br>#include &lt;cmath&gt;<br>#define pi 3.14159265358979323846264338327950288<br>#define INF 1000000007<br>#define EPS 0.000000001<br>using namespace std;<br>int n,i,j;<br>double r,a[51],b[51];<br>class TheFansAndMeetingsDivTwo {<br>  public: double find(vector &lt;int&gt; minJ, vector &lt;int&gt; maxJ, vector &lt;int&gt; minB, vector &lt;int&gt; maxB) {<br>    n=minJ.size();<br>    for(i=0;i&lt;n;i++){<br>        for(j=minJ[i];j&lt;=maxJ[i];j++)<br>            a[j]+=1.0/n/(maxJ[i]-minJ[i]+1);<br>        for(j=minB[i];j&lt;=maxB[i];j++)<br>            b[j]+=1.0/n/(maxB[i]-minB[i]+1);<br>    }<br>    r=0;<br>    for(i=0;i&lt;51;i++)<br>        r+=a[i]*b[i];<br>    return r;<br>  }<br>};<br><br><br>// Powered by FileEdit<br></td>