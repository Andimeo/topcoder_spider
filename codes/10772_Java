            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class TheFansAndMeetingsDivTwo<br>{<br>  public double find(int[] minJ, int[] maxJ, int[] minB, int[] maxB)<br>  {<br>    double ans=0;<br>    int n = minJ.length;<br>    for(int i=0;i&lt;n;++i)<br>    {<br>      for(int j=0;j&lt;n;++j)<br>      {<br>        double cur = 0;<br>        for(int ii=minJ[i];ii&lt;=maxJ[i];++ii)<br>          for(int jj=minB[j];jj&lt;=maxB[j];++jj)if(ii==jj)++cur;<br>        cur/=(maxJ[i]-minJ[i]+1)*(+maxB[j]-minB[j]+1);<br>        ans+=cur;<br>      }<br>    }<br>    return ans/n/n;<br>    <br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>