            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class RouteIntersection<br>{<br>  public string isValid(int N, int[] coords, string moves)<br>  {<br>    int n = coords.Length;<br>    for (int i = 0; i &lt; n; ++i)<br>      for (int j = i; j &lt; n; ++j)<br>      {<br>        bool valid = false;<br>        for (int k = i; k &lt;= j; ++k)<br>        {<br>          int distance = 0;<br>          for (int l = i; l &lt;= j; ++l)<br>            if (coords[l] == coords[k])<br>              distance += (moves[l] == '+') ? 1 : -1;<br>          if (distance != 0)<br>            valid = true;<br>        }<br>        if (!valid)<br>          return "NOT VALID";<br>      }<br>    return "VALID";<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>