            import static java.lang.Math.*;<br>import java.util.*;<br><br>public class RouteIntersection {<br><br>  public String isValid(int N, int[] C, String M) {<br>    HashMap&lt;Integer,Integer&gt; hm = new HashMap&lt;Integer,Integer&gt;();<br>    for(int c:C)<br>      if (!hm.containsKey(c))<br>        hm.put(c,hm.size());<br>    HashSet&lt;String&gt; seen = new HashSet&lt;String&gt;();<br>    int[] P = new int[hm.size()];<br>    seen.add(Arrays.toString(P));<br>    for(int i = 0; i &lt; C.length;i++){<br>      if(M.charAt(i) == '+')<br>        P[hm.get(C[i])]++;<br>      else<br>        P[hm.get(C[i])]--;<br>      if(seen.contains(Arrays.toString(P)))<br>        return "NOT VALID";<br>      seen.add(Arrays.toString(P));<br>    }<br>    return "VALID";<br>  }<br>  public void p(Object... s){System.out.println(Arrays.deepToString(s));}<br>}<br></td>