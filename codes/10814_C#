            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class Cryptography<br>{<br>    public long encrypt(int[] numbers)<br>    {<br>        long result = 0, test;<br> <br>        for (int i = 0; i &lt; numbers.Length; i++)<br>        {<br>            test = 1;<br>            for (int j = 0; j &lt; numbers.Length; j++)<br>            {<br>                if (i == j)<br>                    test = test * (numbers[j] + 1);<br>                else<br>                    test = test * numbers[j];<br>            }<br>            if (test &gt; result)<br>                result = test;<br>        }<br>        return result;<br>    }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>