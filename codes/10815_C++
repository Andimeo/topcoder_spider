            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>typedef long long LL;<br>typedef pair&lt;int,int&gt; PII;<br>typedef vector&lt;int&gt; VI;<br>typedef vector&lt;PII&gt; VPII;<br>#define INF 0x7fffffff<br>#define REP(i,n) for (int i = 0, _##i=(n); i &lt; _##i; i++)<br>#define REPD(i,n) for (int i = (n)-1; i &gt;= 0; i--)<br>#define FOR(i,a,b) for (int i = (a), _##i = (b); i &lt;= _##i; i++)<br>#define FORD(i,a,b) for (int i = (b), _##i = (a); i &gt;= _##i; i--)<br>#define SZ(x) ((int)(x).size())<br>#define CLEAR(x) memset(x,0,sizeof x)<br>#define FILL(x,v)  memset(x,v,sizeof x)<br>#define pb push_back<br>#define MP make_pair<br>#define X first<br>#define Y second<br> <br>class SequencePermutation {<br>public:<br>  int determineConfigurations(int, int);<br>};<br> <br>#define MOD 1000000009LL<br>int a[2048][2048];<br>LL d[2048];<br> <br>int SequencePermutation::determineConfigurations(int N, int M) {<br>  int res;<br>  CLEAR(a);<br>  a[1][0] = 1;<br>  FOR(i,2,N) if (i&gt;0)<br>  {<br>    CLEAR(d);<br>    REP(j,M+1)<br>    {<br>      d[j] = a[i-1][j];<br>      if (j&gt;0)<br>        d[j] = (d[j]+d[j-1])%MOD;<br>    }<br>    REP(j,M+1)<br>    {<br>      int k = min(j,i-1);<br>      a[i][j] = d[j];<br>      if (j-k &gt; 0)<br>        a[i][j] = (d[j]+MOD-d[j-k-1])%MOD;<br>/*      FOR(k,0,)<br>      {<br>        a[i][j] = (a[i][j] + a[i-1][j-k]) % MOD;<br>      }*/<br>    }<br>  }<br>  res = 0;<br>  REP(j,M+1) if ((M-j) % 2 == 0)<br>    res = (res + a[N][j])%MOD;<br>  return res;<br>}<br> <br>//Powered by [KawigiEditNonTest] modified by pivanof!<br></td>