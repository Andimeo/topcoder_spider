            using System;<br>using System.Collections;<br>using System.Collections.Specialized;<br>using System.Collections.Generic;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class TheSequencesLevelOne<br>{<br>  public int[] find(int[] sequence)<br>  {<br>    for(int i = 0 ; i &lt; sequence.Length; i++)<br>    {<br>      int Min = 100000;<br>      int ans = 0;<br>      int I = sequence[i];<br>      for(int j = I - 10; j &lt; I +10; j++)<br>      {<br>        if( j%4 == 0 || j%7==0)<br>        {<br>          if( Math.Abs(I-j) &lt; Min)<br>          {<br>            Min = Math.Abs(I-j);<br>            ans = j;<br>          }<br>        }<br>      }<br>      sequence[i] = ans;<br>    }<br>    return sequence;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>