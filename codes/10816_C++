            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class TheSequencesLevelOne {<br>public:<br>  vector &lt;int&gt; find(vector &lt;int&gt;);<br>};<br> <br>vector &lt;int&gt; TheSequencesLevelOne::find(vector &lt;int&gt; a) {<br>  int n = a.size(), i, x, p, q;<br>  for (i=0;i&lt;n;i++) {<br>    x = a[i];<br>    while (x % 4 &amp;&amp; x % 7) x--;<br>    p = x;<br>    x = a[i];<br>    while (x % 4 &amp;&amp; x % 7) x++;<br>    q = x;<br>    if (a[i]-p &lt;= q-a[i]) a[i] = p;<br>    else a[i] = q;<br>  }<br>  return a;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>