            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class TheSequencesLevelOne<br>{<br>  public int[] find(int[] sequence)<br>  {<br>    for(int i = 0; i &lt; sequence.length; i++)  {<br>      if(!lucky(sequence[i]))  {<br>        sequence[i] = makeLucky(sequence[i]);<br>      }<br>    }<br>    return sequence;<br>  }<br>  public boolean lucky(int x)  {<br>    return x % 4 == 0 || x % 7 == 0;<br>  }<br>  public int makeLucky(int x)  {<br>    for(int d = 1; true; d++)  {<br>      if(lucky(x-d))<br>        return x-d;<br>      if(lucky(x+d))<br>        return x+d;<br>    }<br>  }<br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>