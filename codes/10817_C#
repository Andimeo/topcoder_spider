            using System;<br><br>public class TheSequencesLevelThree<br>    {<br>        public int find(int[] sequence, int k)<br>        {<br>            Array.Sort(sequence);<br>            long[][] matr = new long[sequence.Length][];<br>            for (int i = 0; i &lt; sequence.Length; i++)<br>            {<br>                matr[i] = new long[sequence.Length];<br>            };<br>            int n = sequence.Length;<br>            int[] ns = new int[n];<br>            for(int i = 0; i &lt; n; i++)<br>            {<br>                ns[i] = sequence[n - i - 1];<br>            };<br>            matr[0][0] = 1;<br>            for (int i = 1; i &lt; n; i++)<br>            {<br>                int p = i - 1;<br>                for (int j = 0; j &lt; n; j++)<br>                {<br>                    matr[i][j] = 0;<br>                };<br>               <br>                for (int j = 0; j &lt; i; j++)<br>                {<br>                     <br>                    if (Math.Abs(ns[i] - ns[j]) &lt;= k) { matr[i][p] += matr[p][j]; matr[i][p] = matr[i][p] % 1234567891; };<br>                   <br>                    if ((p!=j)&amp;&amp;(Math.Abs(ns[i] - ns[p]) &lt;= k)) { matr[i][j] += matr[p][j]; matr[i][j] = matr[i][j] % 1234567891; };<br>                                        <br>                };<br>            };<br>            long sum = 0;<br>            for (int i = 1; i &lt; n - 1; i++)<br>            {<br>                sum += matr[n - 1][i];<br>                sum = sum % 1234567891;<br>            };<br>            sum = sum * 2 % 1234567891;<br>            return (int)sum;<br>            <br><br>        }<br><br><br>    }<br></td>