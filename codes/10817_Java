            import java.util.*;<br>import java.math.*;<br> <br>public class TheSequencesLevelThree {<br>  long MOD = 1234567891;<br> <br>  public int find(int[] a, int k) {<br>    Arrays.sort(a);<br>    Long[][] dp = new Long[a.length][a.length];<br>    return (int) solve(a.length - 1, a.length - 1, k, a, dp);<br>  }<br> <br>  private long solve(int i, int j, int k, int[] a, Long[][] dp) {<br>    if (dp[i][j] != null) {<br>      return dp[i][j];<br>    }<br>    int next = Math.min(i, j) - 1;<br>    if (next == -1) {<br>      if (i != a.length - 1 &amp;&amp; j != a.length - 1) {<br>        return 1;<br>      } else {<br>        return 0;<br>      }<br>    }<br>    long res = 0;<br>    if (a[i] - a[next] &lt;= k) {<br>      res += solve(next, j, k, a, dp);<br>    }<br>    if (a[j] - a[next] &lt;= k) {<br>      res += solve(i, next, k, a, dp);<br>    }<br>    return dp[i][j] = res % MOD;<br>  }<br> <br> <br>}<br>// Powered by KawigiEdit 2.1.8 (beta) modified by pivanof!<br></td>