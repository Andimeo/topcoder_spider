            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class NumberNeighbours<br>{<br>  public int numPairs(int[] nums)<br>  {<br>    int n = nums.Length;<br>    int res = 0;<br>    for (int i = 0; i &lt; n; i++)<br>      for (int j = i + 1; j &lt; n; j++)<br>        if (ok(nums[i], nums[j]))<br>          res++;<br>    return res;<br>  }<br> <br>  bool ok(int a, int b)<br>  {<br>    char[] x = a.ToString().ToCharArray();<br>    Array.Sort(x);<br> <br>    char[] y = b.ToString().ToCharArray();<br>    Array.Sort(y);<br>    return int.Parse(new string(x)) ==<br>      int.Parse(new string(y));<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>