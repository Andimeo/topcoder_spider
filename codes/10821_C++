            #include &lt;vector&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class NumberNeighbours {<br>public:<br>  int numPairs(vector &lt;int&gt;);<br>};<br> <br>int NumberNeighbours::numPairs(vector &lt;int&gt; numbers) {<br>vector &lt;int&gt; n1,n2;<br>int le=0,n=0,temp,count=0,i=0,j=0;<br>n1.clear();<br>n2.clear();<br>le=numbers.size();<br>for(i=0;i&lt;le;i++)<br>{<br>  cout&lt;&lt;"i am here";<br>  for(j=i+1;j&lt;le;j++)<br>  {<br>    n=numbers[i];<br>    n1.clear();<br>    n2.clear();<br>    while(n!=0)<br>    {<br>      temp=n%10;<br>      if(temp!=0)<br>      {<br>        n1.push_back(temp);<br>      }<br>      n/=10;<br>    }<br>    n=numbers[j];<br>    while(n!=0)<br>    {<br>      temp=n%10;<br>      if(temp!=0)<br>        n2.push_back(temp);<br>      n/=10;<br>    }<br>    sort(n1.begin(),n1.end());<br>    sort(n2.begin(),n2.end());<br>    if(n1==n2)<br>      count++;<br>  }<br>}  <br>return count;<br>}  <br> <br> <br> <br>//d3vdpro<br></td>