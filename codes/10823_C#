            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class LateProfessor<br>{<br>  public double getProbability(int w1, int w2, int l, int x, int y)<br>  {<br>    if(x == y)<br>    {<br>      int t = x % (w1 + w2);<br>      if(t &lt;= w1)<br>        return 0;<br>      else<br>      {<br>        int p = w1 + w2 - t;<br>        if(p &lt; l)<br>        {<br>          return 0;<br>        }<br>        else<br>        {<br>          return 1;<br>        }<br>      }<br>    }<br>    int o = 0;<br>    for(int i=x; i&lt;y; i++)<br>    {<br>      int t = i % (w1 + w2);<br>      if(t &lt; w1)<br>        o ++;<br>      else<br>      {<br>        int p = w1 + w2 - t;<br>        if(p &lt;= l)<br>        {<br>          o ++;<br>        }<br>        else<br>        {<br>          <br>        }<br>      }<br>    }<br>    return 1  - (double)o / (y - x);<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>