            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br>#include &lt;complex&gt;<br>#include &lt;setjmp.h&gt;<br>#include &lt;ext/hash_map&gt;<br>#include &lt;queue&gt;<br>using namespace std;<br> <br>#define FR(i,a,b) for(int i=a;i&lt;b;i++)<br>#define FOR(i,n) FR(i,0,n)<br>#define FORI(i,v) FOR(i,(int)v.size())<br>#define FORALL(i,v) for(typeof(v.end())i=v.begin();i!=v.end();++i)<br>#define BEND(v) v.begin(),v.end()<br>#define dump(x) cerr &lt;&lt; #x &lt;&lt; " = " &lt;&lt; (x) &lt;&lt; endl;<br>typedef long long ll; typedef long double ld;<br> <br>struct LateProfessor {<br>double getProbability(int waitTime, int walkTime, int lateTime, int bestArrival, int worstArrival) {<br>  int period = waitTime+walkTime;<br>  if (bestArrival != worstArrival) {<br>    int tot = 0;<br>    for (int t = bestArrival; t &lt; worstArrival; ++t) {<br>      int q = t%period;<br> <br>      if (q &lt; waitTime) continue;<br>      if (q+lateTime &gt;= period) continue;<br>      ++tot;<br>    }<br>    return double(tot) / double(worstArrival-bestArrival);<br>  } else {<br>    int t = bestArrival;<br>    int q = t % period;<br> <br>    if (0 &lt;= q &amp;&amp; q &lt;= waitTime) return 0;<br>    if (q+lateTime &gt; period) return 0;<br>    return 1;<br>  }<br>}<br> <br> <br>};<br> <br> <br> <br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>