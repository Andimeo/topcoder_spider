            import java.io.*;<br>import java.math.*;<br>import java.util.*;<br>import static java.lang.Math.*;<br>import static java.lang.Integer.*;<br><br>public class LateProfessor {<br>  public double getProbability(int wait, int walk, int late, int best, int worst) {<br>    int deny=0,state=0,left=wait,tid[]=new int[]{wait,walk};<br>    for(int i=0;i&lt;best;i++) {<br>      if(left==0) {<br>        state^=1;<br>        left=tid[state];<br>      }<br>      left--;<br>    }<br>    if(best==worst) return state==1&amp;&amp;left&gt;=late?1.0:0.0;<br>    for(int i=best;i&lt;worst;i++) {<br>      if(left==0) {<br>        state^=1;<br>        left=tid[state];<br>      }<br>      if(state==1 &amp;&amp; left&gt;late) {<br>//        System.out.printf("%d\n",i); <br>        deny++;<br>      }<br>      left--;<br>    }<br>    return(double)deny/(worst-best);<br>  }<br>}<br></td>