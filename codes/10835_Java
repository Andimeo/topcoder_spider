            import java.util.*;<br>import java.math.*;<br>import static java.lang.Math.*;<br> <br>public class KingdomXCitiesandVillagesAnother {<br> <br>    public double determineLength(int[] cx, int[] cy, int[] vx, int[] vy) {<br>        int c = cx.length;<br>        int v = vx.length;<br>        boolean[] finish = new boolean[v];<br> <br>        double ans = 0;<br>        for (int i=0;i&lt;v;i++) {<br>            double min = 1e60;<br>            int num = -1;<br>            for (int j=0;j&lt;v;j++) {<br>                if (finish[j]) continue;<br>                for (int k=0;k&lt;c;k++) {<br>                    double dx = cx[k]-vx[j];<br>                    double dy = cy[k]-vy[j];<br>                    double dis = sqrt(dx*dx+dy*dy);<br> <br>                    if (min&gt;dis) {<br>                        min = dis;<br>                        num = j;<br>                    }<br>                }<br>                for (int k=0;k&lt;v;k++) {<br>                    if (!finish[k]) continue;<br>                    double dx = vx[k]-vx[j];<br>                    double dy = vy[k]-vy[j];<br>                    double dis = sqrt(dx*dx+dy*dy);<br> <br>                    if (min&gt;dis) {<br>                        min = dis;<br>                        num = j;<br>                    }<br>                }<br>            }<br>            finish[num] = true;<br>            ans += min;<br>        }<br> <br>        return ans;<br>    }<br> <br>    static void debug(Object... arr) { // {{{<br>    } // }}}<br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>