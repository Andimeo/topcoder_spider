            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class TurretPlacement<br>{<br>  public long count(int[] x, int[] y)<br>  {<br>    long ret = 0;<br>    int n = x.Length;<br>    for(int i=0; i&lt;n; i++)<br>    {<br>      for(int j=i+1; j&lt;n; j++)<br>      {<br>        long d = (long)(x[i] - x[j]) * (long)(x[i] - x[j]) + (long)(y[i] - y[j]) * (long)(y[i] - y[j]);<br>        for(long s = 1; s * s &lt;= 4 * d; s++)<br>        {<br>          ret += s - 1;<br>        } <br>      }<br>    }<br>    return ret;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>