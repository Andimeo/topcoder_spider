            public class TurretPlacement {<br>  public long count(int[] x, int[] y) {<br>    int n = x.length;<br>    long res = 0;<br>    for (int i = 0; i &lt; n; i++) {<br>      for (int j = i + 1; j &lt; n; j++) {<br>        long d = (long) (2 * Math.hypot(x[i] - x[j], y[i] - y[j]) + 1e-8);<br>        res += d * (d - 1) / 2;<br>      }<br>    }<br>    return res;<br>  }<br><br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by moj 4.15 [modified TZTester]<br>// Powered by CodeProcessor<br></td>