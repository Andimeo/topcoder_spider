            import java.util.*;<br> <br>public class NetworkXOneTimePad {<br>  public int crack(String[] plaintexts, String[] ciphertexts) {<br>        long[] plain = convert(plaintexts);<br>        long[] cipher = convert(ciphertexts);<br>        int res = 0;<br>        for (long x : plain) {<br>            long key = cipher[0] ^ x;<br>            boolean ok = true;<br>            for (long a : cipher) {<br>                boolean found = false;<br>                for (long b : plain)<br>                    if ((a ^ key) == b)<br>                        found = true;<br>                if (!found) ok = false;<br>            }<br>            if (ok) ++res;<br>        }<br>    return res;<br>  }<br> <br>    private long[] convert(String[] arr) {<br>        long[] res = new long[arr.length];<br>        for (int i = 0; i &lt; arr.length; ++i) res[i] = Long.parseLong(arr[i], 2);<br>        return res;<br>    }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>