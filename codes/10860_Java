            public class LotteryTicket {<br> <br>  public String buy(int price, int b1, int b2, int b3, int b4) {<br>    int a[] = new int[]{b1, b2, b3, b4};<br>    for(int i=0; i&lt;1&lt;&lt;4; i++){<br>      int sum = 0;<br>      for(int j=0; j&lt;4; j++){<br>        if((i&amp;1&lt;&lt;j) &gt; 0)<br>          sum+=a[j];<br>      }<br>      if(sum == price)<br>        return "POSSIBLE";<br>    }<br>    return "IMPOSSIBLE";<br>  }<br> <br>}<br></td>