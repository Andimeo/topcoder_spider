            using System;<br>using System.Collections.Generic;<br><br>public class LotteryCheating {<br>  public int minimalChange(string ID) {<br>    int best = int.MaxValue;<br>    for (long i = 0; ; i++)<br>    {<br>      string s = (i*i).ToString();<br>      if (s.Length &gt; ID.Length)<br>        return best;<br>      while (s.Length &lt; ID.Length)<br>        s = "0" + s;<br>      int dif = 0;<br>      for (int j = 0; j &lt; ID.Length; j++)<br>      {<br>        if (ID[j] != s[j])<br>          dif++;<br>      }<br>      if (dif &lt; best) best = dif;<br>    }<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br></td>