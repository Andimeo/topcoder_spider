            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class LotteryCheating<br>{<br>  public int minimalChange(String id)<br>  {<br>    int n = id.length();<br>    int ans = n + 1;<br>    for (long i = 0; ; ++i) {<br>      String s = Long.toString(i * i);<br>      if (s.length() &gt; n) {<br>        break;<br>      }<br>      while (s.length() &lt; n) {<br>        s = "0" + s;<br>      }<br>      int cur = 0;<br>      for (int j = 0; j &lt; n; ++j) {<br>        if (s.charAt(j) != id.charAt(j)) {<br>          ++cur;<br>        }<br>      }<br>      ans = Math.min(ans, cur);<br>    }<br>    return ans;<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>