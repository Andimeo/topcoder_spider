            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class LotteryPyaterochka<br>{<br>  public double chanceToWin(int n0)<br>  {<br>    checked<br>    {<br>      long n = n0;<br>      long s = 0;<br>      //5<br>      s += n;<br>      //4,1<br>      s += 25 * n * (n - 1);<br>      //3,2<br>      s += 100 * n * (n - 1);<br>      //3,1,1<br>      s += 10 * 5 * 5 * n * (n - 1) * (n - 2) / 2;<br>      n *= 5;<br>      long t = n * (n - 1) * (n - 2) * (n - 3) * (n - 4) / 120;<br>      return (double) s / (double)t;<br>    }<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>