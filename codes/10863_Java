            import java.util.*;<br> <br>public class LotteryPyaterochka {<br>    public double chanceToWin(int N) {<br>        int[] am = new int[N];<br>        return doit(am, 5);<br>    }<br> <br>    private double doit(int[] am, int cnt) {<br>        if (cnt == 0) {<br>            double res = 0.0;<br>            for (int x : am)<br>                if (x &gt;= 3)<br>                    res = 1.0;<br>            return res;<br>        }<br>        int nzero = 0;<br>        int lzero = -1;<br>        int totalLeft = am.length * 5 - (5 - cnt);<br>        double res = 0.0;<br>        for (int i = 0; i &lt; am.length; ++i)<br>            if (am[i] == 0) {<br>                ++nzero;<br>                lzero = i;<br>            } else {<br>                if (am[i] &lt; 5) {<br>                    int[] nam = am.clone();<br>                    ++nam[i];<br>                    res += (5 - am[i]) / (double) totalLeft * doit(nam, cnt - 1);<br>                }<br>            }<br>        if (nzero &gt; 0) {<br>            int[] nam = am.clone();<br>            ++nam[lzero];<br>            res += (5 * nzero) / (double) totalLeft * doit(nam, cnt - 1);<br>        }<br>        return res;<br>    }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>