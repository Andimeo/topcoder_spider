            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections.Generic;<br><br>public class Islands {<br>    const double Eps = 1E-9;<br><br>    public int beachLength(string[] kingdom) {<br>        int res = 0;<br>        for (int i = 0; i &lt; kingdom.Length; i++)<br>        {<br>            for (int j = 0; j &lt; kingdom[i].Length; j++)<br>            {<br>                if (j != 0 &amp;&amp; kingdom[i][j - 1] != kingdom[i][j])<br>                    ++res;<br>                if (i != 0)<br>                {<br>                    int j1 = i%2 == 0 ? j - 1 : j;<br>                    if (j1 &gt;= 0 &amp;&amp; j1 &lt; kingdom[i - 1].Length &amp;&amp; kingdom[i-1][j1] != kingdom[i][j])<br>                        ++res;<br>                    j1++;<br>                    if (j1 &gt;= 0 &amp;&amp; j1 &lt; kingdom[i - 1].Length &amp;&amp; kingdom[i - 1][j1] != kingdom[i][j])<br>                        ++res;<br>                }<br>            }<br>        }<br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>