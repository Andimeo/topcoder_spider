            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class Islands {<br>public:<br>  int beachLength(vector &lt;string&gt;);<br>};<br> <br>int Islands::beachLength(vector &lt;string&gt; s) {<br>  int n = s.size(), m = s[0].length(), i, j, ans = 0, p;<br>  for (i=0;i&lt;n;i++)<br>    for (j=0;j&lt;m;j++) {<br>      if (j &lt; m-1) ans += s[i][j] != s[i][j+1];<br>      if (i &lt; n-1) ans += s[i][j] != s[i+1][j];<br>      if (i &amp; 1) p = j+1;<br>      else p = j-1;<br>      if (i &lt; n-1 &amp;&amp; p &gt;= 0 &amp;&amp; p &lt; m) ans += s[i][j] != s[i+1][p];<br>    }<br>  return ans;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>