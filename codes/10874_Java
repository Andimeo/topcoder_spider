            import java.util.*;<br> <br>public class Islands {<br>    public int beachLength(String[] kingdom) {<br>        int res = 0;<br>        for (int r = 0; r &lt; kingdom.length; ++r)<br>            for (int c = 0; c &lt; kingdom[r].length(); ++c) {<br>                if (kingdom[r].charAt(c) == '#') {<br>                    for (int dr = -1; dr &lt;= 1; ++dr)<br>                        for (int dc = -1; dc &lt;= 1; ++dc) {<br>                            if (dr == 0 &amp;&amp; dc == 0)<br>                                continue;<br>                            if (dr != 0 &amp;&amp; dc == 1 &amp;&amp; r % 2 == 0)<br>                                continue;<br>                            if (dr != 0 &amp;&amp; dc == -1 &amp;&amp; r % 2 != 0)<br>                                continue;<br>                            int nr = r + dr;<br>                            int nc = c + dc;<br>                            if (nr &gt;= 0 &amp;&amp; nr &lt; kingdom.length &amp;&amp; nc &gt;= 0 &amp;&amp; nc &lt; kingdom[nr].length() &amp;&amp; kingdom[nr].charAt(nc) == '.')<br>                                ++res;<br>                        }<br>                }<br>            }<br>        return res;<br>    }<br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>