            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class RabbitVoting<br>{<br>  public string getWinner(string[] names, string[] votes)<br>   {<br>      int[] numVotes = new int[names.Length];<br>      int i, j, maxVotes = 0, idx = -1, ctr = 0;<br>      <br>      for (i = 0; i &lt; names.Length; i++)<br>      {<br>         if (names[i] != votes[i])<br>         {<br>            for (j = 0; j &lt; names.Length; j++)<br>            {<br>               if (names[j] == votes[i])<br>               {<br>                  numVotes[j]++;<br>                  if (maxVotes &lt; numVotes[j])<br>                     maxVotes = numVotes[j];<br>               }<br>            }<br>         }<br>      }<br> <br>      for (i = 0; i &lt; names.Length; i++)<br>      {<br>         if (numVotes[i] == maxVotes)<br>         {<br>            idx = i;<br>            ctr++;<br>         }<br>      }<br> <br>      if (ctr != 1) return "";<br>      else return names[idx];<br>   }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>