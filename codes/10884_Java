<td class="problemText" colspan="8" valign="middle" align="left">
            import java.util.*;<br>import java.math.*;<br>public class VacationTime {<br>public int bestSchedule(int N,int k,int[] w) {<br>int d=N-k+1;<br>int t=0;<br>Arrays.sort(w);<br>int a=1;<br>ArrayList&lt;Integer&gt; list=new ArrayList&lt;Integer&gt;();<br>while(d--&gt;0)<br>{<br>for(int j=a;j&lt;=k;j++)<br>  {<br>    for(int l=0;l&lt;w.length;l++)<br>     {<br>      if(w[l]==j) t++;<br>     }//first for<br>  }//sec for<br> list.add(t);<br> t=0; <br> a++;<br> k++; <br> }//while<br>Collections.sort(list);<br>return list.get(0);<br>}<br><br>}<br></td>