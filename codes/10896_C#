<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class JingleRingle<br>{<br>  public int profit(int[] buyOffers, int[] sellOffers, int tax)<br>  {<br>        for (int i = 0; i &lt; buyOffers.Length; ++i)<br>            buyOffers[i] = buyOffers[i] - (buyOffers[i] * tax) / 100;<br>        Array.Sort(buyOffers);<br>        Array.Sort(sellOffers);<br>        int results = 0;<br>        int ringles = 0;<br>        for (int i = 0; i &lt; Math.Min(buyOffers.Length, sellOffers.Length); i++)<br>        {<br>            ringles += buyOffers[buyOffers.Length - 1 - i] - sellOffers[i];<br>            results = Math.Max(results, ringles);<br>        }<br>        return results;<br>  }<br>}<br></td>