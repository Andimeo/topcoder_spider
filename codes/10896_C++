            #include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br> <br>using namespace std;<br> <br>class JingleRingle {<br>public:<br>  int profit(vector &lt;int&gt;, vector &lt;int&gt;, int);<br>};<br> <br>int JingleRingle::profit(vector &lt;int&gt; b, vector &lt;int&gt; s, int t) {<br>  int m=0,pro=0,i,mid=0;<br>  if(s.size()==0&amp;&amp;b.size()==0)<br>    return 0;<br>  sort(s.begin(),s.end());<br>  sort(b.rbegin(),b.rend());<br>  for(i=0;(i&lt;b.size())&amp;&amp;(i&lt;s.size());i++)<br>  {<br>    m=(b[i]*t)/100;<br>    mid=(b[i]-m)-s[i];<br>    if(mid&lt;0)<br>      return pro;<br>    else<br>      pro+=mid;<br>  }<br>  return pro;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>