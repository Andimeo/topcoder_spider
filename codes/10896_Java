            import java.util.*;<br>public class JingleRingle{<br>   public int profit(int[] buyOffers, int[] sellOffers, int tax){<br>     Arrays.sort(buyOffers);<br>     Arrays.sort(sellOffers);<br>     int max = 0;<br>     for (int i=0,j=buyOffers.length-1;i&lt;sellOffers.length &amp;&amp; j&gt;=0;i++,j--) {<br>       int taxes = buyOffers[j]*tax/100;<br>       if (buyOffers[j]-sellOffers[i]-taxes &gt; 0) max += buyOffers[j]-sellOffers[i]-taxes;<br>       else break;<br>     }<br>     return max;<br>   }<br>}<br></td>