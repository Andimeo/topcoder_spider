            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class TheMoviesLevelOneDivOne<br>{<br>  public long find(int n, int m, int[] r, int[] s)<br>  {<br>    int t = 0;<br>    Dictionary&lt;int, List&lt;int&gt; &gt; a = new Dictionary&lt;int, List&lt;int&gt; &gt;();<br>    for(int i=0; i&lt;r.Length; i++)<br>    {<br>      if(!a.ContainsKey(r[i] - 1))<br>      {<br>        a[r[i] - 1] = new List&lt;int&gt;();<br>      }<br>      a[r[i] - 1].Add(s[i] - 1);<br>    }<br>    long ret = ((long)n - a.Count) * ((long)m - 1);<br>    foreach(List&lt;int&gt; l in a.Values)<br>    {<br>      l.Sort();<br>      ret += hh(l, m);<br>    }<br>    return ret;<br>  }<br>  <br>  long hh(List&lt;int&gt; l, int m)<br>  {    <br>    long ret = 0;<br>    if(l[0] - 1 &gt; 0)<br>    {<br>      ret += l[0] - 1;<br>    }<br>    for(int i=0; i&lt;l.Count - 1; i++)<br>    {<br>      if(l[i+1] - l[i] &gt;= 3)<br>      {<br>        ret += l[i + 1] - l[i] - 2;<br>      }<br>    }<br>    if(m - 2 - l[l.Count - 1] &gt; 0)<br>    {<br>      ret += m - 2 - l[l.Count - 1];<br>    }<br>    return ret;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>