            import java.util.*;<br> <br>public class TheMoviesLevelOneDivOne {<br>    public long find(int n, int m, int[] row, int[] seat) {<br>        Set&lt;Integer&gt; interestingRows = new HashSet&lt;Integer&gt;();<br>        for (int x : row)<br>            interestingRows.add(x);<br>        long res = 0;<br>        res += ((long) (n - interestingRows.size())) * (m - 1);<br>        for (int x : interestingRows) {<br>            Set&lt;Integer&gt; columns = new HashSet&lt;Integer&gt;();<br>            for (int i = 0; i &lt; row.length; ++i)<br>                if (row[i] == x)<br>                    columns.add(seat[i]);<br>            columns.add(0);<br>            columns.add(m + 1);<br>            Integer[] c = columns.toArray(new Integer[0]);<br>            Arrays.sort(c);<br>            for (int i = 0; i &lt; c.length - 1; ++i) {<br>                int am = c[i + 1] - c[i] - 1;<br>                if (am &gt;= 2)<br>                    res += am - 1;<br>            }<br>        }<br>        return res;<br>    }<br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>