            using System;<br> <br>public class TheMoviesLevelOneDivTwo {<br>  public int find(int n, int m, int[] row, int[] seat) {<br>    int res = 0;<br>    bool[,] r = new bool[n, m];<br>    for(int i = 0; i &lt; row.Length; ++i)<br>      r[row[i]-1, seat[i]-1] = true;<br>    for (int i = 0; i &lt; n; ++i)<br>      for (int j = 0; j &lt; m - 1; ++j)<br>        if (!r[i, j] &amp;&amp; !r[i, j + 1])<br>          ++res;<br>    return res;<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>