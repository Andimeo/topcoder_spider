            #include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;iostream&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class TheMoviesLevelOneDivTwo {<br>public:<br>int find(int, int, vector &lt;int&gt;, vector &lt;int&gt;);<br>};<br> <br>int TheMoviesLevelOneDivTwo::find(int n, int m, vector &lt;int&gt; row, vector &lt;int&gt; seat) {<br>int answer[n][m],j,count=0,i;<br>int l=row.size();<br>for(i=0;i&lt;n;i++)<br>{<br>  for(j=0;j&lt;m;j++)<br>  {<br>    answer[i][j]=0;<br>  }<br>}<br>for(i=0;i&lt;l;i++)<br>  answer[row[i]-1][seat[i]-1]=1;<br>for(i=0;i&lt;n;i++)<br>{<br>  for(j=0;j&lt;m;j++)<br>  {<br>  if((j+1)&lt;m)<br>  {<br>    if(answer[i][j+1]==0&amp;&amp;answer[i][j]==0){<br>    count++;<br>    }<br>  }<br>}<br>}<br>return count;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>