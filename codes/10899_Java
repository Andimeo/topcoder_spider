            public class TheMoviesLevelOneDivTwo<br>{<br>    public static int find(int n, int m, int[] row, int[] seat)<br>    {<br>        int s[][] = new int[n][m];<br>        int ans = 0;<br>        for (int i = 0 ; i &lt; row.length ; i++)<br>        {<br>            s[row[i]-1][seat[i]-1] = 1;<br>        }<br><br>        for(int i = 0 ; i &lt; n ; i++)<br>        {<br>            for(int j = 0 ; j &lt; m-1 ; j++)<br>            {<br>                if(s[i][j] == 0 &amp;&amp; s[i][j+1] == 0)<br>                    ans++;<br>            }<br>        }<br>        return ans;<br>    }<br>    <br>}<br></td>