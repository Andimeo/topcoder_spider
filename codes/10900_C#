            // made by diver_ru, made with love<br>using System;<br>using System.Collections.Generic;<br><br>public class TheMoviesLevelTwoDivOne<br>{<br>  public int[] find(int[] length, int[] scary)<br>  {<br>    int N = length.Length;<br>    for (int i = 0; i &lt; N; ++i)<br>    {<br>      length[i] *= 2;<br>      scary[i] *= 2;<br>    }<br><br>    int[] maskScary = new int[1 &lt;&lt; N];<br>    for (int i = 0; i &lt; (1 &lt;&lt; N); ++i)<br>    {<br>      maskScary[i] = 74 * 2 + 1;<br>      for (int j = 0; j &lt; N; ++j)<br>        if (((i &gt;&gt; j) &amp; 1) == 0)<br>          maskScary[i] += 47 * 2 - length[j];<br>    }<br><br>    List&lt;int&gt; res = new List&lt;int&gt;();<br><br>    int[] tab = new int[1 &lt;&lt; N];<br>    int[] next = new int[1 &lt;&lt; N];<br>    for (int i = 0; i &lt; (1 &lt;&lt; N); ++i)<br>    {<br>      if (maskScary[i] &lt; 0)<br>        continue;<br>      tab[i] = 1;<br>      for (int j = 0; j &lt; N; ++j)<br>        if (((i &gt;&gt; j) &amp; 1) != 0 &amp;&amp; maskScary[i] - scary[j] &gt;= 0 &amp;&amp; tab[i ^ (1 &lt;&lt; j)] != 0)<br>        {<br>          if (tab[i] &lt; tab[i ^ (1 &lt;&lt; j)] + 1)<br>          {<br>            tab[i] = tab[i ^ (1 &lt;&lt; j)] + 1;<br>            next[i] = j + 1;<br>          }<br>        }<br>    }<br><br>    int cur = (1 &lt;&lt; N) - 1;<br>    while (next[cur] &gt; 0)<br>    {<br>      res.Add(next[cur] - 1);<br>      cur ^= 1 &lt;&lt; (next[cur] - 1);<br>    }<br>    for (int i = 0; i &lt; N; ++i)<br>    {<br>      if (((cur &gt;&gt; i) &amp; 1) != 0)<br>        res.Add(i);<br>    }<br><br>    return res.ToArray();<br>  }<br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]; C# [05-Aug-2008]<br>// Powered by CodeProcessor<br></td>