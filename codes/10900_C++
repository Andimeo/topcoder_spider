            #include &lt;sstream&gt; <br>#include &lt;iostream&gt;<br>#include &lt;string.h&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;stdlib.h&gt; <br>#include &lt;stdio.h&gt; <br>#include &lt;numeric&gt;<br>#include &lt;math.h&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;stack&gt;<br>#include &lt;queue&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>using namespace std;<br> <br>int n;<br>vector &lt; int &gt; a , b;<br>int dp[1 &lt;&lt; 20] , next[1 &lt;&lt; 20];<br>int used[32];<br> <br>int go ( int mask , int lvl ) {<br>  int &amp;ans = dp[mask];<br>  if ( ans != -1 ) return ans;<br>  ans = 0;<br>  <br>  int i;<br>  <br>  for (i = 0; i &lt; n; i++)<br>    if ( !(mask &amp; (1 &lt;&lt; i)) ) {<br>      if ( lvl - b[i] &gt;= 0 &amp;&amp; (lvl - b[i] + 47 - (a[i] - b[i])) &gt;= 0 ) {<br>        if ( go ( mask | (1 &lt;&lt; i) , lvl - a[i] + 47 ) + 1 &gt; ans ) {<br>          ans = go ( mask | (1 &lt;&lt; i) , lvl - a[i] + 47 ) + 1;<br>          next[mask] = i;<br>        }<br>      }<br>    }<br>    <br>  return ans;<br>}<br> <br>vector &lt;int&gt; solve ( vector &lt;int&gt; length, vector &lt;int&gt; scary ) {<br>  a = length;<br>  b = scary;<br>  <br>  n = (int)a.size();<br>  vector &lt; int &gt; ans;<br>  int i;<br>  <br>  memset ( dp , -1 , sizeof dp );<br>  memset ( next , -1 , sizeof next );<br> <br>  go ( 0 , 74 );<br>  <br>  for (i = 0; next[i] != -1; i |= 1 &lt;&lt; next[i]) {<br>    ans.push_back ( next[i] );<br>    used[ next[i] ] = 1;<br>  }<br>  <br>  for (i = 0; i &lt; n; i++)<br>    if ( !used[i] )<br>      ans.push_back ( i );<br>    <br>  return ans;<br>}<br> <br>class TheMoviesLevelTwoDivOne {<br>  public:<br>  vector &lt;int&gt; find(vector &lt;int&gt; length, vector &lt;int&gt; scary) {<br>    return solve ( length, scary );<br>  }<br>};<br> <br> <br> <br>// Powered by FileEdit<br>// Powered by moj 4.12 [modified TZTester]<br>// Powered by CodeProcessor<br></td>