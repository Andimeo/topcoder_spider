            using System;<br>using System.Collections;<br>using System.Collections.Generic; <br><br>public class TheMoviesLevelTwoDivTwo <br>{<br>  public int[] find(int[] length, int[] scary)<br>  {<br>        if (length.Length == 1)<br>        {<br>            int[] pqqw = { 0 };<br>            return pqqw;<br>        }<br>        <br>        int[] ans = new int[length.Length];<br>        int[] ret = new int[length.Length];<br>        int count = -1;<br>        for (int i = 0; i &lt; ans.Length; i++)<br>        {<br>            ans[i] = i;<br>            ret[i] = 0;<br>        }<br><br>        ans[ans.Length - 1]--;<br>        bool keepOn = true;<br><br>        while (keepOn)<br>        {<br>            for (int i = ans.Length - 1; i &gt;= 0; i--)<br>            {<br>                if (ans[i] == ans.Length - 1)<br>                    ans[i] = 0;<br>                else<br>                {<br>                    ans[i]++;<br>                    break;<br>                }<br>            }<br><br>            for (int i = 0; i &lt; ans.Length; i++)<br>                if (ans[i] != 0) break;<br>                else if (i == ans.Length - 1 &amp;&amp; ans[i] == 0) keepOn = false;<br><br>            bool good = true;<br>            for(int i = 0;i&lt;ans.Length &amp;&amp; good;i++)<br>                for(int j = i+1;j&lt;ans.Length;j++)<br>                    if (ans[i] == ans[j])<br>                    {<br>                        good = false; break;<br>                    }<br>            if (!good) continue;<br><br>            int thisCount = 0;<br>            int scareLevel = 74;<br>            for (int i = 0; i &lt; ans.Length; i++)<br>            {<br>                scareLevel -= scary[ans[i]];<br>                if (scareLevel &lt; 0)<br>                    break;<br><br>                scareLevel += 47;<br><br>                scareLevel -= length[ans[i]] - scary[ans[i]];<br>                if (scareLevel &lt; 0)<br>                    break;<br><br>                thisCount++;<br>            }<br><br>            if (count &gt; thisCount) continue;<br>            if (count &lt; thisCount)<br>            {<br>                count = thisCount;<br>                ans.CopyTo(ret, 0);<br>            }<br><br>            for (int i = 0; i &lt; ret.Length; i++)<br>            {<br>                if (ans[i] &gt; ret[i])<br>                    break;<br>                else if (ans[i] &lt; ret[i])<br>                {<br>                    ans.CopyTo(ret, 0);<br>                    break;<br>                }<br>            }<br>        }<br><br>        return ret;<br>  }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>