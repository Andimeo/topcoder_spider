            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections.Generic;<br><br>public class DoorsGame {<br>    const double Eps = 1E-9;<br><br>    public int determineOutcome(string doors, int trophy) {<br>        bool[] first = new bool[16], second = new bool[16];<br>        for (int i = 0; i &lt; doors.Length; i++)<br>        {<br>            if (i &lt; trophy) first[doors[i] - 'A'] = true;<br>            else second[doors[i] - 'A'] = true;<br>        }<br>        int a = 0, b = 0, c = 0;<br>        for (int i = 0; i &lt; first.Length; i++)<br>        {<br>            if (first[i] &amp;&amp; second[i]) ++c;<br>            else if (first[i]) ++a;<br>            else if (second[i]) ++b;<br>        }<br>        return f(a, b, c);<br>    }<br><br>    int f(int a, int b, int c)<br>    {<br>        if (b &gt;= a + c) return 2*(a + c) - 1;<br>        if (a &gt; b + c) return -2*(b + c);<br>        return 0;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>