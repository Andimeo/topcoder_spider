<td class="problemText" colspan="8" valign="middle" align="left">
            public class DoorsGame {<br>  public int determineOutcome(String doors, int trophy) {<br>    boolean[] dleft = new boolean[16], dright = new boolean[16];<br>    <br>    for (int i = 0; i &lt; trophy; i++) {<br>      dleft[doors.charAt(i) - 'A'] = true;<br>    }<br>    for (int i = trophy; i &lt; doors.length(); i++) {<br>      dright[doors.charAt(i) - 'A'] = true;<br>    }<br>    <br>    int a = 0, b = 0, c = 0;<br>    for (int i = 0; i &lt; 16; i++) {<br>      if (dleft[i] &amp;&amp; dright[i]) {<br>        a++;<br>      } else <br>      if (dleft[i]) {<br>        b++;<br>      } else<br>      if (dright[i]) {<br>        c++;<br>      }<br>      <br>      <br>    }<br>    <br>    if (b + a &lt;= c) {<br>      return Math.min(2 * (b + a) - 1, 16);<br>    }<br>    if (c + a &lt; b) {<br>      return -Math.min(2 * (c + a), 16);<br>    }<br>    return 0;<br>  }  <br>}<br></td>