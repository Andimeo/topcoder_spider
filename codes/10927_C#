            using System;<br>using System.Collections.Generic;<br><br>public class WhatsThisChord {<br>  public string classify(int[] chord) {<br>        string[] name = new string[] { "C", "C#", "D", "D#", "E", "F", "F#", "G", "G#", "A", "A#", "B" };<br>        int[] empty = new int[] { 4, 9, 2, 7, 11, 4 };<br>        int[] major = new int[] { 0, 4, 7};<br>        int[] minor = new int[] { 0, 3, 7 };<br>        int used = 0;<br>        for (int i = 0; i &lt; 6; i++)<br>        {<br>            if (chord[i] == -1) continue;<br>            used |= 1&lt;&lt;((empty[i]+chord[i])%12);<br>        }<br>        for (int i = 0; i &lt; 12; i++)<br>        {<br>            int c = 0;<br>            for (int j = 0; j &lt; 3; j++)<br>            {<br>                c |= 1&lt;&lt;((i + major[j]) % 12);<br>            }<br>            if (used == c) return name[i] + " Major";<br>        }<br>        for (int i = 0; i &lt; 12; i++)<br>        {<br>            int c = 0;<br>            for (int j = 0; j &lt; 3; j++)<br>            {<br>                c |= 1 &lt;&lt; ((i + minor[j]) % 12);<br>            }<br>            if (used == c) return name[i] + " Minor";<br>        }<br>        return "";<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>