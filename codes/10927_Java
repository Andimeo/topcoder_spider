            import java.util.*;<br><br>public class WhatsThisChord {<br>  String[] types = "C C# D D# E F F# G G# A A# B".split(" ");<br>  public String classify(int[] chord) {<br>    boolean[] isnote = new boolean[12];<br>    int[] offset = {4,9,2,7,11,4};<br>    for (int i=0; i&lt;6; i++) {<br>      if (chord[i]==-1) continue;<br>      isnote[(chord[i]+offset[i])%12]=true;<br>    }<br>    int numnotes = 0;<br>    for (int i=0; i&lt;12; i++) if (isnote[i]) numnotes++;<br>    if (numnotes!=3) return "";<br>    for (int i=0; i&lt;12; i++) {<br>      if (isnote[i] &amp;&amp; isnote[(i+4)%12] &amp;&amp; isnote[(i+7)%12]) return types[i]+" Major";<br>    }<br>    for (int i=0; i&lt;12; i++) {<br>      if (isnote[i] &amp;&amp; isnote[(i+3)%12] &amp;&amp; isnote[(i+7)%12]) return types[i]+" Minor";<br>    }    <br>    return "";<br>  }<br>}<br></td>