            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>using System.Text;<br><br>public class SieveOfEratosthenes<br>{<br>    public int lastScratch(int input)<br>    {<br>        bool[] comps = new bool[(int)Math.Sqrt(input) + 4];<br>        for (int i = 2; i * i &lt;= input; i++)<br>        {<br>            if (!comps[i])<br>            {<br>                for (int j = i * 2; j &lt; comps.Length; j+=i)<br>                {<br>                    comps[j] = true;<br>                }<br>            }<br>        }<br>        List&lt;int&gt; primes = new List&lt;int&gt;();<br>        int limit = (int)Math.Sqrt(input);<br>        for (int i = 2; i &lt;= limit; i++)<br>        {<br>            if (!comps[i])<br>            {<br>                primes.Add(i);<br>            }<br>        }<br>        int mult = input / primes[primes.Count-1];<br>        while (mult &gt;= 2)<br>        {<br>            bool bad = false;<br>            for (int i = 0; i &lt; primes.Count - 1; i++)<br>            {<br>                if (mult % primes[i] == 0)<br>                {<br>                    bad = true;<br>                    break;<br>                }<br>            }<br>            if (bad)<br>                mult--<br>                   ;<br>            else break;<br>        }<br>        return primes[primes.Count - 1] * mult;<br>    }<br><br>}<br></td>