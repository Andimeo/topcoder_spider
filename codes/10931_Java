            import java.util.*;<br> <br>public class SieveOfEratosthenes {<br> <br>    boolean isPrime(int n) {<br>        for (int j = 2; j * j &lt;= n; j++) {<br>            if (n % j == 0) {<br>                return false;<br>            }<br>        }<br>        return true;<br>    }<br>    <br>    public int lastScratch(int maxNum) {<br>        int[] p = new int[10000];<br>        int pn = 0;<br>        for (int i = 2; i * i &lt;= maxNum; i++) {<br>            boolean ok = true;<br>            for (int j = 0; j &lt; pn &amp;&amp; p[j] * p[j] &lt;= i; j++) {<br>                if (i % p[j] == 0) {<br>                    ok = false;<br>                }<br>            }<br>            if (ok) {<br>                p[pn++] = i;<br>            }<br>        }<br> <br>        int ans = 0;<br>        for (int i = p[pn - 1]; p[pn - 1] * i &lt;= maxNum; i++) {<br>            if (isPrime(i) || maxNum == 8) {<br>                ans = i * p[pn - 1];<br>            }<br>        }<br> <br>        return ans;<br>    }<br> <br>}<br> <br> <br>// Powered by PopsEdit<br> <br>// Powered by CodeProcessor<br></td>