            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class EqualizeStrings<br>{<br>  public string getEq(string s, string t)<br>  {<br>    string r = "";<br>    for (int i = 0; i &lt; s.Length; i++)<br>      r += getEq(s[i], t[i]);<br>    return r;<br>  }<br>  <br>  char getEq(char a, char b)<br>  {<br>    if (Math.Abs(a-b) &gt;= 13) return 'a';<br>    if (a &lt; b) return a;<br>    return b;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>