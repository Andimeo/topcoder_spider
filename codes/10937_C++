            #line 77 "TwoRegisters.cpp"<br>#include &lt;vector&gt;                                                <br>#include &lt;map&gt;                                                   <br>#include &lt;set&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;cmath&gt;<br>#include &lt;string&gt;<br>#include &lt;complex&gt;<br>using namespace std;<br>class TwoRegisters {<br>   char tmp[1000005];<br>   public:<br>      string minProg(int r) {<br>         int i;<br>   string ret;<br>   for (i = 1; i &lt;= r; i ++) {<br>      int x = r;<br>      int y = i;<br>      int len = 0;<br>      while (x &gt; 0 &amp;&amp; y &gt; 0 &amp;&amp; (x &gt; 1 || y &gt; 1)) {<br>         if (x &gt;= y) {<br>      x -= y;<br>      tmp[len ++] = 'X';<br>         }<br>         else {<br>      y -= x;<br>      tmp[len ++] = 'Y';<br>         }<br>      }<br>      if (x == 1 &amp;&amp; y == 1) {<br>         string cnt;<br>         reverse(tmp, tmp + len);<br>         tmp[len] = 0;<br>         cnt = string(tmp);<br>         if (ret == "" || ret.length() &gt; len || ret.length() == len &amp;&amp; ret &gt; cnt)<br>      ret = cnt;<br>      }<br>   }<br>   return ret;<br>      }<br>};<br> <br> <br>// Powered by FileEdit<br></td>