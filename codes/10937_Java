            import java.util.*;<br> <br>public class TwoRegisters {<br>    StringBuilder bu;<br> <br>    public String minProg(int r) {<br>        int min = Integer.MAX_VALUE;<br>        List&lt;Integer&gt; others = new ArrayList&lt;Integer&gt;();<br>        bu = null;<br>        for (int other = 1; other &lt;= r; ++other) {<br>            int cur = doit(r, other);<br>            if (cur &lt;= min) {<br>                if (cur &lt; min) others.clear();<br>                min = cur;<br>                others.add(other);<br>            }<br>        }<br>        String res = "Z";<br>        for (int other : others) {<br>            bu = new StringBuilder();<br>            doit(r, other);<br>            String cur = bu.reverse().toString();<br>            if (cur.compareTo(res) &lt; 0)<br>                res = cur;<br>        }<br>        return res;<br>    }<br> <br>    private int doit(int a, int b) {<br>        int res = 0;<br>        while (a &gt; 1 || b &gt; 1) {<br>            if (a == b)<br>                return Integer.MAX_VALUE;<br>            if (a &gt; b) {<br>                a -= b;<br>                if (bu != null) {<br>                    bu.append('X');<br>                }<br>            } else {<br>                b -= a;<br>                if (bu != null) {<br>                    bu.append('Y');<br>                }<br>            }<br>            ++res;<br>        }<br>        return res;<br>    }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>