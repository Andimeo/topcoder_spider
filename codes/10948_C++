            #include &lt;vector&gt;<br>#include &lt;fstream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;set&gt;<br>#include &lt;string&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;iostream&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;map&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cmath&gt;<br>#include &lt;queue&gt;<br>#include &lt;numeric&gt;<br>#include &lt;cstdio&gt;<br><br>using namespace std;<br><br>#define fe(i,a,n) for(int i = a, _n = n; i &lt; _n; i++)<br>#define fi(i,a,n) for(int i = a, _n = n; i &lt;= _n; i++)<br>#define LL long long<br><br>class SumRectangle {<br>public:<br>        int getCorner(vector &lt;int&gt; lc, vector &lt;int&gt; tr) {<br>            vector&lt;vector&lt;int&gt; &gt; v;<br>            int n = lc.size(), m = tr.size();<br>            v.resize(n);<br>            fe(i,0,n)<br>                v[i].resize(m);<br>            fe(i,0,n)<br>                v[i][0] = lc[i];<br>            fe(j,0,m)<br>                v[0][j] = tr[j];<br>            fe(i,1,n) fe(j,1,m)<br>                v[i][j] = v[i-1][j-1] - v[i-1][j] - v[i][j-1];<br>            return v[n-1][m-1];<br>        }<br>        <br><br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>