            import java.util.*;<br><br>public class SumRectangle {<br>  public int getCorner(int[] leftColumn, int[] topRow) {<br>    int[][] mat = new int[leftColumn.length][topRow.length];<br>    for (int i=0; i&lt;leftColumn.length; i++) mat[i][0]=leftColumn[i];<br>    for (int i=0; i&lt;topRow.length; i++) mat[0][i]=topRow[i];    <br>    int N = mat.length;<br>    int M = mat[0].length;<br>    for (int i=1; i&lt;N; i++)<br>    for (int j=1; j&lt;M; j++) {<br>      mat[i][j] = mat[i-1][j-1]-mat[i-1][j]-mat[i][j-1];<br>    }<br>    return mat[N-1][M-1];<br>  }<br>}<br></td>