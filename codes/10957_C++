            #include &lt;cstdio&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;cassert&gt;<br>#include &lt;climits&gt;<br>#include &lt;cstring&gt;<br>#include &lt;utility&gt;<br>#include &lt;queue&gt;<br>#include &lt;stack&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;algorithm&gt;<br>using namespace std;<br><br>#ifdef mahou_shoujo_madoka<br>    #define LLF "%I64d"<br>    #define ASSERT(x) assert(x)<br>#else<br>    #define LLF "%lld"<br>    #define ASSERT(x)<br>#endif<br><br>typedef long long LL;<br>typedef pair&lt;int,int&gt; PII;<br><br>class CountingSeries{<br>public:<br>    long long countThem(long long a, long long b, long long c, long long d, long long hi){<br>        long long ret=0;<br>        if(hi&gt;=a) ret=(hi-a)/b+1;<br>        for(long long x=c;x&lt;=hi;x*=d){<br>            if(x&lt;a || (x-a)%b) ret++;<br>            if(d==1) break;<br>        }<br>        return ret;<br>    }<br>};<br></td>