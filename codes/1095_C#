            using System; using System.Collections; public class Rooms { public int[] finalRooms(String[] rooms, String doors, int start) {<br>Hashtable set = new Hashtable();<br>set[start]=true;<br>foreach (char d in doors)<br> {<br> Hashtable s2 = new Hashtable();<br> foreach (int r in set.Keys)<br>  foreach (String adj in rooms[r].Split(new char[]{' '}))<br>   if (adj[0] == d)<br>    {<br>    foreach (String a2 in adj.Substring(2).Split(new char[]{','}))<br>     s2[Convert.ToInt32(a2)] = true;<br>    }<br> <br> set = s2;<br> }<br> <br>int[] result = new int[set.Count];<br>int Z = 0;<br>foreach (int R in set.Keys) { result[Z++] = R; }<br>Array.Sort(result);<br>return result;<br>}}<br></td>