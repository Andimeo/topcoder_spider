            Imports Microsoft.VisualBasic<br>Imports System.Math<br>Imports System<br>Imports System.Collections<br> <br>Public Class Rooms<br>    Public Function finalRooms(ByVal rooms As String(), ByVal doors As String, ByVal start As Integer) As Integer()<br>        Dim pass(50, rooms.Length - 1, rooms.Length - 1) As Boolean<br>        Dim room As Integer<br>        Dim a, b, c As Integer<br>        For room = 0 To rooms.Length - 1<br>            Dim d As String() = rooms(room).Split(" "c)<br>            Dim p As String<br>            For Each p In d<br>                If p.IndexOf(":"c) &gt; -1 Then<br>                    Dim toroom() As String = p.Split(":"c, ","c)<br>                    Dim ind As Integer = AscW(toroom(0).Chars(0)) - AscW("A"c)<br>                    For a = 1 To toroom.Length - 1<br>                        pass(ind, room, toroom(a)) = True<br>                    Next<br>                End If<br>            Next<br>        Next<br>        Dim currooms(rooms.Length - 1) As Boolean<br>        Dim nextrooms(rooms.Length - 1) As Boolean<br>        currooms(start) = True<br>        For a = 0 To doors.Length - 1<br>            ReDim nextrooms(rooms.Length - 1)<br>            For b = 0 To rooms.Length - 1<br>                If currooms(b) Then<br>                    For c = 0 To rooms.Length - 1<br>                        If pass(AscW(doors.Chars(a)) - AscW("A"c), b, c) Then<br>                            nextrooms(c) = True<br>                        End If<br>                    Next<br>                End If<br>            Next<br>            currooms = nextrooms<br>        Next<br>        Dim al As New ArrayList<br>        For a = 0 To rooms.Length - 1<br>            If currooms(a) Then al.Add(a)<br>        Next<br>        Return al.ToArray(GetType(Integer))<br>    End Function<br>End Class<br></td>