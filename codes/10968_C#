            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class KnightCircuit2<br>{<br>  public int maxSize(int w, int h)<br>  {<br>    if (w == 1 || h == 1) return 1;<br>    if (w == 2) return (h + 1) / 2;<br>    if (h == 2) return (w + 1) / 2;<br>    if (h == 3 &amp;&amp; w == 3) return 8;<br>    return w * h;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>