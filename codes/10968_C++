            #include &lt;cstdio&gt;<br>#include &lt;cstring&gt;<br>#include &lt;set&gt;<br>#include &lt;vector&gt;<br>using namespace std;<br><br>class KnightCircuit2<br>{<br>public:<br>    int maxSize(int col, int row)<br>    {<br>        if (row &gt; col) swap(row, col);<br>    if (row == 3)<br>        if(col == 3)<br>                return 8;<br>            else if(col &lt; 3)<br>                return 1;<br>    if (row == 2)<br>            return (col + 1) &gt;&gt; 1;<br>    if (row == 1)<br>            return 1;<br>    return row * col;<br>    }<br>};<br></td>