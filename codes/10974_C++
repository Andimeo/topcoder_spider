            #include&lt;cstdio&gt;<br>#include&lt;iostream&gt;<br>#include&lt;string&gt;<br>#include&lt;sstream&gt;<br>#include&lt;set&gt;<br>#include&lt;cmath&gt;<br>#include&lt;map&gt;<br>#include&lt;vector&gt;<br>#include&lt;algorithm&gt;<br>#include&lt;queue&gt;<br><br>using namespace std;<br>struct OnTheFarmDivTwo{<br>        vector&lt;int&gt; animals(int k1,int k2 )<br>        {<br>                vector &lt;int&gt; ans;<br>                ans.clear();<br>                int x=-1; int y=-1;<br><br>                if ((k2-2*k1)%2==0)<br>                {<br>                        y=(k2-2*k1)/2;<br>                        x=k1-y;<br>                }<br>                if (x&lt;0) return ans;<br>                if (y&lt;0) return ans;<br>                ans.push_back(x);<br>                ans.push_back(y);<br>        }<br>};<br></td>