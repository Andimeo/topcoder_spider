            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class OnTheFarmDivTwo<br>{<br>  public int[] animals(int heads, int legs)<br>  {<br>    int cantidad[]=new int[2];<br>    if(legs%2!=0)return new int[0];<br>    cantidad[0]=(4*heads-legs)/2;<br>    cantidad[1]=heads-cantidad[0];<br>    if(cantidad[0]&lt;0 || cantidad[1]&lt;0) return new int[0];<br>    return cantidad;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>