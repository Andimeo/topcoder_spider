            using System;<br> <br>public class ChildlessNumbers {<br>  public int howMany(int A, int B) {<br>    int res = 0;<br>    for (int i = A; i &lt;= B; ++i)<br>    {<br>      bool f = false;<br>      for (int j = 1; j &lt;= 200; ++j)<br>      {<br>        long p = (long)i * j;<br>        int sum = 0;<br>        while (p &gt; 0)<br>        {<br>          sum += (int)(p % 10);<br>          p /= 10;<br>        }<br>        if (sum == j)<br>        {<br>          f = true;<br>          break;<br>        }<br>      }<br>      if (!f) ++res;<br>    }<br>    return res;<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>