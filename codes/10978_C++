            #include&lt;cstdio&gt;<br>#include&lt;iostream&gt;<br>#include&lt;string&gt;<br>#include&lt;sstream&gt;<br>#include&lt;set&gt;<br>#include&lt;cmath&gt;<br>#include&lt;map&gt;<br>#include&lt;vector&gt;<br>#include&lt;algorithm&gt;<br>#include&lt;queue&gt;<br><br>using namespace std;<br>struct ChildlessNumbers{<br>        int howMany(int n,int m)<br>        {    long long y; int sum,ans;<br>             ans=0;<br>             for (int i=n; i&lt;=m; i++)<br>             {<br>                     bool flag=true;<br>                     for (int j=1; j&lt;=126; j++)<br>                     {<br>                            y=i; y=y*j;<br>                            sum=0;<br>                            while (y&gt;0)<br>                            {<br>                                    sum=sum+y%10;<br>                                    y=y/10;<br>                            }<br>                            if (sum==j)<br>                            {<br>                                    flag=false;<br>                                    break;<br>                            }<br>                     }<br>                     if (flag) ans++;<br>             }<br>             return ans;<br>        }<br>};<br></td>