            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class SequenceOfCommands<br>{<br>  int[] dx = { 1, 0, -1, 0 };<br>  int[] dy = { 0, 1, 0, -1 };<br>  public string whatHappens(string[] commands)<br>  {<br>    int x = 0, y = 0, d = 0;<br>    <br>    string cmd = "";<br>    for ( int i = 0; i &lt; commands.Length; i++ ) {<br>      cmd += commands[i];<br>    }<br>    <br>    for ( int i = 0; i &lt; 4; i++ ) {<br>      for ( int j = 0; j &lt; cmd.Length; j++ ) {<br>        switch( cmd[j] ) {<br>          case 'S':<br>            x += dx[d];<br>            y += dy[d];<br>            break;<br>          case 'L':<br>            d = ( d + 3 ) % 4;<br>            break;<br>          case 'R':<br>            d = ( d + 1 ) % 4;<br>            break;<br>          default:<br>            break;<br>        }<br>      }<br>    }<br>    <br>    if ( x == 0 &amp;&amp; y == 0 ) return "bounded";<br>    return "unbounded";<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>