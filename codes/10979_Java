            import java.util.*;<br> <br>public class SequenceOfCommands {<br>    public String whatHappens(String[] commands) {<br>        int x = 0;<br>        int y = 0;<br>        int d = 0;<br>        int[] dx = new int[]{1, 0, -1, 0};<br>        int[] dy = new int[]{0, 1, 0, -1};<br>        for (int step = 0; step &lt; 4; ++step) {<br>            for (String s : commands) {<br>                for (int i = 0; i &lt; s.length(); ++i) {<br>                    char ch = s.charAt(i);<br>                    if (ch == 'L') d = (d + 3) % 4;<br>                    if (ch == 'R') d = (d + 1) % 4;<br>                    if (ch == 'S') {<br>                        x += dx[d];<br>                        y += dy[d];<br>                    }<br>                }<br>            }<br>        }<br>        if (x == 0 &amp;&amp; y == 0)<br>            return "bounded";<br>        else<br>            return "unbounded";<br>    }<br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>