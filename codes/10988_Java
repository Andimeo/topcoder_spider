            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class BankLottery<br>{<br>  public double expectedAmount(int[] accountBalance, int weeklyJackpot, int weekCount)<br>  {<br>    long sum = weeklyJackpot * weekCount;<br>    long balSum = 0;<br>    for (int a : accountBalance) {<br>      balSum += a;<br>    }<br>    return accountBalance[0] + accountBalance[0]*((double)sum)/balSum;<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>