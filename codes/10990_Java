<td class="problemText" colspan="8" valign="middle" align="left">
            public class TheChroniclesOfAmber {<br> <br>  int N;<br> <br>  public double minimumTime(int[] sX, int[] sY, int[] gX, int[] gY) {<br>    N = sX.length;<br>    double ans = 0;<br>    for (int i = 0; i &lt; N; ++i) {<br>      ans = Math.max((gX[i] - sX[i]) * (gX[i] - sX[i]) + (gY[i] - sY[i]) * (gY[i] - sY[i]), ans);<br>    }<br>    for (int i = 0; i &lt; N; ++i) {<br>      double max = 0;<br>      for (int j = 0; j &lt; N; ++j) {<br>        double min = Double.MAX_VALUE;<br>        for (int k = 0; k &lt; N; ++k) {<br>          if (k == i) continue;<br>          min = Math.min((gX[j] - sX[k]) * (gX[j] - sX[k]) + (gY[j] - sY[k]) * (gY[j] - sY[k]), min);<br>        }<br>        max = Math.max(min, max);<br>      }<br>      ans = Math.min(ans, max);<br>    }<br>    return Math.sqrt(ans);<br>  }<br> <br>}<br></td>