            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class OneRegister<br>{<br>    Dictionary&lt;long, int&gt; m = new Dictionary&lt;long, int&gt;();<br>    public string getProgram(long s, long t)<br>    {<br>        int length = Math.Min(doIt(s, t), doIt(1, t) + 1);<br>        if (length &gt;= 10000)<br>            return ":-(";<br>        string results = "";<br>        while (s != t)<br>        {<br>            if (doIt(s * s, t) + 1 == length)<br>            {<br>                results += '*';<br>                s = s * s;<br>            }<br>            else if (doIt(s + s, t) + 1 == length)<br>            {<br>                results += '+';<br>                s = s + s;<br>            }<br>            else if (doIt(1, t) + 1 == length)<br>            {<br>                results += '/';<br>                s = 1;<br>            }<br>            --length;<br>        }<br>        return results;<br>    }<br>    int doIt(long s, long t)<br>    {<br>        if (s == t)<br>            return 0;<br>        if (s &gt; t)<br>            return 10000;<br>        if (m.ContainsKey(s))<br>            return m[s];<br>        int results = Math.Min(10000, doIt(s + s, t) + 1);<br>        if (s &gt; 1)<br>            results = Math.Min(results, doIt(s * s, t) + 1);<br>        m.Add(s, results);<br>        return results;<br>    }<br>}<br></td>