            import java.util.*;<br><br>public class OneRegister {<br>    String res;<br><br>    public String getProgram(int s, int t) {<br>        if (s == t) {<br>            return "";<br>        }<br>        res = null;<br>        doit(s, t, "");<br>        if (s != 0) doit(1, t, "/");<br>        doit(0, t, "-");<br>        if (res == null) {<br>            return ":-(";<br>        } else {<br>            return res;<br>        }<br>    }<br><br>    private void doit(long s, long t, String prefix) {<br>        if (s == t) {<br>            if (res == null || res.length() &gt; prefix.length() || res.length() == prefix.length() &amp;&amp; res.compareTo(prefix) &gt; 0) {<br>                res = prefix;<br>            }<br>            return;<br>        }<br>        if (s &gt; t)<br>            return;<br>        if (s != 0) doit(s * 2, t, prefix + "+");<br>        if (s != 0 &amp;&amp; s != 1) doit(s * s, t, prefix + "*");<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>