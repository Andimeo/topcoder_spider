            #include &lt;algorithm&gt;  <br>#include &lt;string&gt;  <br>#include &lt;set&gt;  <br>#include &lt;map&gt;  <br>#include &lt;vector&gt;  <br>#include &lt;queue&gt;   <br>#include &lt;iostream&gt;  <br>#include &lt;iterator&gt;  <br>#include &lt;math.h&gt;  <br>#include &lt;cstdio&gt;  <br>#include &lt;cstdlib&gt;  <br>#include &lt;sstream&gt;  <br><br>#pragma comment(linker, "/STACK:60777216")  <br><br>using namespace std;  <br><br>typedef pair&lt;int,int&gt; pii;  <br>typedef long long ll;  <br>typedef vector&lt;int&gt; vi;  <br><br>#define UN(v) SORT(v),v.erase(unique(v.begin(),v.end()),v.end())  <br>#define SORT(c) sort((c).begin(),(c).end())  <br>#define FOR(i,a,b) for (int  i=(a); i &lt; (b); i++)   <br>#define REP(i,n) FOR(i,0,n)   <br>#define CL(a,b) memset(a,b,sizeof(a))  <br>#define pb push_back  <br><br>map&lt;vi,double&gt; r;<br><br>double go(vi v){<br>  if(v.size()&lt;2) return 0.;<br>  if(r.count(v)) return r[v];<br><br>  double val = 0;<br><br>  REP(i,v.size()){<br>    vi v1,v2;<br><br>    int p = v[i];<br><br>    REP(j,v.size())if(i!=j){<br>      if(v[j] &gt; p) v2.pb(v[j]);<br>      else v1.pb(v[j]);<br><br>      if(j&lt;i &amp;&amp; v[j] &gt; p) val++;<br>      if(j&gt;i &amp;&amp; v[j] &lt; p) val++;<br>    }<br><br>    val += go(v1);<br>    val += go(v2);<br>  }<br><br>  return r[v] = val / v.size();<br>}<br><br>class QuickSort {<br>public:<br> double getEval(vector &lt;int&gt; L) {<br>   r.clear();<br>  return go(L);<br> }<br> <br><br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>