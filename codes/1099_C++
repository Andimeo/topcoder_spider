            #include &lt;vector&gt;<br>#include &lt;string&gt;<br> <br>using namespace std;<br> <br>class Regions{<br> <br>long long gcd(long long a, long long b) {<br>        if (a&lt;0) return gcd(-a,b);<br>        if (b&lt;0) return gcd(a,-b);<br>        if (a&lt;b) return gcd(b,a);<br>        if (b==0) return a;<br>        return gcd(b,a%b);<br>}<br>  long long count(long long dr, long long dc) {<br>    if (dr&lt;0) dr=-dr;<br>    if (dc&lt;0) dc=-dc;<br>    long long n=dr+dc;<br>    long long a=dr*2;<br>    long long b=dc*2;<br>    long long m=gcd(a,b);<br>    if (m==0) return n;<br>    a/=m; b/=m;<br>    if (a%2==0) return n;<br>    if (b%2==0) return n;<br>    return n-m/2;<br>  }<br>public:<br>  int numTaxes(vector &lt;int&gt; row, vector &lt;int&gt; col) {<br>    int i;<br>    long long sum=0;<br>    for (i=1;i&lt;row.size();i++) {<br>      sum+=count(row[i]-row[i-1],col[i]-col[i-1]);<br>    }<br>    if (sum&gt;2000000000) return -1;<br>    return (int)(sum);<br>  };<br>};<br> <br> <br>// Powered by FileEdit<br></td>