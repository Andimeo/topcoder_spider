            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class ChickenOracle<br>{<br>  public string theTruth(int n, int eggCount, int lieCount, int liarCount)<br>  {<br>    bool egg = false, chicken = false;<br> <br>    /* Assume egg */<br>    int e = n - lieCount, c = lieCount;<br>    for ( int i = 0; i &lt;= liarCount; i++ ) {<br>      if ( e &gt;= i &amp;&amp; c &gt;= liarCount - i &amp;&amp; ( e - i + ( liarCount - i ) ) == eggCount &amp;&amp;<br>          ( c - (liarCount - i ) + i) == n - eggCount ) <br>          egg = true;<br>    }<br>    /* Assume chicken */<br>    c = n - lieCount;<br>    e = lieCount;            <br>    for ( int i = 0; i &lt;= liarCount; i++ ) {<br>      if ( e &gt;= i &amp;&amp; c &gt;= liarCount - i &amp;&amp; ( e - i + ( liarCount - i ) ) == eggCount &amp;&amp;<br>          ( c - (liarCount - i ) + i) == n - eggCount ) <br>          chicken = true;<br>    }<br>                  <br>    if ( egg &amp;&amp; chicken ) return "Ambiguous";<br>    if ( egg ) return "The egg";<br>    if ( chicken ) return "The chicken";<br>    return "The oracle is a lie";<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>