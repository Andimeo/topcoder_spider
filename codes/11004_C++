            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class ChickenOracle {<br>public:<br>  string theTruth(int, int, int, int);<br>};<br> <br>string ChickenOracle::theTruth(int n, int egg, int lie, int liar) {<br>  int flag = 0, x, a, b, p, q;<br>  for (x=1;x&lt;=2;x++) {<br>    if (x == 1) a = n-lie, b = lie;<br>    else a = lie, b = n-lie;<br>    for (p=0;p&lt;=a;p++) {<br>      q = liar-p;<br>      if (q &gt;= 0 &amp;&amp; q &lt;= b)<br>        if (a-p+q == egg) flag |= x;<br>    }<br>  }<br>  if (flag == 0) return "The oracle is a lie";<br>  if (flag == 1) return "The egg";<br>  if (flag == 2) return "The chicken";<br>  return "Ambiguous";<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>