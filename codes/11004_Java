            import static java.lang.Math.*;<br>import static java.math.BigInteger.*;<br>import static java.util.Arrays.*;<br>import static java.util.Collections.*;<br>import java.math.*;<br>import java.util.*;<br><br>public class ChickenOracle {<br>  public String theTruth(int n, int eggCount, int lieCount, int liarCount) {<br>    if (check(n, eggCount, lieCount, liarCount) &amp;&amp; check(n, n - eggCount, lieCount, liarCount)) {<br>      return "Ambiguous";<br>    } else if (check(n, eggCount, lieCount, liarCount)) {<br>      return "The egg";<br>    } else if (check(n, n - eggCount, lieCount, liarCount)) {<br>      return "The chicken";<br>    } else {<br>      return "The oracle is a lie";<br>    }<br>  }<br>  boolean check(int n, int egg, int lie, int liar) {<br>    for (int a = 0; a &lt;= n; a++) {<br>      int b = lie - a;<br>      if (b &lt; 0) continue;<br>      int c = liar - b;<br>      if (c &lt; 0) continue;<br>      int d = n - a - b - c;<br>      if (d &lt; 0) continue;<br>      if (b + d == egg) return true;<br>    }<br>    return false;<br>  }<br>  void debug(Object...os) {<br>    System.err.println(deepToString(os));<br>  }<br>}<br></td>