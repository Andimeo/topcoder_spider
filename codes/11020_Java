            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class KiwiJuiceEasy<br>{<br>  public int[] thePouring(int[] capacities, int[] bottles, int[] fromId, int[] toId)<br>  {<br>    for(int i=0;i&lt;fromId.length;i++){<br>       while(bottles[fromId[i]]!=0 &amp;&amp; bottles[toId[i]]&lt;capacities[toId[i]]){<br>         bottles[fromId[i]]--;<br>         bottles[toId[i]]++;<br>       }<br>    }<br>    return bottles;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>