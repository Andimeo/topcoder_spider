            public class BestApproximationDiv1 {<br>  public String findFraction(int maxDen, String number) {<br>    int n = Integer.parseInt(number.substring(2));<br>    String ans = null;<br>    int len = 0;<br>    for (int b = 1; b &lt;= maxDen; b++) {<br>      for (long a = 0; a &lt; b; a++) {<br>        long cur = (a * 1000000) / b;<br>        long nn = n;<br>        for (int j = 7; j &gt; len; j--) {<br>          if (cur == nn) {<br>            len = j;<br>            ans = a + "/" + b + " has " + len + " exact digits";<br>          }<br>          cur /= 10;<br>          nn /= 10;<br>        }<br>      }<br>    }<br>    return ans;<br>  }<br><br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by moj 4.15 [modified TZTester]<br>// Powered by CodeProcessor<br></td>