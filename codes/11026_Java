            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class RandomApple<br>{<br>  public double[] theProbability(String[] hundred, String[] ten, String[] one)<br>  {<br>    int n = hundred.length;<br>    int m = hundred[0].length();<br>    int[][] a = new int[n][m];<br>    for (int i = 0; i &lt; n; ++i) {<br>      for (int j = 0; j &lt; m; ++j) {<br>        a[i][j] = Integer.parseInt("" + hundred[i].charAt(j) + ten[i].charAt(j) + one[i].charAt(j));<br>      }<br>    }<br>    int[] bs = new int[n];<br>    int total = 0;<br>    for (int i = 0; i &lt; n; ++i) {<br>      for (int j = 0; j &lt; m; ++j) {<br>        bs[i] += a[i][j];<br>      }<br>      total += bs[i];<br>    }<br>    double[] d = new double[total + 1];<br>    d[0] = 1.;<br>    for (int i = 0; i &lt; n; ++i) {<br>      for (int j = total - bs[i]; j &gt;= 0; --j) {<br>        d[j + bs[i]] += d[j];<br>      }<br>    }<br>    double sum = 0.;<br>    for (double v : d) {<br>      sum += v;<br>    }<br>    sum -= d[0];<br>    double[] d2;<br>    double[] p1 = new double[n];<br>    for (int i = 0; i &lt; n; ++i) {<br>      d2 = d.clone();<br>      for (int j = 0; j &lt;= total - bs[i]; ++j) {<br>        d2[j + bs[i]] -= d2[j];<br>      }<br>      for (int j = bs[i]; j &lt;= total; ++j) {<br>        double with = d[j] - d2[j];<br>        p1[i] += with * (bs[i] / (double)j);<br>      }<br>      p1[i] /= sum;<br>    }<br>    double[] p2 = new double[m];<br>    for (int i = 0; i &lt; n; ++i) {<br>      for (int j = 0; j &lt; m; ++j) {<br>        p2[j] += p1[i] * (a[i][j] / (double)bs[i]);<br>      }<br>    }<br>    return p2;<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>