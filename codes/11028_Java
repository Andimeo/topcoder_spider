            import java.util.*;<br><br>public class TheCoffeeTimeDivOne {<br>  public long find(int n, int[] tea) {<br>    boolean[] t = new boolean[n + 1];<br>    for (int x : tea) {<br>      t[x] = true;<br>    }<br>    long ans = 4 * n;<br>    for (boolean drink : new boolean[]{false, true}) {<br>      int c = 0;<br>      for (int i = n; i &gt;= 1; i--) {<br>        if (t[i] != drink) {<br>          continue;<br>        }<br>        if (c == 0) {<br>          ans += 2 * i + 47;<br>        }<br>        c++;<br>        if (c == 7) {<br>          c = 0;<br>        }<br>      }<br>    }<br>    return ans;<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>