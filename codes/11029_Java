            import java.util.ArrayList;<br><br><br>public class TheCoffeeTimeDivTwo {<br><br>  /**<br>   * @param args<br>   */<br>  public static void main(String[] args) {<br>    // TODO Auto-generated method stub<br><br>  }<br>  <br>  public int find(int n, int[] teaa)<br>  {<br>    ArrayList&lt;Integer&gt; tea = new ArrayList&lt;Integer&gt;();<br>    for (int x=0; x&lt;teaa.length; x++)<br>      tea.add(teaa[x]);<br>    ArrayList&lt;Integer&gt; c = new ArrayList&lt;Integer&gt;();<br>    for (int x=1; x&lt;=n; x++)<br>    {<br>      c.add(x);<br>    }<br>    c.removeAll(tea);<br>    int t = 0;<br>    if (teaa.length&gt;0)<br>      t += (teaa.length/7)*47;<br>    if (teaa.length%7!=0)<br>      t += 47;<br>    if (c.size()&gt;0)<br>      t += ((n-teaa.length)/7)*47;<br>    if (c.size()%7!=0)<br>      t += 47;<br>    t += n*4;<br>    <br>    <br>    for (int x=0; x&lt;c.size(); x++)<br>      for (int y=x+1; y&lt;c.size(); y++)<br>      {<br>        if (c.get(x)&lt;c.get(y))<br>        {<br>          int a = c.get(x);<br>          c.set(x, c.get(y));<br>          c.set(y, a);<br>        }<br>      }<br>    for (int x=0; x&lt;tea.size(); x++)<br>      for (int y=x+1; y&lt;tea.size(); y++)<br>      {<br>        if (tea.get(x)&lt;tea.get(y))<br>        {<br>          int a = tea.get(x);<br>          tea.set(x, tea.get(y));<br>          tea.set(y, a);<br>        }<br>      }<br>    while (c.size()&gt;0)<br>    {<br>      t += c.remove(0)*2;<br>      for (int x=0; x&lt;6 &amp;&amp; c.size()&gt;0; x++)<br>      {<br>        c.remove(0);<br>      }<br>    }<br>    while (tea.size()&gt;0)<br>    {<br>      t += tea.remove(0)*2;<br>      for (int x=0; x&lt;6 &amp;&amp; tea.size()&gt;0; x++)<br>      {<br>        tea.remove(0);<br>      }<br>    }<br>    return t;<br>  }<br><br>}<br></td>