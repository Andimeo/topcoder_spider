            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class TheAirTripDivTwo<br>{<br>  public int find(int[] flights, int fuel)<br>  {<br>    int cc=0;int count=0;<br>    for(int i=0;i&lt;flights.Length;i++)<br>    {<br>      cc+=flights[i];<br>      if(cc&gt;fuel)break;<br>      count++;<br>    }<br>    return count;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>