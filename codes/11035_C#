            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class MovieSeating<br>{<br>    long calc(int n, int m)<br>    {<br>        long result = 1;<br>        for (int i = 0; i &lt; n; ++i)<br>            result *= (m - i);<br> <br>        return result;<br>    }<br> <br>    public long getSeatings(int numFriends, string[] hall)<br>    {<br>        long result = 0;<br>        if (numFriends == 1)<br>        {<br>            for (int i = 0; i &lt; hall.Length; ++i)<br>                for (int j = 0; j &lt; hall[i].Length; ++j)<br>                    if (hall[i][j] != '#')<br>                        ++result;<br>            return result;<br>        }<br> <br>        for (int i = 0; i &lt; hall.Length; ++i)<br>        {<br>            int count = 0;<br>            for (int j = 0; j &lt; hall[i].Length; ++j)<br>                if (hall[i][j] != '#')<br>                    ++count;<br>            if (count &gt;= numFriends)<br>                result += calc(numFriends, count);<br>        }<br> <br>        for (int i = 0; i &lt; hall[0].Length; ++i)<br>        {<br>            int count = 0;<br>            for (int j = 0; j &lt; hall.Length; ++j)<br>                if (hall[j][i] != '#')<br>                    ++count;<br>            if (count &gt;= numFriends)<br>                result += calc(numFriends, count);<br>        }<br> <br>        return result;<br>    }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>