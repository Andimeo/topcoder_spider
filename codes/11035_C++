            #include &lt;cstdio&gt;<br>#include &lt;vector&gt;<br>#include &lt;map&gt;<br>#include &lt;cstring&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;cmath&gt;<br>#include &lt;string&gt;<br><br>using namespace std;<br><br>class  MovieSeating<br>{<br>      public:<br>         long long getSeatings(int n, vector &lt;string&gt; h)<br><br>             {<br>               long a[100]={0} , b[100]={0};<br>               long long ans=0;<br>               <br>            <br>               long m,k,i,j;   <br>               m=h[0].size();<br>               k=h.size();<br>                <br>                 for (i=0 ; i&lt;k ; i++)<br>                 for (j=0 ; j&lt;m ; j++)<br>                  if (h[i][j]=='.')  <br>                  { a[i]++; b[j]++; }<br>                  long f=1; <br>               for (i=1; i&lt;=n ; i++) f=f*i;<br>             <br>            if ( n&gt;1) {  <br>               <br>               long long q=1;<br>               <br>         for (i=0 ; i&lt;k ; i++)<br>        <br>           <br>               if (a[i]&gt;=n)<br>                { q=1;<br>                for (j=a[i]-n+1 ; j&lt;=a[i] ; j++)<br>                        q=q*j;<br>                 for (j=1 ; j&lt;=n ; j++)<br>                       q=q/j;<br>                       <br>                     <br>                       ans=ans+q*f;<br>                  <br>                }<br>                <br>         for (i=0 ; i&lt;m ; i++)<br>               <br>               if (b[i]&gt;=n)<br>                {  q=1;<br>                for (j=b[i]-n+1 ; j&lt;=b[i] ; j++)<br>                        q=q*j;<br>                 for (j=1 ; j&lt;=n ; j++)<br>                       q=q/j;<br>                       <br>                       ans=ans+q*f;<br>                   <br>                }<br>               <br>                }<br>             else<br>              {<br>                  for (i=0 ; i&lt;k ; i++)<br>                  for (j=0 ; j&lt;m ; j++)<br>                  if (h[i][j]=='.')  ans=ans+1;<br>              <br>              }<br>                          <br>              return ans;<br>                }<br>                <br>      };<br></td>