            using System;<br>using System.Collections;<br><br>public class P8XMatrixTransformation {<br>  public string solve(string[] original, string[] target) {<br><br>        int a = 0, b = 0;<br><br>        for(int i = 0; i &lt; original.Length; i++)<br>            for (int j = 0; j &lt; original[0].Length; j++)<br>            {<br>                a += original[i][j] - '0';<br>                b += target[i][j] - '0';<br>            }<br><br>        return a == b ? "YES" : "NO";<br>  }<br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>