            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class P8XMatrixTransformation {<br>public:<br>  string solve(vector &lt;string&gt;, vector &lt;string&gt;);<br>};<br> <br>string P8XMatrixTransformation::solve(vector &lt;string&gt; original, vector &lt;string&gt; target) {<br>  int oo=0,oz=0,to=0,tz=0 ,i , j;<br>  for(i = 0 ; i &lt; original.size() ; i++)<br>  {<br>    for(j = 0 ; j &lt; original[i].size() ; j++)<br>    {<br>      if(original[i][j] == '0')<br>        oz++;<br>      else oo++;<br>    }<br>  }  <br>  for(i = 0 ; i &lt; target.size() ; i++)<br>  {<br>    for(j = 0 ; j &lt; target[i].size() ; j++)<br>    {<br>      if(target[i][j] == '0')<br>        tz++;<br>      else to++;<br>    }<br>  }<br>  if(to==oo &amp;&amp; tz==oz) return "YES";<br>  else return "NO";<br>}<br></td>