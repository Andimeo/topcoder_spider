<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class TimeTravellingGardener<br>{<br>  public int determineUsage(int[] distance, int[] height)<br>  {<br>        int n = distance.Length + 1;<br>        int results = n - 1;<br>        int[] x = new int[n];<br>        x[0] = 0;<br>        for (int i = 0; i &lt; n - 1; ++i)<br>            x[i + 1] = x[i] + distance[i];<br>        for (int i = 0; i &lt; n; ++i)<br>            for (int j = i + 1; j &lt; n; ++j)<br>            {<br>                int count = 0;<br>                for (int k = 0; k &lt; n; ++k)<br>                {<br>                    int ha = (x[k] - x[i]) * height[j] + (x[j] - x[k]) * height[i];<br>                    int hb = x[j] - x[i];<br>                    if (ha &lt; 0 || ha &gt; hb * height[k])<br>                    {<br>                        count = -1;<br>                        break;<br>                    }<br>                    if (ha == hb * height[k])<br>                        ++count;<br>                }<br>                results = Math.Min(results, n - count);<br>            }<br>        return results;<br>  }<br>}<br></td>