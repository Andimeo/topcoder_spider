            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class InternetSecurity<br>{<br>  public string[] determineWebsite(string[] address, string[] keyword, string[] dangerous, int threshold)<br>  {<br>    bool usafe = true;<br>    <br>    int N = address.Length;<br>    bool[] d = new bool[ N ];<br>    <br>    Hashtable ht = new Hashtable();<br>    <br>    for ( int i = 0; i &lt; dangerous.Length; i++ ) {<br>      ht.Add( dangerous[i], true );<br>    }<br>    <br>    string[][] kw = new string[N][];<br>    <br>    for ( int i = 0; i &lt; N; i++ )<br>      kw[i] = keyword[i].Split(' ');<br>      <br>    while ( usafe ) {<br>      usafe = false;<br>      <br>      for ( int i = 0; i &lt; N; i++ ) {<br>        if ( !d[i] ) {<br>          int count = 0;<br>          <br>          for ( int j = 0; j &lt; kw[i].Length; j++ ) {<br>            if ( ht.ContainsKey( kw[i][j] ) ) {<br>              count++;<br>            }<br>          }<br>          <br>          if ( count &gt;= threshold ) {<br>            usafe = true;<br>            d[i] = true;<br>            <br>            for ( int j = 0; j &lt; kw[i].Length; j++ )<br>              if ( !ht.ContainsKey( kw[i][j] ) )<br>                ht.Add( kw[i][j], true );<br>          }<br>        }<br>      }<br>    }<br>    <br>    ArrayList a = new ArrayList();<br>    <br>    for ( int i = 0; i &lt; N; i++ )<br>      if ( d[i] ) {<br>        a.Add( address[i] );<br>      }<br>    <br>    string[] r = new string[ a.Count ];<br>    <br>    for ( int i = 0; i &lt; r.Length; i++ ) r[i] = (string) a[i];<br>    return r;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>