            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class InternetSecurity<br>{<br>  public String[] determineWebsite(String[] address, String[] keyword, String[] dangerous, int threshold)<br>  {<br>    boolean[] safe = new boolean[address.length];<br>    Set&lt;String&gt; dang = new HashSet&lt;String&gt;();<br>    for(String s: dangerous)<br>      dang.add(s);<br>    while(true)  {<br>      boolean change = false;<br>      for(int i = 0; i &lt; address.length; i++)  {<br>        if(!safe[i])  {<br>          int n = 0;<br>          StringTokenizer st = new StringTokenizer(keyword[i]);<br>          while(st.hasMoreTokens())    {<br>            String s = st.nextToken();<br>            if(dang.contains(s))  {<br>              n++;<br>              if(n &gt;= threshold)<br>                break;<br>            }<br>          }<br>          if(n &gt;= threshold)  {<br>            change = true;<br>            safe[i] = true;<br>            st = new StringTokenizer(keyword[i]);<br>            while(st.hasMoreTokens())    {<br>              dang.add(st.nextToken());<br>            }<br>          }<br>        }<br>      }<br>      if(!change)<br>        break;<br>    }<br>    ArrayList&lt;String&gt; ret = new ArrayList&lt;String&gt;();<br>    for(int i = 0; i &lt; safe.length; i++)<br>      if(safe[i])<br>        ret.add(address[i]);<br>    String[] retret = new String[ret.size()];<br>    for(int i = 0; i &lt; retret.length; i++)<br>      retret[i] = ret.get(i);<br>    return retret;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>