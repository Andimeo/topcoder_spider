            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class CircleMarket <br>{<br>  public int makePurchases(int[] openTime, int[] closeTime, int travelTime) <br>  {<br>        int maxStoreOpen = 0;<br>        int res = 0;<br>        for (int i = 0; i &lt; closeTime.Length; i++)<br>        {<br>            if (maxStoreOpen &lt; closeTime[i])<br>            {<br>                maxStoreOpen= closeTime[i];<br>            }<br>        }<br>        int time=0;<br>        int boltvan = 0;<br>        bool[] tilt = new bool[openTime.Length];<br>        for (int i = 0; i &lt; tilt.Length; i++)<br>        {<br>            tilt[i] = false;<br>        }<br>        while (time &lt;= maxStoreOpen)<br>        {<br>            if (time &lt;= closeTime[boltvan] &amp;&amp; time &gt;= openTime[boltvan]&amp;&amp; !tilt[boltvan])<br>            {<br>                //vásárolás<br>                res++;<br>                tilt[boltvan] = true;<br>            }<br><br><br>            //végén körülnézés<br>            boltvan++;<br>            boltvan %= openTime.Length;<br>            time += travelTime;<br>        }<br>        return res;<br>  }<br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by r1cs1]<br>// Powered by CodeProcessor<br></td>