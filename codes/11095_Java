            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class CircleMarket<br>{<br>  public int makePurchases(int[] openTime, int[] closeTime, int travelTime)<br>  {<br>    int n=openTime.length;<br>    boolean []used=new boolean[n];<br>    int ans=0;<br>    int time=0;<br>    while(time&lt;=1000000)<br>    {<br>      for(int i=0;i&lt;n;i++)<br>      {<br>        if(time&gt;=openTime[i] &amp;&amp; time&lt;=closeTime[i] &amp;&amp; !used[i]) {used[i]=true; ans++;}<br>        time+=travelTime;<br>      }<br>    }<br>    return ans;<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>