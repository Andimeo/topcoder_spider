            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class TheNumbersWithLuckyLastDigit<br>{<br>  public int find(int n)<br>  {<br>      int ret = 100;<br>      for (int i = 0; i &lt; 30; i++)<br>      {<br>          for (int j = 0; j &lt; 30; j++)<br>          {<br>              if (i==0&amp;&amp;j==0)<br>              {<br>                  continue;<br>              }<br>              int t1 = i*4 + j*7;<br>              if (t1%10!=n%10)<br>              {<br>                  continue;<br>              }<br>              if (t1&gt;n)<br>              {<br>                  continue;<br>              }<br>              ret = Math.Min(ret, i+j);<br>          }<br>      }<br>      if (ret&gt;90)<br>      {<br>          ret = -1;<br>      }<br>      return ret;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.8 (beta) modified by pivanof!<br></td>