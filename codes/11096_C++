            #include &lt;stdio.h&gt;<br>#include &lt;math.h&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;queue&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>using namespace std;<br> <br>class TheNumbersWithLuckyLastDigit<br>{<br>  public:<br>  int find(int n)<br>  {<br>    int res = 999;<br>    for (int i=0; i&lt;10; i++)<br>      for (int j=0; j&lt;10; j++) {<br>        if (i+j == 0) continue;<br>        int base = i*4+j*7;<br>        if (base&gt;n) continue;<br>        if ((n-base)%10) continue;<br>        res = min(res, i+j);<br>      }<br>    if (res == 999) return -1;<br>    return res;<br>  };<br>};<br></td>