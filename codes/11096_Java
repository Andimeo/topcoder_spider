            import java.util.*;<br> <br>public class TheNumbersWithLuckyLastDigit {<br>  public int find(int n) {<br>    int res = Integer.MAX_VALUE;<br>        for (int n4 = 0; n4 &lt; 10; ++n4)<br>            for (int n7 = 0; n7 &lt; 10; ++n7) {<br>                int rem = n - 4 * n4 - 7 * n7;<br>                if (rem &gt; 0 &amp;&amp; (rem % 10 == 4 || rem % 10 == 7))<br>                    res = Math.min(res, 1 + n4 + n7);<br>            }<br>    return res == Integer.MAX_VALUE ? -1 : res;<br>  }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>