            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class TheTicketsDivOne<br>{<br>  public double find(int n, int m)<br>  {<br>        int ITER = 200;<br>        double winchance = 1.0/6;<br>        double kickchance = 2.0/6;<br>        double rotatechance = 3.0/6;<br>        double[,] chances = new double[n, n];<br>        chances[0, 0] = 1;<br> <br>        for (int i = 1; i &lt; n; i++)<br>        {<br>            for (int a = 0; a &lt; ITER; a++)<br>            {<br>                for (int j = 0; j &lt;= i; j++)<br>                {<br>                    if (j == 0)<br>                    {<br>                        chances[j, i] = winchance + rotatechance * chances[i, i];<br>                    }<br>                    else<br>                    {<br>                        chances[j, i] = rotatechance * chances[j - 1, i] + kickchance * chances[j - 1, i - 1];<br>                    }<br>                }<br>            }<br>        }<br> <br>        return chances[m - 1, n - 1];<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>