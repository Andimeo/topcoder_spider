            #include &lt;cstring&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br> <br>using namespace std;<br> <br>class TheTicketsDivOne {<br>  public:<br>    double find(int n, int m);<br>};<br> <br>long double r[1010][1010];<br> <br>double TheTicketsDivOne::find( int n, int m ) {<br>  r[1][0] = 1.0;<br>  long double k2 = 1.0 / 2;<br>  long double k3 = 1.0 / 3;<br>  long double k6 = 1.0 / 6;<br>  for (int i = 2; i &lt;= n; i++) {<br>    for (int j = 0; j &lt; i; j++)<br>      r[i][j] = 0.0;<br>    for (int step = 0; step * i &lt;= 10000; step++) {<br>      r[i][0] = k6 + k2 * r[i][i - 1];<br>      for (int j = 1; j &lt; i; j++)<br>        r[i][j] = k3 * r[i - 1][j - 1] + k2 * r[i][j - 1];<br>    }<br>  }<br>  return r[n][m - 1];<br>}<br></td>