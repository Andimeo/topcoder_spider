            import java.util.*;<br> <br>public class TheTicketsDivOne {<br>  public double find(int _n, int _m) {<br>        double[][] r = new double[_n + 1][];<br>        r[1] = new double[1];<br>        r[1][0] = 1.0;<br>        for (int n = 2; n &lt;= _n; ++n) {<br>            r[n] = new double[n];<br>            r[n][0] = 1.0 / 6;<br>            for (int j = 1; j &lt; n; ++j) {<br>                r[n][j] = r[n][j - 1] / 2 + r[n - 1][j - 1] / 3;<br>            }<br>            r[n][0] += 0.5 * r[n][n - 1] / (1.0 - Math.pow(0.5, n));<br>            for (int j = 1; j &lt; n; ++j) {<br>                r[n][j] = r[n][j - 1] / 2 + r[n - 1][j - 1] / 3;<br>            }<br>        }<br>    return r[_n][_m - 1];<br>  }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>