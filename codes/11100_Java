            import java.util.*;<br>import java.math.*;<br>import static java.lang.System.err;<br>import static java.lang.Math.*;<br>import static java.lang.Integer.*;<br>import static java.math.BigInteger.*;<br>import static java.util.Arrays.*;<br> <br>public class CoinMachinesGame {<br>    public int maxGames(int coins, int[] need, int[] give) {<br>        int res = 0;<br>        while (true) {<br>          int m_ind = -1;<br>          int min = 10000;<br>          for (int i = 0; i &lt; need.length; i++) {<br>            if (coins &gt;= need[i]) {<br>              int d = need[i] - give[i];<br>              if (d &lt; min) {<br>                min = d;<br>                m_ind = i;<br>              }<br>            }<br>          }<br>          if (m_ind == -1) break;<br>          int g = (coins-give[m_ind])/min;<br>          res += g;<br>          coins -= g*min;<br>        }<br>        return res;<br>    }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>