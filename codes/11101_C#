            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class TxMsg<br>{<br>  public string getMessage(string original)<br>  {<br>    string[] m=original.Split();<br>    string ret="";<br>    for(int i=0;i&lt;m.Length;i++)<br>    {<br>      if(allv(m[i]))<br>      ret+=m[i]+" ";<br>      else<br>      ret+=find(m[i])+" ";<br>    }return ret.Substring(0,ret.Length-1);<br>  }<br>  public static bool allv(string s)<br>  {<br>    char[] ss="aeiou".ToCharArray();<br>    foreach(char t in s)<br>    {<br>      if(Array.IndexOf(ss,t)&lt;0)return false;<br>    }<br>    return true;<br>  }<br>  public static string find(string a)<br>  {<br>    string[]s=a.Split(new char[]{'a','e','i','o','u'});<br>    string ret="";<br>    foreach(string p in s)<br>    {<br>      if(p.Length==0)continue;<br>      ret+=p[0];<br>    }<br>    return ret;<br>  }<br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>