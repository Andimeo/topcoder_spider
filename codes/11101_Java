            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class TxMsg<br>{<br> <br>  public String getMessage(String original)<br>  {  String vowel="aeiou";<br>    original+=" ";<br>    String resp="";<br>    String ss[]=original.split(" ");<br>    for(int i=0;i&lt;ss.length;i++){<br>       String s=ss[i];<br>       int nv=0;<br>       for(int j=0;j&lt;s.length();j++){<br>           if(vowel.indexOf(s.charAt(j))!=-1)nv++;<br>       }<br>       if(nv==s.length())resp+=s;<br>       else{<br>           int k=0;<br>           while(k&lt;s.length()){<br>             if(vowel.indexOf(s.charAt(k))==-1)resp+=s.charAt(k);<br>             while(k&lt;s.length() &amp;&amp; vowel.indexOf(s.charAt(k))==-1){<br>                 <br>                 k++;<br>             }<br>             k++;<br>           }<br>       }<br>       resp+=" ";<br>    } <br>    return resp.substring(0,resp.length()-1);<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>