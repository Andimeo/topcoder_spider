            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class LockersDivOne<br>{<br>  public int lastOpened(int n)<br>  {<br>    return 1 + f(n, 2);<br>  }<br>  <br>  int f(int n, int s)<br>  {<br>    if(n &lt;= 1)<br>      return 0;<br>    int t = (n - 1) / s + 1;<br>    int p = f(n - t, s + 1);<br>    return p / (s - 1) * s + (p % (s - 1) + 1);<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>