            using System;<br>using System.Collections.Generic;<br>   <br>        public class LockersDivTwo<br>        {<br>            public int lastOpened(int N)<br>            {<br>                List&lt;int&gt; a = new List&lt;int&gt;();<br>                for (int i = 1; i &lt;= N; i++)<br>                {<br>                    a.Add(i);<br>                }<br>                int start = 1;<br>                int index = 0;<br>                while (a.Count &gt; 1)<br>                {<br>                    <br>                    if (!(index &lt; a.Count))<br>                    {<br>                        index = 0;<br>                        start++;<br>                    }<br>                    if (a[index] == 42)<br>                    {<br>                        a[index] = 42;<br>                    }<br>                    a.RemoveAt(index);<br>                    index += start;<br>                }<br>                return a[0];<br>            }<br>        }<br></td>