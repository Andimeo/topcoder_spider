            import java.util.*;<br>public class LockersDivTwo<br>{<br>  public int lastOpened(int N)<br>  {<br>  <br>  <br>    ArrayList&lt;Integer&gt; list = new ArrayList&lt;Integer&gt;();<br>    <br>    for (int i = 0 ; i &lt; N ; i++)<br>    {<br>      list.add(i+1);<br>    }<br>    <br>    int p = 2; int o = 1;<br>    <br>    while (1&lt;list.size())<br>    {<br>    <br>    for (int i = 0 ; i &lt; list.size() ; i++)<br>    {<br>      o++;<br>    if (p==o)<br>    {<br>    o=1;<br>    list.remove(i);<br>    <br>      if (list.size()==1)<br>         {<br>          break;<br>         }<br>    }<br>      <br>    }<br>    p++;<br>    o=p-1;<br>    }<br>    <br>    int x = list.get(0);<br>    <br>    return x;<br>  }<br><br>}<br></td>