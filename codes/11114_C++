            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class RowsOrdering {<br>public:<br>  int order(vector &lt;string&gt;);<br>};<br> <br>const int md = (int)(1e9+7);<br> <br>int RowsOrdering::order(vector &lt;string&gt; s) {<br>  int n = s.size(), m = s[0].length(), i, j, cnt[444], a[444];<br>  for (j=0;j&lt;m;j++) {<br>    for (i=0;i&lt;256;i++) cnt[i] = 0;<br>    for (i=0;i&lt;n;i++) cnt[s[i][j]]++;<br>    sort(cnt,cnt+256);<br>    reverse(cnt,cnt+256);<br>    a[j] = 0;<br>    for (i=0;i&lt;256;i++) a[j] += i*cnt[i];<br>  }<br>  sort(a,a+m);<br>  reverse(a,a+m);<br>  long long ans = n, v = 1;<br>  for (i=0;i&lt;m;i++) {<br>    ans = (ans+v*a[i]) % md;<br>    v = v*50 % md;<br>  }<br>  return ans;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>