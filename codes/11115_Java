            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class PermutationSignature<br>{<br>  public int[] reconstruct(String s)<br>  {<br>    int n = s.length() + 1;<br>    int[] ans = new int[n];<br>    ans[0] = 1;<br>    for (int i = 1; i &lt; n; ++i) {<br>      if (s.charAt(i - 1) == 'I') {<br>        ans[i] = i + 1;<br>      } else {<br>        ans[i] = ans[i - 1];<br>      }<br>      for (int j = 0; j &lt; i; ++j) {<br>        if (ans[j] &gt;= ans[i]) {<br>          ans[j]++;<br>        }<br>      }<br>    }<br>    return ans;<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>