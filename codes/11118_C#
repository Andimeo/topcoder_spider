            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class TimeTravellingCellar<br>{<br>  public int determineProfit(int[] profit, int[] decay)<br>  {<br>        int m = int.MinValue;<br>        int N = profit.Length;<br>        for (int i = 0; i &lt; N; i++)<br>        {<br>            for (int j = 0; j &lt; N; j++)<br>            {<br>                if (i == j)<br>                    continue;<br>                m = Math.Max(m, profit[i] - decay[j]);<br>            }<br>        }<br> <br>        return m;    <br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>