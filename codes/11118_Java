            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class TimeTravellingCellar{<br>  public int determineProfit(int[] profit, int[] decay){<br>    int ans = 0;<br>    for(int x=0; x&lt;profit.length; x++){<br>      for(int y=0; y&lt;decay.length; y++){<br>        if(y!=x){<br>          ans=Math.max(profit[x]-decay[y],ans);<br>        }<br>      }<br>    }<br>    return ans;<br>  }<br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>