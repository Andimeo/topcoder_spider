            #include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;iostream&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;string&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>using namespace std;<br>#define For(i,n) for(i=0;i&lt;n;i++)<br>#define sz(i) i.size()<br>#define mst(i,n) memset(i,n,sizeof(i))<br>#define eps 1e-9<br>#define INF 999999999<br>#define MOD 1000000007<br>#define LL long long<br>class AverageAverage<br>{<br>public:<br>    double average(vector &lt;int&gt; n){<br>        double ret=0;<br>        int i;<br>        For(i,sz(n))<br>            ret+=n[i];<br>        ret/=sz(n);<br>        return ret;<br>    }<br>};<br></td>