<td class="problemText" colspan="8" valign="middle" align="left">
            import java.util.*;<br>public class EvaluatingElections {<br>  public int evaluate(int[] districts) {<br>    Arrays.sort(districts);<br>    int max = -1;<br>    int N = districts.length;<br>    for(int wins = 0; wins&lt;=N; wins++) {<br>      for(int draws = 0; draws&lt;=N; draws++) {<br>        if(wins+draws&lt;=N &amp;&amp; wins&lt;=N-draws-wins) {<br>          int vt = 0;<br>          for(int i = 0; i&lt;wins; i++) {<br>            vt += districts[N-1-i];<br>          }<br>          int dr = 0;<br>          for(int i = 0; i&lt;N-wins; i++) {<br>            if (districts[i]%2==0 &amp;&amp; dr&lt;draws) {<br>              dr++;<br>              vt += districts[i]/2;<br>            } else {<br>              vt += (districts[i]-1)/2;<br>            }<br>          }<br>          if (dr!=draws) continue;<br>          if(vt&gt;max) max = vt;<br>        }<br>      }<br>    }<br>    return max+1;<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>