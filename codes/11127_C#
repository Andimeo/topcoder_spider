            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class DigitHoles<br>{<br>  public int numHoles(int number)<br>  {<br>      int[] cnt = {1, 0, 0, 0, 1, 0, 1, 0, 2, 1};<br>      int sum = 0;<br>      foreach (char c in number.ToString())<br>          sum += cnt[c - '0'];<br>      return sum;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.8 (beta) modified by pivanof!<br></td>