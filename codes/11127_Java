<td class="problemText" colspan="8" valign="middle" align="left">
            import java.util.*;<br>public class DigitHoles {<br>public int numHoles(int num) {<br>HashMap&lt;Integer,Integer&gt; hs=new HashMap&lt;Integer,Integer&gt;();<br>hs.put(1,0); hs.put(2,0); hs.put(3,0); hs.put(5,0); hs.put(7,0);<br>hs.put(0,1); hs.put(4,1); hs.put(6,1); hs.put(9,1); hs.put(8,2);<br>int sum=0;<br>while(num&gt;0)<br>{<br>int dg=num%10;<br>sum+=hs.get(dg);<br>num/=10;<br><br>}<br>return sum;<br>}<br>}<br></td>