            import java.util.*;<br>public class NumberMagicEasy {<br>    public static final double EPS = 1e-10;<br>    public static long INF = 1L&lt;&lt;18;<br>    public int theNumber(String answer) {<br>      String s = "";<br>      for(int i=0; i &lt; answer.length(); i++)<br>        s += answer.charAt(i) == 'N' ? 1 : 0; <br>        return Integer.valueOf(s, 2) +1;<br>    }<br><br>}<br>//Welly maloosh 5eer fe 7oka .. maloosh 5eer fe masr<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>