            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class RabbitNumber<br>{<br>  int h;<br>  Hashtable ht = new Hashtable();<br>  public int S( long x ) {<br>    int r = 0;<br>    while ( x &gt; 0 ) {<br>      r += (int) ( x % 10 );<br>      x/=10;<br>    }<br>    return r;<br>  }<br>  <br>  public void go( int left, long now, int sum ) {<br>    if ( now &gt; h ) return;<br> <br>    if ( S( now * now ) == sum * sum ) {<br>      ht.Add( now, 0 );<br>    }<br>    <br>    if ( now &gt; 0 ) go( left, now * 10, sum );<br>    for ( int i = 1; i &lt;= 9; i++ )<br>      if ( left &gt;= i ) {<br>        go( left - i, now * 10 + i, sum + i );<br>      }<br>  }<br> <br>  public int theCount(int low, int high)<br>  {<br>    h = high;<br>    int count = 0;<br>    <br>    go( 15, 0, 0 );<br>    foreach ( long x in ht.Keys ) {<br>      if ( x &gt;= low &amp;&amp; x &lt;= high ) count++;<br>    }<br>  <br>    return count;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>