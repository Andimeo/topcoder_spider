            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;queue&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br><br>using namespace std;<br><br>#define sr(x) ((x)*(x))<br><br>long long getnum(long long a){<br>    long long b=0;<br>    while(a){<br>        long long c=a%10;<br>        a/=10;<br>        b+=c;<br>    }<br>    return b;<br>}<br><br>long long getnew(long long a){<br>    long long b[20],bt=0,bv=0;<br>    //a=8LL;<br>    while(a){<br>        long long c=a%4;<br>        a=a/4;<br>        b[bt++]=c;<br>    }<br>    for(long long i=bt-1; i&gt;=0; i--){<br>        bv*=10;<br>        bv+=b[i];<br>    }<br>    //cout&lt;&lt;bv&lt;&lt;endl;<br>    return bv;<br>}<br><br>class RabbitNumber {<br>public:<br>  int theCount(int low, int high) {<br>    long long a,b;<br>    a=(long long)(low);<br>    b=(long long)(high);<br>    int res=0;<br>    for(int i=1; i&lt;2000000; i++){<br>            long long j;<br>            j=getnew((long long)(i));<br>            if(j&gt;high) break;<br>            if(j&lt;low) continue;<br>            <br>        long long c;<br>        c=j*j;<br>        if(getnum(c) == sr( getnum(j) )){<br>            //cout&lt;&lt;getnum(c)&lt;&lt;" "&lt;&lt;sr( getnum(j) )&lt;&lt;endl;<br>            //cout&lt;&lt;j&lt;&lt;endl;<br>            res++;<br>        }<br>    }<br>    //printf("res= %d\n",res);<br>    return res;<br>  }<br>};<br></td>