            import java.util.*;<br> <br>public class RabbitNumber {<br>    public int theCount(int low, int high) {<br>        int res = 0;<br>        for (int cur = 0; cur &lt; (1 &lt;&lt; 20); ++cur) {<br>            long x = 0;<br>            for (int i = 0; i &lt; 10; ++i) {<br>                x = x * 10 + ((cur &gt;&gt; (2 * i)) &amp; 3);<br>            }<br>            if (x &gt;= low &amp;&amp; x &lt;= high &amp;&amp; isCool(x))<br>                ++res;<br>        }<br>        return res;<br>    }<br> <br>    private boolean isCool(long x) {<br>        return sum(x * x) == sum(x) * sum(x);<br>    }<br> <br>    private int sum(long x) {<br>        int res = 0;<br>        while (x &gt; 0) {<br>            res += x % 10;<br>            x /= 10;<br>        }<br>        return res;<br>    }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>