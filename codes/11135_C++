            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class FractionInDifferentBases {<br>public:<br>  long long getNumberOfGoodBases(long long, long long, long long, long long);<br>};<br>long long gcd(long long x,long long y)<br>{<br>  if(y==0)return x;<br>  return gcd(y,x%y);<br>}<br>long long FractionInDifferentBases::getNumberOfGoodBases(long long P, long long Q, long long A, long long B) {<br>  long long d=gcd(P,Q),i,now_=1;<br>  P/=d,Q/=d;<br>  if(P==0||Q==1)return 0;<br>  for(i=2;i*i&lt;=Q;i++){<br>    if(Q%i==0){<br>      while(Q%i==0){<br>        Q/=i;<br>      }<br>      now_*=i;<br>    }<br>  }<br>  if(Q&gt;1)now_*=Q;<br>  long long res=B/now_-(A-1)/now_;<br>  return B-A+1-res;<br>}<br></td>