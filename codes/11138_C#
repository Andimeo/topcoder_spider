            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class LuckyRemainder {<br>    public int getLuckyRemainder(string X) {<br>        int res = 0;<br>        foreach (char c in X)<br>        {<br>            res = (int)((res + (long)(c - '0') * (1L &lt;&lt; (X.Length - 1)) % 9) % 9);<br>        }<br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>