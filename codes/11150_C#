            using System; <br>using System.Collections; <br>using System.Collections.Generic; <br>using System.Collections.Specialized; <br>using System.Text; <br>using System.Text.RegularExpressions; <br> <br>public class MicrowaveSelling <br>{<br>    public int mostAttractivePrice(int minPrice, int maxPrice) <br>    {<br>        int best = maxPrice;<br>        int maxNum = 0;<br>        for (int i = maxPrice; i &gt;= minPrice; i--)<br>        {<br>            int j = 1;<br>            while(i.ToString().EndsWith(new string('9', j)))<br>                j++;<br>            if (j - 1 &gt; maxNum)<br>            {<br>                maxNum = j - 1;<br>                best = i;<br><br>            }<br><br>          }<br>        return best;<br>    }<br><br><br>} <br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>