            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;queue&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br>#include &lt;complex&gt;<br>#include &lt;climits&gt;<br>#include &lt;queue&gt;<br>#include &lt;ctime&gt;<br>#include &lt;ext/numeric&gt;<br>#include &lt;ext/hash_map&gt;<br>#include &lt;ext/hash_set&gt;<br> <br>using namespace std;<br>using namespace __gnu_cxx;<br> <br>#define PB push_back<br>#define ALL(x) (x).begin(),(x).end()<br>#define rep(i,x,n) for(int i = (x) ; i &lt; (n) ; ++i)<br>#define repit(it,x,n) for(__typeof(x) it = (x) ; it!=(n) ;++it)<br> <br> <br>class MicrowaveSelling {<br>public:<br>  int mostAttractivePrice(int minPrice, int maxPrice) {<br>    int res=-1,mx=0;<br>    rep(i,minPrice,maxPrice+1)<br>    {<br>      int c=0;<br>      int x=i;<br>      while(x%10==9)<br>        c++,x/=10;<br>      if(c&gt;=mx)<br>        res=i,mx=c;<br>    }<br>    return res;<br>  }<br>};<br> <br> <br> <br> <br> <br> <br> <br>//Powered by KawigiEdit 2.1.7 (beta) modified by pivanof!<br></td>