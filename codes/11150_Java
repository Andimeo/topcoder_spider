            public class MicrowaveSelling {<br><br>  public int mostAttractivePrice(int minPrice, int maxPrice) {<br>    int max = 0;<br>    int ans = minPrice;<br>    for (int i = minPrice; i &lt;= maxPrice; i++) {<br>      String string = Integer.toString(i);<br>      int count = func(string);<br>      if(count &gt;= max) {<br>        max = count;<br>        ans = i; <br>      }<br>    }<br>    return ans;<br>  }<br><br>  private int func(String string) {<br>    if(string == "") return 0;<br>    if(!string.endsWith("9")) return 0;<br>    else return 1 + func(string.substring(0, string.length()-1));<br>  }<br>  <br>  public static void main(String[] args) {<br>    MicrowaveSelling ms = new MicrowaveSelling();<br>    System.out.println(ms.mostAttractivePrice(1, 1000000));<br>  }<br>}<br></td>