            #include &lt;sstream&gt; <br>#include &lt;iostream&gt;<br>#include &lt;string.h&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;stdlib.h&gt; <br>#include &lt;stdio.h&gt; <br>#include &lt;numeric&gt;<br>#include &lt;math.h&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;stack&gt;<br>#include &lt;queue&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>using namespace std;<br> <br>int u[64];<br> <br>int solve ( vector &lt;int&gt; a ) {<br>  int ans = 0 , i , j , k;<br>  long long cur;<br>  <br>  for (i = 0; i &lt; (int)a.size(); i++) {<br>    memset ( u , 0 , sizeof u );<br>    cur = a[i];<br>    u[i] = 1;<br>    <br>    for (j = 0; j &lt; (int)a.size(); j++) {<br>      for (k = 0; k &lt; (int)a.size(); k++)<br>        if ( !u[k] &amp;&amp; cur &gt;= a[k] ) {<br>          u[k] = 1;<br>          cur += a[k];<br>        }<br>    }<br>    <br>    for (j = 0; j &lt; (int)a.size(); j++)<br>      if ( !u[j] )<br>        break;<br>      <br>    if ( j == (int)a.size() )<br>      ++ ans;<br>  }<br>  <br>  return ans;<br>}<br> <br>class SlimeXSlimesCity {<br>  public:<br>  int merge(vector &lt;int&gt; population) {<br>    return solve ( population );<br>  }<br>};<br> <br> <br> <br>// Powered by FileEdit<br>// Powered by moj 4.12 [modified TZTester]<br>// Powered by CodeProcessor<br></td>