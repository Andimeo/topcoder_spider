            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class GuessTheNumberGame {<br>    public int possibleClues(int n)<br>    {<br>        long mod = 1000000007;<br>        int i, j;<br>        int MAX = 2000000;<br>        bool[] prime = new bool[MAX];<br>        prime[0] = prime[1] = true;<br>        long res = 1;<br>        for (i = 2; i &lt;= n; i++)<br>        {<br>            if (!prime[i])<br>            {<br>                int count = 1;<br>                long now = i;<br>                while (now &lt;= n)<br>                {<br>                    now *= i;<br>                    count++;<br>                }<br>                res *= count;<br>                res %= mod;<br>                for (j = i + i; j &lt;= n; j += i) prime[j] = true;<br>            }<br>        }<br>        return (int)res;<br>    }<br><br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>