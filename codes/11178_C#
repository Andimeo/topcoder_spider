            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class AmoebaDivTwo<br>{<br>  public int count(string[] table, int K)<br>  {<br>      int n = table.Length;<br>      int m = table[0].Length;<br>      int count = 0;<br>      for (int i = 0; i &lt; n; i++)<br>      {<br>          for (int j = 0; j &lt; m; j++)<br>          {<br>              int k;<br>                for (k = i; k - i &lt; K &amp;&amp; k &lt; n &amp;&amp; table[k][j] == 'A'; ++k) ;<br>                if (k - i == K &amp;&amp; k &lt;= n)<br>                {<br>                    //Console.WriteLine("{0} {1} V", i, j);<br>                    ++count;<br>                }<br> <br>                if (K != 1)<br>                {<br>                    for (k = j; k - j &lt; K &amp;&amp; k &lt; m &amp;&amp; table[i][k] == 'A'; ++k) ;<br>                    if (k - j == K &amp;&amp; k &lt;= m)<br>                    {<br>                        //Console.WriteLine("{0} {1} H", i, j);<br>                        ++count;<br>                    }<br>                }<br>          }<br>      }<br> <br>      return count;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.8 (beta) modified by pivanof!<br></td>