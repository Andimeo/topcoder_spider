            #include &lt;iostream&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>using namespace std;<br><br>int isok(int x,int y,int r,int c,int k,vector&lt;string&gt; table) {<br>    int ans = 0, tc = 0;<br>    // r<br>    for (int i = y; i &lt; c &amp;&amp; tc &lt; k; ++i) {<br>        if ( table[x][i] == 'A' ) tc++;<br>        else break;<br>    }<br>    if ( tc == k ) ans++;<br>    tc = 0;<br>    for (int i = x; i &lt; r &amp;&amp; tc &lt; k; ++i) {<br>        if ( table[i][y] == 'A' ) tc++;<br>        else break;<br>    }<br>    if ( tc == k ) ans++;<br>    return ans;<br>}<br><br>class AmoebaDivTwo {<br>    public :<br>    int count(vector &lt;string&gt; table, int K) {<br>        int ans = 0;<br>        int r = table.size(); int c = table[0].size();<br>        if ( K == 1 ) {<br>          for (int i = 0; i &lt; r; ++i)<br>            for (int j = 0; j &lt; c; ++j) if ( table[i][j] == 'A' ) ans++;<br>          return ans;<br>        }<br>        for (int i = 0; i &lt; r; ++i) {<br>            for (int j = 0; j &lt; c; ++j) if ( table[i][j] == 'A' ) {<br>                ans += isok(i,j,r,c,K,table);<br>            }<br>        }<br>        return ans;<br>    }<br>};<br></td>