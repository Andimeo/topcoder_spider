            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class AmoebaDivTwo<br>{<br>  public int count(String[] table, int K)<br>  {<br>    int a,b,c,d,e,f,g,h,x,y,z;<br>  a=0;<br>  for(x=0;x&lt;table.length;x=x+1)<br>  {<br>  b=0;<br>  for(y=0;y&lt;table[x].length();y=y+1)<br>  {<br>  b=0;<br>  if(table[x].length()-y&gt;=K)<br>  {<br>  for(z=y;z&lt;=y+K-1;z=z+1)<br>  {<br>  if(table[x].charAt(z)=='A')<br>  {<br>  b=b+1;<br>  }<br>  else<br>  {<br>  b=0;<br>  }<br>  }<br>  }<br>  if(b==K)<br>  {<br>  a=a+1;<br>  }<br>  if(K&gt;1)<br>  {<br>  b=0;<br>  if(table.length-x&gt;=K)<br>  {<br>  for(z=x;z&lt;=x+K-1;z=z+1)<br>  {<br>  if(table[z].charAt(y)=='A')<br>  {<br>  b=b+1;<br>  }<br>  else<br>  {<br>  b=-10;<br>  }<br>  }<br>  }<br>  if(b==K)<br>  {<br>  a=a+1;<br>  }  <br>  }<br>  }<br>  }<br>  return a;  <br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>