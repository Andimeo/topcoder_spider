<td class="problemText" colspan="8" valign="middle" align="left">
            public class BadVocabulary<br>{<br>  public int count(string badPrefix, string badSuffix, string badSubstring, string[] vocabulary)<br>  {<br>    int result = 0;<br>    <br>    for (int i = 0; i &lt; vocabulary.Length; i++)<br>    {<br>      if (vocabulary[i].StartsWith(badPrefix) || vocabulary[i].EndsWith(badSuffix) || (vocabulary[i].Length &gt; 2 &amp;&amp; vocabulary[i].Substring(1, vocabulary[i].Length - 2).Contains(badSubstring)))<br>        result++;        <br>    }<br>    <br>    return result;<br>  }<br>}<br></td>