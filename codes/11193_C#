            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class TheBoredomDivOne<br>{<br>  public double find(int n, int m)<br>  {<br>        double[] f = new double[n + m + 2];<br>        f[n + m + 1] = 0;<br>        f[n + m] = 0;<br>        for (int k = n + m - 1; k &gt;= n; --k)<br>        {<br>            int h = n + m - k;<br>            double c1 = (double)k * (double)(k - 1) / 2.0;<br>            double c2 = (double)k * (double)h;<br>            double c3 = (double)h * (double)(h - 1) / 2.0;<br>            double s = (c1 + c2 + c3);<br>            c1 /= s;<br>            c2 /= s;<br>            c3 /= s;<br>            f[k] = (1 + c2 * f[k + 1] + c3 * f[k + 2]) / (1 - c1);<br>        }<br>        return f[n];          <br>  }<br>}<br></td>