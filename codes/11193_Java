            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class TheBoredomDivOne<br>{<br>    <br>  public double find(int n, int m) {<br>    int t = n + m;<br>    double[] bored = new double[m + 1];<br>    bored[0] = 0.0;<br>    for (int i = 1; i &lt;= m; i++) {<br>      double C = t * (t - 1) / 2.0;<br>      double p1 = i * (i - 1) / 2.0;<br>      double p2 = i * (t - i);<br>      double p3 = (t - i) * (t - i - 1) / 2.0;<br>      bored[i] = (C + bored[(i &gt; 1 ? (i - 2) : 0)] * p1 + bored[i - 1]<br>          * p2)<br>          / (C - p3);<br>    }<br>    return bored[m];<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>