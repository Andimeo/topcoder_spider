            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class TheBoredomDivTwo<br>{<br>  public int find(int n, int m, int j, int b)<br>  {<br>  int c=0;<br>  int []a= new int [n+m];<br>  for(int i=0; i&lt;n; i++)  a[i]=1;<br>  for(int i=n; i&lt;m+n; i++)a[i]=0;<br>  a[j-1]=1; a[b-1]=1;<br>  for(int i=0; i&lt;n+m; i++) if(a[i]==1)c++;<br>  return c;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>