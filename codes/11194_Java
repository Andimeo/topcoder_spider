            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class TheBoredomDivTwo<br>{<br>  public int find(int n, int m, int j, int b)<br>  {<br>    boolean arr[]=new boolean[n+m];<br>        for(int i=0;i&lt;n;i++)<br>            arr[i]=true;<br>        arr[j-1]=true;<br>        arr[b-1]=true;<br>        int c=0;<br>        for(int i=0;i&lt;arr.length;i++)<br>          if(arr[i])c++;<br>        return c;      <br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>