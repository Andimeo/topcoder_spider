            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class ToastXToast {<br>    public int bake(int[] undertoasted, int[] overtoasted)<br>    {<br>        int umax = -1;<br>        int umin = 9999999;<br>        int omax = -1;<br>        int omin = 9999999;<br>        int i;<br>        for (i = 0; i &lt; undertoasted.Length; i++)<br>        {<br>            umax = Math.Max(undertoasted[i], umax);<br>            umin = Math.Min(undertoasted[i], umin);<br>        }<br>        for (i = 0; i &lt; overtoasted.Length; i++)<br>        {<br>            omax = Math.Max(overtoasted[i], omax);<br>            omin = Math.Min(overtoasted[i], omin);<br>        }<br>        if (umax &gt;= omax) return -1;<br>        if (umin &gt;= omin) return -1;<br>        if (omin &gt; umax) return 1;<br>        return 2;<br>    }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>