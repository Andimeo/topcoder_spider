            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class BallsConverter<br>{<br>    int convert(char c)<br>    {<br>        return (c &gt;= 'A' &amp;&amp; c &lt;= 'Z') ? (c - 'A') : (c - 'a' + 26);<br>    }<br>  public string theGood(string[] a)<br>  {<br>        int n = a.Length;<br>        for (int i = 0; i &lt; n; ++i)<br>            for (int j = 0; j &lt; n; ++j)<br>                for (int k = 0; k &lt; n; ++k)<br>                    if (a[convert(a[i][j])][k] != a[i][convert(a[j][k])])<br>                        return "Bad";<br>        return "Good";<br>  }<br>}<br></td>