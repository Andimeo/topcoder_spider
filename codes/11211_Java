            public class BallsConverter {<br>  public String theGood(String[] convert) {<br>    int n = convert.length;<br>    int[][] t = new int[n][n];<br>    for (int i = 0; i &lt; n; i++) {<br>      for (int j = 0; j &lt; n; j++) {<br>        char c = convert[i].charAt(j);<br>        if (Character.isLowerCase(c))<br>          t[i][j] = c - 'a' + 26;<br>        else<br>          t[i][j] = c - 'A';<br>      }<br>    }<br>    for (int i = 0; i &lt; n; i++) {<br>      for (int j = 0; j &lt; n; j++) {<br>        for (int k = 0; k &lt; n; k++) {<br>          if (t[i][t[j][k]] != t[t[i][j]][k])<br>            return "Bad";<br>        }<br>      }<br>    }<br>    return "Good";<br>  }<br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by moj 4.12 [modified TZTester]<br>// Powered by CodeProcessor<br></td>