            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class DiceRotation<br>{<br>  public int theCount(int goalx, int goaly)<br>  {<br>        int tt, rr, tr;<br>        if(goalx &gt;= 2 &amp;&amp; goaly &gt;= 2) tr = 2;<br>        else tr = 0;<br> <br>        if(goalx == 4) tt = goaly + 1;<br>        else tt = 0;<br> <br>        if(goaly == 4) rr = goalx + 1;<br>        else rr = 0;<br> <br>        return tt + rr + tr;<br>  }<br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>