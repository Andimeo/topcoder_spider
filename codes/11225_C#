            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class NinePuzzle<br>{<br>  public int getMinimumCost(string init, string goal)<br>  {<br>    int[] X = new int[5];<br>    for(int i=0;i&lt;5;i++){<br>      X[i]=0;<br>    }<br>    for(int i=0;i&lt;init.Length;i++){<br>      switch (init[i]){<br>        case 'R':<br>          X[0]++;<br>        break;<br>        case 'G':<br>          X[1]++;<br>        break;<br>        case 'B':<br>          X[2]++;<br>        break;<br>        case 'Y':<br>          X[3]++;<br>        break;<br>      }<br>      switch (goal[i]){<br>        case 'R':<br>          X[0]--;<br>        break;<br>        case 'G':<br>          X[1]--;<br>        break;<br>        case 'B':<br>          X[2]--;<br>        break;<br>        case 'Y':<br>          X[3]--;<br>        break;<br>      }<br>    }<br>    int res = 0;<br>    for(int i=0;i&lt;4;i++){<br>      if(X[i]&gt;0)<br>        res += X[i];<br>    }<br>    return (res);<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>