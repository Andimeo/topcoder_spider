<td class="problemText" colspan="8" valign="middle" align="left">
            public class NinePuzzle<br>{<br>  public int getMinimumCost(String init, String goal)<br>  {<br>    int[] s = new int[4];<br>    int[] e = new int[4];<br>    for(int i = 0; i &lt; 10;i++)<br>    {<br>      if(init.charAt(i) == 'R')<br>        s[0]++;<br>      if(init.charAt(i) == 'G')<br>        s[1]++;<br>      if(init.charAt(i) == 'B')<br>        s[2]++;<br>      if(init.charAt(i) == 'Y')<br>        s[3]++;<br>      if(goal.charAt(i) == 'R')<br>        e[0]++;<br>      if(goal.charAt(i) == 'G')<br>        e[1]++;<br>      if(goal.charAt(i) == 'B')<br>        e[2]++;<br>      if(goal.charAt(i) == 'Y')<br>        e[3]++;<br>    }<br>    int ret = 0;<br>    for(int i = 0; i &lt; 4;i++)<br>    {<br>      if(e[i] - s[i] &gt; 0) ret += e[i] - s[i];<br>    }<br>    return ret;<br>  }<br>}<br></td>