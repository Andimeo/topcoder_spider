            Public Class NinePuzzle<br>    Private Const LETTERS As String = "RGBY*"<br>    Private Const COLORS As Integer = 4<br><br>    Public Function getMinimumCost(ByVal init As String, ByVal goal As String) As Integer<br>        Dim InitState As Integer() = getState(init)<br>        Dim GoalState As Integer() = getState(goal)<br>        getMinimumCost = 0<br>        For i As Integer = 0 To 4<br>            getMinimumCost += Math.Abs(InitState(i) - GoalState(i))<br>        Next<br>        getMinimumCost /= 2<br>    End Function<br><br>    Private Function getState(ByVal str As String) As Integer()<br>        Dim ret(COLORS) As Integer<br>        For Each letter As Char In str<br>            For i As Integer = 0 To COLORS<br>                If letter = LETTERS(i) Then ret(i) += 1<br>            Next<br>        Next<br>        Return ret<br>    End Function<br><br>End Class<br></td>