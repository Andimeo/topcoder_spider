<td class="problemText" colspan="8" valign="middle" align="left">
            public class LuckyCounter<br>    {<br>        public static int countLuckyMoments(string[] moments)<br>        {<br>            int ans = 0;<br>            for (int i = 0; i &lt; moments.Length; i++)<br>            {<br>                if ((moments[i][0] == moments[i][1] &amp;&amp; moments[i][3] == moments[i][4]) || (moments[i][0] == moments[i][3] &amp;&amp; moments[i][1] == moments[i][4]) || (moments[i][0] == moments[i][4] &amp;&amp; moments[i][1] == moments[i][3]))<br>                {<br>                    ans++;<br>                }<br>            }<br>            return ans;<br>        }<br>    }<br></td>