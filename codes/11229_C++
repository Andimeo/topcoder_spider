            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;queue&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br> <br>class LuckyCounter {<br>public:<br>  int countLuckyMoments(vector &lt;string&gt; m) {<br>    string s;<br>    int i,j=0;<br>    bool flag=false;<br>    for(i=0;i&lt;m.size();i++){<br>        s=m[i];<br>        flag=false;<br>        if(s[0]==s[3] &amp;&amp; s[1]==s[4])<br>          flag=true;<br>        if(s[0]==s[1] &amp;&amp; s[3]==s[4])<br>          flag=true;<br>        if(s[0]==s[4] &amp;&amp; s[1]==s[3])<br>          flag=true;<br>        if(flag)<br>          j++;<br>    }<br>    return j;        <br>  }<br>};<br> <br> <br> <br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>