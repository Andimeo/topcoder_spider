<td class="problemText" colspan="8" valign="middle" align="left">
            public class LuckyCounter<br>{<br>public int countLuckyMoments(String[] moments)<br>{<br>    int count=0;<br>    for(int i=0; i&lt;moments.length; i++)<br>   {<br><br>         char[] cArray = moments[i].toCharArray();<br><br>          if(((cArray[0]==cArray[3])&amp;&amp;(cArray[1]==cArray[4])) ||((cArray[0]==cArray[1])&amp;&amp;(cArray[3]==cArray[4]) || ((cArray[0]==cArray[4])&amp;&amp;(cArray[1]==cArray[3])) ))<br>        {<br>          count++;<br><br>          }<br><br><br><br>         }<br>return count;<br>}<br>}<br></td>