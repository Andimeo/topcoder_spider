            Imports Microsoft.VisualBasic<br> <br>Public Class LuckyCounter<br>  Public Function countLuckyMoments(ByVal moments As String()) As Integer<br>        Dim c As Integer = 0<br> <br>        For Each m As String In moments<br>            Dim a1, b1, a2, b2 As String<br>            Dim ok As Boolean = False<br> <br>            a1 = Mid$(m, 1, 1)<br>            b1 = Mid$(m, 2, 1)<br>            a2 = Mid$(m, 4, 1)<br>            b2 = Mid$(m, 5, 1)<br> <br>            If a1 = a2 And b1 = b2 Then<br>                ok = True<br>            End If<br> <br>            If a1 = b1 And a2 = b2 Then<br>                ok = True<br>            End If<br> <br>            If a1 = b2 And b1 = a2 Then<br>                ok = True<br>            End If<br> <br>            If a1 = b1 And b1 = a2 And a2 = b2 Then<br>                ok = True<br>            End If<br> <br>            If ok Then c += 1<br>        Next<br> <br>    Return c<br>  End Function<br>End Class<br></td>