            using System;<br>public class NetworkXMessageRecovery<br>{<br>  public string recover(string[] messages)<br>  {<br>    int i,j,nl = 0,p,N;<br>    char c;<br>    string ans = "";<br>    bool[] let = new bool[123];<br>    N = messages.Length;<br>    for (i = 0; i &lt; N; i++)<br>      for (j = 0; j &lt; messages[i].Length; j++)<br>      {<br>        p = Convert.ToInt32(messages[i][j]);<br>        if (!let[p])<br>        {<br>          let[p] = true;<br>          nl++;<br>        }<br>      }<br>    re:<br>    for (i = 65; i &lt; 123; i++)<br>    {<br>      if (!let[i])<br>        continue;<br>      c = Convert.ToChar(i);<br>      for (j = 0; j &lt; N; j++)<br>        if (messages[j].IndexOf(c) &gt; 0)<br>          goto next;<br>      ans += Convert.ToString(c);<br>      let[i] = false;<br>      nl--;<br>      for (j = 0; j &lt; N; j++)<br>        if (messages[j].IndexOf(c) == 0)<br>          messages[j] = messages[j].Substring(1);<br>      break;<br>    next :;<br>    }<br>    if (nl != 0)<br>      goto re;<br>    return ans;<br>  }<br>}<br></td>