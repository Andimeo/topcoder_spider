            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class CubePacking<br>{<br>  public int getMinimumVolume( int Ns, int Nb, int L )<br>  {<br>    int res = L * L * L * Nb + ( Ns / ( L * L ) ) * L * L + ( ( Ns % ( L * L ) ) == 0 ? 0 : L * L );<br>    for ( int cur = Math.Max( res - L * L, 1 ); cur &lt; res; ++cur )<br>    {<br>      for ( int x = 1; x * x * x &lt;= cur; ++x )<br>        if ( cur % x == 0 )<br>          for ( int y = x; x * y * y &lt;= cur; ++y )<br>            if ( cur % ( x * y ) == 0 )<br>            {<br>              int z = cur / ( x * y );<br>              int cnt = ( x / L ) * ( y / L ) * ( z / L );<br>              if ( cnt &lt; Nb ) continue;<br>              int left = cur - L * L * L * Nb;<br>              if ( left &lt; Ns ) continue;<br>              res = Math.Min( res, cur );<br>            }<br>    }<br>    return res;<br>  }<br><br>}<br><br><br> <br>// Powered by moj 4.16 [modified TZTester]<br>// Powered by CleanSharp 1.0<br>// Powered by CodeProcessor<br></td>