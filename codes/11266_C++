            #include &lt;algorithm&gt;<br>#include &lt;cassert&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstring&gt;<br>#include &lt;ctime&gt;<br>#include &lt;iostream&gt;<br>#include &lt;set&gt;<br>#include &lt;sstream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br> <br>using namespace std;<br>typedef long long int64;<br>typedef double real;<br>const int MaxN = 10004;<br> <br>class CubePacking<br>{<br>public:<br> int getMinimumVolume (int Ns, int Nb, int L)<br> {<br>  int a, b, c, S, V, res, temp;<br>  S = L * L;<br>  V = S * L;<br>  for (res = V * Nb + Ns; ; res++)<br>  {<br>   for (a = 1; a * a &lt;= res; a++)<br>    if (res % a == 0)<br>    {<br>     temp = res / a;<br>     for (b = 1; b * b &lt;= temp; b++)<br>      if (temp % b == 0)<br>      {<br>       c = temp / b;<br>       if ((a / L) * (b / L) * (c / L) &gt;= Nb)<br>        return res;<br>      }<br>    }<br>  }<br>  return -1;<br> }<br>};<br> <br> <br>//Powered by KawigiEdit 2.1.8 (beta) modified by pivanof!<br></td>