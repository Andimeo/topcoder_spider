            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class CubePacking<br>{<br>  public int getMinimumVolume(int Ns, int Nb, int l)<br>  {<br>    int ans = Ns + Nb * l * l * l;<br>    int[] d = new int[100000];<br>    loop: while (true) {<br>      int divs = 0;<br>      for (int i = 1; i * i &lt;= ans; ++i) {<br>        if (ans % i == 0) {<br>          d[divs++] = i;<br>          if (i * i &lt; ans) {<br>            d[divs++] = ans / i;<br>          }<br>        }<br>      }<br>      for (int i = 0; i &lt; divs; ++i) {<br>        for (int j = i; j &lt; divs; ++j) {<br>          if ((ans / d[i]) % d[j] == 0) {<br>            int dk = ans / d[i] / d[j];<br>            if ((d[i] / l) * (d[j] / l) * (dk / l) &gt;= Nb) {<br>              break loop;<br>            }<br>          }<br>        }<br>      }<br>      ans++;<br>    }<br>    return ans;<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>