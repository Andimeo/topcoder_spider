            import java.util.*;<br>public class GogoXMarisaKirisima {<br>  static String[] g;<br>  static boolean[] vis, reach, end;<br>  static boolean[][] f;<br>  public static int solve (String[] choices) {<br>    g = choices;<br>    f = new boolean[g.length][g.length];<br>    vis = new boolean[g.length];<br>    reach = new boolean[g.length];<br>    end = new boolean[g.length];<br>    <br>    ArrayList&lt;Integer&gt; q = new ArrayList&lt;Integer&gt;();<br>    q.add(0);<br>    vis[0] = true;<br>    while (!q.isEmpty()) {<br>      int x = q.remove(0);<br>      reach[x] = true;<br>      for (int i=0; i&lt;g.length; i++)<br>        if (g[x].charAt(i) == 'Y' &amp;&amp; !vis[i]) {<br>          vis[i] = true;<br>          f[x][i] = true;<br>          q.add(i);<br>        }<br>    }<br>    if (!reach[g.length-1])<br>      return 0;<br>    Arrays.fill(vis, false);<br>    q.add(g.length-1);<br>    vis[g.length-1] = true;<br>    while (!q.isEmpty()) {<br>      int x = q.remove(0);<br>      end[x] = true;<br>      for (int i=0; i&lt;g.length; i++)<br>        if (g[i].charAt(x) == 'Y' &amp;&amp; !vis[i]) {<br>          vis[i] = true;<br>          q.add(i);<br>        }<br>    }<br>    int r = 1;<br>    for (int i=0; i&lt;g.length; i++)<br>      for (int j=0; j&lt;g.length; j++)<br>        if (g[i].charAt(j)=='Y' &amp;&amp; !f[i][j] &amp;&amp; reach[i] &amp;&amp; end[j])<br>          r++;<br>    return r;<br>  }<br>}<br></td>