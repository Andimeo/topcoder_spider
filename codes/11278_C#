            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class UnsortedSequence<br>{<br>  public int[] getUnsorted(int[] s)<br>  {<br>    if(s.Length == 1)<br>      return new int[0];<br>    else {<br>    Array.Sort(s);<br>    for(int i = s.Length - 1; i &gt;= 1; i--) {<br>      if(s[i - 1] != s[i]) {<br>        int temp = s[i - 1];<br>        s[i - 1] = s[i];<br>        s[i] = temp;<br>        return s;<br>      }<br>    }<br>    return new int[0];<br>    }<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>