            #include &lt;iostream&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;set&gt;<br>using namespace std;<br><br>#define X first<br>#define Y second<br>#define pb push_back<br>#define mp make_pair<br>#define sz size()<br>#define all(x) (x).begin(),(x).end()<br><br>class UnsortedSequence {<br>public:<br>   vector &lt;int&gt; getUnsorted( vector &lt;int&gt; s ) {<br>     set&lt;int&gt; voc(all(s));<br>     if(voc.sz &lt;= 1) return vector&lt;int&gt; ();<br>     sort(all(s));<br>     next_permutation(all(s));<br>     return s;<br>   }<br>};<br></td>