            import java.util.*;<br> <br>public class FoxPlayingGame {<br>  public double theMax(int nA, int nB, int paramA, int paramB) {<br>        double scoreA = paramA / 1000.0;<br>        double scoreB = paramB / 1000.0;<br>        double[] pow = new double[nB + 1];<br>        pow[0] = nA * scoreA;<br>        for (int i = 1; i &lt; pow.length; ++i) pow[i] = scoreB * pow[i - 1];<br>        Arrays.sort(pow);<br>        return pow[pow.length - 1];<br>  }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>