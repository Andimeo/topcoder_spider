            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class MarbleDecoration<br>{<br>  public int maxLength(int R, int G, int B)<br>  {<br>    int[] mas = new int[3];<br>    mas[0]=R;<br>    mas[1]=G;<br>    mas[2]=B;<br>    Array.Sort(mas);<br>    int q =mas[1]*2;<br>    if (mas[2]&gt;mas[1])<br>      q++;<br>    return q;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>