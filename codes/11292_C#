            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class StonesGame<br>{<br>  string s1 = "Romeo";<br>  string s2 = "Strangelet";<br>  <br>  bool can(int n, int k, int m, int l)<br>  {<br>    int y2 = m + l + k - 1;<br>    if(y2 % 2 == 0)<br>    {<br>      int y = y2 / 2;<br>      int x = m + l - y;<br>      return 0 &lt;= x &amp;&amp; x &lt;n &amp;&amp; y&gt;= 0 &amp;&amp; y &lt; n &amp;&amp; y&gt;=l &amp;&amp; y&gt;=m &amp;&amp; x&lt;=l &amp;&amp; x &lt;= m; <br>    }<br>    return false;<br>  }<br>  <br>  public string winner(int n, int m, int k, int l)<br>  {<br>    m --;<br>    l --;  <br>    if(can(n, k, m, l))<br>    {<br>      return s1;<br>    }<br>    bool x = false;<br>    for(int i=0; i&lt;n; i++)<br>    {<br>      if(can(n, k, m, i))<br>      {<br>        if(!can(n, k, i, l))<br>        {<br>          x = true;<br>        }<br>      }<br>    }<br>    if(!x)<br>    {<br>      return s2;<br>    }<br>    return "Draw";    <br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>