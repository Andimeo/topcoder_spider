            using System;<br>using System.Collections.Generic;<br><br>public class AnagramFree<br>{<br>    public int getMaximumSubset(string[] S)<br>    {<br>        Dictionary&lt;string, object&gt; dic = new Dictionary&lt;string, object&gt;();<br>        foreach (string s in S)<br>        {<br>            char[] str = s.ToCharArray();<br>            Array.Sort(str);<br>            string ss = new string(str);<br>            if (!dic.ContainsKey(ss))<br>            {<br>                dic.Add(ss, null);<br>            }<br>        }<br>        return dic.Count;<br>    }<br><br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>