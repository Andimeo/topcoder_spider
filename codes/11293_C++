            #include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;vector&gt;<br>#include &lt;set&gt;<br>#include &lt;queue&gt;<br>#include &lt;map&gt;<br> <br>using namespace std;<br> <br>class  AnagramFree{<br>public:<br>  int getMaximumSubset(vector &lt;string&gt; s){<br>    for(int i=0;i&lt;s.size();i++)<br>      sort(s[i].begin(),s[i].end());<br>    sort(s.begin(),s.end());<br>    int ans=0;<br>    string tmp("");<br>    for(int i=0;i&lt;s.size();i++)<br>      if(tmp!=s[i])<br>        tmp=s[i],ans++;<br>    return ans;<br>  }<br>};<br></td>