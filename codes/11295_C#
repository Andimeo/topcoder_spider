            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class MysteriousRestaurant<br>{<br>  int g(char c)<br>  {<br>    if('0' &lt;= c &amp;&amp; c &lt;= '9')<br>      return c - '0';<br>    if('A' &lt;= c &amp;&amp; c &lt;= 'Z')<br>      return c - 'A' + 10;<br>    if('a' &lt;= c &amp;&amp; c &lt;= 'z')<br>      return c - 'a' + 36;<br>    return 0;    <br>  }<br>  bool work(int ds, int[,] a, int b, int d, int t)<br>  {<br>    int r = 0;<br>    for(int i=0; i&lt;7; i++)<br>    {<br>      int w = (ds - i + 6) / 7;<br>      int min = int.MaxValue;<br>      for(int k=0; k&lt;t; k++)<br>      {<br>        int s = 0;<br>        for(int j=0; j&lt;w; j++)<br>        {<br>          s += a[j * 7 + i, k];<br>        }<br>        min = Math.Min(s, min);<br>      }<br>      r += min;<br>    }<br>    return r &lt;= b;<br>  }<br>  public int maxDays(string[] p, int b)<br>  {<br>    int d = p.Length;<br>    int t = p[0].Length;<br>    int[,] a = new int[d, t];<br>    for(int i=0; i&lt;d; i++)<br>    {<br>      for(int j=0; j&lt;t; j++)<br>      {<br>        char c = p[i][j];        <br>        a[i, j] = g(c);<br>      }<br>    }<br>    int left = 0;<br>    int right = d + 1;<br>    while(left + 1 &lt; right)<br>    {<br>      int mid = (left + right) / 2;<br>      if(work(mid, a, b, d, t))<br>      {<br>        left = mid;<br>      }<br>      else<br>      {<br>        right = mid;<br>      }<br>    }<br>    return left;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>