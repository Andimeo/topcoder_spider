            import java.util.*;<br> <br>public class MysteriousRestaurant {<br>  public int maxDays(String[] prices, int budget) {<br>        int right = prices.length + 1;<br>        int left = 0;<br>        while (right - left &gt; 1) {<br>            int middle = (left + right) / 2;<br>            int cost = 0;<br>            for (int i = 0; i &lt; 7; ++i) {<br>                int[] total = new int[prices[0].length()];<br>                for (int j = i; j &lt; middle; j += 7) {<br>                    for (int k = 0; k &lt; total.length; ++k) {<br>                        char ch = prices[j].charAt(k);<br>                        int value;<br>                        if (ch &gt;= '0' &amp;&amp; ch &lt;= '9')<br>                            value = ch - '0';<br>                        else if (ch &gt;= 'A' &amp;&amp; ch &lt;= 'Z')<br>                            value = 10 + ch - 'A';<br>                        else if (ch &gt;= 'a' &amp;&amp; ch &lt;= 'z')<br>                            value = 36 + ch - 'a';<br>                        else<br>                            throw new RuntimeException();<br>                        total[k] += value;<br>                    }<br>                }<br>                int min = Integer.MAX_VALUE;<br>                for (int x : total) min = Math.min(min, x);<br>                cost += min;<br>            }<br>            if (cost &lt;= budget)<br>                left = middle;<br>            else<br>                right = middle;<br>        }<br>    return left;<br>  }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>