            Imports Microsoft.VisualBasic<br>Imports System<br>Imports System.Collections<br>Imports System.Collections.Generic<br>Imports System.Text<br>Imports System.Math<br>Imports Weight = System.Int32<br>Public Class MysteriousRestaurant<br>    Public Function CalcDoller(ByVal Str As String) As Integer<br>        If Asc(Str.Substring(0, 1)) &gt;= Asc("0"c) And Asc(Str.Substring(0, 1)) &lt;= Asc("9"c) Then<br>            CalcDoller = Integer.Parse(Str)<br>        ElseIf Asc(Str.Substring(0, 1)) &gt;= Asc("a"c) And Asc(Str.Substring(0, 1)) &lt;= Asc("z"c) Then<br>            CalcDoller = Asc(Str.Substring(0, 1)) - Asc("a"c) + 36<br>        Else<br>            CalcDoller = Asc(Str.Substring(0, 1)) - Asc("A"c) + 10<br> <br>        End If<br>    End Function<br> <br>    Public Function maxDays(ByVal prices As String(), ByVal budget As Integer) As Integer<br>        Dim i As Integer, j As Integer<br>        Dim M As Integer = Len(prices(0))<br>        Dim N As Integer = prices.Length<br>        Dim Price(prices.Length - 1, M - 1) As Integer<br>        Dim SumPrice(prices.Length - 1, M - 1) As Integer<br>        For i = 0 To N - 1<br>            For j = 0 To M - 1<br>                Price(i, j) = CalcDoller(prices(i).Substring(j, 1))<br>                SumPrice(i, j) = 0<br>            Next<br>        Next<br>        For i = 0 To Min(6, N - 1)<br>            For j = 0 To M - 1<br>                SumPrice(i, j) = CalcDoller(prices(i).Substring(j, 1))<br>            Next<br>        Next<br>        For i = 7 To N - 1<br>            For j = 0 To M - 1<br>                SumPrice(i, j) = SumPrice(i - 7, j) + CalcDoller(prices(i).Substring(j, 1))<br>            Next<br>        Next<br>        maxDays = 0<br>        '[\u20309][\u26085][\u39135][\u12409][\u12428][\u12427][\u12363][\u20206][\u23450]<br>        For i = 1 To N<br>            Dim PSum As Integer = 0<br> <br>            For j = i - 1 To Max(0, i - 1 - 6) Step -1<br>                Dim PMin As Integer = Integer.MaxValue<br>                Console.WriteLine(j)<br>                For k As Integer = 0 To M - 1<br>                    PMin = Min(PMin, SumPrice(j, k))<br>                Next<br>                PSum += PMin<br>            Next<br>            If PSum &lt;= budget Then<br>                maxDays = i<br>            End If<br>        Next<br>    End Function<br> <br>End Class<br></td>