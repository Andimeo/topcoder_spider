            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class ColorfulCards<br>{<br>  public int[] theCards(int N, String colors) {<br>    boolean[] prime = new boolean[N + 1];<br>    Arrays.fill(prime, true);<br>    prime[1] = false;<br>    for (int i = 2; i &lt;= N; i++) {<br>      if (!prime[i])<br>        continue;<br>      for (int j = i * 2; j &lt;= N; j += i)<br>        prime[j] = false;<br>    }<br>    int n = colors.length();<br>    int[] min = new int[n], max = new int[n];<br>    int j = 0;<br>    for (int i = 1; i &lt;= N; i++) {<br>      if (j == n)<br>        break;<br>      if ((colors.charAt(j) == 'R') == prime[i])<br>        min[j++] = i;<br>    }<br>    j = n - 1;<br>    for (int i = N; i &gt;= 1; i--) {<br>      if (j == -1)<br>        break;<br>      if ((colors.charAt(j) == 'R') == prime[i])<br>        max[j--] = i;<br>    }<br>    int[] ans = new int[n];<br>    for (int i=0; i&lt;n; i++)<br>      if (min[i]==max[i])<br>        ans[i] = min[i];<br>      else ans[i] = -1;<br>    return ans;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>