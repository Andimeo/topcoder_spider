            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class ColoredStrokes<br>{<br>  public int getLeast(String[] s)<br>  {<br>    int ans = 0;<br>    int n = s.length;<br>    int m = s[0].length();<br>    for (int i = 0; i &lt; n; ++i) {<br>      for (int j = 0; j &lt; m; ++j) {<br>        if ((s[i].charAt(j) == 'R' || s[i].charAt(j) == 'G') &amp;&amp; (j == 0 || !(s[i].charAt(j - 1) == 'R' || s[i].charAt(j - 1) == 'G'))) {<br>          ans++;<br>        }<br>      }<br>    }<br>    for (int j = 0; j &lt; m; ++j) {<br>      for (int i = 0; i &lt; n; ++i) {<br>        if ((s[i].charAt(j) == 'B' || s[i].charAt(j) == 'G') &amp;&amp; (i == 0 || !(s[i - 1].charAt(j) == 'B' || s[i - 1].charAt(j) == 'G'))) {<br>          ans++;<br>        }<br>      }<br>    }<br>    return ans;<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>