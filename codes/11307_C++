<td class="problemText" colspan="8" valign="middle" align="left">
            #include &lt;vector&gt;<br>using namespace std;<br>class CarrotBoxesEasy {<br>public:<br>  int theIndex(vector&lt;int&gt; carrots, int k) {<br>    int highestIndex = -1;<br>    int highestCarrots = 0;<br>    for (int l = 0; l &lt; k; l++) {<br>      highestCarrots = 0;<br>      highestIndex = -1;<br>      for (int i = 0; i &lt; carrots.size(); i++) {<br>        if (carrots[i] &gt; highestCarrots) {<br>          highestCarrots = carrots[i];<br>          highestIndex = i;<br>        }<br>      }<br>      carrots[highestIndex]--;<br>    }<br>    return highestIndex;<br>  }<br>};<br></td>