            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class P8XGraphBuilder {<br>    public int solve(int[] scores)<br>    {<br>        int N = scores.Length + 1;<br>        int[] dp = new int[1000];<br>        int start = scores[0] * N;<br>        int i, j;<br>        for (i = 1; i &lt; 1000; i++) dp[i] = -99999999;<br>        for (i = 0; i &lt; N; i++)<br>        {<br>            for (j = 1; j &lt; N - 1; j++)<br>            {<br>                dp[i + j] = Math.Max(dp[i + j], dp[i] + scores[j] - scores[0]);<br>            }<br>        }<br>        return start + dp[N - 2];<br>    }<br><br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>