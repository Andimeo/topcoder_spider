            import java.util.*;<br> <br>public class AlternatingLane {<br>        public double expectedBeauty(int[] low, int[] high) {<br>            double res = 0.0;<br>            for (int i = 0; i + 1 &lt; low.length; ++i) {<br>                double add = 0.0;<br>                for (int cur = low[i]; cur &lt;= high[i]; ++cur) {<br>                    double total = high[i + 1] - low[i + 1] + 1;<br>                    int l = low[i + 1];<br>                    int r = Math.min(cur, high[i + 1]);<br>                    if (l &lt;= r) {<br>                        add += (cur - (l + r) / 2.0) * (r - l + 1) / total;<br>                    }<br>                    l = Math.max(cur, low[i + 1]);<br>                    r = high[i + 1];<br>                    if (l &lt;= r) {<br>                        add -= (cur - (l + r) / 2.0) * (r - l + 1) / total;<br>                    }<br>                }<br>                res += add / (high[i] - low[i] + 1);<br>            }<br>            return res;<br>        }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>