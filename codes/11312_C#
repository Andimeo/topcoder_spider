            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class InterestingParty<br>{<br>  public int bestInvitation(string[] first, string[] second)<br>  {<br>    List&lt;string&gt; top=new List&lt;string&gt;();<br>    for(int i=0;i&lt;first.Length;i++)<br>    {<br>      if(!(top.Contains(first[i])))top.Add(first[i]);<br>      if(!(top.Contains(second[i])))top.Add(second[i]);      <br>    }<br>    int max=0;<br>    for(int i=0;i&lt;top.Count;i++)<br>    {<br>      int count=0;<br>      for(int j=0;j&lt;first.Length;j++)<br>      {<br>        if(first[j]==top[i] || second[j]==top[i])count++;<br>      }<br>      max=Math.Max(count,max);<br>    }<br>    return max;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>