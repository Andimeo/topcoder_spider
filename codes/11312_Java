            import java.util.*;<br>public class InterestingParty {<br>    public int bestInvitation(String[] first, String[] second) {<br> <br>      TreeSet&lt;String&gt; set = new TreeSet&lt;String&gt;();<br>      int res = 0;<br> <br>      for (int i = 0; i &lt; second.length; i++) {<br>      set.add(first[i]);<br>      set.add(second[i]);<br>    }<br>      <br>      for (String topic : set) {<br>        int foo = 0;<br>        for (int i = 0; i &lt; second.length; i++) {<br>        if (topic.equals(first[i]) || topic.equals(second[i])) foo++;<br>      }<br>        if (foo &gt; res) res = foo;<br>      }<br> <br> <br>        return res;<br>    <br>    }<br> <br> <br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>