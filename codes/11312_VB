            Imports Microsoft.VisualBasic<br> <br>Public Class InterestingParty<br>  Public Function bestInvitation(ByVal first As String(), ByVal second As String()) As Integer<br>    Dim n As Integer = first.Length<br>        Dim ans As Integer = 0<br>        Dim i, j As Integer<br>        For i = 0 To n - 1<br>            Dim t1 As String = first(i)<br>            Dim t2 As String = second(i)<br> <br>            Dim cur As Integer = 0<br>            For j = 0 To n - 1<br>                If i = j Then Continue For<br>                Dim d1 As String = first(j)<br>                Dim d2 As String = second(j)<br> <br>                If (d1 = t1 Or d2 = t1) Then<br>                    cur += 1<br>                End If<br>            Next<br>            If cur &gt; ans Then<br>                ans = cur<br>            End If<br> <br>            cur = 0<br>            For j = 0 To n - 1<br>                If i = j Then Continue For<br>                Dim d1 As String = first(j)<br>                Dim d2 As String = second(j)<br> <br>                If (d1 = t2 Or d2 = t2) Then<br>                    cur += 1<br>                End If<br>            Next<br>            If cur &gt; ans Then<br>                ans = cur<br>            End If<br>        Next<br> <br>        Return 1 + ans<br>  End Function<br>End Class<br></td>