            import java.util.*;<br><br>public class CubeStickers {<br>  public String isPossible(String[] sticker) {<br>    Map&lt;String, Integer&gt; ok = new HashMap&lt;String, Integer&gt;();<br>    for (String s : sticker) {<br>      add(ok, s);<br>    }<br>    int cnt = 0;<br>    for (int val : ok.values()) {<br>      cnt += val;<br>    }<br>    return cnt &gt;= 6 ? "YES" : "NO";<br>  }<br><br>  static void add(Map&lt;String, Integer&gt; ok, String sticker) {<br>    Integer res = ok.get(sticker);<br>    if (res == null) {<br>      res = 0;<br>    }<br>    if (res &lt; 2)<br>      res++;<br>    ok.put(sticker, res);<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>