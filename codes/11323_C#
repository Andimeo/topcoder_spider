            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class Filtering<br>{<br>    bool accept(int a, int b, int[] sizes, string outcome)<br>    {<br>        for (int i = 0; i &lt; sizes.Length; ++i)<br>        {<br>            if (outcome[i] == 'A' &amp;&amp; !(sizes[i] &gt;= a &amp;&amp; sizes[i] &lt;= b)) return false;<br>            if (outcome[i] == 'R' &amp;&amp; (sizes[i] &gt;= a &amp;&amp; sizes[i] &lt;= b)) return false;<br>        }<br>        return true;<br>    }<br><br>    public int[] designFilter(int[] sizes, string outcome)<br>    {<br>        int[] ret = new int[2];<br><br>        int mindiff = 10000;<br>        for (int a = 1; a &lt;= 100; ++a)<br>            for (int b = a; b &lt;= 100; ++b)<br>            {<br>                if (accept(a, b, sizes, outcome) &amp;&amp; b - a &lt; mindiff)<br>                {<br>                    mindiff = b - a;<br>                    ret[0] = a;<br>                    ret[1] = b;<br>                }<br>            }<br><br>        if (mindiff &gt;= 10000) return new int[0];<br>        return ret;<br>    }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>