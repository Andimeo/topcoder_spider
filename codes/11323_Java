            public class Filtering {<br> <br>  public int[] designFilter(int[] sizes, String outcome) {<br>    int minA=Integer.MAX_VALUE;<br>    int maxB=Integer.MIN_VALUE;<br>    boolean ok=true;<br>    for(int i=0;i&lt;sizes.length;i++) {<br>      if(outcome.charAt(i)=='A') {<br>        if(sizes[i]&lt;minA) {<br>          minA=sizes[i];<br>        }<br>        if(sizes[i]&gt;maxB) {<br>          maxB=sizes[i];<br>        }<br>      }<br>    }<br>    for(int i=0;i&lt;sizes.length;i++) {<br>      if(outcome.charAt(i)=='R') {<br>        if(sizes[i]&gt;=minA &amp;&amp; sizes[i]&lt;=maxB)<br>          ok=false;<br>      }<br>    }<br>    if(ok) {<br>      return new int[]{minA,maxB};<br>    }<br>    else {<br>      return new int[]{};<br>    }<br>  }<br> <br>}<br></td>