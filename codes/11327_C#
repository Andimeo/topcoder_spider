            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class ColorfulRabbits<br>{<br>  public int getMinimum(int[] replies)<br>  {<br>        Dictionary&lt;int, int&gt; rep = new Dictionary&lt;int, int&gt;();<br>        for (int i = 0; i &lt; replies.Length; i++)<br>        {<br>            if (!rep.ContainsKey(replies[i])) rep.Add(replies[i], 0);<br>            rep[replies[i]]++;<br>        }<br> <br>        int ret = 0;<br>        foreach (KeyValuePair&lt;int,int&gt; kvp in rep)<br>        {<br>            int groupsize = kvp.Key + 1;<br>            int ans = kvp.Value;<br>            int mingroups = (ans + groupsize - 1) / groupsize;<br>            ret += mingroups * groupsize;<br>        }<br>        return ret;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>