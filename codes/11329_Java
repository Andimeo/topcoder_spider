            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class RotatedClock<br>{<br>  public String getEarliest(int hourHand, int minuteHand)<br>  {<br>    for (int h = 0; h &lt; 12; ++h) {<br>      for (int m = 0; m &lt; 60; m += 2) {<br>        int hh = 30 * h + m / 2;<br>        int mm = 6 * m;<br>        for (int d = 0; d &lt; 360; d += 30) {<br>          if (hh % 360 == (d + hourHand) % 360 &amp;&amp; mm % 360 == (d + minuteHand) % 360) {<br>            return String.format("%02d:%02d", h, m);<br>          } <br>        }<br>      }<br>    }<br>    return "";<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>