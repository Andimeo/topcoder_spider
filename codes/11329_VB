            Public Class RotatedClock<br>    Public Function getEarliest(ByVal hourHand As Integer, ByVal minuteHand As Integer) As String<br>        Dim i As Integer, j As Integer, h As Integer, m As Integer<br>        getEarliest = "99:99"<br>        Dim R As Integer = 99999999<br> <br>        For i = 0 To 360 Step 30<br>            h = (hourHand + i) Mod 360<br>            m = (minuteHand + i) Mod 360<br>            Dim alm As Integer = (h \ 30) * 60 + (m \ 6)<br>            If h * 2 = alm And m = (m \ 6) * 6 Then<br>                R = Math.Min(R, alm)<br>            End If<br>        Next<br>        Console.WriteLine(R)<br>        If R = 99999999 Then<br>            getEarliest = ""<br>        Else<br>            getEarliest = (R \ 60).ToString("00") + ":" + (R Mod 60).ToString("00")<br>        End If<br>    End Function<br> <br>End Class<br></td>