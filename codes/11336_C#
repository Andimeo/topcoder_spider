            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class GogoXBallsAndBinsEasy<br>{<br>  public int solve(int[] T)<br>  {<br>    int ret = 0;<br>    for(int i = 0; i &lt; (int) T.Length / 2; i++) {<br>      ret += (T[T.Length - 1 - i] - T[i]);<br>    }<br>    return ret;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>