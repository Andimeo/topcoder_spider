            #include &lt;sstream&gt; <br>#include &lt;iostream&gt;<br>#include &lt;string.h&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;stdlib.h&gt; <br>#include &lt;stdio.h&gt; <br>#include &lt;numeric&gt;<br>#include &lt;math.h&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;stack&gt;<br>#include &lt;queue&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>using namespace std;<br> <br>const int mod = 1000000007;<br> <br>vector &lt; int &gt; a;<br>int dp[42][42 * 42][42][2];<br> <br>int go ( int x , int sum , int last , int sign ) {<br>  if ( x == (int)a.size() )<br>    return 1;<br>  <br>  int &amp;ans = dp[x][sum][last][sign];<br>  if ( ans != -1 ) return ans;<br>  <br>  ans = 0;<br>  <br>  if ( a[x] != -1 ) {<br>    if ( a[x] *x  &lt;= sum || x == 0 ) {<br>      if ( sign == 0 || (sign == 1 &amp;&amp; a[x] &gt;= last) ) {<br>        ans += go ( x + 1 , sum + a[x] , a[x] , a[x] &lt; last );<br>        if ( ans &gt;= mod ) ans -= mod;<br>      }<br>    }<br>  } else {<br>    int i;<br>    <br>    for (i = 0; i &lt;= 40; i++)<br>      if ( i * x &lt;= sum || x == 0 ) {<br>        if ( sign == 0 || (sign == 1 &amp;&amp; i &gt;= last) ) {<br>          ans += go ( x + 1 , sum + i , i , i &lt; last );<br>          if ( ans &gt;= mod ) ans -= mod;<br>        }<br>      }<br>  }<br>  <br>  return ans;<br>}<br> <br>int solve ( vector &lt;int&gt; seq ) {<br>  a = seq;<br>  <br>  memset ( dp , -1 , sizeof dp );<br>  <br>  return go ( 0 , 0 , 0 , 0 );<br>}<br> <br>class FoxAverageSequence {<br>  public:<br>  int theCount(vector &lt;int&gt; seq) {<br>    return solve ( seq );<br>  }<br>};<br> <br> <br> <br>// Powered by FileEdit<br>// Powered by moj 4.12 [modified TZTester]<br>// Powered by CodeProcessor<br></td>