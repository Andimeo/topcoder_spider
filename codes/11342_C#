            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class MafiaGame<br>{<br>  public double probabilityToLose(int N, int[] decisions)<br>  {<br>        int[] cnt = new int[N];<br>      foreach (int decision in decisions)<br>      {<br>          cnt[decision]++;<br>      }<br>      int mx = 1;<br>      foreach (int i in cnt)<br>      {<br>          mx = Math.Max(mx, i);<br>      }<br>      if (mx==1)<br>      {<br>          return 0.0;<br>      }<br>      int c1 = 0;<br>      foreach (int i in cnt)<br>      {<br>          if (i==mx)<br>          {<br>              c1++;<br>          }<br> <br>      }<br>      double ret = 1;<br>      ret /= c1;<br>      while (c1&gt;1)<br>      {<br>          int t1 = c1*mx;<br>          int t2 = N - t1;<br>          if (t2%c1==0)<br>          {<br>              return 0;<br>          }<br>          c1 = t2%c1;<br>      }<br>      return ret;<br>    <br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.8 (beta) modified by pivanof!<br></td>