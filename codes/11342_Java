            import java.util.*;<br> <br>public class MafiaGame {<br>  public double probabilityToLose(int N, int[] decisions) {<br>        int[] cnt = new int[N];<br>        for (int x : decisions) ++cnt[x];<br>        int max = 0;<br>        for (int x : cnt) max = Math.max(max, x);<br>        if (max &lt;= 1) {<br>            return 0.0;<br>        }<br>        int am = 0;<br>        for (int x : cnt) if (x == max) ++am;<br>    double res = 1.0 / am;<br>        while (am &gt; 1) {<br>            if (N % am == 0)<br>                return 0.0;<br>            am = N % am;<br>        }<br>    return res;<br>  }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>