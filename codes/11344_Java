            import java.util.*;<br> <br>public class FractalPicture {<br>  public double getLength(int x1, int y1, int x2, int y2) {<br>        return get(x1, y1, x2, y2, 500);<br>  }<br> <br>    private double get(int x1, int y1, int x2, int y2, int depth) {<br>        if (x1 &lt; -27) x1 = -27;<br>        if (x2 &gt; 27) x2 = 27;<br>        if (y1 &lt; 0) y1 = 0;<br>        if (y2 &gt; 81) y2 = 81;<br>        if (x1 &gt; x2 || y1 &gt; y2) return 0;<br>        if (x1 == -27 &amp;&amp; x2 == 27 &amp;&amp; y1 == 0 &amp;&amp; y2 == 81) {<br>            return 54 * depth + 27;<br>        }<br>        if (depth == 1) {<br>            if (x1 &gt; 0 || x2 &lt; 0) return 0;<br>            return y2 - y1;<br>        }<br>        double res = 0.0;<br>        if (x1 &lt;= 0 &amp;&amp; x2 &gt;= 0 &amp;&amp; y1 &lt;= 54) {<br>            res += Math.min(54, y2) - y1;<br>        }<br>        return res + 1.0 / 3 * (get(x1 * 3, (y1 - 54) * 3, x2 * 3, (y2 - 54) * 3, depth - 1) + get((y1 - 54) * 3, x1 * 3, (y2 - 54) * 3, x2 * 3, depth - 1) + get((y1 - 54) * 3, -x2 * 3, (y2 - 54) * 3, -x1 * 3, depth - 1));<br>    }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>