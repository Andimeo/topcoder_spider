            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class TheCowDivTwo<br>{<br>  public static int[][] memo;<br>  public int find(int N, int K)<br>  {<br>    int[][] memo = new int[K+1][N];<br>    memo[0][0] = 1;<br>    for(int i=0; i&lt;N; i++) {<br>      for(int j=K-1; j&gt;=0; j--) {<br>        for(int k=N-1; k&gt;=0; k--) {<br>          memo[j+1][(k+i)%N] = (memo[j+1][(k+i)%N] + memo[j][k])%1000000007;<br>        }<br>      }<br>    }<br>    return memo[K][0];<br>  }<br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>