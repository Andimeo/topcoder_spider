            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>#define REP(i,n) for(int i=0;i&lt;n;i++)<br>using namespace std;<br> <br>class TheProgrammingContestDivTwo {<br>public:<br>  vector &lt;int&gt; find(int, vector &lt;int&gt;);<br>};<br> <br>vector &lt;int&gt; TheProgrammingContestDivTwo::find(int T, vector &lt;int&gt; rT) {<br>   vector&lt;int&gt; ans(2,0);int tim=0, cont=0;<br>   vector&lt;int&gt; sum;   int k=1;<br>   sort(rT.begin(),rT.end());<br> <br>   if(rT[0]&gt;T) return ans;<br>   REP(i,rT.size()){<br>      int t=0;<br>      for(int j=0;j&lt;k;j++){  <br>        t+=rT[j];<br>        cout&lt;&lt;rT[j]&lt;&lt;" ";<br>        tim+=rT[j];<br>        }cout&lt;&lt;"="&lt;&lt;tim&lt;&lt;"-&gt;";<br>        <br>        if(t&lt;=T)cont++;<br>        else {tim-=t;break;}<br>        k++;<br>        cout&lt;&lt;cont&lt;&lt;endl;<br>      }<br>        <br>   <br>   ans[0]=cont;<br>   ans[1]=tim;<br>   return ans;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>