            #include &lt;stdio.h&gt;<br>#include &lt;math.h&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;queue&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>using namespace std;<br> <br>class TheLotteryBothDivs<br>{<br>  public:<br>  double find(vector &lt;string&gt; g)<br>  {<br>    int n = g.size();<br>    map&lt;string, int&gt; accounted;<br>    double res = 0.0;<br>    double prob = 1.0;<br>    for (int i=1; i&lt;=9; i++) {<br>      prob /= 10;<br>      for (int j=0; j&lt;n; j++)<br>        if (g[j].size() == i) {<br>          int ok = 1;<br>          for (int k=1; k&lt;=i; k++)<br>            if (accounted[g[j].substr(g[j].size()-k, k)]) ok = 0;<br>          accounted[g[j]] = 1;<br>          if (ok) res += prob;<br>        }<br>    }<br>    return res;<br>  };<br>};<br></td>