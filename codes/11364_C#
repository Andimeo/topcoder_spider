            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class FoxAndGCDLCM<br>{<br>    long gcd(long a,long b)<br>    {<br>        if (a&lt;b)<br>        {<br>            return gcd(b, a);<br>        }<br>        if (a%b==0)<br>        {<br>            return b;<br>        }<br>        return gcd(b, a%b);<br>    }<br> <br>  public long get(long G, long L)<br>  {<br>      if (L%G!=0)<br>      {<br>          return -1;<br>      }<br>      long D = L/G;<br>      long ret = (D + 1);<br>      for (long i = 2; i*i &lt;= D; i++)<br>      {<br>          if (D%i==0)<br>          {<br>              long t1 = gcd(i, D/i);<br>              if (t1==1)<br>              {<br>                  ret = Math.Min(ret, i + D/i);<br>              }<br>          }<br>      }<br>      return ret*G;<br>    <br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.8 (beta) modified by pivanof!<br></td>