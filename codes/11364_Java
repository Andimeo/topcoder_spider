            import java.util.*;<br>import static java.lang.Math.*;<br>import static java.util.Arrays.*;<br><br>public class FoxAndGCDLCM {<br>  <br>  public long get(long G, long L) {<br>    if (L % G != 0) return -1;<br>    long n = L / G;<br>    long res = Long.MAX_VALUE;<br>    for (long a = 1; a * a &lt;= n; a++) if (n % a == 0) {<br>      long b = n / a;<br>      if (gcd(a, b) != 1) continue;<br>      if (res &gt; (a + b) * G) res = (a + b) * G;<br>    }<br>    if (res == Long.MAX_VALUE) return -1;<br>    return res;<br>  }<br>  <br>  long gcd(long x, long y) {<br>    long t;<br>    while (y != 0) {<br>      t = x % y; x = y; y = t;<br>    }<br>    return x;<br>  }<br>  <br>  void debug(Object...os) {<br>    System.err.println(deepToString(os));<br>  }<br>  <br>}<br></td>