            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class FoxIntegerLevelThree<br>{<br>  public bool check(long x)<br>  {<br>    for (long i=1;i&lt;9;++i)<br>    {<br>      if (x%(9*i)==(i*i)) return true;<br>    }<br>    if (x%81==0) return true;<br>    return false;<br>  }<br> <br>  public long count(long min, long max)<br>  {<br>    long big=22680;<br>    long cnt=0;<br>    for (long i=1;i&lt;=big;++i)<br>      if (check(i)) ++cnt;<br>    long ans=0;<br>    long left=(min-1)/big;<br>    long right=(max-1)/big;<br>    if (left==right)<br>    {<br>      for (long i=min;i&lt;=max;++i)<br>        if (check(i)) ++ans;<br>    }<br>    else<br>    {<br>      for (long i=min;i&lt;=big*(left+1);++i)<br>        if (check(i)) ++ans;<br>      for (long i=big*right+1;i&lt;=max;++i)<br>        if (check(i)) ++ans;<br>      ans+=(right-left-1)*cnt;<br>    }<br>    return ans;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>