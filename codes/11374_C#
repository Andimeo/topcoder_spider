            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class TripleStrings<br>{<br>  public int getMinimumOperations(string init, string goal)<br>  {<br>    for (int i = 0; i &lt; init.Length; i++)<br>    {<br>      if (goal.StartsWith(init.Substring(i)))<br>        return i*2;<br>    }<br>    return init.Length * 2;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>