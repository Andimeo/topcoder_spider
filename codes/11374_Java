            import java.util.*;<br>import java.math.*;<br> <br>public class TripleStrings {<br> <br>    public int getMinimumOperations (String init, String goal) {<br>        int n = init.length();<br>        for (int i = 0; i &lt; n; i++) {<br>            if (goal.startsWith(init.substring(i))) {<br>                return 2 * i;<br>            }<br>        }<br>        return 2 * n;<br>    }<br> <br>    // Library:<br> <br>}<br> <br>// Powered by FileEdit<br>// Powered by UnusedCodeRemover<br>// Powered by CodeProcessor<br></td>