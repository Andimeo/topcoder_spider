            Imports Microsoft.VisualBasic<br>Imports System<br>Imports System.Collections<br>Imports System.Collections.Generic<br>Imports System.Text<br>Imports System.Math<br>Imports Weight = System.Int32<br>Public Class TripleStrings<br>    Public Function getMinimumOperations(ByVal init As String, ByVal goal As String) As Integer<br>        Dim i As Integer, j As Integer<br>        Dim c1 As Integer = 0, c2 As Integer = 0<br>        getMinimumOperations = System.Int32.MaxValue<br>        If goal.StartsWith(init) Then<br>            getMinimumOperations = Min(getMinimumOperations, c1 + c2 + c1 + c2)<br>        End If<br>        For i = 1 To Len(init)<br>            If Mid(init, 1, 1) = "o" Then<br>                c1 += 1<br>            Else<br>                c2 += 1<br>            End If<br>            init = Mid(init, 2)<br>            If goal.StartsWith(init) Then<br>                getMinimumOperations = Min(getMinimumOperations, c1 + c2 + c1 + c2)<br>            End If<br>        Next<br>    End Function<br> <br>End Class<br></td>