            import java.util.*;<br> <br>public class YetAnotherORProblem {<br>  public int countSequences(long[] R) {<br>        long max = 0;<br>        for (long x : R) max = Math.max(x, max);<br>        long[] res = new long[1 &lt;&lt; R.length];<br>        res[0] = 1;<br>        for (long digit = 1; digit &lt;= max; digit &lt;&lt;= 1) {<br>            long[] next = new long[1 &lt;&lt; R.length];<br>            for (int prevMask = 0; prevMask &lt; res.length; ++prevMask) {<br>                for (int curDigitAt = -1; curDigitAt &lt; R.length; ++curDigitAt) {<br>                    int nextMask = 0;<br>                    for (int i = 0; i &lt; R.length; ++i) {<br>                        int needDigit = (R[i] &amp; digit) &gt; 0 ? 1 : 0;<br>                        int haveDigit = curDigitAt == i ? 1 : 0;<br>                        if (needDigit &lt; haveDigit || (needDigit == haveDigit &amp;&amp; (prevMask &amp; (1 &lt;&lt; i)) != 0)) {<br>                            nextMask |= (1 &lt;&lt; i);<br>                        }<br>                    }<br>                    next[nextMask] = (next[nextMask] + res[prevMask]) % 1000000009;<br>                }<br>            }<br>            res = next;<br>        }<br>        return (int) res[0];<br>  }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>