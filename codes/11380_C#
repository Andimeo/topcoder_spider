            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class SRMRoomAssignmentPhase {<br>  public int countCompetitors(int[] ratings, int K) {<br>    int res = 0;<br><br>    int myRating = ratings[0];<br><br>    Array.Sort(ratings);<br>    Array.Reverse(ratings);<br><br>    int myIndex = Array.IndexOf(ratings, myRating);<br><br>    res = myIndex / K;<br><br>    return res;<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>