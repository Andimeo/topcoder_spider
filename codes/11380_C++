            #include&lt;cstdio&gt;<br>#include&lt;cstdlib&gt;<br>#include&lt;cstring&gt;<br>#include&lt;algorithm&gt;<br>#include&lt;vector&gt;<br>using namespace std;<br>class SRMRoomAssignmentPhase<br>{<br>  public:<br>    int countCompetitors(vector &lt;int&gt; ratings,int k)<br>    {<br>      int res=0;<br>      int me=ratings[0];<br>      int size=ratings.size();<br>      int r[10000];<br>      memset(r,0,sizeof(r));<br>      for(int i=0;i&lt;size;i++)<br>        r[i]=ratings[i];<br>      sort(r,r+size);<br>      int i=size-1,cnt=0;<br>      while(r[i]!=me)<br>      {<br>        cnt++;<br>        if(cnt==k) cnt=0,res++;<br>        i--;<br>      }<br>      return res;<br>    }<br>};<br></td>