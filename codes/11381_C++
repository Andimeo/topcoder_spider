            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class SRMCodingPhase {<br>public:<br>  int countScore(vector &lt;int&gt;, vector &lt;int&gt;, int);<br>};<br> <br>int SRMCodingPhase::countScore(vector &lt;int&gt; pts, vector &lt;int&gt; a, int luck) {<br>  int x,y,z,t,i,p,q,ans = 0,b[5],c[5];<br>  for (x=0;x&lt;a[0];x++)<br>    for (y=0;y&lt;a[1];y++)<br>      for (z=0;z&lt;a[2];z++) {<br>        if (x+y+z &gt; luck) continue;<br>        b[0] = a[0]-x; b[1] = a[1]-y; b[2] = a[2]-z;<br>        c[0] = pts[0]-b[0]*2;<br>        c[1] = pts[1]-b[1]*4;<br>        c[2] = pts[2]-b[2]*8;<br>        for (t=0;t&lt;8;t++) {<br>          p = 0; q = 0;<br>          for (i=0;i&lt;3;i++)<br>            if (t &amp; (1 &lt;&lt; i)) p += b[i], q += c[i];<br>          if (p &lt;= 75 &amp;&amp; q &gt; ans) ans = q;<br>        }<br>      }<br>  return ans;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>