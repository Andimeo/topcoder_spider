            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class SRMCodingPhase<br>{<br>  static int L[]={2,4,8};<br>  public static int countScore(int[] points, int[] skills, int luck)<br>  {<br>    int times[]=new int[3];<br>    int ret=0;<br>    for(int p1=0;p1&lt;=luck;p1++)<br>      for(int p2=0;p2&lt;=luck-p1;p2++)<br>        for(int p3=0;p3&lt;=luck-p1-p2;p3++)<br>        {<br>          times[0]=Math.max(1,skills[0]-p1);<br>                    times[1]=Math.max(1,skills[1]-p2);<br>                    times[2]=Math.max(1,skills[2]-p3);<br>                    for(int i=0;i&lt;8;i++)<br>                    {<br>                      int tmpi=i;<br>                      int j=0;<br>                      int ps=0;<br>                      int tot=0;<br>                      while(j&lt;3)<br>                      {<br>                        if(tmpi%2==1)<br>                            {<br>                                tot+=times[j];<br>                                ps+=points[j]-times[j]*L[j];<br>                            }<br>                            j++;<br>                            tmpi/=2;<br>                      }<br>                      if(tot&lt;=75)<br>                        ret=Math.max(ret,ps);<br>                    }<br>        }<br>    return ret;    <br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>