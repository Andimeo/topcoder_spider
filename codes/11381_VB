            Imports Microsoft.VisualBasic<br>Imports System<br>Imports System.Collections<br>Imports System.Collections.Generic<br>Imports System.Text<br>Imports System.Math<br>Imports Weight=System.Int32<br>Public Class SRMCodingPhase<br>  Public Function countScore(ByVal points As Integer(), ByVal skills As Integer(), ByVal luck As Integer) As Integer<br>    Dim i As Integer,j As Integer<br>    Dim v() As Integer=New integer(){2,4,8}<br>    Dim tluck as integer=luck<br>    countScore=0<br>    For i=0 To 7<br>      luck=tluck<br>      Dim TimeCount As Integer=0<br>      For j=0 To 2<br>        If ((i&gt;&gt;j) and 1)=0 then continue for<br>        TimeCount+=skills(j)<br>      Next j<br>      If TimeCount&gt;luck+75 Then Continue For '[\u26178][\u38291][\u20869][\u12395][\u38291][\u12395][\u21512][\u12358]<br>      Dim TRes As integer=0<br>      Dim T(3) As Integer<br>      For j=0 To 2<br>        T(j)=skills(j)<br>      next j<br>      For j=2 To 0 Step -1<br>        If ((i&gt;&gt;j) and 1)=0 then continue for<br>        If t(j)-luck&gt;1 then<br>          t(j)-=luck:luck=0<br>        else<br>          luck-=t(j)-1<br>          t(j)=1<br>        end if<br>        Tres+=points(j)-V(J)*T(J)<br>      Next j<br>      countScore=MAX(countScore,Tres)<br>    Next i<br>  End Function<br> <br>End Class<br></td>