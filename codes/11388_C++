            #include &lt;vector&gt;<br>#include &lt;string&gt;<br><br>using namespace std;<br><br>class ComplementMachine2D<br>{<br>  public:<br>  <br>  int n, m;<br>  char data[50][50];<br>  <br>  int largestSubmatrix(vector &lt;string&gt; matrix)<br>  {<br>    n = matrix.size(); m = matrix[0].size();<br>    for (int i = 0; i &lt; n; i ++)<br>      for (int j = 0; j &lt; m; j ++)<br>        data[i][j] = matrix[i][j];<br>        <br>    int ans = 0;<br>    <br>    for (int i = 0; i &lt; n; i ++)<br>      for (int j = i; j &lt; n; j ++)<br>      {<br>        int last = 0;<br>        if (j - i + 1 &gt; ans)<br>          ans = j - i + 1;<br>        for (int k = 1; k &lt; m; k ++)<br>        {<br>          bool sam = true, dif = true;<br>          for (int l = i; l &lt;= j; l ++)<br>            if (data[l][k] != data[l][last])<br>              sam = false;<br>            else<br>              dif = false;<br>          if (sam || dif)<br>          {<br>            if ((j - i +1) * (k - last + 1) &gt; ans)<br>              ans = (j - i +1) * (k - last + 1);<br>          }<br>          else<br>          {<br>            last = k;<br>          }<br>        }<br>      }<br>    return ans;<br>  }<br>};<br></td>