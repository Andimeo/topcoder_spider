            #include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;stdio.h&gt;<br>using namespace std;<br> <br>#define vs vector&lt;string&gt;<br>#define vi vector&lt;int&gt;<br>#define ll long long<br> <br>vector&lt;string&gt; full;<br> <br>void rec(string a, string b, string x) {<br>  if (x.size()==10) {<br>    full.push_back(x); return;<br>  }<br>  rec(a,b,a+x);<br>  full.push_back(x);<br>  rec(a,b,b+x);<br>  return;<br>}<br> <br>int ok[3000];<br> <br>class  NENE { public: string vulnerable(vs st, vs en) {<br>rec("E","N","NE");<br>full.push_back("N");<br>rec("N","W","NW");<br>full.push_back("W");<br>rec("W","S","SW");<br>full.push_back("S");<br>rec("S","E","SE");<br>full.push_back("E");<br>int n=full.size();<br> <br>int i,j; for(i=0;i&lt;st.size();i++) {<br>  for(j=0;j&lt;n;j++) if (full[j]==st[i]) break;<br>  while(1) { ok[j]=1; if (full[j]==en[i]) break; j++; j%=n; }<br>}<br>for(i=0;i&lt;n;i++) if (ok[i]==0) return full[i]; return "SAFE";<br> <br>}};<br></td>