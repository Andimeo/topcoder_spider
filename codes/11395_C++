            #include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;deque&gt;<br>#include &lt;queue&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;utility&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br><br>using namespace std;<br><br>#define REP(i,n) for((i)=0;(i)&lt;(int)(n);(i)++)<br><br>typedef long long ll;<br><br>class SetMultiples{<br>  public:<br><br>  long long smallestSubset(long long A, long long B, long long C, long long D){<br>    A = max(A, B/2 + 1);<br>    C = max(C, D/2 + 1);<br>    <br>    ll ans = D - C + 1;<br>    <br>    ll x;<br>    for(x=1;x&lt;100000;x++) if(x &gt;= A &amp;&amp; x &lt;= B){<br>      bool bad = false;<br>      if(D/x*x &gt;= C) bad = true;<br>      if(!bad) ans++;<br>    }<br>    <br>    ll n;<br>    for(n=1;n&lt;110000;n++){<br>      ll low = D / (n+1) + 1;<br>      ll high = (C-1) / n;<br>      low = max(low,A);<br>      low = max(low,100000ll);<br>      high = min(high,B);<br>      if(high &gt;= low) ans += high - low + 1;<br>    }<br>    <br>    return ans;<br>  }<br><br>};<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>