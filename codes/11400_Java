            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class RectangleArea<br>{<br>    int[] a;<br> <br>    int parent(int x)<br>    {<br>        while (x!=a[x])<br>            x = a[x];<br>        return x;<br>    }<br> <br>    boolean join(int x, int y)<br>    {<br>        x = parent(x);<br>        y = parent(y);<br>        if (x==y) return false;<br>        a[x] = y;<br>        return true;<br>    }<br> <br>  public int minimumQueries(String[] known)<br>  {<br>      int n = known.length;<br>      int m = known[0].length();<br>      a = new int[n+m];<br>      for (int i=0; i&lt;n+m; i++)<br>          a[i] = i;<br>      for (int i=0; i&lt;n; i++)<br>          for (int j=0; j&lt;m; j++)<br>              if (known[i].charAt(j)=='Y')<br>                  join(i, j+n);<br>      int ans = 0;<br>      for (int i=0; i&lt;n; i++)<br>            for (int j=0; j&lt;m; j++)<br>                if (join(i, j+n))<br>                    ans++;<br>      return ans;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>