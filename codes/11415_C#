            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class AllButOneDivisor<br>{<br>  public int getMinimum(int[] divisors)<br>  {<br>    int k = divisors.Length;<br>    for (int n = 1; n &lt;= 4000000; ++n)<br>    {<br>      int kk = 0;<br>      for (int i = 0; i &lt; k; ++i)<br>        if (n % divisors[i] == 0)<br>          ++kk;<br>      if (kk == k - 1)<br>        return n;<br>    }<br>    return -1;<br>  }<br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>