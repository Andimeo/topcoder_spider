            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class AllButOneDivisor<br>{<br>  public int getMinimum(int[] divisors)<br>  {<br>    for(int n = 1; n &lt;= 1000000; n++)<br>    {<br>      int k = 0;<br>      for(int j = 0 ; j &lt; divisors.length; j++)<br>        if( n % divisors[j] == 0 )<br>          k++;<br>      if( k == divisors.length - 1 )<br>        return n;<br>    }<br>    return -1;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>