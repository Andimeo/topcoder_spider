            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class BlackWhiteMagic {<br>    public int count(string creatures) {<br>        int cnt = 0;<br>        for (int i = 0; i &lt; creatures.Length; i++)<br>            if (creatures[i] == 'W')<br>                cnt++;<br>        int res = 0;<br>        for (int i = 0; i &lt; cnt; i++)<br>            if (creatures[i] != 'W')<br>                res++;<br>        return res;<br><br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>