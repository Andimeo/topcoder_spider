            import java.util.*;<br>import java.math.*;<br>import static java.util.Arrays.*;<br>public class BlackWhiteMagic {<br>  public int count(String creatures) {<br>    int ws = 0;<br>    for(int i = 0; i &lt; creatures.length(); i++){<br>      if(creatures.charAt(i) == 'W') ws++;<br>    }<br>    int ret = 0;<br>    for(int i = 0; i &lt; ws; i++){<br>      if(creatures.charAt(i) != 'W') ret++;<br>    }<br>    return ret;<br>  }<br> <br> <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>