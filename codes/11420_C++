            #include &lt;stdio.h&gt;<br>#include &lt;stdlib.h&gt;<br>#include &lt;math.h&gt;<br>#include &lt;assert.h&gt;<br>#include &lt;map&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;algorithm&gt;<br>using namespace std;<br><br>#define modp(x) (((x)%P+P)%P)<br><br>typedef long long ll;<br><br>class MuddyRoad{<br>  public:<br>  double getExpectedValue(vector &lt;int&gt; road){<br>    vector&lt;double&gt; prob;<br>    for(int i=0;i&lt;road.size();i++)prob.push_back((double)road[i]/100);<br>    int n=prob.size();<br>    double ans=0;<br>    for(int i=1;i&lt;=n-2;i++){<br>      for(int j=i;j&lt;=n-2;j++){<br>        int c=(j-i+1)/2;<br>        double p=1;<br>        p*=(1-prob[i-1]);<br>        p*=(1-prob[j+1]);<br>        for(int k=i;k&lt;=j;k++)p*=prob[k];<br>        ans+=p*c;<br>      }<br>    }<br>    return ans;<br>  }<br>};<br></td>