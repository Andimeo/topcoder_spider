            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class MuddyRoad<br>{<br>  public double getExpectedValue(int[] road)<br>  {<br>    double p = 0;<br>    double[][] grid = new double[road.length][road.length];<br>    for(int i = 0; i &lt; road.length; i++)  {<br>      grid[i][i] = road[i]/100.;<br>    }<br>    for(int i = 0; i &lt; road.length; i++)  {<br>      for(int j = i+1; j &lt; road.length; j++)  {<br>        grid[i][j] = grid[i][j-1] * road[j]/100.;<br>      }<br>    }<br>    for(int i = 0; i &lt; road.length; i++)  {<br>      for(int j = i+1; j &lt; road.length; j++)  {<br>        if(i%2==j%2)  {<br>          p -= grid[i][j];<br>        }<br>        else<br>          p += grid[i][j];<br>      }<br>    }<br>    return p;<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.8 (beta) modified by pivanof!<br></td>