            Imports Microsoft.VisualBasic<br>Imports System<br>Imports System.Collections<br>Imports System.Collections.Generic<br>Imports System.Text<br>Imports System.Math<br>Imports Weight = System.Int32<br>Public Class MuddyRoad<br>    Public Function Solve(ByVal road As Decimal()) As Double<br> <br>    End Function<br>    Public Function getExpectedValue(ByVal road_ As Integer()) As Double<br>        Dim i As Integer, j As Integer<br>        Dim road() As Decimal = New Decimal(road_.Length - 1) {}<br>        Dim n As Integer = road_.Length<br>        For i = 0 To UBound(road_)<br>            road(i) = road_(i) / 100.0<br>        Next<br>        'k<br>        Dim DP2(60, 60) As Decimal<br>        For i = 0 To 60<br>            For j = 0 To 60<br>                DP2(i, j) = 0<br>            Next<br>        Next<br>        DP2(0, 0) = 1<br>        For i = 0 To 50<br>            For j = 0 To 50<br>                '[\u27425][\u12364][\u12475][\u12540][\u12501][\u12398][\u22580][\u21512]<br>                If (i + 1 &lt; n) Then DP2(i + 1, j) += DP2(i, j) * (1 - road(i + 1))<br>                '[\u27425][\u12364][\u12475][\u12540][\u12501][\u12391][\u12399][\u12394][\u12367][\u65292][\u12381][\u12398][\u27425][\u12364][\u12475][\u12540][\u12501][\u12398][\u22580][\u21512]<br>                If i + 2 &lt; n Then<br>                    DP2(i + 2, j) += DP2(i, j) * (road(i + 1) * (1 - road(i + 2)))<br>                    '[\u27425][\u12364][\u12475][\u12540][\u12501][\u12391][\u12399][\u12394][\u12367][\u65292][\u12381][\u12398][\u27425][\u12418][\u12475][\u12540][\u12501][\u12391][\u12399][\u12394][\u12356][\u22580][\u21512]<br>                    DP2(i + 2, j + 1) += DP2(i, j) * (road(i + 1) * (road(i + 2)))<br>                End If<br>            Next<br>        Next<br>        getExpectedValue = 0<br>        For i = 0 To 50<br>            getExpectedValue += DP2(n - 1, i) * i<br>        Next<br>    End Function<br>End Class<br></td>