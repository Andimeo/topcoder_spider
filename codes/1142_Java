            import java.util.*;<br> <br>public class Tether{<br>  double r, goatx, goaty, EPS = 1e-10;<br>  double norm( double x, double y ){<br>    return Math.sqrt(x*x+y*y);<br>  }<br>  double dist( double x, double y ){<br>    if( y&lt;=goaty ){<br>      return norm(x-goatx,y-goaty);<br>    }<br>    x = Math.abs(x);<br>    double n = norm(x,y);<br>    double q = Math.sqrt(n*n-r*r);<br>    double theta0;<br>    double theta = Math.atan2(y,x) + Math.PI/2 - Math.acos(r/n);<br>    if( theta &gt; 2*Math.PI ){<br>      theta -= 2*Math.PI;<br>    }<br>    return q + r*theta;<br>  }<br>  public int deadTrees( int rope, int radius, int[] x, int[] y ){<br>    goatx = 0;<br>    goaty = -radius;<br>    r = radius;<br>    int rv = 0;<br>    int i;<br>    for( i=0; i&lt;x.length; i++ ){<br>      if( dist(x[i],y[i]) &lt; rope + EPS ){<br>        rv++;<br>      }<br>    }<br>    return rv;<br>  }<br> <br> <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>