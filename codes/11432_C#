            using System;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class TheJackpotDivTwo<br>{<br>    public int[] find(int[] money, int jackpot)<br>    {<br>        List&lt;int&gt; listm = new List&lt;int&gt;();<br>        listm.AddRange(money);<br>        while (jackpot &gt;= 1)<br>        {<br>            listm.Sort();<br>            listm[0]++;<br>            jackpot--;<br>        }<br>        listm.Sort();<br>        return listm.ToArray();<br>    }<br>}<br></td>