            import java.util.Arrays;<br><br><br>public class TheJackpotDivTwo {<br>  public int[] find(int[] money, int jackpot)<br>  {<br>    while (jackpot &gt;= 1)<br>    {<br>      Arrays.sort(money);<br>      money[0]++;<br>      jackpot--;<br>    }<br>    <br>    Arrays.sort(money);<br>    return money;<br>  }<br>  /**<br>   * @param args<br>   */<br>  public static void main(String[] args) {<br>    // TODO Auto-generated method stub<br>    int[] money = {1, 2, 3, 4};<br>    int jackpot = 2;<br>    int[] res = new TheJackpotDivTwo().find(money, jackpot);<br>    for (int i : res)<br>      System.out.println(i);<br>  }<br><br>}<br></td>