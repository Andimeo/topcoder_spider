<td class="problemText" colspan="8" valign="middle" align="left">
            #include &lt;iostream&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br><br>using namespace std;<br><br>const int md = 1000000009;<br><br>int f[11][32768];<br><br>class YetAnotherORProblem2<br>{<br>public:<br>  int countSequences(int N, int R)<br>  {<br>    memset(f, 0, sizeof(f));<br>    for (int i = 0; i &lt;= R; i ++)<br>      f[0][i] = 1;<br>    for (int i = 1; i &lt; N; i ++)<br>      for (int j = 0; j &lt; 32768; j ++)<br>      {<br>        int y = j;<br>        f[i][j] = (f[i][j] + f[i-1][j]) % md;<br>        for (int z = y &amp; (-y); z != 0; z = ((z | (~y)) +1) &amp; y)<br>          if (z &lt;= R)<br>            f[i][j] = (f[i][j] + f[i-1][j-z]) % md;<br>      }<br>    int ans = 0;<br>    for (int i = 0; i &lt; 32768; i ++)<br>      ans = (ans + f[N-1][i]) % md;<br>    return ans;<br>  }<br>};<br></td>