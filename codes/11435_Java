            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class YetAnotherORProblem2<br>{<br>  int modulo = 1000000009;<br>  int[][] dp;<br>  int[][] C;<br>  <br>  int calc( int n, int left ){<br>    if ( n == 0 )<br>      dp[n][left] = 1;<br>    if ( dp[n][left] != -1 )<br>      return dp[n][left];<br>    dp[n][left] = 0;<br>    for ( int i = 0; i &lt;= left; i ++ ){<br>      long add = C[left][i];<br>      add = add * calc( n - 1, left - i );<br>      add %= modulo;<br>      dp[n][left] = dp[n][left] + (int)add;<br>      dp[n][left] %= modulo;<br>    }<br>//    System.out.println( n + " " + left + " " + dp[n][left] );<br>    return dp[n][left];<br>  }<br>  public int countSequences(int N, int R)<br>  {<br>    int i, cnt = 0;<br>    dp = new int[N+1][20];<br>    C = new int[20][20];<br>    for ( i = 0; i &lt;= N; i ++ )<br>      for ( int j = 0; j &lt; 20; j ++ )<br>        dp[i][j] = -1;<br>    C[0][0] = 1;<br>    for ( i = 1; i &lt; 20; i ++ ){<br>      C[i][0] = 1;<br>      C[i][i] = 1;<br>      for ( int j = 1; j &lt; i; j ++ )<br>        C[i][j] = C[i-1][j-1] + C[i-1][j];<br>    }<br>    <br>    for ( i = 1; i &lt;= R; i &lt;&lt;= 1 )<br>      cnt ++;<br>    cnt --;<br>    int ret = calc( N, cnt );<br>    for ( i /= 2; i &lt;= R; i ++ ){<br>      int left = cnt;<br>      long add = N;<br>      for ( int j = 0; j &lt; cnt; j ++ )<br>        if ( ( i &amp; ( 1 &lt;&lt; j ) ) != 0 )<br>          left --;<br>      add = add * calc( N - 1, left );<br>      add %= modulo;<br>      ret = ret + (int)add;<br>      ret %= modulo;<br>    }<br>    <br>    return ret;<br>  }<br>  <br>  //<br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>