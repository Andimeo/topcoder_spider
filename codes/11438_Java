            import java.util.HashSet;<br>import java.util.Set;<br><br>public class SquaredSubsets {<br><br>  public long countSubsets(int n, int[] x, int[] y) {<br>    int N = x.length;<br>    Set&lt;Double&gt; sy = new HashSet&lt;Double&gt;();<br>    Set&lt;Double&gt; sx = new HashSet&lt;Double&gt;();<br>    double rightmost = -100000000;<br>    double bottom = 100000000;<br>    for(int i=0;i&lt;N;i++){<br>      sy.add(y[i]+0.0); sy.add(y[i]+0.5); sy.add(y[i]-0.5); sy.add(y[i]+0.0+n); sy.add(y[i]+0.0-n);<br>      sx.add(x[i]+0.0); sx.add(x[i]+0.5); sx.add(x[i]-0.5); sx.add(x[i]+0.0+n); sx.add(y[i]+0.0-n);<br>      rightmost = Math.max(rightmost, x[i]);<br>      bottom = Math.min(bottom, y[i]);<br>    }<br>//    sy.add(bottom-n); sx.add(rightmost+n);<br>    Set&lt;String&gt; sub = new HashSet&lt;String&gt;();<br>    for(double py:sy)for(double px:sx){<br>      double yy = py+n, xx = px-n;<br>      StringBuilder sb = new StringBuilder();<br>      boolean exist = false;<br>      for(int i=0;i&lt;N;i++){<br>        if(xx&lt;=x[i]&amp;&amp;x[i]&lt;=px&amp;&amp;py&lt;=y[i]&amp;&amp;y[i]&lt;=yy){<br>          sb.append('Y');<br>          exist = true;<br>        }<br>        else sb.append('N');<br>      }<br>      if(exist)sub.add(sb.toString());<br>    }<br>//    System.out.println(sub.size());<br>    return sub.size();<br>  }<br><br>}<br></td>