            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class CandyShop<br>{<br>  public int countProbablePlaces(int[] X, int[] Y, int[] R)<br>  {<br>    int ret = 0;<br>    for ( int x = -200; x &lt;= 200; x ++ )<br>      for ( int y = -200; y &lt;= 200; y ++ ){<br>        int i;<br>        for ( i = 0; i &lt; X.length; i ++ )<br>          if ( Math.abs( X[i] - x ) + Math.abs( Y[i] - y ) &gt; R[i] )<br>            break;<br>        if ( i == X.length )<br>          ret ++;<br>      }<br>    <br>    return ret;<br>  }<br>  <br>  //<br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>