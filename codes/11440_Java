            import java.util.*;<br> <br>public class ArtShift {<br>  public int maxShifts(String sequence) {<br>        int numWhite = 0;<br>        int numBlack = 0;<br>        for (int i = 0; i &lt; sequence.length(); ++i)<br>            if (sequence.charAt(i) == 'W')<br>                ++numWhite;<br>            else<br>                ++numBlack;<br>    return rec(sequence.length(), Math.min(numWhite, numBlack), sequence.length(), 1);<br>  }<br> <br>    private int rec(int n, int maxParts, int maxPartLength, int lcm) {<br>        if (maxParts &lt;= 0) return lcm;<br>        int res = lcm;<br>        for (int part = 1; part &lt;= maxPartLength &amp;&amp; part &lt;= n; ++part) {<br>            res = Math.max(res, rec(n - part, maxParts - 1, part, findLcm(lcm, part)));<br>        }<br>        return res;<br>    }<br> <br>    private int findLcm(int a, int b) {<br>        return a / gcd(a, b) * b;<br>    }<br> <br>    private int gcd(int a, int b) {<br>        while (b &gt; 0) {<br>            int t = a % b;<br>            a = b;<br>            b = t;<br>        }<br>        return a;<br>    }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>