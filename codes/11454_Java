            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class FoxAndBusiness<br>{<br>  public double minimumCost(int k, int totalWork, int[] a, int[] p)<br>  {<br>    double l = 0.;<br>    double r = 1e12;<br>    int n = a.length;<br>    double[] vs = new double[n];<br>    for (int it = 0; it &lt; 100; ++it) {<br>      double x = (l + r) * 0.5;<br>      for (int i = 0; i &lt; n; ++i) {<br>        vs[i] = (double)totalWork * p[i] * a[i] - x * a[i];<br>      }<br>      Arrays.sort(vs);<br>      double sum = totalWork * k * 3600.;<br>      for (int i = 0; i &lt; k; ++i) {<br>        sum += vs[i];<br>      }<br>      if (sum &lt;= 0.) {<br>        r = x;<br>      } else {<br>        l = x;<br>      }<br>    }<br>    return r;<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>