            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class TheAlmostLuckyNumbersDivOne {<br>    public long find(long a, long b) {<br>        return get(0, 0, b) - get(0, 0, a - 1);<br><br>    }<br><br>    private long get(long cur, int used, long max)<br>    {<br>        if (cur &gt; max)<br>            return 0;<br>        if (max == 0)<br>            return 0;<br>        long res = 0;<br>        if (cur != 0)<br>            res++;<br>        for (int next = 0; next &lt; 10; next++)<br>        {<br>            if (next == 0 &amp;&amp; cur == 0)<br>                continue;<br>            if (next != 4 &amp;&amp; next != 7 &amp;&amp; used == 1)<br>                continue;<br>            int nused = used;<br>            if (next != 4 &amp;&amp; next != 7)<br>                nused = 1;<br>            res += get(10 * cur + next, nused, max);<br>        }<br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>