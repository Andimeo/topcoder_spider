            import java.util.*;<br> <br>public class TheAlmostLuckyNumbersDivOne {<br>  public long find(long a, long b) {<br>        return iterate(a, b, 0, true);<br>  }<br> <br>    private long iterate(long a, long b, long cur, boolean canBad) {<br>        if (cur &gt; b) return 0;<br>        long res =  0;<br>        if (cur &gt;= a) ++res;<br>        res += iterate(a, b, cur * 10 + 4, canBad);<br>        res += iterate(a, b, cur * 10 + 7, canBad);<br>        if (canBad) {<br>            for (int i = 0; i &lt; 10; ++i) {<br>                if (i == 0 &amp;&amp; cur == 0) continue;<br>                if (i == 4 || i == 7) continue;<br>                res += iterate(a, b, cur * 10 + i, false);<br>            }<br>        }<br>        return res;<br>    }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>