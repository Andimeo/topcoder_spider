<td class="problemText" colspan="8" valign="middle" align="left">
            Public Class TheAlmostLuckyNumbersDivTwo<br>    Public Function find(ByVal a As Integer, ByVal b As Integer) As Integer<br>        Dim count As Integer = 0<br>        Dim i As Integer<br>        For i = a To b<br>            Dim x As Integer = i, subcount As Integer = 0<br>            Do While x &gt; 0 And subcount &lt; 2<br>                Select Case x Mod 10<br>                    Case 4, 7<br>                    Case Else<br>                        subcount = subcount + 1<br>                End Select<br>                x = x \ 10<br>            Loop<br>            If subcount &lt;= 1 Then count = count + 1<br>        Next<br>        Return count<br>    End Function<br>End Class<br></td>