            using System;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class TheLuckyGameDivTwo<br>{<br>    private const int size = 5000;<br>    private const int max = 4747;<br>    public int find(int a, int b, int jLen, int bLen)<br>    {<br>        int[] array = makeArray();<br>        return count(a, b, jLen, bLen, array);<br>    }<br><br>    private int[] makeArray()<br>    {<br>        int[] array = new int[size];<br><br>        for (int i = 1; i &lt;= max; i++)<br>        {<br>            int temp = i;<br>            while (temp &gt; 0)<br>            {<br>                if (temp % 10 != 4 &amp;&amp; temp % 10 != 7) break;<br>                temp /= 10;<br>            }<br>            if (temp &gt; 0) array[i] = array[i - 1];<br>            else<br>            {<br>                array[i] = array[i - 1] + 1;<br>            }<br>        }<br>        return array;<br>    }<br><br>    private int count(int a, int b, int jLen, int bLen, int[] array)<br>    {<br>        int result = 0;<br>        int j = 0;<br>        for (int i = a; i &lt;= b - jLen + 1; i++)<br>        {<br>            int min = size;<br>            for (j = i; j &lt;= (i + jLen - 1) - bLen + 1; j++)<br>            {<br>                if (array[j + bLen - 1] - array[j - 1] &lt; min) min = array[j + bLen - 1] - array[j - 1];<br>            }<br>            if (min &gt; result) result = min;<br>        }<br>        return result;<br>    }<br>}<br></td>