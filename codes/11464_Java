            public class TheLuckyGameDivTwo {<br>  int count = 5000;<br>  int max = 4747;<br>  <br>  public int find(int a, int b, int jLen, int bLen)<br>  {<br>      int[] t=new int[count];<br>      int res=0;<br>      int i = 1;<br>      int j;<br>        while (i &lt;= max)<br>        {<br>            int temp = i;<br>            while(temp &gt; 0){<br>                  if(temp%10!=4&amp;&amp;temp%10!=7) break;<br>                  temp /= 10;<br>            }<br>            if(temp &gt; 0) <br>          {<br>              t[i]=t[i-1];<br>          }<br>            else <br>            {<br>                 t[i]=t[i-1]+1;<br>            }<br>            i++;<br>        }<br>        for(i = a; i &lt;= b-jLen+1;)<br>        {<br>            int min=count;<br>            for(j=i;j&lt;=(i+jLen-1)-bLen+1;j++){<br>                if(t[j+bLen-1]-t[j-1]&lt;min) min=t[j+bLen-1]-t[j-1];<br>            }<br>            if(min&gt;res) <br>          {<br>            res=min;<br>          }<br>            i++;<br>        }<br>        return res;<br>  }<br>}<br></td>