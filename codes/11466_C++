}<br>                                        if (tmp==0) ans++;<br>                                }<br>                                if (i*i!=a&amp;&amp;a/i&gt;4){<br>                                        LL j=a/i;<br>                                        LL x=n;<br>                                        int tmp=0;<br>                                        while (x&gt;0) {<br>                                                int y=x%j;<br>                                                if (y!=4&amp;&amp;y!=7) tmp++;<br>                                                if (tmp&gt;0) break;<br>                                                x=x/j;<br>                                        }<br>                                        if (tmp==0) ans++;<br>                                }<br>                        }<br>                }<br>                a=n-7;<br>                for (LL i=2;i*i&lt;=a;i++){<br>                        if (a%i==0){<br>                                if (i&gt;7){<br>                                        LL j=i;<br>                                        LL x=n;<br>                                        int tmp=0;<br>                                        while (x&gt;0) {<br>                                                int y=x%j;<br>                                                if (y!=4&amp;&amp;y!=7) tmp++;<br>                                                if (tmp&gt;0) break;<br>                                                x=x/j;<br>                                        }<br>                                        if (tmp==0) ans++;<br>                                }<br>                                if (i*i!=a&amp;&amp;a/i&gt;7){<br>                                        LL j=a/i;<br>                                        LL x=n;<br>                                        int tmp=0;<br>                                        while (x&gt;0) {<br>                                                int y=x%j;<br>                                                if (y!=4&amp;&amp;y!=7) tmp++;<br>                                                if (tmp&gt;0) break;<br>                                                x=x/j;<br>                                        }<br>                                        if (tmp==0) ans++;<br>                                }<br>                        }<br>                }<br>                return ans;<br>        }<br><br><br>};<br><br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>