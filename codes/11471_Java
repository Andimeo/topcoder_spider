            import java.util.*;<br>import static java.lang.Math.*;<br>import static java.util.Arrays.*;<br><br>public class LargestSubsequence {<br>  <br>  public String getLargest(String s) {<br>    String res = "";<br>    while (s.length() &gt; 0) {<br>      for (char c = 'z'; ; c--) if (s.indexOf(c) &gt;= 0) {<br>        res += c;<br>        s = s.substring(s.indexOf(c) + 1);<br>        break;<br>      }<br>    }<br>    return res;<br>  }<br>  <br>  void debug(Object...os) {<br>    System.err.println(deepToString(os));<br>  }<br>  <br>}<br></td>