            Imports Microsoft.VisualBasic<br>Imports System<br>Imports System.Collections<br>Imports System.Collections.Generic<br>Imports System.Text<br>Imports System.Math<br>Imports Weight = System.Int32<br>Public Class LargestSubsequence<br>    Public Function getLargest(ByVal s As String) As String<br>        Dim i As Integer, j As Integer = 0<br>        getLargest = ""<br>        Do<br>            j = Search(s, j)<br>            If j = -1 Then Exit Do<br>            getLargest += s(j)<br>            j += 1<br>        Loop<br>    End Function<br>    Public Function Search(ByVal s As String, ByVal start As Integer) As Integer<br>        Dim M As Integer = 0<br>        For i As Integer = start + 1 To Len(s)<br>            M = Max(M, Asc(Mid(s, i, 1)))<br>        Next<br>        For i As Integer = start + 1 To Len(s)<br>            If Asc(Mid(s, i, 1)) = M Then<br>                Return i - 1<br>            End If<br>        Next<br>        Return -1<br>    End Function<br>End Class<br></td>