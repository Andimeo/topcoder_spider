            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class MagicalGirlLevelOneDivTwo<br>{<br>  public double theMinDistance(int d, int x, int y)<br>  {<br>        int xx = Math.Abs(x)-d;<br>        int yy = Math.Abs(y)-d;<br>        if(xx &gt; 0 &amp;&amp; yy &gt; 0) return Math.Sqrt(xx * xx + yy * yy);<br>        if(xx &gt; 0) return xx;<br>        if(yy &gt; 0) return yy;<br>        return 0;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>