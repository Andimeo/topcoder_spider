            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class MagicalGirlLevelOneDivOne {<br>    public string isReachable(int[] jumpTypes, int x, int y)<br>    {<br>        if (x == 0 &amp;&amp; y == 0) return "YES";<br>        if (jumpTypes.Length == 0) return "NO";<br>        if (x % 2 == 0 &amp;&amp; y % 2 == 0) return "YES";<br>        foreach (int a in jumpTypes) if (a % 2 == 0) return "YES";<br>        if (x % 2 != 0 &amp;&amp; y % 2 != 0) return "YES";<br>        return "NO";<br>    }<br><br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>