            import java.util.*;<br><br>public class MagicalGirlLevelTwoDivTwo {<br>    static boolean[][] map;<br>    public String isReachable(int[] jumpTypes, int x, int y) {<br>        x += 30;<br>        y += 30;<br>        map = new boolean[100][100];<br>        map[30][30] = true;<br>        int[] X = new int[10000];<br>        int[] Y = new int[10000];<br>        X[0] = 30;<br>        Y[0] = 30;<br>        int head = 0;<br>        int tail = 1;<br>        while (head &lt; tail) {<br>            int xx = X[head];<br>            int yy = Y[head];<br>            head++;<br>            for (int i = 0 ; i &lt; jumpTypes.length ; i++) {<br>                int n = jumpTypes[i];<br>                int[][] Q = {{n , 1} , {n , -1} , {-n , 1} , {-n , -1} , {1 , n} , {1 , -n} , {-1 , n} , {-1 , -n}};<br>                for (int j = 0 ; j &lt; Q.length ; j++) {<br>                    int curx = xx + Q[j][0];<br>                    int cury = yy + Q[j][1];<br>                    if (available(curx , cury)) {<br>                        X[tail] = curx;<br>                        Y[tail] = cury;<br>                        tail++;<br>                        map[curx][cury] = true;<br>                    }<br>                }<br>            }<br>        }<br>        String res = "YES";<br>        if (map[x][y]) res = "YES"; else res = "NO";<br>        return res;<br>    }<br>    public static boolean available(int x , int y) {<br>        return (x &gt;= 0 &amp;&amp;y &gt;= 0 &amp;&amp; x &lt;= 60 &amp;&amp; y &lt;= 60 &amp;&amp; !map[x][y]);<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>