            using System;<br>using System.Collections;<br>using System.Text.RegularExpressions;                                                    /*<br>using TopCoder.SystemTester.Solver;                                                      */<br> <br>public class Whisper<br>{<br>  public string toWhom(string[] usernames, string typed)<br>  {<br>    int max = -1, maxi=0;<br>    if (typed.ToLower().StartsWith("/msg "))<br>    {<br>      typed=typed.Remove(0,5);<br>      for (int i=0;i&lt;usernames.Length;i++) if (typed.ToUpper().StartsWith(usernames[i].ToUpper()+" ")&amp;&amp;max&lt;usernames[i].Length)<br>                         {<br>                           max = usernames[i].Length;<br>                           maxi = i;<br>                         }<br>      if (max==-1) return "user is not logged in"; else return usernames[maxi];<br>    } else return "not a whisper";<br>  }<br>}<br></td>