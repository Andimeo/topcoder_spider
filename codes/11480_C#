            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class TwiceString<br>{<br>  public string getShortest(string s)<br>  {<br>    for (int i = 1; i &lt; s.Length; i++)<br>        {<br>            if (s.StartsWith(s.Substring(i)))<br>            {<br>                return s + s.Substring(s.Substring(i).Length);<br>            }<br>        }<br>        return s + s;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>