            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class TwiceString<br>{<br>  public String getShortest(String s)<br>  {<br>    String ab = "";<br>    for(int i = s.length()-1; i &gt;= 0; i--) {<br>      ab = s.charAt(i)+ab;<br>      String ret = s;<br>      ret += ab;<br>      if(ret.substring(1).endsWith(s)){<br>        return ret;<br>      }<br>    }  <br>    return "goodb0y";<br>    <br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>