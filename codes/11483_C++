            #line 2 "GameOfLifeDivTwo.cpp"<br>#include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;queue&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br><br>using namespace std;<br><br>class GameOfLifeDivTwo<br>{<br>public:<br>string theSimulation(string init, int T)<br>{<br>string temp=init;<br>int cnt=0;       <br>for(int k=0;k&lt;T;k++)<br>{<br>for(int i=0;i&lt;init.length();i++)        <br> {<br> cnt=0;       <br> if(init[i]=='0')<br>  cnt++;<br> if(init[i+1]=='0'&amp;&amp;i+1&lt;init.length())<br>  cnt++;<br> if(init[i-1]=='0'&amp;&amp;i-1&gt;=0)<br>  cnt++;<br> if(i+1&gt;=init.length()&amp;&amp;init[0]=='0')<br>  cnt++;<br> if(i-1&lt;0&amp;&amp;init[init.length()-1]=='0')<br>  cnt++;<br> if(cnt&gt;=2)<br>  temp[i]='0';<br> else<br>  temp[i]='1';              <br> }<br> init=temp;<br>}  <br>return init;<br>}<br><br>};<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>