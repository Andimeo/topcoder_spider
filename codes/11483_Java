            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class GameOfLifeDivTwo<br>{<br>  public String theSimulation(String init, int T)<br>  {<br>    String s="";<br>    int N=init.length();<br>    if(T == 0)<br>    s=init;<br>    else<br>    {<br>    for(int i=0;i&lt;T;i++)<br>    {<br>    for(int j=0;j&lt;init.length();j++)<br>      {  int x=0;<br>      int y=0;<br>      <br>        if( j== 0)<br>        {<br>          if(init.charAt(j) == '0')<br>            x++;<br>            y++;<br>            if(init.charAt(j+1) == '0')<br>            x++;<br>            else<br>            y++;<br>            if(init.charAt(N-1) == '0')<br>            x++;<br>            else<br>            y++;<br>            if( x &gt;= 2)<br>            s+='0';<br>            else<br>              s+='1';<br>        }<br>        else if( j== N-1)<br>        {<br>          if(init.charAt(j) == '0')<br>            x++;<br>            else<br>            y++;<br>            if(init.charAt(0) == '0')<br>            x++;<br>            else<br>            y++;<br>            if(init.charAt(j-1) == '0')<br>            x++;<br>            else<br>            y++;<br>            if( x &gt;= 2)<br>            s+='0';<br>            else<br>              s+='1';<br>        }<br>        else//if( j== 0)<br>        {<br>          if(init.charAt(j) == '0')<br>            x++;<br>            else<br>            y++;<br>            if(init.charAt(j+1) == '0')<br>            x++;<br>            else<br>            y++;<br>            if(init.charAt(j-1) == '0')<br>            x++;<br>            else<br>            y++;<br>            if( x &gt;= 2)<br>            s+='0';<br>            else<br>              s+='1';<br>        }<br>        }<br>        if( i &lt;T-1)<br>        {<br>          init=s;<br>          s="";<br>          <br>          }<br>          }<br>          }<br>          return s;<br>  }<br> 