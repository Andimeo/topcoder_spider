            Public Class GameOfLifeDivTwo<br>  Public Function theSimulation(init As String, T As Integer) As String<br>    Dim Stat() as integer<br>    Dim NextStat() as integer<br>    Dim cnt as integer<br>    Dim Str as string<br>    dim i as integer,j as integer,nj as integer, bj as integer<br>    redim stat(len(init))<br>    Redim nextstat(len(init))<br>    for i = 1 to len(init)<br>      stat(i) = mid(init,i,1)<br>    next i<br>    <br>    for i = 1 to t<br>      for j = 1 to len(init)<br>        if j = len(init) then nj = 1 else nj = j + 1<br>        if j = 1 then bj = len(init) else bj = j - 1<br>        cnt = 0<br>        if stat(bj) = 1 then cnt = cnt + 1<br>        if stat(j) = 1 then cnt = cnt + 1<br>        if stat(nj) = 1 then cnt = cnt + 1<br>        if cnt &gt;= 2 then<br>          nextstat(j) = 1<br>        else<br>          nextstat(j) = 0<br>        end if<br>      next j<br>      for j = 1 to len(init)<br>        stat(j) = nextstat(j)<br>      next j<br>    next i<br>    str = ""<br>    for i = 1 to len(init)<br>      str = str &amp; format(stat(i))<br>    next i<br>    return str<br>  End Function<br>End Class<br></td>