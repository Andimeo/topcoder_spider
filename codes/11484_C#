            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class FiveHundredEleven {<br>    int[] c;<br>    int[,] dp;<br>    int max = 512;<br>    public string theWinner(int[] cards) {<br>        c = cards;<br>        dp = new int[cards.Length + 1, 512];<br>        for (int i = 0; i &lt;= cards.Length; i++)<br>            for (int j = 0; j &lt; max; j++)<br>                dp[i, j] = -1;<br>        int res = go(0, 0);<br>        if (res == 1)<br>        {<br>            return "Fox Ciel";<br>        }<br>        else<br>            return "Toastman";<br><br>    }<br><br>    private int go(int total,int set)<br>    {<br>        if (total == c.Length)<br>            return 0;<br>        if (dp[total, set] != -1)<br>            return dp[total, set];<br>        int cnt = 0;<br>        int res = 0;<br>        foreach (int x in c)<br>        {<br>            if (sub(x, set))<br>            {<br>                cnt++;<br>            }<br>            else<br>            {<br>                if ((set | x) == 511)<br>                    continue;<br>                int next = go(total + 1, set | x);<br>                if (next == 0)<br>                    res = 1;<br>            }<br>        }<br>        if (cnt &gt; total)<br>        {<br>            int next = go(total + 1, set);<br>            if (next == 0)<br>                res = 1;<br>        }<br>        return dp[total, set]=res;<br>    }<br><br>    private bool sub(int x, int set)<br>    {<br>        for (int i = 0; i &lt; 10; i++)<br>        {<br>            if ((x &amp; (1 &lt;&lt; i)) &gt; 0 &amp;&amp; (set &amp; (1 &lt;&lt; i)) == 0)<br>                return false;<br>        }<br>        return true;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>