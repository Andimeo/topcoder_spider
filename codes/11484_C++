            #include &lt;algorithm&gt;  <br>#include &lt;string&gt;  <br>#include &lt;set&gt;  <br>#include &lt;map&gt;  <br>#include &lt;vector&gt;  <br>#include &lt;queue&gt;   <br>#include &lt;iostream&gt;  <br>#include &lt;iterator&gt;  <br>#include &lt;math.h&gt;  <br>#include &lt;cstdio&gt;  <br>#include &lt;cstdlib&gt;  <br>#include &lt;sstream&gt;  <br><br>#pragma comment(linker, "/STACK:60777216")  <br><br>using namespace std;  <br><br>typedef pair&lt;int,int&gt; pii;  <br>typedef long long ll;  <br>typedef vector&lt;int&gt; vi;  <br><br>#define UN(v) SORT(v),v.erase(unique(v.begin(),v.end()),v.end())  <br>#define SORT(c) sort((c).begin(),(c).end())  <br>#define FOR(i,a,b) for (int  i=(a); i &lt; (b); i++)   <br>#define REP(i,n) FOR(i,0,n)   <br>#define CL(a,b) memset(a,b,sizeof(a))  <br>#define pb push_back  <br><br>vi c;<br>int r[555][55];<br><br>bool go(int mask,int moves){<br>  if(mask==511) return 1;<br>  if(r[mask][moves]!=-1) return r[mask][moves]==1;<br>  <br>  vi c1,c2;<br>  REP(i,c.size()) if((mask&amp;c[i])==c[i]) c1.pb(c[i]);<br>  else c2.pb(c[i]);<br><br>  if(c1.size() &gt; moves &amp;&amp; !go(mask,moves+1)) return r[mask][moves]=1;<br>  REP(i,c2.size())<br>    if(!go(mask|c2[i],moves+1))<br>      return r[mask][moves]=1;<br>  return r[mask][moves]=0;<br>}<br><br>class FiveHundredEleven {<br>public:<br> string theWinner(vector &lt;int&gt; cards) {<br>   CL(r,-1);<br>  c = cards;<br>  return go(0,0) ? "Fox Ciel" : "Toastman";<br> }<br> <br><br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>