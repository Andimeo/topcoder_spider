            import java.util.*;<br> <br>public class FiveHundredEleven {<br>    int[] cards;<br>    int[][] cache;<br> <br>  public String theWinner(int[] cards) {<br>        this.cards = cards;<br>        cache = new int[512][cards.length + 1];<br>        int whoWinning = solve(0, 0);<br>        if (whoWinning == 1) return "Fox Ciel"; else return "Toastman";<br>  }<br> <br>    private int solve(int memory, int cardsPlayed) {<br>        int storedRes = cache[memory][cardsPlayed];<br>        if (storedRes != 0) return storedRes;<br>        int numRemaining = cards.length - cardsPlayed;<br>        for (int x : cards)<br>            if ((x &amp; memory) != x) {<br>                --numRemaining;<br>                int newMemory = x | memory;<br>                if (newMemory != 511) {<br>                    if (solve(newMemory, cardsPlayed + 1) == 2) {<br>                        cache[memory][cardsPlayed] = 1;<br>                        return 1;<br>                    }<br>                }<br>            }<br>        if (numRemaining &gt; 0) {<br>            if (solve(memory, cardsPlayed + 1) == 2) {<br>                cache[memory][cardsPlayed] = 1;<br>                return 1;<br>            }<br>        }<br>        cache[memory][cardsPlayed] = 2;<br>        return 2;<br>    }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>