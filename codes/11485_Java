            import java.util.*;<br>import static java.lang.Math.*;<br>import java.io.*;<br> <br>public class Zoo {<br>  public static void p(Object... o){System.out.println(Arrays.deepToString(o));}<br>  public long theCount(int[] A) {<br>    long ans = 1;<br>    int prev = 2;<br>    boolean seen = false;<br>    for(int i = 0; i &lt; 50; i++){<br>      int c = count(A, i);<br>      if(c &gt; prev)<br>        return 0;<br>      if(c == 2)<br>        ans *= 2;<br>      if(!seen &amp;&amp; c == 1){<br>        ans *= 2;<br>        seen = true;<br>      }<br>      prev = c;<br>    }<br>    return ans;<br>  }<br>  private int count(int[] A, int i) {<br>    int ans = 0;<br>    for(int a:A)<br>      if(a == i)<br>        ans++;<br>    return ans;<br>  }<br> <br>}<br></td>