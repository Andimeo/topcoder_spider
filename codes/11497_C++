            #line 149 "YetAnotherBingoProblem.cpp"<br>#include &lt;iostream&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;map&gt;<br>#include &lt;cstring&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;cmath&gt;<br>#include &lt;ctime&gt;<br>#include &lt;cctype&gt;<br>#include &lt;set&gt;<br>#include &lt;queue&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#define PB push_back<br>#define MP make_pair<br>using namespace std;<br>bool vi[80];<br>int f[80][1100];<br>int st[80];<br>class YetAnotherBingoProblem {<br>  public:<br>  vector&lt;vector&lt;int&gt; &gt; geta(vector &lt;string&gt; a){<br>    vector&lt;vector&lt;int&gt; &gt; ans;<br>    ans.clear();<br>    string s="";<br>    for (int i=0;i&lt;a.size();i++) s+=a[i];<br>    for (int i=0;i&lt;s.size();i++) if (i==0||s[i-1]==','){<br>      vector&lt;int&gt; t;<br>      t.clear();<br>      for (int j=i;j&lt;s.size();j++){<br>        if (s[j]==' ') continue;<br>        if (s[j]==',') break;<br>        int k=0;<br>        for (;j&lt;s.size()&amp;&amp;isdigit(s[j]);j++) k=k*10+s[j]-'0';<br>        j--;<br>        t.PB(k);<br>      }<br>      sort(t.begin(),t.end());<br>      ans.PB(t);<br>    }<br>    return ans;<br>  }<br>  bool find(vector&lt;int&gt; t,int z){<br>    for (int i=0;i&lt;t.size();i++) if (t[i]==z) return true;<br>    return false;<br>  }<br>  <br>  int longestWinningSequence(vector &lt;string&gt; A, vector &lt;string&gt; B) {<br>    vector&lt;vector&lt;int&gt; &gt; a=geta(A),b=geta(B);<br>    int ans=-1;<br>    for (int i=0;i&lt;a.size();i++)<br>    for (int j=0;j&lt;a[i].size();j++){<br>      vector&lt;vector&lt;int&gt; &gt;t;<br>      t.clear();<br>      for (int z=0;z&lt;a.size();z++)<br>        if (!find(a[z],a[i][j])) t.PB(a[z]);<br>      for (int z=0;z&lt;b.size();z++) t.PB(b[z]);<br>      for (int z=1;z&lt;=75;z++){<br>        int tt=0;<br>        for (int i=0;i&lt;t.size();i++) if (find(t[i],z)) tt+=(1&lt;&lt;i);<br>        st[z]=tt;<br>      }<br>      memset(vi,false,sizeof(vi));<br>      memset(f,-1,sizeof(f));<br>      for (int k=0;k&lt;a[i].size();k++) vi[a[i][k]]=true;<br>      f[0][0]=0;<br>      for (int q=0;q&lt;75;q++)<br>      for (int w=0;w&lt;(1&lt;&lt;(int)t.size());w++)<br>      if (f[q][w]!=-1){<br>        if (!vi[q+1]) f[q+1][w|st[q+1]]=max(f[q+1][w|st[q+1]],f[q][w]);<br>        f[q+1][w]=max(f[q+1][w],f[q][w]+1);<br>      }<br>      ans=max(f[75][(1&lt;&lt;(int)t.size())-1],ans);<br>    }<br>    return ans;<br>  }<br>};<br><br><br><br><br>// Powered by FileEdit<br>// Powered by moj 4.16 [modified TZTester]<br>// Powered by CodeProcessor<br></td>