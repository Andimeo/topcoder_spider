            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class TrainingCamp<br>{<br>  public string[] determineSolvers(string[] attendance, string[] problemTopics)<br>  {<br>        string[] ans = new string[attendance.Length];<br>        for(int i = 0; i &lt; attendance.Length; i++)<br>        {<br>            string answ = "";<br>            for(int j = 0; j &lt; problemTopics.Length; j++)<br>            {<br>                bool b = true;<br>                for(int k = 0; k &lt; problemTopics[j].Length; k++)<br>                {<br>                    if(problemTopics[j][k] == 'X' &amp;&amp; attendance[i][k] != 'X')<br>                    {<br>                        b = false;  <br>                        break;<br>                    }<br>                    if(!b)break;<br>                }<br> <br>                if(b)<br>                {<br>                    answ += 'X';<br>                }<br>                else<br>                {<br>                    answ += '-';<br>                }<br> <br>            }<br>            ans[i] = answ;<br>        }<br>        return ans;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>