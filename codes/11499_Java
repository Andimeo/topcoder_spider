            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class TrainingCamp<br>{<br>  public String[] determineSolvers(String[] a, String[] p)<br>  {<br>    String [] ans = new String[a.length];<br>    for(int i=0;i&lt;a.length;i++)<br>    {<br>      String s="";<br>      for(int j=0;j&lt;p.length;j++)<br>      {<br>        int f=0;<br>      for(int k=0;k&lt;p[j].length();k++)<br>        if(p[j].charAt(k) == 'X' &amp;&amp; a[i].charAt(k) == '-')<br>          {<br>            f=1;<br>            break;<br>            }<br>            if(f == 1)<br>              s+="-";<br>              else<br>              s+="X";<br>        <br>      <br>      }<br>      ans[i]=s;<br>    }<br>    <br>    return ans;<br>    <br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>