            using System;<br>using System.Text;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class FortunateNumbers<br>{<br>    public int getFortunate(int[] a, int[] b, int[] c)<br>    {<br>        int i, j, k;<br>        List&lt;int&gt; cnt = new List&lt;int&gt;();<br><br>        for (i = 0; i &lt; a.Length; i++) for (j = 0; j &lt; b.Length; j++) for (k = 0; k &lt; c.Length; k++)<br>                {<br>                    int p = a[i] + b[j] + c[k];<br>                    if (check(p) &amp;&amp; !cnt.Contains(p)) cnt.Add(p);<br>                }<br><br>        return cnt.Count;<br>    }<br>    bool check(int x)<br>    {<br>        while (x &gt; 0)<br>        {<br>            int disit = x % 10;<br>            if (disit != 5 &amp;&amp; disit != 8) return false;<br>            x /= 10;<br>        }<br>        return true;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] Modified for C# by ysn<br>// Powered by CodeProcessor<br></td>