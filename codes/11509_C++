            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br><br>using namespace std;<br><br>class FortunateNumbers {<br>public:<br>    int check(int a){<br>                int r;<br>                while(a!=0){<br>                    r = a%10;<br>                    if( r != 5 &amp;&amp; r != 8 ) return 0;<br>                    a=a/10;<br>                }<br>            return 1;<br>    }<br>    int getFortunate(vector &lt;int&gt; a, vector &lt;int&gt; b, vector &lt;int&gt; c){<br>            int i,j,k,tmp;<br>            set &lt;int&gt; st;<br>            for(i=0;i&lt;a.size();i++){<br>                for(j=0;j&lt;b.size();j++){<br>                    for(k=0;k&lt;c.size();k++){<br>                        tmp = a[i]+b[j]+c[k] ;<br>                        if( check(tmp) ){<br>                            st.insert(tmp);<br>                        }<br>                    }<br>                }<br>            }<br>            return st.size();<br>        }<br>};<br></td>