            Imports Microsoft.VisualBasic<br>Imports System.Collections.Generic<br> <br>Public Class FortunateNumbers<br>  Public Function getFortunate(ByVal a As Integer(), ByVal b As Integer(), ByVal c As Integer()) As Integer<br>        Dim i, j, k, l As Integer<br>        Dim na As Integer = a.GetUpperBound(0)<br>        Dim nb As Integer = b.GetUpperBound(0)<br>        Dim nc As Integer = c.GetUpperBound(0)<br>        Dim ima As New List(Of Integer)()<br> <br>        For i = 0 To na<br>            For j = 0 To nb<br>                For k = 0 To nc<br>                    Dim num As Integer = a(i) + b(j) + c(k)<br>                    Dim snum As String = num.ToString()<br>                    Dim cnt As Integer = 0<br>                    For l = 0 To snum.Length - 1<br>                        If snum(l) = "5" Or snum(l) = "8" Then<br>                            cnt += 1<br>                        End If<br>                    Next<br>                    If cnt = snum.Length AndAlso Not ima.Contains(num) Then<br>                        ima.Add(num)<br>                    End If<br>                Next<br>            Next<br>        Next<br>    <br>        Return ima.Count<br>  End Function<br>End Class<br></td>