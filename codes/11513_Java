<td class="problemText" colspan="8" valign="middle" align="left">
            public class OrthogonalAnagram {<br>  public String solve(String s) {<br>    String res = "";<br>    char a[] = new char[256];<br>    char r[] = new char[256];<br>    int i, j;<br>    for (i = 0; i &lt; s.length (); i++) {<br>      a[s.charAt (i)] ++;<br>      r[s.charAt (i)] ++;<br>    }<br>    for (i = 0; i &lt; s.length (); i++) {<br>      int t = s.length () - i;<br>      int min = 256;<br>      int dev = -1;<br>      for (j = 0; j &lt; 256; j++) {<br>        if (a[j] &gt; 0 &amp;&amp; min == 256 &amp;&amp; s.charAt (i) != j) {<br>          min = j;<br>        }<br>        if (a[j] &gt; t - r[j]) {<br>          return "";<br>        }<br>        if ( a[j] == t - r[j] &amp;&amp; s.charAt (i) != j) {<br>          dev = j;<br>        }<br>      }<br>      if (dev != -1) {<br>        res = res + (char) dev;<br>        a[dev] --;<br>      } else {<br>        res = res + (char) min;<br>        a[min] --;<br>      }<br>      r[s.charAt (i)] --;<br>    }<br>    return res;<br>  }<br>}<br></td>