            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class EllysFractions<br>{<br>  long f(int n)<br>  {<br>    int cnt = 0;<br>    for(int i=0; i&lt;=n; i++)<br>    {<br>      if(p[i])<br>        cnt ++;<br>    }<br>    long r = (1L &lt;&lt; (cnt - 1));<br>    return r;<br>  }<br>  bool[] p = new bool[300];<br>  public long getCount(int n)<br>  {<br>    long r = 0;    <br>    for(int i=2; i&lt;p.Length; i++)<br>    {<br>      p[i] = true;<br>    }<br>    for(int i=2; i&lt;p.Length; i++)<br>    {<br>      if(p[i])<br>      {<br>        for(int j=i + i; j&lt;p.Length; j+=i)<br>        {<br>          p[j] = false;<br>        }<br>      }<br>    }<br>    for(int i=0; i&lt;p.Length; i++)<br>    {<br>      //if(p[i])<br>      //  Console.WriteLine(i);<br>    }<br>    for(int i=2; i&lt;=n; i++)<br>    {<br>      r += f(i);<br>    }<br>    return r;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>