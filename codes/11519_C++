<td class="problemText" colspan="8" valign="middle" align="left">
            #include &lt;cstring&gt;<br>#include &lt;iostream&gt;<br>using namespace std;<br>class EllysFractions<br>{<br>    public:<br>    int prim[300],top;<br>    int cnt[300];<br>    void getPrim()<br>    {<br>        bool flag[300];<br>        top=0;<br>        memset(flag,0,sizeof(flag));<br>        for(int i=2;i&lt;300;++i)<br>        {<br>            if(!flag[i])prim[top++]=i;<br>            for(int j=0;j&lt;top&amp;&amp;prim[j]*i&lt;300;++j)<br>            {<br>                flag[i*prim[j]]=1;<br>                if(i%prim[j]==0)break;<br>            }<br>        }<br>        cnt[1]=0;<br>        for(int i=2;i&lt;300;++i)<br>            if(!flag[i])cnt[i]=cnt[i-1]+1;<br>            else cnt[i]=cnt[i-1];<br>    }<br>    long long getCount(int N)<br>    {<br>        getPrim();<br>        long long ans=0;<br>        for(int i=2;i&lt;=N;++i)<br>        {<br>            ans+=(1LL&lt;&lt;(cnt[i]-1));<br>        }<br>        return ans;<br>    }<br>};<br></td>