            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class NetworkXZeroOne<br>{<br>  public string reconstruct(string message)<br>  {<br>    string kk="";<br>    for(int i=0;i&lt;message.Length;i++)<br>    if(i%2==0)<br>    kk+="x";<br>    else kk+="o";<br>    int ii;<br>    for(ii=0;ii&lt;message.Length;ii++)<br>    {<br>      if(message[ii]=='?')continue;<br>      if(message[ii]!=kk[ii])break;<br>    }<br>    if(ii&gt;=message.Length)return kk;<br>    else<br>    {<br>    kk="";<br>    for(int i=0;i&lt;message.Length;i++)<br>    if(i%2==1)<br>    kk+="x";<br>    else kk+="o";<br>    <br>    }<br>    return kk;  <br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>