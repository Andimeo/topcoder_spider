<td class="problemText" colspan="8" valign="middle" align="left">
            #include&lt;iostream&gt;<br>#include&lt;vector&gt;<br>using namespace std;<br> <br>class EllysDirectoryListing {<br>  public:<br>  vector &lt;string&gt; getFiles(vector &lt;string&gt; files) {<br>    int len = files.size();<br>    int turn = 1;<br>    <br>    for(int i = 0; i &lt; len-2; ++i) {<br>      if((files[i].length() == 1 &amp;&amp; files[i][0] == '.')||<br>        (files[i].length() == 2 &amp;&amp; files[i][0] == '.' &amp;&amp; files[i][1] == '.')) {<br>        if(turn % 2 == 1) {<br>          files[i].swap(files[len-1]);<br>          ++turn;<br>        }<br>        else {<br>          files[i].swap(files[len-2]);<br>          ++turn;<br>        }<br>        --i;<br>      }<br>    }<br>    return files;<br>  }<br>};<br></td>