            Class MonochromaticBoard<br> Public Function theMin(ByVal board As String()) As Integer<br>        Dim med As String() = board.Clone<br>        Dim counter As Integer = 0<br>        Dim all As Boolean = False<br>        Dim theX As Integer = board(0).Length<br>        Dim theY As Integer = board.Length<br>        Dim no As Boolean = True<br>        If theX &lt; theY Then<br>            no = False<br>        End If<br><br>        If no Then<br>            For y As Integer = 0 To theY - 1<br>                all = True<br>                For x As Integer = 0 To theX - 1<br>                    If board(y).Substring(x, 1).Equals("W") Then<br>                        all = False<br>                    End If<br>                Next<br>                If all Then<br>                    counter += 1<br>                End If<br>            Next<br>        End If<br>        <br><br>        If counter &lt; theY Then<br>            For x As Integer = 0 To theX - 1<br>                all = True<br>                For y As Integer = 0 To theY - 1<br>                    If board(y).Substring(x, 1).Equals("W") Then<br>                        all = False<br>                    End If<br>                Next<br>                If all Then<br>                    counter += 1<br>                End If<br>            Next<br>        End If<br><br>        If Not no Then<br>            If counter &lt; theX Then<br>                For y As Integer = 0 To theY - 1<br>                    all = True<br>                    For x As Integer = 0 To theX - 1<br>                        If board(y).Substring(x, 1).Equals("W") Then<br>                            all = False<br>                        End If<br>                    Next<br>                    If all Then<br>                        counter += 1<br>                    End If<br>                Next<br>            End If<br>        End If<br><br><br>        Return counter<