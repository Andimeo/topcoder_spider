            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class CompositeSmash<br>{<br>  public string thePossible(int n, int t)<br>  {<br>    string yes = "Yes";<br>    string no = "No";<br>    a = new int[n + 1];<br>    return calc(n, t) ? yes : no;<br>  }<br>  int[] a;<br>  bool calc(int n, int t)<br>  {<br>    if(a[n] != 0)<br>      return a[n] == 1;<br>    bool res = true;<br>    if(n % t != 0)<br>      res = false;<br>    else if(n == t)<br>    {<br>      res = true;<br>    }<br>    else<br>    {<br>      for(int x=2; x*x&lt;=n; x++)<br>      {<br>        if(n % x == 0)<br>        {<br>          if(calc(x, t) || calc(n / x, t))<br>          {<br>          }<br>          else<br>          {<br>            res = false;<br>          }<br>        }<br>      }<br>    }<br>    a[n] = res ? 1 : 2;<br>    return res;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>