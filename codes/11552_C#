            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class BinaryCards<br>{<br>  public long largestNumber(long A, long B)<br>  {<br>        long x = 1, res = 0;<br>        for (int i = 0; i &lt; 62; i++)<br>        {<br>            if (((B &amp; x) != 0) &amp;&amp; ((A &amp; x) == 0))<br>                res = x;<br>            x *= 2;<br>        }<br>        while (res &gt; 0)<br>        {<br>            B |= res;<br>            res /= 2;<br>        }<br>        return B;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>