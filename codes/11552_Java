            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class BinaryCards<br>{<br> <br>  public long largestNumber(long A, long B)<br>  {<br>    long best=B;<br>    for(int i=0;i&lt;63;i++)<br>    {<br>      long z=((B&gt;&gt;i)&lt;&lt;i);<br>      if(z&lt;=A) continue;<br>      z += z&amp;(-z);<br>      z--;<br>      best=Math.max(best,z);<br>    }<br>    return best;<br>  }<br> <br> <br> <br>  void debug(Object...os)<br>  {<br>    System.out.println(Arrays.deepToString(os));<br>  }<br> <br>  void print(int[][] arr)<br>  {<br>    for(int i=0;i&lt;arr.length;i++)<br>      debug(arr[i]);<br>  }<br>        <br> <br>}<br> <br> <br> <br> <br> <br>//Powered by KawigiEdit 2.1.8 (beta) modified by pivanof!<br></td>