            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class WhichDay<br>{<br>  public string getDay(string[] notOnThisDay)<br>  {<br>    string[] ll={"Sunday", "Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday"};  <br>    for(int i=0;i&lt;ll.Length;i++)<br>    {<br>      if(Array.IndexOf(notOnThisDay,ll[i])&lt;0)return ll[i];<br>    }<br>    return "";<br>    <br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>