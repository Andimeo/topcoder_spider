            #include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;map&gt;<br>#include &lt;vector&gt;<br>using namespace std;<br> <br>class WhichDay{<br>    public:<br>        string getDay(vector&lt;string&gt; v){<br>            vector&lt;string&gt; vv;<br>            //vv.push_back(""Sunday", "Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday".");<br>            vv.push_back("Sunday");<br>            vv.push_back("Monday");<br>            vv.push_back("Tuesday");<br>            vv.push_back("Wednesday");<br>            vv.push_back("Thursday");<br>            vv.push_back("Friday");<br>            vv.push_back("Saturday");<br>            map&lt;string,int&gt; m;<br>            for(int i=0;i&lt;v.size();i++){<br>                m[v[i]]=1;<br>            }<br>            for(int i=0;i&lt;vv.size();i++){<br>                if(m[vv[i]]==0) return vv[i];<br>            }<br>        }<br>};<br></td>