            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class WhichDay<br>{<br>  public String getDay(String[] notOnThisDay)<br>  {<br>    TreeSet&lt;String&gt; D=new TreeSet&lt;String&gt;();<br>    D.add("Monday");<br>    D.add("Tuesday");<br>    D.add("Wednesday");<br>    D.add("Thursday");<br>    D.add("Friday");<br>    D.add("Saturday");<br>    D.add("Sunday");<br>    for (String d : notOnThisDay)<br>      D.remove(d);<br>    return D.first();<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>