            import java.util.*;<br>import static java.util.Arrays.fill;<br>import static java.lang.System.arraycopy;<br><br>public class RGBGame {<br>  public double[] probabilities(String[] board) {<br>    return new double[]{alice(board), bob(board)};<br>   }<br>  <br>  double alice(String[] board) {<br>    int n = board.length;<br>    double p = 1;<br>    fori: for (int i = 0; i &lt; n; i++) {<br>      int q = 0;<br>      for (int j = 0; j &lt; n; j++) {<br>        char c = board[i].charAt(j);<br>        if (c == 'B' || c == 'G') {<br>          continue fori;<br>        }<br>        if (c == '?') ++q;<br>      }<br>      p *= 1 - Math.pow(1. / 3, q);<br>    }<br>    return 1 - p;<br>  }<br>  <br>  double bob(String[] board) {<br>    int n = board.length;<br>    double p = 1;<br>    fori: for (int i = 0; i &lt; n; i++) {<br>      int q = 0;<br>      for (int j = 0; j &lt; n; j++) {<br>        char c = board[i].charAt(j);<br>        if (c == 'B') <br>          continue fori;<br>        if (c == '?') ++q;<br>      }<br>      p *= 1 - Math.pow(2./3, q);<br>    }<br>    return p;<br>  }<br><br>  private static void debug(Object... o) {<br>    System.out.println(Arrays.deepToString(o));<br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>