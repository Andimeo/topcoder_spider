            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class Cut {<br>    public int getMaximum(int[] eelLengths, int maxCuts)<br>    {<br>        int ret = 0;<br>        Array.Sort(eelLengths);<br>        int i;<br>        for (i = 0; i &lt; eelLengths.Length; i++)<br>        {<br>            if (eelLengths[i] % 10 != 0) continue;<br>            while (maxCuts &gt; 0 &amp;&amp; eelLengths[i] &gt; 10)<br>            {<br>                ret += 1;<br>                eelLengths[i] -= 10;<br>                maxCuts--;<br>            }<br>            if (eelLengths[i] == 10)<br>            {<br>                eelLengths[i] -= 10;<br>                ret += 1;<br>            }<br>        }<br>        for (i = 0; i &lt; eelLengths.Length; i++)<br>        {<br>            if (eelLengths[i] % 10 == 0) continue;<br>            while (maxCuts &gt; 0 &amp;&amp; eelLengths[i] &gt; 10)<br>            {<br>                ret += 1;<br>                eelLengths[i] -= 10;<br>                maxCuts--;<br>            }<br>        }<br><br>        return ret;<br>    }<br><br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>