            //  SRM 528 (A)<br> <br>import static java.lang.Math.*;<br>import static java.math.BigInteger.*;<br>import static java.util.Arrays.*;<br>import static java.util.Collections.*;<br>import java.math.*;<br>import java.util.*;<br> <br>public class Cut {<br>  <br>  public int getMaximum(int[] eelLengths, int maxCuts) {<br>    int ans = 0;<br>    int c = maxCuts;<br>    sort(eelLengths);<br>    for (int x: eelLengths) if (x % 10 == 0) {<br>      int tmp = min(x / 10 - 1, c);<br>      ans += tmp;<br>      if (tmp == x / 10 - 1) {<br>        ++ans;<br>      }<br>      c -= tmp;<br>    }<br>    for (int x: eelLengths) if (x % 10 != 0) {<br>      int tmp = min(x / 10, c);<br>      ans += tmp;<br>      c -= tmp;<br>    }<br>    return ans;<br>  }<br>  <br>  void _out(Object...os) {<br>    System.out.println(deepToString(os));<br>  }<br>}<br></td>