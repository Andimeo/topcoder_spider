            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class RedAndGreen<br>{<br>  public int minPaints(string row)<br>  {<br>    int r = int.MaxValue;<br>        for (int i = 0; i &lt; row.Length; i++)<br>        {<br>            int r1 = 0;<br>            for (int j = 0; j &lt; i; j++)<br>            {<br>                if (row[j] == 'G')<br>                {<br>                    r1++;<br>                }<br>            }<br> <br>            for (int j = i + 1; j &lt; row.Length; j++)<br>            {<br>                if (row[j] == 'R')<br>                {<br>                    r1++;<br>                }<br>            }<br> <br>            r = Math.Min(r, r1);<br>        }<br>        return r;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>