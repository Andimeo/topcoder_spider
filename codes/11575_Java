            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class FoxAndIntegers<br>{<br>  public int[] get(int AminusB, int BminusC, int AplusB, int BplusC)<br>  {<br>    for(int A=-200;A&lt;=200;A++)<br>    {<br>      for(int B=-200;B&lt;=200;B++)<br>      {<br>        for(int C=-200;C&lt;=200;C++)<br>        {<br>          if(AminusB==A-B&amp;&amp;BminusC==B-C&amp;&amp;AplusB==A+B&amp;&amp;BplusC==B+C)<br>          {<br>            return new int[]{A,B,C};<br>          }<br>        }<br>      }<br>    }<br>    return new int[0];<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>