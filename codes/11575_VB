<td class="problemText" colspan="8" valign="middle" align="left">
            Public Class FoxAndIntegers<br>      Public Function [get](AminusB As Integer, BminusC As Integer, AplusB As Integer, BplusC As Integer) As Integer()<br>          Dim B2 As Integer, C2 As integer,  res(2) as integer, empty() as integer ={}<br>            B2 = AplusB - AminusB<br>            C2 = BplusC - BminusC <br>            if C2 Mod 2 &lt;&gt; 0 OR B2 Mod 2 &lt;&gt; 0 OR (BminusC + BplusC) &lt;&gt; B2 then <br>              return empty          <br>            end if <br>            res(0) = AminusB + B2/2: res(1) = B2/2: res(2) = C2/2<br>            Return res<br>        End Function<br>    End Class<br></td>