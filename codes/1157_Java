            public class Jumper {<br>// ground<br>char board[][];<br>public int minTime(String[] patterns, int[] s0, String rows) {<br>  int speeds[] = new int[rows.length()];<br>  board = new char[rows.length()+1][20];<br>  for(int i=0;i&lt;rows.length();i++) {<br>    int n = rows.charAt(i)-'0';<br>    String s = patterns[n];<br>    board[i+1] = (s+s+s+s).toCharArray();<br>    speeds[i] = s0[n];<br>  }<br>  int time = 1;<br>  board[0] = "####################".toCharArray();<br>  board[0][0] = 'o';<br> <br> <br>  // ok, someone will find a pathological case and break this<br>  // but I'm tired now!<br>  while(time &lt; 40000) {<br>    // move player<br>    for(int y=board.length;y--&gt;0;)<br>    for(int x=20;x--&gt;0;) {<br>      if(board[y][x] == 'o') {<br>        if(x&gt;0 &amp;&amp; board[y][x-1]=='#') board[y][x-1] = '+';<br>        if(y&gt;0 &amp;&amp; board[y-1][x]=='#') board[y-1][x] = '+';<br>        if(x&lt;19 &amp;&amp; board[y][x+1]=='#') board[y][x+1] = '+';<br>        if(y==board.length-1) return time; // wee!<br>        if(board[y+1][x]=='#') board[y+1][x] = '+';<br>      }<br>    }<br> <br> <br>    for(int y=board.length;y--&gt;0;)<br>    for(int x=20;x--&gt;0;) {<br>      if(board[y][x]=='+') board[y][x]='o';<br>    }<br> <br>    // fluff<br>    for(int y=0; y&lt;speeds.length;) {<br>      int s = speeds[y];<br>      y++;<br>      char b[] = board[y];<br>      while(s&lt;0) {<br>        s++; char c = b[0];<br>        System.arraycopy(b,1,b,0,19);<br>        b[19] = c=='.'?'.':'#';<br>      }<br>      while(s&gt;0) {<br>              s--; char c = b[19];<br>              System.arraycopy(b,0,b,1,19);<br>              b[0] = c=='.'?'.':'#';<br>      }<br>    }<br> <br>/*    for(int y=0;y&lt;board.length;y++) {<br>      System.out.println(new String(board[y]));<br>    }<br>    System.out.println();*/<br>    time++;<br>  }<br>  return -1;<br>}}<br></td>