            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class PointErasingTwo<br>{<br>  public int getMaximum(int[] y)<br>  {<br>    int i, j, k, erase, now;<br> <br>    erase = -1;<br>    for (i = 0; i &lt; y.Length - 1; i++)<br>    {<br>      for (j = i + 1; j &lt; y.Length; j++)<br>      {<br>        now = 0;<br>        for (k = i + 1; k &lt; j; k++)<br>        {<br>          if (k == i || k == j)<br>            continue;<br> <br>          if (y[i] &gt; y[j])<br>          {<br>            if (y[k] &gt; y[j] &amp;&amp; y[k] &lt; y[i])<br>              now++;<br>          }<br>          else<br>          {<br>            if (y[k] &gt; y[i] &amp;&amp; y[k] &lt; y[j])<br>              now++;<br>          }<br>        }<br> <br>        if (now &gt; erase)<br>          erase = now;<br>      }<br>    }<br> <br>    return erase;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>