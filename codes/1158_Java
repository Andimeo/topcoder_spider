            import java.util.*;<br>public class HillHike {<br>  public long numPaths(int distance, int maxHeight, int[] landmarks) {<br>    long[][][][] numways = new long[102][2][52][52];<br>    <br>    numways[1][0][1][0] = 1;<br>    <br>    for (int d=1; d&lt;distance; d++) {<br>      for (int type=0; type&lt;=1; type++) {<br>        for (int h=1; h&lt;=maxHeight; h++) {<br>          int nexttype = type;<br>          if (type==0 &amp;&amp; h==maxHeight) nexttype = 1;<br>          for (int mark=0; mark&lt;landmarks.length+1; mark++) {<br>            int nextmark = mark;<br>            if (mark&lt;landmarks.length &amp;&amp; h==landmarks[mark]) {<br>              nextmark++;<br>            }<br>            <br>            long numhere = numways[d][type][h][mark];<br>            //System.out.println(d+" "+type+" "+h+" "+mark+" "+numhere);<br>            //System.out.println("-&gt;"+(d+1)+" "+nexttype+" "+h+" "+nextmark);<br>            for (int i=-1; i&lt;=1; i++) {<br>              numways[d+1][nexttype][h+i][nextmark]+=numhere;<br>            }<br>          }<br>        }<br>      }<br>    }<br>    <br>    return numways[distance][1][0][landmarks.length];<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>