            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class CorrectMultiplication {<br>    public long getMinimum(int a, int b, int c)<br>    {<br>        long ret = long.MaxValue;<br>        long MAX = 1000050000;<br>        long border = (long)Math.Sqrt(MAX);<br>        int i, j;<br>        for (i = 1; i &lt;= border + 1000; i++)<br>        {<br>            for (j = -2; j &lt;= 2; j++)<br>            {<br>                long A = i;<br>                long B = c / i + j;<br>                while (A &lt;= 0) A++;<br>                while (B &lt;= 0) B++;<br>                ret = Math.Min(ret, Math.Abs(a - A) + Math.Abs(b - B) + Math.Abs(A * B - c));<br>                ret = Math.Min(ret, Math.Abs(a - B) + Math.Abs(b - A) + Math.Abs(A * B - c));<br>            }<br>        }<br>        return ret;<br>    }<br><br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>