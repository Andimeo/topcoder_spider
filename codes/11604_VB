            Imports Microsoft.VisualBasic<br>Imports System<br>Imports System.Collections<br>Imports System.Collections.Generic<br>Imports System.Text<br>Imports System.Math<br>Imports Weight = System.Int32<br>Public Class CorrectMultiplication<br>    Public Shared Sub Swap(Of T)(ByRef a As T, ByRef b As T)<br>        Dim temp As T = a : a = b : b = temp<br>    End Sub<br>    Public Function Solve(ByVal A As Long, ByVal b As Long, ByVal c As Long) As Long<br>        'A&lt;=B<br>        'x-b+ax-c<br>        Solve = 999999999999999L<br>        Dim x As Long<br>        For i As Long= 0 To 50<br>            x = b + i<br>            If x &gt; 0 Then Solve = Min(Solve, Abs(x - b) + Abs(A * x - c)) 'X=b<br>            x = b - i<br>            If x &gt; 0 Then Solve = Min(Solve, Abs(x - b) + Abs(A * x - c)) 'X=b<br> <br>            x = (c \ A) + i<br>            If x &gt; 0 Then Solve = Min(Solve, Abs(x - b) + Abs(A * x - c)) 'AX=c<br>            x = (c \ A) - i<br>            If x &gt; 0 Then Solve = Min(Solve, Abs(x - b) + Abs(A * x - c)) 'AX=c<br>        Next<br> <br> <br>    End Function<br>    Public Function getMinimum(ByVal a As Integer, ByVal b As Integer, ByVal c As Integer) As Long<br>        Dim i As Integer, j As Integer<br>        If a &gt; b Then<br>            Swap(a, b) '[\u27491][\u35215][\u21270]<br>        End If<br>        getMinimum = CType((a - 1), Long) + (b - 1) + (c - 1) '1x1=1<br>        Dim AV As Long<br>        For AV = 1 To 100000<br>            getMinimum = Min(getMinimum, Solve(AV, b, c) + Abs(AV - a))<br>        Next<br>    End Function<br> <br>End Class<br></td>