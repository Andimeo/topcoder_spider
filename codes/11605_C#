            using System;<br> <br>public class RowAndManyCoins<br>{<br>  public string getWinner(string cells)<br>  {<br>    int ka, kb;<br>    ka = kb = 0;<br>        int n = cells.Length;<br>        char prev = 'X';<br>        for (int i = 0; i &lt; n; i++)<br>        {<br>            char ch = cells[i];<br>            if (ch != prev)<br>            {<br>                if (prev == 'A') ka++; else if (prev == 'B') kb++;<br>                prev = ch;<br>            }<br>        }<br>        if (prev == 'A') ka++; else kb++;<br> <br>        if (ka == kb) return "Alice";<br>        else if (kb &gt; ka) return "Bob";<br>    return "Alice";<br>  }<br>}<br></td>