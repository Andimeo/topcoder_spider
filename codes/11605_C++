            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br><br>using namespace std;<br><br>class RowAndManyCoins {<br>public:<br>  string getWinner(string);<br>};<br><br>string RowAndManyCoins::getWinner(string cells) {<br>  int i,a,b;<br>  string s;<br>  vector&lt;char&gt; v;<br>  vector&lt;char&gt; :: iterator it;<br>  s = cells;<br>  for(i =0; i &lt; s.size(); i++) {<br>    v.push_back(s[i]);<br>  }<br>  <br>  it = unique(v.begin(), v.end());<br>  v.resize(it-v.begin());<br>  <br>  if(v[0] == 'A' || v[v.size()-1] == 'A') {<br>    return "Alice";<br>  }<br>  a = b = 0;<br>  for(i = 0; i &lt; v.size(); i++) {<br>    if(v[i] == 'A') <br>      a++;<br>    else b++;<br>  }<br>  if(b&gt;a) return "Bob";<br>  else return "Alice";<br>}<br></td>