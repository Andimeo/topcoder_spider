            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class RowAndManyCoins<br>{<br>  public String getWinner(String cells)<br>  {<br>    int n=cells.length();<br>    if(cells.charAt(0)=='A' &amp;&amp; cells.charAt(n-1)=='A') return "Alice";<br>    if(cells.charAt(0)=='A' &amp;&amp; cells.charAt(n-1)=='B') return "Alice";<br>    if(cells.charAt(0)=='B' &amp;&amp; cells.charAt(n-1)=='A') return "Alice";<br>    if(cells.charAt(0)=='B' &amp;&amp; cells.charAt(n-1)=='B') return "Bob";<br>    return "";<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.8 (beta) modified by pivanof!<br></td>