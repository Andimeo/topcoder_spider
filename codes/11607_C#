            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class MagicDiamonds {<br>    public long minimalTransfer(long n)<br>    {<br>        if (n == 3) return 3;<br>        else if (isPrime(n)) return 2;<br>        return 1;<br>    }<br><br>    bool isPrime(long a)<br>    {<br>        long i;<br>        if (a &lt;= 1) return false;<br>        for (i = 2; i * i &lt;= a; i++) if (a % i == 0) return false;<br>        return true;<br>    }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>