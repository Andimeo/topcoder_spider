<td class="problemText" colspan="8" valign="middle" align="left">
            #include &lt;cstdio&gt;<br>#include &lt;cstring&gt;<br>#include &lt;iostream&gt;<br>#include &lt;algorithm&gt;<br>using namespace std;<br><br>class CorrectMultiplicationTwo{<br>    public:<br>        int getMinimum(int a,int b,int c){<br>            int ans=a+b+c;<br>            for(int i=1;i&lt;=1000000;++i)<br>                for(int j=1;j&lt;=1000000;++j){<br>                    int tmp=abs(i-a)+abs(j-b)+abs(i*j-c);<br>                    if(ans&gt;tmp)ans=tmp;<br>                    if(i*j&gt;c)break;<br>                }<br>            return ans;<br>        }<br>};<br></td>