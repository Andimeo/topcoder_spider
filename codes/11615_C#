<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class AlphabetPath<br>{<br>  public string doesItExist(string[] letterMaze)<br>  {<br>        for (int i = 0; i &lt; letterMaze.Length; ++i)<br>            for (int j = 0; j &lt; letterMaze[i].Length; ++j)<br>                if (letterMaze[i][j] &gt;= 'A' &amp;&amp; letterMaze[i][j] &lt;= 'Y')<br>                {<br>                    bool adjacent = false;<br>                    if (i &gt; 0 &amp;&amp; letterMaze[i - 1][j] == letterMaze[i][j] + 1)<br>                        adjacent = true;<br>                    if (i + 1 &lt; letterMaze.Length &amp;&amp; letterMaze[i + 1][j] == letterMaze[i][j] + 1)<br>                        adjacent = true;<br>                    if (j &gt; 0 &amp;&amp; letterMaze[i][j - 1] == letterMaze[i][j] + 1)<br>                        adjacent = true;<br>                    if (j + 1 &lt; letterMaze[i].Length &amp;&amp; letterMaze[i][j + 1] == letterMaze[i][j] + 1)<br>                        adjacent = true;<br>                    if (!adjacent)<br>                        return "NO";<br>                }<br>        return "YES";<br>  }<br>}<br></td>