            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class AlphabetPath{<br>public:<br>  string doesItExist(vector &lt;string&gt; grid){<br>        int r,c;<br>        int i,j;<br>        r=grid.size();<br>        c=grid[0].size();<br>        bool flag=true;<br>        int ti=-1,tj=-1;<br>        char ch ='A';<br> <br>        for(i=0;i&lt;r;i++){<br>            for(j=0;j&lt;c;j++){<br>                if(grid[i][j]=='A'){<br>                    ti=i;<br>                    tj=j;<br>                    break;<br>                }<br>            }<br>        }<br>        flag=false;<br>        int moves[4][2]={{0,1},{0,-1},{1,0},{-1,0}};<br>        int p,q;<br>        if(ti==-1&amp;&amp;tj==-1) return "NO";<br>        else{<br>            bool temp=false;<br>            int count=0;<br>            while(count&lt;25){<br>                temp=false;<br>                for(i=0;i&lt;4;i++){<br>                    p=ti+moves[i][0];<br>                    q=tj+moves[i][1];<br>                    if(p&gt;=0&amp;&amp;p&lt;r&amp;&amp;q&gt;=0&amp;&amp;q&lt;c){<br>                        if(grid[p][q]==(char)(ch+1)){<br>                            temp=true;<br>                            break;<br>                        }<br>                    }<br>                }<br>                if(temp==true){<br>                    ti=p;<br>                    tj=q;<br>                    ch++;<br>                    count++;<br>                    //cout&lt;&lt;ti&lt;&lt;' '&lt;&lt;tj&lt;&lt;" "&lt;&lt;ch&lt;&lt;count&lt;&lt;endl;<br>                }<br>                else break;<br>            }<br>            if(temp==true){<br>                //cout&lt;&lt;temp&lt;&lt;count&lt;&lt;endl;<br>                return "YES";<br>            }<br>            else{<br>                //cout&lt;&lt;temp&lt;&lt;count&lt;&lt;endl;<br>                return "NO";<br>            }<br>        }<br>  }<br>};<br></td>