            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;queue&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br>#include &lt;cstring&gt;<br>using namespace std;<br> <br>typedef long long ll;<br>string upp = "";<br>ll dp[20][20][20][3];<br>ll calc(int idx, int f, int s, int iseq) {<br>  if(idx == 12) {<br>//    cout&lt;&lt;idx&lt;&lt;endl;<br>    if(iseq&lt;=1)<br>      return abs(f - s);<br>    return 0;<br>  }<br>  ll &amp;ret = dp[idx][f][s][iseq];<br>  if(ret!=-1)<br>    return ret;<br>  ret = 0;<br>  for (int i = 0; i &lt; 10; ++i) {<br>    if(i &lt; upp[idx] - '0')<br>      ret+=calc(idx+1, f+(i==4), s+(i==7), 0);<br>    else if(iseq == 1 &amp;&amp; i == upp[idx] - '0')<br>      ret+=calc(idx+1, f+(i==4), s+(i==7), 1);<br>    else if(iseq == 0)<br>      ret+=calc(idx+1, f+(i==4), s+(i==7), 0);<br>  }<br>  return ret;<br>}<br>class SumOfLuckiness {<br>public:<br>  long long theSum(int A, int B) {<br>    A--;<br>    int tmp = A, cs = 0;<br>    vector&lt;int&gt;v;<br>    while(tmp)<br>      cs++,  v.push_back(tmp%10), tmp/=10;<br>    upp = "";<br>    while(cs+upp.size() &lt; 12)<br>      upp+='0';<br>    <br>    reverse(v.begin(), v.end());<br>    for (int i = 0; i &lt; (int)v.size(); ++i)<br>      upp+=(char)(v[i]+'0');<br> <br>//    cout&lt;&lt;upp&lt;&lt;endl;<br>    memset(dp, -1, sizeof dp);<br>    ll sm = calc(0, 0, 0, 1);<br>    v.clear();<br>    tmp = B;<br>    cs = 0;<br>    while(tmp)<br>      cs++,  v.push_back(tmp%10), tmp/=10;<br> <br>    upp = "";<br>    while(cs+upp.size() &lt; 12)<br>      upp+='0';<br>    reverse(v.begin(), v.end());<br>    for (int i = 0; i &lt; (int)v.size(); ++i) {<br>      upp+=(char)(v[i] + '0');<br>    }<br>//    cout&lt;&lt;upp&lt;&lt;endl;<br> <br>    memset(dp, -1, sizeof dp);<br>    ll bg = calc(0, 0, 0, 1);<br>//    cout&lt;&lt;bg&lt;&lt;" "&lt;&lt;sm&lt;&lt;endl;<br>    return bg-sm;<br>    //calc();<br>  }<br>};<br> <br> <br> <br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>