            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class CheatingQuiz<br>{<br>  public int[] howMany(String A)<br>  {<br>    int[] ans = new int[A.length()];<br>    for(int i = 0; i &lt; A.length(); i++)<br>    {<br>      ans[i] = count(i, A);<br>    }<br>    return ans;<br>  }<br>  <br>  public int count(int i, String A)<br>  {<br>    Set &lt;Character&gt; set = new HashSet &lt;Character&gt; ();<br>    for(int k = i; k &lt; A.length(); k++) set.add(A.charAt(k));<br>    return set.size();<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.8 (beta) modified by pivanof!<br></td>