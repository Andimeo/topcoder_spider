            using System;<br>using System.Collections.Generic;<br><br>public class ShippingCubes<br>{<br>    public int minimalCost(int N)<br>    {<br>        int res = int.MaxValue;<br>        for (int i = 1; i &lt;= N; ++i)<br>        {<br>            for (int j = 1; j &lt;= N; ++j)<br>            {<br>                for (int k = 1; k &lt;= N; ++k)<br>                {<br>                    if (i * j * k == N)<br>                    {<br>                        res = Math.Min(i + j + k, res);<br>                    }<br>                }<br>            }<br>        }<br>        return res;<br>    }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>