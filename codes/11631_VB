            Public Class DengklekTryingToSleep<br>    Public Function minDucks(ByVal ducks As Integer()) As Integer<br>        Dim result As Integer = 0<br>        Dim min As Integer = 101<br>        Dim max As Integer = 0<br>        For Each item As Integer In ducks<br>            If max &lt; item Then<br>                max = item<br>            End If<br>            If min &gt; item Then<br>                min = item<br>            End If<br>        Next<br>        For index As Integer = min To max<br>            Dim contain As Boolean = False<br>            For Each item As Integer In ducks<br>                If item = index Then<br>                    contain = True<br>                End If<br>            Next<br>            If contain = False Then<br>                result = result + 1<br>            End If<br>        Next<br>        Return result<br>    End Function<br>End Class<br></td>