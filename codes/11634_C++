            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class SPartition {<br>public:<br>  long long getCount(string);<br>};<br> <br>long long SPartition::getCount(string s) {<br>  int n = s.length(), i, c0 = 0, c1 = 0, a[55];<br>  for (i=0;i&lt;n;i++) {<br>    if (s[i] == 'o') c0++;<br>    else c1++;<br>    a[i] = (s[i] == 'x');<br>  }<br>  if ((c0 &amp; 1) || (c1 &amp; 1)) return 0;<br>  int m = n/2, t, j, z[55];<br>  long long ans = 0, f[55][55];<br>  for (t=0;t&lt;(1&lt;&lt;m);t++) {<br>    j = 0;<br>    for (i=0;i&lt;m;i++) {<br>      if (t &amp; (1 &lt;&lt; i)) z[i] = 1;<br>      else z[i] = 0;<br>      j += z[i];<br>    }<br>    if (j != c1/2) continue;<br>    for (i=0;i&lt;=m;i++)<br>      for (j=0;j&lt;=m;j++) f[i][j] = 0;<br>    f[0][0] = 1;<br>    for (i=0;i&lt;=m;i++)<br>      for (j=0;j&lt;=m;j++) {<br>        if (i &lt; m &amp;&amp; a[i+j] == z[i]) f[i+1][j] += f[i][j];<br>        if (j &lt; m &amp;&amp; a[i+j] == z[j]) f[i][j+1] += f[i][j];<br>      }<br>    ans += f[m][m];<br>  }<br>  return ans;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>