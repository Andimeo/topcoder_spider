            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class RainyRoad<br>{<br>    public string isReachable(string[] road)<br>    {<br>        int i = 0;<br>        int n = road[0].Length;<br>        string res = "YES";<br><br>        for (i = 0; i &lt; n; i++)<br>        {<br>            if (road[0][i] == 'W' &amp;&amp; road[1][i] == 'W')<br>            {<br>                res = "NO";<br>                break;<br>            }<br>        }<br>        return res;<br>    }<br>}<br><br><br>// Powered by FileEdit<br></td>