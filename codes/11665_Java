            /**<br> * Built using CHelper plug-in<br> * Actual solution is at the top<br> * @author Egor Kulikov (egor@egork.net)<br> */<br>public class DropCoins {<br>  public int getMinimum(String[] board, int K) {<br>    int rowCount = board.length;<br>    int columnCount = board[0].length();<br>    int answer = Integer.MAX_VALUE;<br>    for (int i = 0; i &lt; rowCount; i++) {<br>      for (int j = i; j &lt; rowCount; j++) {<br>        for (int k = 0; k &lt; columnCount; k++) {<br>          int current = 0;<br>          for (int l = k; l &lt; columnCount; l++) {<br>            for (int m = i; m &lt;= j; m++) {<br>              if (board[m].charAt(l) == 'o')<br>                current++;<br>            }<br>            if (current == K) {<br>              answer = Math.min(answer, i + rowCount - j - 1 + Math.min(i, rowCount - j - 1) +<br>                k + columnCount - l - 1 + Math.min(k, columnCount - l - 1));<br>            }<br>          }<br>        }<br>      }<br>    }<br>    if (answer == Integer.MAX_VALUE)<br>      answer = -1;<br>    return answer;<br>  }<br><br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by moj 4.15 [modified TZTester]<br>// Powered by CodeProcessor<br></td>