            #include &lt;string&gt;<br><br>using namespace std;<br><br>#define oo 1000000000<br><br>long f[55][55]={0};<br><br>struct MagicNaming<br>{<br> int maxReindeers(string magicName)<br> {<br>  long i,j,k;<br>  long n=magicName.size();<br>  long ans=0;<br>  <br>  for(i=0;i&lt;=n;i++)<br>    for(j=0;j&lt;=n;j++)<br>      f[i][j]=-oo;<br>  for(i=1;i&lt;=n;i++)<br>    for(j=i;j&lt;=n;j++)<br>      {<br>       for(k=i-1;k&gt;=1;k--)<br>         {<br>          if(magicName.substr(k-1,i-k)+magicName.substr(i-1,j-i+1)&lt;=magicName.substr(i-1,j-i+1)+magicName.substr(k-1,i-k))<br>           f[i][j]&gt;?=f[k][i-1]+1;<br>         }<br>       if(i==1)<br>        f[i][j]=1;<br>      }<br>  for(i=1;i&lt;=n;i++)<br>    if(f[i][n]&gt;ans)<br>     ans=f[i][n];<br>  return ans;<br> }<br>};<br></td>