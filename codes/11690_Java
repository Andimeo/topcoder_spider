            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class MagicBlizzard<br>{<br>  public double expectation(int[] range, int[] amount)<br>  {<br>    double[] exp = new double[10001];<br>    double[] var = new double[10001];<br>    for(int i = 0; i &lt; range.length; i++)  {<br>      int squareSee = (range[i] * 2 + 1) * (range[i] * 2 + 1);<br>      for(int j = 0; j &lt;= range[i]; j++)  {<br>        exp[j] += 1.0 * amount[i] / squareSee;<br>        var[j] += 1.0 * amount[i] / squareSee * (squareSee-1) / squareSee;<br>      }<br>    }<br>    double ret = 0;<br>    for(int i = 0; i &lt; var.length; i++)  {<br>      int count = i == 0 ? 1 : 8 * i;<br>      ret += var[i] * count;<br>      ret += exp[i] * exp[i] * count;<br>    }<br>    return ret;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.8 (beta) modified by pivanof!<br></td>