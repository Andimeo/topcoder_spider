            #line 2 "Isoceles.cpp"<br>using namespace std;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;stdio.h&gt;<br>#include &lt;stdlib.h&gt;<br>class Isoceles {<br>public:<br>    <br><br>    int howMany(vector &lt;int&gt; a, vector &lt;int&gt; b) {<br>  int count = 0;<br><br>  int i, j, k;<br>  int N = a.size();<br>  for (i = 0; i &lt; N; i++) {<br>      for (j = 0; j &lt; N; j++)<br>    if (j != i)<br>        for (k = 0; k &lt; N; k++)<br>      if (k != i) {<br>          int dx1 = a[j] - a[i], dy1 = b[j] - b[i];<br>          int dx2 = a[k] - a[i], dy2 = b[k] - b[i];<br><br>          if (dx1 == dy2 &amp;&amp; dx2 == -dy1)<br>        count++;<br>      }<br>  }<br>  return count;<br>    }<br>};<br><br><br><br>// Powered by FileEdit<br></td>