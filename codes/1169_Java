            import java.util.*;<br>public class Isoceles {<br>  int[] x;<br>  int[] y;<br>  <br>  int dst(int a, int b) {<br>    return (x[a]-x[b])*(x[a]-x[b])+(y[a]-y[b])*(y[a]-y[b]);<br>  }<br>  <br>  boolean check(int rght, int s1, int s2) {<br>    int d1=dst(rght, s1);<br>    int d2=dst(rght, s2);<br>    if (d1!=d2) return false;<br>    <br>    int dh = dst(s1,s2);<br>    return dh==d1+d2;<br>  }<br>  <br>  public int howMany(int[] xs, int[] ys) {<br>    x=xs;<br>    y=ys;<br>    int n = xs.length;<br>    int cnt=0;<br>    for (int i=0; i&lt;n; i++) for (int j=i+1; j&lt;n; j++) for (int k=j+1; k&lt;n; k++) {<br>      if (check(i,j,k) || check(j,k,i) || check(k,j,i)) cnt++;  <br>    }<br>    return cnt;<br>    <br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>