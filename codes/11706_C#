            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class MagicCandy<br>{<br>  public int whichOne(int n)<br>  {<br>    return f(n) + 1;<br>  }<br>  <br>  int f(int n)<br>  {<br>    if(n &lt;= 1)<br>      return 0;<br>    int t = (int)Math.Sqrt(n+0.01);<br>    int v = f(n - t);<br>    int z = (int)(Math.Sqrt(v+0.26) + 0.5);<br>    return v + z;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>