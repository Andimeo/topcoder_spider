            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class MagicStonesStore<br>{<br>  public static bool IsPrime(int n)<br>    {<br>  // Test whether the parameter is a prime number.<br>  if ((n &amp; 1) == 0)<br>  {<br>      if (n == 2)<br>      {<br>    return true;<br>      }<br>      else<br>      {<br>    return false;<br>      }<br>  }<br> <br>  for (int i = 3; (i * i) &lt;= n; i += 2)<br>  {<br>      if ((n % i) == 0)<br>      {<br>    return false;<br>      }<br>  }<br>  return n != 1;<br>    }<br> <br>  public string ableToDivide(int n)<br>  {<br>        for (int i = 1; i &lt;= n; i++)<br>        {<br>            if(IsPrime(i) &amp;&amp; IsPrime(2*n -i)) {<br>                return "YES";<br>            }<br>        }<br>        return "NO";<br>  }<br> <br>//<br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>