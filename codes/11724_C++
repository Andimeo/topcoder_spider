            //in the name of ALLAH <br>#include&lt;string&gt;<br>#include&lt;cmath&gt;<br>#include&lt;vector&gt;<br>#include&lt;algorithm&gt;<br>#include&lt;stack&gt;<br>#include&lt;queue&gt;<br>#include&lt;map&gt;<br>#include&lt;cstdio&gt;<br>using namespace std;<br><br>bool isprime(int x)<br>{<br>  if( x==0 || x==1 ) return false;<br>  if(x==2) return true;<br>  if(x%2==0) return false;<br>  for(int i=3;i&lt;=x/2;i+=2)<br>    if(x%i == 0 ) <br>      return false;<br><br>  return true;<br>}<br>class MagicStonesStore<br>{<br>  public :<br>  string ableToDivide(int n)<br>  {<br>    n*=2;<br>    for(int i=1;i&lt;=n;i++)<br>    {<br>      if(isprime(i) &amp;&amp; isprime(n-i))<br>        return "YES";<br>    }<br>    return "NO";<br>  }<br>};<br>//AmzMohammad<br></td>