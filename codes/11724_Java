            import java.util.*;<br> <br>public class MagicStonesStore {<br>    int a[];<br>    void generate(int n)<br>    {<br>        Arrays.fill(a,1);<br>        for(int i=2;i&lt;=n;i++)<br>        {<br>            if(a[i]==0)<br>                continue;<br>            for(int j=i*i;j&lt;n;j+=i)<br>            {<br>                a[i]=0;<br>            }<br>        }<br>    }<br>  public String ableToDivide(int n) {<br>        a=new int[2*n+1];<br>        generate(2*n);<br>        if(n == 1)<br>            return "NO";<br>        for(int i=2;i&lt;2*n;i++)<br>        {<br>            for(int j=2;j&lt;2*n;j++)<br>            {<br>                if(a[i]==1 &amp;&amp; a[j]==1 &amp;&amp; (i+j)==2*n)<br>                {<br>                    return "YES";<br>                }<br>            }<br>        }<br>    return "NO";<br>  }<br>}<br></td>