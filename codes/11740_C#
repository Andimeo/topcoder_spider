            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class KingSort {<br><br>    class St:IComparable<br>    {<br>        public string name;<br>        public string alpha;<br>        int number;<br>        public St(string s)<br>        {<br>            string[] st = s.Split(' ');<br>            name = st[0];<br>            alpha = st[1];<br>            string[] num1 = new string[] { "", "I", "II", "III", "IV", "V", "VI", "VII", "VIII", "IX" };<br>            string[] num10 = new string[] { "", "X", "XX", "XXX", "XL", "L" };<br>            int i, j;<br>            number = 0;<br>            for (i = 0; i &lt; num10.Length; i++)<br>            {<br>                for (j = 0; j &lt; num1.Length; j++)<br>                {<br>                    string makest = num10[i] + num1[j];<br>                    if (makest == st[1])<br>                    {<br>                        number = i * 10 + j;<br>                    }<br>                }<br>            }<br><br>        }<br><br>        public int CompareTo(object obj)<br>        {<br>            St target = (St)obj;<br>            if (target.name != this.name) return String.CompareOrdinal(this.name, target.name);<br>            return number.CompareTo(target.number);<br>        }<br>    }<br><br>    public string[] getSortedList(string[] kings)<br>    {<br>        int len = kings.Length;<br>        St[] s = new St[len];<br>        for (int i = 0; i &lt; len; i++)<br>        {<br>            s[i] = new St(kings[i]);<br>        }<br>        Array.Sort(s);<br>        string[] ret = new string[len];<br>        for (int i = 0; i &lt; len; i++)<br>        {<br>            ret[i] = s[i].name + " " + s[i].alpha;<br>        }<br>        return ret;<br>    }<br><br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>