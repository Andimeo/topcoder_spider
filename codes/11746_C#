            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>        public class MinskyMysteryDiv2<br>        {<br>            public long computeAnswer(long N)<br>            {<br>                if (N == 0 || N == 1) return -1;<br> <br>                int maxi = (int)Math.Ceiling(Math.Sqrt((double)N));<br>                for (int i = 2; i &lt;= maxi; i++)<br>                {<br>                    if (N % i == 0)<br>                    {<br>                        return N / i + i;<br>                    }<br>                }<br> <br>                return N / 1 + 1;<br>            }<br>        }<br>//Powered by KawigiEdit 2.1.8 (beta) modified by pivanof!<br></td>