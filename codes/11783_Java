            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class PikachuEasy<br>{<br>  public String check(String word)<br>  {<br>    while(word.length() &gt; 0) {<br>      if(word.startsWith("pi") || word.startsWith("ka")) {<br>        word = word.substring(2);<br>        continue;<br>      } else if(word.startsWith("chu")) {<br>        word = word.substring(3);<br>        continue;<br>      }<br>      <br>      return "NO";<br>    }<br>    <br>    return "YES";  <br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>