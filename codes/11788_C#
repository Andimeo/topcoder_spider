            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class CasketOfStarEasy {<br><br>    public int recurse(ref List&lt;int&gt; w)<br>    {<br>        if (w.Count == 3)<br>            return w[0] * w[2];<br><br>        int max = 0;<br><br>        for (int i = 1; i &lt; w.Count-1; i++)<br>        {<br>            int var1 = w[i-1]*w[i+1];<br>            int temp = w[i];<br>            w.RemoveAt(i);<br>            var1 += recurse(ref w);<br>            w.Insert(i, temp);<br>            max = Math.Max(max, var1);<br>        }<br>        return max;<br>    }<br><br>    public int maxEnergy(int[] weight)<br>    {<br>        List&lt;int&gt; w = new List&lt;int&gt;(weight);<br>        return recurse(ref w);<br>    }<br><br>  /*<br>  public static void Main() {<br>    int[] weight;<br>    Console.WriteLine(maxEnergy());<br>    Console.ReadLine();<br>  }<br>   /**/<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>