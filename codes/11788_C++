            #include&lt;cstdio&gt;<br>#include&lt;iostream&gt;<br>#include&lt;algorithm&gt;<br>#include&lt;vector&gt;<br>#include&lt;string&gt;<br><br>using namespace std;<br><br>#define rep(i,n) for(int i=0;i&lt;int(n);i++)<br>#define reps(i,n) for(int i=1;i&lt;=int(n);i++)<br><br>class CasketOfStarEasy{<br>  public:<br>  int saiki(vector&lt;int&gt; w){<br>    if(w.size()==0)return 0;<br>    <br>    int maxi=0;<br>    for(int i=1;i&lt;w.size()-1;i++){<br>      vector&lt;int&gt; h=w;<br>      int sum=0;<br>      <br>      h.erase(h.begin()+i);<br>      sum = h[i-1]*h[i] + saiki(h);<br>      <br>      maxi = max(maxi,sum);<br>    }<br>    <br>    return maxi;<br>  }<br>  int maxEnergy(vector&lt;int&gt; w){<br>    return saiki(w);<br>  }<br>};<br></td>