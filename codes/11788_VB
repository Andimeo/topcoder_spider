<td class="problemText" colspan="8" valign="middle" align="left">
            Public Class CasketOfStarEasy<br><br>        Public Function maxEnergy(weight As Integer()) As Integer<br>            Dim i As Integer, res As Long = 0<br>                        <br>            if weight.length &lt;= 2 then return 0<br>            if weight.length = 3 then return weight(0)*weight(2)<br>                        <br>            for i = 1 to weight.length - 2<br>              Dim newweight(weight.length - 2) as integer<br>              Array.Copy(weight,newweight, i)<br>              Array.Copy(weight, i+1, newweight, i,weight.length - i - 1)<br>              res = Math.max(res, maxEnergy(newweight) + weight(i-1) * weight (i+1))<br>            next            <br>            Return res<br>        End Function<br>        <br>        <br>        <br>    End Class<br></td>