            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class EllysCheckers {<br>    public string getWinner(string board) {<br>        int res = 0;<br>        for (int i = 0; i &lt; board.Length; i++)<br>            if (board[i] == 'o') res += (board.Length-i-1);<br>        return (res % 2 == 0) ? "NO" : "YES";<br>    }<br><br>  /*<br>  public static void Main() {<br>    string board;<br>    Console.WriteLine(getWinner());<br>    Console.ReadLine();<br>  }<br>   /**/<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>