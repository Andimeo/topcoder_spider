            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class MergersDivOne<br>{<br>  public double findMaximum(int[] revenues)<br>  {<br>    Array.Sort(revenues);<br>    double ret = revenues[0];<br>    for(int i = 1; i &lt; revenues.Length; i++)<br>    {<br>      ret = (ret + revenues[i]) / 2d;<br>    }<br>    return ret;<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>