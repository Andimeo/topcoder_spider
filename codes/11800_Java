            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class BinaryPolynomialDivTwo<br>{<br>  public int countRoots(int[] a)<br>  {<br>    int numberOfRoots = 0;<br>    if (a[0] == 0)<br>      numberOfRoots++;<br>    int sum = 0;<br>    for (int elem : a)<br>      sum ^= elem;<br>    if (sum == 0)<br>      numberOfRoots++;<br>    return numberOfRoots;<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>