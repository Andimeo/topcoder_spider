            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class RollingDiceDivTwo<br>{<br>  public int minimumFaces(string[] rolls)<br>  {<br>    string[] temp = new string[rolls.Length];<br>    string cur = "";<br>    for(int i = 0; i &lt; rolls[0].Length; i++) {<br>      cur += "0";<br>    }<br>    for(int i = 0; i &lt; rolls.Length; i++) {<br>      char[] tc = rolls[i].ToCharArray();<br>      Array.Sort(tc);<br>      temp[i] = new string(tc);<br>    }<br>    for(int i = 0; i &lt; temp.Length; i++) {<br>      string newcur = "";<br>      for(int j = 0; j &lt; temp[i].Length; j++)<br>        if(temp[i][j] &gt;= cur[j])<br>          newcur += temp[i][j].ToString();<br>        else<br>          newcur += cur[j].ToString();<br>      cur = newcur;<br>    }<br>    int ret = 0;<br>    for(int i = 0; i &lt; cur.Length; i++) {<br>      ret += Int32.Parse(cur[i].ToString());<br>    }<br>    return ret;<br>  }<br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>