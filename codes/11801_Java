            import java.util.*;<br>public class RollingDiceDivTwo<br>  {<br>  public int minimumFaces(String[] rolls)<br>    {<br>    int N=rolls.length;<br>    int M=rolls[0].length();<br>    char A[][]=new char[N][M];<br>    for(int i=0;i&lt;N;i++) <br>      {<br>      for(int j=0;j&lt;M;j++) <br>        {<br>        A[i][j]=rolls[i].charAt(j);<br>        }<br>      Arrays.sort(A[i]);<br>      }<br>    int ans=0;<br>    for(int j=0;j&lt;M;j++) <br>      {<br>      int max=0;<br>      for(int i=0;i&lt;N;i++) <br>        {<br>        if(A[i][j]-'0'&gt;max)<br>          max=A[i][j]-'0';<br>        }<br>      ans+=max;<br>      }<br>    return ans;<br>    }<br>    }<br>// Powered by CodeProcessor<br></td>