            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class EvenRoute {<br>    public string isItPossible(int[] x, int[] y, int wantedParity)<br>    {<br>        bool[] memo = new bool[2];<br>        for (int i = 0; i &lt; x.Length; i++)<br>        {<br>            int sum = ((x[i] + y[i]) % 2 + 2) % 2;<br>            memo[sum] = true;<br>        }<br>        if (memo[wantedParity]) return "CAN";<br>        else return "CANNOT";<br>    }<br><br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>