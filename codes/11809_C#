            using System;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class FoxAndKgram {<br>    public int maxK(int[] len)<br>    {<br>        bool[] taken = null;<br>        int ret = 0;<br>        for (int i = 1; i &lt;= len.Length; ++i) {<br>            int cnt = 0;<br>            taken = new bool[len.Length];<br>            for (int j = 0; j &lt; len.Length; ++j) {<br>                if (taken[j]) continue;<br>                if (len[j] == i)<br>                    ++cnt;<br>                else {<br>                    for (int k = j + 1; k &lt; len.Length; ++k) {<br>                        if (!taken[k] &amp;&amp; len[k] + len[j] == i - 1) {<br>                            taken[k] = true;<br>                            ++cnt;<br>                            break;<br>                        }<br>                    }<br>                }<br>            }<br>            if (cnt &gt;= i)<br>                ret = i;<br>        }<br>        return ret;<br>    }<br><br><br>    <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>