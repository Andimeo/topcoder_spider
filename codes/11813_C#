            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class FoxAndVacation<br>{<br>        public int maxCities(int total, int[] d) {<br>            Array.Sort(d);<br>            int n = d.Length;<br>            int c = 0;<br>            int rtn = 0;<br>            for (int i = 0; i &lt; n; i++) {<br>                c += d[i];<br>                if (c &gt; total) {<br>                    break;<br>                }<br>                rtn++;<br>            }<br>            return rtn;<br>        }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>