            #include &lt;iostream&gt;<br>#include &lt;string.h&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br><br>using namespace std;<br><br>class EllysJuice<br>{<br>    public:<br>    vector &lt;string&gt; getWinners(vector &lt;string&gt; players)<br>    {<br>        if(players.size()==1) return players;<br>        vector&lt;string&gt; b;<br>        b.clear();<br>        int n=players.size();<br>        int s=1;<br>        int num=1;<br>        sort(players.begin(),players.end());<br>        for(int i=1;i&lt;n;i++)<br>        {<br>            if(players[i]!=players[i-1])<br>            {<br>                num++;<br>                if(s&gt;=2)<br>                {<br>                    b.push_back(players[i-1]);<br>                }<br>                s=1;<br>            }<br>            else<br>            {<br>                s++;<br>                num=0;<br>            }<br>        }<br>        if(s&gt;=2)<br>        {<br>            b.push_back(players[n-1]);<br>        }<br>        sort(b.begin(),b.end());<br>        return b;<br>    }<br>};<br></td>