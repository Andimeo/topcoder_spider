            #include &lt;algorithm&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cmath&gt;<br>#include &lt;deque&gt;<br>#include &lt;iostream&gt;<br>#include &lt;map&gt;<br>#include &lt;numeric&gt;<br>#include &lt;queue&gt;<br>#include &lt;set&gt;<br>#include &lt;sstream&gt;<br>#include &lt;string&gt;<br>#include &lt;utility&gt;<br>#include &lt;vector&gt;<br>using namespace std;<br> <br>class CaptureFish {<br>public:<br>  int getParity(string);<br>};<br> <br>int CaptureFish::getParity(string fish) {<br>  int n = fish.size(), cnt = 0;<br>  for (int i = 0; i &lt; n; i++) {<br>    for (int j = i; j &lt; n; j++) {<br>      bool flag;<br>      flag = true;<br>      for (int k = 0; k &lt; n &amp;&amp; flag; k++) {<br>        if ((k &lt; i || j &lt; k) &amp;&amp; fish[k] == 'O') {<br>          flag = false;<br>        } else if (i &lt;= k &amp;&amp; k &lt;= j &amp;&amp; fish[k] == 'X') {<br>          flag = false;<br>        }<br>      }<br>      cnt += flag;<br>      flag = true;<br>      for (int k = 0; k &lt; n &amp;&amp; flag; k++) {<br>        if ((k &lt; i || j &lt; k) &amp;&amp; fish[k] == 'X') {<br>          flag = false;<br>        } else if (i &lt;= k &amp;&amp; k &lt;= j &amp;&amp; fish[k] == 'O') {<br>          flag = false;<br>        }<br>      }<br>      cnt += flag;<br>    }<br>  }<br>  return cnt &amp; 1;<br>}<br> <br> <br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>