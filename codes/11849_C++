            #include &lt;algorithm&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;complex&gt;<br>#include &lt;numeric&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;queue&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br> <br>using namespace std;<br> <br>#define all(a) (a).begin(), (a).end()<br>#define sz(a) int((a).size())<br>#define FOR(i, a, b) for(int i(a), _b(b); i &lt; _b; ++i)<br>#define REP(i, n) FOR(i, 0, n)<br>#define CL(a, v) memset(a, v, sizeof a)<br>#define pb push_back<br>#define X first<br>#define Y second<br> <br>typedef long long ll;<br>typedef vector&lt;int&gt; vi;<br>typedef pair&lt;int, int&gt; pii;<br> <br>const int inf = 1000000000;<br>int d[1&lt;&lt;16];<br> <br>class FoxAndPhotography {<br>public:<br>  int getMinimumSwaps (vector &lt;int&gt; a, vector &lt;int&gt; b) {<br>    int n = sz(a);<br>    d[0] = 0;<br>    FOR(u, 1, 1&lt;&lt;n) {<br>      d[u] = inf;<br>      int k = 0;<br>      REP(i, n) if(u&amp;1&lt;&lt;i) ++k;<br>      --k;<br>      int x = k;<br>      REP(i, n) if(u&amp;1&lt;&lt;i) {<br>        if(b[i] &gt; a[k]) d[u] = min(d[u], d[u-(1&lt;&lt;i)] + x);<br>        --x;<br>      }<br>    }<br>    int ans = d[(1&lt;&lt;n)-1];<br>    return ans==inf ? -1 : ans;<br>  }<br>};<br></td>