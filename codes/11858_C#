            using System;<br>using System.Collections.Generic;<br><br>public class PlatypusPaternity<br>{<br>    int match(string female, string male, string sibling)<br>    {<br>        int res = 0;<br>        for (int i = 0; i &lt; female.Length; ++i)<br>        {<br>            if (sibling[i] == 'Y')<br>            {<br>                if (female[i] == 'Y' &amp;&amp; male[i] == 'Y')<br>                {<br>                    ++res;<br>                }<br>                else<br>                {<br>                    return 0;<br>                }<br>            }<br>        }<br>        if (res &gt; 0) res += 2;<br>        return res;<br>    }<br>    public int maxFamily(string[] female, string[] male, string[] sibling)<br>    {<br>        int res = 0;<br>        for (int i = 0; i &lt; female.Length; ++i)<br>        {<br>            for (int j = 0; j &lt; male.Length; ++j)<br>            {<br>                for (int k = 0; k &lt; sibling.Length; ++k)<br>                {<br>                    res = Math.Max(res, match(female[i], male[j], sibling[k]));<br>                }<br>            }<br>        }<br>        return res;<br>    }<br><br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>