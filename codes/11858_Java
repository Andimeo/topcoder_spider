            public class PlatypusPaternity {<br><br>  public int maxFamily(String[] female, String[] male, String[] sib) {<br>    int ret = 0;<br>    for(int i = 0; i &lt; sib.length; i++){<br>      for(int j = 0; j &lt; female.length; j++){<br>        if(match(sib[i],female[j]))<br>        for(int k = 0; k &lt; male.length; k++){<br>          if(match(sib[i],male[k])){<br>            int count = 0;<br>            for(int l = 0; l &lt; sib[i].length(); l++){<br>              if(sib[i].charAt(l)=='Y')<br>                count++;<br>            }<br>            ret = Math.max(count+2, ret);<br>          }<br>        }<br>      }<br>    }<br>    return ret;<br>  }<br>  public boolean match(String s, String t){<br>    for(int i = 0; i &lt; s.length(); i++){<br>      if(s.charAt(i)=='Y')<br>        if(t.charAt(i)!='Y')<br>          return false;<br>    }<br>    return true;<br>  }<br><br>}<br></td>