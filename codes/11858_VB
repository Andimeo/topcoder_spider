            Imports System.Collections<br>Imports System.Collections.Generic<br>Imports System.Text<br><br><br>'easy<br>Public Class PlatypusPaternity<br>    Public Function StoL(ByVal s As String) As Long<br>        Dim total As Long = 0<br>        For i As Integer = 1 To s.Length<br>            If Mid(s, i, 1) = "Y" Then<br>                total += Math.Pow(2, i - 1)<br>            End If<br>        Next<br>        Return total<br>    End Function<br>    Public Function count(ByVal L As Long) As Long<br>        Dim total As Long = 0<br>        For i As Integer = 1 To 50<br>            If Math.Pow(2, i - 1) And L Then<br>                total += 1<br>            End If<br>        Next<br>        Return total<br>    End Function<br>    Public Function maxFamily(ByVal femaleCompatibility As String(), ByVal maleCompatibility As String(), ByVal siblingGroups As String()) As Integer<br><br>        Dim females(femaleCompatibility.Length - 1) As Long<br>        For i As Integer = 0 To femaleCompatibility.Length - 1<br>            females(i) = StoL(femaleCompatibility(i))<br>        Next<br><br>        Dim males(maleCompatibility.Length - 1) As Long<br>        For i As Integer = 0 To maleCompatibility.Length - 1<br>            males(i) = StoL(maleCompatibility(i))<br>        Next<br>        Dim siblings(siblingGroups.Length - 1) As Long<br>        For i As Integer = 0 To siblingGroups.Length - 1<br>            siblings(i) = StoL(siblingGroups(i))<br>        Next<br>        Dim max_result As Long = 0<br><br>        For Each sibiling As Long In siblings<br>            Dim sibiling_count As Long = count(sibiling)<br>            If max_result &gt;= sibiling_count Then<br>                Continue For<br>            End If<br>            Dim mother_exists As Boolean = False<br>            For Each female As Long In females<br>                If (female And sibiling) = sibiling Then<br>                    mother_exists = True<br>                    Exit For<br>                End If<br>            Next<br>            If Not mother_exists Then Continue For<br>            Dim father_exists As Boolean = False<br>            For Each male As Long In males<br>                If (male And sibiling) = sibiling Then<br>                    father_exists = True<br>                    Exit For<br>                End If<br>            Next<br>            If Not father_exists Then Continue For<br>            max_result = sibiling_count<br>        Next<br><br>        If max_result = 0 Then<br>            Return 0<br>        End If<br>        Return max_result + 2<br><br>    End Function<br><br>End Class<br></td>