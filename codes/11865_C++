            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class AliceBobShuffle {<br>public:<br>  int countWays(vector &lt;int&gt;, vector &lt;int&gt;, vector &lt;int&gt;, vector &lt;int&gt;);<br>};<br>long long dp[55][55][55],x,mod=1000000007;<br>int AliceBobShuffle::countWays(vector &lt;int&gt; v1, vector &lt;int&gt; v2, vector &lt;int&gt; v3, vector &lt;int&gt; v4) {<br>  dp[0][0][0]=1;<br>  int i,j,a,b;<br>  int s1=v1.size(),s2=v2.size(),s3=v3.size(),s4=v4.size();<br>  if(s1+s2!=s3+s4) return 0;<br>  for(i=0; i&lt;=s1;++i)<br>  {<br>    for(j=0;j&lt;=s2;++j)<br>    {<br>      for(a=0;a&lt;=s3;++a)<br>      {<br>        b=i+j-a;<br>          long long &amp;x=dp[i][j][a];<br>          x%=mod;<br>          if(i&lt;s1)<br>          {<br>            if(a&lt;s3&amp;&amp; v1[i]==v3[a]) dp[i+1][j][a+1] +=x;<br>            if(b&lt;s4&amp;&amp; v1[i]==v4[b]) dp[i+1][j][a] +=x;<br>          }<br>          if(j&lt;s2)<br>          {<br>            if(a&lt;s3 &amp;&amp; v2[j]==v3[a]) dp[i][j+1][a+1] +=x;<br>            if(b&lt;s4 &amp;&amp; v2[j]==v4[b]) dp[i][j+1][a] +=x;<br>          }<br>      }<br>    }<br>  }<br>  return int(dp[s1][s2][s3]%mod);<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>