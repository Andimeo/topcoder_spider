            public class PasswordXGuessing{<br><br>public long howMany(String[] guesses) {<br><br>    char[] ini = guesses[0].toCharArray();<br><br>    int n = ini.length;<br>    int result = 0;<br><br>    for (int i = 0; i &lt; n; i++) {<br><br>      for (int j = 0; j &lt;= 9; j++) {<br>        char[] modified = ini.clone();<br>        modified[i] = (char)('0'+j);<br>        if (test(modified, ini)) {<br>          boolean by1 = true;<br><br>          for (int m = 0; m &lt; guesses.length; m++) {<br>            char[] tem = guesses[m].toCharArray();<br>            if (test(tem, modified) == false) {<br>              by1 = false;<br>              break;<br>            }<br>          }<br>          if (by1 == true)<br>            result++;<br>        }<br>      }<br>    }<br>    return result;<br>  }<br><br>  public boolean test(char[] arra, char[] arrb) {<br>    int num = 0;<br>    int n = arra.length;<br>    for (int i = 0; i &lt; n; i++) {<br>      if (arra[i] != arrb[i]) {<br>        num++;<br>      }<br>    }<br>    return num == 1 ? true : false;<br><br>  }<br><br><br>}<br></td>