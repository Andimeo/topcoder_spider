            using System;<br>using System.Collections.Generic;<br><br>public class RandomColoringDiv2<br>{<br>    public int diff(int a, int b)<br>    {<br>        return Math.Abs(a - b);<br>    }<br>    public int getCount(int maxR, int maxG, int maxB, int startR, int startG, int startB, int d1, int d2)<br>    {<br>        int res = 0;<br>        for (int r = 0; r &lt; maxR; ++r)<br>        {<br>            for (int g = 0; g &lt; maxG; ++g)<br>            {<br>                for (int b = 0; b &lt; maxB; ++b)<br>                {<br>                    if (diff(r, startR) &lt;= d2 &amp;&amp; diff(g, startG) &lt;= d2 &amp;&amp; diff(b, startB) &lt;= d2)<br>                    {<br>                        if (diff(r, startR) &gt;= d1 || diff(g, startG) &gt;= d1 || diff(b, startB) &gt;= d1)<br>                        {<br>                            ++res;<br>                        }<br>                    }<br>                }<br>            }<br>        }<br>        return res;<br>    }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>