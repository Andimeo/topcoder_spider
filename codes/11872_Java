            public class RandomColoringDiv2{<br>  public int getCount(int maxR, int maxG, int maxB, int startR, int startG, int startB, int d1, int d2){<br>    int ans=0;<br>    for(int i=0;i&lt;maxR;i++){<br>      for(int j=0;j&lt;maxG;j++){<br>        for(int k=0;k&lt;maxB;k++){<br>          if(Math.abs(startR-i)&lt;=d2 &amp;&amp; Math.abs(startG-j)&lt;=d2 &amp;&amp; Math.abs(startB-k)&lt;=d2)<br>            if(Math.abs(startR-i)&gt;=d1 || Math.abs(startG-j)&gt;=d1 || Math.abs(startB-k)&gt;=d1)<br>              ans+=1;<br>        }<br>      }<br>    }<br>    return ans;<br>  }<br>}<br></td>