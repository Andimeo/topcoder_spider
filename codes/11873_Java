            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class PasswordXGrid<br>{<br>  public int minSum(String[] horizontal, String[] vertical)<br>  {<br>    int n = vertical.length, m = horizontal[0].length();<br>    int[][] dp = new int[n + 1][m + 1];<br>    for (int i = n; i &gt;= 0; --i) {<br>      for (int j = m; j &gt;= 0; --j) {<br>        if (i &lt; n) {<br>          dp[i][j] = Math.max(dp[i][j], dp[i + 1][j] + (vertical[i].charAt(j) - '0'));<br>        }<br>        if (j &lt; m) {<br>          dp[i][j] = Math.max(dp[i][j], dp[i][j + 1] + (horizontal[i].charAt(j) - '0'));<br>        }<br>      }<br>    }<br>    return dp[0][0];<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>