            Public Class PasswordXGrid<br>    Public Function minSum(ByVal horizontal As String(), ByVal vertical As String()) As Integer<br>        Dim n As Integer = vertical.Length<br>        Dim m As Integer = horizontal(0).Length<br>        Dim max_cost(n, m) As Integer<br>        For j As Integer = 1 To m<br>            max_cost(0, j) = max_cost(0, j - 1) + Integer.Parse(horizontal(0)(j - 1).ToString)<br>        Next<br>        For i As Integer = 1 To n<br>            max_cost(i, 0) = max_cost(i - 1, 0) + Integer.Parse(vertical(i - 1)(0).ToString)<br>            For j As Integer = 1 To m<br>                max_cost(i, j) = Math.Max(max_cost(i - 1, j) + Integer.Parse(vertical(i - 1)(j).ToString), max_cost(i, j - 1) + Integer.Parse(horizontal(i)(j - 1).ToString))<br>            Next<br>        Next<br>        Return max_cost(n, m)<br>    End Function<br> <br>End Class<br></td>