            public class AkariDaisukiDiv2 {<br> <br>  public int countTuples(String S) {<br>    if(S.length()&gt;3){<br>      int v=0;<br>      for(int len=1; len&lt;S.length()-3; len++){        <br>        for(int i=1;i&lt;S.length()-1-len; i++){<br>          String sub=S.substring(i, i+len);<br>          if(sub!=null &amp;&amp; sub.length()==len){<br>            int n=-1;<br>            int shift=i+len;<br>            do{<br>              n=S.indexOf(sub, shift+1);<br>              if(n!=-1 &amp;&amp; n+len&lt;=S.length()-1){<br>                v++;            <br>                shift=n;<br>              }else<br>                break;<br>            }while(true);<br>          }          <br>        }<br>      }  <br>      return v;<br>    }    <br>    return 0;<br>  }<br> <br>}<br></td>