            #include &lt;cstdio&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;cstring&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;queue&gt;<br>#include &lt;cmath&gt;<br>#include &lt;vector&gt;<br>#define MP make_pair<br>#define F first<br>#define S second<br>using namespace std ;<br>#define For(i , n) for(int i = 0 ; i &lt; (n) ; ++i)<br>#define SZ(x)  (int)((x).size())<br>typedef long long LLI;<br>const int maxint = -1u&gt;&gt;2;<br>const double eps = 1e-7;<br> <br>bool vis[3200][3200];<br>int dir[4][2] = {{1, 0}, {-1, 0}, {0, 1}, {0, -1}};<br>queue&lt; pair&lt;pair&lt;int, int&gt;, int&gt; &gt; q;<br> <br>class NonXorLife<br>{<br>    public:<br>        int countAliveCells(vector &lt;string&gt; field, int K)<br>        {<br>            while(!q.empty())<br>                q.pop();<br> <br>            int n = field.size();<br>            int m = field[0].size();<br> <br>            int ans = 0;<br>            memset(vis, 0, sizeof(vis));<br>            for(int i=0;i&lt;n;i++)<br>                for(int j=0;j&lt;m;j++)<br>                    if(field[i][j] == 'o')<br>                        q.push( MP( MP(i+1550, j+1550), 0) ), vis[i+1550][j+1550] = 1, ans++;<br> <br>            while(!q.empty())<br>            {<br>                pair&lt;pair&lt;int, int&gt;, int&gt; now = q.front();<br>                q.pop();<br> <br>                for(int i=0;i&lt;4;i++)<br>                {<br>                    int tx = now.F.F + dir[i][0];<br>                    int ty = now.F.S + dir[i][1];<br> <br>                    int tz = now.S + 1;<br> <br> <br>                    if(tz &lt;= K &amp;&amp; !vis[tx][ty])<br>                    {<br>                        vis[tx][ty] = 1;<br>                        ans++;<br> <br>                        q.push( MP(MP(tx, ty), tz) );<br>                    }<br>                }<br>            }<br> <br> <br>            return ans;<br>        }<br> <br>        <br> <br>};<br> <br> <br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>