            #ifdef defines<br> <br>class EllysLamps {<br>public:<br>    int getMin(string lamps) {<br>        int res = 0;<br>        bool freeN = false;<br>        int countY = 0;<br>        forin(c, lamps) {<br>            if(c=='N') {<br>                if(countY) {<br>                    res++;<br>                } else {<br>                    freeN = true;<br>                }<br>                countY = 0;<br>            } else {<br>                if(freeN) {<br>                    res++;<br>                    freeN = false;<br>                } else if(countY==2) {<br>                    res++;<br>                    countY = 0;<br>                } else {<br>                    countY++;<br>                }<br>            }<br>        }<br>        return res;<br>    }<br>};<br> <br>#else<br> <br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;queue&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;list&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;iterator&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;limits&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br>#include &lt;climits&gt;<br> <br>using namespace std;<br> <br>/* BEGIN&gt;nab */<br>template&lt;class T&gt; typename T::value_type valuetype_(const T&amp; a);<br>#define valuetype(x) decltype(valuetype_(x))<br>#define itertype(x) decltype((x).begin())<br>#define junk(x) __junk##x##__<br>#define enumerateit(i,v,it,x) for(int i=0,junk()=0;!(x).empty()&amp;&amp;i==0;i=1) \<br>for(itertype(x) it=(x).begin(); junk()!=1 &amp;&amp; (junk()=0, it!=(x).end()); it++,i++) \<br>for(valuetype(x) v=*it; junk()++==0; )<br>#define enumerate(i,v,x) enumerateit(i,v,junk(1),x)<br>#define forin(v,x) enumerate(junk(2),v,x)<br>/* END&gt;nab */<br> <br>#define defines<br>#include "EllysLamps.cc"<br> <br>#endif<br>//Powered by KawigiEdit 2.1.8 (beta) modified by pivanof!<br></td>