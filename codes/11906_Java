            /**<br> * Built using CHelper plug-in<br> * Actual solution is at the top<br> * @author Jacob Jiang<br> */<br>public class EllysLamps {<br>    public int getMin(String _lamps) {<br>        char[] lamps = _lamps.toCharArray();<br>        int i = 0;<br>        int n = lamps.length;<br>        int answer = 0;<br>        while (i &lt; n) {<br>            if (i &lt; n - 1 &amp;&amp; lamps[i] != lamps[i + 1]) {<br>                answer++;<br>                i += 2;<br>            } else if (i &lt; n - 2 &amp;&amp; lamps[i] == 'Y' &amp;&amp; lamps[i + 1] == 'Y' &amp;&amp; lamps[i + 2] == 'Y') {<br>                answer++;<br>                i += 3;<br>            } else {<br>                i++;<br>            }<br>        }<br>        return answer;<br>    }<br>}<br></td>