            import math,string,itertools,fractions,heapq,collections,re,array,bisect,random<br> <br>class EllysLamps:<br>  def getMin(self, lamps):<br>    c = 0<br>    l = [0 if lamps[i] == "N" else 1 for i in range(len(lamps))]<br>    while l:<br>      if l[:2] == [0,1] or l[:2] == [1,0]:<br>        l.pop(0)<br>        l.pop(0)<br>        c += 1<br>      elif l[:3] == [1,1,1]:<br>        l.pop(0)<br>        l.pop(0)<br>        l.pop(0)<br>        c += 1<br>      else:<br>        l.pop(0)<br>    return c<br> <br> <br>#Powered by KawigiEdit-pf 2.3.0!<br></td>