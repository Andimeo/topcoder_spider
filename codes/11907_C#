            using System;<br>using System.Collections;<br>public class EllysTSP<br>{<br>    public int getMax(string places)<br>    {<br>        int c = 0, v = 0;<br>        for (int i = 0; i &lt; places.Length; ++i)<br>        {<br>            if (places[i] == 'C')<br>                c++;<br>            else<br>                v++;<br>        }<br>        if (c != v)<br>            return 2 * Math.Min(c, v) + 1;<br>        return 2 * c;<br>    }<br>}<br><br><br>// Powered by FileEdit<br></td>