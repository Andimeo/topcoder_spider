            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class EllysXors<br>{<br>  public long getXor( long L, long R )<br>  {<br>    if ( L == R ) return L;<br>    if ( L % 4 != 0 ) return L ^ getXor( L + 1, R );<br>    if ( R - L &lt; 100 )<br>    {<br>      long res = 0;<br>      for ( long i = L; i &lt;= R; ++i ) res ^= i;<br>      return res;<br>    }<br>    L += ( ( R - L ) / 4L ) * 4L;<br>    return getXor( L, R );<br>  }<br><br>}<br><br><br> <br>// Powered by moj 4.16 [modified TZTester]<br>// Powered by CleanSharp 1.0<br>// Powered by CodeProcessor<br></td>