            /**<br> * Built using CHelper plug-in<br> * Actual solution is at the top<br> */<br>public class EllysXors {<br>    public long getXor(long L, long R) {<br>        long res = 0;<br>        if (L == R) return res ^ L;<br>        if (L % 2 != 0) {<br>            res ^= L;<br>            ++L;<br>        }<br>        if (L == R) return res ^ L;<br>        if (R % 2 == 0) {<br>            res ^= R;<br>            --R;<br>        }<br>        if (L == R) return res ^ L;<br>        long cnt = (R - L + 1) / 2;<br>        res ^= cnt % 2;<br>        return res;<br>    }<br>}<br> <br> <br> <br>// Powered by FileEdit<br>// Powered by moj 4.16 [modified TZTester]<br>// Powered by CodeProcessor<br></td>