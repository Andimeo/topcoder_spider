            public class StrangeDictionary {<br> <br> <br>  public double[] getExpectedPositions(String[] words) {<br>    double expected[] = new double[words.length];<br>    for (int i = 0; i &lt; words.length; i++) {<br>      double exp = 0.0;<br>      for (int j = 0; j &lt; words.length; j++) {<br>        if (i == j)<br>          continue;<br>        exp += probBefore(words[j], words[i]);<br>      }<br>      expected[i] = exp;<br>    <br>    }<br>    return expected;<br>  }<br>  <br>  private double probBefore(String w1, String w2) {<br>    int countBefore = 0;<br>    int denom = 0;<br>    for (int i = 0; i &lt; w1.length(); i++) {<br>      if (w1.charAt(i) == w2.charAt(i))<br>        continue;<br>      denom++;<br>      if (w1.charAt(i) &lt; w2.charAt(i)) <br>        countBefore++;<br>    }<br>    double ret = ( (double) countBefore) / ( (double) denom );<br>    return ret;<br>  }<br> <br> <br> <br> <br>}<br></td>