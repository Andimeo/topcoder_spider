            using System;<br>using System.Collections;<br>public class WorkingRabbits<br>{<br>    public double getEfficiency(string[] profit)<br>    {<br>        int total = 0;<br>        for (int i = 0; i &lt; profit.Length; ++i)<br>        {<br>            for (int j = i + 1; j &lt; profit[i].Length; ++j)<br>            {<br>                total += profit[i][j] - '0';<br>            }<br>        }<br><br>        int rabbits = (profit.Length * (profit.Length - 1)) / 2;<br><br>        return total * 1.0 / rabbits;<br><br>    }<br>}<br><br><br>// Powered by FileEdit<br></td>