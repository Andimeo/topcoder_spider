            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class BlurredDartboard<br>{<br>  public int minThrows(int[] p, int a)<br>  {<br>    int n = p.Length;<br>    int s = 0;<br>    int m = 0;<br>    int c = 0;<br>    bool[] b = new bool[n + 1];<br>    for(int i=0; i&lt;n; i++)<br>    {<br>      s += p[i];<br>      m = Math.Max(m, p[i]);<br>      if(p[i] == 0)<br>        c ++;<br>      b[p[i]] = true;<br>    }<br>    s = n * (n + 1) / 2 - s;    <br>    int r1 = int.MaxValue;<br>    if(m &gt; 0)<br>    {<br>      r1 = (a - 1) / m + 1;<br>    }<br>    int r2 = int.MaxValue;<br>    if(s &gt; 0)<br>    {<br>      int v = a / s;<br>      r2 = v * c;<br>      int rv = a % s;<br>      //Console.WriteLine(rv);<br>      for(int i=1; i&lt;=n &amp;&amp; rv &gt; 0; i++)<br>      {<br>        if(!b[i])<br>        {<br>          rv -= i;<br>          //Console.WriteLine(i + " " + rv);<br>          r2 ++;<br>        }<br>      }<br>    }<br>    int r3 = int.MaxValue;<br>    if(s &gt; 0 &amp;&amp; m &gt; 0)<br>    {<br>      int v = a / s;<br>      r3 = v * c;<br>      int rv = a % s;<br>      if(rv &gt; 0)<br>      {<br>        r3 += (rv - 1) / m + 1;<br>      }<br>    }<br>    //Console.WriteLine(r1 + " " + r2);<br>    return Math.Min(Math.Min(r1, r2), r3);<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>