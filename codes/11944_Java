            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class BlurredDartboard<br>{<br>  public int minThrows(int[] a, int p)<br>  {<br>    int n = a.length;<br>    boolean[] col = new boolean[n + 1];<br>    Arrays.fill(col, true);<br>    int zc = 0;<br>    int max = 0;<br>    for (int x: a) {<br>      if (x == 0) {<br>        zc++;<br>      } else {<br>        col[x] = false;<br>        max = Math.max(max, x);<br>      }<br>    }<br>    int[] prefix = new int[zc + 1];<br>    for (int i = 1, j = 0; i &lt;= n; ++i) {<br>      if (col[i]) {<br>        prefix[j + 1] = prefix[j] + i;<br>        j++;<br>      }<br>    }<br>    int l = 0, r = p;<br>    while (l + 1 &lt; r) {<br>      int x = (l + r) / 2;<br>      long count = 1L * max * x;<br>      if (zc &gt; 0) {<br>        count = Math.max(count, 1L * prefix[zc] * (x / zc) + Math.max(prefix[x % zc], 1L * max * (x % zc)));<br>      }<br>      if (count &gt;= p) {<br>        r = x;<br>      } else {<br>        l = x;<br>      }<br>    }<br>    return r;<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>