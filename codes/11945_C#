            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class ElectionFraudDiv1 {<br>    public int MinimumVoters(int[] percentages)<br>    {<br>        for (int ret = 1; ret &lt; 3001; ret++)<br>        {<br>            int max = 0;<br>            int min = 0;<br>            bool flag = true;<br>            int[] mindp = new int[101];<br>            int[] maxdp = new int[101];<br>            for (int i = 0; i &lt; 101; i++)<br>            {<br>                mindp[i] = 99999;<br>                maxdp[i] = -1;<br>            }<br>            for (int i = 0; i &lt;= ret; i++)<br>            {<br>                int per = ((i * 1000) / ret + 5) / 10;<br>                if (per &lt; 0) per = 0;<br>                if (per &gt; 100) per = 100;<br>                if (mindp[per] == 99999) mindp[per] = i;<br>                maxdp[per] = i;<br>            }<br>            foreach (int item in percentages)<br>            {<br>                int mi = mindp[item];<br>                int ma = maxdp[item];<br>                if (mi &gt; ma)<br>                {<br>                    flag = false;<br>                    break;<br>                }<br>                min += mi;<br>                max += ma;<br>            }<br>            if (flag &amp;&amp; max &gt;= ret &amp;&amp; ret &gt;= min) return ret;<br>        }<br>        return -1;<br>    }<br><br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>