            import java.util.*;<br>import java.io.*;<br><br>public class ElectionFraudDiv1 {<br>  <br>  int ceilDiv(int a, int b) {<br>    return a % b == 0 ? a / b : a / b + 1;<br>  }<br>  <br>  static final int MAX = 5000;<br><br>  public int MinimumVoters(int[] p) {<br>    <br>    int n = p.length;<br>    <br>    outer: for (int ans = 1; ans &lt;= MAX; ans++) {<br>      <br>      //System.err.println(ans);<br>      <br>      int sumLow = 0;<br>      int sumHigh = 0;<br>      <br>      for (int x : p) {<br>        int low = 0, high = 0;<br>        if (x &gt; 0 &amp;&amp; x &lt; 100) {<br>          low = ceilDiv((2 * x - 1) * ans, 200);<br>          high = ceilDiv((2 * x + 1) * ans, 200) - 1;<br>        } else if (x == 0) {<br>          low = 0;<br>          high = ceilDiv(ans, 200) - 1;<br>        } else if (x == 100) {<br>          low = ceilDiv(199 * ans, 200);<br>          high = ans;<br>        }<br>        <br>        if (low &gt; high)<br>          continue outer;<br>        sumLow += low;<br>        sumHigh += high;<br>      }<br>      if (sumLow &lt;= ans &amp;&amp; sumHigh &gt;= ans)<br>        return ans;<br>    }<br>    <br>    return -1;<br>  }<br><br>}<br></td>