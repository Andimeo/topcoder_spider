<td class="problemText" colspan="8" valign="middle" align="left">
            class BoardSplitting<br>    {<br>        public int minimumCuts(int desiredLength, int desiredCount, int actualLength)<br>        {<br>            int olen = (desiredLength - desiredLength % actualLength); // dol[\u382]ina ene<br>            int cmp=0,left=0,cuts=0;<br>            while (cmp &lt; desiredCount)<br>            {<br>                int tlen = olen;<br>                while (desiredLength - tlen &gt; 0)<br>                {<br>                    if (tlen + left &gt; desiredLength)<br>                    {<br>                        left = tlen - desiredLength;<br>                        tlen = desiredLength;<br>                        cuts++;<br>                    }<br>                    else<br>                    {<br>                        tlen += left;<br>                        left = actualLength;<br>                     }<br>                }<br>                cmp++;<br>            }<br>            return cuts;<br>        }<br>    }<br></td>