            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class GreatFairyWar<br>{<br>  public int minHP(int[] dps, int[] hp)<br>  {<br>    int tot=0;<br>    for(int i=0;i&lt;dps.Length;i++)<br>    {<br>      int sum=0;<br>      for(int j=i;j&lt;dps.Length;j++)<br>      {<br>        sum+=dps[j];<br>      }<br>      tot+=hp[i]*sum;<br>    }<br>    return tot; <br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>