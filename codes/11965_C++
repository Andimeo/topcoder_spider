; i &lt; n; ++i)<br>            {<br>                for(int j = 0; j &lt; m; ++j)<br>                {<br>                    if(topr[i] &gt;= btmr[j])  continue;<br>                    if(btmr[j] * toph[i] &gt; topr[i] * btmh[j])  g[i][j] = 1;<br>                }<br>            }<br>            int cnt(0);<br>            memset(match, -1, sizeof(match));<br>            for(int i = 0;i &lt; n; ++i)<br>            {<br>                memset(vis, 0, sizeof(vis));<br>                if(find(i))  ++cnt;<br>            }<br>            return cnt;<br>        }<br><br><br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>