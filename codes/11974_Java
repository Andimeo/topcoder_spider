            /**<br> * Built using CHelper plug-in<br> * Actual solution is at the top<br> */<br>public class FlipGame {<br>    public int minOperations(String[] board) {<br>        int height = board.length;<br>        int width = board[0].length();<br>        boolean[][] field = new boolean[height][width];<br>        for (int row = 0; row &lt; height; ++row)<br>            for (int col = 0; col &lt; width; ++col)<br>                field[row][col] = board[row].charAt(col) == '1';<br>        int res = 0;<br>        while (true) {<br>            int at = 0;<br>            for (int row = 0; row &lt; height; ++row) {<br>                for (int i = at; i &lt; width; ++i)<br>                    if (field[row][i]) at = i + 1;<br>                for (int i = 0; i &lt; at; ++i)<br>                    field[row][i] = !field[row][i];<br>            }<br>            if (at == 0) break;<br>            ++res;<br>        }<br>        return res;<br>    }<br>}<br> <br> <br> <br>// Powered by FileEdit<br>// Powered by moj 4.16 [modified TZTester]<br>// Powered by CodeProcessor<br></td>