            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class KingdomAndDucks<br>{<br>    public int minDucks(int[] duckTypes)<br>    {<br>        int kind = 0, max = 0;<br>        int[] count = new int[51];<br>        foreach(int type in duckTypes)<br>        {<br>            count[type]++;<br>        }<br><br>        foreach (int c in count)<br>        {<br>            if (c &gt; 0)<br>            {<br>                kind++;<br>                max = Math.Max(max, c);<br>            }<br>        }<br>        <br>        return max * kind;<br>    }<br><br> <br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] Modified for C# by ysn<br>// Powered by CodeProcessor<br></td>