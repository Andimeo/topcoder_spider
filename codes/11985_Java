            import java.util.Arrays;<br><br>public class KingdomAndDucks {<br><br>  public int minDucks(int[] duckTypes) {<br>    int[] amounts=new int[50];<br>    Arrays.fill(amounts, 0);<br>    for(int i=0; i&lt;duckTypes.length; i++)<br>      amounts[duckTypes[i]-1]++;<br>              <br>    int typesCount=0;<br>    int max=0;<br>    for(int i=0; i&lt;amounts.length; i++){<br>      if(amounts[i]&gt;0) <br>        typesCount++;<br>      if(amounts[i]&gt;max) max=amounts[i];<br>    }<br>    return max*typesCount;<br>  }<br><br>}<br></td>