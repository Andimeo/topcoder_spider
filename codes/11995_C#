            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class CheckerExpansion<br>{<br>    char get(long t,long x,long y)<br>    {<br>        long sum = x + y;<br>        if (sum%2==1)<br>        {<br>            return '.';<br>        }<br>        if (x&lt;y)<br>        {<br>            return '.';<br>        }<br>        long r = sum/2;<br>        if (r&gt;t)<br>        {<br>            return '.';<br>        }<br>        long c = (x - y)/2;<br>        long temp = r | c;<br>        if (temp!=r)<br>        {<br>            return '.';<br>        }<br>        else if (r%2==0)<br>        {<br>            return 'A';<br>        }<br>        else{<br>            return 'B';<br>        }<br>    }<br>  public string[] resultAfter(long t, long x0, long y0, int w, int h)<br>  {<br>      t = t-1;<br>        string[] ret = new string[h];<br>      for (int i = 0; i &lt; h; i++)<br>      {<br>          string temp = "";<br>          for (int j = 0; j &lt; w; j++)<br>          {<br>              temp += get(t, x0 + j, y0 + h - i - 1);<br>          }<br>          ret[i] = temp;<br>      }<br>      return ret;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.8 (beta) modified by pivanof!<br></td>