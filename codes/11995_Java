            /**<br> * Built using CHelper plug-in<br> * Actual solution is at the top<br> */<br>public class CheckerExpansion {<br>    int comb(long n, long k) {<br>        if ((k &amp; (~n)) != 0) return 0; else return 1;<br>    }<br> <br>    public String[] resultAfter(long t, long x0, long y0, int w, int h) {<br>    String[] res = new String[h];<br>        for (int dy = 0; dy &lt; h; ++dy) {<br>            res[dy] = "";<br>            for (int dx = 0; dx &lt; w; ++dx) {<br>                long x = x0 + dx;<br>                long y = y0 + h - 1 - dy;<br>                char ch;<br>                if ((x + y) % 2 != 0)<br>                    ch = '.';<br>                else {<br>                    long n = (x + y) / 2;<br>                    long k = y;<br>                    if (n &lt; 0 || n &gt;= t)<br>                        ch = '.';<br>                    else if (k &lt; 0 || k &gt; n)<br>                        ch = '.';<br>                    else {<br>                        if (comb(n, k) == 0)<br>                            ch = '.';<br>                        else {<br>                            if (n % 2 == 0)<br>                                ch = 'A';<br>                            else<br>                                ch = 'B';<br>                        }<br>                    }<br>                }<br>                res[dy] += ch;<br>            }<br>        }<br>        return res;<br>    }<br> <br> <br>}<br> <br> <br> <br>// Powered by FileEdit<br>// Powered by moj 4.16 [modified TZTester]<br>// Powered by CodeProcessor<br></td>