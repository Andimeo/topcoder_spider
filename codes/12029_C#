<td class="problemText" colspan="8" valign="middle" align="left">
            class ANDEquation<br>    {<br>        public int restoreY(int[] A)<br>        {<br>            int Y, tmp;<br>            for (int i = 0; i &lt; A.Length; i++)<br>            {<br>                Y = A[i];<br>                if (i == 0) tmp = A[1];<br>                else tmp = A[0];<br>                for (int j = 0; j &lt; A.Length; j++)<br>                {<br>                    if (i == j) continue;<br>                    tmp &amp;= A[j];<br>                }<br>                if (tmp == Y) return Y;<br>            }<br>            return -1;<br>        }<br>    }<br></td>