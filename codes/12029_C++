            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;queue&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br> <br>class ANDEquation {<br>public:<br>  int restoreY(vector &lt;int&gt; A) {<br>    <br>    int a;<br>    <br>    for ( int i = 0; i &lt; A.size(); i++ ) {<br>      a = ~0;<br>      for ( int j = 0; j &lt; A.size(); j++ ) {<br>        if ( i != j ) a &amp;= A[j];<br>      }<br>      if ( A[i] == a ) return A[i];<br>    }<br>    <br>    return -1;<br>          <br>          <br>  }<br>};<br> <br> <br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>