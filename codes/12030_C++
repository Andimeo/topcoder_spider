            #include &lt;algorithm&gt;<br>#include &lt;cmath&gt;<br>#include &lt;deque&gt;<br>#include &lt;functional&gt;<br>#include &lt;iostream&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;sstream&gt;<br>#include &lt;string&gt;<br>#include &lt;utility&gt;<br>#include &lt;vector&gt;<br>#include &lt;queue&gt;<br>using namespace std;<br> <br>typedef long long LL;<br>const int MAXN=1000;<br>const LL MOD=(LL)1e9+7LL;<br> <br>int bino[MAXN][MAXN];<br> <br>void init(){ fill(bino[0],bino[0]+MAXN*MAXN,-1); bino[0][0]=bino[1][0]=bino[1][1]=1; }<br> <br>int nch(int n,int k){<br>    if(n&lt;k) return 0;<br>    int &amp;re=bino[n][k];<br>    if(re==-1)<br>        if(!k || !(n-k)) re=1;<br>        else re=(int)(((LL)nch(n-1,k)+(LL)nch(n-1,k-1))%MOD);<br>    return re;<br>}<br> <br>struct SpacetskE {<br>    int countsets(int L,int H,int K) {<br>        init();<br>        int so=0;<br>        if(K==1) return (L+1)*(H+1);<br>        for(int x=0; x&lt;=L; x++){<br>            for(int l=0; l&lt;=L; l++)<br>                for(int h=1; h&lt;=H; h++)<br>                    if(x-l==0){<br>                        so=(so+nch(H+1,K))%MOD;   <br>                        break;<br>                    }else if(__gcd(abs(x-l),h)==1){<br>                        int cx=x,ch=0,cnt=0;<br>                        while(cx&gt;=0 &amp;&amp; cx&lt;=L &amp;&amp; ch&lt;=H){<br>                            cnt++;<br>                            cx+=(l-x);<br>                            ch+=h;<br>                        }<br>                        so=(so+nch(cnt,K))%MOD;   <br>                    }<br>        }<br>        return so;<br>    };<br>};<br> <br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>