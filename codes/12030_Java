            /**<br> * ****************************************************************************<br> * Copyright (c) 2012 by Institute of Computing Technology,<br> * Chinese Academic of Sciences, Beijing, China.<br> * &lt;p/&gt;<br> * Author: Ruijian Wang<br> * File: SpacetskE.java<br> * Date: 2012-06-07<br> * Time: 23:35:44<br> * ****************************************************************************<br> */<br> <br>public class SpacetskE {<br>    int C[][];<br> <br>    int add(int a, int b) {<br>        return (a + b) % 1000000007;<br>    }<br> <br>    int gcd(int a, int b) {<br>        int m;<br>        while (a % b != 0) {<br>            m = a % b;<br>            a = b;<br>            b = m;<br>        }<br>        return b;<br>    }<br> <br>    public int countsets(int L, int H, int K) {<br>        if (K == 1) {<br>            return (L+1)*(H+1);<br>        }<br> <br>        C = new int[202][];<br>        for (int i = 0; i &lt; 202; i++) {<br>            C[i] = new int[202];<br>        }<br> <br>        C[0][0] = 0;<br>        C[1][0] = C[1][1] = 1;<br> <br>        for (int i = 2; i &lt; 202; i++) {<br>            C[i][0] = C[i][i] = 1;<br>            for (int j = 1; j &lt; i; j++) {<br>                C[i][j] = (C[i - 1][j - 1] + C[i - 1][j]) % 1000000007;<br>            }<br>        }<br> <br>        int res = 0;<br> <br>        for (int x = 0; x &lt;= L; x++) {<br>            for (int ey = 1; ey &lt;= H; ey++) {<br>                for (int ex = 0; ex &lt;= L; ex++) {<br>                    int l = Math.abs(x - ex);<br>                    int h = ey;<br>                    int g = gcd(l, h);<br>                    if (g &gt;= K-1) {<br>                        res = add(res, C[g][K-1]);<br>                    }<br>                }<br>            }<br>        }<br> <br>        return res;<br>    }<br> <br> <br>}<br> <br> <br> <br>// Powered by FileEdit<br>// Powered by moj 4.17 [modified TZTester]<br>// Powered by CodeProcessor<br></td>