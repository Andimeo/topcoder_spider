            import java.util.Set;<br>import java.util.HashSet;<br> <br>/**<br> * Built using CHelper plug-in<br> * Actual solution is at the top<br> */<br>public class RotatingBot {<br>    public int minArea(int[] moves) {<br>        Set&lt;Integer&gt; seen = new HashSet&lt;Integer&gt;();<br>        Set&lt;Integer&gt; outside = new HashSet&lt;Integer&gt;();<br>        int BIG = 10000;<br>        int x = BIG / 2;<br>        int y = BIG / 2;<br>        int dx = 1;<br>        int dy = 0;<br>        seen.add(y * BIG + x);<br>        for (int li = 0; li &lt; moves.length; ++li) {<br>            int len = moves[li];<br>            for (int i = 0; i &lt; len; ++i) {<br>                x = x + dx;<br>                y = y + dy;<br>                int cell = y * BIG + x;<br>                if (seen.contains(cell)) return -1;<br>                seen.add(cell);<br>            }<br>            if (li + 1 &lt; moves.length) {<br>                int badx = x + dx;<br>                int bady = y + dy;<br>                int badcell = bady * BIG + badx;<br>                if (!seen.contains(badcell)) outside.add(badcell);<br>            }<br>            int tmp = dy;<br>            dy = dx;<br>            dx = -tmp;<br>        }<br>        int minx = BIG;<br>        int maxx = 0;<br>        int miny = BIG;<br>        int maxy = 0;<br>        for (int cell : seen) {<br>            x = cell % BIG;<br>            y = cell / BIG;<br>            minx = Math.min(minx, x);<br>            miny = Math.min(miny, y);<br>            maxx = Math.max(maxx, x);<br>            maxy = Math.max(maxy, y);<br>        }<br>        for (int cell : outside) {<br>            x = cell % BIG;<br>            y = cell / BIG;<br>            if (x &gt;= minx &amp;&amp; x &lt;= maxx &amp;&amp; y &gt;= miny &amp;&amp; y &lt;= maxy) return -1;<br>        }<br>        return (maxx - minx + 1) * (maxy - miny + 1);<br>    }<br> <br> <br>}<br> <br> <br> <br>// Powered by FileEdit<br>// Powered by moj 4.16 [modified TZTester]<br>// Powered by CodeProcessor<br></td>