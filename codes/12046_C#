<td class="problemText" colspan="8" valign="middle" align="left">
            public class TwoRectangles<br>{<br><br>    public static string describeIntersection(int[] A, int[] B)<br>    {<br>        int ModeV = -1;<br>        int ModeH = -1;<br><br>        if (A[0] == B[2] || A[2] == B[0]) { ModeV = 0; }<br>        if (A[0] &lt; B[2] &amp;&amp; A[2] &gt; B[0]) { ModeV = 1; }<br><br>        if (A[1] == B[3] || A[3] == B[1]) { ModeH = 0; }<br>        if (A[1] &lt; B[3] &amp;&amp; A[3] &gt; B[1]) { ModeH = 1; }<br><br>        if (ModeV == -1 || ModeH == -1) { return "none"; }<br>        if (ModeV == 0 &amp;&amp; ModeH == 0) { return "point"; }<br>        if (ModeV == 0 || ModeH == 0) { return "segment"; }<br>        return "rectangle";<br>    }<br><br>}<br></td>