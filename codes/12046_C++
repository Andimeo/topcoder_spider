            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class TwoRectangles {<br>public:<br>  string describeIntersection(vector &lt;int&gt;, vector &lt;int&gt;);<br>};<br>struct node<br>{<br>  int x1, y1, x2, y2;<br>  node(){}<br>  node(int a,int b,int c,int d)<br>  {<br>    x1=a; y1=b; x2=c; y2=d;<br>  }<br>}a,b;<br>string TwoRectangles::describeIntersection(vector &lt;int&gt; A, vector &lt;int&gt; B) {<br>  a=node(A[0],A[1],A[2],A[3]);<br>  b=node(B[0],B[1],B[2],B[3]);<br>  int x1,y1,x2,y2;<br>  x1=max(a.x1,b.x1);<br>  y1=max(a.y1,b.y1);<br>  x2=min(a.x2,b.x2);<br>  y2=min(a.y2,b.y2);<br>  if(x2&gt;x1 &amp;&amp;y2&gt;y1) return "rectangle";<br>  if(x2&lt;x1 ||y2&lt;y1) return "none";<br>  if(x2&gt;x1 || y2&gt;y1) return "segment";<br>  return "point";<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>