<td class="problemText" colspan="8" valign="middle" align="left">
            public class TwoRectangles{<br>  public String describeIntersection(int[] A,int[] B){<br>    int x1=0,x2=0,y1=0,y2=0;<br>    x1=(A[0]&gt;B[0]?A[0]:B[0]);<br>    y1=(A[1]&gt;B[1]?A[1]:B[1]);<br>    x2=(A[2]&lt;B[2]?A[2]:B[2]);<br>    y2=(A[3]&lt;B[3]?A[3]:B[3]);<br>    if(x1&gt;x2||y1&gt;y2)<br>      return "none";<br>    if(x1!=x2&amp;&amp;y1!=y2)<br>      return "rectangle";<br>    if(x1==x2&amp;&amp;y1==y2)<br>      return "point";<br>    if(x1==x2||y1==y2)<br>      return "segment";<br>    return "none";<br>  }<br>}<br></td>