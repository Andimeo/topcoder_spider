            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class ContestWinner<br>{<br>    public int getWinner(int[] events)<br>    {<br>        int max = 0;<br>        int who = 0;<br>        int[] count = new int[1000001];<br><br>        foreach (int i in events)<br>        {<br>            count[i]++;<br>            if (count[i] &gt; max) { max = count[i]; who = i; }<br>        }<br>        <br>        return who;<br>    }<br><br> <br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] Modified for C# by ysn<br>// Powered by CodeProcessor<br></td>