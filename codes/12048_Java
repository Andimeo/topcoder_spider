            import java.util.*;<br>import java.awt.Point;<br><br>public class ContestWinner {<br><br>  static void pln(Object... obj) {<br>    pf(obj);<br>    System.out.println();<br>  }<br><br>  static void pf(Object... obj) {<br>    for (int i = 0; i &lt; obj.length; i++) {<br>      if (i != 0)<br>        System.out.print(' ');<br>      System.out.print(obj[i]);<br><br>    }<br>  }<br><br>  public static int getWinner(int[] events) {<br>    int winner = -1;<br>    int max = 0;<br>    int cnt[] = new int[1000001];<br>    for (int i = 0; i &lt; events.length; i++) {<br>      cnt[events[i]]++;<br>      if (cnt[events[i]] &gt; max) {<br>        winner = events[i];<br>        max = cnt[events[i]];<br>      }<br>    }<br>    return winner;<br>  }<br><br>}<br></td>