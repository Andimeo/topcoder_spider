            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class KleofasTail<br>{<br>  public long countGoodSequences(long K, long A, long B)<br>  {<br>      if (K==0)<br>      {<br>          return B - A + 1;<br>      }<br>      long st = K;<br>      long ad = 0;<br>      if (st%2==0)<br>      {<br>          ad = 1;<br>      }<br>      long ret = 0;<br>      while (st&lt;=B)<br>      {<br>          long mn1 = Math.Max(A, st);<br>          long mx1 = Math.Min(B, st + ad);<br>          if (mx1&gt;=mn1)<br>          {<br>              ret = ret + mx1 - mn1 + 1;<br>          }<br>          st *= 2;<br>          ad = 2*ad + 1;<br>      }<br>      return ret;<br>    <br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.8 (beta) modified by pivanof!<br></td>