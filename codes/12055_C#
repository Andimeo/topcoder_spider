            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class Pillars<br>{<br>  public double getExpectedLength( int w, int x, int y )<br>  {<br>    double res = 0;<br>    for ( int d = -100000; d &lt;= 100000; ++d )<br>    {<br>      int mina = Math.Max( 1, 1 + d );<br>      int maxa = Math.Min( x, y + d );<br>      if ( mina &lt;= maxa )<br>      {<br>        double dist = Math.Sqrt( w * w + (long)d * d );<br>        res += dist * ( maxa - mina + 1 );<br>      }<br>    }<br>    return res / x / y;<br>  }<br><br>}<br><br><br> <br>// Powered by moj 4.16 [modified TZTester]<br>// Powered by CleanSharp 1.0<br>// Powered by CodeProcessor<br></td>