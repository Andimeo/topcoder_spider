            #define LOCAL<br><br>/** ` Micro Mezzo Macro Flation -- Overheated Economy ., **/<br><br>#include &lt;algorithm&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;sstream&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;bitset&gt;<br>#include &lt;queue&gt;<br>#include &lt;stack&gt;<br>#include &lt;cmath&gt;<br>#include &lt;ctime&gt;<br>#include &lt;list&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br><br>using namespace std;<br><br>#define REP_1_C(i, n) for (int n____=int(n),i=1;i&lt;=n____;++i)<br>typedef double DB;<br><br>/* .................................................................................................................................. */<br><br><br>DB sqr(DB x){<br>    return x * x;<br>}<br><br>DB f(int w, int x, int y){<br><br>    DB q = min(x, y), p = w * q, pp, qq;<br><br>    REP_1_C(i, max(x, y)){<br>        qq = min(x, max(0, y - i)), pp = sqrt(sqr(w) +  sqr(i)) * qq;<br>        p += pp, q += qq;<br>        qq = min(max(0, x - i), y), pp = sqrt(sqr(w) +  sqr(i)) * qq;<br>        p += pp, q += qq;<br><br>        //cout &lt;&lt; p &lt;&lt; " " &lt;&lt; q &lt;&lt; endl;<br>    }<br><br>    return p / q;<br>}<br><br><br>class Pillars {<br>public:<br>  double getExpectedLength(int w, int x, int y) {<br>        return f(w, x, y);<br>  }<br>};<br><br><br><br><br>// Powered by FileEdit<br>// Powered by moj 4.16 [modified TZTester]<br>// Powered by CodeProcessor<br></td>