            public class Pillars {<br>  double[] cache = new double[123456];<br>  public double getExpectedLength(int w, int x, int y) {<br>    int a = Math.min(x, y);<br>    int b = Math.max(x, y);<br>    for (int i = 1; i &lt;= b; i++) {<br>      cache[i] = cache[i-1] + Math.sqrt(1.0*i*i + 1.0*w*w);<br>    }<br>    double rez = 0;<br>    for (int i = 1; i &lt;= a; i++) {<br>      rez += cache[i-1] + w + cache[b - i];<br>    }<br>    rez /= x;<br>    rez /= y;<br>    return rez;<br>  }<br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by moj 4.16 [modified TZTester]<br>// Powered by CodeProcessor<br></td>