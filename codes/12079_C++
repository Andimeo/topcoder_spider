            #include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cctype&gt;<br>#include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;sstream&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;queue&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;utility&gt;<br>#include &lt;functional&gt;<br>using namespace std;<br>int a[99];<br>int n;long long int num[99];<br>bool g[99][99];<br>long long int solve(int down){<br>  for(int i=49; i&gt;=down; i--){<br>    for(int j=0; j&lt;n; j++)<br>      if(num[j]&amp;(1LL&lt;&lt;i))<br>        g[i][j]=true;<br>      else<br>        g[i][j]=false;<br>    g[i][n]=(bool)(a[i]);<br>  }<br>  int res=n;<br>  for(int i=49; i&gt;=down; i--){<br>    //choose main variable<br>    int v=-1;<br>    for(int j=0; j&lt;n; j++)if(g[i][j]){<br>      v=j;<br>      break;<br>    }<br>    if(v==-1){<br>      if(g[i][n])return 0;<br>      continue;<br>    }<br>    res--;<br>    for(int k=i-1; k&gt;=down; k--)<br>      if(g[k][v])<br>        for(int j=0; j&lt;=n; j++)<br>          g[k][j]^=g[i][j];<br>  }<br>  return 1LL&lt;&lt;res;<br>}<br>class XorCards{<br>  public:<br>  long long numberOfWays(vector&lt;long long&gt; number, long long limit){<br>    n=number.size();<br>    for(int i=0; i&lt;n; i++)<br>      num[i]=number[i];<br>    long long int res=0;<br>    for(int i=49; i&gt;=0; i--)<br>      if(limit&amp;(1LL&lt;&lt;i)){<br>        a[i]=0;<br>        res+=solve(i);<br>        a[i]=1;<br>      }else<br>        a[i]=0;<br>    res+=solve(0);<br>    return res;<br>  }<br>  <br> <br>};<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>