            #include &lt;iostream&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;vector&gt;<br>#include &lt;cmath&gt;<br>#include &lt;memory.h&gt;<br>#include &lt;string&gt;<br>#include &lt;set&gt;<br>#include &lt;queue&gt;<br>#include &lt;map&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;sstream&gt;<br>#include &lt;stack&gt;<br>#include &lt;ctime&gt;<br>using namespace std;<br>#define forn(i,n) for(int i=0;i&lt;(n);++i)<br>#define forv(i,v) forn(i,(int)(v).size())<br>#define iinf 1000000000<br>#define linf 1000000000000000000LL<br>#define dinf 1e200<br>#define all(v) (v).begin(),(v).end()<br>#define pb push_back<br>#define mp make_pair<br>#define lng long long<br>#define SQ(a) ((a)*(a))<br>#define PI 3.14159265359<br>#define PII pair&lt;int,int&gt; <br>#define X first<br>#define Y second<br>typedef unsigned int uint;<br><br>class ChromaticNumber {public:int minColors(vector &lt;string&gt; graph) ;};<br><br>vector&lt;string&gt; gr;<br>bool was[100];<br>int n;<br><br>int dfs(int v){<br>  if(was[v])<br>    return 0;<br>  was[v]=true;<br>  int r=1;<br>  forn(p,n){<br>    if(p==v||gr[v][p]=='Y')<br>      continue;<br>    r+=dfs(p);<br>  }<br>  return r;<br>}<br><br>int ChromaticNumber :: minColors(vector &lt;string&gt; graph) {<br>  gr=graph;<br>  n=gr.size();<br>  int res=0;<br>  forn(i,n)<br>    was[i]=false;<br>  forn(i,n){<br>    forn(j,i){<br>      forn(k,j){<br>        if(gr[i][j]=='N'&amp;&amp;gr[i][k]=='N'&amp;&amp;gr[k][j]=='N'){<br>          ++res;<br>          was[i]=was[j]=was[k]=true;<br>        }<br>      }<br>    }<br>  }<br>  forn(i,n){<br>    res+=(dfs(i)+1)/2;<br>  }<br>  return res;<br>}<br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br>#pragma region tc testing code<br>#pragma endregion<br><br><br>// Powered by FileEdit<br>// Powered by moj 4.16 [modified TZTester]<br>// Powered by CodeProcessor<br></td>