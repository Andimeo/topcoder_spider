            import java.util.*;<br>import java.math.*;<br><br>public class XorGame {<br>  public int play(String S, String E) {<br>    long start = Long.parseLong(S, 2);<br>    long finish = Long.parseLong(E, 2);<br>    start ^= finish;<br>    if (start == 0)<br>      return 0;<br>    int length = S.length();<br>    for (int i = 0; i &lt; length; i++) {<br>      for (int j = i + 1; j &lt;= length; j++) {<br>        if (start == (1L &lt;&lt; j) - (1L &lt;&lt; i))<br>          return 1;<br>      }<br>    }<br>    if (length &gt;= 5)<br>      return -1;<br>    if (start == 11 || start == 13)<br>      return 3;<br>    return 2;<br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>