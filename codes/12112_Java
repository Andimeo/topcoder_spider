            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class LittleElephantAndBooks<br>{<br>  public int getNumber(int[] pages, int number)<br>  {<br>    Arrays.sort(pages);<br>    int ans = 0;<br>    for(int x=0; x&lt;number - 1; x++){<br>      ans += pages[x];<br>    }<br>    return ans + pages[number];<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.8 (beta) modified by pivanof!<br></td>