            import java.util.Arrays;<br> <br>//<br>//<br>public class LittleElephantAndDouble {<br>    public String getAnswer(int[] A) {<br>        Arrays.sort(A);<br>        for (int i = 1; i &lt; A.length; ++i) {<br>            while (A[i] &gt; A[i - 1]) {<br>                A[i - 1] &lt;&lt;= 1;<br>            }<br>            if (A[i] != A[i - 1])<br>                return "NO";<br>        }<br>        return "YES";<br>    }<br> <br>}<br> <br> <br> <br>// Powered by FileEdit<br>// Powered by moj 4.15 [modified TZTester]<br>// Powered by CodeProcessor<br></td>