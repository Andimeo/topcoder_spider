<td class="problemText" colspan="8" valign="middle" align="left">
            Public Class EasyConversionMachine<br>   Public Function isItPossible(originalWord As String, finalWord As String, k As Integer) As String<br>        Dim distance as Integer = GetLetterDistance(originalWord, finalWord)<br>        If distance = k Then<br>            Return "POSSIBLE"<br>        Else<br>            If k &gt; distance And (k - distance) Mod 2 = 0 Then<br>                Return "POSSIBLE"<br>            End If<br>            Return "IMPOSSIBLE"<br>        End If<br>    End Function<br><br>    Private Function GetLetterDistance(source As String, final As String) As Integer<br>        Dim s As Char() = source.ToCharArray<br>        Dim f As Char() = final.ToCharArray<br>        Dim moves As Integer = 0<br>        For i As Integer = 0 To s.Length - 1<br>            If s(i) &lt;&gt; f(i) Then<br>                ' No move<br>                moves = moves + 1<br>            End If<br>        Next<br>        Return moves<br>    End Function<br>    <br>    End Class<br></td>