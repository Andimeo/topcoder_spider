            import java.util.*;<br> <br>public class ShipBoxes {<br>    int area(int[] a, int[] b) {<br>  int[] z = new int[3];<br>  z[0] = a[0] + b[0];<br>  z[1] = Math.max(a[1], b[1]);<br>  z[2] = Math.max(a[2], b[2]);<br>  int[] y = new int[3];<br>  y[0] = z[0] * z[1];<br>  y[1] = z[1] * z[2];<br>  y[2] = z[2] * z[0];<br>  Arrays.sort(y);<br>  return 2 * (y[0] * 2 + y[1] + y[2]);<br>    }<br> <br>    int[][] p;<br> <br>    public int bestCost(int[] box1, int[] box2) {<br>        p = new int[6][];<br>  p[0] = new int[]{0, 1, 2};<br>  p[1] = new int[]{0, 2, 1};<br>  p[2] = new int[]{1, 0, 2};<br>  p[3] = new int[]{1, 2, 0};<br>  p[4] = new int[]{2, 0, 1};<br>  p[5] = new int[]{2, 1, 0};<br> <br>  int[] a;<br>  int[] b;<br>  int ans = Integer.MAX_VALUE;<br>  for (int i = 0; i &lt; 6; i++) {<br>      for (int j = 0; j &lt; 6; j++) {<br>    a = new int[3];<br>    b = new int[3];<br>    for (int k = 0; k &lt; 3; k++) {<br>        a[k] = box1[p[i][k]];<br>        b[k] = box2[p[j][k]];<br>    }<br>    int t = area(a, b);<br>    if (t &lt; ans) {<br>        ans = t;<br>    }<br>      }<br>  }<br>  return ans;<br>    }<br>}<br> <br> <br> <br>// Powered by PopsEdit<br></td>