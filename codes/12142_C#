            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class ColorfulWolves<br>{<br>  public int getmin(string[] colormap)<br>  {<br>      int N = colormap.Length;<br>        int[,] dist = new int[N,N];<br>      for (int i = 0; i &lt; N; i++)<br>      {<br>          for (int j = 0; j &lt; N; j++)<br>          {<br>              dist[i, j] = 1000000;<br>          }<br>          List&lt;int&gt; l = new List&lt;int&gt;();<br>          for (int j = 0; j &lt; N; j++)<br>          {<br>              if (colormap[i][j]=='Y')<br>              {<br>                  l.Add(j);<br>              }<br>          }<br>          int[] a = l.ToArray();<br>          for (int j = 0; j &lt; a.Length; j++)<br>          {<br>              dist[i, a[j]] = j;<br>          }<br>      }<br>      for (int i = 0; i &lt; N; i++)<br>      {<br>          for (int j = 0; j &lt; N; j++)<br>          {<br>              for (int k = 0; k &lt; N; k++)<br>              {<br>                  dist[j, k] = Math.Min(dist[j, k], dist[j, i] + dist[i, k]);<br>              }<br>          }<br>      }<br>      if (dist[0,N-1]&gt;3000)<br>      {<br>          return -1;<br>      }<br>      return dist[0, N - 1];<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.8 (beta) modified by pivanof!<br></td>