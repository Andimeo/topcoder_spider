            import java.util.*;<br>import static java.lang.Math.*;<br>import static java.util.Arrays.*;<br> <br>public class ColorfulWolves {<br>  <br>  int INF = 1 &lt;&lt; 29;<br>  <br>  public int getmin(String[] colormap) {<br>    int n = colormap.length;<br>    char[][] cs = new char[n][];<br>    for (int i = 0; i &lt; n; i++) cs[i] = colormap[i].toCharArray();<br>    int[][] g = new int[n][n];<br>    for (int i = 0; i &lt; n; i++) {<br>      int cost = 0;<br>      for (int j = 0; j &lt; n; j++) {<br>        if (cs[i][j] == 'Y') {<br>          g[i][j] = cost;<br>          cost++;<br>        } else {<br>          g[i][j] = INF;<br>        }<br>      }<br>      g[i][i] = 0;<br>    }<br>    for (int k = 0; k &lt; n; k++) for (int i = 0; i &lt; n; i++) for (int j = 0; j &lt; n; j++) g[i][j] = min(g[i][j], g[i][k] + g[k][j]);<br>    if (g[0][n - 1] &gt;= INF) return -1;<br>    return g[0][n - 1];<br>  }<br>  <br>  void debug(Object...os) {<br>    System.err.println(deepToString(os));<br>  }<br>  <br>}<br></td>