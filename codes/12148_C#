            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class FoxAndFlowerShopDivTwo<br>{<br>  public int theMaxFlowers(string[] flowers, int r, int c)<br>  {<br>        int R = flowers.Length, C = flowers[0].Length;<br>        int i, j;<br>        int sum = 0, max = 0;<br>        for (i = 0; i &lt; r; i++)<br>            for (j = 0; j &lt; C; j++)<br>                if (flowers[i][j] == 'F') sum++;<br>        max = Math.Max(max, sum);<br> <br>        sum = 0;<br>        for (i = r+1; i &lt; R; i++)<br>            for (j = 0; j &lt; C; j++)<br>                if (flowers[i][j] == 'F') sum++;<br>        max = Math.Max(max, sum);<br> <br>        sum = 0;<br>        for (i = 0; i &lt; R; i++)<br>            for (j = 0; j &lt; c; j++)<br>                if (flowers[i][j] == 'F') sum++;<br>        max = Math.Max(max, sum);<br> <br>        sum = 0;<br>        for (i = 0; i &lt; R; i++)<br>            for (j = c+1; j &lt; C; j++)<br>                if (flowers[i][j] == 'F') sum++;<br>        max = Math.Max(max, sum);<br> <br>        return max;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>