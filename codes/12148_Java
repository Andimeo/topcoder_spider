            public class FoxAndFlowerShopDivTwo {<br> <br>  public int theMaxFlowers(String[] flowers, int r, int c) {<br>    final int n = flowers.length;<br>    final int m = flowers[0].length();<br>    int res = 0;<br>    int cnt = 0;<br>    for (int i = 0; i &lt; n; ++i){<br>      for (int j = 0; j &lt; c; ++j){<br>        if (flowers[i].charAt(j) == 'F'){<br>          cnt++;<br>        }<br>      }<br>    }<br>    res = Math.max(res, cnt);<br>    cnt = 0;<br>    for (int i = 0; i &lt; n; ++i){<br>      for (int j = c + 1; j &lt; m; ++j){<br>        if (flowers[i].charAt(j) == 'F'){<br>          cnt++;<br>        }<br>      }<br>    }<br>    res = Math.max(res, cnt);<br>    cnt = 0;<br>    for (int i = 0; i &lt; r; ++i){<br>      for (int j = 0; j &lt; m; ++j){<br>        if (flowers[i].charAt(j) == 'F'){<br>          cnt++;<br>        }<br>      }<br>    }<br>    res = Math.max(res, cnt);<br>    cnt = 0;<br>    for (int i = r + 1; i &lt; n; ++i){<br>      for (int j = 0; j &lt; m; ++j){<br>        if (flowers[i].charAt(j) == 'F'){<br>          cnt++;<br>        }<br>      }<br>    }<br>    res = Math.max(res, cnt);<br>    return res;<br>  }<br> <br>}<br></td>