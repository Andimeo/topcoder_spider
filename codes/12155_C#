            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class CuttingBitString {<br>    public int getmin(string S)<br>    {<br>        int MAX = 99999;<br>        long first = 1;<br>        List&lt;string&gt; l = new List&lt;string&gt;();<br>        while (true)<br>        {<br>            string s = bitstring(first);<br>            if (s.Length &gt; 50) break;<br>            l.Add(s); first *= 5;<br>        }<br>        int[] dp = new int[S.Length + 1];<br>        for (int i = 0; i &lt; dp.Length; i++)<br>        {<br>            dp[i] = MAX;<br>        }<br>        dp[0] = 0;<br><br>        for (int i = 0; i &lt; dp.Length; i++)<br>        {<br>            foreach (string item in l)<br>            {<br>                if (i + item.Length &gt; S.Length) continue;<br>                if (item == S.Substring(i, item.Length))<br>                {<br>                    dp[i + item.Length] = Math.Min(dp[i + item.Length], dp[i] + 1);<br>                }<br>            }    <br>        }<br><br>        if (dp[S.Length] == MAX) return -1;<br>        else return dp[S.Length];<br>    }<br><br>    string bitstring(long a)<br>    {<br>        string ret = "";<br>        while (a &gt; 0)<br>        {<br>            if (a % 2 == 0) ret = "0" + ret;<br>            else ret = "1" + ret;<br>            a /= 2;<br>        }<br>        return ret;<br>    }<br><br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>