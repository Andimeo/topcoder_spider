            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class TheBrickTowerEasyDivTwo<br>{<br>  public int find(int redCount, int redHeight, int blueCount, int blueHeight)<br>  {<br>    List&lt;int&gt; hh=new List&lt;int&gt;();<br>    hh.Add(redHeight);<br>    if(redHeight!=blueHeight)<br>    hh.Add(blueHeight);<br>    <br>    int sum=0;int rr=redCount;<br>    int bb=blueCount;<br>    for(int i=0;i&lt;redCount+blueCount;i++)<br>    {<br>      if(i%2==0)<br>      {<br>        if(rr==0)break;<br>        sum+=redHeight;rr--;<br>      }<br>      else <br>      {<br>        if(bb==0)break;<br>        sum+=blueHeight;bb--;<br>      }<br>      if(!hh.Contains(sum))hh.Add(sum);<br>    }<br>    rr=redCount;bb=blueCount; sum=0;<br>    for(int i=0;i&lt;redCount+blueCount;i++)<br>    {<br>      if(i%2==1)<br>      {<br>        if(rr==0)break;<br>        sum+=redHeight;rr--;<br>      }<br>      else <br>      {<br>        if(bb==0)break;<br>        sum+=blueHeight;bb--;<br>      }<br>      if(!hh.Contains(sum))hh.Add(sum);<br>    }<br>    <br>    return hh.Count;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>