            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class TheBrickTowerMediumDivOne {<br>    public int[] find(int[] heights)<br>    {<br>        int n = heights.Length;<br>        bool[] used = new bool[n];<br>        int[] ret = new int[n];<br>        for (int i = 0; i &lt; n; i++)<br>        {<br>            ret[i] = -1;<br>        }<br>        int pre = 100;<br>        int count = 0;<br>        for (int i = 0; i &lt; n; i++)<br>        {<br>            if (heights[i] &lt;= pre)<br>            {<br>                ret[count++] = i;<br>                used[i] = true;<br>                pre = heights[i];<br>            }<br>        }<br>        while (count &lt; n)<br>        {<br>            int min = 9999;<br>            int minnum = -1;<br>            for (int i = 0; i &lt; n; i++)<br>            {<br>                if (used[i]) continue;<br>                if (min &gt; heights[i])<br>                {<br>                    minnum = i;<br>                    min = heights[i];<br>                }<br><br>            }<br>            ret[count++] = minnum;<br>            used[minnum] = true;<br>        }<br>        return ret;<br>    }<br><br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>