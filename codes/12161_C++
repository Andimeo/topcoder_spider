            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;map&gt;<br>#include &lt;list&gt;<br>#include &lt;set&gt;<br>#include &lt;queue&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;stack&gt;<br>#include &lt;deque&gt;<br>#include &lt;cmath&gt;<br>#include &lt;memory.h&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cctype&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;utility&gt;<br>using namespace std;<br><br>#define FOR(i,a,b) for(int (i)=(a);(i)&lt;(b);(i)++)<br>#define PB push_back<br>#define MP make_pair<br>#define ALL(a)  (a).begin(),(a).end()<br>#define PII pair&lt;int,int&gt;<br>#define VI vector&lt;int&gt;<br><br>typedef unsigned long long ull;<br>typedef long long ll;<br><br>const double pi=3.141592653589793;<br>const int INF=2000000000;<br><br>class TheBrickTowerMediumDivOne {<br>public:<br>vector &lt;int&gt; find(vector &lt;int&gt; h)<br>{<br>  vector&lt;int&gt; res;<br>  vector&lt;bool&gt; used(h.size(),0);<br>  int n = h.size();<br>  int d = INF;<br>  FOR(i,0,n){<br>    if (h[i] &lt;= d){<br>      d = h[i];<br>      res.PB(i);<br>      used[i] = 1;<br>    }<br>  }<br>  vector&lt;pair&lt;int,int&gt; &gt; b;<br>  FOR(i,0,n)<br>    if (!used[i])<br>      b.PB(MP(h[i],i));<br>  sort(ALL(b));<br>  FOR(i,0,b.size())<br>    res.PB(b[i].second);<br>  return res;<br>}<br><br><br>};<br> <br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>