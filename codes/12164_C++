            #include &lt;cstdio&gt;<br>#include &lt;iostream&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cmath&gt;<br>#include &lt;set&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;map&gt;<br>#include &lt;bitset&gt;<br>#include &lt;vector&gt;<br>#include &lt;ctime&gt;<br><br>#define maxlongint 2147483647<br>#define bigp 1234567891<br><br>using namespace std;<br><br>class TheBrickTowerHardDivTwo {<br>public:<br>   int find( int C, int K, int H ) {<br>       int f[50][5][5][5][5][8],ans=0,q;<br>       memset(f,0,sizeof(f));<br>       f[0][0][0][0][0][0]=1;<br>       for (int i=0;i&lt;H;i++)<br>       {<br>           for (int i1=0;i1&lt;=C;i1++)<br>               for (int i2=0;i2&lt;=C;i2++)<br>                   for (int i3=0;i3&lt;=C;i3++)<br>                       for (int i4=0;i4&lt;=C;i4++)<br>                           for (int i5=0;i5&lt;=K;i5++)<br>                               if (f[i][i1][i2][i3][i4][i5]!=0)<br>                               {<br>                                   for (int j1=1;j1&lt;=C;j1++)<br>                                       for (int j2=1;j2&lt;=C;j2++)<br>                                           for (int j3=1;j3&lt;=C;j3++)<br>                                               for (int j4=1;j4&lt;=C;j4++)<br>                                               {<br>                                                   q=i5+(j1==i1)+(j2==i2)+(j3==i3)+(j4==i4)+(j1==j2)+(j1==j3)+(j4==j2)+(j4==j3);<br>                                                   if (q&lt;=K)<br>                                                   {<br>                                                       f[i+1][j1][j2][j3][j4][q]=<br>                                                       ((long long)(f[i][i1][i2][i3][i4][i5])+(f[i+1][j1][j2][j3][j4][q]))%bigp;<br>                                                   }<br>                                               }<br>                               }<br>       }<br>       for (int i=1;i&lt;=H;i++)<br>           for (int i1=1;i1&lt;=C;i1++)<br>               for (int i2=1;i2&lt;=C;i2++)<br>                   for (int i3=1;i3&lt;=C;i3++)<br>                       for (int i4=1;i4&lt;=C;i4++)<br>                           for (int i5=0;i5&lt;=K;i5++)<br>                               ans=((long long)ans+f[i][i1][i2][i3][i4][i5])%bigp;<br>        return ans;<br>   }<br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by moj 4.17 [modified TZTester]<br>// Powered by CodeProcessor<br></td>