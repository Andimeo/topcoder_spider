            import java.util.*;<br>import java.math.*;<br><br>public class FloodFill3D {<br>  public long countBlack(String[] SArray, String[] TArray, String[] UArray) {<br>    char[] s = unite(SArray);<br>    char[] t = unite(TArray);<br>    char[] u = unite(UArray);<br>    long answer = (long)s.length * t.length * u.length;<br>    char[][] all = {s, t, u};<br>    for (int k = 0; k &lt; 3; k++) {<br>      s = all[k];<br>      long start = s.length;<br>      for (int i = 0; i &lt; s.length; i++) {<br>        if (s[i] != s[0]) {<br>          start = i;<br>          break;<br>        }<br>      }<br>      long end = 0;<br>      if (start != s.length) {<br>        for (int i = s.length - 1; i &gt;= 0; i--) {<br>          if (s[i] != s[s.length - 1]) {<br>            end = s.length - i - 1;<br>            break;<br>          }<br>        }<br>      }<br>      System.out.println(start + " " + end);<br>      long[] count = new long[3];<br>      count[k] = start;<br>      for (int i = 0; i &lt; 3; i++) {<br>        if (i != k) {<br>          for (int j = 0; j &lt; all[i].length; j++) {<br>            if (s[0] == all[i][j])<br>              count[i]++;<br>          }<br>        }<br>      }<br>      answer -= count[0] * count[1] * count[2];<br>      System.out.println(count[0] + " " + count[1] + " " + count[2]);<br>      Arrays.fill(count, 0);<br>      count[k] = end;<br>      for (int i = 0; i &lt; 3; i++) {<br>        if (i != k) {<br>          for (int j = 0; j &lt; all[i].length; j++) {<br>            if (s[s.length - 1] == all[i][j])<br>              count[i]++;<br>          }<br>        }<br>      }<br>      answer -= count[0] * count[1] * count[2];<br>      all[k] = new char[(int)(s.length - start - end)];<br>      System.arraycopy(s, (int)start, all[k], 0, all[k].length);<br>    }<br>    return answer;<br>  }<br>  <br>  char[] unite(String[] array) {<br>    StringBuilder builder = new StringBuilder();<br>    for (String s : array)<br>      builder.append(s);<br>    return builder.toString().toCharArray();<br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>