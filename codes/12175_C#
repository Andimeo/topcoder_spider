            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class XorTravelingSalesman<br>{<br>  public int maxProfit( int[] cityValues, string[] roads )<br>  {<br>    int n = cityValues.Length;<br>    bool[,] was = new bool[n, 1024];<br>    was[0, cityValues[0]] = true;<br>    Queue&lt;int&gt; q = new Queue&lt;int&gt;();<br>    q.Enqueue( 0 );<br>    q.Enqueue( cityValues[0] );<br>    int res = 0;<br>    while ( q.Count &gt; 0 )<br>    {<br>      int u = q.Dequeue();<br>      int mask = q.Dequeue();<br>      res = Math.Max( res, mask );<br>      for ( int i = 0; i &lt; n; ++i )<br>        if ( roads[u][i] == 'Y' )<br>        {<br>          int nextMask = mask ^ cityValues[i];<br>          if ( !was[i, nextMask] )<br>          {<br>            was[i, nextMask] = true;<br>            q.Enqueue( i );<br>            q.Enqueue( nextMask );<br>          }<br>        }<br>    }<br>    return res;<br>  }<br><br>}<br><br><br> <br>// Powered by moj 4.16 [modified TZTester]<br>// Powered by CleanSharp 1.0<br>// Powered by CodeProcessor<br></td>