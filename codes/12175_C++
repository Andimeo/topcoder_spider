            #include &lt;cstdio&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;queue&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;cstring&gt;<br>#include &lt;algorithm&gt;<br>using namespace std;<br><br>typedef pair&lt;int,int&gt; PII;<br>typedef long long LL;<br>const int INF = 1000000007;<br><br>class XorTravelingSalesman{<br>public:<br>    int dp[50][1024];<br>    int maxProfit(vector&lt;int&gt; a, vector&lt;string&gt; e){<br>        int n=a.size();<br>        memset(dp,0,sizeof(dp));<br>        dp[0][a[0]]=1;<br>        queue&lt;PII&gt; q;<br>        q.push(PII(0,a[0]));<br>        int ans=0;<br>        while(q.size()){<br>            int x=q.front().first;<br>            int t=q.front().second;<br>            ans=max(ans,t);<br>            q.pop();<br>            for(int y=0;y&lt;n;y++){<br>                if(e[x][y]=='N') continue;<br>                int r=t^a[y];<br>                if(!dp[y][r]){<br>                    dp[y][r]=1;<br>                    q.push(PII(y,r));<br>                }<br>            }<br>        }<br>        return ans;<br>    }<br>};<br></td>