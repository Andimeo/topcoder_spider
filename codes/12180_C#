            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class XorBoard {<br><br>    long mod = 555555555;<br>    int[,] C;<br>    public int count(int H, int W, int Rcount, int Ccount, int S)<br>    {<br>        C = new int[3200, 3201];<br>        for (int i = 0; i &lt; 3200; i++)<br>        {<br>            C[i, 0] = 1;<br>            for (int j = 1; j &lt;= i; j++)<br>            {<br>                C[i, j] = (int)((C[i - 1, j - 1] + C[i - 1, j]) % mod);<br>            }<br>        }<br>        long ret = 0;<br>        for (int i = 0; i &lt;= Math.Min(H, Rcount); i++)<br>        {<br>            if ((Rcount - i) % 2 == 1) continue;<br>            int Rnokori = (Rcount - i) / 2;<br>            for (int j = 0; j &lt;= Math.Min(W, Ccount); j++)<br>            {<br>                if ((Ccount - j) % 2 == 1) continue;<br>                int Cnokori = (Ccount - j) / 2;<br>                int tempS = i * W + j * H - 2 * i * j;<br>                if (tempS != S) continue;<br>                long tempret = (long)C[H, i] * (long)C[W, j];<br>                tempret %= mod;<br>                tempret *= C[H + Rnokori - 1, H - 1];<br>                tempret %= mod;<br>                tempret *= C[W + Cnokori - 1, W - 1];<br>                ret += tempret;<br>                ret %= mod;<br>            }<br>        }<br><br>        return (int)ret;<br>    }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>