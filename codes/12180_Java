            import java.util.*;<br>import static java.lang.Math.*;<br>import static java.util.Arrays.*;<br> <br>public class XorBoard {<br>  <br>  int M = 555555555;<br>  <br>  public int count(int H, int W, int Rcount, int Ccount, int S) {<br>    int[][] comb = new int[2340 + 1][2340 + 1];<br>    comb[0][0] = 1;<br>    for (int i = 1; i &lt;= 2340; i++) {<br>      comb[i][0] = 1;<br>      for (int j = 1; j &lt;= 2340; j++) {<br>        comb[i][j] = (comb[i - 1][j - 1] + comb[i - 1][j]) % M;<br>      }<br>    }<br>    long res = 0;<br>    for (int h = 0; h &lt;= H; h++) if (h &lt;= Rcount &amp;&amp; (Rcount - h) % 2 == 0) {<br>      for (int w = 0; w &lt;= W; w++) if (w &lt;= Ccount &amp;&amp; (Ccount - w) % 2 == 0) {<br>        if (h * W + w * H - h * w * 2 == S) {<br>          res += (long)comb[H][h] * comb[W][w] % M * comb[(Rcount - h) / 2 + H - 1][H - 1] % M * comb[(Ccount - w) / 2 + W - 1][W - 1] % M;<br>          res %= M;<br>        }<br>      }<br>    }<br>    debug(res);<br>    return (int)res;<br>  }<br>  <br>  void debug(Object...os) {<br>    System.err.println(deepToString(os));<br>  }<br>  <br>}<br></td>