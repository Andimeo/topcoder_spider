            import java.util.*;<br><br>public class TheAnimalProgrammingCompetitions {<br>  final static long MOD = 1234567891;<br>  <br>    public int find(int[] rabbits) {<br>      long[][] C = new long[201][201];<br>      C[0][0] = 1;<br>      for (int i = 1; i &lt; C.length; i++) {<br>        C[i][0] = 1;<br>        for (int j = 1; j &lt; C[i].length; j++) {<br>          C[i][j] = (C[i - 1][j] + C[i - 1][j - 1]) % MOD;<br>        } <br>      }<br>      <br>      int n = rabbits.length;<br>      <br>      long[][][] dp = new long[n + 1][201][n + 1];<br>      for (int k = 0; k &lt;= n; k++) {<br>        dp[0][0][k] = 1;<br>      }<br>      int[] sum = new int[n + 1];<br>      for (int i = 1; i &lt;= n; i++) {<br>        sum[i] = sum[i - 1] + rabbits[i - 1];<br>      }<br>      for (int i = 1; i &lt;= n; i++) {<br>        for (int j = 0; j &lt;= sum[i]; j++) {<br>          for (int k = i; k &lt;= n; k++) {<br>            for (int t = 0; t &lt;= 4; t++) {<br>              for (int z = 0; z &lt;= rabbits[i - 1]; z++) {<br>                int jj = j - t - z;<br>                if (jj &gt;= 0 &amp;&amp; jj &lt;= sum[i - 1]) {<br>                  long u = dp[i - 1][jj][k - 1];<br>                  int from = sum[i - 1] - jj;<br>                  if (from &gt;= t) {<br>                    long v = 1;<br>                    for (int tt = 0; tt &lt; t; tt++) {<br>                      v = (v * (4 - tt)) % MOD;<br>                    }<br>                    v = (v * C[from][t]) % MOD;<br>                    v = (v * C[rabbits[i - 1]][z]) % MOD;<br>                    for (int tt = 0; tt &lt; z; tt++) {<br>                      v = (v * (4 * (k - 1)  - jj - tt)) % MOD;<br>                    }<br>                    <br>                    //System.out.println(i + " " + j + " " + k + " jj " + jj + " " + u + " " + v);<br>                    dp[i][j][k] = (dp[i][j][k] + v * u) % MOD;<br>                  }<br>                }<br>              }<br>            }<br>            //System.out.println(i + " " + j + " " + k + " " + dp[i][j][k]);<br>          }<br>        }<br>      }<br>      <br>      int ans = (int) dp[n][sum[n]][n];<br>      return ans;<br>    }<br>}<br><br><br>// Powered by FileEdit<br></td>