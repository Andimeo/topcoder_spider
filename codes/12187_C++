            #include &lt;algorithm&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;queue&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cmath&gt;<br>using namespace std;<br> <br>class CatAndRabbit<br>{ <br>    public: <br>    string getWinner(string tiles) <br>    { <br>        int lc = 0;<br>        int n = tiles.size();<br>        int res = 0;<br>        bool flag = false;<br>        for(int i = 0; i&lt;n; i++) {<br>            if(tiles[i] == '.'){lc++; continue;}<br>            flag = true;<br>            res^=lc;<br>            lc = 0;<br>        }<br>        if(!flag) return "Rabbit";<br>        res^=lc;<br>        return (res == 0)?"Rabbit":"Cat";<br>    } <br>    <br> <br>}; <br> <br> <br> <br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>