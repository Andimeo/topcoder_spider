<td class="problemText" colspan="8" valign="middle" align="left">
            public class CatAndRabbit {<br>  public String getWinner(String tiles) {<br>    int nimSum = 0;<br>    boolean correct = false;<br>    int whiteLength = 0;<br>    for (int i = 0; i &lt; tiles.length(); i++) {<br>      if (tiles.charAt(i) == '.') ++whiteLength;<br>      else {<br>        correct = true;<br>        nimSum ^= whiteLength;<br>        whiteLength = 0;<br>      }<br>    }<br>    if (!correct) return "Rabbit";<br>    return nimSum == whiteLength ? "Rabbit" : "Cat";<br>  }<br>}<br></td>