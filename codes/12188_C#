            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class XorBoardDivTwo<br>{<br>  public int theMax(string[] board)<br>  {<br>      int ans = 0;<br>    for (int i = 0; i &lt; board.Length; ++i)<br>    {<br>        for (int j = 0; j&lt; board[0].Length; ++j)<br>        {<br>            char[][] tmp = new char[board.Length][];<br>                for (int x = 0; x &lt; board.Length; ++x)<br>                {<br>                    tmp[x] = new char[board[0].Length];<br>                    for (int y = 0; y &lt; board[0].Length; ++y)<br>                    {<br>                        tmp[x][y] = board[x][y];<br>                    }<br>                }<br>                       <br>                for (int i1 = 0; i1 &lt; board.Length; ++i1)<br>                {<br>                    if (tmp[i1][j] == '0') tmp[i1][j] = '1'; else tmp[i1][j] = '0';<br>                }<br>                for (int i1 = 0; i1 &lt; board[0].Length; ++i1)<br>                {<br>                    if (tmp[i][i1] == '0') tmp[i][i1] = '1'; else tmp[i][i1] = '0';<br>                }<br> <br>            int s = 0;<br>                for (int x = 0; x &lt; board.Length; ++x)<br>                {<br>                    for (int y = 0; y &lt; board[0].Length; ++y)<br>                    {<br>                        if (tmp[x][y] == '1' ) s++;                        <br>                    }<br>                }<br>                if (s &gt; ans) ans = s;<br> <br>        }<br>    }<br>      return ans;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>