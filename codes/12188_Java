            /**<br> * Built using CHelper plug-in<br> * Actual solution is at the top<br> * @author Lokesh Khandelwal aka (codeKNIGHT | phantom11)<br> */<br>public class XorBoardDivTwo {<br>  public int theMax(String[] board) {<br>        int i,j,max=0;<br>        for(i=0;i&lt;board.length;i++)<br>        {<br>            for(j=0;j&lt;board[0].length();j++)<br>            {<br>                int a[][]=new int[board.length][board[0].length()];<br>                for(int k=0;k&lt;board.length;k++)<br>                {<br>                    for(int l=0;l&lt;board[0].length();l++)<br>                    {<br>                        if(board[k].charAt(l)=='0')<br>                            a[k][l]=0;<br>                        else a[k][l]=1;<br>                    }<br>                }<br>                for(int k=0;k&lt;board[i].length();k++)<br>                {<br>                    if(a[i][k]==0)<br>                        a[i][k]=1;<br>                    else a[i][k]=0;<br>                }<br>                for(int k=0;k&lt;board.length;k++)<br>                {<br>                    if(a[k][j]==0)<br>                        a[k][j]=1;<br>                    else a[k][j]=0;<br>                }<br>                int c=0;<br>                for(int k=0;k&lt;board.length;k++)<br>                {<br>                    for(int l=0;l&lt;board[0].length();l++)<br>                    {<br>                        if(a[k][l]==1)<br>                            c++;<br>                    }<br>                }<br>                if(c&gt;max)<br>                    max=c;<br>            }<br>        }<br>    return max;<br>  }<br>  }<br><br><br><br>// Powered by FileEdit<br>// Powered by moj 4.16 [modified TZTester]<br>// Powered by CodeProcessor<br></td>