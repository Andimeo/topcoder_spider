            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;sstream&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;queue&gt;<br>#include &lt;map&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;iostream&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstring&gt;<br>#include &lt;functional&gt;<br>#include &lt;utility&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br>#include &lt;string&gt;<br>using namespace std;<br><br>#define CLEAR(a) memset((a),0,sizeof(a))<br>#define CLEARF(a) memset((a),-1,sizeof(a))<br>//const int inf = 1000000000;<br>//const int mod = 1000000007;<br>//const int maxn =;<br>typedef long long LL;<br>int vis[30];<br>class  ChocolateBar{<br>  public :<br>  int maxLength(string letters){<br>    int i, j, k;<br>    int n = letters.length();<br>    int ans = 0;<br>    for (i = 0; i &lt; n; i++)<br>      for (j = i; j &lt; n; j++){<br>        CLEAR(vis);<br>        for (k = i; k &lt;= j; k++){<br>          vis[letters[k] - 'a']++;  <br>        }<br>        bool flag = 1;<br>        for (k = 0; k &lt; 26; k++)<br>          if(vis[k] &gt; 1) {flag = 0;break;}<br>        if (flag) ans = max(ans, j - i + 1);<br>      }<br>    return ans;<br>  }<br>};<br></td>