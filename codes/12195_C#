            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class FoxAndMountainEasy<br>{<br>  public string possible(int n, int h0, int hn, string history)<br>  {<br>    if ((n + hn - h0) % 2 == 1)<br>      return "NO";<br>    int u = (n + hn - h0) / 2, d = (n - hn + h0) / 2;<br>    int ui = 0, di = 0;<br>    for (int i = 0; i &lt; history.Length; ++i) {<br>      if (history[i] == 'U') ++ui;<br>      else ++di;<br>    }<br>    if (ui &gt; u || di &gt; d) return "NO";<br>    int h = h0 + u - ui;<br>    for (int i = 0; i &lt; history.Length; ++i) {<br>      if (history[i] == 'U') ++h;<br>      else --h;<br>      if (h &lt; 0) return "NO";<br>    }<br>    return "YES";<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>