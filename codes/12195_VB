            Imports Microsoft.VisualBasic<br>Imports System<br>Imports System.Collections<br>Imports System.Text<br>Imports System.Collections.Generic<br>Imports System.Math<br> <br>Public Class FoxAndMountainEasy<br>    Public Function possible(ByVal n As Integer, ByVal h0 As Integer, ByVal hn As Integer, ByVal history As String) As String<br>        Dim i As Integer, j As Integer<br>        Dim U As Integer, D As Integer<br>        If h0 &lt; hn Then<br>            U = hn - h0 + ((n - hn + h0) \ 2)<br>            D = ((n - hn + h0) \ 2)<br>        Else<br>            U = (n - h0 + hn) \ 2<br>            D = h0 - hn + (n - h0 + hn) \ 2<br>        End If<br>        If U + D = n AndAlso h0 + U - D = hn Then<br>            ' [\u24038][\u12363][\u12425][\u35519][\u12409][\u12427]<br>            Dim hist_min As Integer = 0<br>            Dim now As Integer = 0<br>            Dim found As Boolean = False<br>            For i = 0 To history.Length - 1<br>                If history(i) = "U"c Then<br>                    now += 1<br>                    U -= 1<br>                Else<br>                    now -= 1<br>                    D -= 1<br>                End If<br>                hist_min = Min(hist_min, now)<br>            Next<br>            If U &lt; 0 OrElse D &lt; 0 Then<br>                Return "NO"<br>            End If<br>            For h As Integer = -hist_min To 300000<br>                Dim tu As Integer = 0, td As Integer = 0<br>                If h &gt; h0 Then<br>                    tu += Abs(h - h0)<br>                Else<br>                    td += Abs(h0 - h)<br>                End If<br>                If h + now &gt; hn Then<br>                    td += Abs(h + now - hn)<br>                Else<br>                    tu += Abs(hn - (h + now))<br>                End If<br>                If tu &lt;= U AndAlso td &lt;= D Then<br>                    found = True<br>                End If<br>            Next<br>            If found Then<br>                Return "YES"<br>            Else<br> <br>                Return "NO"<br>            End If<br>        Else<br>            Return "NO"<br>        End If<br>    End Function<br>End Class<br></td>