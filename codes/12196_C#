            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>using System.Text.RegularExpressions;<br><br>public class IncubatorEasy<br>{<br>    public int maxMagicalGirls(string[] love)<br>    {<br>        int n = love.Length;<br>        int m = 1 &lt;&lt; n;<br>        int ans = 0;<br>        for (int j = 0; j &lt; m; j++)<br>        {<br>            int x = j;<br>            int y = 0;<br>            for (int a = 0; a &lt; n + 1; a++)<br>            {<br>                if (a != 0)<br>                {<br>                    x = y;<br>                }<br>                for (int i = 0; i &lt; n; i++)<br>                {<br>                    if (((1 &lt;&lt; i) &amp; x) != 0)<br>                    {<br>                        for (int k = 0; k &lt; n; k++)<br>                        {<br>                            if (love[i][k] == 'Y')<br>                            {<br>                                y = y | (1 &lt;&lt; k);<br>                            }<br>                        }<br>                    }<br>                }<br>            }<br><br>            int c = 0;<br>            for (int i = 0; i &lt; n; i++)<br>            {<br>                if (((1 &lt;&lt; i) &amp; j) != 0 &amp;&amp; ((1 &lt;&lt; i) &amp; x) == 0)<br>                {<br>                    c++;<br>                }<br>            }<br>            if (c &gt; ans)<br>            {<br>                ans = c;<br>            }<br>        }<br>        return ans;<br>    }<br>}<br></td>