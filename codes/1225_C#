            using System;<br>using System.Collections;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class PeopleCircle<br>{<br>    public string order(int numMales,int numFemales,int K)<br>    {<br>        StringBuilder s = new StringBuilder();<br> <br>        for (int i=0;i&lt;numMales;i++) s.Append('M');<br> <br>        while (numFemales-- &gt; 0)<br>        {<br>            s.Insert(0,'F');<br>            rotate(s,K%s.Length-1);<br>        }<br> <br>        return s.ToString();<br>    }<br> <br>    void rotate(StringBuilder s,int K)<br>    {<br>        if (K &gt; 0)<br>        {<br>            String s1 = s.ToString().Substring(s.Length-K);<br>            s.Remove(s.Length-K,K);<br>            s.Insert(0,s1);<br>        }<br>        else<br>        {<br>            String s1 = s.ToString().Substring(0,-K);<br>            s.Remove(0,-K);<br>            s.Append(s1);<br>        }<br>    }<br>}<br></td>