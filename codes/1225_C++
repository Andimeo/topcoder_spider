            #line 2 "PeopleCircle.cpp"<br><br>#include &lt;iostream&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;map&gt;<br>#include &lt;sstream&gt;<br>#include &lt;cmath&gt;<br><br>using namespace std;<br><br><br>class PeopleCircle {<br>  public:<br>  string order(int numMales, int numFemales, int K) {<br>    int n=numMales+numFemales;<br>    string ans(n,'M');<br>    vector&lt;bool&gt; gone(numMales+numFemales,false);<br>    int pos=0;<br>    for(int i=0;i&lt;numFemales;i++) {<br>      int j=K;<br>      while (j&gt;1 || gone[pos]) {        <br>        if (!gone[pos])<br>          j--;<br>        pos=(pos+1)%n;<br>      }<br>      ans[pos]='F';<br>      gone[pos]=true;<br>      pos=(pos+1)%n;<br>    }<br>    return ans;<br>  }<br>};<br><br><br>// Powered by FileEdit<br></td>