            import java.util.*;<br>public class PeopleCircle {<br> <br>   public String order ( int numMales, int numFemales, int K ) {<br>     List l = new ArrayList();<br>     StringBuffer sb = new StringBuffer();<br>     for (int i = 0; i &lt; numMales+numFemales; i++) {<br>      sb.append('M');<br>      l.add(new Integer(i));<br>    }<br>    int pos=0;<br>    for (int i = 0; i &lt; numFemales; i++) {<br>      pos = (pos + K - 1) % l.size();<br>      sb.setCharAt(((Integer)l.get(pos)).intValue(),'F');<br>      l.remove(pos);<br>    }<br>    return sb.toString();<br>  }<br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>