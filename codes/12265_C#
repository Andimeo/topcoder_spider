            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class TheJediTest {<br>    public int minimumSupervisors(int[] students, int K)<br>    {<br>        int ret = 0;<br>        for (int i = 0; i &lt; students.Length; i++)<br>        {<br>            int next = (students[i] + K - 1) / K;<br>            int nokori = next * K;<br>            ret += next;<br>            nokori -= students[i];<br>            int diff;<br>            if (i + 1 &lt; students.Length)<br>            {<br>                diff = Math.Min(students[i + 1], nokori);<br>                nokori -= diff;<br>                students[i + 1] -= diff;<br>            }<br>            if (i + 2 &lt; students.Length)<br>            {<br>                diff = Math.Min(students[i + 2], nokori);<br>                nokori -= diff;<br>                students[i + 2] -= diff;<br>            }<br>        }<br>        return ret;<br>    }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] Modified for C# by ysn<br>// Powered by CodeProcessor<br></td>