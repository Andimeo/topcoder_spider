<td class="problemText" colspan="8" valign="middle" align="left">
            #include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br>#include &lt;iostream&gt;<br>#include &lt;vector&gt;<br>#include &lt;queue&gt;<br>#include &lt;algorithm&gt;<br><br>using namespace std ;<br><br>class TheJediTest<br>{<br>    public :<br>      int minimumSupervisors(vector &lt;int&gt; students, int K)<br>      {<br>        int n = students.size() ;<br>        int ans = 0 ;<br>        int i ;<br>        int pre = 0 ;<br>        <br>        for(i=0;i&lt;n;i++)<br>        {<br>          if(students[i]&gt;K) pre = 0 ;<br>          ans += students[i]/K ;<br>          students[i] %= K ;<br>          if(students[i]&gt;0)<br>          {<br>            if(pre!=0)<br>            {<br>              if(i==n-1) ans++ ;<br>              else<br>              {<br>                int canr = K-students[i] ;<br>                ans++ ;<br>                students[i+1] -= canr ;<br>                if(students[i+1]&lt;0) students[i+1] = 0 ;<br>                pre = 0 ;<br>              }              <br>            }<br>            else<br>            {<br>              if(i==n-1) ans++ ;<br>              else<br>              {<br>                pre = students[i] ;<br>                students[i+1] += students[i] ;<br>              }<br>            }<br>          }<br>        }<br>        <br>        return ans ;<br>      }<br>} ;<br></td>