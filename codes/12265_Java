            /**<br> * Built using CHelper plug-in<br> * Actual solution is at the top<br> */<br>public class TheJediTest {<br>    public int minimumSupervisors(int[] students, int K) {<br>        int extra = 0;<br>        int res = 0;<br>        for (int i = 0; i &lt; students.length; ++i) {<br>            int x = students[i];<br>            int needToFull = (K - extra % K) % K;<br>            if (needToFull &lt;= x) {<br>                x -= needToFull;<br>                extra += needToFull;<br>                if (extra % K != 0) throw new RuntimeException();<br>                res += extra / K;<br>                extra = x;<br>            } else {<br>                extra += x;<br>                needToFull -= x;<br>                if (i + 1 &lt; students.length) {<br>                    int more = Math.min(needToFull, students[i + 1]);<br>                    students[i + 1] -= more;<br>                    extra += more;<br>                }<br>                res += (extra + K - 1) / K;<br>                extra = 0;<br>            }<br>        }<br>        res += (extra + K - 1) / K;<br>        return res;<br>    }<br>}<br></td>