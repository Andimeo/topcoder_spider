            using System.Collections.Generic;<br><br>  public class ValueHistogram<br>    {<br>        public string[] build(int[] values)<br>        {<br>            int[] f = new int[10];<br>            for (int i = 0; i &lt; values.Length; i++)<br>            {<br>                f[values[i]]++;<br>            }<br><br>            int max = 0;<br>            for (int i = 0; i &lt; f.Length; i++)<br>            {<br>                if (f[i] &gt; max) max = f[i];<br>            }<br><br>            List&lt;string&gt; res = new List&lt;string&gt;();<br>            for (int i = max + 1; i &gt; 0; i--)<br>            {<br>                string temp = "";<br>                for (int j = 0; j &lt; f.Length; j++)<br>                {<br>                    if (f[j] &gt;= i) temp += "X";<br>                    else temp += ".";<br>                }<br>                res.Add(temp);<br>            }<br><br>            return res.ToArray();<br>        }<br>    }<br></td>