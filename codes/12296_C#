            using System;<br>using System.Text;<br>using System.Collections;<br>using System.Diagnostics;<br>using System.Collections.Generic;<br>public class TomekPhone<br>{<br>    public int minKeystrokes(int[] occurences, int[] keySizes)<br>    {<br>        int res;<br>        int sum = 0;<br>        for (int i = 0; i &lt; keySizes.Length; i++)<br>        {<br>            sum += keySizes[i];<br>        }//for i<br>        if (sum &lt; occurences.Length)<br>            return -1;<br><br>        res = 0;<br>        Array.Sort(occurences);<br>        List&lt;int&gt; need = new List&lt;int&gt;();<br>        for (int i = 0; i &lt; keySizes.Length; i++)<br>        {<br>            for (int j = 0; j &lt; keySizes[i]; j++)<br>            {<br>                need.Add(j + 1);<br>            }//for j<br>        }//for i<br>        need.Sort();<br>        int cnt = 0;<br>        for (int i = occurences.Length - 1; i &gt;= 0; i--)<br>        {<br>            res += need[cnt++] * occurences[i];<br>        }//forrev i<br>        return res;<br>    }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] Modified for C# by ysn<br>// Powered by CodeProcessor<br></td>