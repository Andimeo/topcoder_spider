            import java.util.Arrays;<br> <br>/**<br> * Built using CHelper plug-in<br> * Actual solution is at the top<br> */<br>public class TomekPhone {<br>  public int minKeystrokes(int[] occurences, int[] keySizes) {<br>        Arrays.sort(occurences);<br>        int ptr = occurences.length - 1;<br>        int res = 0;<br>        outer: for (int at = 0; at &lt; 100; ++at) {<br>            for (int x : keySizes) {<br>                if (x &gt; at) {<br>                    res += (at + 1) * occurences[ptr];<br>                    --ptr;<br>                    if (ptr &lt; 0) break outer;<br>                }<br>            }<br>        }<br>        if (ptr &lt; 0) return res; else return -1;<br>  }<br>}<br></td>