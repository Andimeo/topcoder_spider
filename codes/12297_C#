            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class TypingDistance {<br>    public int minDistance(string keyboard, string word)<br>    {<br>        int res = 0;<br>        for (int i = 1; i &lt; word.Length;i++ )<br>        {<br>            res += Math.Abs (keyboard.IndexOf (word[i - 1]) - keyboard.IndexOf (word[i]));<br>        }<br><br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>