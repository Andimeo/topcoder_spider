            public class PastingPaintingDivTwo {<br><br>  public long countColors(String[] c, int T) {<br>    char[][] grid = new char[105][105];<br>    long ret = 0;<br>    int bf = Math.min(T, Math.max(c.length, c[0].length()));<br>    int bf2;<br>    for (bf2 = 0; bf2 &lt; bf; bf2++ ){<br>      for (int i = 0; i &lt; c.length; i++) {<br>        for (int j = 0; j &lt; c[i].length(); j++) {<br>          if (c[i].charAt(j) == 'B'&amp;&amp;grid[bf2+1+i][bf2+1+j] != 'B') {<br>            ret++;<br>            grid[bf2+1+i][bf2+1+j] = 'B';<br>          }<br>        }<br>      }<br>    }<br>    if (bf2 &gt;= T) return ret;<br>    long diff = 0;<br>    for (;bf2 &lt; bf+1; bf2++ ){<br>      for (int i = 0; i &lt; c.length; i++) {<br>        for (int j = 0; j &lt; c[i].length(); j++) {<br>          if (c[i].charAt(j) == 'B'&amp;&amp;grid[bf2+1+i][bf2+1+j] != 'B') {<br>            diff++;<br>            grid[bf2+1+i][bf2+1+j] = 'B';<br>          }<br>        }<br>      }<br>    }<br>    <br>    return ret+diff*(T-bf);<br>  }<br>}<br></td>