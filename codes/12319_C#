            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class CucumberMarket<br>{<br>  public string check(int[] price, int budget, int k)<br>  {<br>        Array.Sort(price);<br>        Array.Reverse(price);<br>        int sum = 0;<br>        for (int i = 0; i &lt; k; i++)<br>            sum += price[i];<br>        if (sum &gt; budget)<br>            return "NO";<br>        return "YES";<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>