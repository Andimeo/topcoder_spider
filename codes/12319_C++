            #include&lt;iostream&gt;<br>#include&lt;algorithm&gt;<br>#include&lt;string&gt;<br>#include&lt;vector&gt;<br>using namespace std;<br>class CucumberMarket{<br>  public:<br>  string check(vector &lt;int&gt; p, int b, int k)<br>  {<br>    int sz = p.size(),i,sum = 0;<br>    bool f = true;<br>    sort(p.begin(),p.end());<br>    for(i= sz-1;i&gt;=0;i--)<br>    {<br>      sum  += p[i];<br>      k--;<br>      if(sum &gt; b)<br>      {<br>        f = false;<br>        break;    <br>      }<br>      if(!k)break;<br>    }    <br>    if(f)return "YES";<br>    return "NO";<br>  }<br>};<br></td>