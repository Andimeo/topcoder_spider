            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class SpellCards<br>{<br>  public int maxDamage(int[] level, int[] damage)<br>  {<br>    int ret = 0;<br>    for(int qq = 0; qq &lt;= level.length; qq++)  {<br>      int[][] dp = new int[level.length+1][level.length+1];<br>      for(int i = level.length-1; i &gt;= 0; i--)  {<br>        for(int j = i+1; j &lt; dp.length; j++)  {<br>          for(int k = 0; i + level[i] + k &lt; dp.length; k++)  {<br>            dp[i][k+level[i]] = Math.max(dp[i][k+level[i]], dp[j][k] + damage[i]);<br>            ret = Math.max(ret, dp[i][k+level[i]]);<br>          }<br>        }<br>      }<br>      int[] cA = new int[level.length];<br>      int[] dA = new int[damage.length];<br>      for(int i = 0; i &lt; cA.length; i++)  {<br>        int j = (i+1);<br>        if(j == level.length)<br>          j = 0;<br>        cA[i] = level[j];<br>        dA[i] = damage[j];<br>      }<br>      level = cA;<br>      damage = dA;<br>    }<br>    return ret;<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.8 (beta) modified by pivanof!<br></td>