            #include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;ctime&gt;<br>#include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;cstring&gt;<br>#include &lt;vector&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;algorithm&gt;<br><br>using namespace std;<br><br>int dp[55][55];<br><br>class SpellCardsEasy {<br>public:<br>    int f(const vector &lt;int&gt; &amp;level, const vector &lt;int&gt; &amp;damage, int index, int right) {<br>        if (index == -1) return 0;<br>        if (dp[index][right] != -1) return dp[index][right];<br><br>        int sol = f(level, damage, index - 1, right + 1);<br>        if (level[index] - 1 &lt;= right) {<br>            sol = max(f(level, damage, index-1, right - level[index] + 1) + damage[index], sol);<br>        }<br>        return dp[index][right] = sol;<br>    }<br><br>    int maxDamage( vector &lt;int&gt; level, vector &lt;int&gt; damage ) {<br>        memset(dp, -1, sizeof dp);<br><br>        return f(level, damage, level.size() - 1, 0);<br>    }<br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by moj 4.15 [modified TZTester]<br>// Powered by CodeProcessor<br></td>