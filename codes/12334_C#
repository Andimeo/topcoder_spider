            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class FoxAndHandleEasy<br>{<br>  public string isPossible(string S, string T)<br>  {<br>    for (int i = 0; i &lt; S.Length; ++i)<br>    {<br>        string s2 = S;<br>        string r = s2.Insert(i, S);<br>        if (r == T) return "Yes";<br>    }<br>      return "No";<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>