            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class PenguinTiles<br>{<br>  public int minMoves(string[] tiles)<br>  {<br>    for (int i = 0; i &lt; tiles.Length; ++i)<br>    {<br>        for (int j = 0; j &lt; tiles[i].Length; ++j)<br>        {<br>            if (tiles[i][j] == '.')<br>            {<br>                    if (i == tiles.Length - 1 &amp;&amp; j == tiles[i].Length - 1) return 0;<br>                    if (i == tiles.Length-1 || j == tiles[i].Length -1) return 1;<br>            }<br>        }<br>    }<br>      return 2;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>