            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br><br>using namespace std;<br><br>class PenguinTiles {<br>public:<br>  int minMoves(vector &lt;string&gt;);<br>};<br><br>int PenguinTiles::minMoves(vector &lt;string&gt; tile) <br>{<br><br>int x, y;<br>int row=tile.size(),col=tile[0].size();<br><br>for(int i=0;i&lt;row;i++)<br>  for(int j=0;j&lt;col;j++)<br>    if( tile[i][j]=='.' )<br>      { x=i;y=j;<br>       }<br>       <br><br>    <br>  int ans=0;<br>  <br>   if( x!=row-1 )<br>      ans++;<br>       <br>      <br>   if( y!=col-1 )<br>      ans++;<br>     <br>     <br>    return  ans;           <br>                        <br>  <br>}<br></td>