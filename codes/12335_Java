            public class PenguinTiles {<br> <br>  public int minMoves(String[] tiles) {<br>    int H = tiles.length;<br>    int W = tiles[0].length();<br>    int ans = 2;<br>    for(int a=0;a&lt;H;a++){<br>      for(int b=0;b&lt;W;b++){<br>        if(tiles[a].charAt(b)=='.'){<br>          if(a==H-1)ans--;<br>          if(b==W-1)ans--;<br>          return ans;<br>        }<br>      }<br>    }<br>    return ans;<br>  }<br> <br>}<br></td>