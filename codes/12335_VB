            Public Class PenguinTiles<br>    Public Function minMoves(tiles As String()) As Integer<br>        Dim Count As Integer = 0<br>        For I As Integer = 0 To tiles.Length - 1<br>            If tiles(I).Contains(".") Then<br>                If I &lt;&gt; tiles.Length - 1 Then Count += 1<br>                Dim J As Integer = tiles(I).IndexOf(".")<br>                If J &lt;&gt; tiles(I).Length - 1 Then Count += 1<br>                Return Count<br>            End If<br>        Next<br>        Return 0<br>    End Function<br>End Class<br></td>