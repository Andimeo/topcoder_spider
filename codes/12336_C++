            //#pragma comment(linker, "/STACK:16777216")<br>#include &lt;stdio.h&gt;<br>#include &lt;ctype.h&gt;<br>#include &lt;iostream&gt;<br>#include &lt;math.h&gt;<br>#include &lt;string.h&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;stdlib.h&gt;<br>#include &lt;time.h&gt;<br>#include &lt;queue&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;utility&gt;<br>#include &lt;assert.h&gt;<br><br>#define MPI 3.141592653589793238462643<br>#define eps 1e-8<br>#define inf ((int)1e9)<br>#define pb push_back<br>#define mp make_pair<br><br>using namespace std;<br><br>long long res=1;<br>bool G[60][60];<br>long long Deg[60];<br><br>class <br>PenguinSledding<br>{<br>public:<br>long long countDesigns(int n, vector &lt;int&gt; V1, vector &lt;int&gt; V2)<br>{<br>  int m=(int)V1.size(), i, j, k;<br>  for (i=0; i&lt;m; i++)<br>    V1[i]--, V2[i]--, Deg[V1[i]]++, Deg[V2[i]]++, G[V1[i]][V2[i]]=1, G[V2[i]][V1[i]]=1;<br>  for (i=0; i&lt;n; i++)<br>    for (j=i+1; j&lt;n; j++)<br>      for (k=j+1; k&lt;n; k++)<br>        if (G[i][j] &amp;&amp; G[j][k] &amp;&amp; G[k][i])<br>          res++;<br>  res-=m;<br>  for (i=0; i&lt;n; i++)<br>    res+=(1ll&lt;&lt;Deg[i])-1ll;<br>  return res;<br>}<br><br>};<br></td>