<td class="problemText" colspan="8" valign="middle" align="left">
            class AlternateColors<br>{<br>public string getColor(long r, long g, long b, long k)<br>{<br>long min;<br>min=r;<br>if(g&lt;min)min=g;<br>if(b&lt;min)min=b;<br>if(min==r)<br>{<br>if(k&lt;=3*min)<br>{<br>if(k%3==1)return "RED";<br>if(k%3==2)return "GREEN";<br>if(k%3==0)return "BLUE";<br>}<br>b-=min;<br>k-=3*min;<br>g-=min;<br>long min2;<br>min2=g;<br>if(b&lt;min2)min2=b;<br>if(k&lt;2*min2)<br>{<br>if(k%2==1)return "GREEN";<br>if(k%2==0)return "BLUE";<br>}<br>if(min2==b)return "GREEN";<br>return "BLUE";<br>}<br><br>if(min==g)<br>{<br>if(k&lt;=3*min)<br>{<br>if(k%3==1)return "RED";<br>if(k%3==2)return "GREEN";<br>if(k%3==0)return "BLUE";<br>}<br>b-=min;<br>k-=3*min;<br>r-=min;<br>long min2;<br>min2=r;<br>if(b&lt;min2)min2=b;<br>if(k&lt;2*min2)<br>{<br>if(k%2==1)return "RED";<br>if(k%2==0)return "BLUE";<br>}<br>if(min2==b)return "RED";<br>return "BLUE";<br>}<br>if(min==b)<br>{<br>if(k&lt;=3*min)<br>{<br>if(k%3==1)return "RED";<br>if(k%3==2)return "GREEN";<br>if(k%3==0)return "BLUE";<br>}<br>r-=min;<br>k-=3*min;<br>g-=min;<br>long min2;<br>min2=g;<br>if(r&lt;min2)min2=r;<br>if(k&lt;2*min2)<br>{<br>if(k%2==1)return "RED";<br>if(k%2==0)return "GREEN";<br>}<br>if(min2==r)return "GREEN";<br>return "RED";<br>}<br>return "HELLO";<br>}<br>};<br></td>