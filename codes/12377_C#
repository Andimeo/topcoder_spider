            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class TheSquareRootDilemma {<br>    public int countPairs(int N, int M)<br>    {<br>        int MAX = 1000000;<br>        int[] dp = new int[MAX];<br>        for (int i = 1; i &lt;= N; i++)<br>        {<br>            dp[getamari(i)]++;<br>        }<br>        int ret = 0;<br>        for (int j = 1; j &lt;= M; j++)<br>        {<br>            ret += dp[getamari(j)];<br>        }<br>        return ret;<br>    }<br><br>    int getamari(int a)<br>    {<br>        int ret = 1;<br>        for (int i = 2; i * i &lt;= a; i++)<br>        {<br>            while (a % i == 0)<br>            {<br>                if (a % (i * i) == 0)<br>                {<br>                    a /= i * i;<br>                    continue;<br>                }<br>                ret *= i;<br>                a /= i;<br>            }<br>        }<br>        if (a != 1) ret *= a;<br>        return ret;<br>    }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] Modified for C# by ysn<br>// Powered by CodeProcessor<br></td>