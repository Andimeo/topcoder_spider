            import java.util.*;<br>@SuppressWarnings("unchecked")<br> <br>public class TheSquareRootDilemma<br>{<br>  public int countPairs(int N, int M)<br>  {<br>    int res = 0;<br> <br>        for(int i = 1;i &lt;= N;i++)<br>        {<br>            int val = 1;<br>            int tmp = i;<br>            for(int j = 2;j*j &lt;= tmp;j++)<br>                if(tmp%j == 0)<br>                {<br>                    int cnt = 0;<br>                    while (tmp%j == 0)<br>                    {<br>                        tmp /= j;<br>                        cnt++;<br>                    }<br>                    if(cnt%2 == 1)<br>                        val *= j;<br>                }<br>            if(tmp != 1)<br>                val *= tmp;<br> <br>            res += Math.round(Math.floor(Math.sqrt(M/val)));<br>        }<br> <br>        return res;<br>  }<br> <br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>