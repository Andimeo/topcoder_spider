            import java.util.Arrays;<br> <br>/**<br> * Built using CHelper plug-in<br> * Actual solution is at the top<br> */<br>public class StringGame {<br>  public int[] getWinningStrings(String[] S) {<br>        int[][] cnt = new int[S.length][26];<br>        for (int i = 0; i &lt; S.length; ++i)<br>            for (int j = 0; j &lt; S[i].length(); ++j)<br>                ++cnt[i][S[i].charAt(j) - 'a'];<br>        int[] res = new int[S.length];<br>        int rcnt = 0;<br>        for (int i = 0; i &lt; S.length; ++i) {<br>            boolean[] alive = new boolean[S.length];<br>            Arrays.fill(alive, true);<br>            alive[i] = false;<br>            while (true) {<br>                boolean updated = false;<br>                for (int j = 0; j &lt; 26; ++j) {<br>                    boolean hadLess = false;<br>                    boolean hadMore = false;<br>                    for (int k = 0; k &lt; S.length; ++k) if (alive[k]) {<br>                        if (cnt[k][j] &gt; cnt[i][j]) {<br>                            hadMore = true;<br>                            break;<br>                        }<br>                        if (cnt[k][j] &lt; cnt[i][j]) {<br>                            hadLess = true;<br>                        }<br>                    }<br>                    if (!hadMore &amp;&amp; hadLess) {<br>                        updated = true;<br>                        for (int k = 0; k &lt; S.length; ++k) if (alive[k] &amp;&amp; cnt[k][j] &lt; cnt[i][j]) {<br>                            alive[k] = false;<br>                        }<br>                    }<br>                }<br>                if (!updated) break;<br>            }<br>            boolean any = false;<br>            for (boolean x : alive) if (x) any = true;<br>            if (!any) {<br>                res[rcnt++] = i;<br>            }<br>        }<br>        int[] sres = new int[rcnt];<br>        System.arraycopy(res, 0, sres, 0, rcnt);<br>        return sres;<br>  }<br>}<br></td>