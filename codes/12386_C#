            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class NewArenaPassword {<br>    public int minChange(string oldPassword, int K)<br>    {<br>        int len = oldPassword.Length;<br>        uni = new int[len];<br>        for (int i = 0; i &lt; len; i++)<br>        {<br>            uni[i] = -1;<br>        }<br>        for (int i = 0; i &lt; K; i++)<br>        {<br>            connect(i, len - K + i);<br>        }<br>        <br>        int ret = 0;<br>        for (int i = 0; i &lt; len; i++)<br>        {<br>            if (uni[i] &gt;= 0) continue;<br>            int count = 0;<br>            int max = 0;<br>            Dictionary&lt;char, int&gt; dic = new Dictionary&lt;char, int&gt;();<br>            for (int j = 0; j &lt; len; j++)<br>            {<br>                if (getuni(j) == i)<br>                {<br>                    char c = oldPassword[j];<br>                    if (!dic.ContainsKey(c)) dic[c] = 0;<br>                    dic[c]++;<br>                    count++;<br>                }<br>            }<br>            foreach (int item in dic.Values)<br>            {<br>                max = Math.Max(max, item);<br>            }<br>            ret += count - max;<br>        }<br><br><br>        return ret;<br>    }<br><br>    int[] uni;<br>    int getuni(int a)<br>    {<br>        if (uni[a] &lt; 0) return a;<br>        else return uni[a] = getuni(uni[a]);<br>    }<br><br>    void connect(int a, int b)<br>    {<br>        a = getuni(a);<br>        b = getuni(b);<br>        if (a == b) return;<br>        uni[b] = a;<br>    }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] Modified for C# by ysn<br>// Powered by CodeProcessor<br></td>