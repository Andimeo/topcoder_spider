            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class NewArenaPassword {<br>public:<br>  int minChange(string, int);<br>};<br> <br>int NewArenaPassword::minChange(string s, int K) {<br>  int n = s.size(), i, g[55][55], was[55], x[55], cnt[255];<br>  for (int i=0;i&lt;n;i++)<br>    for (int j=0;j&lt;n;j++) g[i][j] = 0;<br>  for (int i=0;i&lt;K;i++) g[i][n-K+i] = 1;<br>  for (int i=0;i&lt;n;i++) was[i] = 0;<br>  int ans = 0;<br>  for (int ii=0;ii&lt;n;ii++)<br>    if (!was[ii]) {<br>      int i = 1, e = 1;<br>      x[1] = ii;<br>      was[ii] = 1;<br>      while (i &lt;= e) {<br>        for (int j=0;j&lt;n;j++)<br>          if (g[x[i]][j] == 1 &amp;&amp; !was[j]) {<br>            e++;<br>            x[e] = j;<br>            was[j] = 1;<br>          }<br>        i++;<br>      }<br>      for (char c='a';c&lt;='z';c++) cnt[c] = 0;<br>      for (int i=1;i&lt;=e;i++) cnt[s[x[i]]]++;<br>      int mx = 0;<br>      for (char c='a';c&lt;='z';c++)<br>        if (cnt[c] &gt; mx) mx = cnt[c];<br>      ans += e-mx;<br>    }<br>  return ans;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>