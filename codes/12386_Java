            /**<br> * Built using CHelper plug-in<br> * Actual solution is at the top<br> */<br>public class NewArenaPassword {<br>    public int minChange(String oldPassword, int K) {<br>        int n = oldPassword.length();<br>        boolean[][] eq = new boolean[n][n];<br>        for (int i = 0; i &lt; K; ++i) {<br>            eq[i][n - K + i] = true;<br>            eq[n - K + i][i] = true;<br>        }<br>        for (int i = 0; i &lt; n; ++i) eq[i][i] = true;<br>        for (int k = 0; k &lt; n; ++k)<br>            for (int i = 0; i &lt; n; ++i)<br>                for (int j = 0; j &lt; n; ++j)<br>                    eq[i][j] |= eq[i][k] &amp;&amp; eq[k][j];<br>        boolean[] mark = new boolean[n];<br>        int res = 0;<br>        for (int i = 0; i &lt; n; ++i) if (!mark[i]) {<br>            int[] cnt = new int[26];<br>            int total = 0;<br>            for (int j = 0; j &lt; n; ++j) if (eq[i][j]) {<br>                mark[j] = true;<br>                ++cnt[oldPassword.charAt(j) - 'a'];<br>                ++total;<br>            }<br>            int max = 0;<br>            for (int x : cnt) max = Math.max(max, x);<br>            res += total - max;<br>        }<br>        return res;<br>    }<br>}<br></td>