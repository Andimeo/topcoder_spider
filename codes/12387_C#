            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class EasyHomework<br>{<br>  public string determineSign(int[] A)<br>  {<br>    <br>        int neg=0;<br>        foreach(int i in A)<br>        {<br>            if (i==0) return "ZERO";<br>             if (i&lt;0) neg++;<br> <br>        }<br>        if (neg % 2 == 1) return "NEGATIVE";<br>        <br>        <br>          return  "POSITIVE";<br>            <br>  }<br> <br> <br>}<br> <br>//Powered by KawigiEdit 2.1.8 (beta) modified by pivanof!<br></td>