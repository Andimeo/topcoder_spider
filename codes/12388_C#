            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class TheDevice {<br>    public int minimumAdditional(string[] plates)<br>    {<br>        int ret = 0;<br>        for (int j = 0; j &lt; plates[0].Length; j++)<br>        {<br>            int[] num = new int[2];<br>            for (int i = 0; i &lt; plates.Length; i++)<br>            {<br>                num[plates[i][j] - '0']++;<br>            }<br>            int temp = 0;<br>            if (num[0] &lt;= 0) temp += 1;<br>            if (num[1] &lt;= 1) temp += 2 - num[1];<br>            ret = Math.Max(temp, ret);<br>        }<br>        return ret;<br>    }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] Modified for C# by ysn<br>// Powered by CodeProcessor<br></td>