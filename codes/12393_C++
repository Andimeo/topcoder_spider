            #include &lt;algorithm&gt;<br>#include &lt;bitset&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cmath&gt;<br>#include &lt;iostream&gt;<br>#include &lt;map&gt;<br>#include &lt;numeric&gt;<br>#include &lt;queue&gt;<br>#include &lt;set&gt;<br>#include &lt;sstream&gt;<br>#include &lt;stack&gt;<br>#include &lt;string&gt;<br>#include &lt;utility&gt;<br>#include &lt;vector&gt;<br>using namespace std;<br> <br>#define FOR(i,l,r) for(int i=(l);i&lt;(r);i++)<br>#define FOREACH(i,v) for(typeof((v).begin()) i=(v).begin();i!=(v).end();i++)<br> <br>map&lt; pair&lt;int, int&gt;, int &gt; parse(string word) {<br>  map&lt; pair&lt;int, int&gt;, int &gt; res;<br>  int n = word.size();<br>  for (int i = 0; i &lt; n; i += 2) {<br>    char a = word[i], b = i + 1 &lt; n ? word[i + 1] : ' ';<br>    if (a &gt; b) swap(a, b);<br>    res[make_pair(a, b)]++;<br>  }<br>  return res;<br>}<br> <br>class EllysReversals {<br>public:<br>int getMin(vector &lt;string&gt; words) {<br>  set&lt; map&lt; pair&lt;int, int&gt;, int &gt; &gt; s;<br>  FOREACH(it, words) {<br>    map&lt; pair&lt;int, int&gt;, int &gt; m = parse(*it);<br>    if (s.find(m) != s.end()) {<br>      s.erase(m);<br>    } else {<br>      s.insert(m);<br>    }<br>  }<br>  return s.size();<br>}<br>};<br> <br> <br>// Powered by FileEdit<br>// Powered by moj 4.17 [modified TZTester]<br>// Powered by CodeProcessor<br></td>