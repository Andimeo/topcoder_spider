            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class EllysReversals<br>{<br>  public int getMin(String[] words) {<br>    HashSet&lt;String&gt; hs = new HashSet&lt;String&gt;();<br>    for (String s : words) {<br>      s = min(s);<br>      if (hs.contains(s)) {<br>        hs.remove(s);<br>      } else {<br>        hs.add(s);<br>      }<br>    }<br>    return hs.size();<br>  }<br> <br>  private String min(String s) {<br>    String[] pairs = new String[s.length() / 2];<br>    for (int i = 0; i &lt; pairs.length; i++) {<br>      pairs[i] = Math.min(s.charAt(2 * i), s.charAt(2 * i + 1)) + ""<br>          + Math.max(s.charAt(2 * i), s.charAt(2 * i + 1));<br>    }<br>    Arrays.sort(pairs);<br>    StringBuilder ans = new StringBuilder();<br>    for (String str : pairs) {<br>      ans.append(str);<br>    }<br>    if (s.length() % 2 != 0) {<br>      ans.append(s.charAt(s.length() - 1));<br>    }<br>    return ans.toString();<br>  }<br> <br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>