            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class EllysCandyGame<br>{<br>  String[] player;<br>  int[] s;<br>  int[] count;<br>  public String getWinner(int[] sweets)<br>  {<br>    player = new String[]{"Elly", "Kris"};<br>    s = sweets;<br>    count = new int[2];<br>    return get(0);<br>  }<br>  <br>  public String get(int p) {<br>    String r = null;<br>    for(int i=0; i&lt;s.length; i++) {<br>      if(s[i] == 0)<br>        continue;<br>      int take = s[i];<br>      s[i] = 0;<br>      if(i+1 &lt; s.length) s[i+1] *= 2;<br>      if(i-1 &gt;= 0) s[i-1] *= 2;<br>      <br>      count[p] += take;<br>      String result = get((p+1)%2);<br>      <br>      s[i] = take;<br>      if(i+1 &lt; s.length) s[i+1] /= 2;<br>      if(i-1 &gt;= 0) s[i-1] /= 2;<br>      count[p] -= take;<br>      <br>      if(result.equals(player[p]))<br>        return result;<br>      if(r==null || !r.equals("Draw"))<br>        r = result;<br>    }<br>    if(r==null) {<br>      if(count[0] &gt; count[1]) <br>        return player[0];<br>      else if(count[0] &lt; count[1])<br>        return player[1];<br>      else<br>        return "Draw";<br>    }<br>    return r;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>