            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;queue&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br>#include &lt;cstring&gt;<br> <br>#define inf 214748364<br>#define max(a,b)  ((a)&gt;(b)?(a):(b))<br>#define min(a,b)  ((a)&lt;(b)?(a):(b))<br> <br>using namespace std;<br>typedef long long ll;<br> <br>const int maxn = 1000001;<br>double eps = 1e-10;<br>struct tinfo<br>{<br>  int a,b;<br>}  g[maxn];<br> <br>double res,minn;<br>int n,tar;<br> <br>void check(double a)<br>{<br>  if(a&lt;minn||a&gt;=res)<br>    return;<br>  for(int i=0;i&lt;n;++i)<br>  {<br>    int num = g[i].a/a;<br>    for(int j=-1;j&lt;=1;++j)<br>    {<br>      double p = (num+j)*a;<br>      if(g[i].a+eps&lt;p&amp;&amp;p&lt;g[i].b-eps)<br>        return;<br>    }<br>  }<br>  res = a;<br>}<br> <br>class  TheFrog<br>{<br>  public:<br>  double  getMinimum(int D, vector&lt;int&gt; L, vector&lt;int&gt; R)<br>  {<br>    res = D;<br>    n = L.size();<br>    tar = D;<br>    minn = 0;<br>    for(int i=0;i&lt;n;++i)<br>    {<br>      g[i].a = L[i], g[i].b = R[i];<br>      minn = max(minn,g[i].b-g[i].a);<br>    }<br>    for(int i=0;i&lt;n;++i)<br>    if(L[i])<br>    for(int j=1;j&lt;=30010;++j)<br>    {<br>      check(L[i]/(double)j);<br>      check(R[i]/(double)j);<br>    }<br>    for(int j=1;j&lt;=30010;++j)<br>      check(D/(double)j);<br>    return res;<br>  }<br>};<br> <br> <br> <br> <br> <br> <br>//Powered by KawigiEdit 2.1.8 (beta) modified by pivanof!<br></td>