            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class TheFrog<br>{<br>  public double getMinimum(int D, int[] L, int[] R)<br>  {<br>    HashSet&lt;Integer&gt; set = new HashSet&lt;Integer&gt;();<br>    int n = L.length;<br>    for (int i = 0; i &lt; n; ++i) {<br>      set.add(L[i]);<br>      set.add(R[i]);<br>    }<br>    double ans = D;<br>    for (int d: set) {<br>      for (int i = 1; i &lt;= d; ++i) {<br>        double x = 1.0 * d / i;<br>        boolean flag = x &lt; ans;<br>        for (int j = 0; j &lt; n &amp;&amp; flag; ++j) {<br>          flag &amp;= (L[j] * i / d + 1) * d &gt;= R[j] * i;<br>        }<br>        if (flag) {<br>          ans = x;<br>        }<br>      }<br>    }<br>    return ans;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>