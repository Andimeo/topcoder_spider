            using System;<br><br>public class TheDeviceDiv2<br>{<br>  public string identify(string[] plates)<br>  {<br>    if(plates.Length == 1)<br>      return "NO";<br>    <br>    bool resp = true;<br>    for(int i = 0; i &lt; plates[0].Length &amp;&amp; resp; i++)<br>    {<br>      int unos = 0;<br>      int ceros = 0;<br>      for(int j = 0; j &lt; plates.Length; j++)<br>      {<br>        if(plates[j].ToCharArray()[i] == '0')<br>          ceros++;<br>        if(plates[j].ToCharArray()[i] == '1')<br>          unos++;          <br>      }<br>      resp = unos &gt; 1 &amp;&amp; ceros &gt; 0;<br>    }  <br>    return resp ? "YES" : "NO";  <br>  }<br>}<br></td>