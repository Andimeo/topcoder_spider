            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class TheDeviceDiv2<br>{<br>  public String identify(String[] plates)<br>  {<br>    for (int i = 0; i &lt; plates[0].length(); i++) {<br>      int onecount = 0;<br>      int zerocount = 0;<br>      for (int j = 0; j &lt; plates.length; j++) {<br>        if (plates[j].charAt(i) == '0') zerocount++;<br>        else onecount++;<br>      }<br>      if (!(onecount &gt;= 2 &amp;&amp; zerocount &gt;= 1)){<br>        return "NO";<br>      }<br>    }<br>    return "YES";<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>