            using System;<br> <br>public class TheJediTestDiv2 {<br>  public int countSupervisors(int[] students, int Y, int J) {<br>    int result = -1;<br>    for (int i = 0; i &lt; students.Length; i++) {<br>      for (int y = 0; y &lt; students.Length; y++) {<br>        int t = 0;<br>        for (int j = 0; j &lt; students.Length; j++) {<br>          int num = Math.Max(0, students[j] - (j == y ? Y : 0));<br>          if (num == 0) continue;<br>          // Console.WriteLine(J.ToString() + "," + (num/J).ToString());<br>          t = t + (num / J);<br>          t = t + (num % J == 0 ? 0 : 1);<br>        }<br>        if (result &lt; 0 || result &gt; t)<br>          result = t;<br>      }<br>    }<br>    return result;<br>  }<br>}<br></td>