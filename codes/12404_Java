            public class TheJediTestDiv2 {<br> <br>  public int countSupervisors(int[] students, int Y, int J) {<br>    int c = 0, n = students.length, t, m = 1000000;<br>    for(int i = 0; i &lt; n; ++i)  {<br>      t = students[i];<br>      students[i] = Math.max(0, students[i] - Y);<br>      c = 0;<br>      for(int j = 0; j &lt; n; ++j)  {<br>        c += students[j]/J;<br>        if(students[j] % J &gt; 0)<br>          c++;<br>      }<br>      m = Math.min(m, c);<br>      students[i] = t;<br>    }<br>    return m;<br>  }<br>  <br>  public static void main(String[] args)  {<br>    TheJediTestDiv2 obj = new TheJediTestDiv2();<br>    int ans = 0;<br>    // ans = obj.countSupervisors;<br>    System.out.println(ans);<br>  }<br> <br>}<br></td>