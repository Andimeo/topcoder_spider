            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class FoxAndFencingEasy<br>{<br>  public string WhoCanWin(int mov1, int mov2, int d)<br>  {<br>    if(mov1 &gt;= d)<br>    {<br>      return "Ciel";<br>    }<br>    if(mov1 &gt;= mov2 * 2 +1)<br>    {<br>      return "Ciel";<br>    }<br>    if(mov2 &gt;= mov1 * 2 +1)<br>    {<br>      return "Liss";<br>    }<br><br>    return "Draw";<br>  }<br>  <br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>