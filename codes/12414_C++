            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class FoxAndFencingEasy {<br>public:<br>  string WhoCanWin(int, int, int);<br>};<br> <br>string FoxAndFencingEasy::WhoCanWin(int mov1, int mov2, int d) {<br>  if(d&lt;=mov1)<br>    return "Ciel" ;<br>  else if(d+mov1&lt;=mov2)<br>    return "Liss" ;<br>  else{<br>    int minn=min(mov1,mov2) ;<br>    int maxn=max(mov1,mov2) ;<br>    if(minn==maxn)<br>      return "Draw" ;<br>    if(minn==mov1)<br>    {<br>      if(minn+1+minn&lt;=maxn)<br>        return "Liss" ;<br>      else<br>        return "Draw" ;<br>    }<br>    else{<br>      if(minn+1+minn&lt;=maxn)<br>        return "Ciel" ;<br>      else<br>        return "Draw" ;<br>    }<br>  }<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>