            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br>#include &lt;memory.h&gt;<br> <br>using namespace std;<br> <br>class LISNumber {<br>public:<br>  int count(vector &lt;int&gt;, int);<br>};<br> <br>const int md = 1000000007;<br> <br>int pw(int a, int b) {<br>  int x = 1, step = 1 &lt;&lt; 30;<br>  while (step &gt; 0) {<br>    x = (long long)x*x % md;<br>    if (step &amp; b) x = (long long)x*a % md;<br>    step &gt;&gt;= 1;<br>  }<br>  return x;<br>}<br> <br>int C[3010][3010];<br>int inv[3010], f[3010];<br> <br>int LISNumber::count(vector &lt;int&gt; c, int K) {<br>  int n = c.size(), len = 0;<br>  for (int i=0;i&lt;n;i++) len += c[i];<br>  memset(C, 0, sizeof(C));<br>  for (int i=0;i&lt;=3000;i++) C[i][0] = 1;<br>  for (int i=1;i&lt;=3000;i++)<br>    for (int j=1;j&lt;=i;j++) {<br>      C[i][j] = C[i-1][j] + C[i-1][j-1];<br>      if (C[i][j] &gt;= md) C[i][j] -= md;<br>    }<br>  for (int i=0;i&lt;=3000;i++) inv[i] = pw(i, md-2);<br>  f[0] = 0;<br>  for (int i=1;i&lt;=K;i++) {<br>    f[i] = 1;<br>    for (int j=0;j&lt;n;j++) {<br>      for (int u=1;u&lt;=c[j];u++) f[i] = (long long)f[i] * (i-u+1) % md;<br>      for (int u=1;u&lt;=c[j];u++) f[i] = (long long)f[i] * inv[u] % md;<br>    }<br>    for (int j=1;j&lt;i;j++) {<br>      int ft = f[j];<br>      ft = (long long)ft * C[len+i-j][i-j] % md;<br>      f[i] = (f[i] + md - ft) % md;<br>    }<br>  }<br>  return f[K];<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>