            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class Chopsticks<br>{<br>  public int getmax(int[] l)<br>  {<br>        int sum = 0;<br>        Array.Sort(l);<br>        for (int i = 0; i &lt; l.Length; i++)<br>            if (i + 1 &lt; l.Length &amp;&amp; l[i] == l[i + 1])<br>            {<br>                sum++;<br>                i++;<br>            }<br>        return sum;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>