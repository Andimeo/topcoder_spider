            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class RobotHerb<br>{<br>  int[] dx = new int[]{-1,0,1,0};<br>  int[] dy = new int[]{0,1,0,-1};<br>  public long getdist(int T, int[] a)<br>  {<br>    long rep = T/4;<br>    long go = T%4;<br>    long x = 0;<br>    long y = 0;<br>    int dir = 0;<br>    for(int k = 0; k &lt; 4; k++)  {<br>      for(int i = 0; i &lt; a.length; i++)  {<br>        x += dx[dir] * a[i];<br>        y += dy[dir] * a[i];<br>        dir += a[i];<br>        dir %= 4;<br>      }<br>    }<br>    long xTemp = 0;<br>    long yTemp = 0;<br>    dir = 0;<br>    for(int k = 0; k &lt; go; k++)  {<br>      for(int i = 0; i &lt; a.length; i++)  {<br>        xTemp += dx[dir] * a[i];<br>        yTemp += dy[dir] * a[i];<br>        dir += a[i];<br>        dir %= 4;<br>      }<br>    }<br>    return Math.abs(x * rep + xTemp) + Math.abs(y * rep + yTemp);<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.8 (beta) modified by pivanof!<br></td>