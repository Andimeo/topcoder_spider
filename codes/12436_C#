            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class FoxAndMp3 {<br><br>    int N;<br>    List&lt;string&gt; l;<br>    <br>    public string[] playList(int n)<br>    {<br>        N = n;<br>        l = new List&lt;string&gt;();<br>        for (int i = 1; i &lt; 10; i++)<br>        {<br>            dfs(i);<br>        }<br>        return l.ToArray();<br>    }<br><br>    void dfs(long a)<br>    {<br>        if (l.Count &gt;= 50) return;<br>        if (a &gt; N) return;<br>        l.Add(a.ToString() + ".mp3");<br>        for (int i = 0; i &lt; 10; i++)<br>        {<br>            dfs(a * 10 + i);<br>        }<br>    }<br><br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] Modified for C# by ysn<br>// Powered by CodeProcessor<br></td>