            #include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;iostream&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;map&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;queue&gt;<br>#include &lt;string&gt;<br>#include &lt;set&gt;<br>#include &lt;cstring&gt;<br>using namespace std;<br><br>#define For(i,n) for(int i=0;i&lt;n;i++)<br>#define ALL(n) n.begin(),n.end()<br>#define pb push_back<br>#define sz(i) int(i.size())<br><br>string myitoa(int t){<br>    string ret = "";<br>    while(t){<br>        ret+=t%10+'0';<br>        t/=10;<br>    }<br>    reverse(ALL(ret));<br>    return ret;<br>}<br>class FoxAndMp3<br>{<br>public:<br>        vector &lt;string&gt; playList(int n)<br>        {<br>            string tail = ".mp3";<br>            vector &lt;string&gt; ret;<br>            priority_queue&lt;string,vector&lt;string&gt;, greater&lt;string&gt; &gt;pq;<br>            pq.push("1");<br>            set&lt;string&gt;rep;<br>            while(sz(ret)&lt;min(n,50)){<br>                string tp = pq.top(); pq.pop();<br>                int value = atoi(tp.c_str());<br>                if(rep.count(tp)==0)<br>                    rep.insert(tp),ret.pb(tp+tail);<br><br>                if(value*10LL&lt;=n)<br>                    pq.push(tp+"0");<br>                if(value+1&lt;=n)<br>                    pq.push(myitoa(value+1) );<br>            }<br>            return ret;<br>        }<br>};<br></td>