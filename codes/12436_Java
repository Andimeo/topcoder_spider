            import java.util.*;<br><br>public class FoxAndMp3 {<br>  <br>  int max = 50;<br>  int tmp = 0;<br>  int n;<br>  String[] ans;<br>  <br>  void go(long k) {<br>    if (tmp == ans.length || k &gt; n) {<br>      return;<br>    }<br>    ans[tmp++] = k + ".mp3";<br>    for (int i = 0; i &lt; 10; i++) {<br>      go(k * 10 + i);<br>    }<br>  }<br><br>  public String[] playList(int n) {<br>    this.n = n;<br>    ans = new String[Math.min(n,  max)];<br>    for (int i = 1; i &lt; 10; i++) {<br>      go(i);<br>    }<br>    return ans;<br>  }<br><br>}<br></td>