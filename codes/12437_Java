            import java.util.*;<br>public class FoxAndMp3Easy {<br>  public String[] playList(int n) {<br>    List&lt;String&gt; mp3List = new ArrayList&lt;String&gt;();<br>    for(int i=1;i&lt;=n;i++) {<br>      mp3List.add(String.valueOf(i));<br>    }<br>    Collections.sort(mp3List, new Comparator&lt;String&gt;() { <br>        public int compare(String a, String b) { <br>         return a.compareTo(b); <br>         } <br>        });<br>        int len = mp3List.size();<br>        if(len&gt;50)<br>          len = 50;<br>        String[] result = new String[len];<br>        for(int i=0;i&lt;len;i++) {<br>          result[i] = mp3List.get(i)+".mp3";<br>        }<br>        return result;<br>  }<br>}<br></td>