            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class LISNumberDivTwo<br>{<br>  public int calculate(int[] seq)<br>  {<br>        int num = 1;<br>        for (int i = 0; i &lt; seq.Length-1; i++)<br>        {<br>            if (seq[i] &gt;= seq[i + 1])<br>            {<br>                num++;<br>            }<br>        }<br>        return num;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>