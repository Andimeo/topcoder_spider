            #include &lt;algorithm&gt;<br>#include &lt;bitset&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cmath&gt;<br>#include &lt;iostream&gt;<br>#include &lt;map&gt;<br>#include &lt;numeric&gt;<br>#include &lt;queue&gt;<br>#include &lt;set&gt;<br>#include &lt;sstream&gt;<br>#include &lt;stack&gt;<br>#include &lt;string&gt;<br>#include &lt;utility&gt;<br>#include &lt;vector&gt;<br>using namespace std;<br> <br>#define FOR(i,l,r) for(int i=(l);i&lt;(r);i++)<br> <br>// vector &lt;int&gt; knowledge<br> <br>class EllysPairs {<br>public:<br>int getDifference(vector &lt;int&gt; knowledge) {<br>  int n = knowledge.size();<br>  sort(knowledge.begin(), knowledge.end());<br>  FOR(i, 0, n / 2) {<br>    knowledge[i] += knowledge[n - 1 - i];<br>  }<br>  sort(knowledge.begin(), knowledge.begin() + n / 2);<br>  return knowledge[n / 2 - 1] - knowledge[0];<br>}<br>};<br> <br> <br>// Powered by FileEdit<br>// Powered by moj 4.17 [modified TZTester]<br>// Powered by CodeProcessor<br></td>