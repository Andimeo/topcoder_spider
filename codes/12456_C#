            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class TheOlympiadInInformatics<br>{<br>    bool g(int[] s, int k, int t)<br>    {<br>        foreach (int i in s)<br>        {<br>            k -= i / (t + 1);<br>            if (k &lt; 0)<br>                return false;<br>        }<br>        return true;<br>    }<br> <br>  public int find(int[] sums, int k)<br>  {<br>        int L = 0;<br>        int H = 1000000000;<br>        while (L &lt; H)<br>        {<br>            int t = (L + H) / 2;<br>            if (g(sums, k, t))<br>                H = t;<br>            else<br>                L = t + 1;<br>        }<br>               return L;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>