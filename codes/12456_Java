            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class TheOlympiadInInformatics<br>{<br>  public int find(int[] sums, int k)<br>  {<br>    int l = 0, r = 1000000007;<br>    while (l &lt; r) {<br>      int m = (l + r) / 2;<br>      int t = k;<br>      for (int i: sums) {<br>        t -= i / (m + 1);<br>        if (t &lt; 0) {<br>          break;<br>        }<br>      }<br>      if (t &lt; 0) {<br>        l = m + 1;<br>      } else {<br>        r = m;<br>      }<br>    }<br>    return r;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>