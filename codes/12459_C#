            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class EllysNewNickname<br>{<br>  public int getLength(string nickname)<br>  {<br>        string v = "aeiouy";<br>        int vow = 0;<br>        string s = "";<br>        for (int i = 0; i &lt; nickname.Length; i++)<br>        {<br>            string c = nickname[i].ToString();<br>            if (v.Contains(c))<br>            {<br>                if (vow == 0)<br>                    s += c;<br>                vow++;<br>            }<br>            else<br>            {<br>                vow = 0;<br>                s += c;<br>            }<br>        }<br>        return s.Length;                    <br> <br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>