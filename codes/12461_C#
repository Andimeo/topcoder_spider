            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class EllysCoprimesDiv2<br>{<br>  public int getCount(int[] num)<br>  {<br>        Array.Sort(num);<br>        int n = num.Length;<br>        int sum = 0;<br>        for (int i = 0; i &lt; n-1; i++)<br>        {<br>            if (GCD(num[i], num[i + 1]) &gt; 1)<br>                sum += GetMin(num[i], num[i + 1]);<br>        }<br>        return sum;<br>  }<br> <br>    private int GCD(int a, int b)<br>    {<br>        if (b == 0) return a;<br>        return GCD(b, a % b);<br>    }<br> <br>    private int GetMin(int a, int b)<br>    {<br>        int last = a;<br>        for (int i = a + 1; i &lt; b; i++)<br>            if (GCD(last, i) == 1 &amp;&amp; GCD(i, b) == 1)<br>            {<br>                return 1;<br>            }<br>        return 2;<br>    }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>