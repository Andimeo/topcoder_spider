            #include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br><br>using namespace std;<br><br>class EllysCoprimesDiv2<br>{<br>// a&lt;b!!<br>int gcd(int a, int b)<br>{<br>  if(a==0)<br>    return b;<br>  else<br>    return gcd(b%a, a);<br>}<br><br>public: int getCount(vector &lt;int&gt; n)<br>{<br>sort(n.begin(), n.end());<br>int a=0;<br>for(int i=1;i&lt;n.size();i++)<br>{<br>  if(gcd(n[i], n[i-1])!= 1)<br>  {<br>    bool good = false;<br>    for(int j=n[i-1]+1;j&lt;n[i];j++)<br>      if(gcd(n[i-1], j) ==1)<br>        if(gcd(j, n[i]) == 1)<br>        {<br>          good = true;<br>          break;<br>        }<br>    if(good)<br>      a++;<br>    else<br>      a+=2;<br>  }<br>}<br>return a;<br>};<br>};<br></td>