            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class EllysCoprimesDiv2<br>{<br>  <br>  int gcd (int a, int b) {<br>  if (b == 0)<br>    return a;<br>  else<br>    return gcd (b, a % b);<br>  }<br>  <br>  boolean ex(int l, int r){<br>  for(int i=l+1;i&lt;r;i++)<br>    if (gcd(l,i)+gcd(i,r)==2)<br>      return true;<br>  return false;<br>  }<br> <br>  public int getCount(int[] a)<br>  {<br>    Arrays.sort(a);<br>    int n = a.length;<br>    int ans = 0;<br>    for(int i=1;i&lt;n;i++)<br>    {<br>      if(gcd(a[i-1],a[i])==1){}<br>      else<br>      if(ex(a[i-1],a[i]))<br>      ans++;<br>      else<br>      ans+=2;<br>    }<br>    return ans;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>