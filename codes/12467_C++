            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class WolfPackDivTwo {<br>public:<br>  int calc(vector &lt;int&gt;, vector &lt;int&gt;, int);<br>};<br> <br>#define p 1000000007<br>#define F first<br>#define S second<br>#define mp make_pair<br> <br>map&lt;pair&lt;int,int&gt; , long long &gt; ar[50],tmp;<br> <br>int WolfPackDivTwo::calc(vector &lt;int&gt; x, vector &lt;int&gt; y, int m) {<br>  for(int i=0;i&lt;x.size();i++)<br>    ar[i][ mp( x[i] , y[i] ) ] = 1;<br>  for(int i=0;i&lt;m;i++){<br>    for(int j=0;j&lt;x.size();j++){<br>        tmp.clear();<br>        for( map&lt;pair&lt;int,int&gt;,long long&gt;::iterator k=ar[j].begin() ; k!=ar[j].end() ; k++ ){<br>          k-&gt;S %= p;<br>          tmp[ mp( (k-&gt;F.F)+1 , k-&gt;F.S ) ] += k-&gt;S;<br>          tmp[ mp( k-&gt;F.F , (k-&gt;F.S)+1 ) ] += k-&gt;S;<br>          tmp[ mp( (k-&gt;F.F)-1 , k-&gt;F.S ) ] += k-&gt;S;<br>          tmp[ mp( k-&gt;F.F , (k-&gt;F.S)-1 ) ] += k-&gt;S;<br>        }<br>        ar[j].clear();<br>        swap( ar[j] , tmp );<br>    }<br>  }<br>  long long ans = 0;<br>  <br>  for( map&lt;pair&lt;int,int&gt;,long long&gt;::iterator i = ar[0].begin() ; i!=ar[0].end() ; i++ ){<br>    long long mul=1;<br>    for( int j=0;j&lt;x.size();j++ )<br>      mul = (mul*( ar[j][ mp( i-&gt;F.F , i-&gt;F.S ) ] ) ) % p;<br>    ans = ( ans + mul )%p;<br>  }<br>  return (int)ans;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>