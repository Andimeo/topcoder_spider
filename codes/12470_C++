            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cmath&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;queue&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;cstring&gt;<br>#include &lt;numeric&gt;<br>using namespace std;<br>typedef long long ll;<br>#define F0(i,n) for (i = 0; i &lt; n; i++)<br>#define F1(i,n) for (i = 1; i &lt;= n; i++)<br>#define CL(a,x) memset(x, a, sizeof(x));<br>#define SZ(x) ((int)x.size())<br>const double eps = 1e-10;<br>const int inf = 1000000009;<br>int i, j, k, m, n, l;<br>int ans;<br> <br>struct TeamContest { <br>int worstRank(vector &lt;int&gt; s) {<br>    sort(s.begin(), s.begin() + 3);<br>    int str = s[0] + s[2];<br>    s.erase(s.begin(), s.begin() + 3);<br>    ans = 1;<br>    while (!s.empty())<br>    {<br>        n = SZ(s);<br>        sort(s.begin(), s.end());<br>        if (s[n-3] + s[n-1] &lt;= str) break;<br>        i = n-3;<br>        while (i &gt; 0 &amp;&amp; s[i-1] + s[n-1] &gt; str) i--;<br>        s.erase(s.begin()+i, s.begin()+i+2);<br>        s.pop_back();<br>        ans++;<br>    }<br>  return ans;<br>} <br>  <br> <br>}; <br> <br> <br> <br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>