            using System;<br>using System.Collections;<br>public class TeamContestEasy<br>{<br>    public int worstRank(int[] strength)<br>    {<br>        int ret = 1;<br>        int s = strength[0] + strength[1] + strength[2] - Math.Min(strength[0], Math.Min(strength[1], strength[2]));<br>        strength[0] = 0; strength[1] = 0; strength[2] = 0;<br>        Array.Sort(strength);<br>        Array.Reverse(strength);<br>        int t = strength.Length / 3;<br>        ArrayList list = new ArrayList();<br>        int pos = (t - 1) * 2 - 1;<br>        for (int i = 0; i &lt; t - 1 &amp;&amp; i &lt; pos; i++)<br>        {<br>            for (int j = pos; j &gt; i; j--)<br>            {<br>                if (strength[i] + strength[j] &gt; s)<br>                {<br>                    ret++;<br>                    pos = j - 1;<br>                    break;<br>                }<br>            }<br>        }<br>        return ret;<br>    }<br>}<br></td>