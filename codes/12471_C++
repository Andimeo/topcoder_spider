            #line 5 "TeamContestEasy.cpp"<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;set&gt;<br>using namespace std;<br>bool cmp(int a,int b)<br>{<br>    return a&gt;b;<br>}<br>class TeamContestEasy {<br>  public:<br>  int worstRank(vector &lt;int&gt; strength) {<br>    int a[50], i, j, p, s, l, r, ans = 1;<br>    p = strength[0]+strength[1]+strength[2];<br>    s = strength[0];<br>    s =min(s,strength[1]);<br>    s =min(strength[2],s);<br>    p -=s;<br>    j = 0;<br>    for(i = 3; i&lt; strength.size(); i++)<br>    a[j++] = strength[i];<br>    sort(a,a+j,cmp);<br>    l = 0;<br>    r = (j*2)/3 - 1;<br>    while(l &lt; r)<br>    {<br>        if (a[l] + a[r] &gt; p)<br>        {<br>            ans ++;<br>            l++;<br>            r--;<br>        } else r--;<br>    }<br>    return ans;<br>  }<br><br><br>};<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>