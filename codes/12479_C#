            public class CityMap<br>{<br>  public string getLegend(string[] c, int[] p)<br>  {<br>    int[] ch = new int[26];<br>    for(int i=0;i&lt;c.Length;i++)<br>    {<br>      for(int j=0;j&lt;c[i].Length;j++)<br>      {<br>        if(c[i][j]!='.')<br>        {<br>          ch[c[i][j]-'A']+=1;<br>        }<br>      }<br>    }<br>    string ans = "";<br>    for(int i=0;i&lt;p.Length;i++)<br>    {<br>      for(int j=0;j&lt;26;j++)<br>      {<br>        if(ch[j] == p[i]){<br>          ans += ""+(char)((int)'A'+j);<br>          ch[j] = -1;<br>        }<br>      }<br>    }<br>    return ans;<br>  }<br>}<br></td>