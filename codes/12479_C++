            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br>#include &lt;cstring&gt;<br> <br>using namespace std;<br> <br>#define REP(i,n) for(int(i)=0;(i)&lt;(int)(n);(i)++)<br>#define RREP(i,n) for(int(i)=(n)-1;(i)&gt;=0;(i)--)<br>#define SZ(c) ((int)(c).size())<br>#define ITER(it,c) for(__typeof((c).begin()) it=(c).begin();it!=(c).end();it++)<br>#define FIND(x,c) ((c).find((x))!=(c).end())<br> <br> <br>class CityMap <br>{<br>public:<br>  string getLegend(vector &lt;string&gt;, vector &lt;int&gt;);<br>};<br> <br>string CityMap::getLegend(vector &lt;string&gt; cityMap, vector &lt;int&gt; POIs) <br>{<br>      string ret="";<br>    int a[30] = {0};<br>    bool b[30] = {0};<br>    REP(i,SZ(cityMap))<br>    {<br>        string temp = cityMap[i];<br>        REP(j,SZ(temp))<br>        {<br>            if(temp[j]!='.')<br>                a[temp[j]-'A']++;<br>        }<br>    }<br> <br>    REP(i,SZ(POIs))<br>    {<br>        int j;<br>        for(j=0 ; j&lt;26 ; j++)<br>            if(a[j]==POIs[i] &amp;&amp; !b[j])<br>            {<br>                ret+='A'+j;<br>                b[j]=true;<br>                break;<br>            }<br>    }<br> <br>    return ret;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>