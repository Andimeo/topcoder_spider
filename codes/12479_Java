            import java.util.HashMap;<br><br>public class CityMap {<br><br>  public String getLegend(String[] c, int[] p) {<br>    int[] a = new int[26];<br>    for(String s: c) <br>      for(char i: s.toCharArray())<br>        if(i!='.')a[i-'A']++;<br>    HashMap&lt;Integer, String&gt; map = new HashMap&lt;Integer, String&gt;();<br>    for(int i=0; i&lt;a.length;i++)<br>      map.put(a[i], ""+(char)('A'+i));<br>    String o="";<br>    for(int i: p) o+=map.get(i);<br>    <br>    return o;<br>  }<br><br>}<br></td>