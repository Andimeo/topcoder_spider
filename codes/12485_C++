            #include &lt;iostream&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;numeric&gt;<br>#include &lt;climits&gt;<br>#include &lt;bitset&gt;<br>#include &lt;string&gt;<br>#include &lt;iostream&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;list&gt;<br>#include &lt;cstring&gt;<br>#define mod 1000000007<br>using namespace std;<br>map &lt;long long, long long&gt; m;<br>class TheNumberGameDiv2<br>{<br>    private:<br>        long long a;<br>        long long b;<br>        long long f (long long x)<br>        {<br>            if (x == b) {<br>                return 0;<br>            }<br>            if (m[x] != 0) {<br>                return m[x];<br>            }<br>            long long y = 0,mm;<br>            mm = 100000000LL;<br>            m[x] = mm;<br>            mm = min (mm, 1 + f (x/10));<br>            long long xx = x;<br>            while (x != 0) {<br>                y = y*10 + (x%10);<br>                x = x/10;<br>            }<br>            mm = min (mm, 1 + f(y));<br>            <br>            return m[xx]=mm;<br>        }<br><br>    public:<br>        int minimumMoves(int A, int B)<br>        {<br>            this -&gt; a = A;<br>            this -&gt; b = B;<br>            long long x;<br>            x = f (a);<br>            if (x &gt;= 100000000LL) {<br>                return -1;<br>            }<br>            return x;<br>        }<br>};<br></td>