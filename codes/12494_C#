            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class TheLargestString {<br>    public string find(string s, string t)<br>    {<br>        int len = s.Length;<br>        string[] dp = new string[len + 1];<br>        dp[0] = "";<br>        for (int i = 1; i &lt;= len; i++)<br>        {<br>            dp[i] = new string((char)('a' - 1), i * 2);<br>        }<br><br>        for (int i = 0; i &lt; len; i++)<br>        {<br>            for (int j = len - 1; j &gt;= 0; j--)<br>            {<br>                string ns = dp[j].Substring(0, j) + s[i] + dp[j].Substring(j, j) + t[i];<br>                if (String.CompareOrdinal(ns, dp[j + 1]) &gt; 0) dp[j + 1] = ns;<br>            }<br>        }<br>        string ret = new string((char)('a' - 1), 1);<br>        for (int i = 0; i &lt;= len; i++)<br>        {<br>            if (String.CompareOrdinal(dp[i], ret) &gt; 0) ret = dp[i];<br>        }<br>        return ret;<br><br>    }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] Modified for C# by ysn<br>// Powered by CodeProcessor<br></td>