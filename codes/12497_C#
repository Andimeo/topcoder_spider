            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class TheNumberGameDivTwo {<br>    bool[] wins;<br>    public string find(int n) {<br>        wins = new bool[n+1];<br>        for (int i = 4; i &lt;= n; i++)<br>        {<br>            wins[i] = CanWin(i);<br>        }<br>        <br>        return wins[n]?"John":"Brus";<br>    }<br><br>    private bool CanWin(int n)<br>    {<br>        for (int i = 2; i &lt; n; i++)<br>        {<br>            if (n % i == 0 &amp;&amp; !wins[n - i])<br>                return true;<br>        }<br>        return false;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>