            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class TheSwapsDivOne<br>{<br>  public double find( string[] sequence, int k )<br>  {<br>    string s = "";<br>    foreach ( string s1 in sequence )<br>    {<br>      s += s1;<br>    }<br>    int n = s.Length;<br><br>    double pSelf = 1;<br>    for ( int i = 1; i &lt;= k; ++i )<br>    {<br>      pSelf = pSelf * ( n - 2.0 ) / n + ( 1 - pSelf ) * ( 2.0 / n / ( n - 1.0 ) );<br>    }<br><br>    double res = 0;<br>    for ( int i = 0; i &lt; n; ++i )<br>    {<br>      int val = s[i] - '0';<br>      for ( int j = 0; j &lt; n; ++j )<br>      {<br>        double p;<br>        if ( i == j )<br>        {<br>          p = pSelf;<br>        }<br>        else<br>        {<br>          p = ( 1 - pSelf ) / ( n - 1 );<br>        }<br><br>        res += p * val * ( j + 1 ) * ( n - j );<br>      }<br>    }<br><br>    return res * 2 / n / ( n + 1 );<br>  }<br><br>}<br><br><br> <br>// Powered by moj 4.16 [modified TZTester]<br>// Powered by CleanSharp 1.0<br>// Powered by CodeProcessor<br></td>