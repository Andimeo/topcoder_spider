            import java.util.*;<br><br>public class TheSwapsDivOne {<br>  public double find(String[] sequence, int k) {<br>    StringBuilder sb = new StringBuilder();<br>    for (String s : sequence) {<br>      sb.append(s);<br>    }<br>    String s = sb.toString();<br>    int n = s.length();<br>    <br>    double a = 1;<br>    for (int i = 0; i &lt; k; i++) {<br>      double b = 1 - a;<br>      double aa = a * (1 - 2.0 / n) + b * (2.0 / (n * (n - 1)));<br>      a = aa;<br>    }<br>    <br>    double[] coef = new double[n];<br>    double sum = 0;<br>    for (int i = 0; i &lt; n; i++) {<br>      coef[i] = (i + 1) * (n - i) * 2.0 / (n * (n + 1));<br>      sum += coef[i];<br>    }<br>    double ans = 0;<br>    for (int i = 0; i &lt; n; i++) {<br>      int v = s.charAt(i) - '0';<br>      ans += v * (a * coef[i] + (1 - a) * (sum - coef[i]) / (n - 1)); <br>    }<br>    return ans;<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>