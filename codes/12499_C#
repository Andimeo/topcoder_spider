            public class TheSwapsDivTwo<br>{<br>  public int find(int[] sequence){<br>    bool osnovni = false;<br>    int cnt=0;<br>    for(int i=0;i&lt;sequence.Length-1;i++){<br>      for (int j=i+1;j&lt;sequence.Length;j++){<br>        if (sequence[i]==sequence[j]) osnovni = true;<br>        else cnt++;<br>        <br>      }<br>    }<br>    if (osnovni) cnt+=1;<br>    return cnt;<br>  }<br>}<br></td>