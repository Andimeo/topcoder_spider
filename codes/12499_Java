            public class TheSwapsDivTwo {<br>   public int find(int[] sequence) {<br><br>     int n = sequence.length;<br>     int c =0;<br>     int have =0;<br>     for(int i =0 ; i &lt; n;i++)<br>       for(int j = i+1;j &lt; n;j++)<br>         if(sequence[i] != sequence[j])c++;<br>         else have++;<br>     have = Math.min(have, 1);<br>     c+=have;<br>     return c;<br>   }<br><br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by moj 4.17 [modified TZTester]<br>// Powered by CodeProcessor<br></td>