            import java.util.*;<br>public class UniqueTriangles {<br>  int gcd(int x, int y) {<br>    if(x&lt;y) return gcd(y, x);<br>    if(y==0) return x;<br>    return gcd(y, x%y);<br>  }<br>  public int howMany(int[] x, int[] y) {<br>    TreeSet ts = new TreeSet();<br>    for(int i = 0; i&lt;x.length; i++) <br>    for(int j = i+1; j&lt;x.length; j++)<br>    for(int k = j+1; k&lt;x.length; k++) {<br>      int det = (x[i]-x[j])*(y[i]-y[k]) - (y[i]-y[j])*(x[i]-x[k]);<br>      if(det!=0) {<br>        int sqr1 = (x[i]-x[j])*(x[i]-x[j]) + (y[i]-y[j])*(y[i]-y[j]);<br>        int sqr2 = (x[i]-x[k])*(x[i]-x[k]) + (y[i]-y[k])*(y[i]-y[k]);  <br>        int sqr3 = (x[j]-x[k])*(x[j]-x[k]) + (y[j]-y[k])*(y[j]-y[k]);<br>        if(sqr1&gt;sqr2) {<br>          int z = sqr2;<br>          sqr2 = sqr1;<br>          sqr1 = z;  <br>        }<br>        if(sqr2&gt;sqr3) {<br>          int z = sqr2;<br>          sqr2 = sqr3;<br>          sqr3 = z;  <br>        }<br>        if(sqr1&gt;sqr2) {<br>          int z = sqr2;<br>          sqr2 = sqr1;<br>          sqr1 = z;  <br>        }<br>        int g = gcd(gcd(sqr1, sqr2), sqr3);<br>        sqr1/=g;<br>        sqr2/=g;<br>        sqr3/=g;<br>        ts.add(sqr1+" "+sqr2+" "+sqr3);<br>      }<br>    }<br>    return ts.size();<br>  }<br> <br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>