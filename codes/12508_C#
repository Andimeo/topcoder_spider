            using System;<br>using System.Collections.Generic;<br><br>public class TheExperimentDiv2 <br>{<br>  public int[] determineHumidity(int[] intensity, int L, int[] leftEnd) <br>  {<br>        int[] re = new int[leftEnd.Length];<br><br>        for (int i = 0; i &lt; leftEnd.Length; i++)<br>        {<br>            for (int j = leftEnd[i]; j &lt; leftEnd[i] + L; j++)<br>            {<br>                re[i] += intensity[j];<br>                intensity[j] = 0;<br>            }<br>        }<br>        return re;<br>  }<br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>