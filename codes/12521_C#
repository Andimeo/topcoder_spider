            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class EllysRoomAssignmentsDiv2<br>{<br>  public double getProbability(string[] ratings)<br>  {<br>    int R;<br>    int N;<br>        double probability = 0.0;<br>        string r = "";<br> <br>        for (int i = 0; i &lt; ratings.Length; i++)<br>        {<br>            r = r + ratings[i];<br>        }<br> <br>        string[] registrants = r.Split(new char[] { ' ' });<br>        int EllyRating = int.Parse(registrants[0]);<br> <br>    N = registrants.Length;<br> <br>        if ((N % 20) == 0)<br>    {<br>      R = N / 20;<br>    }<br>    else<br>    {<br>      R = N / 20 + 1;<br>    }<br> <br>        int[] ratingsValues = new int[] {};<br> <br>        Array.Resize(ref ratingsValues, N);<br> <br>        int index = 0;<br> <br>        foreach (string registrant in registrants)<br>        {<br>            ratingsValues[index] = int.Parse(registrant);<br>            index++;<br>        }<br> <br>        Array.Sort(ratingsValues);<br>        Array.Reverse(ratingsValues);<br>        <br>        int EllyRanking = -1;<br> <br>        for (int i = 0; i &lt; ratingsValues.Length; i++)<br>        {<br>            if (ratingsValues[i] == EllyRating)<br>            {<br>                EllyRanking = i + 1;<br>            }<br>        }<br> <br>    if ((N &lt;= 20) || (EllyRanking == 1))<br>    {<br>      probability = 1.0;<br>    }<br>        // If Elly is in the top R scores, then probablility is 0<br>        else if (EllyRanking &lt;= R)<br>        {<br>            probability = 0.0;<br>        }<br>        else<br>        {<br>            probability = 1.0 / R;<br>        }<br>    <br>        return probability;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>