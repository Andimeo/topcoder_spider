            #include &lt;algorithm&gt;<br>#include &lt;cassert&gt;<br>#include &lt;sstream&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;iostream&gt;<br>using namespace std;<br> <br>class EllysRoomAssignmentsDiv2<br>{<br>public:<br> <br>double getProbability(vector &lt;string&gt; ratings)<br>{<br>  string rating;<br>  for (int i = 0; i &lt; ratings.size(); ++i)<br>    rating += ratings[i];<br>  istringstream iss(rating);<br>  vector&lt;int&gt; regs;<br>  int member;<br>  while (iss &gt;&gt; member) regs.push_back(member);<br>  int elly = regs[0];<br>  sort(regs.begin(), regs.end(), greater&lt;int&gt;());<br>  if (regs[0] == elly) return 1.0;<br> <br>  int pos = 0;<br>  for (; pos &lt; regs.size() &amp;&amp; regs[pos] &gt; elly; ++pos) ;<br>  assert(regs[pos] == elly);<br>  int N = regs.size();<br>  int R = (N + 19) / 20; cout &lt;&lt; N &lt;&lt; ' ' &lt;&lt; R &lt;&lt; endl;<br> <br>  if (pos &lt; R) return 0.0;<br>  <br>  return 1.0 / R;<br>}<br> <br>};<br></td>