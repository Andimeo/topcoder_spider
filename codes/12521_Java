            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class EllysRoomAssignmentsDiv2<br>{<br>  public double getProbability(String[] ratings)<br>  {<br>    StringBuilder sb=new StringBuilder();<br>    for(String s:ratings)sb.append(s);<br>    <br>    String[]str=sb.toString().split(" ");<br>    int[]rat=new int[str.length];<br>    for(int i=0;i&lt;rat.length;i++){<br>      rat[i]=Integer.parseInt(str[i]);<br>    }<br>    int el=rat[0];<br>    <br>    <br>    <br>    int n=rat.length;<br>    Arrays.sort(rat);<br>    <br>    int num=-1;<br>    <br>    for(int i=n;i--&gt;0;){<br>      if(rat[i]==el){<br>        num=n-i;<br>        break;<br>      }<br>    }<br>    <br>    int r=0;<br>    if(n%20==0){<br>      r=n/20;<br>    }else{<br>      r=n/20+1;<br>    }<br>    <br>    <br>    //System.out.println(num);<br>    if(r==1)return 1.0;<br>    if(num==1)return 1.0;<br>    if(num&gt;r){<br>      return (double)1/r;<br>    }else return 0.0;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>