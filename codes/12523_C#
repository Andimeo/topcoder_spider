            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class UndoHistory<br>{<br>    public int minPresses(string[] lines)<br>    {<br>        List&lt;string&gt; undos = new List&lt;String&gt;();<br>        undos.Add("");<br>        string text = "";<br>        int res = 0;<br>        foreach (string s in lines)<br>        {<br>            int teres = 999999999;<br>            if (s.StartsWith(text))<br>            {<br>                teres = s.Length - text.Length;<br>            }<br>            foreach (string t in undos)<br>            {<br>                if (s.StartsWith(t))<br>                {<br>                    teres = Math.Min(teres, s.Length - t.Length + 2);<br>                }<br>            }<br>            res += teres + 1;<br>            for (int i = 0; i &lt;= s.Length; i++)<br>            {<br>                undos.Add(s.Substring(0, i));<br>            }<br>            text = s;<br>        }<br>        return res;<br>    }<br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>