            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class UndoHistory<br>{<br>  public int minPresses(String[] lines)<br>  {<br>    HashSet&lt;String&gt; prefixes = new HashSet&lt;String&gt;();<br>    int ans = 0;<br>    String prev = "";<br>    for (String s : lines) {<br>      int cur = s.length() + 3;<br>      if (s.startsWith(prev)) {<br>        cur = s.length() - prev.length() + 1;<br>      }<br>      for (int i = 1; i &lt;= s.length(); ++i) {<br>        if (prefixes.contains(s.substring(0, i))) {<br>          cur = Math.min(cur, s.length() - i + 3);<br>        }<br>        prefixes.add(s.substring(0, i));<br>      }<br>      prev = s;<br>      ans += cur;<br>    }<br>    return ans;<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.8 (beta) modified by pivanof!<br></td>