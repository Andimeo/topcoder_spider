            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class PrimalUnlicensedCreatures<br>{<br>  public int maxWins(int initialLevel, int[] grezPower)<br>  {<br>    int res = 0;<br>            Array.Sort(grezPower);<br>            for (int i = 0; i &lt; grezPower.Length; i++)<br>            {<br>                if (initialLevel &gt; grezPower[i])<br>                {<br>                    initialLevel += (grezPower[i] / 2);<br>                    res++;<br>                }<br>            }<br>            return res;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.8 (beta) modified by pivanof!<br></td>