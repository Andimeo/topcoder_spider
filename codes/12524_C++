            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br>#include &lt;string&gt;<br>#include &lt;cstring&gt;<br> <br>using namespace std;<br> <br>class PrimalUnlicensedCreatures {<br>public:<br>  int maxWins(int, vector &lt;int&gt;);<br>};<br> <br>int PrimalUnlicensedCreatures::maxWins(int il, vector &lt;int&gt; gp) {<br>  <br>  sort(gp.begin(),gp.end());<br>  int c=0;<br>  <br>  for (int i=0;i&lt;gp.size();i++)<br>  {<br>    if (il &gt; gp[i])<br>    {<br>      c++;<br>      il+=gp[i]/2;<br>    }<br>    else<br>      break;<br>  }<br>  return c;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>