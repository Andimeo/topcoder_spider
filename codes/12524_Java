            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br>public class PrimalUnlicensedCreatures {<br>  public int maxWins(int initialLevel, int[] grezPower) {<br>    Arrays.sort(grezPower);<br>    int n = grezPower.length;<br>    int i = 0;<br>    for (i = 0; i &lt; n; i++) {<br>      if (initialLevel &gt; grezPower[i]) {<br>        initialLevel += grezPower[i] / 2;<br>      } else<br>        return i;<br>    }<br> <br>    return i;<br>  }<br> <br> <br>}<br>// Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>