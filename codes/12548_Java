            import java.util.*;<br>import static java.lang.Math.*;<br>import java.io.*;<br> <br>public class DancingFoxes {<br>  public static void p(Object... o){System.out.println(Arrays.deepToString(o));}<br>  public int minimalDays(String[] F) {<br>    int N = F.length;<br>    int[][] D = new int[N][N];<br>    for (int[] d:D)<br>      Arrays.fill(d, 1000);<br>    for (int i = 0; i &lt; N; i++)<br>      D[i][i] = 0;<br>    for (int i = 0; i &lt; N ;i++)<br>      for (int j = 0; j &lt; N; j++)<br>        if (F[i].charAt(j) == 'Y')<br>          D[i][j] = 1;<br> <br>    for (int i = 0; i &lt; N; i++) {<br>      for (int a = 0; a &lt; N; a++) {<br>        for (int b = 0; b &lt; N; b++) {<br>          D[a][b] = min(D[a][b], D[a][i]+D[i][b]);<br>        }<br>      }<br>    }<br> <br>    int d = D[0][1];<br>    if (d &gt;= 1000)<br>      return -1;<br> <br>    int ans = 0;<br>    while (d &gt; 1) {<br>      int num = (d+1)/3;<br>      ans++;<br>      d -= num;<br>    }<br>    return ans;<br>  }<br> <br>}<br></td>