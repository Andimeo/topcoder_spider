            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br>using kp.Algo;<br><br>public class EelAndRabbit<br>{<br>  public int getmax( int[] l, int[] t )<br>  {<br>    int res = 0;<br>    for ( int i = 0; i &lt; t.Length; ++i )<br>      for ( int j = 0; j &lt; t.Length; ++j )<br>      {<br>        int cur = 0;<br>        if ( t[i] == t[j] )<br>        {<br>          for ( int k = 0; k &lt; t.Length; ++k )<br>            if ( (long)t[i] - (long)t[k] &gt;= 0 &amp;&amp; (long)t[i] - (long)t[k] - (long)l[k] &lt;= 0 )<br>              ++cur;<br>        }<br>        else<br>        {<br>          for ( int k = 0; k &lt; t.Length; ++k )<br>            if ( ( (long)t[i] - (long)t[k] &gt;= 0 &amp;&amp; (long)t[i] - (long)t[k] - (long)l[k] &lt;= 0 )<br>                || ( (long)t[j] - (long)t[k] &gt;= 0 &amp;&amp; (long)t[j] - (long)t[k] - (long)l[k] &lt;= 0 ) )<br>              ++cur;<br>        }<br>        res = Math.Max( res, cur );<br>      }<br>    return res;<br>  }<br><br>}<br><br>namespace kp.Algo { }<br><br><br>// Powered by FileEdit<br>// Powered by moj 4.16 [modified TZTester]<br>// Powered by CleanSharp 1.0<br>// Powered by CodeProcessor<br></td>