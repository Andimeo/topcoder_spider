<td class="problemText" colspan="8" valign="middle" align="left">
            import java.util.HashSet;<br> <br>/**<br> * Built using CHelper plug-in<br> * Actual solution is at the top<br> * @author ilyakor<br> */<br>public class EelAndRabbit {<br>    public int getmax(int[] l, int[] t) {<br>        int n = l.length;<br>        int res = 0;<br>        HashSet&lt;Integer&gt; moments = new HashSet&lt;Integer&gt;();<br>        for (int i = 0; i &lt; n; ++i) {<br>            moments.add(t[i]);<br>            moments.add(t[i] + l[i]);<br>        }<br>        for (int x : moments) {<br>            for (int y : moments) {<br>                int cur = 0;<br>                for (int i = 0; i &lt; n; ++i) {<br>                    if (t[i] &lt;= x &amp;&amp; t[i] + l[i] &gt;= x)<br>                        ++cur;<br>                    else if (t[i] &lt;= y &amp;&amp; t[i] + l[i] &gt;= y)<br>                        ++cur;<br>                }<br>                res = Math.max(res, cur);<br>            }<br>        }<br>        return res;<br>    }<br>}<br></td>