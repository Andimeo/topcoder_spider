            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class ShoutterDiv1 {<br>public:<br>  int count(vector &lt;string&gt;, vector &lt;string&gt;, vector &lt;string&gt;, vector &lt;string&gt;, vector &lt;string&gt;, vector &lt;string&gt;, vector &lt;string&gt;, vector &lt;string&gt;);<br>};<br> <br>const int inf = 42424242;<br> <br>int s[3333], t[3333], f[3333], a[3333], res[3333];<br> <br>int ShoutterDiv1::count(vector &lt;string&gt; s1000, vector &lt;string&gt; s100, vector &lt;string&gt; s10, vector &lt;string&gt; s1, vector &lt;string&gt; t1000, vector &lt;string&gt; t100, vector &lt;string&gt; t10, vector &lt;string&gt; t1) {<br>  int n = 0;<br>  for (int i=0;i&lt;s1000.size();i++)<br>    for (int j=0;j&lt;s1000[i].length();j++) {<br>      s[n] = (s1000[i][j]-48)*1000 + (s100[i][j]-48)*100 + (s10[i][j]-48)*10 + (s1[i][j]-48)*1;<br>      t[n] = (t1000[i][j]-48)*1000 + (t100[i][j]-48)*100 + (t10[i][j]-48)*10 + (t1[i][j]-48)*1;<br>      n++;<br>    }<br>  for (int i=0;i&lt;n;i++) res[i] = 0;<br>  {<br>    for (int i=0;i&lt;n;i++) a[i] = i;<br>    for (int i=0;i&lt;n;i++)<br>      for (int j=i+1;j&lt;n;j++)<br>        if (t[a[i]] &lt; t[a[j]]) {<br>          int tmp = a[i];<br>          a[i] = a[j];<br>          a[j] = tmp;<br>        }<br>    int rs = s[0];<br>    for (int i=0;i&lt;n;i++)<br>      if (s[i] &gt; rs) rs = s[i];<br>    for (int i=0;i&lt;n;i++)<br>      if (t[a[i]] &gt;= rs) f[i] = 0; else {<br>        f[i] = inf;<br>        for (int j=0;j&lt;i;j++)<br>          if (s[a[j]] &lt;= t[a[i]])<br>            if (f[j]+1 &lt; f[i]) f[i] = f[j]+1;<br>        if (f[i] == inf) return -1;<br>      }<br>    for (int i=0;i&lt;n;i++) res[a[i]] += f[i];<br>  }<br>  {<br>    for (int i=0;i&lt;n;i++) a[i] = i;<br>    for (int i=0;i&lt;n;i++)<br>      for (int j=i+1;j&lt;n;j++)<br>        if (s[a[i]] &gt; s[a[j]]) {<br>          int tmp = a[i];<br>          a[i] = a[j];<br>          a[j] = tmp;<br>        }<br>    int lt = t[0];<br>    for (int i=0;i&lt;n;i++)<br>      if (t[i] &lt; lt) lt = t[i];<br>    for (int i=0;i&lt;n;i++)<br>      if (s[a[i]] &lt;= lt) f[i] = 0; else {<br>        f[i] = inf;<br>        for (int j=0;j&lt;i;j++)<br>          if (t[a[j]] &gt;= s[a[i]])<br>            if (f[j]+1 &lt; f[i]) f[i] = f[j]+1;<br>        if (f[i] == inf) return -1;<br>      }<br>    for (int i=0;i&lt;n;i++) res[a[i]] += f[i];<br>  }<br>  int ans = 0;<br>  for (int i=0;i&lt;n;i++) {<br>    int cur = res[i];<br>    for (int j=0;j&lt;n;j++)<br>      if (s[j] &lt;= t[i] &amp;&amp; s[i] &lt;= t[j])<br>        if (res[j]+1 &lt; cur) cur = res[j]+1;<br>    ans += cur;<br>  }<br>  return ans;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>