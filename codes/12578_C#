            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections.Generic;<br><br>public class ShoutterDiv2 {<br>    public int count(int[] s, int[] t) {<br>        int res = 0;<br>        int l = s.Length;<br>        for (int i = 0; i &lt; l; i++)<br>        {<br>            for (int j = 0; j &lt; l; j++)<br>            {<br>                if (i != j)<br>                {<br>                    if (s[i] &gt; t[j] || s[j] &gt; t[i])<br>                    {<br><br>                    }<br>                    else<br>                    {<br>                        res++;<br>                    }<br>                }<br>            }<br>        }<br>        return res / 2;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>