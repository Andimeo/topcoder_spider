            #include &lt;iostream&gt;<br>#include &lt;vector&gt;<br><br>using namespace std;<br><br>class ShoutterDiv2{<br><br>    public:<br>    int count(vector &lt;int&gt; s, vector &lt;int&gt; t){<br>        int a = 0;<br>        for(int i =0; i &lt; s.size();i++){<br>            for(int j = i+1;j&lt;s.size();j++){<br>                if(s[i] &gt;= s[j] &amp;&amp; t[j] &gt;= s[i])<br>                    a++;<br>                if(s[i] &lt; s[j] &amp;&amp; t[i] &gt;= s[j])<br>                    a++;<br>            }<br><br>    }<br>    return a;<br>}<br>};<br></td>