            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class ShoutterDiv2<br>{<br>  public int count(int[] s, int[] t)<br>  {<br>    int countw=0;<br>    for(int i=0;i&lt;s.length;i++) {<br>      for(int j=i+1;j&lt;s.length;j++) {<br>        int time1=Math.max(s[i],s[j]);<br>        int time2=Math.min(t[i],t[j]);<br>        if(time1&lt;=time2)countw++;<br>      }<br>    }<br>    return countw;<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.8 (beta) modified by pivanof!<br></td>