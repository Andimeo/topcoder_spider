<td class="problemText" colspan="8" valign="middle" align="left">
            public class WallGameDiv2<br>{<br>  private static final int HUGE = 1000000;<br><br>  public int play(String[] costs)<br>  {<br>    int h = costs.length;<br>    int w = costs[0].length();<br>    <br>    int icosts[][] = new int[h][w];<br>    for(int i=0; i&lt;h; i++)<br>      for(int j=0; j&lt;w; j++)<br>        if(costs[i].charAt(j)=='x')<br>          icosts[i][j] = HUGE;<br>        else<br>          icosts[i][j] = costs[i].charAt(j)-'0';<br>    <br>    int optprice[][] = new int[h][w];<br>    <br>    optprice[0][0] = 0;<br>    for(int i=1; i&lt;w; i++)<br>      optprice[0][i] = HUGE;<br>      <br>    for(int i=1; i&lt;h; i++)<br>      for(int j=0; j&lt;w; j++)<br>      {<br>        optprice[i][j] = HUGE;<br>        for(int ent=0; ent&lt;w; ent++)<br>        {<br>          int addval = 0;<br>          if(ent&gt;j)<br>            for(int ii=j; ii&lt;=ent; ii++)<br>              addval += (icosts[i-1][ii]);<br>          else<br>            for(int ii=ent; ii&lt;=j; ii++)<br>              addval += (icosts[i-1][ii]);<br>          int tryval = addval + optprice[i-1][ent];<br>          if(tryval &lt; HUGE)<br>          {<br>            if(optprice[i][j] == HUGE || tryval &gt; optprice[i][j])<br>              optprice[i][j] = tryval;<br>          }<br>        }<br>      }<br>    int res = -1;<br>    for(int j=0; j&lt;w; j++)<br>    {<br>      int tryval = optprice[h-1][j]+icosts[h-1][j];<br>      if(tryval &lt; HUGE &amp;&amp; tryval &gt; res)<br>        res = tryval;<br>    }<br>    return res;<br>  }<br>}<br></td>