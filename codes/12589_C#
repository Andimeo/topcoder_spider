            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class BlackAndWhiteSolitaire<br>{<br>  public int minimumTurns(string cardFront)<br>  {<br>        int n = cardFront.Length;<br> <br>        int cnt1 = 0, cnt2 = 0;<br>        for (int i = 0; i &lt; n; i++)<br>        {<br>            if (i % 2 == 0 &amp;&amp; cardFront[i] == 'B') cnt1++;<br>            else if (i % 2 == 1 &amp;&amp; cardFront[i] == 'W') cnt1++;<br> <br>            if (i % 2 == 0 &amp;&amp; cardFront[i] == 'W') cnt2++;<br>            else if (i % 2 == 1 &amp;&amp; cardFront[i] == 'B') cnt2++;<br>        }<br>        return Math.Min(cnt1, cnt2);<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.8 (beta) modified by pivanof!<br></td>