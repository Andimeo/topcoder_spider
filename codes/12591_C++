            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class YetAnotherANDProblem {<br>public:<br>  string test(vector &lt;int&gt;, int, vector &lt;int&gt;);<br>};<br> <br>string YetAnotherANDProblem::test(vector &lt;int&gt; X, int K, vector &lt;int&gt; q) {<br>  int n = q.size(), m = X.size();<br>  string ret = "";<br>  if (K == 1) {<br>    for (int u=0;u&lt;n;u++) {<br>      char ans = '-';<br>      for (int i=0;i&lt;m;i++)<br>        for (int j=i+1;j&lt;m;j++)<br>          if ((X[i] &amp; X[j]) == q[u]) ans = '+';<br>      ret += ans;<br>    }<br>    return ret;<br>  }<br>  for (int i=0;i&lt;n;i++) ret += "-";<br>  for (int t=0;t&lt;(1&lt;&lt;m);t++) {<br>    int res = -1, cnt = 0;<br>    for (int i=0;i&lt;m;i++)<br>      if (t &amp; (1 &lt;&lt; i)) {<br>        res &amp;= X[i];<br>        cnt++;<br>      }<br>    if (cnt &gt;= 3 &amp;&amp; cnt &lt;= (1LL &lt;&lt; K))<br>      for (int i=0;i&lt;n;i++)<br>        if (q[i] == res) ret[i] = '+';<br>  }<br>  return ret;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>