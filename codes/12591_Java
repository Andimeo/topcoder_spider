            import static java.util.Arrays.sort;<br>import java.util.*;<br> <br>public class YetAnotherANDProblem {<br>  <br>  public String test(int[] X, int K, int[] queries) {<br>    int[] list = getAll(X, K);<br>    char[] ret = new char[queries.length];<br>    for (int i = 0; i &lt; queries.length; i++) {<br>      boolean ok = Arrays.binarySearch(list, queries[i]) &gt;= 0;<br>      ret[i] = ok?'+':'-';<br>    }<br>    return new String(ret);<br>  }<br> <br>  private int[] getAll(int[] x, int k) {<br>    int n = x.length;<br>    long high = 1L &lt;&lt; k;<br>    long low = 3;<br>    if (k == 1) {<br>      high = 2;<br>      low = 2;<br>    }<br>    List&lt;Integer&gt; list = new ArrayList&lt;Integer&gt;();<br>    for (int mask = 0; mask &lt; 1 &lt;&lt; n; mask++) {<br>      int cnt = Integer.bitCount(mask);<br>      if (cnt &lt; low ||cnt &gt; high) {<br>        continue;<br>      }<br>      int ok = ~0;<br>      for (int i = 0; i &lt; n; i++) {<br>        if((mask &amp; (1&lt;&lt;i)) != 0) {<br>          ok &amp;= x[i];<br>        }<br>      }<br>      list.add(ok);<br>    }<br>    int[] arr = new int[list.size()];<br>    for (int i = 0; i &lt; arr.length; i++) {<br>      arr[i] = list.get(i);<br>    }<br>    sort(arr);<br>    return arr;<br>  }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>