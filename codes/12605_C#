            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class SpaceWarDiv2<br>{<br>  public int minimalFatigue(int[] magicalGirlStrength, int[] enemyStrength, int[] enemyCount)<br>  {<br>        Array.Sort(magicalGirlStrength);<br>        Array.Sort(enemyStrength,enemyCount);<br>        Array.Reverse(magicalGirlStrength);<br>       <br>        Array.Reverse(enemyStrength);<br>        Array.Reverse(enemyCount);<br>        int duz1 = magicalGirlStrength.Length;<br>        int[] umor = new int[duz1];<br>        int duz2 = enemyStrength.Length;<br>        if (enemyStrength[0] &gt; magicalGirlStrength[0]) return -1;<br> <br>        for (int i = 0; i &lt; duz2; i++)<br>        {<br>            int min = duz1;<br>            for (int j = 0; j &lt; duz1; j++)<br>            {<br>                if (magicalGirlStrength[j] &lt; enemyStrength[i]) { min = j; break; }<br>            }<br>          <br>            for (int k = 0; k &lt; enemyCount[i]; k++)<br>            {<br>                <br>                int min2 = 0;<br>                int vrijednost = umor[0];<br>                for (int j = 1; j &lt; min; j++)<br>                {<br>                    if (umor[j] &lt; vrijednost)<br>                    {<br>                        vrijednost = umor[j];<br>                        min2 = j;<br>                    }<br>                }<br>                umor[min2] += 1;<br>            }<br>        }<br>        Array.Sort(umor);<br>        Array.Reverse(umor);<br> <br>            return umor[0];<br>  }<br> <br> <br>}<br> <br>//Powered by KawigiEdit 2.1.8 (beta) modified by pivanof!<br></td>