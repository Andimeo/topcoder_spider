            import java.util.Arrays;<br><br>public class SpaceWarDiv2 {<br><br>  public int minimalFatigue(int[] magicalGirlStrength, int[] enemyStrength,<br>      int[] enemyCount) {<br>    int[] enemy = new int[101];<br>    int count = 0;<br>    for (int i = 0; i &lt; enemyStrength.length; i++) {<br>      enemy[enemyStrength[i]] += enemyCount[i];<br>      count += enemyCount[i];<br>    }<br>    int F = 0;<br>    Arrays.sort(magicalGirlStrength);<br>    boolean have = false;<br>    while (true) {<br>      have = false;<br>      out: for (int i = magicalGirlStrength.length - 1; i &gt;= 0; i--) {<br>        for (int j = 100; j &gt; 0; j--) {<br>          if (enemy[j] &gt; 0 &amp;&amp; magicalGirlStrength[i] &gt;= j) {<br>            enemy[j]--;<br>            count--;<br>            have = true;<br>            continue out;<br>          }<br>        }<br>      }<br>      F++;<br>      if (count == 0) {<br>        return F;<br>      }<br>      if (!have &amp;&amp; count &gt; 0) {<br>        return -1;<br>      }<br><br>    }<br>  }<br><br>}<br></td>