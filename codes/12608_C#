            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class TravelOnMars {<br>    public int minTimes(int[] range, int startCity, int endCity)<br>    {<br>        int n = range.Length;<br>        int[] dp = new int[n];<br>        int MAX = 999999;<br>        for (int i = 0; i &lt; n; i++)<br>        {<br>            dp[i] = MAX;<br>        }<br>        dp[startCity] = 0;<br>        for (int k = 0; k &lt; n; k++)<br>        {<br>            for (int i = 0; i &lt; n; i++)<br>            {<br>                for (int j = -range[i]; j &lt;= range[i]; j++)<br>                {<br>                    int next = ((i + j) % n + n) % n;<br>                    dp[next] = Math.Min(dp[next], dp[i] + 1);<br>                }<br>            }<br>        }<br>        return dp[endCity];<br>    }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] Modified for C# by ysn<br>// Powered by CodeProcessor<br></td>