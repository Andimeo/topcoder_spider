            import java.util.*;<br><br>public class TravelOnMars {<br><br>  public int minTimes(int[] range, int startCity, int endCity) {<br>    int n = range.length;    <br>    int[][] d = new int[n][n];<br>    final int infinity = 1000000;<br>    for (int[] arr : d) {<br>      Arrays.fill(arr, infinity);<br>    }<br>    for (int i = 0; i &lt; n; i++) {<br>      for (int j = -range[i]; j &lt;= range[i]; j++) {<br>        int k = (i + j) % n;<br>        if (k &lt; 0) k += n;<br>        d[i][k] = 1;<br>      }<br>    }<br>    for (int i = 0; i &lt; n; i++) {<br>      d[i][i] = 0;<br>    }<br>    for (int k = 0; k &lt; n; k++) {<br>      for (int i = 0; i &lt; n; i++) {<br>        for (int j = 0; j &lt; n; j++) {<br>          d[i][j] = Math.min(d[i][j], d[i][k] + d[k][j]);<br>        }<br>      }      <br>    }<br>    return d[startCity][endCity];<br>  }<br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>