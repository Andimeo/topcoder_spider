            using System;<br>using System.Collections.Generic;<br><br>public class SwappingDigits {<br>    public string minNumber(string num) {<br>        string result = num;<br><br>        char[] a = num.ToCharArray();<br>        for (int i = 0; i &lt; num.Length; i++)<br>        {<br>            for (int j = i + 1; j &lt; num.Length; j++)<br>            {<br>                char tmp = a[i];<br>                a[i] = a[j];<br>                a[j] = tmp;<br><br>                if (a[0] != '0' &amp;&amp; IsLess(a, result))<br>                {<br>                    result = new string(a);<br>                }<br><br>                tmp = a[i];<br>                a[i] = a[j];<br>                a[j] = tmp;<br>            }<br>        }<br><br>            return result;<br>    }<br><br>    private bool IsLess(char[] a, string result)<br>    {<br>        for (int i = 0; i &lt; a.Length; i++)<br>        {<br>            if (a[i] &lt; result[i])<br>            {<br>                return true;<br>            }<br>            else if (a[i] &gt; result[i])<br>            {<br>                return false;<br>            }<br>        }<br><br>        return false;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>