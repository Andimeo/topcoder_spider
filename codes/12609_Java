            import java.util.*;<br>public class SwappingDigits {<br>   public String minNumber(String nu) {<br>      String r = nu;<br>      int n = nu.length();<br>      for(int i = 0; i &lt; n; ++i) <br>         for(int j = i + 1; j &lt; n; ++j) {<br>            char[] c = nu.toCharArray(); <br>            if(c[j] == '0' &amp;&amp; i == 0) continue;<br>            char t = c[i];<br>            c[i] = c[j];<br>            c[j] = t;<br>            if(new String(c).compareTo(r) &lt; 0)<br>               r = new String(c);<br>         }<br>      return r;<br>   }<br>}<br></td>