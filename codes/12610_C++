str(14,3);<br>    //cout &lt;&lt; reg &lt;&lt; " " &lt;&lt; date &lt;&lt; " " &lt;&lt; seq &lt;&lt; endl;<br>    int chksum = 99;<br>    if(id[17] == 'X') chksum= 10;<br>    else chksum = id[17] - '0';<br>    int rf = 0, df = 0, sf = 0,cf = 0;<br>    f(i,0,rc.sz) {<br>        if(reg == rc[i]){<br>                rf = 1;<br>                break;<br>        }<br>    }<br>        int yr = toint(date.substr(0,4));<br>        int mn = toint(date.substr(4,2));<br>        int dd = toint(date.substr(6,2));<br>        if(yr &lt; 1900 || yr &gt; 2011) re "Invalid";<br>        if(valid_date(yr,mn,dd)) df = 1;<br>        string zero("000");<br>        int sq = toint(seq);<br>        if(seq == zero) re "Invalid";<br>        if(sq%2==1) sf = 1;<br>        else if(sq%2 == 0) sf = 2;<br>        ll cal = 0;<br>        f(i,1,18) {<br>            cal += (pw[i]*(id[17-i]-'0'));<br>        }<br>        cal += chksum;<br>        if(cal%11 == 1) cf = 1;<br>        if(rf &gt; 0 &amp;&amp; df &gt; 0 &amp;&amp; sf &gt; 0 &amp;&amp; cf &gt; 0) {<br>            if(sf == 1) re "Male";<br>            if(sf == 2) re "Female";<br>        }<br>        else re "Invalid";<br>  }<br>};<br><br><br><br><br>// Powered by FileEdit<br>// Powered by TesterDream 1.2.4 by fushar (December 19 2012)<br>// Powered by CodeProcessor<br></td>