            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class LittleElephantAndXor<br>{<br>  int lim, a, b;<br>  long[][] dp = new long[32][8];<br>  <br>  long dpWork(){<br>    for(int i = 0; i &lt; 32; i ++)<br>      for(int j = 0; j &lt; 8; j ++)<br>        dp[i][j] = 0;<br>    dp[31][7] = 1;<br>    for(int i = 31; i &gt; 0; i --){<br>      for(int j = 0; j &lt; 8; j ++){<br>        boolean ah, bh, ch;<br>        ah = ((j &gt;&gt; 0) &amp; 1) == 1;<br>        bh = ((j &gt;&gt; 1) &amp; 1) == 1;<br>        ch = ((j &gt;&gt; 2) &amp; 1) == 1;<br>        for(int s = 0; s &lt; 2; s ++){<br>          for(int k = 0; k &lt; 2; k ++){<br>            if(ah &amp;&amp; s &gt; ((a &gt;&gt; (i - 1)) &amp; 1))<br>              continue;<br>            if(bh &amp;&amp; k &gt; ((b &gt;&gt; (i - 1)) &amp; 1))<br>              continue;<br>            int nx = s ^ k;<br>            if(ch &amp;&amp; nx &gt; ((lim &gt;&gt; (i - 1)) &amp; 1))<br>              continue;<br>            int t = 0;<br>            if(ah &amp;&amp; s == ((a &gt;&gt; (i - 1)) &amp; 1))<br>              t = t | 1;<br>            if(bh &amp;&amp; k == ((b &gt;&gt; (i - 1)) &amp; 1))<br>              t = t | (1 &lt;&lt; 1);<br>            if(ch &amp;&amp; nx == ((lim &gt;&gt; (i - 1)) &amp; 1))<br>              t = t | (1 &lt;&lt; 2);<br>            dp[i - 1][t] += dp[i][j];<br>          }<br>        }<br>      }<br>    }<br>    long ans = 0;<br>    for(int i = 0; i &lt; 8; i ++)<br>      ans += dp[0][i];<br>    return ans;<br>  }<br>  <br>  public long getNumber(int A, int B, int C)<br>  {<br>    lim = C;<br>    a = A;<br>    b = B;<br>    return dpWork();<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>