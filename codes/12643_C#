            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class TopFox<br>{<br>  public int possibleHandles(string familyName, string givenName)<br>  {<br>    List&lt;string&gt; ss=new List&lt;string&gt;();<br>    for(int i=1;i&lt;=familyName.Length;i++)<br>    {<br>      for(int j=1;j&lt;=givenName.Length;j++)<br>      {<br>        string kk=familyName.Substring(0,i)+givenName.Substring(0,j);<br>        if(!ss.Contains(kk))ss.Add(kk);<br>      }<br>    }<br>    return ss.Count;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>