            #include&lt;iostream&gt;<br>#include&lt;cstdio&gt;<br>#include&lt;set&gt;<br>using namespace std;<br>class TopFox<br>{<br>public :<br>    int possibleHandles(string familyName,string givenName)<br>    {<br>        int f = familyName.length(),g=givenName.length();<br>        set&lt;string&gt; S;<br>        string s1="",s2="";<br>        for(int i=0;i&lt;f;++i)<br>        {<br>            s1 += familyName[i];<br>            for(int j=0;j&lt;g;++j)<br>            {<br>                s2 += givenName[j];<br>                S.insert(s1+s2);<br>            }<br>            s2 = "";<br>        }<br>        return (int)S.size();<br>    }<br>};<br></td>