            /**<br> * Built using CHelper plug-in<br> * Actual solution is at the top<br> */<br>public class History {<br>    static final int INF = (int) 1e9;<br> <br>    public String verifyClaims(String[] dynasties, String[] battles, String[] queries) {<br>        int n = dynasties.length;<br>        int[][] years = new int[n][];<br>        for (int i = 0; i &lt; n; ++i) {<br>            String[] part = dynasties[i].split(" ", -1);<br>            years[i] = new int[part.length];<br>            for (int j = 0; j &lt; part.length; ++j) {<br>                years[i][j] = Integer.parseInt(part[j]);<br>            }<br>        }<br>        int[][] minDelta = new int[n][n];<br>        for (int i = 0; i &lt; n; ++i)<br>            for (int j = 0; j &lt; n; ++j) {<br>                if (i == j) minDelta[i][j] = 0; else minDelta[i][j] = -INF;<br>            }<br>        StringBuilder bb = new StringBuilder();<br>        for (String x : battles) bb.append(x);<br>        String[] bpart = bb.toString().split(" ", -1);<br>        for (String bp : bpart) {<br>            int a = bp.charAt(0) - 'A';<br>            int ai = bp.charAt(1) - '0';<br>            int b = bp.charAt(3) - 'A';<br>            int bi = bp.charAt(4) - '0';<br>            minDelta[a][b] = Math.max(minDelta[a][b], years[b][bi] - years[a][ai + 1] + 1);<br>            minDelta[b][a] = Math.max(minDelta[b][a], years[a][ai] - years[b][bi + 1] + 1);<br>        }<br>        for (int k = 0; k &lt; n; ++k)<br>            for (int i = 0; i &lt; n; ++i)<br>                for (int j = 0; j &lt; n; ++j)<br>                    if (minDelta[i][k] &gt; -INF &amp;&amp; minDelta[k][j] &gt; -INF)<br>                        minDelta[i][j] = Math.max(minDelta[i][j], minDelta[i][k] + minDelta[k][j]);<br>        String res = "";<br>        for (String bp : queries) {<br>            int a = bp.charAt(0) - 'A';<br>            int ai = bp.charAt(1) - '0';<br>            int b = bp.charAt(3) - 'A';<br>            int bi = bp.charAt(4) - '0';<br>            if (years[b][bi] - years[a][ai + 1] + 1 &lt;= -minDelta[b][a] &amp;&amp; years[a][ai] - years[b][bi + 1] + 1 &lt;= -minDelta[a][b]) {<br>                res += "Y";<br>            } else {<br>                res += "N";<br>            }<br>        }<br>        return res;<br>    }<br>}<br></td>