            public class StringWeightDiv2<br>{<br>  public int countMinimums( int l )<br>  {<br>    long result = 1;<br><br>    if( l &lt;= 26 )<br>    {<br>      for( int i = 0; i &lt; l; i++ )<br>      {<br>        result = Mult( result, 26 - i );<br>      }<br>    }<br>    else<br>    {<br>      result = 26;<br>      for( int i = 0; i &lt; 25; i++ )<br>      {<br>        result = Mult( result, l - 1 - i );<br>      }<br>    }<br><br>    return (int)result;<br>  }<br><br>  private long Mult( long a, long b )<br>  {<br>    return ( a * b ) % 1000000009L;<br>  }<br>}<br></td>