            import java.util.*;<br>public class TeamsSelection {<br>    public String simulate(int[] preference1, int[] preference2) {<br>        char res[]=new char[preference1.length];<br>        Arrays.fill(res, ' ');<br>        int idx[]={0,0};<br>        int turn=0;<br>        while(turn&lt;res.length) {<br>            if(turn%2==0){//P1<br>                int n=(preference1[idx[0]]);<br>                while(res[n-1]!=' '){<br>                    idx[0]++;<br>                    n=(preference1[idx[0]]);<br>                }<br>                res[n-1]='1';<br>            }else if(turn%2==1){//P2<br>                int n=(preference2[idx[1]]);<br>                while(res[n-1]!=' '){<br>                    idx[1]++;<br>                    n=(preference2[idx[1]]);<br>                }<br>                res[n-1]='2';<br>            }<br>            turn++;<br>        }<br>        return new String(res);<br>    }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>