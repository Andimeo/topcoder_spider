            public class PiecewiseLinearFunctionDiv2<br>{<br>  public int[] countSolutions( int[] y, int[] query )<br>  {<br>    int[] res = new int[ query.Length ];<br><br>    for( int i = 0; i &lt; y.Length - 1; i++ )<br>    {<br>      int iStart = y[ i ];<br>      int iEnd = y[ i + 1 ];<br><br>      for( int j = 0; j &lt; query.Length; j++ )<br>      {<br>        if( res[ j ] == -1 )<br>          continue;<br><br>        if( query[ j ] == iStart &amp;&amp; query[ j ] == iEnd )<br>        {<br>          res[ j ] = -1;<br>          continue;<br>        }<br><br>        if( iStart &lt;= query[ j ] &amp;&amp; query[ j ] &lt; iEnd )<br>          res[ j ]++;<br><br>        if( iStart &gt;= query[ j ] &amp;&amp; query[ j ] &gt; iEnd )<br>          res[ j ]++;<br><br>        if( i == y.Length - 2 &amp;&amp; iEnd == query[ j ] )<br>        {<br>          res[ j ]++;<br>        }<br>      }<br>    }<br><br>    return res;<br>  }<br>}<br></td>