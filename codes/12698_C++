            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;queue&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br>#define pb push_back<br> <br>class PiecewiseLinearFunctionDiv2 {<br>public:<br>  vector &lt;int&gt; countSolutions(vector &lt;int&gt; Y, vector &lt;int&gt; query) {<br>    vector&lt;int&gt; ans;<br>    for(int i=0; i&lt;query.size(); i++)<br>    {<br>      int temp=query[i];<br>      int cnt=0;<br>      int flag=0;<br> <br>      for(int j=0; j&lt;Y.size()-1; j++)<br>      {<br>        int y1=Y[j], y2=Y[j+1];<br>        if(temp==y1 &amp;&amp; temp==y2)<br>          flag=1;<br> <br>        if(temp&gt;y1 &amp;&amp; temp&lt;y2)<br>          cnt++;<br>        else if(temp&lt;y1 &amp;&amp; temp&gt;y2)<br>          cnt++;<br>      }<br>      for(int j=0; j&lt;Y.size(); j++)<br>        if(Y[j]==temp)<br>          cnt++;<br>      if(flag==0)<br>        ans.pb(cnt);<br>      else<br>        ans.pb(-1);<br>    }<br>    return ans;<br>  }<br>};<br> <br> <br> <br>//Powered by KawigiEdit 2.1.7 (beta) modified by pivanof!<br></td>