<td class="problemText" colspan="8" valign="middle" align="left">
            public class PiecewiseLinearFunctionDiv2<br>{<br>  public static int[] countSolutions(int[] Y, int[] query)<br>  {<br>    int[] count = new int[query.length];<br>    for (int j = 0; j &lt; query.length; j++)<br>      if (Y[0] == query[j])<br>        count[j] = 1;<br>    for (int i = 1; i &lt; Y.length; i++)<br>    {<br>      for (int j = 0; j &lt; query.length; j++)<br>      {<br>        if (count[j] == -1)<br>          continue;<br>        if (Y[i - 1] == query[j] &amp;&amp; Y[i] == query[j])<br>          count[j] = -1;<br>        if (Y[i - 1] &gt; query[j] &amp;&amp; Y[i] &lt;= query[j] || Y[i - 1] &lt; query[j] &amp;&amp; Y[i] &gt;= query[j])<br>          count[j]++;<br>      }<br>    }<br>    return count;<br>  }<br>}<br></td>