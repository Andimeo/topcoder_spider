            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class ThreeColorabilityEasy<br>{<br>  public string isColorable(string[] cells)<br>  {<br>        int h = cells.Length;<br>        int w = cells[0].Length;<br>        int cnt = 0;<br> <br>        bool good = true;<br>        for (int i = 0; i &lt; h-1; i++)<br>            for (int j = 0; j &lt; w-1; j++)<br>            {<br>                cnt = 0;<br>                if (cells[i][j] == 'Z') cnt++;<br>                if (cells[i+1][j] == 'Z') cnt++;<br>                if (cells[i][j+1] == 'Z') cnt++;<br>                if (cells[i+1][j+1] == 'Z') cnt++;<br>                if (cnt == 3 || cnt == 1) good = false;<br>            }<br> <br>        if (good)<br>            return "Yes";<br>        return "No";<br> <br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>