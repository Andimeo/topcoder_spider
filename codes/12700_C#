            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class InsertZ<br>{<br>  public string canTransform(string goal, string init)<br>  {<br>    if(init==goal)return "Yes";<br>    string ret="";<br>    for(int i=0;i&lt;init.Length;i++)<br>    {<br>      if(init[i]=='z')continue;<br>      ret+=init[i];<br>    }<br>    <br>    if(ret==goal)return "Yes";<br>    return "No";<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>