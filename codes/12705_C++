            #include &lt;algorithm&gt;<br>#include &lt;string&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;vector&gt;<br>#include &lt;queue&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iterator&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;sstream&gt;<br>#include &lt;cstring&gt;<br><br>using namespace std;<br><br>#define FOR(i,a,b) for (int _b=(b), i=(a); i &lt;= _b; i++)<br>#define CLEAR(x) memset(x,0,sizeof x);<br>typedef long long LL;<br><br>#define N 1024000<br>#define MOD 1000000009<br><br>class TheFourSquares {<br>public:<br>  LL a[N],sa[N];<br>  int countPlacements(int n, int m) {<br>    if (n &gt; m) swap(n, m);<br>    CLEAR(a);<br>    CLEAR(sa);<br>    FOR(mab, 1, n-1)<br>    {<br>      a[mab] += 2*min(n-mab,mab);<br>      if (mab * 2 &lt;= n)<br>        a[mab]--;<br>    }<br>    FOR(mab, 1, m)<br>    {<br>      sa[mab] = sa[mab - 1] + a[mab];<br>      sa[mab] %= MOD;<br>    }<br>    LL res = 0;<br>    FOR(i, 1, m)<br>      res += (a[i] * (LL)sa[m-i]) % MOD;<br>    res %= MOD;<br>    return res;<br>  }<br>  <br><br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>