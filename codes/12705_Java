            /**<br> * Built using CHelper plug-in<br> * Actual solution is at the top<br> */<br>public class TheFourSquares {<br>    final static long M = 1000000009;<br>    public int countPlacements(int n, int m) {<br>        if (n &lt; m) {<br>            int t = n; n = m; m = t;<br>        }<br> <br>        long a = 0;<br>        for (int s = 1; s &lt;= n - 1 &amp;&amp; s &lt; m; s++) {<br>            long v = 1L * (n - s) * (n - s);<br>            if (m &lt; n - s + n - s &amp;&amp; m &gt;= n - s + 1) {<br>                long z = (n - s + n - s - m);<br>                v -= 1L * z * (z + 1) / 2;<br>            } else if (m &lt; n - s + 1) {<br>                v = 1L * m * (m - 1) / 2;<br>            }<br> <br>            long w = 0;<br>            if (s &gt; m - s) {<br>                w = 2 * (m - s);<br>            } else {<br>                w = 2 * s - 1;<br>            }<br> <br>            v %= M;<br>            w %= M;<br>            a = (a + w * v) % M;<br> <br>        }<br>        return (int) a;<br>    }<br>}<br></td>