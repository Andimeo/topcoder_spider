            import time<br><br>class GUMIAndSongsDiv1:<br>    def maxSongs(self, duration, tone, T):<br>        n = len(duration)<br>        songs = []<br>        for i in xrange(n):<br>            songs.append([tone[i],duration[i]])<br>        songs.sort()<br>        <br>        best = 0<br>        for first in xrange(n):<br>            for last in xrange(first,n):<br>                extra = songs[last][0]-songs[first][0]<br>                t = T-extra<br>                sub = []<br>                for i in xrange(first,last+1):<br>                    sub.append(songs[i][1])<br>                sub.sort()<br>                <br>                cnt = 0<br>                for i in xrange(len(sub)):<br>                    if t-sub[i] &gt;= 0:<br>                        t -= sub[i]<br>                        cnt += 1<br>                best = max(best,cnt)<br>                <br>        return best<br></td>