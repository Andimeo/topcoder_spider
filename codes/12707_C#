            public class GUMIAndSongsDiv2<br>{<br>  public int maxSongs(int[] d, int[] t, int T)<br>  {<br>    int ans = 0;<br>    for(int i=0;i&lt;d.Length;i++)<br>    {<br>      for(int j=i+1;j&lt;d.Length;j++)<br>      {<br>        if(t[i]&gt;t[j])<br>        {<br>          int p = t[i];<br>          t[i] = t[j];<br>          t[j] = p;<br>          <br>          p = d[i];<br>          d[i] = d[j];<br>          d[j] = p;<br>        }<br>      }<br>    }<br>    for(int i=1;i&lt;=1&lt;&lt;d.Length;i++)<br>    {  <br>      int c = 0;<br>      int s = 0;<br>      int p = -1;<br>      for(int j=0;j&lt;d.Length;j++)<br>      {<br>        if(((int)i &amp; (int)(1&lt;&lt;j)) != 0)<br>        {<br>          c+=1;<br>          s+=d[j];<br>          if(p!=-1)<br>          {<br>            s += System.Math.Abs(t[j] - p);<br>          }<br>          p = t[j];<br>        }<br>      }<br>      if(s&lt;=T)<br>      {<br>        if(ans &lt; c)<br>          ans = c;<br>      }<br>    }<br>    return ans;<br>  }<br>}<br></td>