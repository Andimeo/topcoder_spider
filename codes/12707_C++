            #include&lt;cstdio&gt;<br>#include&lt;cstring&gt;<br>#include&lt;cstdlib&gt;<br>#include&lt;cmath&gt;<br>#include&lt;algorithm&gt;<br>#include&lt;iostream&gt;<br>#include&lt;vector&gt;<br>#include&lt;string&gt;<br>using namespace std;<br>typedef long long LL;<br>int dp[1&lt;&lt;15][20];<br>int AB(int x){if(x&lt;0)return -x;return x;}<br>class GUMIAndSongsDiv2<br>{<br>  public:<br>  int maxSongs(vector &lt;int&gt; t, vector &lt;int&gt; b, int T)<br>  {<br>    int n=t.size();<br>    memset(dp,-1,sizeof(dp));<br>    for(int i=0;i&lt;n;i++)<br>    dp[1&lt;&lt;i][i]=t[i];<br>    for(int i=0;i&lt;(1&lt;&lt;n);i++)<br>    for(int j=0;j&lt;n;j++)<br>    if(dp[i][j]!=-1)<br>    {<br>      for(int k=0;k&lt;n;k++)<br>      {<br>        if(i&amp;(1&lt;&lt;k))continue;<br>        int cs=dp[i][j]+t[k]+AB(b[j]-b[k]);<br>        int ii=i^(1&lt;&lt;k);<br>        if(dp[ii][k]==-1)dp[ii][k]=cs;<br>        else dp[ii][k]=min(dp[ii][k],cs);<br>      }<br>    }<br>    int ret=0;<br>    for(int i=0;i&lt;(1&lt;&lt;n);i++)<br>    for(int j=0;j&lt;n;j++)<br>    if(dp[i][j]!=-1&amp;&amp;dp[i][j]&lt;=T)<br>    {<br>      int ct=0;<br>      for(int k=0;k&lt;n;k++)<br>      if(i&amp;(1&lt;&lt;k))ct++;<br>      ret=max(ret,ct);<br>    }<br>    return ret;<br>  }<br>};<br></td>