            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class GUMIAndSongsDiv2<br>{<br>    public int maxSongs(int[] du, int[] to, int T)<br>  {<br>    int tt=0;<br>    <br>    for(int i=0;i&lt;(1&lt;&lt;du.length);i++)<br>    {<br>      int ans=0,cnt=0;<br>      List&lt;Integer&gt;a=new ArrayList&lt;Integer&gt;();<br>      for(int j=0;j&lt;du.length;j++)<br>        if((i&amp;(1&lt;&lt;j))&gt;0)<br>        {<br>          ans+=du[j];<br>          a.add(to[j]);<br>          cnt++;<br>        }<br>      Collections.sort(a, new Comparator&lt;Integer&gt;(){<br>        @Override<br>        public int compare(Integer arg0, Integer arg1) {<br>          // TODO Auto-generated method stub<br>          return arg0-arg1;<br>        }});<br>      if(cnt&lt;=tt)continue;<br>      for(int j=1;j&lt;cnt;j++)<br>      {<br>      ans+=a.get(j)-a.get(j-1);<br>        <br>      }<br>      if(ans&lt;=T)tt=cnt;<br>    }<br>    <br>    <br>    return tt;<br>    <br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>