            public class KeyDungeonDiv2<br>{<br>  public int countDoors(int[] d, int[] e, int[] k)<br>  {<br>    int ans = 0;<br>    for(int i=0;i&lt;d.Length;i++)<br>    {<br>      int r = k[0];<br>      int g = k[1];<br>      int w = k[2];<br>      r = r - d[i];<br>      g = g - e[i];<br>      if(r&lt;0)<br>      {<br>        w = w+r;        <br>      }<br>      if(g&lt;0)<br>      {<br>        w = w+g;<br>      }<br>      if(w&gt;=0)<br>        ans += 1;<br>    }<br>    return ans;<br>  }<br>}<br></td>