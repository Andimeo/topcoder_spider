            import math,string,itertools,fractions,heapq,collections,re,array,bisect<br> <br>class KeyDungeonDiv2:<br>    def countDoors(self, doorR, doorG, keys):<br>      result=0<br>        for i in xrange(len(doorR)):<br>          red=doorR[i]-keys[0]<br>          green=doorG[i]-keys[1]<br>          if red&lt;=0 and green&lt;=0:<br>            result+=1<br>            continue<br>          else:<br>            red=max(0,red)<br>            green=max(0,green)<br>            if red+green&lt;=keys[2]:<br>              result+=1<br>        return result<br> <br> <br>#Powered by KawigiEdit-pf 2.2.2!<br></td>