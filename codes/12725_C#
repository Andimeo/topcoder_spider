            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Linq;<br> <br>public class FoxAndChess<br>{<br>  public string ableToMove(string begin, string target)<br>  {<br>    if (begin.Count(x =&gt; x == 'L') != target.Count(x =&gt; x == 'L')) return "Impossible";<br>    if (begin.Count(x =&gt; x == 'R') != target.Count(x =&gt; x == 'R')) return "Impossible";<br>    int idx = 0;<br>    for (int i = 0; i &lt; target.Length; i++)<br>    {<br>      if (target[i] == '.') continue;<br>      while (begin[idx] == '.') idx++;<br>      if (target[i] != begin[idx]) return "Impossible";<br>      if (target[i] == 'L' &amp;&amp; idx &lt; i) return "Impossible";<br>      if (target[i] == 'R' &amp;&amp; idx &gt; i) return "Impossible";<br>      idx++;<br>    }<br>    return "Possible";<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>