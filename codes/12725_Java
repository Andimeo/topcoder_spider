            import java.util.ArrayList;<br><br>public class FoxAndChess {<br><br>  public String ableToMove(String q, String w) {<br>    String qq = q.replaceAll("\\.", "");<br>    String ww = w.replaceAll("\\.", "");<br>    if (!qq.equals(ww))<br>      return "Impossible";<br>    ArrayList&lt;Integer&gt; rq = new ArrayList&lt;Integer&gt;();<br>    ArrayList&lt;Integer&gt; lq = new ArrayList&lt;Integer&gt;();<br>    ArrayList&lt;Integer&gt; rw = new ArrayList&lt;Integer&gt;();<br>    ArrayList&lt;Integer&gt; lw = new ArrayList&lt;Integer&gt;();<br><br>    for (int i = 0; i &lt; q.length(); i++) {<br>      if (q.charAt(i) == 'L')<br>        lq.add(i);<br>      if (q.charAt(i) == 'R')<br>        rq.add(i);<br>      if (w.charAt(i) == 'L')<br>        lw.add(i);<br>      if (w.charAt(i) == 'R')<br>        rw.add(i);<br>    }<br><br>    for (int i = 0; i &lt; lq.size(); i++) {<br>      if (lq.get(i) &lt; lw.get(i))<br>        return "Impossible";<br>    }<br><br>    for (int i = 0; i &lt; rq.size(); i++) {<br>      if (rq.get(i) &gt; rw.get(i))<br>        return "Impossible";<br>    }<br><br>    return "Possible";<br>  }<br><br>}<br></td>