            class FoxAndChess:<br>    def ableToMove(_, begin, target):<br>        N = len(begin)<br> <br>        if begin.count('.') != target.count('.'):<br>            return 'Impossible'<br> <br>        j = 0<br>        for i in xrange(N):<br>            if begin[i] != '.':<br>                while target[j] == '.':<br>                    j += 1<br> <br>                if begin[i] == target[j] == 'L' and i &gt;= j:<br>                    pass<br>                elif begin[i] == target[j] == 'R' and i &lt;= j:<br>                    pass<br>                else:<br>                    return 'Impossible'<br> <br>                j += 1<br> <br>        return 'Possible'<br></td>