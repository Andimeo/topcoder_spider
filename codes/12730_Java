            import java.util.*;<br> <br>public class GooseTattarrattatDiv1 {<br>    public int getmin(String s) {<br>      int[] count = new int[26];<br>      int[] comp = new int[26];<br>      for (int i = 0; i &lt; comp.length; i++) {<br>        comp[i] = i;<br>      }<br>      for (int i = 0; i &lt; s.length(); i++) {<br>        count[s.charAt(i) - 'a']++;<br>      }<br>      for (int i = 0; i &lt; s.length(); i++) {<br>        int x = s.charAt(i) - 'a';<br>        int y = s.charAt(s.length() - 1 - i) - 'a';<br>        x = comp[x];<br>        y = comp[y];<br>        for (int j = 0; j &lt; comp.length; j++) {<br>          if (comp[j] == x) {<br>            comp[j] = y;<br>          }<br>        }<br>      }<br>      int ans = 0;<br>      for (int c = 0; c &lt; comp.length; c++) {<br>        int size = 0;<br>        int max = 0;<br>        for (int i = 0; i &lt; comp.length; i++) {<br>          if (comp[i] != c) {<br>            continue;<br>          }<br>          size += count[i];<br>          max = Math.max(max, count[i]);<br>        }<br>        ans += size - max;<br>      }<br>      return ans;<br>    }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>