            using System;<br>using System.Collections.Generic;<br>using System.Linq;<br>using System.Text;<br>using System.Threading;<br>using System.Threading.Tasks;<br><br>class GearsDiv2<br>{<br>    public static int getmin(string S)<br>    {<br>        if (S.Length == 1)<br>            return 0;<br>        int[][] a = new int[S.Length][];<br>        for (int i = 0; i &lt; S.Length; i++)<br>            a[i] = new int[2];<br>        int ans = 10000000;<br>        a[0][1] = 1;<br>        a[0][0] = -100000;<br>        for (int i = 1; i &lt; S.Length; i++)<br>        {<br>            a[i][0] = maxx(a[i - 1][0], a[i - 1][1]);<br>            a[i][1] = a[i - 1][0] + 1;<br>            if (S[i] != S[i - 1])<br>                a[i][1] = maxx(a[i][1], a[i - 1][1] + 1);<br>        }<br>        if (S[0] != S[S.Length - 1])<br>            ans = S.Length - maxx(a[S.Length - 1][0], a[S.Length - 1][1]);<br>        else<br>            ans = S.Length - a[S.Length - 1][0];<br>        a[0][1] = -1000000;<br>        a[0][0] = 0;<br>        for (int i = 1; i &lt; S.Length; i++)<br>        {<br>            a[i][0] = maxx(a[i - 1][0], a[i - 1][1]);<br>            a[i][1] = a[i - 1][0] + 1;<br>            if (S[i] != S[i - 1])<br>                a[i][1] = maxx(a[i][1], a[i - 1][1] + 1);<br>        }<br>        if (S.Length - maxx(a[S.Length - 1][0], a[S.Length - 1][1]) &lt; ans)<br>            ans = S.Length - maxx(a[S.Length - 1][0], a[S.Length - 1][1]);<br>        return ans;<br>    }<br>    static int maxx(int a, int b)<br>    {<br>        if (a &gt; b) return a;<br>        return b;<br>    }<br>    static void Main(string[] args)<br>    {<br>        Console.WriteLine(GearsDiv2.getmin("RRR"));<br>    }<br>}<br></td>