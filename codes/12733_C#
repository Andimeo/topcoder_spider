            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class GooseTattarrattatDiv2<br>{<br>  public int getmin(string S)<br>  {<br>        int n = S.Length;<br>        int[] a = new int[26];<br>        int max = 0;<br>        for (int i = 0; i &lt; n; i++)<br>        {<br>            int v = S[i] - 'a';<br>            a[v]++;<br>            if (a[v] &gt; max)<br>                max = a[v];<br>        }<br>        return n - max;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>