            import java.util.*;<br>import java.math.*;<br><br>public class GooseTattarrattatDiv2 {<br>   public int getmin(String S) {<br>     int[] freq = new int[26];<br>     for(int i = 0; i &lt; S.length(); i++)<br>       freq[S.charAt(i) - 'a']++;<br>     <br>     Arrays.sort(freq);<br>     int ans = S.length() -freq[25];<br>     <br>     return ans;<br>   }<br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by moj 4.16 [modified TZTester]<br>// Powered by CodeProcessor<br></td>