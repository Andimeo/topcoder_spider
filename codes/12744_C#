            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections.Generic;<br><br>public class FoxAndGomoku {<br>    public string win(string[] board) {<br>        string res = "";<br><br>        for (int i = 0; i &lt; board.Length; i++)<br>        {<br>            for (int j = 0; j &lt; board[0].Length; j++)<br>            {<br>                int []dx = new int[]{0,1,1,-1};<br>                int []dy = new int[]{1,0,1,1};<br>                for (int k = 0; k &lt; dx.Length; k++)<br>                {<br>                    <br>                    try{<br>                        bool notfound = false;<br>                    for(int l = 0; l &lt; 5; l++){<br>                        if(board[i + dx[k] * l][j + dy[k] * l] == '.'){<br>                            notfound = true;<br>                            break;<br>                        }<br>                    }<br>                        if(!notfound){<br>                            return "found";<br>                        }<br>                            <br>                    }catch(Exception e)<br>                    {<br>                        <br>                    }<br>                }<br>            }<br>        }<br>        return "not found";<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>