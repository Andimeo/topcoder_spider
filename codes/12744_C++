            #include &lt;iostream&gt; <br>#include &lt;cstdio&gt; <br>#include &lt;cstdlib&gt; <br>#include &lt;string&gt; <br>#include &lt;cmath&gt; <br>#include &lt;vector&gt; <br>#include &lt;algorithm&gt; <br>#include &lt;climits&gt;<br>#include &lt;stack&gt;<br>#include &lt;queue&gt;<br>using namespace std;<br><br>class FoxAndGomoku{ <br>public: <br>    <br>    string win(vector &lt;string&gt; board){<br>        vector&lt;vector&lt;int&gt; &gt; count1(board.size()+2, vector&lt;int&gt;(board.size()+2,0)); <br>        vector&lt;vector&lt;int&gt; &gt; count2(board.size()+2, vector&lt;int&gt;(board.size()+2,0)); <br>        vector&lt;vector&lt;int&gt; &gt; count3(board.size()+2, vector&lt;int&gt;(board.size()+2,0)); <br>        vector&lt;vector&lt;int&gt; &gt; count4(board.size()+2, vector&lt;int&gt;(board.size()+2,0)); <br>        for(int i = 0; i &lt; board.size(); i++){<br>            for(int j = 0; j &lt; board.size(); j++){<br>                if(board[i][j]=='o'){<br>                    count1[i+1][j+1] = count1[i+1][j]+1;<br>                    count2[i+1][j+1] = count2[i][j+1]+1;<br>                    count3[i+1][j+1] = count3[i][j]+1;<br>                    count4[i+1][j+1] = count4[i][j+2]+1;<br>                }<br>                if(count1[i+1][j+1]==5<br>                || count2[i+1][j+1]==5<br>                || count3[i+1][j+1]==5<br>                || count4[i+1][j+1]==5)<br>                    return string("found");<br>            }<br>        }<br>        return string("not found");<br>    } <br>};<br></td>