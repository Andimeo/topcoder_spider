            public class FoxAndGomoku {<br> <br>  public String win(String[] board) {<br>    int n = board.length;<br>    for (int i = 0; i &lt; n; ++i) {<br>      for (int j = 0; j &lt; n; ++j) {<br>        int count = 0;<br>        for (int k = 0; k &lt; 5 &amp;&amp; k + j &lt; n; ++k) {<br>          if (board[i].charAt(j + k) == 'o') {<br>            ++count;<br>          }<br>        }<br>        if (count &gt; 4) {<br>          return "found";<br>        }<br>      }<br>    }<br>    for (int i = 0; i &lt; n; ++i) {<br>      for (int j = 0; j &lt; n; ++j) {<br>        int count = 0;<br>        for (int k = 0; k &lt; 5 &amp;&amp; k + i &lt; n; ++k) {<br>          if (board[i + k].charAt(j) == 'o') {<br>            ++count;<br>          }<br>        }<br>        if (count &gt; 4) {<br>          return "found";<br>        }<br>      }<br>    }<br>    for (int i = 0; i &lt; n; ++i) {<br>      for (int j = 0; j &lt; n; ++j) {<br>        int count = 0;<br>        for (int k = 0; k &lt; 5 &amp;&amp; k + j &lt; n &amp;&amp; k + i &lt; n; ++k) {<br>          if (board[i + k].charAt(j + k) == 'o') {<br>            ++count;<br>          }<br>        }<br>        if (count &gt; 4) {<br>          return "found";<br>        }<br>      }<br>    }<br>    for (int i = 0; i &lt; n; ++i) {<br>      for (int j = 0; j &lt; n; ++j) {<br>        int count = 0;<br>        for (int k = 0; k &lt; 5 &amp;&amp; j - k &gt;= 0 &amp;&amp; k + i &lt; n; ++k) {<br>          if (board[i + k].charAt(j - k) == 'o') {<br>            ++count;<br>          }<br>        }<br>        if (count &gt; 4) {<br>          return "found";<br>        }<br>      }<br>    }<br> <br>    return "not found";<br>  }<br> <br>}<br></td>