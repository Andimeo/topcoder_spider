            import java.util.*;<br>@SuppressWarnings("unchecked")<br> <br>public class TheTree<br>{<br>  public int maximumDiameter(int[] cnt)<br>  {<br>    int n = cnt.length;<br> <br>        int res = n;<br> <br>        for(int i = 0;i &lt; n;i++)<br>            for(int j = 0;j &lt; n;j++)<br>            {<br>                int lowest = Math.min(i, j);<br> <br>                while (lowest &gt;= 0 &amp;&amp; cnt[lowest] != 1) lowest--;<br> <br>                res = Math.max(i-lowest+j-lowest, res);<br>            }<br> <br>        return res;<br>  }<br> <br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>