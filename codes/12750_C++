            #include &lt;iostream&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cassert&gt;<br>#include &lt;cstring&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;complex&gt;<br>#include &lt;utility&gt;<br>#include &lt;algorithm&gt;<br>using namespace std;<br><br>typedef long long llg;<br><br>llg dp[3000005];<br><br>class YetAnotherTwoTeamsProblem {<br>public:<br>  long long count(vector &lt;int&gt; v) {<br>    int n = v.size(), sum = 0, mid;<br>    for(int i = 0; i &lt; n; i++)  sum += v[i];<br>    dp[0] = 1; mid = sum/2;<br>    sort(v.begin(), v.end());<br>    llg ans = 0;<br>    for(int i = n-1; i &gt;= 0; i--) {<br>      for(int j = mid; j &lt;= mid+v[i]+1; j++) {<br>        if(j&gt;=v[i] &amp;&amp; j&gt;sum-j &amp;&amp; j-v[i]&lt;sum-j+v[i])  ans += dp[j-v[i]];<br>      }<br>      for(int j = mid+v[i]+1; j &gt;= v[i]; j--)  dp[j] += dp[j-v[i]];<br>    }<br>    return  ans;<br>  }<br>};<br></td>