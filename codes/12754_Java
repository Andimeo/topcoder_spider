            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class ConvertibleStrings<br>{<br>  static char[] a, b;<br>  static char[] map;<br>  static int min;<br> <br>  static void solve(int i, int mask) {<br>    if (i == 9) {<br>      int cnt = 0;<br>      for (int j = 0; j &lt; a.length; j++)<br>        if (b[j] != map[a[j] - 'A'])<br>          cnt++;<br>      min = Math.min(min, cnt);<br>      return;<br>    }<br>    for (int j = 0; j &lt; 9; j++) {<br>      if ((mask &amp; (1 &lt;&lt; j)) == 0) {<br>        map[i] = (char) (j + 'A');<br>        solve(i + 1, mask | (1 &lt;&lt; j));<br>      }<br>    }<br>  }<br> <br>  public int leastRemovals(String A, String B) {<br>    map = new char[9];<br>    a = A.toCharArray();<br>    b = B.toCharArray();<br>    min = Integer.MAX_VALUE;<br>    solve(0, 0);<br>    return min;<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>