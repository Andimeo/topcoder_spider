            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class WolfDelaymaster {<br>    public string check(string str) {<br><br>        do<br>        {<br>            int w = 0;<br>            for (int n = 0; n &lt; str.Length; n++)<br>                if (str[n] == 'w') w++;<br>                else break;<br>            if (w == 0) return "INVALID";<br>            if (str.Length &lt; w * 4) return "INVALID";<br>            for(int n=w; n&lt;2*w; n++)<br>                if (str[n] != 'o') return "INVALID";<br><br>            for (int n = 2*w; n &lt; 3 * w; n++)<br>                if (str[n] != 'l') return "INVALID";<br><br>            for (int n = 3*w; n &lt; 4 * w; n++)<br>                if (str[n] != 'f') return "INVALID";<br>            str = str.Substring(4 * w);<br><br>        } while (str.Length &gt; 0);<br><br>            return "VALID"; ;<br>    }<br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>