            class WolfDelaymaster:<br>  def getsubwolf(self,str):<br>    i = 0<br>    w, o, l, f = 0, 0, 0, 0<br>    while i &lt; len(str) and str[i] == 'w':<br>      w += 1<br>      i += 1<br>    while i &lt; len(str) and str[i] == 'o':<br>      o += 1<br>      i += 1<br>    while i &lt; len(str) and str[i] == 'l':<br>      l += 1<br>      i += 1<br>    while i &lt; len(str) and str[i] == 'f':<br>      f += 1<br>      i += 1<br>    if w == o == l == f:<br>      return str[i:]<br>    else:<br>      return None<br>  def check(self, str):<br>    while len(str):<br>      str = self.getsubwolf(str)<br>      if str is None:<br>        return 'INVALID'<br>    return 'VALID'<br></td>