            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class RaiseThisBarn {<br>    public int calc(string str) {<br>        int res=0;<br>        for (int n = 1; n &lt; str.Length; n++)<br>        {<br>            int a=0;<br>            int b=0;<br>            for (int m = 0; m &lt; n; m++)<br>                if (str[m] == 'c') a++;<br>            for(int m=n; m&lt;str.Length; m++)<br>                if (str[m] == 'c') b++;<br>            if (a == b) res++;<br>        }<br>        return res;<br>    }<br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>