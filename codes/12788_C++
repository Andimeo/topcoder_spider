            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br>#include &lt;cstring&gt;<br>using namespace std;<br>#define mod 1000000007<br>int a[111111],la;<br>int num[111111];<br>int add(int x,int y){<br>  return (x+=y)&lt;mod?x:x-mod;<br>}<br>int dec(int x,int y){<br>  return add(x,mod-y);<br>}<br>int mul(int x,int y){<br>  return (int)(((long long int)(x))*y%mod);<br>}<br>int powmod(int x,int y){<br>  int r=1;<br>  while(1){<br>    if(y&amp;1)r=mul(r,x);<br>    if(y&gt;&gt;=1)x=mul(x,x);else<br>      break;<br>  }<br>  return r;<br>}<br>class PairsOfStrings {<br>  public:<br>  int getNumber(int n, int k) {<br>    la=0;<br>    for(int i=1; i*i&lt;=n; i++)<br>      if(n%i==0){<br>        a[la++]=i;<br>        a[la++]=n/i;<br>      }<br>    sort(a,a+la);<br>    la=unique(a,a+la)-a;<br>    int res=0;<br>    for(int i=0; i&lt;la; i++){<br>      num[i]=powmod(k,a[i]);<br>      for(int j=0; j&lt;i; j++)<br>        if(a[i]%a[j]==0)<br>          num[i]=dec(num[i],num[j]);<br>      res=add(res,mul(num[i],a[i]));<br>    }<br>    return res;<br>  }<br>  <br><br>};<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>