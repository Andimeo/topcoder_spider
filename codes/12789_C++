            #line 83 "LotsOfLines.cpp"<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;memory.h&gt;<br>#include &lt;iostream&gt;<br>using namespace std;<br><br>int gcd(int p, int q) {<br>    return q ? gcd(q, p%q) : p;<br>}<br><br>class LotsOfLines<br>{ <br>public: <br>    long long countDivisions(int A, int B) <br>    {<br>        long long sum = 0;<br>        for (int i = 1; i &lt; A; ++i) {<br>            for (int j = 1; j &lt; B; ++j) {<br>                if (gcd(i, j) != 1)<br>                    continue;<br>                sum += (A - i) * (B - j);<br>            }<br>        }<br>        sum *= 2;<br>        sum += (A - 1) * B;<br>        return sum + A * B + 1;<br>    } <br>        <br> <br>}; <br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>