            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Diagnostics;<br>using System.Linq;<br>using System.Numerics;<br>using System.Text;<br><br>public class IncrementAndDoubling {<br><br>    static public int BitCount(int x) {<br>        int bitCount = 0;<br>        for (; x != 0; x = x &amp; (x - 1)) bitCount++;<br>        return bitCount;<br>    }<br><br>    public int getMin(int[] desiredArray) {<br>        int maxv = desiredArray.Max();<br>        int maxBit = -1;<br>        for (int i = 0; i &lt; 30; i++) {<br>            if (((1 &lt;&lt; i) &amp; maxv) != 0) {<br>                maxBit = i;<br>            }<br>        }<br>        if (maxBit == -1) return 0;<br>        return maxBit + desiredArray.Select(BitCount).Sum();<br>    }<br><br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by moj 4.17 [modified TZTester]<br>// Powered by CodeProcessor<br></td>