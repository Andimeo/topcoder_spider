            #include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;cmath&gt;<br>#include &lt;ctime&gt;<br>#include &lt;vector&gt;<br>#include &lt;queue&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br><br>#define maxlongint 2147483647<br>#define LL long long<br>#define pb push_back<br>#define mp make_pair<br><br>using namespace std;<br><br>int a[100];<br>int n;<br><br>class IncrementAndDoubling<br>{<br>    public:<br>    int getMin(vector &lt;int&gt; a)<br>    {<br>        int n=a.size();<br>        int ans=0;<br>        while(1)<br>        {<br>          bool flag=true;<br>          for(int i=0;i&lt;n;i++)if(a[i]){flag=false;break;}<br>          if(flag)break;<br>          for(int i=0;i&lt;n;i++)if(a[i]&amp;1){ans++;a[i]--;}<br>          flag=true;<br>          for(int i=0;i&lt;n;i++)if(a[i]){flag=false;break;}<br>          if(flag)break;<br>          ans++;<br>          for(int i=0;i&lt;n;i++)a[i]/=2;<br>        }<br>        return ans;<br>    }<br>};<br></td>