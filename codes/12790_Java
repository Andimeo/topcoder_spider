            import java.util.*;<br>@SuppressWarnings("unchecked")<br> <br>public class IncrementAndDoubling<br>{<br>  public int getMin(int[] desiredArray)<br>  {<br>    int res = 0;<br>        for(int num : desiredArray)<br>            for(int i = 0;i &lt; 10;i++)<br>                if((num&amp;(1&lt;&lt;i)) != 0)<br>                    res = Math.max(res, i);<br>        for(int num : desiredArray)<br>            res += Integer.bitCount(num);<br>        return res;<br>  }<br> <br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>