            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class FoxAndClassroom<br>{<br>public string ableTo(int n, int m)<br>  {<br>      if (GTD(n, m) == 1)<br>      {<br>          return "Possible";<br> <br>      }<br>      return "Impossible";<br>  }<br> <br>    public int GTD(int n, int m)<br>    {<br>        if (n%m == 0) return m;<br>        if (m == 1)<br>        {<br>            return 1;<br>        }<br>        return GTD(m, n%m);<br>    }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>