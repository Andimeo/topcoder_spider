            #include &lt;algorithm&gt;<br>#include &lt;iostream&gt;<br>#include &lt;fstream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;queue&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cmath&gt;<br>#include &lt;list&gt;<br>#include &lt;cstring&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br><br>using namespace std;<br><br>#define NMAX 2147483647<br>#define LMAX 9223372036854775807LL<br>#define pb push_back<br>#define pob pop_back<br>#define mp make_pair<br>#define st first<br>#define nd second<br>#define FOR(i,a,b) for(int i=(a);i&lt;(b);++i)<br>#define FORD(i,a,b) for(int i=(a);i&gt;(b);--i)<br>#define REP(i,n) FOR(i,0,n)<br>#define FORE(i,a,b) for(int i=(a);i&lt;=(b);++i)<br><br>template&lt;class T&gt; T gcd(T a,T b) { return (b==0)?a:gcd(b,a%b); }<br>template&lt;class T&gt; T lcm(T a,T b) { return a*(b/gcd(a,b)); }<br>int DAY[] = {31, 28, 31, 30, 31 , 30, 31, 31, 30 , 31, 30, 31};<br>string Rs[] = {"I","IV","V","IX","X","XL","L","XC","C","CD","D","CM","M"};<br>int Ri[]   = {1, 4, 5, 9, 10, 40, 50, 90, 100, 400, 500, 900, 1000};<br>double eps = 1e-9;<br><br>class AstronomicalRecordsEasy<br>{<br>  public:<br>  int minimalPlanets(vector &lt;int&gt; A, vector &lt;int&gt; B)<br>  {<br>    //$CARETPOSITION$<br>    int ans = NMAX;<br>    REP(i,A.size())REP(j,B.size())<br>    {<br>      int a = A[i];<br>      int b = B[j];<br>      int temp = A.size()+B.size();<br>      REP(ii,A.size()) REP(jj,B.size())<br>      {<br>        if(A[ii]*b==B[jj]*a)<br>        {<br>          temp--;<br>          break;<br>        }<br>      }<br>      ans = min(ans,temp);<br>    }<br>    return ans;<br>  }<br><br><br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>