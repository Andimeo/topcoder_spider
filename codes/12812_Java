            import java.util.HashSet;<br> <br>public class AstronomicalRecordsEasy {<br> <br>  public int minimalPlanets(int[] A, int[] B) {<br> <br>    int min = Integer.MAX_VALUE;<br>    for (int ai = 0; ai &lt; A.length; ai++) {<br>      for (int bi = 0; bi &lt; B.length; bi++) {<br>        HashSet&lt;Integer&gt; ints = new HashSet&lt;Integer&gt;();<br>        int amul=B[bi];<br>        int bmul=A[ai];<br>        for(int a:A){<br>          ints.add(a*amul);<br>        }<br>        for(int b:B){<br>          ints.add(b*bmul);<br>        }<br>        if(min&gt;ints.size()){<br>          min=ints.size();<br>        }<br>      }<br>    }<br>    return min;<br>  }<br> <br>}<br></td>