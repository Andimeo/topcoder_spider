            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Diagnostics;<br><br>public class AlienAndGame<br>{<br>    public int getNumber(string[] board)<br>    {<br>        int n = board.Length, m = board[0].Length;<br>        int max = 1;<br>        for (int i1 = 0; i1 &lt; n; i1++)<br>        {<br>            for (int j1 = 0; j1 &lt; m; j1++)<br>            {<br>                int d = Math.Min(n - i1, m - j1);<br>                bool allOk = true;<br>                for (int k = 1; k &lt;= d; k++)<br>                {<br>                    for (int sub = i1; sub &lt; k + i1; sub++)<br>                    {<br>                        string row = board[sub].Substring(j1, k);<br>                        if (row == new string('W', k) || row == new string('B', k)) continue;<br>                        allOk = false;<br>                    }<br>                    if (allOk) max = Math.Max(max, k * k);<br>                }<br>                <br>            }<br>        }<br>        return max;<br>    }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>