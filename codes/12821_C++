            #include &lt;vector&gt;<br>#include &lt;map&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;iostream&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;string&gt;<br> <br>using namespace std;<br>int arr[55][55];<br> <br>class AlienAndGame {<br>public:<br>  int getNumber(vector &lt;string&gt; board) {<br>  int i, j;<br>  int k, l, n, mx;<br>     int t;<br>    for (i = 0; i &lt; board.size(); i++) {<br>       for (j = 0; j &lt; board[i].size(); j++) {<br>         arr[i][j] = 0;<br>         l = j;<br>         while (board[i][j] == board[i][l]) {<br>           l++;<br>           arr[i][j]++;<br>         }<br>      }<br>    }  <br>   mx = 0;<br>   for (i = 0; i &lt; board.size(); i++) {<br>       for (j = 0; j &lt; board[i].size(); j++) {<br>         t = 100000;<br>         for (k = i; k &lt; board.size(); k++) {<br>           for (l = k; l &lt; board.size(); l++) {<br>             t = min(t, arr[l][j]);<br>             n = l-k+1;<br>             n = min(t, n);<br>             mx = max(min(t,n)*min(t,n),mx);<br>           }<br>         }<br>       }<br>   }<br>   return mx;<br>}<br>};<br></td>