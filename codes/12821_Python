            class AlienAndGame:<br>    def testSameColor(self, line):<br>        c = line[0]<br>        for l in line:<br>            if l != c:<br>                return False<br>        return True<br>    <br>    def testOneSquare(self, board, s, y, x):<br>        for i in range(y, y+s):<br>            l = board[i][x:x+s]<br>            if not self.testSameColor(l):<br>                return False<br>        return True<br>            <br>    def testSquare(self, board, h, w, s):<br>        for y in range(0, h-s+1):<br>            for x in range(0, w-s+1):<br>                if self.testOneSquare(board, s, y, x):<br>                    return True<br>        return False<br>    <br>    def getNumber(self, board):<br>        w = len(board[0])<br>        h = len(board)<br>        maxs = min(w, h)<br>        for i in range(2, maxs+1):<br>            if not self.testSquare(board, h, w, i):<br>                return (i-1)*(i-1)<br>        return maxs*maxs<br></td>