            public class TheShuttles{<br>//  public static void main(String args[]) {<br>//    int cnt [] = {9, 4};<br>//    System.out.println(getLeastCost(cnt, 10, 5) );<br>//  }<br>  <br>  public int getLeastCost(int[] cnt, int baseCost, int seatCost) {<br>    int min = 1&lt;&lt;30;<br>    for (int i=1;i&lt;=100;i++) {<br>      int ans = 0;<br>      for (int j=0;j&lt;cnt.length;j++) {<br>        ans = ans + (baseCost + i*seatCost) * (cnt[j]%i ==0 ? cnt[j]/i : cnt[j]/i + 1);<br>      }<br>      if (min &gt; ans ) {<br>        min = ans;<br>      }<br>    }<br>    return min;<br>  }<br>}<br></td>