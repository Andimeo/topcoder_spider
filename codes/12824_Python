            # -*- coding: utf-8 -*-<br>import math,string,itertools,fractions,heapq,collections,re,array,bisect<br> <br>class TheShuttles:<br>    def getLeastCost(self, cnt, baseCost, seatCost):<br>        res = 10000000<br>        for x in range(1,101):<br>            wyn = 0<br>            for y in range(len(cnt)):<br>                if cnt[y]%x == 0:<br>                    ile = cnt[y]//x<br>                else:<br>                    ile = (cnt[y]//x)+1<br>                wyn = wyn + ile* (baseCost + x*seatCost)<br>            res = min(wyn,res)<br>        return res<br> <br># Powered by Greed 2.0-beta<br></td>