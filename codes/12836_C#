            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class FoxAndSightseeing<br>{<br>  public int getMin(int[] position)<br>  {<br>    int min=Int32.MaxValue;<br>    for(int i=1;i&lt;position.Length-1;i++)<br>    {<br>      List&lt;int&gt;ll=new List&lt;int&gt;();<br>      for(int j=0;j&lt;position.Length;j++)<br>      {<br>        if(i==j)continue;<br>        ll.Add(position[j]);<br>      }<br>      int sum=0;<br>      for(int j=0;j&lt;ll.Count-1;j++)<br>      {<br>        sum+=Math.Abs(ll[j]-ll[j+1]);<br>      }min=Math.Min(sum,min);<br>    }<br>    return min;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>