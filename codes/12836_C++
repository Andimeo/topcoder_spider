            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;queue&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br> <br>class FoxAndSightseeing {<br>public:<br>  int getMin(vector &lt;int&gt; position) {<br>      int vl=position.size();<br>      int mn=1e9;<br>      for(int i=1;i&lt;vl-1;i++)<br>        {<br>            int tmp=0;<br>            for(int j=0;j&lt;i-1;j++)<br>                tmp+=abs(position[j]-position[j+1]);<br>            tmp+=abs(position[i-1]-position[i+1]);<br>            for(int j=i+1;j&lt;vl-1;j++)<br>                tmp+=abs(position[j]-position[j+1]);<br>            mn=min(tmp, mn);<br>        }<br>return mn;<br>  }<br>};<br> <br> <br> <br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>