            import java.util.*;<br><br>public class FoxAndSightseeing {<br>  public int getMin(int[] position) {<br>    int min = 1 &lt;&lt; 29;<br>    for (int i = 1; i &lt; position.length - 1; i++) {<br>      int sum = 0;<br>      int prev = 0;<br>      for (int j = 0; j &lt; position.length; j++) {<br>        if (i != j) {<br>          sum += Math.abs(position[j] - position[prev]);<br>          prev = j;<br>        }<br>      }<br>      min = Math.min(min, sum);<br>    }<br>    return min;<br>  }<br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by moj 4.17 [modified TZTester]<br>// Powered by CodeProcessor<br></td>