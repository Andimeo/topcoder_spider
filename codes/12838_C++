<td class="problemText" colspan="8" valign="middle" align="left">
            #include &lt;stdio.h&gt;<br>#include &lt;vector&gt;<br>using namespace std;<br>class SparseFactorialDiv2 {<br>public:<br>  long long getCount(long long lo, long long hi, long long divisor) {<br>    long long k;<br>    long long nlo, nhi;<br>    long long sum = 0;<br>    vector&lt;long long&gt; p(divisor, -1);<br>    int cnt = divisor;<br>    for (k=0;k*k&lt;hi &amp;&amp; cnt&gt;0;k++) {<br>      if (p[k*k%divisor] == -1) {<br>        p[k*k%divisor] = k;<br>        cnt--;<br>      }<br>    }<br>    int i;<br>    for (i=0;i&lt;divisor;i++) {<br>      if(p[i]&lt;0)continue;<br>      k=p[i];<br>      nlo = lo-k*k-1;<br>      if (nlo&lt;0) nlo=0;<br>      nhi = hi-k*k;<br>      sum += (long long)(nhi/divisor)-(long long)((nlo)/divisor);<br>    }<br>    return sum;<br>  }<br>};<br></td>