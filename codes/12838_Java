            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class SparseFactorialDiv2<br>{<br>  long dowork(long l,long r,long divisor){<br>  return (r/divisor-(l-1)/divisor);<br>  }<br>  public long getCount(long lo, long hi, long divisor)<br>  {<br>    HashSet&lt;Long&gt; hh=new HashSet&lt;Long&gt;();<br>    long sum=0;<br>    for (long i=0;i*i&lt;hi;i++){<br>      long l=lo-i*i;<br>      if (l&lt;1) l=1;<br>      long r=hi-i*i;<br>      if (!hh.contains(r%divisor)){sum+=dowork(l,r,divisor);hh.add(r%divisor);}<br>    }<br>    return sum;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>