            import math<br>import string<br> <br>class SparseFactorialDiv2:<br>    def getCount(self, lo, hi, divisor):<br>        resid = []<br>        residmap = {}<br> <br>        for i in range(divisor):<br>          if not ((i * i) % divisor) in resid:<br>            resid.append((i * i) % divisor)<br>            residmap[(i * i) % divisor] = i*i + divisor<br>        <br>        result = 0<br>        for r in resid:<br>          lb = max(residmap[r], lo) - r<br>          ub = hi - r<br>          <br>          while not (lb % divisor == 0): lb += 1<br>          <br>          if lb &gt; ub: continue<br>          else: result += (ub-lb)/divisor + 1<br>          <br>        return result<br> <br> <br>#Powered by KawigiEdit-pfx 2.1.9!<br></td>