            using System;<br>using System.Text;<br>using System.Collections;<br><br>public class Assemble {<br>  int [] c;<br>  long [,] m;<br>  bool [,] used;<br>  long n;<br><br>  long solve(long a, long b)<br>  {<br>    if (b-a &lt;= 1) return 0;<br><br>    if (used[a,b]) return m[a,b];<br><br>    used[a,b]=true;<br><br>    long best = long.MaxValue;<br><br>    for(long i = a+1; i&lt;b; i++)<br>    {<br><br>      long cost = solve(a,i) + solve(i,b) + (c[a]+i-a) * c[i] * (c[b]+b-i);<br>      if (cost &lt; best) best= cost;<br>    }<br><br>    m[a,b]=best;<br>    return best;<br>  }<br><br>  public int minCost(int[] connect) {<br><br>    n = connect.Length-1;<br>    c = connect;<br>    m = new long[n+1,n+1];<br>    used = new bool[n+1,n+1];<br><br><br><br><br><br><br>    return (int)solve(0,n);<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester for c# [12-oct-2003]<br>// Powered by CodeProcessor<br></td>