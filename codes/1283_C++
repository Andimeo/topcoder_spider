            #include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cmath&gt;<br>#include &lt;queue&gt;<br>using namespace std;<br>typedef vector&lt;int&gt; VI;  typedef vector&lt;vector&lt;int&gt; &gt; VVI;<br>typedef vector&lt;string&gt; VS;  typedef vector&lt;vector&lt;string&gt; &gt; VVS;<br>typedef signed long long i64;  typedef unsigned long long u64;<br><br>VI a;<br>int memo[100][100];<br>int doit(int s, int e) {<br>  if( s == e ) return 0;<br>  int &amp;ret = memo[s][e];<br>  if( ret ) return ret;<br>  ret = 1000000000;<br>  for( int i = s; i &lt; e; i++ )<br>    ret &lt;?= doit(s, i) + doit(i+1, e) + <br>      (a[s]+i-s+1)*a[i+1]*(a[e+1]+e-i);<br>  return ret;<br>}<br><br>class Assemble {<br>public:<br>int minCost(vector &lt;int&gt; A) {<br>  a = A;<br>  return doit(0, a.size()-2);<br>}<br>};<br><br><br>// Powered by FileEdit<br></td>