<td class="problemText" colspan="8" valign="middle" align="left">
            public class Assemble {<br> <br>int[] a;<br>int N;<br>int [] [] cache;<br>int NIL = -1;<br> <br>int solve(int A, int B) {<br>  if (A==B) return 0;<br>  if (A+1==B) return 0;<br> <br>  if (cache[A][B] != NIL)<br>    return cache[A][B];<br> <br>  int min_cost = -1, min_k = -1;<br>  for (int k = A+1; k &lt; B; k++) {<br>    int cost = (a[A] + (k-A)) * a[k] * (a[B] + (B-k)) + solve(A, k) + solve(k, B);<br>    if (min_k == -1 || cost &lt;  min_cost) {<br>      min_k = k; <br>      min_cost = cost;<br>    }<br>  }<br> <br>  cache[A][B] = min_cost;<br>  return cache[A][B];<br>}<br> <br>public int minCost(int[] connect) {<br>  a = connect;<br>  N = connect.length;<br>  cache = new int[N][N];<br>  for (int i=0;i&lt;N;i++)  for (int j=0;j&lt;N;j++) cache[i][j] = NIL;<br> <br>  return solve(0, N-1);<br>}<br> <br>}<br></td>