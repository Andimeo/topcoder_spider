            /**<br> * Built using CHelper plug-in<br> * Actual solution is at the top<br> */<br>public class LittleElephantAndString {<br>    public int getNumber(String A, String B) {<br>        int[] cnt = new int[26];<br>        for (int i = 0; i &lt; A.length(); ++i)<br>            ++cnt[A.charAt(i) - 'A'];<br>        for (int i = 0; i &lt; B.length(); ++i)<br>            --cnt[B.charAt(i) - 'A'];<br>        for (int x : cnt) if (x != 0) return -1;<br>        int pos = A.length() - 1;<br>        for (int i = B.length() - 1; i &gt;= 0; --i) {<br>            while (pos &gt;= 0 &amp;&amp; A.charAt(pos) != B.charAt(i)) --pos;<br>            if (pos &lt; 0) {<br>                return i + 1;<br>            }<br>            --pos;<br>        }<br>        return 0;<br>    }<br>}<br></td>