            #include &lt;algorithm&gt;<br>#include &lt;iostream&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;cmath&gt;<br>#include &lt;queue&gt;<br>#include &lt;stack&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>using namespace std;<br>class WinterAndCandies<br>{<br>    public:<br>    int getNumber(vector &lt;int&gt; type)<br>    {<br>        int cnt[55],ans=0,p=1;<br>        memset(cnt,0,sizeof(cnt));<br>        for(int i=0;i&lt;type.size();i++) cnt[type[i]]++;<br>        for(int i=1;i&lt;=50;i++)<br>        {<br>            p*=cnt[i];<br>            ans+=p;<br>        }<br>        return ans;<br>    }<br>};<br></td>