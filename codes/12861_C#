            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class BinPacking {<br>    public int minBins(int[] item)<br>    {<br>        Array.Sort(item);<br>        int ret = 9999;<br>        for (int i = 0; i &lt;= item.Length; i += 3)<br>        {<br>            if (i != 0 &amp;&amp; item[i - 1] != 100) break;<br>            int temp = i / 3;<br>            int min = i;<br>            int max = item.Length - 1;<br><br>            while (min &lt; max)<br>            {<br>                if (item[max] + item[min] &lt;= 300)<br>                {<br>                    temp++;<br>                    max--; min++;<br>                }<br>                else<br>                {<br>                    max--;<br>                    temp++;<br>                }<br>            }<br>            temp += max - min + 1;<br>            ret = Math.Min(temp, ret);<br><br>        }<br>        return ret;<br>    }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] Modified for C# by ysn<br>// Powered by CodeProcessor<br></td>