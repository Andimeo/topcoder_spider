            #include &lt;cstdio&gt;<br>#include &lt;cstring&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br><br>using namespace std; <br><br>struct BinPacking {<br>  int minBins(vector &lt;int&gt; item) {<br>    sort(item.rbegin(), item.rend());<br>    int n = item.size();<br>    vector&lt;int&gt; used(n, 0);<br>    int ret = 0;<br>    for (int i = 0; i &lt; n; ++i) {<br>      if (used[i] == 0)<br>      {<br>        int remain = 300 - item[i];<br>        used[i] = 1;<br>        for (int j = i + 1; j &lt; n; ++j) {<br>          if (used[j] == 0 &amp;&amp; remain &gt;= item[j])<br>          {<br>            remain -= item[j];<br>            used[j] = 1;<br>          }<br>        }<br>        ret++;<br>      }<br>    }<br>    return ret;<br>  }<br>};<br><br><br>// Powered by FileEdit<br></td>