            using System;<br>using System.Collections.Generic;<br>using System.Linq;<br>using System.Text;<br>using System.Threading.Tasks;<br><br><br>class BinPackingEasy<br>{<br>    public int minBins(int[] item)<br>    {<br>        int ct = 0;<br>        List&lt;int&gt; X = item.ToList();<br><br>        X.Sort();<br>        int wt = 0;<br>        while (X.Count != 0)<br>        {<br>            wt = 0;<br>            for (int i = X.Count - 1; i &gt;= 0; i--)<br>            {<br>                if (X[i] + wt &lt;= 300)<br>                {<br>                    wt += X[i];<br>                    X.RemoveAt(i);<br>                }<br>            }<br>            ct++;<br>        }<br><br>        return ct;<br>    }<br>}<br></td>