            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;queue&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br> <br>class BinPackingEasy {<br>public:<br>  int minBins(vector &lt;int&gt; item) {<br>    sort(item.begin(), item.end());<br>    int lo = 0, hi = item.size() - 1;<br>    int ans = 0;<br>    while (lo &lt;= hi){<br>      if (item[hi] + item[lo] &lt;= 300) {<br>        lo++, hi--;<br>        ans++;<br>      }else{<br>        hi--;<br>        ans++;<br>      }<br>    }<br>    return ans;<br>  }<br>};<br> <br> <br> <br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>