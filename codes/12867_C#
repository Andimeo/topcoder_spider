            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class BigFatInteger<br>{<br>    public static IList&lt;long[]&gt; Factors(long n)<br>    {<br>        List&lt;long[]&gt; ret = new List&lt;long[]&gt;();<br>        long[] factor = new long[] { 1L, 1L };<br>        for (long p = 2L; p * p &lt;= n; ++p)<br>        {<br>            if (n % p == 0)<br>            {<br>                n /= p;<br>                factor = new long[] { p, 1L };<br>                ret.Add(factor);<br>                while (n % p == 0)<br>                {<br>                    n /= p;<br>                    factor[1]++;<br>                }<br>            }<br>        }<br>        if (n &gt; 1)<br>        {<br>            if (n == factor[0]) factor[1]++;<br>            else ret.Add(new long[] { n, 1L });<br>        }<br>        return ret;<br>    }<br>  public int minOperations(int A, int B)<br>  {<br>        IList&lt;long[]&gt; factors = Factors(A);<br> <br>        int ans = factors.Count;<br> <br>        int max = 0;<br>        for (int i = 0; i &lt; factors.Count; i++)<br>        {<br>            long x = factors[i][1] * B;<br>            double l = Math.Log(x, 2);<br> <br>            int ll = (int)Math.Floor(l);<br>            if ((1 &lt;&lt; ll) != x) ll++;<br> <br>            max = Math.Max(max, ll);<br>        }<br>        return ans + max;<br>  }<br> <br>  #region Testing code generated by KawigiEdit<br>  [STAThread]<br>  public static void Main(string[] args)<br>  {<br>    int answer, desiredAnswer;<br>    bool errors = false;<br>    DateTime time;<br>    <br>    time = DateTime.Now;<br>    answer = new BigFatInteger().minOperations(6, 1);<br>    Console.WriteLine("Time: " + (DateTime.Now-time).TotalSeconds + " seconds");<br>    desiredAnswer = 2;<br>    Console.WriteLine("Your answer:");<br>    Console.WriteLine("\t" + answ