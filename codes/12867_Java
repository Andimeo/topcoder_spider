<td class="problemText" colspan="8" valign="middle" align="left">
            import java.util.*;<br><br>public class BigFatInteger {<br><br>  public int minOperations(int A, int B) {<br>    int countPrimes = 0, maxExp = 0;<br>    <br>    for (int i = 2; i &lt;= A; i++) {<br>      if (A % i == 0) {<br>        <br>        countPrimes++;<br>        <br>        int cur = 0;<br>        while (A % i == 0) {<br>          cur++;<br>          A /= i;<br>        }<br>        <br>        maxExp = Math.max(cur, maxExp);<br>      }<br>    }<br>    <br>    <br>    int ans = countPrimes;<br>    ans += getExp(maxExp * B);<br>    return ans;<br>  }<br>  <br>  int getExp(int k) {<br>    if (k == 1) {<br>      return 0;<br>    }<br>    return getExp((k + 1) / 2) + 1;<br>  }<br><br>}<br></td>