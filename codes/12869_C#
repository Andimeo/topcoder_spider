            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class MiniatureDachshund<br>{<br>  public int maxMikan(int[] mikan, int weight)<br>  {<br>    Array.Sort(mikan);int count=0;<br>    for(int i=0;i&lt;mikan.Length;i++)<br>    {  <br>      if(mikan[i]+weight&gt;5000)break;<br>      weight+=mikan[i];<br>      count++;<br>    }<br>    return count;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>