            //Author Shvm<br>#include &lt;iostream&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;vector&gt;<br>#include &lt;map&gt;<br>#include &lt;iterator&gt;<br>#include &lt;set&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstring&gt;<br>#include &lt;string&gt;<br>#include &lt;queue&gt;<br>#include &lt;stack&gt;<br>#include &lt;utility&gt;<br>#include &lt;list&gt;<br>#include &lt;cassert&gt;<br>#include &lt;sstream&gt;<br><br>using namespace std;<br><br>class MiniatureDachshund {<br>public:<br>    int maxMikan(vector &lt;int&gt; m, int w) {<br>        int res = 0;<br>    sort (m.begin(),m.end());<br>    for (int i = 0; i &lt; m.size(); i++) {<br>      if (w+m[i] &lt;= 5000) {<br>        res++;<br>        w += m[i];<br>      }<br>    }<br>        return res;<br>    }<br><br>};<br><br><br><br>//Powered by codeprocessor<br>//Powered By fileedit<br>//Powered by Example Builder<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>