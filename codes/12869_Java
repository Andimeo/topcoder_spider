            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class MiniatureDachshund<br>{<br>  public int maxMikan(int[] mikan, int weight)<br>  {    <br>    Arrays.sort(mikan);<br>    int c  = 0;<br>    for(int i=0;i&lt;mikan.length;i++)<br>    {<br>      if((weight+mikan[i])&gt;5000)<br>        break;<br>      weight = weight + mikan[i];<br>      c++;<br>    }<br>    return c;<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>