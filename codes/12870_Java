            import java.util.*;<br> <br>public class BigFatInteger2 {<br>    public String isDivisible(int A, int B, int C, int D) {<br>        for (int i = 2; i * i &lt;= Math.max(A, C); ++i) {<br>            long kC = 0;<br>            while (C % i == 0) {<br>                C /= i;<br>                kC += D;<br>            }<br>            long kA = 0;<br>            while (A % i == 0) {<br>                A /= i;<br>                kA += B;<br>            }<br>            if (kC &gt; kA) return "not divisible";<br>        }<br>        if (C == 1)   return "divisible";<br>        if (A == C &amp;&amp;  B &gt;= D) return "divisible";<br>        return "not divisible";<br>    }<br>}<br></td>