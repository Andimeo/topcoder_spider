            // Bekzhan Bekbolatuly<br><br>#include &lt;cmath&gt;<br>#include &lt;ctime&gt;<br>#include &lt;iostream&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;time.h&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br><br>using namespace std;<br><br>#define pb push_back<br>#define mp make_pair<br>#define INF 2000000000<br>#define mod 1000000007<br>#define abs(x) ((x)&gt;=0?(x):-(x))<br>#define fi first<br>#define se second<br><br>typedef long long ll;<br><br>ll d[55][55];<br><br>ll gcd(ll a, ll b)<br>{<br>    if(!b)<br>    return a;<br>    return gcd(b,a%b);<br>}<br><br>class SimilarNames2 {<br>public:<br>   int count( vector &lt;string&gt; s, int l ) {<br>        ll n=s.size(),ans=0,calc=1;<br>        for(ll i=1;i&lt;=n-l;i++)<br>            calc*=i,calc%=mod;<br>        for(ll i=0;i&lt;n;i++)<br>            d[i][1]=1;<br>        for(ll len=2;len&lt;=l;len++)<br>            for(ll i=0;i&lt;n;i++)<br>            for(ll j=0;j&lt;n;j++)<br>            if(i!=j&amp;&amp;s[j].size()&lt;=s[i].size())<br>            {<br>                if(s[i].substr(0,s[j].size())==s[j])<br>                d[i][len]+=d[j][len-1],d[i][len]%=mod;<br>            }<br>        for(ll i=0;i&lt;n;i++)<br>            ans+=d[i][l]*calc,ans%=mod;<br>        return ans;<br>   }<br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>