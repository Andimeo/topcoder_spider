            using System;<br>using System.Collections;<br><br>public class Ordered <br>{<br>  public static bool inc (int[] v)<br>  {<br>    for (int i = 0; i &lt; v.Length - 1; i++)<br>      if (!(v[i] &lt; v[i + 1]))<br>        return false;<br><br>    return true;<br>  }<br><br>  public static bool noninc (int[] v)<br>  {<br>    for (int i = 0; i &lt; v.Length - 1; i++)<br>      if (!(v[i] &gt;= v[i + 1]))<br>        return false;<br><br>    return true;<br>  }<br><br>  public static bool dec (int[] v)<br>  {<br>    for (int i = 0; i &lt; v.Length - 1; i++)<br>      if (!(v[i] &gt; v[i + 1]))<br>        return false;<br><br>    return true;<br>  }<br><br>  public static bool nondec (int[] v)<br>  {<br>    for (int i = 0; i &lt; v.Length - 1; i++)<br>      if (!(v[i] &lt;= v[i + 1]))<br>        return false;<br><br>    return true;<br>  }<br><br>  public static int gcd (int x, int y)<br>  {<br>    if (x == 0)<br>      return y;<br>    else<br>      return gcd(y % x, x);<br>  }<br><br>  public static string mean (int[] v)<br>  {<br>    int s = 0;<br><br>    foreach (int x in v)<br>      s += x;<br><br>    int g = gcd(s, v.Length);<br><br>    return string.Format("{0}/{1}", s / g, v.Length / g);<br>  }<br><br>  public static int freq (int[] v)<br>  {<br>    int bc = 0;<br>    int bn = 0;<br><br>    foreach (int x in v)<br>    {<br>      int c = 0;<br><br>      foreach (int y in v)<br>        if (x == y)<br>          c++;<br><br>      if (c &gt; bc)<br>      {<br>        bc = c;<br>        bn = x;<br>      }<br>    }<br><br>    return bc;<br>  }<br><br>  public static string getType (int[] v) <br>  {<br>    if (inc(v))<br>      return string.Format("ASCENDING {0}", mean(v));<br>    else if (dec(v))<br>      return string.Format("DESCENDING {0}", mean(v));<br>    else if (noninc(v))<br>      return string.Format("NONASCENDING {0}", freq(v));<br>    else if (nondec(v))<br>      return string.Format("NONDESCENDING {0}", freq(v));<br>    else<br>      return "NOTHING";<br>  }<br><br>  public static void Main ()<br>  {<br>    Console.WriteLine(getType(new int[] {1,2,4,11}));<br>    Console.WriteLine(getType(new int[] {1,2,2,2,3,4}));<br>    Console.WriteLine(getType(new int[] {6,5,1}));<br>    Console.WriteLine(getType(new int[] {5,5,4,4,1}));<br>    Console.WriteLine(getType(new int[] {1,2,3,4,1}));<br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>