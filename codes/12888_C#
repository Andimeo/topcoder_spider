            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class ORSolitaire {<br>    public int getMinimum(int[] numbers, int goal)<br>    {<br>        int n = numbers.Length;<br>        bool[] ng = new bool[n];<br>        for (int i = 0; i &lt; n; i++)<br>        {<br>            if ((goal | numbers[i]) != goal) ng[i] = true;<br>        }<br><br>        int ret = n;<br>        for (int i = 0; i &lt; 31; i++)<br>        {<br>            int a = 1 &lt;&lt; i;<br>            if ((goal &amp; a) == 0) continue;<br>            int count = 0;<br>            for (int j = 0; j &lt; n; j++)<br>            {<br>                if (ng[j]) continue;<br>                if ((numbers[j] &amp; a) != 0) count++;<br>            }<br>            ret = Math.Min(ret, count);<br>        }<br>        return ret;<br>    }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] Modified for C# by ysn<br>// Powered by CodeProcessor<br></td>