            # -*- coding: utf-8 -*-<br>import math,string,itertools,fractions,heapq,collections,re,array,bisect<br> <br>class ORSolitaire:<br>    def getMinimum(self, numbers, goal):<br>        counts = [0 for x in range(31)]<br>        ored = 0<br>        for a in numbers:<br>            if (a | goal) != goal:<br>                continue<br>            ored |= goal<br>            for i in range(31):<br>                if (a &gt;&gt; i) &amp; 1:<br>                    counts[i] += 1<br> <br>        if ored != goal:<br>            return 0<br> <br>        ret = 1000<br>        for i in range(31):<br>            if (goal &gt;&gt; i) &amp; 1:<br>                ret = min(ret, counts[i])<br>        return ret<br> <br># Powered by Greed 2.0-beta<br></td>