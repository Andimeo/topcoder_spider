            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class ORSolitaireDiv2<br>{<br>  public int getMinimum(int[] numbers, int goal)<br>  {<br>    List&lt;int&gt; candidates = new List&lt;int&gt;();<br>    for (int i = 0; i &lt; numbers.Length; i++)<br>      if ((numbers[i] | goal) == goal)<br>        candidates.Add(numbers[i]);<br> <br>    if (candidates.Count == 0) return 0; //impossible<br> <br>    int[] f = new int[32];<br>    for (int i = 0; i &lt; candidates.Count; i++)<br>      for (int j = 0; j &lt; 32; j++)<br>        if (((candidates[i] &gt;&gt; j) &amp; 1) == 1)<br>          f[j]++;<br> <br>    int ans = int.MaxValue;<br>    for (int i = 0; i &lt; 32; i++)<br>    {<br>      if (((goal &gt;&gt; i) &amp; 1) == 1)<br>      {<br>        if (f[i] == 0) return 0; //impossible<br>        ans = Math.Min(ans, f[i]);<br>      }<br>    }<br> <br>    if (ans == int.MaxValue)<br>      return 0;<br> <br>    return ans;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.8 (beta) modified by pivanof!<br></td>