            public class ORSolitaireDiv2{<br>//  public static void main(String args[]) {<br>//    int numbers[]= {503, 505, 152, 435, 491, 512, 1023, 355, 510, 500, 502, 255, 63, 508, 509, 511, 60, 250, 254, 346};<br>//    System.out.println(getMinimum(numbers, 510));<br>//  }<br>  <br>  public int getMinimum(int[] numbers, int goal) {<br>    for (int i=0;i&lt;numbers.length;i++) {<br>      if ((goal | numbers[i]) != goal) {<br>        numbers[i] = -1;<br>      }<br>    }<br>    <br>    int ans=0;<br>    for (int i=0; i&lt;numbers.length;i++) {<br>      if (numbers[i]!=-1) {<br>        ans|=numbers[i];<br>      }<br>    }<br>    <br>    if (ans != goal) {<br>      return 0;<br>    }<br>    <br>    int min = 20;<br>    for (int i=0;i&lt;=31;i++) {<br>      int p = 1&lt;&lt;i;<br>      if (p&gt;goal) {<br>        break;<br>      }<br>      if ((goal&amp;p)!=0) {<br>        int cnt=0;<br>        for (int j=0;j&lt;numbers.length;j++) {<br>          if (numbers[j]==-1) {<br>            continue;<br>          }<br>          if ((numbers[j]&amp;p)!=0) {<br>            cnt++;<br>          }<br>        }<br>        if (cnt &lt; min) {<br>          min = cnt;<br>        }<br>      }<br>    }<br>    return min;<br>  }<br>}<br></td>