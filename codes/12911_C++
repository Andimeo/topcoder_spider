            #include &lt;functional&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;iostream&gt;<br>#include &lt;fstream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;numeric&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br>#include &lt;climits&gt;<br>#include &lt;string&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;vector&gt;<br>#include &lt;queue&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;cmath&gt;<br>#include &lt;list&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;ctime&gt;<br>using namespace std;<br>class DivideByZero<br>{<br>public:<br>        int CountNumbers(vector &lt;int&gt; numbers)<br>        {<br>            int res=numbers.size();<br>            int ck[200];<br>            memset(ck,0,sizeof(ck));<br>            for(int i=0;i&lt;numbers.size();i++){<br>                ck[numbers[i]]=1;<br>                // cout&lt;&lt;numbers[i];<br>            }<br>            while(true){<br>                int n=0;<br>                vector&lt;int&gt; tmp;<br>                sort(numbers.begin(),numbers.end());<br>                for(int i=numbers.size()-1;i&gt;0;i--){<br>                    for(int j=i-1;j&gt;=0;j--){<br>                        int tp=numbers[i]/numbers[j];<br>                        if(ck[tp]==0){<br>                            tmp.push_back(tp);<br>                            n++;<br>                            ck[tp]=1;<br>                        }<br>                        <br>                    }<br>                }<br>                if(n==0) break;<br>                for(int i=0;i&lt;tmp.size();i++)<br>                    numbers.push_back(tmp[i]);<br>                res+=n;<br>                //cout&lt;&lt;res;<br>            }<br>            return int(res) ;<br>        }<br> <br>};<br> <br> <br> <br>// Powered by FileEdit<br></td>