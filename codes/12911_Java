            //package com.hissain.in.helper.topcoder.srm610;<br> <br>import java.util.ArrayList;<br>import java.util.Arrays;<br> <br>public class DivideByZero {<br>  public static int CountNumbers(int[] numbers) {<br>    ArrayList&lt;Integer&gt; a = new ArrayList&lt;Integer&gt;();<br> <br>    for (int i = 0; i &lt; numbers.length; i++) {<br>      a.add(numbers[i]);<br>    }<br> <br>    for (int i = 0; i &lt; a.size(); i++) {<br>      for (int j = 0; j &lt; a.size(); j++) {<br>        if (i != j) {<br>          if (a.get(i) &lt; a.get(j)) {<br>            int nn = a.get(j) / a.get(i);<br> <br>            if (!a.contains(nn)) {<br>              a.add(nn);<br>              i = 0;<br>              j = 0;<br>            }<br>          }<br>        }<br>      }<br>    }<br>    return a.size();<br>  }<br>}<br></td>