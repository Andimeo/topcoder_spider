            // never forget to say twice say twice!<br> <br>#include &lt;algorithm&gt;<br>#include &lt;string&gt;<br>using namespace std;<br> <br>#line 3 "PowerOfThree.cpp"<br> <br>int m3(int a) {<br>  a %= 3;<br>  if(a&lt;0) a+=3;<br>  if(a==2) return -1;<br>  return a;<br>  }<br> <br>int d3(int a) {<br>  return (a - m3(a)) / 3;<br>  }<br> <br>class PowerOfThree {<br>  public:<br>  string ableToGet(int x, int y) {<br>    // !FDI<br> <br>    if(x == 0 &amp;&amp; y == 0) return "Possible";<br>    int u0 = m3(x), u1 = m3(y);<br>    if(u0 == 0 &amp;&amp; u1 == 0) return "Impossible";<br>    if(u0 != 0 &amp;&amp; u1 != 0) return "Impossible";<br>    return ableToGet(d3(x), d3(y));<br>    }<br>  };<br> <br> <br> <br>// Powered by FileEdit<br>// Powered by moj 4.17 [modified TZTester]<br>// Powered by CodeProcessor<br></td>