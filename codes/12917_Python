            # -*- coding: utf-8 -*-<br>import math,string,itertools,fractions,heapq,collections,re,array,bisect<br> <br>def s3(x):<br>    ret = []<br>    while x &gt; 0:<br>        ret.append(x % 3)<br>        x /= 3<br>    return ret<br> <br>class PowerOfThree:<br>    def ableToGet(self, x, y):<br>        if x &lt; 0: x = -x<br>        if y &lt; 0: y = -y<br>        px = s3(x)<br>        py = s3(y)<br>        while x &gt; 0 or y &gt; 0:<br>            if (x % 3 and y % 3) or (x % 3 == 0 and y % 3 == 0):<br>                return "Impossible"<br> <br>            if x % 3 == 1:<br>                x -= 1<br>            elif x % 3 == 2:<br>                x += 1<br>            elif y % 3 == 1:<br>                y -= 1<br>            elif y % 3 == 2:<br>                y += 1<br>            x /= 3<br>            y /= 3<br>        #print ""<br>        #print px<br>        #print py<br>        return "Possible"<br> <br># Powered by Greed 2.0-beta<br></td>