            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Linq;<br> <br>public class LCMSet<br>{<br>    public int gcd(int a, int b)<br>    {<br>        int c;<br>        while (b != 0)<br>        {<br>            c = a%b;<br>            a = b;<br>            b = c;<br>        }<br>        return a;<br>    }<br> <br>    public int lcm(int a, int b)<br>    {<br>        return a/gcd(a, b)*b;<br>    }<br> <br>    public int lcm(IEnumerable&lt;int&gt; A)<br>    {<br>        int res = 1;<br>        foreach (var i in A)<br>        {<br>            res = lcm(res, i);<br>        }<br>        return res;<br>    }<br> <br>    public bool Decomposed(int[] A, int a)<br>    {<br>        int k = lcm(A.Where(x =&gt; a%x == 0));<br>        return k == a;<br>    }<br> <br>  public string equal(int[] A, int[] B)<br>  {<br>        foreach (var a in A)<br>        {<br>            if (!Decomposed(B, a))<br>                return "Not equal";<br>        }<br> <br>        foreach (var b in B)<br>        {<br>            if (!Decomposed(A, b))<br>                return "Not equal";<br>        }<br>      return "Equal";<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>