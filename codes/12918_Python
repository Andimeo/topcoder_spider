            import math,string,itertools,fractions,heapq,collections,re,array,bisect,random<br> <br> <br>def gcd(a, b):<br>    while b &gt; 0:<br>        c = a % b<br>        a = b<br>        b = c<br>    return a<br> <br> <br>def is_in_lcm(x, A):<br>    lcm = 1<br>    for y in A:<br>        if x % y == 0:<br>            lcm = lcm * y / gcd(lcm, y)<br>    return lcm == x<br> <br> <br>class LCMSet:<br>    def equal(self, A, B):<br>        for x in A:<br>            if not is_in_lcm(x, B):<br>                return "Not equal"<br>        for x in B:<br>            if not is_in_lcm(x, A):<br>                return "Not equal"<br>        return "Equal"<br> <br> <br> <br> <br> <br> <br> <br> <br> <br> <br>#Powered by KawigiEdit-pf 2.3.0!<br></td>