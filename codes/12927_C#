            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class TypoCoderDiv2<br>{<br>  public int count(int[] rating)<br>  {<br>    int re = rating[0] &gt;= 1200 ? 1 : 0;<br>    for(int i =0; i&lt; rating.Length - 1; i++)<br>    {<br>      if((rating[i] &lt; 1200 &amp;&amp; rating[i+1] &gt;= 1200) || (rating[i] &gt;= 1200 &amp;&amp; rating[i+1] &lt; 1200))<br>      {<br>        re++;<br>      }<br>    }<br><br>    return re;<br>  }<br>  <br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>