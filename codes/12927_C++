            #include &lt;iostream&gt;<br>#include &lt;cmath&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;cstdio&gt;<br>#include&lt;stack&gt;<br>#include&lt;vector&gt;<br>#include&lt;cstring&gt;<br>#include&lt;set&gt;<br>#include&lt;map&gt;<br>#include&lt;queue&gt;<br>#include&lt;string&gt;<br>typedef long long int LL;<br>typedef unsigned long long int ULL;<br>#define sf(a) scanf("%d",&amp;a);<br>#define pf(a) printf("%d",a);<br>#define sl(a) scanf("%lld",&amp;a);<br>#define pl(a) printf("%lld",a);<br>#define tr(container,it) for(typeof(container.begin()) it = container.begin(); it != container.end(); it++)<br>#define sz(a) int((a).size())<br>#define all(c) c.begin(),c.end() //all elements of a container<br>#define rall(c) c.rbegin(),c.rend() <br>using namespace std;<br>#define MAX(a, b) ((a) &gt; (b) ? (a) : (b))<br>#define MIN(a, b) ((a) &lt; (b) ? (a) : (b))<br>#define ABS(x) ((x) &gt; 0 ? (x) : -(x))<br>#define REP(i, n) for (int i = 0 ; i &lt; (n) ; i ++)<br>#define FOR(i, s, n) for (int i = (s) ; i &lt; (n) ; i ++)<br>#define SET(v, val) memset(v, val, sizeof(v))<br>#define mp(a,b)    make_pair(a,b)<br>#define pb(n)      push_back(n)<br>#define PI 3.14159265<br>#define F first<br>#define S second<br>typedef vector &lt;int&gt; vi;<br> <br>template&lt; class T &gt; T max(T &amp;a, T &amp;b) { return (!(a &lt; b) ? a : b); }<br>template&lt; class T &gt; T min(T &amp;a, T &amp;b) { return (a &lt; b ? a : b); }<br>template&lt; class T &gt; T gcd(T a, T b) { return (b != 0 ? gcd&lt;T&gt;(b, a%b) : a); }<br>template&lt; class T &gt; T lcm(T a, T b) { return (a / gcd&lt;T&gt;(a, b) * b); }<br>template&lt; class T &gt; T mod(T a, T b) { return (a &lt; b ? a : a % b); }<br> <br>int power(int a,int b,int c){   //a^b % c<br>    long long x=1,y=a; // long long is taken to avoid overflow of intermediate results<br>    while(b &gt; 0){<br>        if(b%2 == 1){<br>            x=(x*y)%c;<br>        }<br>        y = (y*y)%c; // squaring the base<br>        b /= 2;<br>    }<br>    return x%c;<br>}<br>class TypoCoderDiv2 {<br>  public:<br>  int count(vector &lt;int&gt; rating) {<br>    int ans=0;<br>    for(int i=0;i&lt;sz(rating)-1;i++)<br>    {<br>      if((rating[i]&gt;=1200 &amp;&amp; rating[i+1]&lt;1200) || (rating[i]&lt;1200 &amp;&amp; rating[i+1]&gt;=1200))<br>          ans++;<br>    }<br>    if(rating[0]&gt;=1200)<br>      ans++;<br>    return ans;<br>          <br>  }<br>};<br> <br> <br>// Powered by FileEdit<br>// Powered by TesterDream 1.2.4 by fushar (December 19 2012)<br>// Powered by CodeProcessor<br></td>