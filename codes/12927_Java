            // Paste me into the FileEdit configuration dialog<br><br>public class TypoCoderDiv2 {<br>   public int count(int[] rating) {<br>    int r = 0;<br>    int c = 500;<br>    for(int i = 0 ; i &lt; rating.length ; i++)<br>    {<br>      if(c &lt; 1200 &amp;&amp; rating[i] &gt;= 1200 || c &gt;= 1200 &amp;&amp; rating[i] &lt; 1200)<br>        r++;<br>      c = rating[i];<br>    }<br>    return r;<br>   }<br><br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by moj 4.16 [modified TZTester]<br>// Powered by CodeProcessor<br></td>