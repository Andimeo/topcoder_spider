<td class="problemText" colspan="8" valign="middle" align="left">
            Public Class TypoCoderDiv2<br>    Function count(ByVal rating As Integer()) As Integer<br>        Dim iCount As Integer = 0<br>        Dim iPrevious As Integer = 500<br><br>        For Each i As Integer In rating<br>            If (i &gt;= 1200 AndAlso iPrevious &lt; 1200) OrElse (i &lt; 1200 AndAlso iPrevious &gt;= 1200) Then<br>                iCount = iCount + 1<br>                iPrevious = i<br>            End If<br>        Next<br>        Return iCount<br>    End Function<br>End Class<br></td>