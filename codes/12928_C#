            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class PilingRectsDiv2 {<br>    public int getmax(int[] X, int[] Y, int limit) {<br>        int res=0;<br>        int len=X.Length;<br>        int[] xs = new int[len];<br>        int[] ys = new int[len];<br>        for(int n=0; n&lt;len; n++)<br>            if(X[n]&lt;Y[n]){<br>                xs[n]=X[n];<br>                ys[n]=Y[n];<br>            }else{<br>                xs[n]=Y[n];<br>                ys[n]=X[n];<br>            }<br><br>        for(int n=1; n&lt;=200; n++)<br>            for(int m=1; m&lt;=200; m++)<br>                if (n * m &gt;= limit)<br>                {<br><br>                    int cnt = 0;<br>                    for (int a = 0; a &lt; len; a++)<br>                        if (xs[a] &gt;= n &amp;&amp; ys[a] &gt;= m)<br>                            cnt++;<br>                    if (cnt &gt; res) res = cnt;<br>                }<br>        if (res == 0) return -1;<br>        return res;<br>    }<br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>