<td class="problemText" colspan="8" valign="middle" align="left">
            public class PilingRectsDiv2 {<br>  public int getmax(int[] X, int Y[], int lim) {<br>    int len = X.length;<br>    int max = -1;<br>    for (int d=1; d&lt;=lim; d++) {<br>      int e = ceil(lim, d);<br>      <br>      int num = 0;<br>      for (int i = 0; i&lt;len; i++) {<br>        if (X[i] &gt;= d &amp;&amp; Y[i] &gt;= e) num++;<br>        else if (Y[i] &gt;= d &amp;&amp; X[i] &gt;= e) num++;<br>      }<br>      <br>      if (num &gt; 0) max = Math.max(max, num);<br>    }<br>    return max;<br>  }<br>  <br>  int ceil(int a, int b) {<br>    if (a%b==0) return a/b;<br>    else return a/b+1;<br>  }<br>}<br></td>