            import math,string,itertools,fractions,heapq,collections,re,array,bisect,random<br> <br>class PilingRectsDiv2:<br>    def getmax(self, X, Y, limit):<br>        biggest = -1<br>        for x in xrange(1, 201):<br>            for y in xrange(1, 201):<br>                if x*y &lt; limit:<br>                    continue<br>                cnt = 0<br>                for i in xrange(len(X)):<br>                    if X[i] &gt;= x and Y[i] &gt;= y:<br>                      cnt+=1<br>                    elif Y[i] &gt;= x and X[i] &gt;= y:<br>                      cnt+=1<br>                if cnt:<br>                    biggest = max(biggest, cnt)<br>        return biggest<br> <br> <br>#Powered by KawigiEdit-pf 2.3.0!<br></td>