            #include&lt;iostream&gt;<br>#include&lt;cstdio&gt;<br>#include&lt;cstring&gt;<br>#include&lt;algorithm&gt;<br>#include&lt;cstdlib&gt;<br>#include&lt;vector&gt;<br>#include&lt;string&gt;<br>#include&lt;cmath&gt;<br> <br>using namespace std;<br>int n;<br>long long jc[55],jca[55],dp[55][55],p2[55];<br>long long exgcd(long long x,long long p,long long w)<br>{<br>  x%=p;<br>  w%=p;<br>  if(w==0)<br>    return 0;<br>  return (exgcd(p,x,(x*p-w)%x)*p+w)/x;<br>}<br>class BlackBoxDiv2<br>{<br>  public:<br>  int i,j,k,n=0,m=0;<br>  int count(string front, string side)<br>  {<br>    for(i=0;i&lt;front.size();++i)<br>      if(front[i]=='B')<br>        ++n;<br>    for(i=0;i&lt;side.size();++i)<br>      if(side[i]=='B')<br>        ++m;<br>    if(n==0&amp;&amp;m==0)<br>      return 1;<br>    if(n==0||m==0)<br>      return 0;<br>    jc[0]=jca[0]=1;<br>    for(i=1;i&lt;=50;++i)<br>    {<br>      jc[i]=jc[i-1]*i%1000000007;<br>      jca[i]=exgcd(jc[i],1000000007,1);<br>    }<br>    dp[0][0]=1;<br>    p2[0]=1;<br>    for(i=1;i&lt;=50;++i)<br>      p2[i]=p2[i-1]*2%1000000007;<br>    for(i=1;i&lt;=n;++i)<br>      for(j=1;j&lt;=m;++j)<br>      {<br>        for(k=0;k&lt;j;++k)<br>          dp[i][j]=(dp[i][j]+dp[i-1][k]*p2[k]%1000000007*jc[m-k]%1000000007*jca[j-k]%1000000007*jca[m-j]%1000000007)%1000000007;<br>        dp[i][j]=(dp[i][j]+dp[i-1][j]*(p2[j]-1)%1000000007)%1000000007;<br>      }<br>    return (int)dp[n][m];  <br>  }<br> <br>};<br></td>