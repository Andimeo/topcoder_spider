            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class SplitIntoPairs<br>{<br>    public int makepairs(int[] A, int X)<br>    {<br>        Array.Sort(A);<br>        int count = 0;<br>        for (int i = 0; i &lt; A.Length; i += 2)<br>        {<br>            long num = (long)A[i] * (long)A[i + 1];<br>            if (num &gt;= X)<br>            {<br>                count++;<br>            }<br>        }<br> <br>        return count;<br>    }<br>}<br> <br>//Powered by KawigiEdit 2.1.8 (beta) modified by pivanof!<br></td>