            #include &lt;cmath&gt;<br>#include &lt;ctime&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>using namespace std;<br><br>class SplitIntoPairs {<br>public:<br>  int makepairs( vector &lt;int&gt; A, int X ) {<br>    sort( A.begin(), A.end() );<br>    int ret = 0;<br>    for( int i = 0; i &lt; A.size(); i += 2 ) {<br>      if( (long long)A[i] * A[i + 1] &gt;= (long long)X ) ++ret;<br>    }<br>    return ret;<br>  }<br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by moj 4.17 [modified TZTester]<br>// Powered by CodeProcessor<br></td>