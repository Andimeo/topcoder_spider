            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class SplitIntoPairs<br>{<br>  public int makepairs(int[] A, int X)<br>  {<br>    Arrays.sort(A);<br>    int n = A.length;<br>    int res = 0;<br>    for(int i=0;i&lt;n;i+=2)<br>    {<br>      if((long)A[i]*(long)A[i+1]&gt;=X)<br>        res++;<br>    }  <br>    return res;<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>