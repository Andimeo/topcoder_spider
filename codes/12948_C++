            #include&lt;vector&gt;<br>#include&lt;string&gt;<br>#include&lt;algorithm&gt;<br>#include&lt;set&gt;<br>#include&lt;map&gt;<br>#include&lt;queue&gt;<br>#include&lt;cmath&gt;<br>#include&lt;cstdio&gt;<br>#include&lt;iostream&gt;<br>#include&lt;sstream&gt;<br>#include&lt;cstdlib&gt;<br>#define MOD 1000000007<br>#define ADD(X,Y) ((X) = ((X) + (Y)%MOD) % MOD)<br>using namespace std;<br>typedef long long i64; typedef vector&lt;int&gt; ivec; typedef vector&lt;string&gt; svec;<br><br>class AlienAndHamburgers {<br>  public:<br>  int getNumber(vector &lt;int&gt; type, vector &lt;int&gt; taste)<br>  {<br>    int ret = 0;<br>    <br>    vector&lt;int&gt; gn;<br>    for(int i=1;i&lt;=100;i++){<br>      vector&lt;int&gt; V;<br>      for(int j=0;j&lt;type.size();j++) if(type[j] == i) V.push_back(taste[j]);<br>      if(V.size()==0) continue;<br><br>      sort(V.begin(), V.end());<br>      int sum = 0;<br>      sort(V.begin(), V.end());<br>      reverse(V.begin(), V.end());<br>      for(int j=0;j&lt;V.size();j++){<br>        if(j&gt;0 &amp;&amp; V[j]&lt;0) break;<br>        sum += V[j];<br>      }<br><br>      gn.push_back(sum);<br>    }<br><br>    sort(gn.begin(), gn.end());<br>    reverse(gn.begin(), gn.end());<br>    int s = 0;<br>    for(int i=0;i&lt;gn.size();i++){<br>      s += gn[i];<br>      //printf("%d\n", gn[i]);<br>      ret = max(ret, s*(i+1));<br>    }<br><br>    return ret;<br>  }<br>  <br><br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>