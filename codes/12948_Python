            import math,string,itertools,fractions,heapq,collections,re,array,bisect,random<br> <br>class AlienAndHamburgers:<br>    def getNumber(self, type, taste):<br>        eaten = set()<br>        s = 0<br>        n = len(taste)<br>        extra = []<br>        for i in range(n):<br>            if taste[i] &gt;= 0:<br>                s += taste[i]<br>                eaten.add(type[i])<br>            else:<br>                extra.append([-taste[i], type[i]])<br> <br>        extra.sort()<br>        t = len(eaten)<br>        res = s * t<br>        for x in extra:<br>            if not x[1] in eaten:<br>                s -= x[0]<br>                t += 1<br>                eaten.add(x[1])<br>                res = max(res, s * t)<br> <br>        return res<br> <br> <br>#Powered by KawigiEdit-pf 2.3.0!<br></td>