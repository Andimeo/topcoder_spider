            using System;<br>using System.Collections.Generic;<br> <br>public class AlienAndPassword<br>{<br>  public int getNumber (string S)<br>  {<br>    HashSet&lt;string&gt; hs = new HashSet&lt;string&gt; ();<br>    for (int i=0; i&lt;S.Length; i++) {<br>      hs.Add (S.Remove (i, 1));<br>    }<br>    return hs.Count;<br>  }<br>}<br></td>