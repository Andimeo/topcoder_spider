            import java.util.HashSet;<br><br>// Paste me into the FileEdit configuration dialog<br><br>public class AlienAndPassword {<br>   public int getNumber(String S) {<br>    HashSet&lt;String&gt; h = new HashSet&lt;String&gt;();<br>    for(int i =0; i &lt; S.length() ; i++)<br>    {<br>      h.add(S.substring(0 , i ) + S.substring(i + 1));<br>    }<br>    return h.size();<br>   }<br><br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by moj 4.16 [modified TZTester]<br>// Powered by CodeProcessor<br></td>