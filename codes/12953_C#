            using System;<br><br>public class FoxAndWord<br>{<br>    public int howManyPairs(string[] words)<br>    {<br>        int answer = 0;<br><br>        for (int i = 0; i &lt; words.Length; i++)<br>        {<br>            for (int j = i + 1; j &lt; words.Length; j++)<br>            {<br>                string a = words[i];<br>                string b = words[j];<br><br>                if (a.Length == b.Length)<br>                {<br>                    for (int k = 1; k &lt; a.Length; k++)<br>                    {<br>                        string c = a.Substring(k,a.Length-k) + a.Substring(0, k);<br><br>                        if (c.Equals(b))<br>                        {<br>                            answer++;<br>                            break;<br>                        }<br>                    }<br>                }<br>            }<br>        }<br><br>        return answer;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>