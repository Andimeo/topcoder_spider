            #include &lt;iostream&gt;<br>#include &lt;stdio.h&gt;<br>#include &lt;stdlib.h&gt;<br>#include &lt;stack&gt;<br>#include &lt;vector&gt;<br>#include &lt;string.h&gt;<br><br>using namespace std;<br><br>class FoxAndWord<br>{<br>    public:<br>        int howManyPairs(vector &lt;string&gt; words)<br>        {<br>            int count = 0, i, j, k, m;<br>            int n = words.size();<br>            for(i=0; i&lt;n-1; i++)<br>            {<br>                int len1 = words[i].size();<br>                for(j=i+1; j&lt;n; j++)<br>                {<br>                    int ctr = 0;<br>                    int len2 = words[j].size();<br>                    if(len1==len2)<br>                    {<br>                        for(k=0; k&lt;len2; k++)<br>                        {<br>                            if(words[j][k]==words[i][0])<br>                            {<br>                                ctr = 1;<br>                                for(m=0; m&lt;len1; m++)<br>                                {<br>                                    if(words[j][(k+m)%len2]!=words[i][m])<br>                                    {<br>                                        ctr = 0;<br>                                        break;<br>                                    }<br>                                }<br>                            }<br>                            if(ctr==1)<br>                                break;<br>                        }<br>                    }<br>                    if(ctr==1)<br>                    {<br>                        cout&lt;&lt;words[i]&lt;&lt;" "&lt;&lt;words[j]&lt;&lt;endl;<br>                        count++;<br>                    }<br>                }<br>            }<br>            return count;<br>        }<br>};<br></td>