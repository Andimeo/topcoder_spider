            // Paste me into the FileEdit configuration dialog<br><br>public class FoxAndWord<br>{<br>  public int howManyPairs(String[] words)<br>  {<br>    int r = 0;<br>    for (int i = 0; i &lt; words.length; i++)<br>      for (int j = i + 1; j &lt; words.length; j++)<br>      {<br>        if (words[i].length() == words[j].length())<br>        {<br>          for(int k = 0 ; k &lt; words[i].length() ; k++)<br>          {<br>            String a = words[i].substring(0, k);<br>            String b = words[i].substring(k);<br>            if( (b + a).equals(words[j]))<br>            {<br>              r++;<br>              break;<br>            }<br>          }<br>        }<br>      }<br>    return r;<br>  }<br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by moj 4.16 [modified TZTester]<br>// Powered by CodeProcessor<br></td>