            # -*- coding: utf-8 -*-<br>import math,string,itertools,fractions,heapq,collections,re,array,bisect<br> <br>class FoxAndWord:<br>    def howManyPairs(self, words):<br>        words = list(set(words))<br>        t = 0<br>        for i in range(len(words)-1):<br>            for j in range(i+1,len(words)):<br>                for k in range(1,len(words[i])):<br>                    if words[j] == words[i][k:] + words[i][:k]:<br>                        t += 1<br>                        break<br>        return t<br> <br># Powered by Greed 2.0-beta<br></td>