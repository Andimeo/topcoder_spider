            //program PalindromicSubstringsDiv1<br><br>#include&lt;iostream&gt;<br>#include&lt;cstdio&gt;<br>#include&lt;cstdlib&gt;<br>#include&lt;cstring&gt;<br>#include&lt;algorithm&gt;<br>#include&lt;vector&gt;<br><br>using namespace std;<br><br>class PalindromicSubstringsDiv1<br>{<br>public:<br>  double expectedPalindromes(vector&lt;string&gt; a,vector&lt;string&gt; b)<br>    {<br>      string s="";<br>      for(int i=0;i&lt;a.size();i++)<br>        s+=a[i];<br>      for(int i=0;i&lt;b.size();i++)<br>        s+=b[i];<br>      int n=s.size();<br>      double ans=0;<br>      for(int i=0;i&lt;n;i++)<br>        {<br>          ans+=1;<br>          double t=1;<br>          for(int l=i-1,r=i+1;l&gt;=0&amp;&amp;r&lt;n;l--,r++)<br>            {<br>              if(s[l]=='?'||s[r]=='?')<br>                t/=26;<br>              else if(s[l]!=s[r])<br>                t=0;<br>              ans+=t;<br>            }<br>        }<br>      for(int i=0;i&lt;n-1;i++)<br>        {<br>          double t=1;<br>          for(int l=i,r=i+1;l&gt;=0&amp;&amp;r&lt;n;l--,r++)<br>            {<br>              if(s[l]=='?'||s[r]=='?')<br>                t/=26;<br>              else if(s[l]!=s[r])<br>                t=0;<br>              ans+=t;<br>            }<br>        }<br>      return ans;<br>    }<br>};<br></td>