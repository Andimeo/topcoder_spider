  v1.push_back(make_pair(j,k)) ;<br>            vector&lt; pair&lt;int,int&gt; &gt; v2 ;<br>            for (int l=0; l&lt;=60; l++)<br>                for (int m=0; m&lt;=60; m++)<br>                    if (dp[i-1][l][m] != 0x3f3f3f3f)<br>                        v2.push_back(make_pair(l,m)) ;<br><br>            for (int x=0; x&lt;v1.size(); x++)<br>                for (int y=0; y&lt;v2.size(); y++)<br>                {<br>                    int j=v1[x].first,k=v1[x].second,l=v2[y].first,m=v2[y].second;<br>                    dp[i][j][k] = min(dp[i][j][k] , dp[i-1][l][m] + max(0,j-l) + max(0,k-m));<br>                }<br>        }<br>        int ans=1000000000;<br>        for (int j=0; j&lt;=60; j++)<br>            for (int k=0; k&lt;=60; k++)<br>                ans = min(ans , dp[n][j][k]);<br>        return ans ;<br><br>        }<br><br><br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>