            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class EllysSortingTrimmer<br>{<br>  public string getMin(string S, int L)<br>  {<br>    string res = S;<br>    for (int i = S.Length - L; i &gt;= 0; i--)<br>    {<br>      char[] c = S.Substring(i, L).ToCharArray();<br>      Array.Sort(c);<br>      S = S.Substring(0, i) + new string(c);<br>      if (S.CompareTo(res) &lt; 0) res = S;<br>    }<br>    return res;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>