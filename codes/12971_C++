            #include &lt;iostream&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;map&gt;<br>#include &lt;list&gt;<br>#include &lt;set&gt;<br>#include &lt;queue&gt;<br>#include &lt;stack&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br>#include &lt;bits/stdc++.h&gt;<br>#include &lt;cmath&gt;<br>#include &lt;climits&gt;<br>#define inf 99999<br>#define fr(i ,a ,b) for(int i = a; i &lt; b; i++)<br>#define rev(i, a, b) for(int i = b; i &gt;= a; i--)<br>#define fre(i, a, b) for(int i = a; i &lt;= b; i++)<br>#define inp(n) scanf("%d", &amp;n)<br>#define inpf(n) scanf("%f", &amp;n)<br>#define inpl(n) scanf("%lld", &amp;n)<br>#define out(n) printf("%d\n", n)<br>#define outl(n) printf("%lld\n", n)<br>#define ll long long int<br>#define sz size()<br>using namespace std;<br>class EllysSortingTrimmer {<br>  public:<br>  string getMin(string s, int l) {<br>      int p = s.size();<br>            int ct = 0;<br>            char ch[200];<br>            for (int i = 0; i &lt;= p - l; i++) {<br>                    sort(s.begin()+p-l-i, s.end()- i);<br>            }<br>            for (int i = 0; i &lt; l; i++) {<br>                        ch[i] = s[i];<br>            }<br>            ch[l] = '\0';<br>            return ch;<br><br>  }<br>};<br><br><br>// Powered by FileEdit<br>// Powered by TesterDream 1.2.4 by fushar (December 19 2012)<br>// Powered by CodeProcessor<br></td>