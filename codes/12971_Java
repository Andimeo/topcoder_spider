            import java.util.ArrayDeque;<br>import java.util.Arrays;<br>import java.util.Queue;<br>import java.util.Set;<br>import java.util.TreeSet;<br> <br>public class EllysSortingTrimmer {<br> <br>  public String getMin(String S, int L) {<br>    Queue&lt;String&gt; queue = new ArrayDeque&lt;&gt;();<br>    Set&lt;String&gt; set = new TreeSet&lt;&gt;();<br>    queue.add (S);<br>    <br>    while (! queue.isEmpty()) {<br>      String s = queue.remove();<br>      set.add(s);<br>      for (int i = 0; i &lt;= s.length() - L; i++) {<br>        String initPart = s.substring (0, i);<br>        String sortingPart = s.substring (i, i+L);<br>        sortingPart = sort(sortingPart);<br>        String tmpString = initPart + sortingPart;<br>        <br>        if(set.contains(tmpString)==false)<br>          queue.add(tmpString);<br>      }<br>    }<br>    return set.iterator().next();<br>  }<br>  <br>  String sort(String s) {<br>    char[] ch = s.toCharArray();<br>    Arrays.sort(ch);<br>    return new String(ch);<br>  }<br> <br>}<br></td>