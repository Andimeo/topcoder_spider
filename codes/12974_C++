            //<br>//  main.cpp<br>//  TCOR1A<br>//<br>//  Created by Ningchen Ying on 4/13/14.<br>//  Copyright (c) 2014 Ningchen Ying. All rights reserved.<br>//<br><br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;string.h&gt;<br>using namespace std;<br><br>class EllysScrabble {<br>public:<br>  string getMin(string, int);<br>};<br><br>string EllysScrabble::getMin(string A, int K) {<br>  int N = (int)A.length();<br>    string res;<br>    <br>    int ync[500];<br>  for(int i=0;i&lt;500;i++)ync[i]=0;<br>  <br>  for (int i = 0;i &lt; N;i++){<br>    if (i-K&gt;=0 &amp;&amp; !ync[i-K]){<br>      res+=A[i-K];<br>            ync[i-K] = 1;<br>      continue;<br>    }<br>    int mmx = 500;<br>        int cnt = -1;<br>    for (int j = 0;j&lt;N;j++){<br>            if (j-i&lt;=K &amp;&amp; j-i&gt;= -K &amp;&amp; !ync[j]){<br>                if (A[j]&lt;mmx){<br>                    mmx = A[j];<br>                    cnt = j;<br>                }<br>            }<br>        }<br>    ync[cnt] = 1;<br>    res+=A[cnt];<br>  }<br>  return res;<br>}<br></td>