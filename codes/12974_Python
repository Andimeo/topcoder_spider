            import math,string,itertools,fractions,heapq,collections,re,array,bisect,random<br> <br>class EllysScrabble:<br>  def getMin(self, letters, maxDistance):<br>    m = maxDistance<br>    l = ["!"] * m + list(letters) + ["_"] * m<br>    s = len(letters)<br>    used = [1] * m + [0] * (s+m)<br>    res = []<br>    for i in range(m,m+s):<br>      if not used[i-m]:<br>        res += l[i-m]<br>        used[i-m] = 1<br>      else:<br>        avail = [(999 if used[j] else ord(l[j]), j) for j in range(i-m,i+m+1)]<br>        avail.sort(key = lambda x: x[0])<br>        res += l[avail[0][1]]<br>        used[avail[0][1]] = 1<br>    return "".join(res)<br> <br> <br>#Powered by KawigiEdit-pf 2.3.0!<br></td>