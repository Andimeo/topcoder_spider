            using System;<br>using System.Collections.Generic;<br>using System.Linq;<br><br>public class EllysNumberGuessing<br>{<br>    bool Test(int[] guesses, int[] answers,int t)<br>    {<br>        if (t &lt; 1)<br>        {<br>            return false;<br>        }<br>        if (t &gt; 1000000000)<br>        {<br>            return false;<br>        }<br>        int N = guesses.Length;<br>        for (int i = 0; i &lt; N; i++)<br>        {<br>            if (Math.Abs(t - guesses[i]) != answers[i])<br>            {<br>                return false;<br>            }<br>        }<br>        return true;<br><br>    }<br><br>    public int getNumber(int[] guesses, int[] answers)<br>    {<br>        int N = guesses.Length;<br>        HashSet&lt;int&gt; hs = new HashSet&lt;int&gt;();<br>        for (int i = 0; i &lt; N; i++)<br>        {<br>            if (Test(guesses, answers, guesses[i] - answers[i]))<br>            {<br>                hs.Add(guesses[i] - answers[i]);<br>            }<br>            if (Test(guesses, answers, guesses[i] + answers[i]))<br>            {<br>                hs.Add(guesses[i] + answers[i]);<br>            }<br>        }<br>        if (hs.Count==0)<br>        {<br>            return -2;<br>        }<br>        if (hs.Count&gt;1)<br>        {<br>            return -1;<br>        }<br>        return hs.First();<br>    }<br>}<br></td>