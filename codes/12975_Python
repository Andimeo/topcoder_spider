            class EllysNumberGuessing:<br>  def getNumber(self, guesses, answers):<br>    posSet = getSet(guesses[0], answers[0])<br>    for i in range(1, len(guesses)):<br>      posSet &amp;= getSet(guesses[i], answers[i])<br>    if len(posSet) == 1:<br>      return list(posSet)[0]<br>    if len(posSet) &gt; 0:<br>      return -1<br>    return -2<br>  <br>def getSet(guess, answer):<br>  high = guess + answer<br>  low = guess - answer<br>  ans = set()<br>  if high &lt;= 1000000000:<br>    ans.add(high)<br>  if low &gt;= 1:<br>    ans.add(low)<br>  return ans<br></td>