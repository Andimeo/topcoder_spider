<td class="problemText" colspan="8" valign="middle" align="left">
            import java.util.Arrays;<br> <br>/**<br> * Built using CHelper plug-in<br> * Actual solution is at the top<br> */<br>public class MinimumSquare {<br>    public long minArea(int[] x, int[] y, int K) {<br>        int[] tmp = new int[x.length];<br>        long res = Long.MAX_VALUE;<br>        for (int sx : x)<br>            for (int sy : y) {<br>                int ptr = 0;<br>                for (int i = 0; i &lt; x.length; ++i) {<br>                    if (x[i] &gt;= sx &amp;&amp; y[i] &gt;= sy) {<br>                        tmp[ptr++] = Math.max(x[i] - sx, y[i] - sy);<br>                    }<br>                }<br>                if (ptr &gt;= K) {<br>                    Arrays.sort(tmp, 0, ptr);<br>                    long side = 2 + tmp[K - 1];<br>                    res = Math.min(res, side * side);<br>                }<br>            }<br>        return res;<br>    }<br>}<br></td>