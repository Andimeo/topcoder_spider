            import math,string,itertools,fractions,heapq,collections,re,array,bisect,random<br> <br>class MinimumSquare:<br>    def minArea(self, x, y, K):<br>        n = len(x)<br> <br>        a = list(x)<br>        b = list(y)<br>        a.sort()<br>        b.sort()<br>        res = 10**30<br>        for i in range(n):<br>            for j in range(n):<br>                ok = 0<br>                for k in range(n):<br>                    if x[k] &gt;= a[i] and y[k] &gt;= b[j]:<br>                        ok += 1<br>                if ok &gt;= K:<br>                    val = []<br>                    for k in range(n):<br>                        if x[k] &gt;= a[i] and y[k] &gt;= b[j]:<br>                            val.append(max(x[k]-a[i],y[k]-b[j]))<br>                    val .sort()<br>                    res = min(res, (val[K-1]+2)**2)<br> <br>        return res<br> <br> <br> <br> <br> <br>#Powered by KawigiEdit-pf 2.3.0!<br></td>