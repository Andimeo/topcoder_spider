            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class EllysSubstringSorter<br>{<br>  public string getMin(string S, int L)<br>  {<br>    List&lt;string&gt;done=new List&lt;string&gt;();<br>    for(int i=0;i&lt;=S.Length-L;i++)<br>    {<br>      string rep="";<br>      for(int j=i;j&lt;L+i;j++)<br>      {<br> <br>        rep+=S[j];<br>      }<br>      char[]cc=rep.ToCharArray();<br>      Array.Sort(cc);<br> <br>      done.Add(S.Substring(0,i)+(new string(cc))+S.Substring(i+L));<br>    }<br>    done.Sort();<br>    return done[0];<br>      <br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>