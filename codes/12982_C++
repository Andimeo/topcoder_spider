            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;iostream&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;utility&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;climits&gt;<br>#include &lt;cctype&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;ctime&gt;<br>#include &lt;queue&gt;<br>#include &lt;stack&gt;<br>#include &lt;iterator&gt;<br> <br>#define ll long long<br>#define ull unsigned long long<br>#define pb push_back<br>#define mp make_pair<br>#define ff first<br>#define ss second<br>using namespace std;<br> <br> <br>long long mina(vector&lt;int&gt;x,vector&lt;int&gt;y)<br>{<br>  if(x.size()==1)<br>  {<br>    return 4;<br>  }<br>  long long  xmin=INT_MAX,xmax=INT_MIN;<br>  for(int i=0;i&lt;(int)x.size();i++)<br>  {<br>    xmin=min(xmin,(ll)x[i]);<br>    xmax=max(xmax,(ll)x[i]);<br>  }<br>  long long  ymin=INT_MAX,ymax=INT_MIN;<br>  for(int i=0;i&lt;(int)y.size();i++)<br>  {<br>    ymin=min(ymin,(ll)y[i]);<br>    ymax=max(ymax,(ll)y[i]);<br>  }<br>  <br>  ll side = abs((ymax+1)-(ymin-1));<br>  side = max(side,abs((xmax+1)-(xmin-1)));<br>  return side*side;<br>  <br>}<br> <br>class MinimumSquareEasy <br>{<br>  public:<br>  long long minArea(vector &lt;int&gt; x, vector &lt;int&gt; y)<br>   {<br>    int n=x.size();<br>    ll ans = LONG_LONG_MAX;<br>    vector&lt;int&gt;xx,yy;<br>    for(int i=0;i&lt;n;i++)<br>    {<br>      xx.push_back(x[i]);<br>      yy.push_back(y[i]);<br>      <br>    }<br>    ans = min(ans,mina(x,y));<br>    for(int j=0;j&lt;n;j++)<br>    {<br>      xx.clear();<br>      yy.clear();<br>      for(int i=0;i&lt;n;i++)<br>      {<br>        if(i==j) continue;<br>        xx.push_back(x[i]);<br>        yy.push_back(y[i]);<br>        <br>      }<br>      ans = min(ans,mina(xx,yy));<br>    }<br>    for(int k=0;k&lt;n;k++){<br>    for(int j=0;j&lt;n;j++)<br>    {<br>      if(j==k) continue;<br>      xx.clear();<br>      yy.clear();<br>      for(int i=0;i&lt;n;i++)<br>      {<br>        if(i==k) continue;<br>        if(i==j) continue;<br>        xx.push_back(x[i]);<br>        yy.push_back(y[i]);<br>        <br>      }<br>      ans = min(ans,mina(xx,yy));<br>    }<br>  }<br>    <br>    return  ans;<br>   }<br>};<br> <br> <br> <br> <br>// Powered by FileEdit<br>// Powered by TesterDream 1.2.4 by fushar (December 19 2012)<br>// Powered by CodeProcessor<br></td>