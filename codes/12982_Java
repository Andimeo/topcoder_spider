            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br>public class MinimumSquareEasy {<br>  public long minArea(int[] x, int[] y) {<br>    long min = Long.MAX_VALUE;<br>    int n = x.length;<br>    for (int i = 0; i &lt; n; i++) {<br>      for (int j = i + 1; j &lt; n; j++) {<br>        int minx = Integer.MAX_VALUE, maxx = Integer.MIN_VALUE, miny = Integer.MAX_VALUE, maxy = Integer.MIN_VALUE;<br>        for (int k = 0; k &lt; n; k++) {<br>          if (k == i || k == j) continue;<br>          minx = Math.min(minx, x[k]);<br>          maxx = Math.max(maxx, x[k]);<br>          miny = Math.min(miny, y[k]);<br>          maxy = Math.max(maxy, y[k]);<br>        }<br>        long l = Math.max(maxy - miny, maxx - minx) + 2;<br>        min = Math.min(min, l * l);<br>      }<br>    }<br>    return min;<br>  }<br> <br>}<br> <br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>