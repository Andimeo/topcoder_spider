            #include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstring&gt;<br>#include &lt;ctime&gt;<br>#include &lt;iostream&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;set&gt;<br>#include &lt;vector&gt;<br>#include &lt;sstream&gt;<br>#include &lt;typeinfo&gt;<br>#include &lt;fstream&gt;<br>using namespace std;<br>#define MOD 1000000007<br>#define MAXN 56<br>int dp[MAXN][MAXN][MAXN][MAXN];<br>int rec(int s, int a, int b, int c)<br>{<br>    if (s == 0)<br>    {<br>        if (a + b + c == 0) return 1;<br>        return 0;<br>    }<br>    if (a + b + c &lt; s) return 0;<br>    int &amp;ret = dp[s][a][b][c];<br>    if (ret != -1) return ret;<br>    ret = 0;<br>    if (a) ret = (ret + rec(s-1,a-1,b,c)) % MOD;<br>    if (b) ret = (ret + rec(s-1,a,b-1,c)) % MOD;<br>    if (c) ret = (ret + rec(s-1,a,b,c-1)) % MOD;<br>    if (a &amp;&amp; b) ret = (ret + rec(s-1,a-1,b-1,c)) % MOD;<br>    if (b &amp;&amp; c) ret = (ret + rec(s-1,a,b-1,c-1)) % MOD;<br>    if (c &amp;&amp; a) ret = (ret + rec(s-1,a-1,b,c-1)) % MOD;<br>    if (a &amp;&amp; b &amp;&amp; c) ret = (ret + rec(s-1,a-1,b-1,c-1)) % MOD;<br>    return ret;<br>}<br>class VocaloidsAndSongs<br>{<br>public:<br>    int count(int S, int gumi, int ia, int mayu)<br>    {<br>        memset(dp, -1, sizeof(dp));<br>        return rec(S,gumi,ia,mayu);<br>        return 0;<br>    }<br>};<br> <br>// Powered by Greed 2.0-RC<br></td>