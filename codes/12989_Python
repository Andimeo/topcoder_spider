            import math,string,itertools,fractions,heapq,collections,re,array,bisect,random<br> <br>MODULO = 1000000007<br> <br>def comb(n,m):<br>  result = 1<br>  if m &gt; n/2:<br>    m = n-m<br>  for i in xrange(m):<br>    result = (result * (n-i))/(i+1) <br>  return result % MODULO<br> <br> <br>def helper(S, x, y, z):<br>  temp = (comb(S, x) * comb(S,y) * comb(S, z)) % MODULO<br>  return temp<br> <br>class VocaloidsAndSongs:<br>    def count(self, S, gumi, ia, mayu):<br>      if S &gt; sum([gumi, ia, mayu]):<br>        return 0<br>        result = 0<br>        temp = max(gumi, ia, mayu)<br>        for i in xrange(S-temp+1):<br>          result = (result + ((-1)**i)*comb(S, i)*helper(S-i,gumi,ia,mayu)) %MODULO<br>        return result % MODULO<br> <br> <br>#Powered by KawigiEdit-pf 2.3.0!<br></td>