            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;queue&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;list&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>using namespace std;<br> <br>#define debug(x)<br> <br>class PackingBallsDiv1 {<br>  public:<br>  int minPacks(int K, int A, int B, int C, int D) {<br>    vector&lt;int&gt; X(K);<br>    X[0]=A;<br>    for(int i=1; i&lt;K; i++)<br>    {<br>        X[i] = ((long long)X[i-1]*B+C)%D + 1;<br>    }<br>    int res = 0;<br>    for(int i=0; i&lt;K; i++)<br>    {<br>        res+=X[i]/K;<br>        X[i]%=K;<br>    }<br>    sort(X.begin(), X.end());<br>    int ans = res+K;<br>    for(int i=0; i&lt;K; i++)<br>    {<br>        ans=min(ans, res+X[i]+K-1-i);<br>    }<br>    return ans;<br>  }<br> <br> <br>};<br> <br> <br> <br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>