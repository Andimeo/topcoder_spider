            class PackingBallsDiv1:<br>  def minPacks(self, K, A, B, C, D):<br>    ans1 = 0<br>    x = []<br>    x.append(A)<br>    ans1 = x[0] / K;<br>    for i in range(K - 1):<br>      x.append((x[i] * B + C) % D + 1)<br>      ans1 += x[i + 1] / K<br><br>    for i in range(K):<br>      x[i] %= K<br>  <br>    x = sorted(x)<br>    ans = 234923748342792384<br><br>    s = ""<br><br>    for i in range(K + 1):<br>      if (i != 0):<br>        ans = min(ans, K - i + x[i - 1])<br>#        if (ans == n - i + x[i - 1]):<br>#          s = str(n) + "000" + str(i) + "000" + str(x[i - 1])<br>      else:<br>        ans = min(ans, K - i)<br>#        if (ans == n - i):<br>#          s = str(n) + "000" + str(i) + "000" + str(x[i - 1])<br><br>    return ans + ans1<br></td>