            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class MysticAndCandies {<br>    public int minBoxes(int C, int X, int[] low, int[] high)<br>    {<br>        int n = low.Length;<br>        Array.Sort(low);<br>        Array.Sort(high);<br>        int ret = n;<br>        int sum = 0;<br>        for (int i = 0; i &lt; n; i++)<br>        {<br>            sum += low[n - i - 1];<br>            if (sum &gt;= X)<br>            {<br>                ret = i + 1;<br>                break;<br>            }<br>        }<br><br>        sum = 0;<br>        for (int i = 0; i &lt; n; i++)<br>        {<br>            sum += high[i];<br>            if (C - sum &gt;= X) ret = Math.Min(ret, n - i - 1);<br>        }<br><br>        return ret;<br>    }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] Modified for C# by ysn<br>// Powered by CodeProcessor<br></td>