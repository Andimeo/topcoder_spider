            #include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;vector&gt;<br>#include &lt;cmath&gt;<br>#include &lt;iostream&gt;<br>#include &lt;map&gt;<br>using namespace std;<br>class MysticAndCandiesEasy{<br>public:<br>  int minBoxes(int C,int X,vector&lt;int&gt; high){<br>      int i;<br>      sort(high.begin(),high.end());<br>      i=0;<br>      while(C-high[i]&gt;=X){<br>          C-=high[i];<br>          i++;<br>      }  <br>      return high.size()-i;<br>  }<br>};<br></td>