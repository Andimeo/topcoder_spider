            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class OneDimensionalRobotEasy<br>{<br>  public int finalPosition(string commands, int A, int B)<br>    {<br>        int pos = 0;<br> <br>        for(int i = 0; i &lt; commands.Length; i++)<br>        {<br>            if (commands[i] == 'L' &amp;&amp; pos &gt; -A)<br>                pos--;<br>            else if (commands[i] == 'R' &amp;&amp; pos &lt; B)<br>                pos++;<br>        }<br>        return pos;<br>    }<br> <br> <br>}<br>//Powered by KawigiEdit-pfx 2.1.9!<br></td>