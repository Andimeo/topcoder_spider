<td class="problemText" colspan="8" valign="middle" align="left">
            #include &lt;cstdio&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>using namespace std;<br> <br>const int MAXN = 55;<br>int N,arr[MAXN][MAXN];<br> <br>class BigOEasy{<br>  public:<br>  string isBounded(vector &lt;string&gt; graph){<br>    for(int i=0;i&lt;graph.size();i++)  for(int j=0;j&lt;graph[i].length();j++) arr[i][j] = (graph[i][j] == 'Y');<br>    N = graph.size();<br>    <br>    for(int k=0;k&lt;N;k++) for(int i=0;i&lt;N;i++) for(int j=0;j&lt;N;j++) arr[i][j] |= (arr[i][k] &amp; arr[k][j]);<br>    <br>    for(int i=0;i&lt;N;i++) for(int j=0;j&lt;N;j++) for(int k=0;k&lt;N;k++){<br>      if(i == j || i == k || j == k) continue;<br>      if(graph[i][j] == 'Y' &amp;&amp; graph[i][k] == 'Y' &amp;&amp; arr[j][i] &amp;&amp; arr[k][i]) return "Unbounded";<br>    }<br>    return "Bounded";<br>  }<br>};<br></td>