            public class BigOEasy {<br> <br>  int n;<br>  char[][] grid;<br>  boolean[] visited;<br>  int found;<br>  void dfs(int i, int start) {<br>    if (i == start) found++;<br>    if (visited[i]) return;<br>    visited[i] = true;<br>    for (int j=0; j&lt;n; j++) {<br>      if (grid[i][j]=='N') continue;<br>      dfs(j, start);<br>    }<br>  }<br>  <br>  public String isBounded(String[] graph) {<br>    <br>    n = graph.length;<br>    grid = new char[n][n];<br>    for (int i=0; i&lt;n; i++) grid[i] = graph[i].toCharArray();<br>    <br> <br>    for (int i=0; i&lt;n; i++) {<br>      visited = new boolean[n];<br>      found = 0;<br>      dfs(i, i);<br>      if (found &gt; 2) {<br>        return "Unbounded";<br>      }<br>    }<br>    <br>    return "Bounded";<br>  }<br> <br>}<br></td>