            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class MagicalStringDiv1 {<br>    public int getLongest(string S) {<br>        int[] open = new int[S.Length];<br><br>        int count = 0;<br>        int ret = 0;<br>        for (int i = 0; i &lt; S.Length; i++)<br>        {<br>            if (S[i] == '&gt;') count++;<br>            open[i] = count;<br>        }<br><br>        count = 0;<br>        for (int i = S.Length-1; i &gt;= 0; i--)<br>        {<br>            if (S[i] == '&lt;') count++;<br>            ret = Math.Max(ret, Math.Min(open[i]*2, count*2));<br>        }<br><br>        return ret;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>