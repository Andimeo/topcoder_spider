            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class MagicalStringDiv2<br>{<br>  public int minimalMoves(string S)<br>  {<br>    int re = 0;<br>    for(int i = 0; i &lt; S.Length /2; i++)<br>    {<br>      if(S[i] == '&lt;')<br>      {<br>        re++;<br>      }<br>    }<br>    for (int i = S.Length / 2; i &lt; S.Length; i++)<br>    {<br>      if (S[i] == '&gt;')<br>      {<br>        re++;<br>      }<br>    }<br><br>    return re;<br>  }<br>  <br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>