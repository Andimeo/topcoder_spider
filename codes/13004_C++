            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>using namespace std;<br><br>class MagicalStringDiv2 {<br>public:<br>   int minimalMoves( string sahu ) {<br>    int dev = 0;<br>    for(int i = 0; i &lt; sahu.size()/2; ++i)<br>      if(sahu[i] == '&lt;') dev++;<br>    for(int i = (sahu.size()/2); i &lt; sahu.size(); ++i)<br>      if(sahu[i] == '&gt;') dev++;<br>    return dev;<br>   }<br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by moj 4.18 [modified TZTester]<br>// Powered by CodeProcessor<br></td>