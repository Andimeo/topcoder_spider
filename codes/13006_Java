            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class TaroString<br>{<br>  public String getAnswer(String S)<br>  {<br>    String trimmed = "";<br>    for (int i = 0; i &lt; S.length(); i++) {<br>      if (S.charAt(i) == 'C' || S.charAt(i) == 'A' || S.charAt(i) == 'T')<br>        trimmed += S.charAt(i);<br>    }<br>    if (trimmed.equals("CAT"))<br>      return "Possible";<br>    else<br>      return "Impossible";<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>