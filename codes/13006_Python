            import math,string,itertools,fractions,heapq,collections,re,array,bisect,random<br> <br>class TaroString:<br>    def getAnswer(self, S):<br>        if S.count("C") == 1 and S.count("A") == 1 and S.count("T") == 1:<br>          if S.index("C") &lt; S.index("A") &lt; S.index("T"):<br>            return "Possible"<br>        return "Impossible"<br> <br> <br>#Powered by KawigiEdit-pf 2.3.0!<br></td>