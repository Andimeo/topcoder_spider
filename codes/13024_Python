            import math, string, itertools, fractions, heapq, collections, re, array, bisect, random<br> <br> <br>class AlbertoTheAviator:<br>    def MaximumFlights(self, F, duration, refuel):<br>        n = len(duration)<br>        a = [[refuel[i], duration[i]] for i in range(n)]<br>        a.sort(reverse=True)<br> <br>        d = [0] * (F + 1)<br>        for i in range(n):<br>            for j in range(F + 1):<br>                if j &gt;= a[i][1]:<br>                    jj = j - a[i][1] + a[i][0]<br>                    d[jj] = max(d[jj], d[j] + 1)<br> <br>        return max(d)<br> <br> <br> <br>#Powered by KawigiEdit-pf 2.3.0!<br></td>