            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class TheMatrix<br>{<br>  public int MaxArea(String[] board)<br>  {<br>    final int h = board.length;<br>    final int w = board[0].length();<br>    int mat[][] = new int[h][w];<br>    int acc[][] = new int[h][w];<br>    for ( int i = 0; i &lt; h; ++i )<br>      for ( int j = 0; j &lt; w; ++j ) {<br>        mat[i][j] = ( board[i].charAt(j) - '0' );<br>        if ( (i+j)%2 == 0 ) mat[i][j] = 1 - mat[i][j];<br>        <br>        acc[i][j] = mat[i][j];<br>        if ( i &gt; 0 ) acc[i][j] += acc[i-1][j];<br>        if ( j &gt; 0 ) acc[i][j] += acc[i][j-1];<br>        if ( i &gt; 0 &amp;&amp; j &gt; 0 ) acc[i][j] -= acc[i-1][j-1];<br>      }<br>    <br>    <br>    int ans = 1;<br>    int si, sj, bi, bj, cnt, area;<br>    for ( si = 0; si &lt; h; ++si )<br>      for ( bi = si; bi &lt; h; ++bi )<br>        for ( sj = 0; sj &lt; w; ++sj )<br>          for ( bj = sj; bj &lt; w; ++bj )<br>          {<br>            area = (bi-si+1)*(bj-sj+1);<br>            cnt = acc[bi][bj];<br>            if ( sj &gt; 0 ) cnt -= acc[bi][sj-1];<br>            if ( si &gt; 0 ) cnt -= acc[si-1][bj];<br>            if ( si &gt; 0 &amp;&amp; sj &gt; 0 ) cnt += acc[si-1][sj-1];<br>            <br>            if ( cnt == 0 || cnt == area )<br>              ans = Math.max ( ans, area );<br>          }<br>    <br>    return ans;<br>  }<br>  <br>  void println ( Object o ) { System.out.println(o); }<br>  void print ( Object o ) { System.out.print(o); }<br>  void println ( ) { System.out.println(); }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>