            import math,string,itertools,fractions,heapq,collections,re,array,bisect,random<br> <br>class TheMatrix:<br>    def MaxArea(self, board):<br>        n = len(board)<br>        m = len(board[0])<br>        a = [[1] * m]<br>        for i in range(1, n):<br>            a.append([1] * m)<br>            for j in range(m):<br>                if board[i][j] != board[i - 1][j]:<br>                    a[i][j] = a[i - 1][j] + 1<br> <br>        res = 1<br>        for i in range(n):<br>            for j in range(m):<br>                mm = a[i][j]<br>                res = max(res, mm)<br>                k = j + 1<br>                while k &lt; m and board[i][k] != board[i][k - 1]:<br>                    mm = min(mm, a[i][k])<br>                    res = max(res, mm * (k - j + 1))<br>                    k += 1<br> <br> <br>        return res<br> <br> <br> <br> <br> <br> <br> <br> <br> <br>#Powered by KawigiEdit-pf 2.3.0!<br></td>