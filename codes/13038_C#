            using System.Collections.Generic;<br><br>class InterestingNumber<br>    {<br>        public string isInteresting(string x)<br>        {<br>            List&lt;int&gt; digitIndexes = new List&lt;int&gt;();<br>            for (int i = 0; i &lt;= 9; ++i)<br>            {<br>                digitIndexes.Clear();<br><br>                for (int j = 0; j &lt; x.Length; ++j)<br>                {<br>                    int number = x[j] - '0';<br>                    if ( number == i )<br>                        digitIndexes.Add(j);<br>                }<br><br>                if (digitIndexes.Count == 0)<br>                    continue;<br>                else if (digitIndexes.Count == 2)<br>                {<br>                    int diffrence = digitIndexes[1] - digitIndexes[0] - 1;<br>                    if (diffrence != i)<br>                        return "Not interesting";<br>                }<br>                else<br>                    return "Not interesting";<br><br>            }<br>            return "Interesting";<br>        }<br>    }<br></td>