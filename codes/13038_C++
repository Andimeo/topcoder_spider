or &lt;int &gt; idx[10];<br>    FOR(i,0,len)<br>    {<br>      int num = x[i]-'0';<br>      idx[num].push_back(i);<br>    }<br>    good = true;<br>    FOR(i,0,10)<br>    {<br>      if(idx[i].size()==0)<br>        continue;<br>      good &amp;= (abs(idx[i][1]-idx[i][0]-1) == i);<br>    }<br>    if(good)<br>      return "Interesting";<br>    return "Not interesting";<br>  } <br>  <br> <br>}; <br> <br> <br> <br> <br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>