<td class="problemText" colspan="8" valign="middle" align="left">
            using System.Collections.Generic;<br>public class LCMSetEasy<br>{<br>  public string include(int[] S, int x)<br>  {<br>    List&lt;int&gt; divs = new List&lt;int&gt;();<br>    foreach (int s in S)<br>    {<br>      if (x%s ==0)<br>        divs.Add(s);<br>    }<br>    if (divs.Count==0) return "Impossible";<br>    <br>    List&lt;int&gt; els = new List&lt;int&gt;();<br>    els.Add(divs[0]);<br>    foreach (int i in divs)<br>    {  int z = i;<br>      foreach (var e in els)<br>      {<br>        z=z/GCD(z,e);<br>      }<br>      els.Add(z);<br>    }<br>    <br>    for (int i=0;i&lt;els.Count;i++)<br>    {<br>      int p = GCD(x,els[i]);<br>      x = x/p;<br>      if (x==1) return "Possible";<br>    }<br>    return "Impossible";<br>  }<br>  public int GCD(int x, int y)<br>  {<br>    while (y&gt;0)<br>    {<br>      int t = y;<br>      y = x%y;<br>      x=t;<br>    }<br>    return x;<br>  } <br>}<br></td>