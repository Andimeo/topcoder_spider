            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class LCMSetEasy<br>{<br>  public String include(int[] S, int x)<br>  {<br>    int rem,done;<br>    rem = x;<br>    done = 1;<br>    for(int i=0;i&lt;S.length;i++) {<br>      if(x%S[i]==0) {<br>        rem = rem/(S[i]/gcd(S[i],done));<br>        done = x/rem;<br>      }<br>    }<br>    if(rem==1 &amp;&amp; done==x) <br>      return "Possible";<br>    return "Impossible";<br>  }<br>  public int gcd(int number1, int number2) {<br>        if(number2 == 0){<br>            return number1;<br>        }<br>        return gcd(number2, number1%number2);<br>    }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>