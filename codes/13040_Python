<td class="problemText" colspan="8" valign="middle" align="left">
            class LCMSetEasy:<br>    def gcd(self,a, b):<br>        while b:      <br>            a,b = b, a % b<br>        return a<br><br>    def lcm(self,a, b):<br>        #print "%d %d" % (a, b)<br>        return a * b // self.gcd(a, b)<br><br>    def lcm_all(self, l):<br>        return reduce(self.lcm, l)  <br><br>    def include(self, S, x):<br>        l = []<br>        for s in S:<br>            if x % s == 0:<br>                l.append(s)<br>        #print l<br>        if len(l) == 0:<br>            return 'Impossible'<br>        lcm = self.lcm_all(l)<br>        if lcm == x:<br>            return 'Possible'<br>        return 'Impossible'<br></td>