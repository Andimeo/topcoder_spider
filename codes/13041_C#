            using System;<br> <br>public class EmoticonsDiv2 {<br>  public int printSmiles(int smiles)<br>  {<br>      return (int) GetNum(smiles, 0, 1, new long?[1001,1001]);<br>  }<br> <br>    private long GetNum(int smiles, int clipboard, int total, long?[,] memo)<br>    {<br>        if (total &gt; smiles) return Int32.MaxValue;<br>        if (total == smiles) return 0;<br> <br>        // memo check<br>        if (memo[clipboard, total].HasValue)<br>        {<br>            return memo[clipboard, total].Value;<br>        }<br> <br>        // just paste<br>        long paste;<br>        if (clipboard &gt; 0)<br>        {<br>            paste = 1 + GetNum(smiles, clipboard, total + clipboard, memo);<br>        }<br>        else<br>        {<br>            paste = Int32.MaxValue;<br>        }<br> <br>        // copy &amp;&amp; paste<br>        long copyPaste = 2 + GetNum(smiles, total, total*2, memo);<br> <br>        long res = Math.Min(paste, copyPaste);<br> <br>        memo[clipboard, total] = res;<br> <br>        return res;<br>    }<br> <br>}<br>// Powered by Greed 2.0-beta<br></td>