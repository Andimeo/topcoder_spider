            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class PowersOfTwo {<br>public:<br>  long long count(vector &lt;long long&gt; powers);<br>};<br> <br>long long PowersOfTwo::count(vector &lt;long long&gt; powers) {<br>  long long a[51];<br>  for (int i=0;i&lt;=50;i++)<br>    a[i]=0;<br>  long long p[52];<br>  p[0]=1;<br>  for (int i=1;i&lt;=50;i++)<br>    p[i]=p[i-1]*2;<br>  vector &lt;long long&gt;::iterator cur;<br>  for (cur=powers.begin();cur&lt;powers.end();cur++)<br>    for (int j=0;j&lt;=50;j++)<br>      if (*cur==p[j]) a[j]++;<br>  long long S=0;<br>  for (int i=0;i&lt;=50;i++)<br>  {<br>    S+=p[i]*a[i];<br>    for (int j=i+1;j&lt;=50;j++)<br>      if (S&gt;=p[j])<br>      {<br>        a[i]+=p[j-i]*a[j];<br>        S+=p[j]*a[j];<br>        a[j]=0;<br>      }<br>  }<br>  S=1;<br>  for (int i=0;i&lt;=50;i++)<br>    S*=(a[i]+1);<br>  return S;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>