<td class="problemText" colspan="8" valign="middle" align="left">
            import java.util.ArrayList;<br>import java.util.Collections;<br>import java.util.HashSet;<br> <br>public class PowersOfTwo {<br>    public long count(long[] powers) {<br>        ArrayList&lt;Long&gt; sPoswers = new ArrayList&lt;Long&gt;(powers.length);<br>        for (long p : powers) sPoswers.add(p);<br>        Collections.sort(sPoswers);<br>        long max = 0;<br>        HashSet&lt;Long&gt; current = new HashSet&lt;Long&gt;();<br>        long total = 1;<br>        current.add(0l);<br>        Long last = null;<br>        for (Long p : sPoswers) {<br>            if (p != last &amp;&amp; last != null &amp;&amp; p &gt; max) {<br>                total *= current.size();<br>                current = new HashSet&lt;Long&gt;();<br>                current.add(0l);<br>            }<br> <br>            HashSet&lt;Long&gt; newVals = new HashSet&lt;Long&gt;();<br>            for (Long val : current)<br>                newVals.add(val + p);<br>            current.addAll(newVals);<br> <br>            max += p;<br>            last = p;<br>        }<br>        total *= (current.size() != 0 ? current.size() : 1);<br>        return total;<br>    }<br> <br>}<br></td>