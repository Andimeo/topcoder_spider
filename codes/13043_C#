            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class LeftAndRightHandedDiv2<br>{<br>  public int count(string S)<br>  {<br>    int sum = 0;<br><br>    for(int i = 0; i&lt;S.Length-1; i++)<br>    {<br>      if(S[i] == 'R' &amp;&amp; S[i+1] == 'L')<br>      {<br>        sum++;<br>      }<br>    }<br><br>    return sum;<br>  }<br>  <br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>