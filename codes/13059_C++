                     {<br>                              kk=k;ll=l;oo=o;<br>                              if (kk==31) kk=32;<br>                              if (ll==31) ll=32;<br>                              if (oo==31) oo=32;<br>                              d[1-now][kk][ll][oo]+=d[now][k][l][o];<br>                            }<br>                          }<br>                          else<br>                          {<br>                            if ((k==j)||(l==j)||(o==j)||(k==j+35)||(l==j+35)||(o==j+35))<br>                            {<br>                              kk=k;ll=l;oo=o;<br>                              if (kk==31) kk=32;<br>                              if (ll==31) ll=32;<br>                              if (oo==31) oo=32;<br>                              if (kk==j+35) kk=j;<br>                              if (ll==j+35) ll=j;<br>                              if (oo==j+35) oo=j;<br>                              d[1-now][kk][ll][oo]+=d[now][k][l][o];<br>                            }<br>                            else<br>                            {<br>                              if ((j!=0)&amp;&amp;((k==j-1)||(l==j-1)||(o==j-1)||(k==31)||(l==31)||(o==31)))<br>                              {<br>                                kk=k;ll=l;oo=o;<br>                                if (kk==j-1) kk=31;<br>                                if (ll==j-1) ll=31;<br>                                if (oo==j-1) oo=31;<br>                                d[1-now][kk][ll][oo]+=d[now][k][l][o];<br>                              }<br>                              kk=k;ll=l;oo=o;<br>                              b1=1;<br>                              if (kk==30)<br>                              {<br>                                kk=j+35;<br>                              }<br>                              else if (ll==30)<br>                              {<br>                                ll=j+35;<br>                              }<br>                              else if (oo==30)<br>                              {<br>                                oo=j+35;<br>                              }<br>                              else b1=0;<br>                              if (b1==1)<br>                              {<br>                                if (kk==31) kk=32;<br>                                if (ll==31) ll=32;<br>                                if (oo==31) oo=32;<br>                                d[1-now][kk][ll][oo]+=d[now][k][l][o];<br>                              }<br>                              kk=k;ll=l;oo=o;<br>                              if (kk==31) kk=32;<br>                              if (ll==31) ll=32;<br>                              if (oo==31) oo=32;<br>                              d[1-now][kk][ll][oo]+=d[now][k][l][o];<br>                              <br>                            }<br>                          }<br>                          <br>                        }<br>                  now=1-now;<br>                }<br>              return d[now][32][32][32]+d[now][31][32][32]+d[now][32][31][32]+d[now][32][32][31];<br>            } <br>        <br> <br>        }; <br> <br> <br> <br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>