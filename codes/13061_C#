            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class ColorfulCoins<br>{<br>  public int minQueries(long[] values)<br>  {<br>        Dictionary&lt;long, int&gt; ratios = new Dictionary&lt;long, int&gt;();<br>        for (int i = 1; i &lt; values.Length; i++)<br>        {<br>            long ratio = values[i]/values[i-1];<br>            if (!ratios.ContainsKey(ratio)) ratios.Add(ratio, 0);<br>            ratios[ratio]++;<br>        }<br> <br>        long totcoins = 0;<br>        List&lt;long&gt; sortrats = new List&lt;long&gt;(ratios.Keys);<br>        sortrats.Sort();<br>        int ret = 1;<br> <br>        foreach (long ratio in sortrats)<br>        {<br>            totcoins += ratios[ratio];<br>            long canFind = ratio;<br>            int queries = 1;<br>            while (canFind &lt;= totcoins)<br>            {<br>                queries++;<br>                canFind *= ratio;<br>            }<br>            ret = Math.Max(ret, queries);<br>        }<br> <br>        return ret;<br> <br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>