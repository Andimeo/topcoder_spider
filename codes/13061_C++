            #include &lt;math.h&gt;<br>#include &lt;stdio.h&gt;<br>#include &lt;string.h&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;queue&gt;<br>#include &lt;map&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;cmath&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>using namespace std;<br> <br>class ColorfulCoins<br>{<br>  public:<br>  int minQueries(vector&lt;long long&gt; values_)<br>  {<br>    vector&lt;int&gt; V;<br>    for (int i=0; i&lt;values_.size()-1; i++)<br>      V.push_back(min(100LL, values_[i+1] / values_[i]));<br>    int n = V.size();<br>    if (!n) return 1;<br>    <br>    sort(V.begin(), V.end());<br>    for (int i=0; i&lt;n; i++) printf("%d ", V[i]);<br>    <br>    int cnt = 1;<br>    while (1) {<br>      int ok = 1;<br>      for (int i=0; i&lt;n; i++) {<br>        long long t = 1;<br>        for (int j=0; j&lt;cnt; j++) t *= V[i];<br>        if (t &lt; i+2) { ok = 0; break; }<br>      }<br>      <br>      if (ok) return cnt;<br>      cnt ++;<br>    }<br>  };<br>};<br></td>