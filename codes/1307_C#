            using System;<br>using System.Collections;<br><br>public class RoadWork <br>{<br>  public class Interval<br>  {<br>    public int start;<br>    public int end;<br><br>    public Interval (int start, int end)<br>    {<br>      this.start = start;<br>      this.end = end;<br>    }<br><br>    public static Interval operator * (Interval i1, Interval i2)<br>    {<br>      return new Interval(Math.Max(i1.start, i2.start), Math.Min(i1.end, i2.end));<br>    }<br><br>    public static Interval operator + (Interval i1, Interval i2)<br>    {<br>      return new Interval(Math.Min(i1.start, i2.start), Math.Max(i1.end, i2.end));<br>    }<br><br>    public int Length<br>    {<br>      get<br>      {<br>        return Math.Max(0, end - start);<br>      }<br>    }<br><br>    public override string ToString()<br>    {<br>      return string.Format("({0}, {1})", start, end);<br>    }<br>  }<br><br>  public static int fraudFeet (int[] start, int[] end) <br>  {<br>    ArrayList a = new ArrayList();<br><br>    Interval[] v = new Interval[start.Length];<br><br>    for (int i = 0; i &lt; start.Length; i++)<br>      v[i] = new Interval(start[i], end[i]);<br><br>    for (int i = 0; i &lt; v.Length; i++)<br>      for (int j = i + 1; j &lt; v.Length; j++)<br>        a.Add(v[i] * v[j]);<br><br>    int s = 0;<br><br>    bool flag = true;<br><br>    while (flag)<br>    {<br>      //Code.print(a);<br><br>      flag = false;<br><br>      for (int i = 0; i &lt; a.Count; i++)<br>      {<br>        Interval ai = (Interval)a[i];<br><br>        for (int j = i + 1; j &lt; a.Count; j++)<br>        {<br>          Interval aj = (Interval)a[j];<br><br>          if ((ai * aj).Length &gt; 0)<br>          {<br>            a[i] = ai = ai + aj;<br>            a[j] = new Interval(0, 0);<br><br>            //Console.WriteLine("{0} {1} {2}", ai, aj, a[i]);<br><br>            flag = true;<br>          }<br>        }<br>      }<br>    }<br><br>    foreach (Interval ai in a)<br>      s += ai.Length;<br><br>    return s;<br>  }<br><br>  public static void Main ()<br>  {<br>    Console.WriteLine(fraudFeet(new int[] {50,50,50}, new int[] {58,58,60}));<br>    Console.WriteLine(fraudFeet(new int[] {171234,12,20,30}, new int[] {171236,20,30,40}));<br>    Console.WriteLine(fraudFeet(new int[] {12,32,92}, new int[] {991,161,1093}));<br>    Console.WriteLine(fraudFeet(new int[] {0,0,1000000000,17}, new int[] {1000000000,2000000000,2000000000,45874}));<br>    //Console.WriteLine(fraudFeet(new int[] , new int[] ));<br>    //Console.WriteLine(fraudFeet(new int[] , new int[] ));<br>    //Console.WriteLine(fraudFeet(new int[] , new int[] ));<br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>