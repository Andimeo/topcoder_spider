            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;iostream&gt;<br>#include &lt;strstream&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>using namespace std;<br> <br>class RoadWork  {<br>public:<br> int fraudFeet(vector &lt;int&gt; a, vector &lt;int&gt; b);<br>};<br> <br>int rst;<br>vector&lt;int&gt; bd;<br> <br>int findit(vector&lt;int&gt;&amp; A, int ni)<br>{<br>  int i;<br>  int len=A.size();<br>  for(i=0;i&lt;len;i++)<br>    if(A[i]==ni) return(i);<br>  A.push_back(ni);<br>  return(-1);<br>}<br> <br>void sortthisguy(vector&lt;int&gt;&amp; A)<br>{<br>  int len=A.size();<br>  int i,j,tmp;<br>  for(i=0;i&lt;len;i++)<br>    for(j=i+1;j&lt;len;j++)<br>      if(A[i]&gt;A[j])<br>      {<br>        tmp=A[i];<br>        A[i]=A[j];<br>        A[j]=tmp;<br>      }<br>}<br> <br>int RoadWork ::fraudFeet(vector &lt;int&gt; a, vector &lt;int&gt; b) <br>{<br>  bd.clear();<br>  int i,j,k;<br>  for(i=0;i&lt;a.size();i++)<br>  {<br>    findit(bd, a[i]);<br>    findit(bd, b[i]);<br>  }<br>  sortthisguy(bd);<br>  int n=bd.size();<br>  rst=0;<br>  for(i=0;i&lt;n-1;i++)<br>  {<br>    k=0;<br>    for(j=0;j&lt;a.size();j++)<br>      if(a[j]&lt;=bd[i] &amp;&amp; b[j]&gt;=bd[i+1]) k++;<br>    if(k&gt;=2) rst+=(bd[i+1]-bd[i]);<br>  }<br>  return(rst);<br>}<br> <br> <br>// Powered by PopsEdit<br></td>