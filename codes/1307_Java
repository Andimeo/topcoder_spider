            import java.util.*;<br>public class RoadWork {<br>  public int fraudFeet(int[] start, int[] end) {<br>    Vector V = new Vector();<br>    V.add(new Integer(0));<br>    V.add(new Integer(2000000000));<br>    for (int i = 0; i &lt; start.length; i++) {<br>      if (!V.contains(new Integer(start[i]))) V.add(new Integer(start[i]));<br>      if (!V.contains(new Integer(end[i]))) V.add(new Integer(end[i]));<br>    }<br>    <br>    int [] x = new int[V.size()];<br>    int n = x.length;<br>    for (int i = 0; i &lt; n; i++) x[i] = ((Integer)(V.elementAt(i))).intValue();<br>    Arrays.sort(x);<br>    <br>    //for(int i = 0; i &lt; n; i++) System.out.println(x[i]);<br>    <br>    int ret = 0;<br>    for (int i = 0; i &lt; n - 1; i++) {<br>      int a = x[i];<br>      int b = x[i+1];<br>      int cnt = 0;<br>      for (int j = 0; j &lt; start.length; j++) {<br>        if ((a &gt;= start[j]) &amp;&amp; (b &lt;= end[j])) {<br>          cnt++;<br>        }<br>      }<br>      if (cnt &gt; 1) {<br>        ret += x[i+1] - x[i];<br>      }<br>    }<br>    <br>    return ret;<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>