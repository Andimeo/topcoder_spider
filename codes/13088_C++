b) { return b?gcd(b,a%b):a;}<br><br>set&lt;int&gt; c,ans;<br>class AmebaDiv1 {<br>  public:<br>    int count(vector &lt;int&gt; X)  {<br>      int n=SZ(X);<br>      c.clear();<br>      ans.clear();<br>      rep(i,0,n) {<br>        int a=X[i];<br>        rep(j,0,n) if (a==X[j]) a*=2;<br>        c.insert(a);<br>      }<br>      rep(i,0,n) if (!c.count(X[i])) ans.insert(X[i]);<br>      return SZ(ans);<br>    }<br>  <br><br>};<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>