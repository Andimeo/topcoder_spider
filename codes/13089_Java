            // Paste me into the FileEdit configuration dialog<br><br>public class AmebaDiv2 {<br>   public int simulate(int[] X, int A) {<br>    int s = A;<br>    for(int x : X)<br>      if(x == s)<br>        s += x;<br>    return s;<br>   }<br><br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by moj 4.16 [modified TZTester]<br>// Powered by CodeProcessor<br></td>