            #include &lt;string&gt;<br>#include &lt;bits/stdc++.h&gt;<br>using namespace std;<br><br>class ColorfulCoinsEasy {<br>public:<br>   string isPossible( vector &lt;int&gt; val ) {<br>  int i;<br>  int n = 0;<br>  n = val.size();<br>  int loki[n + 5];<br>  int b[30000] = {0};<br>  memset(b, 0, sizeof(b));  <br>  for(i = 0;i &lt; n - 1;i++) {<br>    loki[i] = val[i + 1] / val[i];<br>    if(val[i + 1] % val[i] == 0) {<br>      loki[i]--;<br>    }<br>  }<br>  sort(loki,loki +  n - 1);<br>  for(i = n - 2;i &gt;= 0;i--) {<br>    int flag = 0;<br>    for(int j = loki[i];j &gt;= 1;j--) {<br>    //  cout &lt;&lt; j &lt;&lt;"  ";<br>      if(b[j] == 0) {<br>        b[j] = 1;<br>        flag = 1;<br>        break;<br>      }<br>    }<br>    //cout &lt;&lt;"\n";<br>    if(flag == 0) {<br>      return "Impossible";<br>    }<br>  }<br>  return "Possible";<br>   }<br>};<br></td>