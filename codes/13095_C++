            #include &lt;algorithm&gt;<br>#include &lt;bitset&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br>#include &lt;ctime&gt;<br>#include &lt;deque&gt;<br>#include &lt;functional&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;iostream&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;numeric&gt;<br>#include &lt;queue&gt;<br>#include &lt;set&gt;<br>#include &lt;sstream&gt;<br>#include &lt;stack&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;utility&gt;<br> <br>using namespace std;<br> <br>#define all(c) c.begin(),c.end()<br>#define rev(c) c.rbegin(),c.rend()<br>#define inc(i,a,b) for(int i=a;i&lt;b;++i)<br>#define dec(i,a,b) for(int i=b-1;i&gt;=a;--i)<br>#define bit(i) (ll(1)&lt;&lt;(i))<br> <br>typedef long long ll;<br>typedef long double ld;<br>typedef vector&lt;int&gt; vi;<br>typedef vector&lt;vi&gt; mi;<br> <br>const string CANT = "a", TRY = "b";<br> <br>string m[50][50];<br>string s, a, b;<br>int n, na, nb;<br> <br>string f(int i, int j) {<br>  if (j == n)<br>    return (i == na) ? "" : CANT;<br> <br>  if (m[i][j] == TRY) {<br>    string t = CANT;<br>    if (i &lt; na &amp;&amp; (s[j] == '?' || s[j] == a[i])) {<br>      string u = f(i+1, j+1);<br>      if (u != CANT)<br>  t = min(a[i] + u, t);<br>    }<br>    if (j-i &lt; nb &amp;&amp; (s[j] == '?' || s[j] == b[j-i])) {<br>      string u = f(i, j+1);<br>      if (u != CANT)<br>  t = min(b[j-i] + u, t);<br>    }<br>    m[i][j] = t;<br>  }<br> <br>  return m[i][j];<br>}<br> <br>class MergeStrings { <br>public: <br>    string getmin(string S, string A, string B) { <br>      s = S, a = A, b = B;<br>      n = s.size(), na = a.size(), nb = b.size();<br>      inc(i, 0, 50)<br>  inc(j, 0, 50)<br>  m[i][j] = TRY;<br>      <br>      string v = f(0,0);<br>      if (v == CANT)<br>  v = "";<br>      return v;<br>    }<br>    <br> <br>}; <br> <br> <br> <br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>