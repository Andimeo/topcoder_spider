            using System;<br>using System.CodeDom.Compiler;<br>using System.Collections.Generic;<br>using System.Diagnostics;<br>using System.Xml;<br>using System.Xml.Linq;<br><br>  class WakingUpEasy<br>    {<br>        public int countAlarms(int[] volume, int S)<br>        {<br>            LinkedList&lt;int&gt; volumes = new LinkedList&lt;int&gt;(volume);<br>            int alarmCount = 0;<br><br>            while (S &gt; 0)<br>            {<br>                int currentAlarm = volumes.First.Value;<br>                volumes.RemoveFirst();<br>                S -= currentAlarm;<br>                alarmCount++;<br><br>                volumes.AddLast(currentAlarm);<br>            }<br><br>            return alarmCount;<br>        }<br>    }<br></td>