            #include &lt;sstream&gt;<br>#include &lt;queue&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cmath&gt;<br>#include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br><br>using namespace std;<br><br>class WakingUpEasy<br>{<br>public:<br>  int countAlarms(vector &lt;int&gt; v, int s)<br>  {<br>      int len=v.size(),count=0,i=0;<br><br>      while(s&gt;0) {<br>          s-=v[i++];<br>          count++;<br>          if(i==len) {<br>              i=0;<br>          }<br>      }<br><br>      return count;<br><br>  }<br>};<br></td>