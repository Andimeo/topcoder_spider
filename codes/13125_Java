            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class WakingUpEasy<br>{<br>  public int countAlarms(int[] volume, int S)<br>  {<br>    int Count =0;<br>    while(true)<br>    {<br>      if(S&lt;=0)break;<br>      for(int i = 0 ; i &lt; volume.length;++i)<br>      {<br>        Count++;<br>        S -=volume[i];<br>        if(S&lt;=0)break;<br>        <br>        //int temp = volume[i];<br>      }<br>      <br>    }<br>    <br>    return Count;<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>