            import java.util.*;<br> <br>public class MessageMess {<br>  public Map table = new HashMap();<br> <br>  public String restore(String[] dictionary, String message) {<br>    if (table.containsKey(message))<br>      return (String) table.get(message);<br> <br>    int found = 0;<br>    String s = "";<br>    for (int i=0;i&lt;dictionary.length;i++) {<br>      if (message.equals(dictionary[i])) { found++; s = message; }<br>      else if (message.startsWith(dictionary[i])) {<br>        String result = restore(dictionary, message.substring(dictionary[i].length()));<br>        if (result.equals("AMBIGUOUS!")) {<br>          table.put(message, "AMBIGUOUS!");<br>          return result;<br>        } else if (result.equals("IMPOSSIBLE!")) {<br>          // Do nothing<br>        } else {<br>          found++;<br>          s = dictionary[i] + " " + result;<br>        }<br>      }<br>    }<br>    if (found == 0) {<br>      s = "IMPOSSIBLE!";<br>    } else if (found != 1) {<br>      s = "AMBIGUOUS!";<br>    }<br>    table.put(message, s);<br>    return s;<br>  }<br>}<br></td>