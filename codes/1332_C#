<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br> <br>public class Pareto<br>{<br>   string[] feelings = new string[] { "awful", "bad", "fair", "fairly-happy", "happy", "ecstatic" };<br> <br>   public int optima(string[] policy)<br>   {<br>      int result = 0;<br>      int numPolicies = policy.Length;<br>      int numPeople = policy[0].Split(' ').Length;<br>      for (int i = 0; i &lt; numPolicies; ++i)<br>      {<br>         bool so = false;<br>         for (int j = 0; j &lt; numPolicies; ++j)<br>         {<br>            if (i == j) continue;<br>            bool better = false;<br>            bool worse = false;<br>            for (int k = 0; k &lt; numPeople; ++k)<br>               switch (compare(policy[i].Split(' ')[k], policy[j].Split(' ')[k]))<br>               {<br>                  case -1: better = true;<br>                     break;<br>                  case 1: worse = true;<br>                     break;<br>               }<br>            so = better &amp;&amp; !worse;<br>            if (so) break;<br>         }<br>         if (!so) ++result;<br>      }<br>      return result;<br>   }<br> <br>   private int compare(string a, string b)<br>   {<br>      if (Array.IndexOf(feelings, a) - Array.IndexOf(feelings, b) &gt; 0) return 1;<br>      if (Array.IndexOf(feelings, a) == Array.IndexOf(feelings, b)) return 0;<br>      return -1;<br>   }<br>}<br></td>