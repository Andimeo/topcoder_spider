            using System;<br>using System.Collections;<br><br>public class NameSort<br>{<br>  static string[] o;<br>  static string[] c;<br><br>  public static int Compare (int xi, int yi)<br>  {<br>    string[] sxs = c[xi].Split(' ');<br>    string[] sys = c[yi].Split(' ');<br><br>    string sx = sxs[sxs.Length - 1].ToUpper();<br>    string sy = sys[sys.Length - 1].ToUpper();<br><br>    return sx.CompareTo(sy);<br><br>    /*<br>    int t = sx.CompareTo(sy);<br><br>    if (t != 0)<br>      return t;<br>    else<br>      return xi - yi;<br>      */<br>  }<br><br>  public static string[] newList (string[] l) <br>  {<br>    //o = new string[l.Length];<br>    c = l;<br><br>    //c.CopyTo(o, 0);<br><br>    for (int i = 0; i &lt; c.Length; i++)<br>      for (int j = i + 1; j &lt; c.Length; j++)<br>        if (Compare(i, j) &gt;= 0)<br>        {<br>          string t = c[i];<br>          c[i] = c[j];<br>          c[j] = t;<br>        }<br><br>    return c;<br>  }<br><br>  public static void Main ()<br>  {<br>    /*<br>    Code.print(newList(new string[] {"Tom Jones","ADAMS","BOB ADAMS", "Tom Jones","STEVE jONeS"}));<br>    Code.print(newList(new string[] {"C A R Hoare","Kenny G", "A DeForest Hoar","Kenny Gee"}));<br>    Code.print(newList(new string[] {"Trudy","Trudy","TRUDY"}));<br>    Code.print(newList(new string[] {"tIm JoNeS", "Tim Jones", "tom JoNes", "tim joness", "tiM joneS"}));<br>    Code.print(newList(new string[] {"Alan","aLan","alAn","alaN","ALan","AlAn","AlaN","aLAn","aLaN","alAN"}));<br>    Code.print(newList(new string[] {"Al Thompson","Bob Johnson", "John Thompson", "John D Thompson","Bob D Johnson"}));<br>    //Code.print(newList(new string[] ));<br>    */<br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>