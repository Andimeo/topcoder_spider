            import java.util.*;<br>public class NameSort {<br>  public String[] newList(String[] list) {<br>    for(int i = 0;i&lt;list.length;i++) {<br>      for(int j = i+1;j&lt;list.length;j++) {<br>        String last1 = list[i].substring(list[i].lastIndexOf(' ')+1);<br>        String last2 = list[j].substring(list[j].lastIndexOf(' ')+1);<br>        if(last1.toUpperCase().compareTo(last2.toUpperCase()) &gt;= 0) {<br>          String temp = list[i];<br>          list[i] = list[j];<br>          list[j] = temp;<br>        }<br>      }<br>    }<br>    return list;<br>          <br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>