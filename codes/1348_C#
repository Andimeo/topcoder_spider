            using System;<br>using System.Collections;<br>public class BenfordsLaw {<br>  public BenfordsLaw()<br>  {<br><br>  }<br>  public int questionableDigit(int[] transactions, int threshold) {<br>    double[] freq = new double[10];<br>    int i;<br>    double expected;<br><br>    for(i = 0; i &lt; transactions.Length; i++)<br>    {<br>      freq[int.Parse(transactions[i].ToString().Substring(0,1))] += 1;<br>    }<br><br>    for(i = 1; i &lt;= 9; i++)<br>    {<br>      Console.WriteLine("FREQ:" + freq[i]);<br>      expected = Math.Log10(1 + (1/(double)i));<br>      expected *= transactions.Length;<br>      Console.WriteLine(expected);<br>      if(freq[i] &gt; (expected * (double)threshold))<br>        return i;<br>      if(freq[i] &lt; ( expected / (double)threshold))<br>        return i;<br>    }<br><br>    return -1;<br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>