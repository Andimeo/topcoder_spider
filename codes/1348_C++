            #include &lt;stdio.h&gt;<br>#include &lt;stdlib.h&gt;<br>#include &lt;iostream&gt;<br>#include &lt;map&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;deque&gt;<br>#include &lt;math.h&gt;<br>using namespace std;<br> <br>typedef int I;<br>typedef vector&lt;int&gt; VI;<br>typedef string S;<br>typedef vector&lt;string&gt; VS;<br>typedef bool B;<br>typedef vector&lt;bool&gt; VB;<br>typedef double D;<br>typedef vector&lt;double&gt; VD;<br> <br>struct BenfordsLaw<br>{<br>I questionableDigit(VI tr, I th)<br>{<br>  VI x;<br>  for(I i=0; i&lt;10;i++) x.push_back(0);<br>  <br>  for (I i =0; i&lt;tr.size();i++)<br>  {<br>    I y=tr[i];<br>    while (y&gt;=10) y/=10;<br>    x[y-1]++;<br>    cout &lt;&lt; "added: " &lt;&lt; y &lt;&lt; endl;<br>  }<br>  for (I i=0; i&lt;9; i++)<br>  {<br>    D j = i+1;<br>    cout &lt;&lt; j &lt;&lt; ": ";<br>    D j2 = 1+1/j;<br>    D k = log10(j2)*tr.size();<br>    cout &lt;&lt; k &lt;&lt; " " &lt;&lt; x[i] &lt;&lt; endl;<br>    if (x[i] &gt;= th * k)<br>      return i+1;<br>    if (x[i] &lt;= (double)k / (double)th)<br>      return i+1;<br>  }<br>  return -1;<br>}<br>};<br></td>