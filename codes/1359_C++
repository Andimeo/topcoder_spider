            #line 2 "Surveyor.cpp"<br><br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;ctype.h&gt;<br>#include &lt;math.h&gt;<br>#include &lt;iostream&gt;<br>#include &lt;set&gt;<br><br>using namespace std;<br>typedef long long ll;<br>typedef long double ld;<br><br>#define LS &lt;<br>#define FOR(k,start,end) for(typeof(start) k=(start); k LS (end); ++k)<br><br>class  Surveyor {<br>  public:<br>  int area(string direction, vector &lt;int&gt; length) {<br>    int y = 0;<br>    int ar = 0;<br>    for(int k = 0; k&lt;direction.size(); k++) {<br>      int l = length[k];<br>      char d = direction[k];<br>      if(d == 'N') y += l;<br>      if(d == 'S') y -= l;<br>      if(d == 'W') ar += l*y;<br>      if(d == 'E') ar -= l*y;<br>      }<br>    if(ar &lt; 0) ar = -ar;<br>    return ar;<br>    }<br>  };<br><br><br>// Powered by FileEdit<br></td>