            using System;<br> <br>public class WordParts<br>{<br>  string org;<br>  string comm;<br>  int [,]was;<br>  <br>  int divide(int n,int pos)<br>  {<br>    if (was[n,pos]!= -2) return was[n,pos];<br>    string com = comm.Substring(0,pos);<br>    if (com.Length==0) return n;<br>    int min=999;<br> <br>    for (int i=0;i&lt;pos;i++)<br>    {<br>      string ss = com.Substring(i,pos-i);<br>      int inx = org.IndexOf(ss);<br>      int inxL = org.LastIndexOf(ss);<br>      if (org.Length-ss.Length&gt;=0)<br>      if ((inx==0)||(inxL==org.Length-ss.Length))<br>      {<br>        min = Math.Min(min,divide(n+1,i));<br>      }<br>    }<br>    was[n,pos] = min;<br>    return min;<br>  }<br> <br>  public int partCount(string original, string compound)<br>  {<br>    was = new int[60,60];<br>    for (int y=0;y&lt;60;y++)<br>    for (int x=0;x&lt;60;x++) was[x,y] = -2;<br> <br>    org = original;<br>    comm = compound;<br>    int res = divide(0,comm.Length);<br>    if (res==999) return -1;<br>           else return res;<br>  }<br>}<br></td>