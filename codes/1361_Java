            public class WordParts<br>{<br>  public static int partCount(String original, String compound)<br>  {<br>    int[] min=new int[51];<br>    int a,b,c,d,n=compound.length(), n2=original.length();<br>    min[0]=0;<br>    for(a=1;a&lt;=n;a++)<br>    {<br>      c=10000;<br>      for(b=0;b&lt;a;b++)<br>        if(min[b]&gt;-1 &amp;&amp; min[b]+1&lt;c)<br>        {<br>          d=original.indexOf(compound.substring(b,a));<br>          if(d==0)<br>          {<br>            c=min[b]+1;<br>          }<br>          else<br>          {<br>            d=original.indexOf(compound.substring(b,a),n2-(a-b));<br>            if(d&gt;=0 &amp;&amp; d==n2-(a-b))  c=min[b]+1;<br>          }<br>        }<br>      <br>    <br>      if (c==10000) min[a]=-1; else min[a]=c;<br>    }<br>    return min[n];<br>  }<br> <br>}<br></td>