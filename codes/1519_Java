            /*/*///*/*/ Powered by radedit!            /*/*/*/*/*/ ;<br>/*/*//*/*/  import java . util .           /*/*/*/*/*/ ;<br>/*/*//*/*/  import java . lang . reflect . /*/*/*/*/*/ ;<br>public class BinaryCardinality {<br>   boolean ddd = false ;<br>  int bc(int v) {<br>    int r = 0 ;<br>    while (v != 0) {<br>      r++ ;<br>      v &amp;= v - 1 ;<br>    }<br>    return r ;<br>  }<br>  int cmp(int a, int b) {<br>    int bca = bc(a) ;<br>    int bcb = bc(b) ;<br>    if (bca != bcb) {<br>      return bca - bcb ;<br>    }<br>    return a - b ;<br>  }<br>  public int[] arrange(int[] numbers) {<br>    int i, j, k ;<br>    int n = numbers.length ;<br>    for (i=0; i&lt;n; i++) {<br>      for (j=0; j&lt;i; j++) {<br>  if (cmp(numbers[i], numbers[j]) &lt; 0) {<br>    int t = numbers[i] ;<br>    numbers[i] = numbers[j] ;<br>    numbers[j] = t ;<br>  }<br>      }<br>    }<br>    return numbers ;<br>  }<br>}<br></td>