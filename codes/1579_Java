            import java.util.*;<br> <br>public class GreedyChange{<br>  public int smallest( int[] denoms ){<br>    Arrays.sort(denoms);<br>    int[] mins = new int[1000001];<br>    int i,j;<br>    for( i=0; i&lt;=1000000; i++ ){<br>      mins[i] = 9999999;<br>    }<br>    mins[0] = 0;<br>    for( i=1; i&lt;=1000000; i++ ){<br>      for( j=0; j&lt;denoms.length; j++ ){<br>        if( denoms[j]&lt;=i ){<br>          mins[i] = Math.min(mins[i], 1+mins[i-denoms[j]]);<br>        }<br>      }<br>    }<br>    int index=0;<br>    for( i=1; i&lt;=1000000; i++ ){<br>      while( 1+index&lt;denoms.length &amp;&amp; denoms[1+index]&lt;=i ){<br>        index++;<br>      }<br>      if( mins[i] != 1+mins[i-denoms[index]] ){<br>        //System.out.println( i + " " + index + " " + mins[i] + " " + 1+mins[i-denoms[index]] );<br>        return i;<br>      }<br>    }<br>    return -1;<br>  }<br> <br> <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>