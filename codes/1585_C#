            using System; using System.Collections; using System.Text; using System.Text.RegularExpressions;<br>public class BusinessTasks<br>   {<br>   public string getTask(string[] list, int n)<br>      {<br>      ArrayList ar = new ArrayList(list);<br>      int pos=0;<br>      while (ar.Count &gt; 1)<br>         {<br>         pos = (pos+n-1)%ar.Count;<br>         ar.RemoveAt(pos);<br>         }<br>      return (String)ar[0];<br>      }<br><br><br>} // TopC#der<br><br><br>// Powered by FileEdit<br></td>