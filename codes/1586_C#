            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class JarBox {<br>    static double cos30 = Math.Cos(Math.PI/6);<br>    public int numJars(int RR, int WL2) {<br>        double WL = WL2;<br>        double R = RR;<br>        WL /= 2;<br>        int res=0;<br>        for ( int r = 1 ; r*R &lt;= WL ; r++ ) {<br>            for ( int c = 1 ; c &lt;= 5000 ; c++ ) {<br>                if ( r == 1 ) {<br>                    if ( (c+1)*R*2 &lt;= WL ) {<br>                        if ( c &gt; res ) {<br>                            res = c;<br>                        }<br>                    } else {<br>                        break;<br>                    }<br>                    continue;<br>                } else {<br>                    double W = 2*R*c;<br>                    double H = 2*R*((r-1)*cos30+1);<br>                    if ( H+W &gt; WL ) break;<br>                    int erc = r%2==0 ? r/2 : (r/2+1);<br>                    int orc = r - erc;<br>                    int NJ = erc*c+orc*(c-1);<br>                    if ( NJ &gt; res ) {<br>                        res = NJ;<br>                    }<br>                    W += R;<br>                    if ( H+W &gt; WL ) continue;<br>                    NJ = r*c;<br>                    if ( NJ &gt; res ) {<br>                        res = NJ;<br>                    }<br>                }<br>            }<br>        }<br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>