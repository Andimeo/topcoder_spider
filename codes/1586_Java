            import java.util.*;<br>public class JarBox {<br>  int doit(int r, int wl, int rw) {<br>    double h = (wl - rw * r * 2) / 2.0;<br>    int i = 0;<br>    while (r * (2 + i * Math.sqrt(3)) &lt;= h) i++;<br>    return i * (rw / 2) - (rw % 2 == 0 ? i / 2 : 0);<br>  }<br>  public int numJars(int r, int wl) {<br>    int m = 1;<br>    for (int i = 3; i &lt;= (wl - 4) / 2; i++)<br>      m = Math.max(m, doit(r, wl, i));<br>    return m;<br>  }<br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>