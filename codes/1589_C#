<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>public class RectangularGrid<br>{<br>  public long countRectangles( int width, int height )<br>  {<br>    long count = 0;<br>    for( int i=0; i&lt;width; i++ )<br>    {<br>      for( int j=0; j&lt;height; j++ )<br>      {<br>        if( j==i )<br>          continue;<br>        count += (width - i )* (height-j);<br>      }<br>    }<br>    return count;<br>  }<br>}<br></td>