            #line 2 "RectangularGrid.cpp"<br><br>#include &lt;iostream&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;map&gt;<br>#include &lt;sstream&gt;<br>#include &lt;cmath&gt;<br><br>using namespace std;<br><br><br>class RectangularGrid {<br>  public:<br>  long long countRectangles(int width, int height) {<br>    long long cnt=0;<br>    for(int i=1;i&lt;=width;i++)<br>      for(int j=1;j&lt;=height;j++) {<br>        if (i==j) continue;<br>        cnt+=(width-i+1)*(height-j+1);<br>      }<br>    return cnt;<br>  }<br>};<br><br><br>// Powered by FileEdit<br></td>