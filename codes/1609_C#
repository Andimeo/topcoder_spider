            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class TickTick {<br>    void count( double s, double[] d, Hashtable ht ) {<br>        string r = "";<br>        int i = 0;<br>        char nc = 'S';<br>        while( i != d.Length ) {<br>            if ( d[i] &lt; s ) {<br>                r += nc;<br>                nc = 'S';<br>                i++;<br>            }  else {<br>                s += 1;<br>                nc = 'D';<br>            }<br>        }<br>        ht[r] = null;<br>    }<br>    public int count(string[] evt) {<br>        Hashtable ht = new Hashtable();<br>        double[] d = new double[evt.Length];<br>        for ( int i = 0; i != d.Length ; i++ )<br>            d[i] = double.Parse(evt[i]);<br>        foreach ( string s in evt ) {<br>            string t = s.Substring(s.IndexOf("."));<br>            double tt = double.Parse(t)-0.000000001;<br>            count( tt, d, ht );<br>            tt = double.Parse(t)+0.000000001;<br>            count( tt, d, ht );<br>        }<br>        return ht.Count;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>