            using System;<br>using System.Collections;<br>public class SuperRot {<br>  public SuperRot()<br>  {<br><br>  }<br>  public string decoder(string message) {<br>    string retval = "";<br>    char d;<br>    foreach(char c in message)<br>    {<br>      if((int)c &gt;= (int)'A' &amp;&amp; (int)c &lt;= (int)'M')<br>      {<br>        d = (char)((int)c + 13);<br>      }<br>      else if((int)c &gt; (int)'M' &amp;&amp; (int)c &lt;= (int)'Z')<br>      {<br>        d = (char)((int)c - 13);<br>      }<br>      else if((int)c &gt;= (int)'a' &amp;&amp; (int)c &lt;= (int)'m')<br>      {<br>        d = (char)((int)c + 13);<br>      }<br>      else if((int)c &gt; (int)'m' &amp;&amp; (int)c &lt;= (int)'z')<br>      {<br>        d = (char)((int)c - 13);<br>      }<br>      else if((int)c &gt;= (int)'0' &amp;&amp; (int)c &lt;= (int)'4')<br>      {<br>        d = (char)((int)c + 5);<br>      }<br>      else if((int)c &gt; (int)'4' &amp;&amp;  (int)c &lt;= (int)'9')<br>      {<br>        d = (char)((int)c - 5);<br>      }<br>      else<br>      {<br>        d = ' ';<br>      }<br>      retval += d;<br>    }<br><br>    return retval;<br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>