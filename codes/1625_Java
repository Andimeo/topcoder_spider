<td class="problemText" colspan="8" valign="middle" align="left">
            import java.util.*;<br>public class SuperRot {<br>    public String decoder(String coded) {<br>        StringBuffer buf=new StringBuffer();<br>        for(int i=0;i&lt;coded.length();++i){<br>            char c=coded.charAt(i);<br>            int d=0;<br>            if(c&gt;='A'&amp;&amp;c&lt;='M')d='N'-'A';<br>            else if(c&gt;='N'&amp;&amp;c&lt;='Z')d='A'-'N';<br>            else if(c&gt;='a'&amp;&amp;c&lt;='m')d='n'-'a';<br>            else if(c&gt;='n'&amp;&amp;c&lt;='z')d='a'-'n';<br>            else if(c&gt;='0'&amp;&amp;c&lt;='4')d='5'-'0';<br>            else if(c&gt;='5'&amp;&amp;c&lt;='9')d='0'-'5';<br>            buf.append((char)(c+d));<br>        }<br>        return buf.toString();<br>    }<br>}<br></td>