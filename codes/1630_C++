            #include &lt;vector&gt;<br>#include &lt;sstream&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;queue&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>using namespace std;<br>typedef long long ll;<br>typedef vector&lt;int&gt; vi; typedef vector&lt;vi&gt; vvi;<br>typedef vector&lt;string&gt; vs; typedef vector&lt;vs&gt; vvs;<br>typedef istringstream iss; typedef ostringstream oss;<br>#define pq priority_queue<br>#define let(a,b) typeof(b) a(b)<br>#define all(a) a.begin(),a.end()<br>#define forall(a,b) for (let(a,b.begin());a!=b.end();a++)<br>#define forrall(a,b) for (let(a,b.rbegin());a!=b.rend();a++)<br>#define contains(a,b) (find(all(a),b)!=a.end())<br>template&lt;typename T&gt; string str(const T &amp;t){oss s;s&lt;&lt;t;return s.str();}<br>template&lt;typename T&gt; ll Int(const T &amp;t){stringstream s;s&lt;&lt;t;ll r;s&gt;&gt;r;return r;}<br>template&lt;typename T&gt; double Double(const T &amp;t){stringstream s;s&lt;&lt;t;double r;s&gt;&gt;r;return r;}<br>ll gcd(ll a, ll b){return b?gcd(b,a%b):a;}<br>class ContinuedFractions{<br>public:<br>vector &lt;int&gt; squareRoot(int n)<br>{<br>vector &lt;int&gt; ans;<br>set&lt;pair&lt;int,int&gt; &gt; got;<br>int a=0,b=1,sn=sqrt(double(n));<br>while (1)<br>{<br>    int k=(sn-a)/b;<br>    int na = -(a+b*k);<br>    int nb = (n-(a+b*k)*(a+b*k))/b;<br>    ans.push_back(k);<br>    if (!got.insert(make_pair(na,nb)).second) return ans;<br>    a=na;<br>    b=nb;<br>}<br>return ans;<br>}<br>};<br><br><br><br>// Powered by FileEdit<br></td>