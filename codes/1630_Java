            import java.util.*;<br> <br>public class ContinuedFractions {<br>  public long Extract (long a, long b, long c){<br>    return (long) ((double) Math.sqrt ((double) a) + b) / c;<br>  }<br>  <br>  public long Sqrt(long n){<br>    long d = (long) Math.sqrt (n);<br>    if ((d+1)*(d+1) == n) return d=1;<br>    return d;<br>  }<br>  <br>  public int[] squareRoot(int n) {<br>    ArrayList arr = new ArrayList();<br>    HashSet hash = new HashSet();<br>    long a = n;<br>    long b = -Sqrt (a);<br>    long c = 1;<br>    hash.add (a + " " + b + " " + c);<br>    arr.add (new Integer ((int) -b));<br>    while (true){<br>      System.out.println (a + " " + b + " " + c);<br>      long ap, bp, cp, np;<br>      cp = (a - b*b)/c;<br>      ap = a;<br>      bp = -b;<br>      long j = Extract (ap, bp, cp);<br>      bp -= j * cp;<br>      <br>      a = ap; b = bp; c = cp;<br>      String s = a + " " + b + " " + c;<br>      arr.add (new Integer ((int) j));<br>      if (hash.contains (s)) break;<br>      hash.add (s);<br>    }<br>    <br>    int ret[] = new int[arr.size()];<br>    for (int i = 0; i &lt; ret.length; i++) ret[i] = ((Integer) arr.get(i)).intValue();<br>    return ret;<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>