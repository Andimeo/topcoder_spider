            import java.util.*;<br>import java.math.*;<br>public class RPN {<br>  public int evaluate(String expr) {<br>    try {<br>      StringTokenizer st = new StringTokenizer(expr," ");<br>      int[] stack = new int[1000];<br>      int pos = 0;<br>      <br>      while (st.hasMoreTokens()) {<br>        String t = st.nextToken();<br>        if (Character.isDigit(t.charAt(0))) {<br>          stack[pos++] = Integer.parseInt(t);<br>        } else if (t.equals("+")) {<br>          if (pos&lt;2) return -999;<br>          stack[pos-2]=stack[pos-2]+stack[pos-1];<br>          pos--;<br>        } else if (t.equals("-")) {<br>          if (pos&lt;2) return -999;<br>          stack[pos-2]=stack[pos-2]-stack[pos-1];<br>          pos--;<br>        } else if (t.equals("*")) {<br>          if (pos&lt;2) return -999;<br>          stack[pos-2]=stack[pos-2]*stack[pos-1];<br>          pos--;<br>        } else if (t.equals("~")) {<br>          if (pos&lt;1) return -999;<br>          stack[pos-1]=-stack[pos-1];<br>        }<br>      }<br>      <br>      if (pos&gt;1) return -999;<br>      return stack[0];<br>      <br>    } catch (Exception e) { e.printStackTrace(); return -999; }<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>