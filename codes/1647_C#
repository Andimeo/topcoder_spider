<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br> <br>public class Iditarod<br>{<br>  public int avgMinutes(string[] times) <br>  {<br>    int sum = 0;<br>    int count = 0;<br>    foreach (string t in times) <br>    {<br>      string[] p = t.Split(new char[] {' ', ':'});<br>      int hour = Convert.ToInt32 (p[0]);<br>      int minute = Convert.ToInt32 (p[1]);<br>      if (hour == 12) <br>      {<br>        if (p[2] == "AM,") hour -= 12;<br>      }<br>      else {<br>        if (p[2] == "PM,") hour += 12;<br>      }<br>      int day = Convert.ToInt32 (p[4]);<br>      hour += (day - 1) * 24;<br>      minute += (hour * 60);<br>      minute -= 480;<br>      sum += minute;<br>      count++;<br>    }<br>    return (int) ((double)sum / (double)count + 0.5);<br>  }<br>}<br></td>