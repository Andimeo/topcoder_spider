            import java.util.*;<br>public class Iditarod {<br>  public int avgMinutes(String[] times) {<br>    int starttime=60*8+1*60*24;<br>    int timetot=0;<br>    for(int i=0; i&lt;times.length; i++){<br>      StringTokenizer st = new StringTokenizer(times[i],": M,");<br>      int time=60*(Integer.parseInt(st.nextToken())%12);<br>      time+=Integer.parseInt(st.nextToken());<br>      if(st.nextToken().equals("P")){<br>        time+=60*12;<br>      }<br>      st.nextToken();<br>      time+=Integer.parseInt(st.nextToken())*24*60;<br>      time-=starttime;<br>      timetot+=time;<br>    }<br>    return (int)(((timetot+0.0)/times.length)+.5);<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>