            Imports Microsoft.VisualBasic<br>Imports System.Math<br>Imports System<br>Imports System.Collections<br> <br>Public Class Iditarod<br>    Public Function avgMinutes(ByVal times As String()) As Integer<br>        Dim startTime As New Date(2000, 1, 1, 8, 0, 0)<br>        Dim a As Integer<br>        Dim all As Double<br>        For a = 0 To times.Length - 1<br>            Dim list() As String = times(a).Split(":"c, " "c)<br>            Dim time As Date<br>            If times(a).StartsWith("12") Then<br>                time = New Date(2000, 1, 1, IIf(list(2).Chars(0) = "P", 12, 0), list(1), 0)<br>                time = time.AddDays(CInt(list(4)) - 1)<br>            Else<br>                time = New Date(2000, 1, 1, IIf(list(2).Chars(0) = "P", 12, 0) + list(0), list(1), 0)<br>                time = time.AddDays(CInt(list(4)) - 1)<br>            End If<br>            all += time.Subtract(startTime).TotalMinutes<br>        Next<br>        Dim avg As Double = all / times.Length<br>        If avg - Floor(avg) &gt;= 0.5 Then<br>            Return Ceiling(avg)<br>        Else<br>            Return Floor(avg)<br>        End If<br>    End Function<br>End Class<br></td>