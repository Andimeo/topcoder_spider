            #include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;list&gt;<br>#include &lt;cmath&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;iostream&gt;<br>#include &lt;algorithm&gt;<br> <br>using namespace std;<br> <br>class Soccer {<br>  public:<br>  int maxPoints(vector &lt;int&gt; wins, vector &lt;int&gt; ties) {<br>    int max = -1;<br>    int i;<br>    for (i = 0; i &lt; wins.size(); ++i)<br>      max &gt;?= wins[i] * 3 + ties[i] * 1;<br>    return max;<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>