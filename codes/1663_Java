<td class="problemText" colspan="8" valign="middle" align="left">
            import java.util.*;<br>import java.math.*;<br>public class DQuads {<br>  public int count(String[] flights) {<br>    int n = flights.length;<br>    int[][] edges = new int[n][n];<br>    for (int i=0; i&lt;n; i++) {<br>      StringTokenizer st = new StringTokenizer(flights[i]);<br>      while (st.hasMoreTokens()) {<br>        edges[i][Integer.parseInt(st.nextToken())]++;<br>      }<br>    }<br>    <br>    int res=0;<br>    for (int i=0; i&lt;n; i++)<br>    for (int j=0; j&lt;n; j++)<br>    for (int k=0; k&lt;n; k++)<br>    for (int l=0; l&lt;n; l++) {<br>      if (i==j || i==k || i==l) continue;<br>      if (j==k || j==l) continue;<br>      if (k==l) continue;<br>      <br>      if (edges[i][k]&gt;0 || edges[k][i]&gt;0 || edges[j][l]&gt;0 || edges[l][j]&gt;0) continue;<br>      <br>      res+=edges[i][j]*edges[j][k]*edges[k][l]*edges[l][i];<br>    }<br>    <br>    return res/4;<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>