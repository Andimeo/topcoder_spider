            using System;<br>using System.Collections;<br> <br>public class PaternityTest<br>{<br>    public int[] possibleFathers(string c, string m, string[] men)<br>    {<br>        ArrayList al = new ArrayList();<br>        for( int i = 0; i &lt; men.Length; i++ )<br>        {<br>            bool match = true;<br> <br>            string f = men[i];<br> <br>            int mc=0;<br>            int fc=0;<br>            for( int j = 0; j &lt; c.Length; j++ )<br>            {<br>                if( c[j]==m[j] ) mc++;<br>                if( c[j]==f[j] ) fc++;<br>                if( c[j]!=m[j]&amp;&amp;c[j]!=f[j] ) match = false;<br>            }<br> <br> <br>            if( match &amp;&amp; mc&gt;=c.Length/2 &amp;&amp; fc&gt;=c.Length/2 ) al.Add(i);<br>        }<br>        return (int[])al.ToArray(typeof(int));<br>    }<br>}<br></td>