            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;iostream&gt;<br>#include &lt;algorithm&gt;<br> <br>using namespace std;<br> <br>class PaternityTest {<br>  public:<br>  vector &lt;int&gt; possibleFathers(string child, string mother, vector &lt;string&gt; men) {<br>    vector&lt;int&gt; retval;<br>    int i, j, c;<br>    <br>    for (i = 0; i  &lt; men.size(); ++i)<br>    {<br>      c = 0;<br>      for (j = 0; j &lt; child.length(); ++j)<br>      {<br>        if (child[j] != men[i][j]) {if (child[j] != mother[j]) break;}<br>        else c++;<br>      }<br>      if (j == child.length() &amp;&amp; c &gt;= men[0].size() / 2) retval.push_back (i);<br>    }<br>    return retval;<br>  }<br> <br>};<br> <br> <br>// Powered by PopsEdit<br></td>