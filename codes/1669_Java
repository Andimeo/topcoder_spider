            import java.util.*;<br>public class PaternityTest {<br>  public int[] possibleFathers(String child, String mother, String[] men) {<br>          int n = men.length;<br>          int[] res = new int[n];<br>          int ri=0;<br>          int l = men[0].length();<br>          for (int i=0;i&lt;n;i++) {<br>            int mo = 0;<br>            int fo = 0;<br>            int b= 0;<br>            boolean ok = true;<br>            for (int j=0;ok &amp;&amp; j&lt;child.length();j++) {<br>              char c= child.charAt(j);<br>              char f = men[i].charAt(j);<br>              char m = mother.charAt(j);<br>              if (c == m &amp;&amp; c == f)<br>                b++;<br>              else if (c==m)<br>                mo++;<br>              else if (c ==f)<br>                fo++;<br>              else<br>                ok = false;<br>            }<br>//            System.out.println("For " + men[i] + " mo=" + mo + " fo=" + fo);<br>            if (ok &amp;&amp; fo &lt;= l/2 &amp;&amp; mo &lt;= l/2)<br>              res[ri++]=i;<br>          }<br>          int[] rr = new int[ri];<br>          for (int i=0;i&lt;ri;i++)<br>            rr[i] = res[i];<br>          return rr;<br><br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>