            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class Bonuses {<br>  public int[] getDivision(int[] p) {<br>    int[] res = new int[p.Length];<br>    int sum = 0;<br>    int max = 0;<br>    foreach ( int n in p ) {<br>      sum += n;<br>      max = Math.Max(max,n);<br>    }<br>    int np = 100;<br>    for ( int i = 0; i != p.Length; i++) {<br>      int tp = (100*p[i])/sum;<br>      res[i] = tp;<br>      np -= tp;<br>    }<br>    Console.WriteLine("Leftover "+np);<br>    while ( np != 0) {<br>      for ( int i = 0; i != p.Length; i++) {<br>        if ( p[i] == max ) {<br>          res[i]++;<br>          np--;<br>          if ( np == 0)<br>            break;<br>        }<br>      }<br>      max--;<br>    }<br>    return res;<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>