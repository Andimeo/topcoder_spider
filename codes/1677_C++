            #line 2 "Bonuses.cpp"<br><br>#include &lt;iostream&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;map&gt;<br>#include &lt;sstream&gt;<br>#include &lt;cmath&gt;<br><br>using namespace std;<br><br><br>class Bonuses {<br>  public:<br>  vector &lt;int&gt; getDivision(vector &lt;int&gt; a) {<br>    int n=a.size(),sum=0;<br>    vector&lt;int&gt; ans(n,0);<br>    for(int i=0;i&lt;n;i++) sum+=a[i];<br>    int left=100;<br>    for(int i=0;i&lt;n;i++) {<br>      ans[i]=100*a[i]/sum;<br>      left-=100*a[i]/sum;<br>    }<br>    vector&lt;bool&gt; done(n,false);<br>    while (left--) {<br>      int h=-1,b=0;<br>      for(int i=0;i&lt;n;i++)<br>        if (!done[i] &amp;&amp; a[i]&gt;h) {<br>          h=a[i];<br>          b=i;<br>        }<br>      done[b]=true;<br>      ans[b]++;<br>    }<br>    return ans;    <br>  }<br>};<br><br><br>// Powered by FileEdit<br></td>