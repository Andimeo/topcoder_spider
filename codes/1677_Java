            /*/*///*/*/ Powered by radedit!            /*/*/*/*/*/ ;<br>/*/*//*/*/  import java . util .           /*/*/*/*/*/ ;<br>/*/*//*/*/  import java . lang . reflect . /*/*/*/*/*/ ;<br>public class Bonuses {<br>   boolean ddd = false ;<br>  public int[] getDivision(int[] points) {<br>    int i, j, k ;<br>    int s = 0 ;<br>    int n = points.length ;<br>    int[] p = new int[n] ;<br>    for (i=0; i&lt;n; i++) {<br>      s += points[i] ;<br>    }<br>    int used = 0 ;<br>    boolean u[] = new boolean[n] ;<br>    for (i=0; i&lt;n; i++) {<br>      int pp = 100 * points[i] / s ;<br>      p[i] = pp ;<br>      used += pp ;<br>    }<br>    for (i=0; i&lt;n &amp;&amp; used &lt; 100; i++) {<br>      k = -1 ;<br>      used++ ;<br>      for (j=0; j&lt;n; j++) {<br>  if (!u[j] &amp;&amp; (k == -1 || points[j] &gt; points[k])) {<br>    k = j ;<br>  }<br>      }<br>      p[k]++ ;<br>      u[k] = true ;<br>    }<br>    return p ;<br>  }<br>}<br></td>