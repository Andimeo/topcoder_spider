            import java.util.*;<br> <br>public class Sets <br>{<br>  public int[] operate(int[] A, int[] B, String operation)<br>  {<br>    ArrayList res = new ArrayList();<br>    boolean found = false;<br>    <br>    if(operation.equals("UNION"))<br>    {<br>      for(int i = 0; i &lt; A.length; i++)<br>      {<br>        Integer m = new Integer(A[i]);<br>        <br>        if(!res.contains(m))<br>        {<br>          res.add(m);<br>        }<br>      }<br>      <br>      for(int i = 0; i &lt; B.length; i++)<br>      {<br>        Integer m = new Integer(B[i]);<br>        <br>        if(!res.contains(m))<br>        {<br>          res.add(m);<br>        }<br>      }<br>    }<br>    else if(operation.equals("SYMMETRIC DIFFERENCE"))<br>    {  <br>      for(int i = 0; i &lt; A.length; i++)<br>      {<br>        found = false;<br>        <br>        for(int j = 0; j &lt; B.length; j++)<br>        {<br>          if(A[i] == B[j])<br>          {<br>            found = true;<br>            break;<br>          }<br>        }<br>        <br>        if(!found)<br>        {<br>          Integer m = new Integer(A[i]);<br>          <br>          if(!res.contains(m))<br>          {<br>            res.add(m);<br>          }<br>        }<br>      }<br>      <br>      for(int i = 0; i &lt; B.length; i++)<br>      {<br>        found = false;<br>        <br>        for(int j = 0; j &lt; A.length; j++)<br>        {<br>          if(B[i] == A[j])<br>          {<br>            found = true;<br>            break;<br>          }<br>        }<br>        <br>        if(!found)<br>        {<br>          Integer m = new Integer(B[i]);<br>          <br>          if(!res.contains(m))<br>          {<br>            res.add(m);<br>          }<br>        }<br>      }<br>    }<br>    else<br>    {<br>      for(int i = 0; i &lt; A.length; i++)<br>      {<br>        found = false;<br>        <br>        for(int j = 0; j &lt; B.length; j++)<br>        {<br>          if(A[i] == B[j])<br>          {<br>            found = true;<br>            break;<br>          }<br>        }<br>        <br>        if(found)<br>        {<br>          Integer m = new Integer(A[i]);<br>          <br>          if(!res.contains(m))<br>          {<br>            res.add(m);<br>          }<br>        }<br>      }<br>    }<br>    <br>    int[] ret = new int[res.size()];<br>    <br>    for(int i = 0; i &lt; ret.length; i++)<br>    {<br>      ret[i] = ((Integer) res.get(i)).intValue();<br>    }<br>    <br>    Arrays.sort(ret);<br>    return ret;<br>  }<br> <br>  <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>