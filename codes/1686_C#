            using System;<br>using System.Collections;<br>public class Quipu {<br>  public Quipu()<br>  {<br><br>  }<br>  public int readKnots(string knots) {<br>    int count = 0;<br>    int i;<br>    int current;<br>    int b = 1;<br><br>    current = 0;<br>    for(i = knots.Length - 2;i &gt;= 0; i--)<br>    {<br>      if(knots.Substring(i,1) == "-")<br>      {<br>        count += (b * current);<br>        current = 0;<br>        b = b * 10;<br>      }<br>      else<br>      {<br>        current++;<br>      }<br>    }<br><br>    return count;<br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>