            using System;<br>using System.Collections;<br>public class QuipuReader {<br>  public int[] readKnots(string[] knots) {<br>    int[] ret = new int[knots.Length];<br>    <br>    for (int i = 0; i &lt; knots[0].Length; i++) {<br>      bool x = false;<br>      for (int j = 0; j &lt; knots.Length; j++) {<br>        if ((knots[j][i] == 'X') &amp;&amp; (i &gt; 0) &amp;&amp; (knots[j][i-1] == '-')) {<br>          x = true;<br>        }<br>      }<br>      if (x) {<br>        x = true;<br>        for (int j = 0; j &lt; knots.Length; j++) {<br>          if ((knots[j][i] == 'X') &amp;&amp; (i &gt; 0) &amp;&amp; (knots[j][i-1] == 'X')) {<br>            x = false;<br>          }<br>        }<br>        if (x) {<br>          for (int j = 0; j &lt; knots.Length; j++) {<br>            ret[j] *= 10;<br>          }          <br>        }<br>      }<br>      for (int j = 0; j &lt; knots.Length; j++) {<br>        if (knots[j][i] == 'X') {<br>          ret[j]++;<br>        }<br>      }<br>    }<br> <br>    return (ret);<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>