            import java.util.*;<br> <br>public class QuipuReader<br>{<br> <br>    public int[] readKnots(String[] knots)<br>    {<br>        int[] result = new int[knots.length], curdig = new int[knots.length];<br> <br>        boolean[] last = new boolean[knots.length];<br> <br>        for (int i = 0;i &lt; knots[0].length();i++)<br>        {<br>            boolean newDig = true;<br> <br>            for (int j = 0;j &lt; knots.length;j++)<br>            {<br>                if (!(!last[j] || (last[j] &amp;&amp; knots[j].charAt(i) == '-')))<br>                {<br>                    newDig = false;<br>                }<br>            }<br> <br>            boolean worthit = false;<br> <br>            for (int j = 0;j &lt; knots.length;j++)<br>            {<br>                if (curdig[j] != 0)<br>                {<br>                    worthit = true;<br>                    break;<br>                }<br>            }<br> <br>            if (newDig &amp;&amp; worthit)<br>            {<br>                for (int j = 0;j &lt; knots.length;j++)<br>                {<br>                    result[j] *= 10;<br>                    result[j] += curdig[j];<br>                    curdig[j] = 0;<br>                }<br>            }<br> <br>            for (int j = 0;j &lt; knots.length;j++)<br>            {<br>                if (knots[j].charAt(i) == 'X')<br>                {<br>                    last[j] = true;<br>                    curdig[j]++;<br>                }<br>                else last[j] = false;<br>            }<br>        }<br> <br>        for (int j = 0;j &lt; knots.length;j++)<br>        {<br>            if (knots[j].charAt(knots[0].length() - 1) == 'X')<br>            {<br>                for (int i = 0;i &lt; knots.length;i++)<br>                {<br>                    result[i] *= 10;<br>                    result[i] += curdig[i];<br>                }<br> <br>                break;<br>            }<br>        }<br> <br>        return result;<br>    }<br> <br>}<br></td>