            import java.util.*;<br>public class PowerOutage {<br>  public int estimateTimeOut(int[] fromJunction, int[] toJunction, int[] ductLength) {<br>    <br>    int [][] table = new int [50][50];<br>    for (int i=0; i &lt; 50; i++)<br>    for (int j=0; j &lt; 50; j++)<br>      table[i][j] = 0;<br>    int doubleDuct = 0;<br>    <br>    for (int i=0;i &lt; fromJunction.length; i++)<br>    {<br>      table[fromJunction[i]][toJunction[i]] = ductLength[i];<br>      table[toJunction[i]][fromJunction[i]] = ductLength[i];<br>      doubleDuct += ductLength[i]*2;  <br>    }<br>    <br>    boolean change = true;<br>    <br>    while (change){ change = false;<br>    for (int i=0; i &lt; table.length; i++)<br>    {<br>      <br>      for (int j=0; j &lt; table[0].length; j++)<br>      {<br>        if (j==i) continue;<br>        <br>        for (int k = 0; k &lt; 50; k++)<br>        {<br>          if (table[i][k] == 0 || table[k][j] == 0 ) continue;<br>          <br>          if (table[i][j] == 0)<br>          {<br>            table[i][j] = table[i][k] + table[k][j];         <br>            change = true;  <br>          }<br>          else<br>          {<br>            int n = Math.min(table[i][j], table[i][k]+ table[k][j]);  <br>            if (n != table[i][j]) {change = true; table[i][j] = n;}  <br>          }<br>        }<br>      }  <br>    }<br>  }<br>    <br>    int maxFromZero = 0;<br>    for (int i=0; i &lt; 50; i++)<br>    maxFromZero = Math.max(maxFromZero, table[0][i]);<br>    <br>    return doubleDuct  - maxFromZero;<br>    <br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>