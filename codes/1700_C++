            #include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;sstream&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br> <br>using namespace std;<br> <br>class PrefixCode<br>{<br>public:<br>  string isOne(vector &lt;string&gt; words)<br>  {<br>    for (int i = 0; i &lt; words.size(); i++)<br>    {<br>      for (int j = 0; j &lt; words.size(); j++)<br>      {<br>        if (i != j)<br>        {<br>          if (words[j].substr(0, words[i].length()) == words[i])<br>          {<br>            string ret = "No, ";<br>            stringstream ret2;<br>            ret2 &lt;&lt; i;<br>            ret += ret2.str();<br>            return ret;<br>          }<br>        }<br>      }<br>    }<br> <br>    return "Yes";<br>  }<br>};<br></td>