            #line 2 "MergeSort.cpp"<br><br>#include &lt;iostream&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;map&gt;<br>#include &lt;sstream&gt;<br>#include &lt;cmath&gt;<br><br>using namespace std;<br><br><br>class MergeSort {<br>  public:<br>  <br>  int mergesort(vector&lt;int&gt; &amp;list)<br>  {<br>    if (list.size()&lt;=1) return 0;<br>    vector&lt;int&gt; a,b;<br>    int n=list.size();<br>    for(int i=0;i&lt;n/2;i++) a.push_back(list[i]);<br>    for(int i=n/2;i&lt;n;i++) b.push_back(list[i]);<br>    int cnt=mergesort(a)+mergesort(b);<br>    int ai=0,bi=0;<br>    while (ai&lt;a.size() &amp;&amp; bi&lt;b.size()) {<br>      cnt++;<br>      if (a[ai]&lt;b[bi])<br>        list[ai+bi]=a[ai++];<br>      else if (a[ai]&gt;b[bi])<br>        list[ai+bi]=b[bi++];<br>      else {<br>        list[ai+bi]=a[ai++];<br>        list[ai+bi]=b[bi++];<br>      }<br>    }<br>    while (ai&lt;a.size()) list[ai+bi]=a[ai++];<br>    while (bi&lt;b.size()) list[ai+bi]=b[bi++];<br>    return cnt;<br>  }<br>  <br>  int howManyComparisons(vector &lt;int&gt; a) {<br>    return mergesort(a);<br>  }<br>};<br><br><br>// Powered by FileEdit<br></td>