            public class CaseysArt {<br>  int n,m,k;<br>  double [][]f;<br> <br>  void rec(int i,int po,int pr,int cu){<br>    int ind=(~pr)&amp;(k-1);<br>    f[i][cu]+=f[i-1][ind];<br>    for(int j=po;j&lt;m-1;j++){<br>      int pr1=(pr&gt;&gt;j)%2;<br>      int pr2=(pr&gt;&gt;(j+1))%2;<br>      int cu1=(cu&gt;&gt;j)%2;<br>      int cu2=(cu&gt;&gt;(j+1))%2;<br> <br>      if(pr1==0 &amp;&amp; pr2==0 &amp;&amp; cu1==0) <br>        rec(i,j+1,pr | (1&lt;&lt;j) | (1&lt;&lt;(j+1)),cu | (1&lt;&lt;j));<br> <br>      if(pr1==0 &amp;&amp; pr2==0 &amp;&amp; cu2==0) <br>        rec(i,j+1,pr | (1&lt;&lt;j) | (1&lt;&lt;(j+1)),cu | (1&lt;&lt;(j+1)));<br> <br>      if(pr1==0 &amp;&amp; cu1==0 &amp;&amp; cu2==0) <br>        rec(i,j+1,pr | (1&lt;&lt;j),cu | (1&lt;&lt;j) | (1&lt;&lt;(j+1)));<br> <br>      if(pr2==0 &amp;&amp; cu1==0 &amp;&amp; cu2==0) <br>        rec(i,j+1,pr | (1&lt;&lt;(j+1)),cu | (1&lt;&lt;j) | (1&lt;&lt;(j+1)));<br>    }<br>  }<br>  <br>  public double howManyWays(int length, int width){<br>      n=length;<br>      m=width;<br>      k=1&lt;&lt;m;<br>      f=new double[n][k];<br>      int i,j;<br>      f[0][0]=1;<br>      for(i=1;i&lt;k;i++) f[0][i]=0;<br>      for(i=1;i&lt;n;i++) {<br>        for(j=0;j&lt;k;j++) f[i][j]=0;<br>        rec(i, 0, 0, 0);<br>      }<br>      return f[n-1][k-1];<br>  }<br>  <br>  public static void main(String[] args) {<br>    CaseysArt caseysArt1 = new CaseysArt();<br>    caseysArt1.howManyWays(3,4);<br>  }<br> <br>}<br></td>