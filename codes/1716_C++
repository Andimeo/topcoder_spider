            #include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cmath&gt;<br>#include &lt;queue&gt;<br>using namespace std;<br>typedef vector&lt;int&gt; VI;  typedef vector&lt;vector&lt;int&gt; &gt; VVI;<br>typedef vector&lt;string&gt; VS;  typedef vector&lt;vector&lt;string&gt; &gt; VVS;<br>typedef signed long long i64;  typedef unsigned long long u64;<br><br>class LeaguePicks {<br>public:<br>vector &lt;int&gt; returnPicks(int position, int friends, int picks) {<br>  int i, j, k, x, y, z, n;<br>  vector &lt;int&gt; ret;<br><br>  for( i = 0; i &lt; picks; i++ ) {<br>    x = i%(friends*2);<br>    if( x == position-1 ) ret.push_back(i+1);<br>    if( x == friends + friends - position ) ret.push_back(i+1);<br>  }<br>  return ret;<br>}<br>};<br><br><br>// Powered by FileEdit<br></td>