            #include &lt;algorithm&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br><br>using namespace std;<br><br>class ImageDithering {<br>   public:<br>   int count(string dithered, vector &lt;string&gt; screen) {<br>   int i,j, n,m, num=0;<br>   <br>   n=screen.size();<br>   m=screen[0].size();<br>   <br>   <br>   for (i=0;i&lt;n;i++) {<br>      for (j=0;j&lt;m;j++)<br>         if (dithered.find(screen[i][j])&lt;dithered.size()) num++;      <br>      }<br>   <br>   return num;  <br>  <br>   }<br><br><br><br><br><br><br><br>   <br> <br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>