            import java.util.*;<br> <br>public class ImageDithering <br>{<br>  public int count(String dithered, String[] screen)<br>  {<br>    int c = 0;<br>    <br>    for(int i = 0; i &lt; screen.length; i++)<br>    {<br>      String s = screen[i];<br>      <br>      for(int j = 0; j &lt; s.length(); j++)<br>      {<br>        if(dithered.indexOf(s.charAt(j)) != -1)<br>        {<br>          c++;<br>        }<br>      }<br>    }<br>    <br>    return c;<br>  }<br> <br>  <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>