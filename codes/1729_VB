            Public Class TopCan<br>    Public Function minSurface(ByVal volume As Integer) As Double<br>        Dim r, h As Double<br> <br>        r = (volume / (2 * System.Math.PI)) ^ (1 / 3)<br>        Return (2 * System.Math.PI * (r ^ 2)) + (2 * volume / r)<br>    End Function<br>End Class<br></td>