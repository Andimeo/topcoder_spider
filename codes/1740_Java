            public class CeyKaps {<br> <br> <br>    public String decipher(String in, String[] in2) {<br> <br>  for (int i = 0; i &lt; in2.length; i++) {<br>      char c = in2[i].charAt(0);<br>      char c2 = in2[i].charAt(2);<br>      for (int j = 0; j &lt; in.length(); j++) {<br>     if (in.charAt(j) == c) {<br>        in = in.substring(0,j) + c2 + in.substring(j+1,in.length());<br>       } else if (in.charAt(j) == c2) {<br>        in = in.substring(0,j) + c + in.substring(j+1,in.length());<br>      }<br> <br>      }<br>  }<br>  return in;<br> <br>    }<br> <br> <br> <br>}<br></td>