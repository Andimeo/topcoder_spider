            using System;<br> <br>public class MNS<br>{<br>  int [] n;<br> <br>  bool [] used = new Boolean[9];<br>  int [] m = new int [9];<br>  int total=0;<br> <br>  bool check()<br>  {<br>    int x = m[0]+m[1]+m[2];<br>    for(int i = 0; i&lt;3; i++)<br>    {<br>      if (m[i*3] + m[i*3+1] + m[i*3+2] != x) return false;<br>      if (m[i] + m[i+3] + m[i+6] != x) return false;<br>    }<br>    return true;<br>  }<br> <br>  void test(int i)<br>  {<br>    int j,last;<br> <br>    if (i == 9)<br>    {<br>      if (check()) total++;<br>      return;<br>    }<br> <br>    last = -1;<br> <br>    for(j=0; j&lt;9; j++)<br>    if (!used[j] &amp;&amp; n[j] != last)<br>    {<br>      last = n[j];<br>      used[j] = true;<br>      m[i] = n[j];<br>      test(i+1);<br>      used[j] = false;  <br>    }<br> <br> <br>  }<br> <br>  public int combos(int[] numbers)<br>  {<br>    Array.Sort(numbers);<br>    <br>    n = numbers;<br> <br>    test(0);<br>    return total;<br> <br>  }<br> <br>}<br></td>