            /*/*///*/*/ Powered by radedit!            /*/*/*/*/*/ ;<br>/*/*//*/*/  import java . util .           /*/*/*/*/*/ ;<br>/*/*//*/*/  import java . lang . reflect . /*/*/*/*/*/ ;<br>public class Gauss {<br>   boolean ddd = false ;<br>  public String[] whichSums(String target) {<br>    int i, j, k ;<br>    long nt = new Long(target).longValue() * 2 ;<br>    i = 1 ;<br>    while (i * (long)i &lt; 2 * nt) i++ ;<br>    List r = new ArrayList() ;<br>    while (i &gt; 1) {<br>      if (nt % i == 0) {<br>  long low = (nt / i) - (i-1) ;<br>  if (low % 2 == 0 &amp;&amp; low &gt; 0) {<br>    r.add("[" + (low / 2) + ", " + (low / 2 + i - 1) + "]") ;<br>  }<br>      }<br>      i-- ;<br>    }<br>    return (String[])r.toArray(new String[0]) ;<br>  }<br>}<br></td>