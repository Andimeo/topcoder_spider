            using System;<br> <br> <br>  public class StreetParking<br>  {<br>    string Street;<br>    public char StreetAt (int r) <br>    {<br>      if (r &lt; 0) return '-';<br>      if (r &gt;= Street.Length) return '-';<br>      return Street[r];<br>    }<br>    public int freeParks(string street)<br>    {<br>      Street = street;<br>      int r = 0;<br>      for (int i = 0; i &lt; street.Length; i++) <br>      {<br>        if ((street[i] == '-') &amp;&amp;<br>          StreetAt(i + 1) != 'B' &amp;&amp; StreetAt(i + 1) != 'S' &amp;&amp;<br>          StreetAt(i + 2) != 'B' &amp;&amp;<br>          StreetAt(i - 1) != 'S') r++;<br>      }<br>      return r;<br>    }<br> <br>}<br></td>