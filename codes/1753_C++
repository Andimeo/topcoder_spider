            #include&lt;iostream&gt;<br>#include&lt;string&gt;<br>#include&lt;vector&gt;<br>#include&lt;cmath&gt;<br> <br>using namespace std;<br> <br>class StreetParking<br>{<br>public:<br>  int freeParks(string street)<br>  {<br>    string t;<br>    int biao[60],i,sum;<br> <br>    t=street;<br>    for (i=0;i&lt;60;i++) biao[i]=0;<br>    sum=0;<br>    for (i=0;i&lt;t.size();i++)<br>    {<br>      if (t[i]=='D') biao[i]=1;<br>      if (t[i]=='B')<br>      {<br>        biao[i]=1;<br>        if (i-1&gt;=0) biao[i-1]=1;<br>        if (i-2&gt;=0) biao[i-2]=1;<br>      }<br>      if (t[i]=='S')<br>      {<br>        biao[i]=1;<br>        if (i-1&gt;=0) biao[i-1]=1;<br>        if (i+1&lt;t.size()) biao[i+1]=1;<br>      }<br>    }<br>    sum=0;<br>    for (i=0;i&lt;t.size();i++) if (biao[i]==0) sum++;<br>    return (sum);<br>  };<br>};<br></td>