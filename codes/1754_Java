            import java.util.*;<br>public class UserId {<br>  public String id(String[] inUse, String first, String middle, String last) {<br>          Set s = new HashSet();<br>          for (int i=0;i&lt;inUse.length;i++)<br>            s.add( inUse[i]);<br>          String f = strip(first);<br>          String m = strip(middle);<br>          String l = strip(last);<br>          System.out.println(f + "," + m + "," + l);<br>          if (f == null || m == null || l == null)<br>            return "BAD DATA";<br>          if (f.length() &lt; 2 || l.length() &lt; 2)<br>            return "BAD DATA";<br>          String res = make(""  + f.charAt(0), l,"");<br>          if (!s.contains(res)) return res;<br>          if (m.length() &gt; 0) {<br>            res = make("" + f.charAt(0) + m.charAt(0), l,"");<br>            if (!s.contains(res)) return res;<br>          }<br>          for (int i=101;i&lt;200;i++) {<br>            res = make(""+f.charAt(0), l, Integer.toString(i).substring(1));<br>            if (!s.contains(res)) return res;<br>          }<br>          return "ERROR";<br>  }<br>        public String make(String a, String l, String r)<br>        {<br>          int len = a.length() + r.length();<br>          int sz = 8 - len;<br>          if (sz &lt; l.length())<br>            return a + l.subSequence(0,sz) + r;<br>          else<br>            return a+l+r;<br>        }<br>        public String strip(String s) {<br>          String res = "";<br>          for (int i=0;i&lt;s.length();i++) {<br>            char c=s.charAt(i);<br>            if (Character.isLetter(c))<br>              res += Character.toLowerCase(s.charAt(i));<br>            else if (c != '\'' &amp;&amp; c != ' ')<br>              return null;<br>          }<br>          return res;<br>        }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>