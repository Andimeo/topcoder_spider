            import java.util.*;<br>public class AlphaDice {<br>  static int[][] corners = {<br>    {4,0,1},<br>    {4,1,2},<br>    {4,2,3},<br>    {4,3,0},<br>    {1,0,5},<br>    {2,1,5},<br>    {3,2,5},<br>    {0,3,5}<br>  };<br>    <br>  int[] c = new int[60];<br>  int[] r = new int[60];  <br>  <br>  char[] cur = new char[6];<br>  <br>  String[] roll;<br>  char[][] old = new char[60][3];<br>  <br>  int reached;<br>  <br>  void rec(int depth) {<br>    for (int i=0; i&lt;5; i++) if (cur[i] != 0) {<br>      for (int j=i+1; j&lt;6; j++) {<br>        if (cur[i]==cur[j]) return;<br>      }<br>    }<br>    <br>    if (depth&gt;reached) reached = depth;<br>    <br>    if (depth&gt;=roll.length) return;<br>    <br>    for (int tc=0; tc&lt;8; tc++) for (int cr=0; cr&lt;3; cr++) {<br>      boolean ok = true;<br>      for (int i=0; i&lt;3; i++) {<br>        if (cur[corners[tc][(i+cr)%3]] != 0 &amp;&amp; cur[corners[tc][(i+cr)%3]] != roll[depth].charAt(i)) {<br>          ok = false;<br>          break;<br>        }<br>      }<br>      <br>      if (!ok) continue;<br>      //System.out.println("Here "+depth + " " + tc +" " +cr);<br>      for (int i=0; i&lt;3; i++) {<br>        old[depth][i] = cur[corners[tc][(i+cr)%3]];<br>        cur[corners[tc][(i+cr)%3]] = roll[depth].charAt(i);<br>      }<br>      <br>      rec(depth+1);<br>      <br>      for (int i=0; i&lt;3; i++) {<br>        cur[corners[tc][(i+cr)%3]] = old[depth][i];<br>      }<br>    }<br>  }<br>  <br>  public int badData(String[] roll) {<br>    for (int i=0; i&lt;3; i++) cur[corners[0][i]] = roll[0].charAt(i);<br>    this.roll = roll;<br>    <br>    rec(1);<br>    <br>    if (reached&gt;=roll.length) return -1;<br>    return reached;<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>