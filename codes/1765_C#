            using System;<br>using System.Collections;<br>public class FixedPointTheorem {<br>  public FixedPointTheorem()<br>  {<br><br>  }<br>  public double cycleRange(double R) {<br>    double x;<br>    double low = 0;<br>    double high = 0;<br>    x= .25;<br>    for(int i = 0; i &lt; 200000;i++)<br>    {<br>      x = R * x * (1 - x);<br>    }<br>    x = R * x * (1 - x);<br>    low = x;<br>    high = x;<br>    for(int i = 0; i &lt; 999; i++)<br>    {<br>      x = R * x * (1 - x);<br>      if( x &lt; low)<br>        low = x;<br><br>      if( x &gt; high)<br>        high = x;<br>    }<br><br>    return high - low;<br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>