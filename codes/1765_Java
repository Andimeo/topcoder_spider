            import java.util.*;<br>public class FixedPointTheorem {<br>  public double cycleRange(double R) {<br>    double x = 0.25;<br>    for (int i = 0; i &lt; 200000; ++i) {<br>      x = R * x * (1 - x);<br>    }<br>    double max = 0; <br>    double min = 999999;<br>    for (int i = 0; i &lt; 1000; ++i) {<br>      x = R * x * (1 - x);<br>      max = Math.max(max, x);<br>      min = Math.min(min, x);<br>    }<br>    return max - min;<br>    <br>  }<br>  <br> <br>  public static void main(String[] args) {<br>    runTest(-1);<br>  }<br> <br>  <br>// BEGIN CUT HERE<br>  public static void runTest(int Case) {   FixedPointTheorem obj = new FixedPointTheorem();if ((Case == -1) || (Case == 0)) test_case_0(obj); if ((Case == -1) || (Case == 1)) test_case_1(obj); if ((Case == -1) || (Case == 2)) test_case_2(obj); if ((Case == -1) || (Case == 3)) test_case_3(obj); if ((Case == -1) || (Case == 4)) test_case_4(obj); if ((Case == -1) || (Case == 5)) test_case_5(obj); if ((Case == -1) || (Case == 6)) test_case_6(obj); }<br>  public static void verify_case(int Case, double expected, double received) { <br>    System.out.print("Test Case #" + Case + ": ");<br>    if (expected == received) System.out.println("PASSED");<br>    else { System.out.println("FAILED");<br>    System.out.println("Expected: " + expected);<br>    System.out.println("Received: " + received);<br> } }<br> <br>  private static void test_case_0(FixedPointTheorem obj) { double Arg0 = 0.1; double Arg1 = 0.0; <br>    verify_case(0, Arg1, obj.cycleRange(Arg0)); }<br> <br>  private static void test_case_1(FixedPointTheorem obj) { double Arg0 = 3.05; double Arg1 = 0.14754098360655865; <br>    verify_case(1, Arg1, obj.cycleRange(Arg0)); }<br> <br>  private static void test_case_2(FixedPointTheorem obj) { double Arg0 = 3.4499; double Arg1 = 0.4175631735867292; <br>    verify_case(2, Arg1, obj.cycleRange(Arg0)); }<br> <br>  private static void test_case_3(FixedPointTheorem obj) { double Arg0 = 3.55; double Arg1 = 0.5325704489850351; <br>    verify_case(3, Arg1, obj.cycleRange(Arg0)); }<br> <br>  private static void test_case_4(FixedPointTheorem obj) { double Arg0 = 3.565; double Arg1 = 0.5454276003030636; <br>    verify_case(4, Arg1, obj.cycleRange(Arg0)); }<br> <br>  private static void test_case_5(FixedPointTheorem obj) { double Arg0 = 3.5689; double Arg1 = 0.5489996297493569; <br>    verify_case(5, Arg1, obj.cycleRange(Arg0)); }<br> <br>  private static void test_case_6(FixedPointTheorem obj) { double Arg0 = 3.00005; double Arg1 = 0.004713996108955176; <br>    verify_case(6, Arg1, obj.cycleRange(Arg0)); }<br> <br>// END CUT HERE<br> <br> <br>  private static void out(String s) {<br>    System.out.println(s);<br>  }<br> <br>}<br> <br> <br>// Powered by PopsEdit<br>// Powered by TZTester.Java 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>