            import java.util.*;<br>public class Gems {<br>  int ret = 0;<br>  private boolean check(char[][] b) {<br>    boolean bv = true;<br>    for(int i=0; i&lt;b.length; i++) {<br>      for(int j=0; j&lt;b[0].length; j++) {<br>        bv = true;<br>        char t = b[i][j];<br>        for(int x=0;x&lt;3;x++) {<br>          if(j+x&gt;=b[0].length) {<br>            bv = false;<br>            break;<br>          }<br>          if(b[i][j+x] != t) {<br>            bv = false;<br>            break;<br>          }<br>        }<br>        if(bv == true)<br>          return true;<br>          <br>        bv = true;<br>        <br>        for(int x=0;x&lt;3;x++) {<br>          if(i+x&gt;=b.length) {<br>            bv = false;<br>            break;<br>          }<br>          if(b[i+x][j] != t) {<br>            bv = false;<br>            break;<br>          }<br>        }<br>        if(bv == true)<br>          return true;<br>      }<br>    }<br>    <br>    return false;<br>  }<br>  public int numMoves(String[] board) {<br>    char[][] b = new char[board.length][];<br>    for(int i=0; i&lt;board.length; i++)<br>      b[i] = board[i].toCharArray();<br>      <br>    boolean[][][][] m = new boolean[50][50][50][50];<br>    <br>    for(int i=0;i&lt;board.length;i++) {<br>      for(int j=0; j&lt;board[i].length(); j++) {<br>        <br>        for(int y=-1;y&lt;=1;y++) {<br>          for(int x=-1;x&lt;=1;x++) {<br>            if(i+y&lt;0 || i+y&gt;=board.length || j+x&lt;0 || j+x &gt;= board[0].length())<br>              continue;<br>              <br>            if(x+y == 0 || x+y == -2 || x+y == 2)<br>              continue;<br>              <br>            if(m[i][j][i+y][j+x])<br>              continue;<br>              <br>            char t = b[i+y][j+x];<br>            b[i+y][j+x] = b[i][j];<br>            b[i][j] = t;<br>            <br>            if(check(b)) {<br>              ret++;<br>            }<br>            <br>            t = b[i+y][j+x];<br>            b[i+y][j+x] = b[i][j];<br>            b[i][j] = t;<br>              <br>            m[i][j][i+y][j+x] = true;<br>            m[i+y][j+x][i][j] = true;<br>          }<br>        }<br>      }<br>    }<br>    <br>    return ret;<br>  }<br>  public static void main(String[] args) {<br>    //Gems temp = new Gems();<br>    //System.out.println(temp.numMoves(String[] board));<br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>