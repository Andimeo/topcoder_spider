<td class="problemText" colspan="8" valign="middle" align="left">
            #include &lt;vector&gt;<br>#include &lt;string&gt;<br> <br>using namespace std;<br> <br>class ProfitCalculator<br>{<br>  public:<br>    int percent(vector&lt;string&gt; items);<br>};<br> <br>int ProfitCalculator::percent(vector&lt;string&gt; items)<br>{<br>  int i, size;<br>  float p, c;<br>  double tp, tc;<br>  double profit;<br>  <br>  tp = 0;<br>  tc = 0;<br>  size = items.size();<br>  for (i = 0; i &lt; size; i++)<br>  {<br>    sscanf(items[i].c_str(), "%f %f", &amp;p, &amp;c);<br>    tp = tp + p;<br>    tc = tc + c;<br>  }<br> <br>  profit = tp - tc;<br>  return ((int) (profit * 100.0 / tp));<br>}<br></td>