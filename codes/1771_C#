            using System;<br>using System.Collections;<br>public class Collision {<br>  public double probability(int[] assignments, int ids) {<br>    int sum = 0;<br>    foreach (int i in assignments) {<br>      sum += i;<br>    }<br>    if (sum &gt; ids)<br>      return 0;<br>    int assigned1 = 0;<br>    double prob1 = 1;<br>    double prob2 = 1;<br>    for (int i = 0; i &lt; assignments.Length; ++i) <br>    {<br>      for (int j = 0; j &lt; assignments[i]; ++j) <br>      {<br>        int t = assigned1;<br>        if (i &gt; 0)<br>          prob2 *= Convert.ToDouble(ids - t) / (ids - j);<br>        prob1 *= Convert.ToDouble(ids - assigned1++) / ids;<br>      }<br>    }<br>    return prob2 - prob1;<br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>