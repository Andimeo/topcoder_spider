<td class="problemText" colspan="8" valign="middle" align="left">
            public class Collision {<br>public double probability(int[] ass, int ids) {<br>  // naive = nice<br>  try {<br>  double z1 = 1, z2=1;<br>  int left = ids, vfra1 = ids, vfra2 = ids;<br>  for(int i=ass.length;i--&gt;0;) {<br>    vfra1 = ids;<br>    for(int j=ass[i];j--&gt;0;) {<br>      z1 = z1 * left / vfra1;<br>      z2 = z2 * left / vfra2;<br>      left--;<br>      vfra1--;<br>    }<br>  }<br>  if(left&lt;0) return 0;<br>  return z1-z2;<br>}catch(Exception e) {<br>  return 0;<br>}<br> <br>}}<br></td>