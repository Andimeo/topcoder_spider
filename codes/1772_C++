            #pragma warning( disable : 4786 )<br><br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;functional&gt;<br>#include &lt;deque&gt;<br>#include &lt;cmath&gt;<br><br>using namespace std;<br><br>class Inventory {<br>  public:<br>  int monthlyOrder(vector &lt;int&gt; a, vector &lt;int&gt; b) {<br>    int mct = 0;<br>    double tot = 0;<br>    for( int i = 0; i &lt; a.size(); i++ ) {<br>      if( b[ i ] == 0 )<br>        continue;<br>      tot += 30.0 * a[ i ] / b[ i ];<br>      mct++;<br>    }<br>    return (int)ceil( tot / mct );<br>  }<br>};<br><br><br><br>// Powered by FileEdit<br></td>