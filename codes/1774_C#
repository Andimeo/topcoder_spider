<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>using System.Collections;<br><br><br>public class MostProfitable {<br>  public string bestItem(int[] costs, int[] prices, int[] sales, string[] items) {<br>    <br>    int bestI = -1;<br>    int bestP = 0;<br>    for (int i = 0; i &lt; costs.Length; i++)<br>    {<br>      int p = (prices[i] - costs[i]) * sales[i];<br>      if (p &gt; bestP)<br>      {<br>        bestP = p;<br>        bestI = i;<br>      }<br>    }<br><br>    return bestI == -1 ? "" : items[bestI];<br>  }<br>}<br><br><br><br>// Powered by FileEdit<br></td>