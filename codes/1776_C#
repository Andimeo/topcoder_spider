            public class DequeSort<br>  {<br>    public DequeSort()<br>    {<br>    }<br> <br>    public int minDeques(int[] data)<br>    {<br>      int n = data.Length;<br>      int[] mas = new int[n];<br>      int i,j;<br> <br>      for(i = 0; i &lt; n; i++)<br>        mas[i] = i;<br> <br>      for(i = 0; i &lt; n; i++)<br>        for(j = i+1; j &lt; n; j++)<br>          if (data[i] &gt; data[j])<br>          {<br>            int k = data[i];<br>            data[i] = data[j];<br>            data[j] = k;<br>            k = mas[i];<br>            mas[i] = mas[j];<br>            mas[j] = k;<br>          }<br>      int m = 1;<br>      bool f = true;<br>      for(i = 1; i &lt; n; i++)<br>        if (f)<br>        {<br>          if (mas[i-1] &lt; mas[i])<br>            f = false;<br>        }<br>        else<br>        {<br>          if (mas[i-1] &gt; mas[i])<br>          {<br>            f = true;<br>            m++;<br>          }<br>        }<br>      return m;<br>    }<br>  }<br></td>