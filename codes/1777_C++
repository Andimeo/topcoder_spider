            #include &lt;string&gt;<br>#include &lt;stack&gt;<br>#include &lt;vector&gt;<br>#include &lt;stdio.h&gt;<br>#include &lt;iostream&gt;<br>#include &lt;string.h&gt;<br>#include &lt;sstream&gt;<br>#include &lt;ctype.h&gt;<br>#include &lt;math.h&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;numeric&gt;<br>#include &lt;time.h&gt;<br>#include &lt;stdlib.h&gt;<br>using namespace std;<br> <br>class DiskSpace<br>{<br>public:<br>  int minDrives(vector &lt;int&gt; used, vector &lt;int&gt; total)<br>  {<br>    int totalU=accumulate(used.begin(), used.end(), 0);<br>    sort(total.rbegin(), total.rend());<br>    <br>    for(int i=0; i&lt;total.size(); i++)<br>    {<br>      if(totalU&lt;=total[i])<br>        return i+1;<br>      totalU-=total[i];<br>    }<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>