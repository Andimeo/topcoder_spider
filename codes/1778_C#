            using System;<br>using System.Collections;<br>public class BombSweeper {<br>  public BombSweeper()<br>  {<br><br>  }<br>  public double winPercentage(string[] board) {<br>    double wins = 0;<br>    double losses = 0;<br>    bool bLoss = false;<br><br>    for(int i = 0; i &lt; board.Length;i++)<br>    {<br>      for(int j = 0; j &lt; board[i].Length;j++)<br>      {<br>        if(board[i].Substring(j,1) == "B")<br>          losses++;<br>        else<br>        {<br>          //check neighbors<br>          //ul<br>          bLoss = false;<br>          if(i &gt; 0 &amp;&amp; j &gt; 0)<br>          {<br>            if(board[i-1].Substring(j-1,1) == "B")<br>              bLoss = true;<br>          }<br>          //uc<br>          if(i &gt; 0)<br>          {<br>            if(board[i-1].Substring(j,1) == "B")<br>              bLoss = true;<br>          }<br>          //ur<br>          if(i &gt; 0 &amp;&amp; j &lt; (board[i].Length - 1))<br>          {<br>            if(board[i-1].Substring(j+1,1) == "B")<br>              bLoss = true;<br>          }<br>          //cl<br>          if(j &gt; 0)<br>          {<br>            if(board[i].Substring(j-1,1) == "B")<br>              bLoss = true;<br>          }<br>          //cr<br>          if(j &lt; (board[i].Length - 1))<br>          {<br>            if(board[i].Substring(j+1,1) == "B")<br>              bLoss = true;<br>          }<br>          //ll<br>          if(i &lt; (board.Length - 1) &amp;&amp; j &gt; 0)<br>          {<br>            if(board[i+1].Substring(j-1,1) == "B")<br>              bLoss = true;<br>          }<br>          //lc<br>          if(i &lt; (board.Length - 1))<br>          {<br>            if(board[i+1].Substring(j,1) == "B")<br>              bLoss = true;<br>          }<br>          //ur<br>          if(i &lt; (board.Length - 1) &amp;&amp; j &lt; (board[i].Length - 1))<br>          {<br>            if(board[i+1].Substring(j+1,1) == "B")<br>              bLoss = true;<br>          }<br><br>          if(!bLoss)<br>            wins++;<br>        }<br>      }<br>    }<br><br>    Console.WriteLine(wins);<br>    Console.WriteLine(losses);<br>    return (wins / (wins + losses)) * 100;<br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>