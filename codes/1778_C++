            #include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cmath&gt;<br>#include &lt;queue&gt;<br>using namespace std;<br>typedef vector&lt;int&gt; VI;  typedef vector&lt;vector&lt;int&gt; &gt; VVI;<br>typedef vector&lt;string&gt; VS;  typedef vector&lt;vector&lt;string&gt; &gt; VVS;<br>typedef signed long long i64;  typedef unsigned long long u64;<br><br>class BombSweeper {<br>public:<br>double winPercentage(vector &lt;string&gt; a) {<br>  int i, j, k, x, y, z, n;<br>  double ret;<br>  <br>  for( x = 0; x &lt; a.size(); x++ )<br>  for( y = 0; y &lt; a[0].size(); y++ ) if( a[x][y] == 'B' ) {<br>    for( i = -1; i &lt;= 1; i++ )<br>    for( j = -1; j &lt;= 1; j++ )<br>      if( x+i &gt;= 0 &amp;&amp; x+i &lt; a.size() )<br>      if( y+j &gt;= 0 &amp;&amp; y+j &lt; a[0].size() )<br>      if( a[x+i][y+j] == '.' ) a[x+i][y+j] = ',';<br>  }<br>  i = j = 0;<br>  for( x = 0; x &lt; a.size(); x++ )<br>  for( y = 0; y &lt; a[0].size(); y++ ) if( a[x][y] == '.' )<br>    i++;<br>  for( x = 0; x &lt; a.size(); x++ )<br>  for( y = 0; y &lt; a[0].size(); y++ ) if( a[x][y] == 'B' )<br>    j++;<br>  return i*100/(double)(i+j);<br>}<br>};<br><br><br>// Powered by FileEdit<br></td>