            import java.util.*;<br>public class CheatCode<br>{<br>public int[] matches(String keys, String[] codes)<br>{<br>   ArrayList a = new ArrayList();<br>   for (int i = 0; i &lt; codes.length; i++)<br>      if (match(codes[i], keys)) a.add(new Integer(i));<br>   int[] res = new int[a.size()];<br>   for (int i = 0; i &lt; res.length; i++)<br>      res[i] = ((Integer) a.get(i)).intValue();<br>   return res;<br>}<br> <br>boolean match(String code, String keys)<br>{<br>   for (int i = 0; i &lt; keys.length(); i++)<br>      if (f(code, keys.substring(i), '*')) return true;<br>   return false;   <br>}<br> <br>boolean f(String code, String s, char last)<br>{<br>   if (code.length() == 0) return true;<br>   if (s.length() == 0) return false;<br>   if (code.charAt(0) == s.charAt(0))<br>      return f(code.substring(1), s.substring(1), code.charAt(0));<br>   else if (s.charAt(0) == last)<br>      return f(code, s.substring(1), last);<br>   else<br>      return false;<br>}<br>}<br></td>