            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class Table {<br>    public string[] layout(string[] t) {<br>        int R=t.Length;<br>        int C= 0;<br>        foreach( string x in t[0].Split(")".ToCharArray()) ) {<br>            if(x.Length == 0) continue;<br>            string[] y = x.Substring(1).Split(",".ToCharArray());<br>            C += Int32.Parse(y[0]);<br>        }<br>        char[,] m = new char[R,C];<br>        for( int r = 0; r != R; r++ ) {<br>            int c = 0;<br>            foreach( string x in t[r].Split(")".ToCharArray()) ) {<br>                while(c!=C&amp;&amp;m[r,c]!=0)c++;<br>                if(c==C) break;<br>                if(x.Length == 0) continue;<br>                string[] y = x.Substring(1).Split(",".ToCharArray());<br>                int cs = Int32.Parse(y[0]);<br>                int rs = Int32.Parse(y[1]);<br>                char ch = y[2][0];<br>                for ( int rr= r; rs != 0; rs--,rr++ ) {<br>                    int cx= cs;<br>                    for ( int cc = c; cx != 0; cx--,cc++ ) {<br>                        m[rr,cc] = ch;<br>                    }<br>                }<br>                c+=cs;<br>            }<br>        }<br>        string[] res = new string[R];<br>        for ( int r = 0; r != R; r++ ) {<br>            res[r]="";<br>            for( int c = 0; c != C; c++ ) {<br>                res[r] += m[r,c];<br>            }<br>        }<br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>