            import java.util.*;<br> <br>public class EndOfContest<br>{ public String[] calcEarnings(String[] sc, int[] money)<br>  { String[] names=new String[sc.length];<br>    double[] scores=new double[sc.length];<br>    int[] places=new int[sc.length];<br>    int[] cash=new int[sc.length];<br>    boolean[] lezero=new boolean[sc.length];<br>    for (int i=0; i&lt;sc.length; i+=1)<br>    { StringTokenizer st=new StringTokenizer(sc[i], " ");<br>      names[i]=st.nextToken();<br>      scores[i]=Double.parseDouble(st.nextToken());<br>      if (scores[i]&lt;=0)<br>        lezero[i]=true;<br>      else<br>        lezero[i]=false; }<br>    int n;<br>    for (int i=0; i&lt;scores.length; i+=n)<br>    { int max=i;<br>      double mx=scores[i];<br>      for (int j=0; j&lt;scores.length; j+=1)<br>      { if (i==j)<br>          continue;<br>        else if (scores[j]&gt;mx)<br>        { mx=scores[j];<br>          max=j; } }<br>      n=0;<br>      for (int j=0; j&lt;scores.length; j+=1)<br>      { if (scores[j]==mx)<br>        { n+=1;<br>          scores[j]=-99999999.99;<br>          places[j]=i+1; } }<br>      int mon=0;<br>      for (int j=i; j&lt;i+n; j+=1)<br>        if (j&lt;money.length)<br>          mon+=money[j];<br>      for (int j=0; j&lt;places.length; j+=1)<br>      { if (places[j]==i+1)<br>        { if (lezero[j])<br>            cash[j]=0;<br>          else if (i&lt;money.length)<br>            cash[j]=mon/n;<br>          else<br>            cash[j]=0; } } }<br>    String[] r=new String[scores.length];<br>    for (int i=0; i&lt;r.length; i+=1)<br>      r[i]=names[i]+" "+places[i]+", "+cash[i];<br>    return r; }<br> <br>  public static void main(String[] args)<br>  { String[] scores=    {"a: 100.00",<br> <br>    "b: 100.00",<br> <br>    "c: 100.00",<br> <br>    "d: 100.00",<br> <br>    "e: 100.00",<br> <br>    "f: 100.00",<br> <br>    "g: 100.00"};<br>    int[] money={1000,500};<br>   String[] r=((new EndOfContest()).calcEarnings(scores, money));<br>   for (int i=0; i&lt;r.length; i+=1)<br>     System.out.println("("+r[i]+")");<br> <br> <br>  } }<br></td>