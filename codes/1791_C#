            using System;<br>using System.Text;<br>using System.Collections;<br><br>public class TireRotation <br>{<br>  public int getCycle(string initial, string current) <br>  {<br>    int ret=1;<br>    string current2=initial;<br><br>    while(current!=current2)<br>    {<br>      if(ret&gt;4)<br>        return -1;<br><br>      string A=current2[0].ToString(),B=current2[1].ToString(),C=current2[2].ToString(),D=current2[3].ToString();<br><br>      current2=D+C+A+B;<br>      ret++;<br>    }<br><br>    return ret;<br>  }<br>}<br><br><br><br>// Powered by FileEdit<br></td>