            using System;<br> <br> <br>  public class LCMRange<br>  {<br>    /// &lt;summary&gt;<br>    /// The main entry point for the application.<br>    /// &lt;/summary&gt;<br>    [STAThread]<br>    static void Main(string[] args)<br>    {<br>      //<br>      // TODO: Add code to start application here<br>      //<br>    }<br> <br>    private int gcd( int a, int b )<br>    {<br>      if( a &lt; b ) return gcd( b, a );<br>      if( b == 0 ) return a;<br>      return gcd( b, a%b );<br>    }<br> <br>    private int xlcm( int a, int b )<br>    {<br>      return a*b/gcd( a, b );<br>    }<br> <br>    public int lcm(int first, int last)<br>    {<br>      int l = 1;<br>      for( int i = first; i &lt;= last; i++ )<br>      {<br>        l = xlcm( l, i );<br>      }<br>      return l;<br>    }<br>  }<br></td>