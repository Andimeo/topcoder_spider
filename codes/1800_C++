            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br> <br>using namespace std;<br> <br>class CardCount {<br>  public:<br>  vector &lt;string&gt; dealHands(int numPlayers, string deck) {<br>    <br> <br>vector&lt;string&gt; res(numPlayers);<br> <br>int count = 0;<br> <br>  for(int j = 0; j &lt; deck.size() / numPlayers; j++)<br>  {<br>    for(int i = 0; i &lt; numPlayers; i++)<br>    {<br>      res[i] += deck[count];<br>      count++;<br>    }<br>  }<br>  <br>  return res;<br>}<br>  // end of dealHands<br> <br>  <br> <br>};<br> <br>// end of CardCount<br> <br>// Powered by PopsEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>