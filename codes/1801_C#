            using System;<br>using System.Collections;<br>public class StringTrain {<br>  public string buildTrain(string[] cars) {<br>    string train = cars[0];<br>    for(int c=1; c&lt;cars.Length; c++) {<br>      for( int l = (cars[c].Length &gt; train.Length ? train.Length : cars[c].Length) -1 ; l &gt; 0 ; l-- ) {<br>      //  Console.WriteLine(l);<br>        if (train.EndsWith(cars[c].Substring(0,l))) {<br>          train += cars[c].Substring(l);<br>          Console.WriteLine("Added car " + c + ":" + cars[c]);<br>          break;<br>        }<br>      }<br>    }<br>    int tlength = train.Length;<br>    string end = "";<br>    while(train.Length &gt; 0) {<br>      end = train.Substring(train.Length-1,1) + end;<br>      train = train.Replace(train.Substring(train.Length-1,1),"");<br>    }<br>    return tlength.ToString()+ " " + end;<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>