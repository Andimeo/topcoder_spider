            import java.util.*;<br>public class StringTrain {<br>  public String buildTrain(String[] cars) {<br>    String train = cars[0];<br>    <br>    for (int i = 1; i &lt; cars.length; i++) {<br>      String s = cars[i];<br>      for (int j = s.length() -1 ; j &gt;0 ; j--) {<br>        String s1 = s.substring(0, j);<br>        if (!train.equals(s1) &amp;&amp; train.endsWith(s1)) {<br>          train += s.substring(j);<br>          break;<br>        }<br>      }<br>    }<br>    <br>    <br>    int n = train.length();<br>    <br>    String t = "";<br>    <br>    for (int i = 0; i &lt; train.length(); i++) {<br>      char ch = train.charAt(i);<br>      String s = train.substring(i+1);<br>      if (s.indexOf(ch) &lt; 0) t+=ch;<br>    }<br>    <br>    return ""+n+" "+t;<br>    <br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>