            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class IsHomomorphism {<br>  public string[] numBad(string[] ss, string[] tt, int[] mm) {<br>      ArrayList al = new ArrayList();<br>      for ( int a = 0 ; a != ss.Length ; a++ ) {<br>          for ( int b = 0 ; b != ss.Length ; b++ ) {<br>              int lhs = mm[ss[a][b]-'0'];<br>              int rhs = tt[mm[a]][mm[b]]-'0';<br>              if ( lhs != rhs ) {<br>                  al.Add("("+a+","+b+")");<br>              }<br>          }<br>      }<br>      return (string[])al.ToArray(typeof(string));<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>