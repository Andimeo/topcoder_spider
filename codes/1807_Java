            import java.util.*;<br>public class IsHomomorphism {<br>  int rows,cols;<br>  int[] map;<br>  int domap(int x)<br>  {<br>    return map[x];<br>  }<br>  int oper(int a,int b,String[] map)<br>  {<br>    return map[a].charAt(b) - '0';<br>  }<br>  public String[] numBad(String[] source, String[] target, int[] mapping) {<br>          map = mapping;<br>           rows = source.length;<br>           cols = source[0].length();<br>           List res = new ArrayList();<br>          for (int a=0;a&lt;rows;a++) {<br>            for (int b=0;b&lt;rows;b++) {<br>              int a_b = oper(a,b,source);<br>              int left = domap(a_b);<br>              int right = oper(domap(a),domap(b), target);<br>              if (left != right) {<br>                res.add( "(" + a+"," + b + ")");<br>              }<br>            }<br>          }<br>          String[] r = new String[res.size()];<br>          for (int i=0;i&lt;r.length;i++)<br>            r[i] = (String)res.get(i);<br>          return r;<br><br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>