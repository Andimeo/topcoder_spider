            import java.util.*;<br>public class Pool {<br> <br>   public int rackMoves ( int[] triangle ) {<br>     int sw = 0;<br>     if (triangle[4] != 8) {<br>       sw = 1;<br>       for (int i = 0; i &lt; triangle.length; i++) {<br>        if (triangle[i] == 8) {<br>          triangle[i] = triangle[4];<br>          triangle[4] = 8;<br>        }<br>      }<br>     }<br>     int sox = 0;<br>     if (triangle[0] &lt; 8) sox++;<br>    if (triangle[3] &lt; 8) sox++;<br>    if (triangle[5] &lt; 8) sox++;<br>    if (triangle[7] &lt; 8) sox++;<br>    if (triangle[9] &lt; 8) sox++;<br>    if (triangle[10] &lt; 8) sox++;<br>    if (triangle[12] &lt; 8) sox++;<br>    if (sox &lt; 4) return sox + sw;<br>    else return (7 - sox) + sw;<br>  }<br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>