            import java.util.*;<br>public class Rochambo {<br>  public int wins(String opp) {<br>    int toRet = 0;<br>    boolean same = false;<br>    for(int i = 0;i&lt;opp.length();i++) {<br>      if(i&lt;2) {<br>        if(opp.charAt(i)=='S') {<br>          toRet++;<br>        }<br>      } else {<br>        if(opp.charAt(i-1)==opp.charAt(i-2)) {<br>          if(opp.charAt(i-1)==opp.charAt(i)) {<br>            toRet++;<br>          }<br>        } else if(opp.charAt(i)!=opp.charAt(i-1) &amp;&amp; opp.charAt(i)!=opp.charAt(i-2)) {<br>          toRet++;<br>        }          <br>      }<br>    }<br>    return toRet;<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>