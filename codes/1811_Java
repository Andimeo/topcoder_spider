            // CHRIST I HATE DATES!!!<br>public class CalendarISO {<br>  boolean leap(int y) {<br>    if(0==(y%400)) return true;<br>    if(0==(y%100)) return false;<br>    return (0==(y%4));<br>  }<br>int julian(int Y,int M,int D) {<br>  int week = 3,m=1,d=1,y=1;<br>  while(y&lt;Y) {<br>    week += 365;<br>    if(leap(y)) week++;<br>    y++;<br>  }<br>  int length[]=new int[] {<br>    42, 31, leap(Y)?29:28,<br>    31,30,31,30,31,31,30,31,30,30 };<br>  while(d&lt;M) {<br>    week += length[d];<br>    d++;<br>  }<br>  return week + D-1;<br>}<br>public int weekNumber(int Y, int M, int D) {<br>  int week = julian(Y,1,1);<br>  int jan1 = week;<br>  int jan1d = week%7;<br>  int w0d = jan1 - jan1d;<br>  if(jan1d&lt;=3) w0d-=7;<br>  week = julian(Y,M,D);<br>  int day = (week - w0d)/7;<br>  if(day==0) return weekNumber(Y-1,12,31);<br>  if((julian(Y+1,1,1)-w0d)/7 == day)<br>    if(julian(Y+1,1,1)%7&lt;=3) return 1;<br>  return day;<br>}}<br></td>