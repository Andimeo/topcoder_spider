            import java.util.*;<br>public class Satellites {<br>    static final double rad = 6400.;<br>    static class Point {<br>        double lat;<br>        double lng;<br>        double height;<br>        double x;<br>        double y;<br>        double z;<br>        double a;<br>        public Point(double lat, double lng, double height) {<br>            a = Math.sqrt(height*(height+rad*2.));<br>            z = Math.sin(Math.toRadians(lat));<br>            x = Math.cos(Math.toRadians(lat))*Math.sin(Math.toRadians(lng));<br>            y = Math.cos(Math.toRadians(lat))*Math.cos(Math.toRadians(lng));<br>            x*=rad+height;<br>            y*=rad+height;<br>            z*=rad+height;<br>        }<br>        static Point parseSat(String sat) {<br>            return new Point(Double.parseDouble(sat.substring(0, 9)),<br>                    Double.parseDouble(sat.substring(10, 19)), <br>                    Double.parseDouble(sat.substring(20, 29)));<br>        }<br>        static Point parseRock(String sat) {<br>            return new Point(Double.parseDouble(sat.substring(0, 9)),<br>                    Double.parseDouble(sat.substring(10, 19)), <br>                    400.);<br>        }<br>        boolean visible(Point p) {<br>            double dx = p.x-x;<br>            double dy = p.y-y;<br>            double dz = p.z-z;<br>            double d = Math.sqrt(dx*dx+dy*dy+dz*dz);<br>            return d&lt;=a+p.a;<br>        }<br>    }<br>  public int[] detectable(String[] rockets, String[] satellites) {<br>        Point[] r = new Point[rockets.length];<br>        Point[] s = new Point[satellites.length];<br>        <br>        for(int i = 0; i&lt;r.length; i++) {<br>            r[i] = Point.parseRock(rockets[i]);<br>        }<br>        for(int i = 0; i&lt;s.length; i++) {<br>            s[i] = Point.parseSat(satellites[i]);<br>        }<br>        int[] res = new int[r.length];<br>        int pos = 0;<br>        for(int i = 0; i&lt;r.length; i++) {<br>            int cnt = 0;<br>            for(int j = 0; j&lt;s.length; j++) {<br>                if(s[j].visible(r[i])) cnt++;<br>            }<br>            if(cnt&gt;=3) {<br>                res[pos] = i;<br>                pos++;<br>            }<br>        }<br>        int[] res2 = new int[pos];<br>        for(int i = 0; i&lt;pos; i++) res2[i] = res[i];<br>    return res2;<br>  }<br> <br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>