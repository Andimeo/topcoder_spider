            using System; using System.Collections; public class Logger { public string[] log(string[] messages, string[] priorities, string[] prec, string loggingPriority) {<br>ArrayList result = new ArrayList();<br> <br>for (int i=0; i&lt;prec.Length; i++)<br> prec[i] = prec[i].ToLower();<br> <br>int a, b;<br>ParsePri(loggingPriority, prec, out a, out b);<br> <br>for (int i=0; i&lt;messages.Length; i++)<br> {<br> int c, d;<br> ParsePri(priorities[i], prec, out c, out d);<br> if (c &gt; a || (c==a &amp;&amp; d&gt;=b))<br>  result.Add(messages[i]);<br> }<br> <br>return (string[]) result.ToArray(typeof(String));<br>}<br> <br>void ParsePri(String pri, String[] prec, out int a, out int b)<br>{<br>int pos = pri.IndexOf(' ');<br>if (pos &gt;= 0)<br> {<br> b = Convert.ToInt32(pri.Substring(pos+1));<br> pri = pri.Substring(0, pos);<br> }<br>else<br> b = 0;<br> <br>a = Array.IndexOf(prec, pri.ToLower());<br>} // ParsePri<br> <br>}<br></td>