            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;map&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>using namespace std;<br> <br>class LetterFreq {<br>  public:<br>  vector &lt;int&gt; getFreqs(vector &lt;string&gt; a) {<br>    vector&lt;int&gt; ret(26);  <br>    for (int i = 0; i &lt; a.size(); ++i) {<br>      for (int j = 0; j &lt; a[i].size(); ++j) {<br>        unsigned int x = a[i][j] - 'A';<br>        if (x &gt;= 26) {<br>          x = a[i][j] - 'a';<br>        }<br>        if (x &lt; 26) {<br>          ret[x]++;<br>        }<br>      }<br>    }<br>    return ret;<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>