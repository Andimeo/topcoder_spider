            import java.util.*;<br>public class LetterFreq {<br>  public int[] getFreqs(String[] doc) {<br>    int[] lf=new int[26];<br>    for(int i=0;i&lt;doc.length;i++){<br>      doc[i]=doc[i].toLowerCase();<br>      for(int c=0;c&lt;doc[i].length();c++){<br>        char cur=doc[i].charAt(c);<br>        if(Character.isLetter(cur))lf[cur-'a']++;<br>      } //end ascending loop over chars in doc[i]<br>    } //end ascending loop over array doc<br>    return lf;<br>  }<br> <br>  //contest code ends here<br> <br>  <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>