            import java.util.*;<br><br>public class HardDequeSort {<br>  public int minDeques(int[] data) {<br>    int res = 0;<br>        int sorted[] = new int[data.length];<br>        for (int i = 0; i &lt; sorted.length; i++) {<br>            sorted[i] = data[i];<br>        }<br>        Arrays.sort(sorted);<br>        int n = 0;<br>        boolean used[] = new boolean[sorted.length];<br><br>        while (n &lt; sorted.length) {<br>            res++;<br>            int last = 0;<br>            for (int i = sorted.length - 1; i &gt;= 0; i--) {<br>                if (!used[i] &amp;&amp; n &lt; sorted.length &amp;&amp; data[i] == sorted[n]) {<br>                    used[i] = true;<br>                    last = i;<br>                    n++;<br>                }<br>            }<br>            for (int i = last; i &lt; sorted.length; i++) {<br>                if (!used[i] &amp;&amp; n &lt; sorted.length &amp;&amp; data[i] == sorted[n]) {<br>                    used[i] = true;<br>                    n++;<br>                }<br>            }<br>        }<br>    return res;<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>