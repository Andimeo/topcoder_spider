            using System;<br>using System.Collections;<br>public class PaperFold <br>{<br>  int bw,bh;<br>  public int numFolds(int[] paper, int[] box) <br>  {<br>    bw = box[0];<br>    bh = box[1];<br>    rec(paper[0],paper[1],0);<br>    return best == 1000 ? -1 : best;<br>  }<br><br>  int best = 1000;<br>  void rec( double w, double h, int times )<br>  {<br>    if ( times &gt; 8 )<br>      return;<br>    if ( (w&lt;=bw+.000001 &amp;&amp; h&lt;=bh+.000001) || (w&lt;=bh+.000001 &amp;&amp; h&lt;=bw+.000001) )<br>      best = Math.Min(best, times);<br>    rec( w/2.0, h, times+1 );<br>    rec( w, h/2.0, times+1 );<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>