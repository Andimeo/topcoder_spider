<td class="problemText" colspan="8" valign="middle" align="left">
            public class PaperFold {<br>  public int numFolds(int[] paper, int[] box) {<br>    int x = box[0], y = box[1];<br>    int folds1 = 0;<br>    while (x &lt; paper[0]) { folds1++; x *= 2; }<br>    while (y &lt; paper[1]) { folds1++; y *= 2; }<br>    x = box[1]; y = box[0];<br>    int folds2 = 0;<br>    while (x &lt; paper[0]) { folds2++; x *= 2; }<br>    while (y &lt; paper[1]) { folds2++; y *= 2; }<br>    if (folds1 &gt; 8 &amp;&amp; folds2 &gt; 8) return -1;<br>    return (folds1 &lt; folds2) ? folds1 : folds2;<br>  }<br>}<br></td>