            #include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cmath&gt;<br>#include &lt;queue&gt;<br>using namespace std;<br>typedef vector&lt;int&gt; VI;  typedef vector&lt;vector&lt;int&gt; &gt; VVI;<br>typedef vector&lt;string&gt; VS;  typedef vector&lt;vector&lt;string&gt; &gt; VVS;<br>typedef signed long long i64;  typedef unsigned long long u64;<br><br>double memo[100][100];<br><br>class PointSystem {<br>public:<br>double oddsOfWinning(int a, int b, int c) {<br>  int i, j, k, x, y, z, n;<br>  for( i = 0; i &lt; 100000; i++ ) {<br>    for( x = 0; x &lt;= a+b; x++ )<br>    for( y = 0; y &lt;= a+b; y++ ) {<br>      if( x &gt; a &amp;&amp; y &gt; a ) {memo[x][y] = memo[x-1][y-1]; continue;}<br>      if( x &gt;= a &amp;&amp; x &gt;= y+b ) {memo[x][y] = 0.0; continue;}<br>      if( y &gt;= a &amp;&amp; y &gt;= x+b ) {memo[x][y] = 1.0; continue;}<br>      memo[x][y] = ((c*memo[x][y+1])+((100-c)*memo[x+1][y]))/100.0;<br>    }<br>  }<br>  return memo[0][0];<br>}<br>};<br><br><br>// Powered by FileEdit<br></td>