            using System;<br>using System.Collections;<br> <br>  public class PartySeats<br>  {<br> <br>    public string[] seating(string[] attendees)<br>    {<br>      string[] empty = new string[0];<br> <br>      ArrayList boys = new ArrayList();<br>      ArrayList girls = new ArrayList();<br>      foreach(string a in attendees)<br>      {<br>        string[] b = a.Split(' ');<br>        if(b[1].ToLower().Equals("boy"))<br>          boys.Add(b[0]);<br>        else<br>          girls.Add(b[0]);<br>      }<br> <br>      boys.Sort();<br>      girls.Sort();<br> <br>      if(boys.Count != girls.Count) return empty;<br>      if(boys.Count % 2 == 1) return empty;<br> <br>      ArrayList result = new ArrayList();<br>      result.Add("HOST");<br>      for(int i = 0; i &lt; boys.Count / 2; i++)<br>      {<br>        result.Add(girls[i]);<br>        result.Add(boys[i]);<br>      }<br>      result.Add("HOSTESS");<br> <br>      for(int i = 0; i &lt; boys.Count / 2; i++)<br>      {<br>        result.Add(boys[i + (boys.Count / 2)]);<br>        result.Add(girls[i + (boys.Count / 2)]);<br>      }<br> <br>      return (string[]) result.ToArray(typeof(string));;<br> <br>    }<br>}<br></td>