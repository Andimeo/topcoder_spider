<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>using System.Collections;<br>public class ParallelSpeedup {<br>  public int numProcessors(int k, int overhead) {<br>    int ret = 0;<br>    long best = long.MaxValue;<br> <br>    for (int i = 1; i &lt;= k; i++) {<br>      long c = ((long)i * ((long)i-1) / 2 * (long)overhead) + (k / i);<br>      if (k % i != 0) {<br>        c++;<br>      }<br>      if (c &lt; best) {<br>        ret = i;<br>        best = c;<br>      }<br>    }<br> <br>    return (ret);<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>