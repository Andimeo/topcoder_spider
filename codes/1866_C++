            #pragma warning( disable : 4786 )<br><br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;functional&gt;<br>#include &lt;deque&gt;<br><br>using namespace std;<br><br>class ParallelSpeedup {<br>  public:<br>  int numProcessors(int k, int overhead) {<br>    int bestt = 1000000000;<br>    int bite = -1;<br>    for( int i = 1; i &lt; 1000; i++ ) {<br>      int tover = ( i * ( i - 1 ) ) / 2;<br>      tover *= overhead;<br>      tover += ( k + i - 1 ) / i;<br>      if( tover &lt; bestt ) {<br>        bestt = tover;<br>        bite = i;<br>      }<br>    }<br>    return bite;<br>  }<br>};<br><br><br><br>// Powered by FileEdit<br></td>