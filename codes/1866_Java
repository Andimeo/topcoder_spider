<td class="problemText" colspan="8" valign="middle" align="left">
            import java.util.*;<br> <br>public class ParallelSpeedup<br>{<br>    public int numProcessors(int k, int o)<br>    {<br>        int min = k;<br>        int ret = 1;<br>        for (int i=2; i &lt; 10000; i++)<br>        {<br>            int c = k/i + ((k%i != 0)?1:0) + o * (i * (i-1))/2;<br>            if (c &lt; min)<br>            {<br>                min = c;<br>                ret = i;<br>            }<br>        }<br>        <br>        return ret;<br>    }<br> <br>    public static void main(String args[])<br>    {<br>        ParallelSpeedup x = new ParallelSpeedup();<br>     <br>        //Example 0<br>        show(x.numProcessors(12,<br>            1));<br>        show(2);<br>        System.out.println();<br> <br>        //Example 1<br>        show(x.numProcessors(50,<br>            3));<br>        show(3);<br>        System.out.println();<br> <br>        //Example 2<br>        show(x.numProcessors(9,<br>            10));<br>        show(1);<br>        System.out.println();<br> <br>        //Example 3<br>        show(x.numProcessors(3333,<br>            2));<br>        show(12);<br>        System.out.println();<br> <br>        //Example 4<br>        show(x.numProcessors(1000000,<br>            4));<br>        show(63);<br>        System.out.println();<br> <br>    }<br> <br>    //Prints an int<br>    public static void show(int i)<br>    {<br>        System.out.println(i);<br>    }<br>}<br></td>