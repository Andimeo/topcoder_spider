            #include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;math.h&gt;<br>#include &lt;sstream&gt;<br> <br>using namespace std;<br> <br>int gcd(int a, int b)<br>{<br>  if (b &gt; a) return gcd(b, a);<br>  else if (a%b==0) return b;<br>  else return gcd(b, a%b);<br>}<br> <br>int lcm(int a, int b)<br>{<br>  return a * b / gcd(a, b);<br>}<br> <br>int sum(vector&lt;int&gt; a)<br>{<br>  int total=0;<br>  for(int i=0; i&lt;a.size(); i++)<br>    total += a[i];<br>  return total;<br>}<br> <br>class NumberGuesser<br>{public:<br> <br>int guess(string s)<br>{  int temp = 0;<br>  for(int i=0; i&lt;s.size(); i++)<br>    temp += s[i] -'0';<br>  temp = temp % 9;<br>  return 9-temp;<br>}<br>};<br></td>