            import java.util.*;<br>public class MineField {<br>  public String[] getMineField(String mineLocations) {<br>    char a[][] = new char[9][9];<br>    <br>    StringTokenizer st = new StringTokenizer( mineLocations, "(,)" );<br>    while ( st.hasMoreTokens() ) {<br>      int x = Integer.parseInt( st.nextToken() );<br>      int y = Integer.parseInt( st.nextToken() );<br>      <br>      a[x][y] = 'M';<br>    }<br>    <br>    int dx[] = { 1, 1, 1, 0, 0, -1, -1, -1 };<br>    int dy[] = { 1, -1, 0, 1, -1, 0, 1, -1 };<br>    for ( int i = 0; i &lt; 9; i++ )<br>      for ( int j = 0; j &lt; 9; j++ )<br>      if ( a[i][j] == 'M' ) {<br>        for ( int k = 0; k &lt; 8; k++ ) {<br>          int sx = i + dx[k];<br>          int sy = j + dy[k];<br>          <br>          if ( sx &gt;= 0 &amp;&amp; sx &lt; 9 &amp;&amp; sy &gt;= 0 &amp;&amp; sy &lt; 9 &amp;&amp; a[sx][sy] != 'M' )<br>          a[sx][sy]++;<br>        }<br>      }<br>      <br>      for ( int i = 0; i &lt; 9; i++ )<br>      for ( int j = 0; j &lt; 9; j++ )<br>      if ( a[i][j] != 'M' )<br>      a[i][j] += '0';<br>      <br>      String[] ans = new String[9];<br>      for ( int i = 0; i &lt; 9; i++ )<br>      ans[i] = new String( a[i] );<br>      <br>      return ans;<br>  }<br>  <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>