<td class="problemText" colspan="8" valign="middle" align="left">
            public class Swimmers <br>  {<br>    public int[] getSwimTimes(int[] distances, int[] speeds, int current) <br>    {<br>      int[] ret;<br>      ret = new int[distances.Length];<br>      double d;<br>      for(int i=0; i&lt;distances.Length; i++)  <br>      {<br>        if(speeds[i]&lt;=current &amp;&amp; distances[i]&gt;0) <br>        {<br>          ret[i]=-1;<br>        }<br>        else <br>        {<br>          d = (1.0*distances[i]/(speeds[i]+current)) + (1.0*distances[i] / (speeds[i]-current));<br>          ret[i] = (int)d;<br>        }<br>      }<br> <br> <br>      return ret;<br>    }<br>  }<br></td>