<td class="problemText" colspan="8" valign="middle" align="left">
            #include &lt;iostream&gt;<br>#include &lt;vector&gt;<br> <br>using namespace std;<br> <br>class Swimmers<br>{<br>public:<br>  vector &lt;int&gt; getSwimTimes(vector &lt;int&gt; distances, vector &lt;int&gt; speeds, int current)<br>  {<br>    vector &lt;int&gt; returnVec;<br>    for(int i = 0; i &lt; distances.size(); i++)<br>    {<br>      if(distances[i] == 0)<br>        returnVec.push_back(0);<br>      else if(speeds[i] &lt;= current)<br>        returnVec.push_back(-1);<br>      else<br>        returnVec.push_back((int) ((double)distances[i]/((double)speeds[i]+(double)current) + (double)distances[i]/((double)speeds[i]-(double)current)));<br>    }<br>    return returnVec;<br>  }<br>};<br></td>