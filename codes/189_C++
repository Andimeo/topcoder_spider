            #include &lt;stdio.h&gt;<br>#include &lt;set&gt;<br>#include &lt;vector&gt;<br>#include &lt;map&gt;<br>#include &lt;iostream&gt;<br>#include &lt;string&gt;<br> <br>using namespace std;<br> <br>class Clock {<br>public:<br> <br>  vector&lt; int &gt; getAngles( string in ) {<br>    double hr;<br>    double min;<br>    double sec;<br> <br>    sscanf( in.c_str(), "%lf:%lf:%lf", &amp;hr, &amp;min, &amp;sec );<br>    if( hr == 12 ) hr = 0;<br> <br>    min += sec / 60;<br>    hr += min / 60;<br> <br>    vector&lt; int &gt; out;<br> <br>    cout &lt;&lt; hr &lt;&lt; ":" &lt;&lt; min &lt;&lt; ":" &lt;&lt; sec;<br> <br>    out.push_back( (int)( hr * 360 / 12 ) );<br>    out.push_back( (int)( min * 360 / 60 ) );<br>    out.push_back( (int)( sec * 360 / 60 ) );<br> <br>    return out;<br> <br>  };<br> <br>};<br></td>