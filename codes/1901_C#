            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class FairWorkload {<br>    public int getMostWork(int[] f, int n) {<br>        for ( int res = 0 ; res &lt;= f.Length*1000; res++ ) {<br>            int cnt = 0 ;<br>            int wc = 1;<br>            for ( int i = 0 ; i != f.Length; i++ ) {<br>                if ( f[i] &gt; res ) {<br>                    wc = n+1;<br>                    break;<br>                }<br>                if ( cnt + f[i] &lt;= res ) {<br>                    cnt += f[i];<br>                } else {<br>                    cnt = f[i];<br>                    wc++;<br>                }<br>            }<br>            if ( cnt &lt;= res &amp;&amp; wc &lt;= n )<br>                return res;<br>        }<br>        return 0;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>