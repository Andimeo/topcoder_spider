            import java.util.*;<br>public class FairWorkload {<br>  public boolean could( int[] folders, int workers, int max ) {<br>    int cur = 0;<br>    for ( int i = 0; i &lt; folders.length; i++ ) {<br>      cur += folders[i];<br>      if ( cur &gt; max ) {<br>        cur = folders[i];<br>        workers--;<br>      }<br>      if ( cur &gt; max ) {<br>        return false;<br>      }<br>    }<br>    return workers &gt; 0;<br>  }<br>  <br>  public int getMostWork(int[] folders, int workers) {<br>    for ( int i = 0; true; i++ ) {<br>      if ( could( folders, workers, i ) ) {<br>        return i;<br>      }<br>    }<br>  }<br>  <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>