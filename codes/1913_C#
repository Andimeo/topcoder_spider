            using System;<br>using System.Collections;<br>public class OldestOne {<br>  public string oldest(string[] data) <br>   {<br>     int max = 0;<br>      string maxn = null;<br>      foreach(string s in data)<br>      {<br>         string name;<br>         int age = GetAge(s, out name);<br>     <br>         if (age &gt; max)<br>         {<br>            max = age;<br>            maxn = name;<br>         }         <br>      }<br>      return maxn;<br>  }<br><br><br>   public int GetAge(string t, out string name)<br>   {<br>      string[] a = t.Split(' ');<br>      name = "";<br>      foreach(string s in a)<br>      {<br>         int num;<br>         try<br>         {<br>            num = Convert.ToInt32(s);<br>            int pos = t.IndexOf(s);<br>            name = t.Substring(0, pos).Trim();<br>            return num;<br>         }<br>         catch(Exception)<br>         {<br>         }<br>      }<br>      return -1;<br>   }<br><br><br>}<br><br><br>// Powered by FileEdit<br></td>