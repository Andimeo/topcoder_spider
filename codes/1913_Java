            import java.util.*;<br>public class OldestOne {<br>  public String oldest(String[] data) {<br>    int N = data.length;<br><br>    int oldest=0;<br>    int maxage=0;<br>    String name="";<br><br>    for (int i=0; i&lt;N; i++)<br>    {<br>      int j = 0;<br>      while (!Character.isDigit(data[i].charAt(j))) j++;<br>      String n = data[i].substring(0,j).trim();<br>      int k=j;<br>      while (!Character.isWhitespace(data[i].charAt(j))) j++;<br>      int age = Integer.parseInt(data[i].substring(k,j));<br><br>      if (age &gt; maxage)<br>      {<br>        name = n;<br>        maxage = age;<br>      }<br>    }<br>    return name;<br><br>    <br>  }<br>  <br>}<br><br><br>// Powered by FileEdit<br></td>