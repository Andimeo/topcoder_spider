            using System;<br>using System.Text;<br>using System.Collections;<br><br>public class RecurrenceRelation {<br>  int mod10(int x)<br>  {<br>    x = x % 10;<br>    if (x&lt;0) x+=10;<br>    return x;<br>  }<br>  public int moduloTen(int[] c, int[] ini, int N) {<br><br>    int m = c.Length;<br>    <br>    if (N&lt;m)<br>      return mod10(ini[N]);<br><br>    for(int i = m; i&lt;=N; i++)<br>    {<br>      int l = 0;<br>      for(int j = 0; j&lt;m; j++)<br>      {<br>        l += mod10(mod10(ini[j]) * mod10(c[j]));<br>      }<br>      for(int j = 0; j&lt;m-1; j++)<br>      {<br>        ini[j] = ini[j+1];<br>      }<br>      ini[m-1] = mod10(l);<br>    <br>    }<br><br>    return mod10(ini[m-1]);<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester for c# [12-oct-2003]<br>// Powered by CodeProcessor<br></td>