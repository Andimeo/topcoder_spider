            #include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cmath&gt;<br>#include &lt;queue&gt;<br>using namespace std;<br>typedef vector&lt;int&gt; VI;  typedef vector&lt;vector&lt;int&gt; &gt; VVI;<br>typedef vector&lt;string&gt; VS;  typedef vector&lt;vector&lt;string&gt; &gt; VVS;<br>typedef signed long long i64;  typedef unsigned long long u64;<br><br>class RecurrenceRelation {<br>public:<br>int moduloTen(vector &lt;int&gt; a, vector &lt;int&gt; b, int c) {<br>  int i, j, k, x, y, z, n;<br>  int ret;<br><br>  if( c &lt; a.size() ) return (b[c]%10+10)%10;<br>  for( i = 0; i &lt;= c-a.size(); i++ ) {<br>    x = 0;<br>    for( j = 0; j &lt; a.size(); j++ ) x += b[j]*a[j];<br>    b.push_back((x%10+10)%10);<br>    b.erase(b.begin());<br>  }<br>  return b.back();<br>}<br>};<br><br><br>// Powered by FileEdit<br></td>