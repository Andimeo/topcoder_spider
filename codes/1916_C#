            using System;<br>using System.Text;<br>using System.Collections;<br><br>public class Carbon14 {<br>  public int[] dateRange(int concentration, int err) {<br><br>    double max = concentration+err;<br>    double min = concentration-err;<br><br>    int t1 =(int) Math.Floor(-8267 * Math.Log(max/10000.0));<br>    int t2 = (int)Math.Ceiling(-8267 * Math.Log(min/10000.0));<br>    return new int [2] { t1,t2 };<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester for c# [12-oct-2003]<br>// Powered by CodeProcessor<br></td>