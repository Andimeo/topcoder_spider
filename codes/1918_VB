            Imports Microsoft.VisualBasic<br>Imports System.Math<br>Imports System<br>Imports System.Collections<br>Imports System.Text<br> <br>Public Class FlowerGarden<br>    Private Structure combo<br>        Implements IComparable<br> <br>        Dim height As Integer<br>        Dim bloom As Integer<br>        Dim wilt As Integer<br> <br>        Public Function CompareTo(ByVal obj As Object) As Integer Implements System.IComparable.CompareTo<br>            Return obj.height - height<br>        End Function<br>    End Structure<br> <br>    Public Function getOrdering(ByVal height As Integer(), ByVal bloom As Integer(), ByVal wilt As Integer()) As Integer()<br>        Dim a, b, c As Integer<br>        Dim f(height.Length - 1) As combo<br>        For a = 0 To f.Length - 1<br>            f(a).height = height(a)<br>            f(a).bloom = bloom(a)<br>            f(a).wilt = wilt(a)<br>        Next<br>        Array.Sort(f)<br> <br>        Dim rel(f.Length - 1, f.Length - 1) As Boolean<br>        For a = 0 To f.Length - 1<br>            For b = 0 To f.Length - 1<br>                If a &lt;&gt; b Then<br>                    If f(a).height &lt; f(b).height Then<br>                        rel(a, b) = True<br>                    ElseIf f(a).bloom &gt; f(b).wilt OrElse f(a).wilt &lt; f(b).bloom Then<br>                        rel(a, b) = True<br>                    End If<br>                End If<br>            Next<br>        Next<br> <br>        Dim ret(f.Length - 1) As Integer<br>        Dim hit(f.Length - 1) As Boolean<br> <br>        Dim test As Boolean<br>        For a = 0 To f.Length - 1<br>            For b = 0 To f.Length - 1<br>                If hit(b) = False Then<br>                    test = True<br>                    For c = 0 To f.Length - 1<br>                        If b &lt;&gt; c AndAlso (rel(b, c) = False AndAlso hit(c) = False) Then<br>                            test = False<br>                            Exit For<br>                        End If<br>                    Next<br>                    If test Then<br>                        ret(a) = f(b).height<br>                        hit(b) = True<br>                        Exit For<br>                    End If<br>                End If<br>            Next<br>        Next<br> <br>        Return ret<br>    End Function<br>End Class<br></td>