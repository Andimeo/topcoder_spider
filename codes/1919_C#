            using System;<br>using System.Collections;<br> <br>public class WindowWasher<br>{<br>  public WindowWasher()<br>  {<br>  }<br> <br>  public int fastest(int w, int h, int[] s)<br>  {<br>    ArrayList col=new ArrayList();<br>    foreach(int x in s)<br>      col.Add(x*h);<br>    col.Sort();<br> <br>        int[] time=new int[s.Length];<br>    for(int i=0;i&lt;time.Length;i++) time[i]=0;<br> <br>    int min=0;<br> <br>    while(w--&gt;0)<br>    {<br>            int f=0;<br>      bool free=false;<br>      for(int i=0;i&lt;time.Length;i++)<br>      {<br>        if(time[i]+(int)col[i]&lt;min)<br>        {<br>          time[i]+=(int)col[i];<br>          free=true;<br>          break;<br>        }<br> <br>        if(time[f]+(int)col[f]&gt;time[i]+(int)col[i])<br>          f=i;<br>      }<br> <br>      if(!free)<br>      {<br>        min=(time[f]+=(int)col[f]);<br>      }<br>    }<br> <br>    return min;<br>  }<br>}<br></td>