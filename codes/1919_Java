            import java.util.*;<br>import java.util.regex.*;<br>public class WindowWasher {<br>  public int fastest(int width, int height, int[] washTimes) <br>    {<br>    int max = 0;<br>    int[] used  = new int[washTimes.length]; <br>    int i=0;<br>    for(i=0;i&lt;used.length;i++)<br>      used[i]=0;<br> <br>    for(i = 0; i &lt; width;i++)<br>      {<br>      int fastest = 0;<br>      int atend = 2000000000;<br>      for(int j=0; j&lt; used.length;j++)<br>        {<br>        int myend= used[j] + washTimes[j];<br>        if(myend &lt; atend)<br>          {<br>          fastest = j;<br>          atend = myend;<br>          }<br>        }<br>      used[fastest] = atend;<br>      if(atend &gt; max)<br>        max = atend;<br>      }<br> <br>    <br> <br>    return height * max;<br>    }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>