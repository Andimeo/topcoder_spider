            using System;<br>using System.Collections;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class TerribleEncryption<br>{<br>  public string decrypt(string pool, int[] data, int[] keys)<br>  {<br>    string res = "";<br>    for (int i = 0; i &lt; data.Length; ++i){<br>      int k = 1;<br>      while ((data[i] * k) % keys[i] != 1) ++k;<br>      k %= pool.Length;<br>      res += pool[k];<br>    }<br>    return res;<br>  }<br>}<br>//Powered by [KawigiEdit]<br></td>