            import java.util.*;<br> <br>public class TerribleEncryption<br>{<br>    public String decrypt(String pool, int[] data, int[] keys)<br>    {<br>        String ret = "";<br>        for (int i=0; i &lt; data.length; i++)<br>        {<br>            for (int k = 1; k &lt;= 100000; k++)<br>            {<br>                if ((data[i] * k) % keys[i] == 1)<br>                {<br>                    int j = k % (pool.length());<br>                    ret += pool.charAt(j);<br>                    break;<br>                }<br>            }<br>        }<br>        <br>        return ret;<br>    }<br> <br>}<br></td>