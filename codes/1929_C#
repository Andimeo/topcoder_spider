            using System;<br>using System.Text;<br>using System.Collections;<br><br>public class FullMonitor {<br>  public int numLasers(string[] pixels) {<br>    bool [,] xy = new bool[1024,1024];<br>    bool [,] yz = new bool[1024,1024];<br>    bool [,] xz = new bool[1024,1024];<br>    int l = 0;<br><br><br><br>    foreach(string s in pixels)<br>    {<br>      string [] p = s.Split(",".ToCharArray());<br><br>      int x = int.Parse(p[0]);<br>      int y = int.Parse(p[1]);<br>      int z = int.Parse(p[2]);<br><br><br><br>      if (!xy[x,y]) l++;<br>      if (!xz[x,z]) l++;<br>      if (!yz[y,z]) l++;<br><br>      xy[x,y] = true;<br>      xz[x,z] = true;<br>      yz[y,z] = true;<br><br>      for(int xx = 0; xx&lt;1024; xx++)<br>      if (xx != x)<br>      {<br>        if (xy[xx,y] &amp;&amp; xz[xx,z] &amp;&amp; yz[y,z])<br>        {<br>          string ss = string.Format("{0},{1},{2}",xx,y,z);<br>          if (Array.IndexOf(pixels, ss)&lt;0) <br>            return -1;<br>        }<br>      }<br>      for(int yy = 0; yy&lt;1024; yy++)<br>        if (yy != y)<br>        {<br>          if (xy[x,yy] &amp;&amp; xz[x,z] &amp;&amp; yz[yy,z])<br>          {<br>            string ss = string.Format("{0},{1},{2}",x,yy,z);<br>            if (Array.IndexOf(pixels, ss)&lt;0) <br>              return -1;<br>          }<br>        }<br>      for(int zz = 0; zz&lt;1024; zz++)<br>        if (zz != z)<br>        {<br>          if (xy[x,y] &amp;&amp; xz[x,zz] &amp;&amp; yz[y,zz])<br>          {<br>            string ss = string.Format("{0},{1},{2}",x,y,zz);<br>            if (Array.IndexOf(pixels, ss)&lt;0) <br>              return -1;<br>          }<br>        }<br><br><br>    }<br>    return l;<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester for c# [12-oct-2003]<br>// Powered by CodeProcessor<br></td>