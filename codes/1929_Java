            import java.util.*;<br> <br>public class FullMonitor {<br>public int numLasers(String[] pix) {<br>  int i, j, k, x, y, z;<br>  TreeSet xy = new TreeSet(), xz = new TreeSet(), yz = new TreeSet();<br> <br>  StringTokenizer st;<br>  for (i = 0; i &lt; pix.length; i++) {<br>    st = new StringTokenizer(pix[i], " ,");<br>    x = Integer.parseInt(st.nextToken());<br>    y = Integer.parseInt(st.nextToken());<br>    z = Integer.parseInt(st.nextToken());<br>    xy.add("" + x + " " + y);<br>    xz.add("" + x + " " + z);<br>    yz.add("" + y + " " + z);<br>  }<br>  Iterator A, B, C;<br>  int c = 0;<br>  A = xy.iterator();<br>  while (A.hasNext()) {<br>     String XY = A.next().toString();<br>     st = new StringTokenizer(XY);<br>     x = Integer.parseInt(st.nextToken());<br>     y = Integer.parseInt(st.nextToken());<br>     B = xz.iterator();<br>     while (B.hasNext()) {<br>        String XZ = B.next().toString();<br>        st = new StringTokenizer(XZ);<br>        if (x != Integer.parseInt(st.nextToken())) continue;<br>        z = Integer.parseInt(st.nextToken());<br>        C = yz.iterator();<br>        while (C.hasNext()) {<br>           String YZ = C.next().toString();<br>           st = new StringTokenizer(YZ);<br>           if (y == Integer.parseInt(st.nextToken()) &amp;&amp; z == Integer.parseInt(st.nextToken()))<br>              c++;<br>        }<br>     }<br>  }<br>  if (c &gt; pix.length) return -1;<br>  return xy.size() + xz.size() + yz.size();<br>}}<br></td>