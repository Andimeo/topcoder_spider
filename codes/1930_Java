            import java.util.*;<br>public class LevelUp {<br>  public int toNextLevel(int[] expNeeded, int received) {<br>    for(int i = 0; i &lt; expNeeded.length; i++) {<br>       if (received &lt; expNeeded[i]) return expNeeded[i] - received;<br>     } //end for i  <br>     <br>     return -1;<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>