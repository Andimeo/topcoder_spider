            using System;<br>using System.Text;<br>using System.Collections;<br><br>public class AustrianLotto {<br>  public int[] evaluate(string drawing, string[] picks) {<br><br>    int [] sol = new int[7];<br><br>    string[] d = drawing.Split(" ".ToCharArray());<br><br>    foreach(string x in picks)<br>    {<br>      string[] y = x.Split(" ".ToCharArray());<br><br>      int cnt = 0;<br><br>      foreach(string z in y)<br>      {<br>        if (Array.IndexOf(d, z)&gt;=0)<br>          cnt++;        <br>      }<br><br>      sol[cnt]++;<br>    }<br>    return sol;<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester for c# [12-oct-2003]<br>// Powered by CodeProcessor<br></td>