            import java.util.*;<br>public class AustrianLotto {<br>  public int[] evaluate(String drawing, String[] picks) {<br>    drawing = " " + drawing + " ";<br>    int[] r = new int[7];<br>    for (int i = 0; i &lt; picks.length; i++) {<br>      int count = 0;<br>      String[] ss = picks[i].split(" ");<br>      for (int j = 0; j &lt; ss.length; j++) {<br>        if (drawing.indexOf(" " + ss[j] + " ") &gt;= 0) count++;<br>      }<br>      r[count]++;<br>    }<br>    return r;<br>  <br>  }<br> <br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>