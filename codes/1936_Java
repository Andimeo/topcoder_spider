            import java.util.*;<br>public class CongruenceLock {<br>  public int minPushes(int current, int target, int[] buttons) {<br>    int [] min = new int[100000];<br>    Arrays.fill(min, Integer.MAX_VALUE);<br>    <br>    int [] q = new int[100000];<br>    int get = 0, put = 0;<br>    <br>    min[current] = 0;<br>    q[put++] = current;<br>    <br>    if (current == target) return 0;<br>    <br>    while (get &lt; put) {<br>      current = q[get++];<br>      int m = min[current];<br>      for (int i = 0; i &lt; buttons.length; i++) {<br>        int next = (current + buttons[i]) % 100000;<br>        if (min[next] == Integer.MAX_VALUE) {<br>          min[next] = m + 1;<br>          if (next == target) return min[next];<br>          q[put++] = next;<br>        }<br>      }<br>    }<br>    <br>    return -1;<br>  }<br> <br> <br> <br> <br> <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>