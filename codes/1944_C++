            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include&lt;iostream&gt;<br>#include&lt;algorithm&gt;<br>#include&lt;map&gt;<br>#include&lt;sstream&gt;<br>#include&lt;set&gt;<br>#define VV vector&lt; vector&lt;int&gt; &gt;<br>#define S string<br>using namespace std;<br>class RPG {<br>  public:<br>  vector &lt;int&gt; dieRolls(vector &lt;string&gt; dice) {<br>    int maxi=0,mini=0;<br>    for(int i=0;i&lt;dice.size();i++)<br>    {<br>      stringstream s(dice[i]);<br>      int a,b;<br>      char x;<br>      s&gt;&gt;a&gt;&gt;x&gt;&gt;b;<br>      maxi+=a*b;<br>      mini+=a;<br>    }<br>    vector&lt;int&gt; result(3);<br>    result[0]=mini;<br>    result[1]=maxi;<br>    result[2]=(mini+maxi)/2;<br>    return result;<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>