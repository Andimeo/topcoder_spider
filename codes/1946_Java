            public class AlgoHill {<br>public int astar(String[] hill, int sy, int sx, int gy, int gx) {<br>   int X = hill.length;<br>   int Y = hill[0].length();<br>   int h[][] = new int[hill.length][hill[0].length()];<br>   int c[][] = new int[hill.length][hill[0].length()];<br>   int i, j, x, y;<br>   for (i = 0; i &lt; X; i++)<br>   for (j = 0; j &lt; Y; j++) {<br>     h[i][j] = hill[i].charAt(j);<br>     c[i][j] = 99999;<br>   }<br>   c[sx][sy] = 0;<br>   for (i = 0; i &lt; X*Y; i++)<br>   for (x = 0; x &lt; X; x++)<br>   for (y = 0; y &lt; Y; y++) {<br>     if (x &gt; 0) {<br>        if (h[x-1][y] &gt; h[x][y]) j = 3; else<br>        if (h[x-1][y] &lt; h[x][y]) j = 1; else<br>        j = 2;<br>        c[x-1][y] = Math.min(c[x-1][y], c[x][y] + j);<br>     }<br>     if (x &lt; X-1) {<br>        if (h[x+1][y] &gt; h[x][y]) j = 3; else<br>        if (h[x+1][y] &lt; h[x][y]) j = 1; else<br>        j = 2;<br>        c[x+1][y] = Math.min(c[x+1][y], c[x][y] + j);<br>     }<br>     if (y &gt; 0) {<br>        if (h[x][y-1] &gt; h[x][y]) j = 3; else<br>        if (h[x][y-1] &lt; h[x][y]) j = 1; else<br>        j = 2;<br>        c[x][y-1] = Math.min(c[x][y-1], c[x][y] + j);<br>     }<br>     if (y &lt; Y-1) {<br>        if (h[x][y+1] &gt; h[x][y]) j = 3; else<br>        if (h[x][y+1] &lt; h[x][y]) j = 1; else<br>        j = 2;<br>        c[x][y+1] = Math.min(c[x][y+1], c[x][y] + j);<br>     }<br>  }<br>  j = 0;<br>  for (x = 0; x &lt; X; x++) {<br>  for (y = 0; y &lt; Y; y++) {<br>    System.out.print(" " + c[x][y]);<br>    if (c[x][y] + Math.abs(x-gx) + Math.abs(y-gy) &lt;= c[gx][gy]) j++;<br>  }<br>  System.out.println();<br>  }<br>  return j;<br>}}<br></td>