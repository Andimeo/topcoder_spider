            using System;<br>using System.Collections;<br>public class CrossCountry<br>{<br>  class team : IComparable<br>  {<br>    public ArrayList scores;<br>    public char name;<br>    public int sixth;<br>    public int score;<br>    public team(char c)<br>    {<br>      name = c;<br>      scores = new ArrayList();<br>      sixth = 999999;<br>    }<br>    public int CompareTo(object o)<br>    {<br>      team rhs = o as team;<br>      if(score &lt; rhs.score) return -1;<br>      if(score &gt; rhs.score) return 1;<br>      if(sixth &lt; rhs.sixth) return -1;<br>      if(sixth &gt; rhs.sixth) return 1;<br>      if(name &lt; rhs.name) return -1;<br>      if(name &gt; rhs.name) return 1;<br>      return 0;<br>    }<br>    public void tally()<br>    {<br>      scores.Sort();<br>      if(scores.Count &lt; 5) { score = 999999; return; }<br>      score = 0;<br>      for(int i=0;i&lt;5;i++)<br>        score += (int)scores[i];<br>      if(scores.Count &gt;=6) sixth = (int)scores[5];<br>    }<br>  }<br>  public string scoreMeet(int numTeams,string finish)<br>  {<br>    team []scores = new team[numTeams];<br>    for(int i=0;i&lt;scores.Length;i++) scores[i] = new team((char)('A' + i));<br>    for(int i=0;i&lt;finish.Length;i++)<br>    {<br>      scores[finish[i]-'A'].scores.Add(i+1);<br>    }<br>    foreach(team t in scores)<br>    {<br>      t.tally();<br>    }<br>    Array.Sort(scores);<br>    string result="";<br>    foreach(team t in scores)<br>    {<br>      if(t.scores.Count &gt; 4)<br>      {<br>        result = result + t.name;<br>      }<br>    }<br>    return result;<br>  }<br> <br>}<br></td>