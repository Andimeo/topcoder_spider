            import java.util.*;<br>public class RockSkipping {<br>  public double probability(String pads, int maxDist) {<br>    pad=new boolean[pads.length()];<br>    for(int i=0; i&lt;pad.length; i++){<br>      pad[i]=(pads.charAt(i)=='X');<br>    }<br>    cache=new double[pad.length][maxDist+8];<br>    for(int i=0; i&lt;cache.length; i++) Arrays.fill(cache[i],-1);<br>    return expected(0,maxDist)*100;<br>  }<br>  boolean[] pad;<br>  double[][] cache;<br>  public double expected(int howfar, int max){<br>    if(pad[howfar%pad.length]) return 0;<br>    if(max==0) return 1;<br>    if(cache[howfar][max]!=-1) return cache[howfar][max];<br>    double prob=0;<br>    for(int i=1; i&lt;=max; i++){<br>      prob+=expected((howfar+i)%pad.length,max-1)/(double)(max);<br>    }<br>    return cache[howfar][max]=prob;<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>