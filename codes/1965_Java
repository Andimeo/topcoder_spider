            import java.util.*;<br>public class ElevatorLimit {<br>  public int[] getRange(int[] a, int[] b, int p) {<br>    int []ret=new int[2];<br>    <br>    int min=1000001;<br>    int max=-1;<br>    for(int x=0;x&lt;1000000; x++){<br>      if(test(x, a, b, p)) {<br>        min=Math.min(min, x);<br>        max=Math.max(max, x);<br>      }<br>    }<br>    ret[0]=min; ret[1]=max;<br>    if(min==1000001 || max==-1) return new int[0];<br>    return ret;<br>  }<br>  <br>  boolean test(int s, int []a, int []b, int p){<br>    if(s&gt;p) return false;<br>    for(int x=0; x&lt;a.length; x++){<br>      s-=b[x];<br>      if(s&lt;0) return false;<br>      s+=a[x];<br>      if(s&gt;p) return false;<br>    }<br>    return true;<br>  }<br> <br> <br>  /** begin cut - don't modify this line*/<br>  public static void main(String[] a) {<br>    new ElevatorLimit().runTestCase(0);<br>    new ElevatorLimit().runTestCase(1);<br>    new ElevatorLimit().runTestCase(2);<br>    new ElevatorLimit().runTestCase(3);<br>    new ElevatorLimit().runTestCase(4);<br>    new ElevatorLimit().runTestCase(5);<br>    new ElevatorLimit().runTestCase(6);<br>  }<br> <br>  public void runTestCase(int nbr) {<br>    switch(nbr) {<br>      case 0 : {<br>        checkOutput(getRange(new int[] {1,0}, new int[] {0,1}, 1), new int[] { 0,  0 }, 0); break;<br>      }<br>      case 1 : {<br>        checkOutput(getRange(new int[] {1,0}, new int[] {0,1}, 2), new int[] { 0,  1 }, 1); break;<br>      }<br>      case 2 : {<br>        checkOutput(getRange(new int[] {0,1}, new int[] {1,0}, 1), new int[] { 1,  1 }, 2); break;<br>      }<br>      case 3 : {<br>        checkOutput(getRange(new int[] {0,2}, new int[] {1,0}, 1), new int[] { }, 3); break;<br>      }<br>      case 4 : {<br>        checkOutput(getRange(new int[] {6, 85, 106, 1, 199, 76, 162, 141} , new int[] {38, 68, 62, 83, 170, 12, 61, 114} , 668), new int[] { 223,  500 }, 4); break;<br>      }<br>      case 5 : {<br>        checkOutput(getRange(new int[] {179