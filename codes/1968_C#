            using System; // my code is unbreakable!<br> <br>public class SkipRope<br>{<br>  public int[] partners(int[] candidates, int height)<br>  {<br>    int closest1 = 0, closest2 = 0;<br>    int curr, diff1=999, diff2=999;<br>    int max = candidates.Length;<br>    Array.Sort(candidates);<br>    for(int i=0;i&lt;max;i++)<br>    {<br>      curr = Math.Abs(height-candidates[i]);<br>      if(curr&lt;=diff1) <br>      { <br>        diff2 = diff1; diff1 = curr; closest2 = closest1; closest1 = candidates[i];<br>      }<br>      else<br>      {<br>        if(curr&lt;=diff2) { diff2 = curr; closest2 = candidates[i];}<br>      }<br>    }<br>    if(closest1&gt;closest2){int temp = closest1; closest1 = closest2; closest2 = temp;}<br>    return new int[]{closest1,closest2};<br>  }<br>}<br>// Powered by greed<br></td>