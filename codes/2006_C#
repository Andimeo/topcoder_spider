            using System;<br>using System.Collections;<br>public class Computers {<br>  long[,] am;<br><br>  public long choices(int n, int minDif, int minInComp, int amount) {<br>    n -= minInComp * amount;<br>    if (n &lt; 0)<br>      return 0;<br><br>    am = new long[n + 1, amount + 1];<br><br>    for (int i = 1; i &lt;= n; ++i) <br>    {<br>      am[i, 0] = 0;<br>    }<br>    am[0, 0] = 1;<br><br>    for (int i = 0; i &lt;= n; ++i)<br>    {<br>      long sum = am[i, 0];<br>      for (int k = 1; k &lt;= amount; ++k)<br>      {<br>        am[i, k] = sum;<br>        if (i &gt;= k)<br>          am[i, k] += am[i - k, k];<br>        if (i &gt;= k * minDif)<br>          sum += am[i - k * minDif, k];<br>      }<br>    }<br><br>    return am[n, amount];<br>  }<br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>