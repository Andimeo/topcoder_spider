            #define FR(i,a,b) for (int i = (a); i &lt; (b); i++)<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;stdio.h&gt;<br>#include &lt;stdlib.h&gt;<br>#include &lt;string.h&gt;<br>#include &lt;math.h&gt;<br>#include &lt;ctype.h&gt;<br>#include &lt;setjmp.h&gt;<br>#include &lt;regex.h&gt;<br>#include &lt;utility&gt;<br>#define ll long long<br>#define ld long double<br>#define EPS 1e-9<br>using namespace std;<br> <br>struct Computers {<br>ll rv[1024][1024];<br>ll doit(int n, int md, int mic, int am) {<br> if (mic) n -= mic * am;<br> if (n &lt; 0) return 0;<br> if (!n) return 1;<br> if (rv[n][am]+1) return rv[n][am];<br> ll ans = doit(n, md, 1, am);<br> FR(i,1,am)<br>  ans += doit(n, md, md, am-i);<br> return rv[n][am] = ans;<br>}<br>long long choices(int n, int md, int mic, int am) {<br> memset(rv,-1,sizeof(rv));<br> return doit(n,md,mic,am);<br>}};<br>// Powered by ted<br></td>