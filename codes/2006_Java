            import java.util.*;<br>import java.io.*;<br>import java.text.*;<br>import java.math.*;<br>import static java.lang.Math.*;<br>import static java.util.Arrays.*;<br>public class Computers {<br>  int dif;<br>  public long choices(int n, int minDif, int minInComp, int amount) {<br>    dif = minDif;<br>    n -= amount*minInComp;<br>    if(n &lt; 0) return 0;<br>    memo = new long[n+1][amount+1];<br>    for(int i = 0; i &lt; memo.length; i++){<br>      fill(memo[i],-1);<br>    }<br>    return doit(n,amount);<br>  }<br>  long[][] memo;<br>  long doit(int n, int left){<br>    if(n == 0 &amp;&amp; left == 0) return 1;<br>    if(left == 0) return 0;<br>    if(n &lt; 0) return 0;<br>    if(left==1){<br>      return 1;<br>    }<br>    if(memo[n][left] != -1) return memo[n][left];<br>    long ret = doit(n-left, left);<br>    for(int i = 1; i &lt;= left; i++){<br>      ret += doit(n-((left-i)*dif), left-i);<br>    }<br>    memo[n][left] = ret;<br>  //  System.out.println(n+" "+left+" "+ret);<br>    return ret;<br>  }<br> <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>