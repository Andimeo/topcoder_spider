            #include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;map&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cmath&gt;<br>//#include &lt;kitchen_sink&gt;<br>using namespace std;<br> <br>#define fo(i,n) for((i)=0;(i)&lt;(n);(i)++)<br>#define fi fo(i,n)<br>#define fj fo(j,n)<br>#define fk fo(k,n)<br>#define fl fo(l,n)<br>#define fir(n) fo(i,n)<br>#define fjr(n) fo(j,n)<br>#define fkr(n) fo(k,n)<br>#define flr(n) fo(l,n)<br>#define vs vector&lt;string&gt;<br>#define vi vector&lt;int&gt;<br>#define pb(x) push_back(x)<br>#define cs c_str()<br>#define sz size()<br>#define ins(x) insert(x)<br>#define df double<br>#define dub (double)<br>#define ll long long<br> <br> <br> <br>ll f[2][4][4][101][101];<br> <br>ll ans;<br> <br>void ack(int x, int xx, ll a) {<br>//  printf("%d %d %lld\n",x,xx,a);<br>  ans+=a*(xx-x);<br>}<br> <br>class ConvexPoly { public: long long count(int xx, int y) {<br>int i,j,k,l,i2,j2,k2,l2,x,z;<br>for(x=xx;x&lt;=xx;x++) {<br>  fo(z,2) fir(4) fjr(4) fkr(101) flr(101) f[z][i][j][k][l]=0;<br>  fir(y) for(j=i+1;j&lt;y;j++) {<br>    fkr(3) flr(3) f[0][k][l][i][j]=1;<br>  }<br>  fir(y) { j=i;<br>    fkr(3) flr(3) if (k&lt;l) f[0][k][l][i][j]=1;<br>  }<br>  for(z=1;z&lt;x;z++) {<br>if (z&gt;1) {<br>  fir(4) fjr(4) fkr(101) flr(101) f[0][i][j][k][l]=f[1][i][j][k][l];<br>  fir(4) fjr(4) fkr(101) flr(101) f[1][i][j][k][l]=0;<br>}<br>    fir(y) for(j=i;j&lt;y;j++) {<br>      fkr(3) flr(3) {<br>        if (k==0) i2=i-1;<br>        if (k==1) i2=i;<br>        if (k==2) i2=i+1;<br>        if (l==0) j2=j-1;<br>        if (l==1) j2=j;<br>        if (l==2) j2=j+1;<br>        if (i2&lt;0 || i2&gt;=y || j2&lt;0 || j2&gt;=y) continue;<br>        if (i2&gt;j2) continue;<br>        ack(z,xx,f[0][k][l][i][j]);<br>//printf("%d %d %d %d %d %d\n",k,l,i,j,i2,j2);<br>        for(k2=k;k2&lt;3;k2++) for(l2=l;l2&gt;=0;l2--) f[1][k2][l2][i2][j2]+=f[0][k][l][i][j];<br>      }<br>    }<br>  }<br>}<br>return ans;<br>}};<br></td>