            import java.util.*;<br>public class Harmony {<br>  public int[] mostHarmonious(int[] f) {<br>    Arrays.sort(f);<br>    int[] best = null;<br>    int[] bds = null;<br>    <br>    for (int i = 0; i &lt; f.length; i++) {<br>      for (int j = i + 1; j &lt; f.length; j++) {<br>        for (int k = j + 1; k &lt; f.length; k++) {<br>          int[] c = new int[3];<br>          c[0] = f[i]; c[1] = f[j]; c[2] = f[k];<br>          int[] ds = new int[3];<br>          ds[0] = getD(c[0], c[1]);<br>          ds[1] = getD(c[0], c[2]);<br>          ds[2] = getD(c[1], c[2]);<br>          Arrays.sort(ds);<br>          //System.out.println("c" +c[0] + "," +c[1]+"," + c[2]);<br>          //System.out.println("d" +ds[0] + ","+ ds[1]+"," + ds[2]);<br>          <br>          if (best == null) {    <br>            //System.out.println("Better");<br>          best = c;<br>          bds = ds;<br>          continue;<br>        }<br>        <br>          <br>          if (ds[2] &gt; bds[2]) continue;<br>          if (ds[2] == bds[2]) {<br>            if (ds[1] &gt; bds[1]) continue;<br>            if (ds[1] == bds[1]) {<br>              if (ds[0] &gt; bds[0]) continue;<br>              if (ds[0] == bds[0]) {<br>                if (c[0] &gt; best[0]) continue;<br>                if (c[0] == best[0]) {<br>                  if (c[1] &gt; best[1]) continue;<br>                  if (c[1] == best[1]) {<br>                    if (c[2] &gt; best[2]) continue;<br>                  }<br>                }<br>              }<br>            }<br>          }<br>            //System.out.println("Better");<br>          best = c;<br>          bds = ds;<br>        }  <br>      }  <br>    }<br>    return best;<br>    <br>  }<br>  <br>  public int getD(int s, int b) {<br>    //System.out.println("GCD" + gcd(200, 250));<br>    return s / gcd(s, b);  <br>  }<br>  <br>  public int gcd(int a, int b) {<br>    if (b == 0) return Math.abs(a);<br> <br>    return gcd(b, a % b);<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>