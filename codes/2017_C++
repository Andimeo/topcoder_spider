            class EngineersPrimes{<br>public:<br> <br>long long smallestNonPrime(int N)<br>{<br>  long long * ptr = new long long [N+2];<br>  if (N==1)<br>    return 9;<br>  ptr[1]=2;<br>  int num=1;<br>  for(int k=3;;k++)<br>  {<br>    bool prime =true;<br>    for(int i=1;ptr[i]*ptr[i]&lt;=k; i++)<br>    {<br>      if(k%ptr[i]==0)<br>      {<br>      prime=false;<br>      break;<br>      }  <br>      <br>    }<br>    if(prime)<br>    ptr[++num]=k;<br>    if(num==N+1)<br>    {<br>      long long ret= (long long)ptr[N+1]*(long long)ptr[N+1];<br>      return ret;<br>    }<br> <br>  }<br>  <br> <br>}<br>};<br></td>