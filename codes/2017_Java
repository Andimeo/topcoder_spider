            import java.util.*;<br>public class EngineersPrimes {<br>  long findPrime(int num)<br>  {<br>    for (int n=0; n&lt;num; n++)<br>    {<br>    long t = p[n];<br>    boolean notprime = true;<br>    while (notprime)<br>    {<br>      t++;<br>      notprime = false;<br>      long s = 1L + (long)Math.sqrt(t);<br>      for (int i=0; (i&lt;=n) &amp;&amp; (!notprime) &amp;&amp; (p[i] &lt; s); i++)<br>        if (t%p[i] == 0)<br>          notprime = true;<br>    }<br>    p[n+1] = t;<br>    }<br>    return p[num];<br>  }<br>  long p[];<br>  public long smallestNonPrime(int N) {<br>    p = new long[N+2];<br>    p[0] = 2;<br>    long prime = findPrime(N);<br>    return (prime*prime);<br>  }<br>  <br>}<br><br><br>// Powered by FileEdit<br></td>