            import java.util.*;<br>public class ArgParser {<br>String[] invalid = new String[]{ "INVALID" };<br>public String[] parse(String ss) {<br>  if (!ss.startsWith("{")) return new String[]{ "INVALID" };<br>  if (!ss.endsWith("}") || ss.endsWith("\\}")) return new String[]{ "INVALID" };<br>  ArrayList rr = new ArrayList();<br>  String w = "";<br>  for (int p = 1; p &lt; ss.length() - 1; ++p) {<br>    char c = ss.charAt(p);<br>    if (c == ',') { if (ss.charAt(p+1) != ' ') return invalid;<br>      ++p;<br>      rr.add(w);<br>      w = "";<br>      continue;<br>    }<br>    if (c == '{' || c == '}') return invalid;<br>    if (c == '\\' &amp;&amp; ",{}".indexOf(ss.charAt(p+1)) &gt;= 0) {<br>      ++p;<br>    }<br>    w += ss.charAt(p);<br>  }<br>  rr.add(w);<br>  String[] r = new String[rr.size()];<br>  for (int i = 0; i &lt; rr.size(); ++i) r[i] = ""+rr.get(i);<br>  return r;<br>}<br>  <br>}<br></td>