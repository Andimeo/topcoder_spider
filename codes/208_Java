            import java.util.*;<br>public class NTicTacToe {<br>  int[][][] data = new int[30][30][30];<br>  int n;<br> <br>  public String compute(String[] xx, String[] oo, int nn) {<br>    n = nn;<br>    for (int i = 0; i &lt; xx.length; ++i) {<br>      int[] a = parse(xx[i]);<br>      data[a[0]][a[1]][a[2]] = 1;<br>      if (winner() == 1) return "X," + (i+1);<br>      if (i &gt;= oo.length) continue;<br>      a = parse(oo[i]);<br>      data[a[0]][a[1]][a[2]] = 2;<br>      if (winner() == 2) return "O," + (i+1);<br>    }<br>    return "NO WINNER";<br>  }<br> <br>  int[] parse(String s) {<br>    StringTokenizer tok = new StringTokenizer(s,",");<br>    int x = Integer.parseInt(tok.nextToken());<br>    int y = Integer.parseInt(tok.nextToken());<br>    int z = Integer.parseInt(tok.nextToken());<br>    return new int[]{ x, y, z};<br>  }<br> <br> <br>  int winner() {<br>    for (int x = 0; x &lt; n; ++x) for (int y = 0; y &lt; n; ++y) for (int z = 0; z &lt; n; ++z) if (check(x, y, z)) return data[x][y][z];<br>    return 0;<br>  }<br> <br>  boolean check(int x, int y, int z) {<br>    for (int dx = -1; dx &lt;= 1; ++dx) {<br>      for (int dy = -1; dy &lt;= 1; ++dy) {<br>        for (int dz = -1; dz &lt;= 1; ++dz) {<br>       if ((dx != 0 || dy != 0 || dz != 0) &amp;&amp; check(x, y, z, dx, dy, dz)) return true;<br>    }}}<br>    return false;<br>  }<br> <br>  boolean check(int x, int y, int z, int dx, int dy, int dz) {<br>    int a = get(x, y, z);<br>    if (a == 0) return false;<br>    for (int i = 1; i &lt; n; ++i) if (get(x + i*dx, y + i*dy, z + i*dz) != a) return false;<br>    return true;<br>  }<br> <br>  int get(int x, int y, int z) {<br>    if (x &lt; 0 || x &gt;= n || y &lt; 0 || y &gt;= n || z &lt; 0 || z &gt;= n) return 0;<br>    return data[x][y][z];<br>  }<br>}<br></td>