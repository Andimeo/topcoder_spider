            import java.util.*;<br> <br>public class MatchMaker implements Comparator {<br> <br>String gender;<br>String[] u;<br>int cu=0;<br> <br>public String[] getBestMatches(String[] users, String currentUser, int sf) {<br>  u = users;<br>  for (int i=0; i&lt;users.length; i++) {<br>    StringTokenizer st = new StringTokenizer(users[i]);<br>    if (st.nextToken().equals(currentUser)) {<br>      cu = i;<br>      st.nextToken();<br>      gender= st.nextToken();<br>    }<br>  }<br> <br>  ArrayList res = new ArrayList();<br>  for (int i=0; i&lt;users.length; i++) if (i!=cu) {<br>    int s = sim(users[i],users[cu]);<br>    if (s==-1) continue;<br>    System.out.println(users[i] + " " + s);<br>    if (s&gt;=sf)<br>      res.add(users[i]);<br>  }<br> <br>  Collections.sort(res, this);<br> <br>  String[] r = new String[res.size()];<br>  for (int i=0; i&lt;res.size(); i++) {<br>    r[i]=(new StringTokenizer(res.get(i).toString())).nextToken();<br>  }<br> <br>  return r;<br>}<br> <br>public int compare(Object a, Object b) {<br>  return sim(b.toString(), u[cu])-sim(a.toString(), u[cu]);<br>}<br> <br>int sim(String a, String b) {<br>  StringTokenizer sa = new StringTokenizer(a);<br>  StringTokenizer sb = new StringTokenizer(b);<br> <br>  sa.nextToken();<br>  sb.nextToken();<br>  if (!sa.nextToken().equals(gender)) return -1;<br>  sb.nextToken();<br>  sa.nextToken();<br>  sb.nextToken();<br> <br>  int s = 0;<br>  while (sa.hasMoreTokens()) {<br>    if (sa.nextToken().equals(sb.nextToken())) s++;<br>  }<br>  return s;<br>}<br> <br> <br>}<br></td>