            import java.util.*;<br>public class ProbabilityTree {<br>    String t[];<br>    double prob[]=new double[1000];<br>  public int[] getOdds(String[] tree, int lowerBound, int upperBound) {<br>        t=tree;<br>        for (int i=0;i&lt;1000;i++) prob[i]=-1;<br>        prob[0]=Double.parseDouble(tree[0])/100;<br>        for (int i=1;i&lt;tree.length;i++)<br>            Solve(i);<br>        int arr[]=new int[1000];<br>        int count=0;<br>  for (int i=0;i&lt;tree.length;i++) {<br>//    System.out.println(i+" "+prob[i]);<br>            if ((prob[i]*100&gt;=lowerBound) &amp;&amp; (prob[i]*100&lt;=upperBound)) {<br>                arr[count]=i;<br>                count++;<br>    }<br>}<br>        int ret[]=new int[count];<br>        for (int i=0;i&lt;count;i++)<br>            ret[i]=arr[i];<br>        return ret;<br>  }<br> <br>    private void Solve(int n) {<br>        String[] sp=t[n].split(" ");<br>        int root=Integer.parseInt(sp[0]);<br>        double lp=Double.parseDouble(sp[1])/100;<br>        double rp=Double.parseDouble(sp[2])/100;<br>        if (prob[root]&lt;0)<br>            Solve(root);<br>        prob[n]=prob[root]*lp+(1-prob[root])*rp;<br>    }<br>}<br> <br>// Powered by PopsEdit<br></td>