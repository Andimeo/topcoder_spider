            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class Books {<br>    public int sortMoves(string[] bb) {<br>        int[] len = new int[bb.Length];<br>        for ( int i = 0 ; i != bb.Length; i++ ) {<br>            len[i] = 1;<br>            for ( int j = i-1; j &gt;= 0; j-- ) {<br>                if ( ( string.CompareOrdinal( bb[j], bb[i]) &lt;= 0 ) &amp;&amp; (len[i] &lt; len[j]+1)) {<br>                    len[i] = len[j]+1;<br>                }<br>            }<br>        }<br>        int max = 0;<br>        for ( int i = 0 ; i != len.Length; i++)<br>            max = Math.Max( max, len[i] );<br>        return bb.Length - max;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>