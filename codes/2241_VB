<td class="problemText" colspan="8" valign="middle" align="left">
            Imports Microsoft.VisualBasic<br>Public Class ClockWalk<br> <br>    Public Function finalPosition(ByVal flips As String) As Integer<br>        Dim pos As Integer = 12<br>        Dim i As Integer = 0<br>        Dim flip As Char<br> <br>        For i = 0 To (flips.Length-1)<br>            flip = flips.Chars(i)<br>            If flip ="h" Then<br>                pos = moveone(pos, 1 * (i + 1))<br>            Else<br>                pos = moveone(pos, -1 * (i + 1))<br>            End If<br>        Next<br>        Return pos<br>    End Function<br> <br>    Function Moveone(ByVal pos As Integer, ByVal units As Integer) As Integer<br>        Dim rv As Integer<br>        rv = pos + units<br>        While rv &gt; 12<br>            rv -= 12<br>        End While<br>        While rv &lt;= 0<br>            rv += 12<br>        End While<br>        Return rv<br>    End Function<br>End Class<br></td>