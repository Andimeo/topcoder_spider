            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class InstantRunoff {<br>    public string outcome(string cc, string[] bb) {<br>        if ( cc.Length == 0 ) return "";<br>        string res="";<br>        int[] cnt = new int[256];<br>        for ( int i = 0 ; i != bb.Length; i++ ) {<br>            cnt[bb[i][0]]++;<br>        }<br>        int maxi = 0, mini = 0;<br>        for ( int i = 1; i != cc.Length; i++ ) {<br>            if ( cnt[cc[i]] &lt; cnt[cc[mini]] ) mini=i;<br>            if ( cnt[cc[i]] &gt; cnt[cc[maxi]] ) maxi=i;<br>        }<br>        if ( cnt[cc[maxi]] * 2 &gt; bb.Length ) {<br>            return ""+cc[maxi];<br>        } else {<br>            string nc = "";<br>            string[] nb = new string[bb.Length];<br>            for ( int i = 0 ; i != cc.Length ; i++ ) {<br>                if ( cnt[cc[mini]] != cnt[cc[i]] )<br>                    nc += cc[i];<br>            }<br>            for ( int i = 0 ; i != bb.Length; i++ ) {<br>                nb[i] = "";<br>                for ( int j = 0 ; j != bb[i].Length ; j++ ) {<br>                    if ( nc.IndexOf(bb[i][j]) != -1 )<br>                        nb[i] += bb[i][j];<br>                }<br>            }<br>            return outcome(nc,nb);<br>        }<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>