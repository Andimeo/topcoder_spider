            import java.util.*;<br>import java.math.*;import java.awt.*;import java.awt.geom.*;<br>public class InstantRunoff {<br>  public String outcome(String candidates, String[] ballots) {<br>    while(candidates.length()&gt;0) {<br>      int[] vt = new int[candidates.length()];<br>      int ct=0;<br>      for(int x=0;x&lt;ballots.length;x++) {<br>        for(int y=0;y&lt;ballots[x].length();y++) {<br>          String ff = ballots[x].charAt(y)+"";<br>          if(candidates.indexOf(ff)&gt;=0) {<br>            vt[candidates.indexOf(ff)]++;<br>            ct++;<br>            break;<br>          }  <br>        }<br>      }<br>      <br>      int max=0;<br>      int min = Integer.MAX_VALUE;<br>      for(int x=0;x&lt;vt.length;x++) {<br>        max = Math.max(max, vt[x]);<br>        min = Math.min(min, vt[x]);<br>      }<br>      <br>      String rc="";<br>      for(int x=0;x&lt;vt.length;x++) {<br>        if(vt[x]==max &amp;&amp; max&gt;ct/2) rc+=candidates.charAt(x)+"";<br>      }<br>      if(rc.length()==1) return rc;<br>      <br>      String tmp="";<br>      for(int x=0;x&lt;vt.length;x++) {<br>        if(vt[x]!=min) tmp+=candidates.charAt(x)+"";<br>      }<br>      candidates=tmp;<br>    }<br>    return "";<br>  }<br>  <br>/** begin cut - don't modify this line*/<br>  public static void main(String[] a) {<br>    new InstantRunoff().runTestCase(0);<br>    new InstantRunoff().runTestCase(1);<br>    new InstantRunoff().runTestCase(2);<br>    new InstantRunoff().runTestCase(3);<br>    new InstantRunoff().runTestCase(4);<br>  }<br> <br>  public void runTestCase(int nbr) {<br>    switch(nbr) {<br>      case 0 : {<br>        checkOutput(outcome("ABC", new String[] {"ACB", "BCA", "ACB", "BCA", "CBA"}), "B", 0); break;<br>      }<br>      case 1 : {<br>        checkOutput(outcome("DCBA", new String[] {"ACBD", "ACBD", "ACBD", "BCAD", "BCAD", "DBCA", "CBDA"}), "B", 1); break;<br>      }<br>      case 2 : {<br>        checkOutput(outcome("ACB", new String[] {"ACB", "BCA"