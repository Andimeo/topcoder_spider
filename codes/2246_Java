            import java.util.*;<br> <br>public final class SelfCatalogue {<br> <br>  private static int max[] = {<br>    3,<br>    12,<br>    7,<br>    5,<br>    4,<br>    3,<br>    3,<br>    3,<br>    3,<br>    3,<br>  };<br>  <br>  private static int _t[] = new int[10];<br>  <br>  private static boolean check(int a[]) {<br>    int t[] = _t;<br>    for(int i = 0; i &lt; 10; i++)<br>      t[i] = 0;<br>    for(int i = 0; i &lt; 10; i++) {<br>      int b = a[i];<br>      if(b &gt; 0) {<br>        t[i]++;<br>        if(b &lt; 10)<br>          t[b]++;<br>        else {<br>          t[b % 10]++;<br>          t[b / 10]++;<br>        }<br>      }<br>    }<br>    for(int i = 0; i &lt; 10; i++)<br>      if(a[i] != t[i])<br>        return false;<br>    return true;<br>  }<br>  <br>  public int[] construct(int[] counts) {<br>    int a[] = new int[10];<br>    for(int i = 0; i &lt; 10; i++) {<br>      if(counts[i] &gt; max[i])<br>        return new int[0];<br>      if(counts[i] &gt; 0)<br>        a[i] = counts[i];<br>    }<br>    for(;;) {<br>      if(check(a))<br>        return a;<br>      int p = 9;<br>      for(;;) {<br>        if(counts[p] &lt; 0) {<br>          if(a[p] &lt; max[p]) {<br>            a[p]++;<br>            break;<br>          }<br>          a[p] = 0;<br>        }<br>        if(--p &lt; 0)<br>          return new int[0];<br>      }<br>    }<br>  }<br> <br>}<br></td>