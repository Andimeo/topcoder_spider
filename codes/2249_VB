            Imports Microsoft.VisualBasic<br>Imports System.Math<br>Imports System<br>Imports System.Collections<br>Imports System.Text<br> <br>Public Class Matching<br>    Public Function findMatch(ByVal first As String(), ByVal second As String()) As String()<br>        Dim ret(3) As String<br>        ret(0) = doit(New String() {"CIRCLE", "SQUIGGLE", "DIAMOND"}, first(0), second(0))<br>        ret(1) = doit(New String() {"RED", "BLUE", "GREEN"}, first(1), second(1))<br>        ret(2) = doit(New String() {"SOLID", "STRIPED", "EMPTY"}, first(2), second(2))<br>        ret(3) = doit(New String() {"ONE", "TWO", "THREE"}, first(3), second(3))<br>        Return ret<br>    End Function<br> <br>    Private Function doit(ByVal poss() As String, ByVal f As String, ByVal s As String) As String<br>        Dim c(2) As Integer<br>        Dim ret(3) As String<br>        Dim a As Integer<br>        For a = 0 To 2<br>            If f = poss(a) Then c(a) += 1<br>            If s = poss(a) Then c(a) += 1<br>        Next<br>        If c(0) = 2 OrElse c(1) = 2 OrElse c(2) = 2 Then<br>            If c(0) = 2 Then<br>                Return poss(0)<br>            ElseIf c(1) = 2 Then<br>                Return poss(1)<br>            ElseIf c(2) = 2 Then<br>                Return poss(2)<br>            End If<br>        End If<br>        If c(0) = 0 Then<br>            Return poss(0)<br>        ElseIf c(1) = 0 Then<br>            Return poss(1)<br>        ElseIf c(2) = 0 Then<br>            Return poss(2)<br>        End If<br>    End Function<br> <br>End Class<br></td>