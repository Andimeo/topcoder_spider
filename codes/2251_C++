            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;iostream&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;cmath&gt;<br> <br>using namespace std;<br> <br>class RGBColor<br>{<br>public:<br>  vector &lt;int&gt; getComplement(vector &lt;int&gt; rgb)<br>  {<br>    vector&lt;int&gt; ans(rgb.size());<br>    int i;<br>    for (i=0;i&lt;rgb.size();i++)<br>    {<br>      ans[i] = 255-rgb[i];<br>    }<br> <br>    bool good = true;<br>    for (i=0;i&lt;rgb.size();i++)<br>    {<br>      if (!(mabs(rgb[i]-ans[i]) &lt;=32))<br>      {<br>        good = false;<br>      }<br>    }<br> <br>    if (!good)<br>      return ans;<br>    else<br>    {<br>      for (i=0;i&lt;ans.size();i++)<br>      {<br>        if (rgb[i]+128&lt;=255)<br>          ans[i]=rgb[i]+128;<br>        else<br>          ans[i]=rgb[i]-128;<br>      }<br>      return ans;<br>    }<br>    <br>  }<br> <br>  int mabs(int x)<br>  {<br>    if (x&lt;0)<br>      return -x;<br>    return x;<br>  }<br> <br> <br> <br>};<br></td>