            import java.util.*;<br>public class RGBColor {<br>  public int[] getComplement(int[] rgb) {<br>    int[] compl=new int[]{255-rgb[0],255-rgb[1],255-rgb[2]};<br>    <br>    for(int i=0;i&lt;3;i++)<br>      if(Math.abs(rgb[i]-compl[i])&gt;32)<br>        return compl;<br>    <br>    for(int i=0;i&lt;3;i++)<br>    {<br>      if((rgb[i]+128)&lt;=255)<br>        compl[i]=rgb[i]+128;<br>      else<br>        compl[i]=rgb[i]-128;  <br>    }<br>    <br>    return compl;<br>  }<br>  public static void main(String[] args) {<br>    <br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>