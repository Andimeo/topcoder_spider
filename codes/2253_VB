            Imports Microsoft.VisualBasic<br>Imports System.Math<br>Imports System<br>Imports System.Collections<br>Imports System.Text<br> <br>Public Class TrueFalse<br>    Dim corr() As Integer<br>    Dim key() As String<br>    Dim N As Integer<br>    Dim ret As String<br> <br>    Public Function answerKey(ByVal graded As String()) As String<br>        ReDim corr(graded.Length - 1)<br>        ReDim key(graded.Length - 1)<br>        Dim a As Integer<br>        For a = 0 To graded.Length - 1<br>            Dim s() As String = graded(a).Split(" "c)<br>            corr(a) = s(0)<br>            key(a) = s(1)<br>        Next<br>        N = key(0).Length<br>        doit("")<br>        If ret = "" Then<br>            Return "inconsistent"<br>        Else<br>            Return ret<br>        End If<br>    End Function<br> <br>    Public Function doit(ByVal s As String) As Boolean<br>        Dim i As Integer = s.Length<br>        If i = N Then<br>            ret = s<br>            Return True<br>        End If<br>        Dim w As String = "FT"<br>        Dim a, b As Integer<br>        For a = 0 To 1<br>            Dim ss As String = s &amp; w.Chars(a)<br>            Dim test As Boolean = False<br>            For b = 0 To key.Length - 1<br>                If key(b).Chars(i) = w.Chars(a) Then<br>                    test = True<br>                    corr(b) -= 1<br>                End If<br>            Next<br>            If test Then<br>                For b = 0 To key.Length - 1<br>                    If corr(b) &lt; 0 OrElse corr(b) &gt; N - i - 1 Then<br>                        test = False<br>                        Exit For<br>                    End If<br>                Next<br>                If test Then<br>                    If doit(ss) Then<br>                        Return True<br>                    End If<br>                End If<br>            End If<br>            For b = 0 To key.Length - 1<br>                If key(b).Chars(i) = w.Chars(a) Then<br>                    corr(b) += 1<br>                End If<br>            Next<br>        Next<br>        Return False<br>    End Function<br>End Class<br></td>