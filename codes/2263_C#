            using System;<br>using System.Text;<br>using System.Collections;<br><br>public class WaterLevel {<br>    public double netAmt(int evapNormal, int evapFlood, int[] rain) {<br>    double h = 0, t = 0, tx;<br>    for (int i = 0; i &lt; rain.Length; i++)<br>    {<br>      t = 0;<br>      while (t &lt; 1)<br>      {<br>        if (h &lt;= 0)<br>        {<br>          if (rain[i] &lt;= evapNormal) { h -= (1 - t) * (evapNormal - rain[i]); t = 1; }<br>          else<br>          {<br>            tx = Math.Min(-h / (rain[i] - evapNormal), 1 - t);<br>            h -= tx * (evapNormal - rain[i]);<br>            t += tx;<br>          }<br>        }<br>        if (h &gt;= 0)<br>        {<br>          if (rain[i] &gt;= evapFlood) { h += (1 - t) * (rain[i] - evapFlood); t = 1; }<br>          else<br>          {<br>            tx = Math.Min(h / (evapFlood - rain[i]), 1 - t);<br>            h += tx * (rain[i] - evapFlood);<br>            t += tx;<br>          }<br>        }<br>        if (h == 0 &amp;&amp; rain[i] &gt; evapNormal &amp;&amp; rain[i] &lt; evapFlood) break;<br>      }<br>    }<br>    return h;<br>    }<br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>