            using System;<br>using System.Text;<br>using System.Collections;<br><br>public class OnLineRank <br>{<br>   public int calcRanks(int k, int[] scores) <br>   {<br>      int l = scores.Length;<br>      int tot = 0;<br><br>      for(int i = 0; i &lt; l; i++)<br>      {<br>         int rn = 1;<br>         for (int p = 1; p &lt;=k; p++)<br>         {<br>            int j = i - p;<br>            if (j &lt; 0) continue;<br>            if (scores[j] &gt; scores[i]) rn++;<br>         }<br>         tot += rn; <br>      }<br>      return tot;<br><br>   }<br>   <br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>