<td class="problemText" colspan="8" valign="middle" align="left">
            #include&lt;iostream&gt;<br>#include&lt;vector&gt;<br>using namespace std;<br> <br>class OnLineRank<br>{<br>    public :<br>    int calcRanks(int k,vector &lt;int&gt; scores);<br>};<br>int OnLineRank::calcRanks(int k,vector &lt;int&gt; scores)<br>{<br>    int sum=0,rank,l;<br>    int n = scores.size(),i,j;<br>    for(i=0;i&lt;n;i++)<br>    {<br>        for(rank=1,j=i-1,l=0;l&lt;k &amp;&amp; j&gt;=0;j--,l++)<br>                if (scores[j]&gt;scores[i])<br>                        rank++;<br>        sum += rank;<br>    } <br>    return sum;<br>}<br></td>