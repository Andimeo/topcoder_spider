            Option Explicit On <br>Option Strict On<br> <br>Imports System<br>Imports System.Collections<br> <br>Public Class OnLineRank<br>    Public Function calcRanks(ByVal k As Integer, ByVal scores As Integer()) As Integer<br>        Dim tc As Integer<br>        Dim mc As Integer = scores.Length<br>        Dim ret As Integer = 1<br> <br>        For tc = 1 To mc - 1<br>            ret += Me.getRank(k, scores, tc)<br>        Next<br>        Return ret<br>    End Function<br>    Private Function getRank(ByVal k As Integer, ByVal scores As Integer(), ByVal actPos As Integer) As Integer<br>        Dim ranks As Integer = 0<br>        Dim actV As Integer = scores(actPos)<br>        Dim tmpSt As Integer = actPos - k<br>        Dim tc As Integer<br> <br>        If tmpSt &lt; 0 Then tmpSt = 0<br>        For tc = tmpSt To actPos - 1<br>            If scores(tc) &gt; actV Then ranks += 1<br>        Next<br>        Return ranks+1<br>    End Function<br>End Class<br></td>