            using System;<br>using System.Collections;<br>using System.Text;<br> <br>public class Measures <br>{<br>    static void Main(string[] args)<br>    {<br>        int[] t = new int[] { 1, 6, 9 }<br>;<br> <br>        Console.WriteLine(beatsPerM(t));<br>    }<br> <br>    public static int beatsPerM(int[] loudness)<br>    {<br>        int n = 2;<br>        int i, j,k,max;<br>        int p, q;<br> <br>        for (n = 2; n &lt;= 10; n++)<br>        {<br>            for (i = 0; i &lt; n; i++)<br>            {<br>                p = q = 0;<br>                j = i;<br>                while (true)<br>                {<br>                    if (j + n &lt;= loudness.Length)<br>                    {<br>                        p++;<br>                        max=0;<br>                        for (k = j; k &lt; j + n; k++)<br>                            max = Math.Max(max, loudness[k]);<br>                        if (max == loudness[j])<br>                            q++;<br>                    }<br>                    else<br>                        break;<br>                    j += n;<br>                }<br>                if (p &gt; 0 &amp;&amp; (double)q / (double)p &gt;= 0.8)<br>                    return n;<br>            }<br> <br>        }<br> <br>        return -1;<br> <br>    }<br>}<br></td>