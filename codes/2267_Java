            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class Measures<br>{<br>  private double check(int[] loudness, int offset, int bpm)<br>  {<br>    int full = 0;<br>    int valid = 0;<br>    for (int x=offset; x+bpm &lt;= loudness.length; x += bpm)<br>    {<br>      full++;<br>      int y;<br>      for (y=1; y&lt;bpm; y++)<br>      {<br>        if (loudness[x+y] &gt; loudness[x]) break;<br>      }<br>      if (y == bpm) valid++;<br>    }<br>    return 1.0 * valid/full;<br>  }<br>  <br>  public int beatsPerM(int[] loudness)<br>  {<br>    for (int bpm=2; bpm&lt;=10; bpm++)<br>    {<br>      if (bpm &gt; loudness.length) return -1;<br>      <br>      for (int x=0; x&lt;bpm; x++)<br>      {    <br>        if (check(loudness, x, bpm) &gt;= 0.8) return bpm;<br>      }<br>    }<br>    <br>    return -1;<br>  }<br>  <br>  <br>}<br> <br>//Powered by [KawigiEdit]<br></td>