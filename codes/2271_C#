            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class TeXLeX {<br>    public int[] getTokens(string inp) {<br>        ArrayList res = new ArrayList();<br>        while ( inp.Length != 0 ) {<br>            if ( inp.StartsWith("^^") &amp;&amp; inp.Length &gt;= 3 ) {<br>                inp = inp.Substring(2);<br>                int k=0,t=0;<br>                if ( inp.Length &gt;= 2 &amp;&amp; (k="0123456789abcdef".IndexOf(inp[0])) != -1 &amp;&amp; (t="0123456789abcdef".IndexOf(inp[1])) != -1 ) {<br>                    inp = inp.Substring(2);<br>                    inp = ((char)(t+16*k))+inp;<br>                } else {<br>                    int x = (int)inp[0];<br>                    inp = inp.Substring(1);<br>                    if ( x &gt; 63 ) {<br>                        inp = ((char)(x-64))+inp;<br>                    } else {<br>                        inp = ((char)(x+64))+inp;<br>                    }<br>                }<br>            } else {<br>                res.Add((int)inp[0]);<br>                inp = inp.Substring(1);<br>            }<br>        }<br>        return (int[])res.ToArray(typeof(int));<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>