            using System;<br><br>public class NumberofFiboCalls {<br>  public int[] fiboCallsMade(int n) {<br>    if (n == 0)<br>      return new int[]{1, 0};<br>    else<br>    {<br>      int a = 0;<br>      int b = 1;<br>      for (int i = 1; i &lt; n; ++i)<br>      {<br>        int c = a + b;<br>        a = b;<br>        b = c;<br>      }<br>      return new int[]{a, b};<br>    }<br>  }<br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>