            import java.util.*;<br>public class AWordGame {<br>    class Word {<br>  boolean complete;<br>  int value;<br>  String w;<br>  Word bet;<br>  ArrayList follow = new ArrayList();<br>  // verdi for den som SIER dette ordet. 1000 er vinn N?, -1000 er tap n?<br>  int o() {<br>      if(value!=0) return value;<br>      if(complete) return -1000;<br>      int best = 99000; // anta vi taper<br>      String bestw;<br>      for(int i=follow.size();i--&gt;0;) {<br>    Word n = (Word) follow.get(i);<br>    int nv = -n.o();<br>    if(nv&gt;0) nv--; else nv++; //discount<br>    if(nv &lt; best) { best=nv; bet = n; }<br>    else if(nv == best) {<br>        if(bet.w.compareTo(n.w)&gt;0) bet = n;<br>    }<br> <br>      }<br>      value = best;<br>      return best;<br>  }<br>    }<br>    HashMap h = new HashMap();<br>    Word put(String s, boolean c, Word prev) {<br>  Word w = (Word)h.get(s);<br>  if(w==null) {<br>      h.put(s, w = new Word());<br>      w.w = s;<br>  }<br>  w.complete |= c;<br>  if(prev!=null) w.follow.add(prev);<br>  return w;<br>    }<br> <br>public String outcome(String[] wordList) {<br>    for(int i=wordList.length;i--&gt;0;) {<br>  String r[] = wordList[i].split(" ");<br>  for(int zx=r.length;zx--&gt;0;) {<br>      String s = r[zx];<br>      Word prev = put(s,true, null);<br>      for(int j=s.length();j--&gt;0;) {<br>    prev = put(s.substring(0,j), false, prev);<br>      }<br>  }<br>    }<br>    Word rot = (Word)h.get("");<br>    rot.o();<br>    while(! rot.complete) rot = rot.bet;<br>    return rot.w;<br>}}<br></td>