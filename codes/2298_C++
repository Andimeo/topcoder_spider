)if (a[i]!=b[i]){<br>    if (i1&lt;0) i1=i;<br>    else if (i2&lt;0) i2=i;<br>    else return false;<br>  }<br>  if (i2&lt;0||a[i2]==b[i1]&amp;&amp;a[i1]==b[i2]) return true;<br>  return false;<br>}<br> <br>bool w[64];<br>map&lt;string,VS&gt; vr;<br> <br>class WordPuzzle {<br>  public:<br>  vector &lt;string&gt; getConversions(vector &lt;string&gt; dict, string source, string target) {<br>    sort(zall(dict));<br>    dict.push_back(source);<br>    vr[source].push_back(source);<br>    queue&lt;string&gt; qu;<br>    qu.push(source);<br>    while(!qu.empty()){<br>      string s=qu.front();<br>      qu.pop();<br>      if (cango(s,target)){<br>        VS r=vr[s];<br>        r.push_back(target);<br>        return r;<br>      }<br>      forv(i,dict)if (!vr.count(dict[i])&amp;&amp;cango(s,dict[i])){<br>        qu.push(dict[i]);<br>        vr[dict[i]]=vr[s];<br>        vr[dict[i]].push_back(dict[i]);<br>      }<br>    }<br>    return VS();<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>