            #line 2 "SquareCode.cpp"<br><br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br><br>using namespace std;<br><br>#define FOR(k,start,end) for(typeof(start) k=(start); k!=(end); ++k)<br><br>class  SquareCode {<br>  public:<br>  vector &lt;string&gt; completeIt(vector &lt;string&gt; g) {<br>    int n = g.size();<br>    for(int x = 0; x &lt;n/2; x++) for(int y=0; y&lt;n/2; y++) {<br>      char c1 = g[y][x];<br>      char c2 = g[n-1-x][y];<br>      char c3 = g[n-1-y][n-1-x];<br>      char c4 = g[x][n-1-y];<br>      int i = 0;<br>      if(c1 == '.') i++;<br>      if(c2 == '.') i++;<br>      if(c3 == '.') i++;<br>      if(c4 == '.') i++;<br>      if(i &gt; 1) return vector&lt;string&gt;();<br>      if(i == 0) g[y][x] = '.';<br>      }<br>    return g;<br>    }<br>  };<br><br><br>// Powered by FileEdit<br></td>