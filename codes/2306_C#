<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>using System.Collections;<br> <br>public class IBEvaluator<br>{<br>  public int [] getSummary (int [] P, int [] A)<br>  {<br>    int [] ret = new int [7];<br>    int [] te = new int [7];<br>    for (int i=0; i&lt;7; i++)<br>      te[i]=0;<br>    int t = P.Length;<br>    for (int i=0; i&lt;t; i++)<br>      te[Math.Abs(P[i]-A[i])]++;<br>    for (int i=0; i&lt;7; i++)<br>      ret[i]=(te[i]*100)/t;<br>    return ret;<br>  }<br>}<br></td>