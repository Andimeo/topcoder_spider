            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include&lt;math.h&gt;<br>using namespace std;<br>class IBEvaluator {<br>  public:<br>  vector &lt;int&gt; getSummary(vector &lt;int&gt; pred, vector &lt;int&gt; act) {<br>    int size=act.size();<br>    int i,j;<br>    vector&lt;int&gt; ret(7);<br>    vector &lt;int&gt; diff(7);<br>    for(i=0;i&lt;size;i++)<br>    {<br>      int dif = abs(pred[i]-act[i]);<br>      diff[dif]++;<br>    }<br>    for(i=0;i&lt;7;i++)<br>    {<br>      ret[i]=diff[i]*100.00/size;<br>    }<br>    return ret;<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>