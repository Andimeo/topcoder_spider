            using System;<br>using System.Collections;<br>using System.Globalization;<br>using System.Text;<br>using System.Text.RegularExpressions;<br><br>public class SlayingDeer <br>{<br>  public int getTime( int A, int B, int C ) <br>  {<br>    int result = 0;<br>    if ( A * 45 - B * 30 &lt;= 0 ) return -1;<br>    while( C &gt; 0 )<br>    {<br>      if ( A * 45 - B * 30 &lt; C )<br>      {<br>        result += 45; C -= A * 45 - B * 30;<br>      }<br>      else if ( A * 30 - B * 30 &gt;= C )<br>      {<br>        return (C - 1) / (A - B) + 1 + result;<br>      }<br>      else<br>      {<br>        result += 30;<br>        C -= (A - B) * 30;<br>        return (C - 1) / A + 1 + result;<br>      }<br>    }    <br>    return result;<br>  }<br><br>  NumberFormatInfo nfi = new CultureInfo( "en-US", false ).NumberFormat;<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>