            Imports Microsoft.VisualBasic<br>Imports System.Math<br>Imports System<br>Imports System.Collections<br>Imports System.Text<br> <br>Public Class SlayingDeer<br>    Public Function getTime(ByVal A As Integer, ByVal B As Integer, ByVal C As Integer) As Integer<br>        Dim B2 As Double = B * 30 / 45<br>        If A &lt;= B2 Then Return -1<br>        Dim t1 As Double = C / (A - B2)<br>        Dim t0 As Integer = Floor(t1 / 45) * 45<br>        If (C - t0 * (A - B2)) = 0 Then Return t0<br>        If (A - B) * 30 &gt; (C - t0 * (A - B2)) Then<br>            Return t0 + Ceiling((C - t0 * (A - B2)) / (A - B))<br>        Else<br>            Return t0 + 30 + Ceiling((C - t0 * (A - B2) - 30 * (A - B)) / A)<br>        End If<br>    End Function<br>End Class<br></td>