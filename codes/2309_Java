            import java.util.*;<br>public class LunchScheduler {<br>    int n;<br>    static final int[] fact = {1, 1, 2, 6, 24, 120, 720, 720*7, 720*7*8, 720*7*8*9};<br>    void genPer(int[] ord, int pos) {<br>        Arrays.fill(ord, -1);<br>        for(int i = 0; i&lt;n; i++) {<br>            int k = pos/fact[n-i-1];<br>            pos = pos%fact[n-i-1];<br>            for(int j = 0; j&lt;n; j++) {<br>                if(ord[j]==-1) k--;<br>                if(k&lt;0) {<br>                    ord[j] = i;<br>                    break;<br>                }<br>            }<br>        }<br>    }<br>  public int getOverlap(String[] M) {<br>        n = M.length;<br>        int[] ord = new int[n];<br>        int[] end = new int[n];<br>        int[] counter = new int[n];<br>        int best = 100;<br>        for(int pos = 0; pos &lt;fact[n]; pos++) {<br>            genPer(ord, pos);<br>            for(int i = 0; i&lt;n; i++) {<br>                end[i] = ord[i];<br>                for(int j = 0; j&lt;n; j++) {<br>                    if(i!=j &amp;&amp; M[i].charAt(j)=='1' &amp;&amp; ord[j]&gt;end[i]) {<br>                        end[i] = ord[j];<br>                    }<br>                }<br>            }<br>            Arrays.fill(counter, 0);<br>            for(int i = 0; i&lt;n; i++) {<br>                for(int k = ord[i]; k&lt;=end[i]; k++) counter[k]++;<br>            }<br>            int max = 0;<br>            for(int i = 0; i&lt;n; i++) {<br>                if(max&lt;counter[i]) max = counter[i];<br>            }<br>            if(max&lt;best) best = max;<br>        }            <br>            <br>    return best;<br>  }<br> <br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>