<td class="problemText" colspan="8" valign="middle" align="left">
            public class RSABreaker<br>  {<br> <br>    int gcd (int a, int b)<br>    {<br>      int r;<br>      while (b &gt; 0)<br>      {<br>        r = a % b;<br>        a = b;<br>        b = r;<br>      }<br>      return a;<br>    }<br> <br>    int Power (int a, int k, int p)<br>    {<br>      int res = 1;<br>      while (k &gt; 0)<br>      {<br>        if (k % 2 == 1)<br>        {<br>          res = (res*a)%p;<br>          k--;<br>        }<br>        else<br>        {<br>          a = (a*a)%p;<br>          k = k / 2;<br>        }<br>      }<br>      return (res % p);<br>    }<br> <br>    public int decrypt (int e,int n,int b)<br>    {<br>      int m = 0;<br>      for (int i = 1;i&lt;n;i++)<br>        if (gcd(n,i) == 1)<br>        {m++;}<br> <br>      int d = 0;<br>      int PhiByM = 0;<br>            <br>      for (int i = 1;i&lt;m;i++)<br>        if (gcd(m,i) == 1)<br>        {PhiByM++;}<br> <br>      d = Power(e,PhiByM-1,m);<br>      int ans = Power (b,d,n);<br>      return ans;<br>    }<br>  <br>  }<br></td>