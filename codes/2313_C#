            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class Spamatronic {<br>    void toks( string s, Hashtable ht ) {<br>        s = "@"+s+"@";<br>        int i = 0 ;<br>        string t = "";<br>        bool dolet = false;<br>        while( i != s.Length ) {<br>            if ( dolet ) {<br>                if(char.IsLetter(s[i])) {<br>                    t += s[i++];<br>                } else {<br>                    if ( t.Length != 0 ) {<br>                        ht[t.ToLower()]=null;<br>                        t="";<br>                        dolet = false;<br>                        i++;<br>                    }<br>                }<br>            } else {<br>                if(char.IsLetter(s[i])) {<br>                    dolet = true;<br>                    t += s[i++];<br>                } else {<br>                    i++;<br>                }<br>            }<br>        }<br>        if ( t.Length != 0 ) ht[t.ToLower()]=null;<br>    }<br>    public int[] filter(string[] ks, string[] nm) {<br>        ArrayList al = new ArrayList();<br>        Hashtable spam = new Hashtable();<br>        foreach( string s in ks ) toks(s,spam);<br>        for ( int i = 0 ; i != nm.Length; i++ ) {<br>            Hashtable hh = new Hashtable();<br>            toks(nm[i],hh);<br>            int n = 0;<br>            foreach( string ss in hh.Keys )<br>                if ( spam.ContainsKey(ss))<br>                    n++;<br>            if (4*n &gt;= 3*hh.Count) {<br>                toks( nm[i], spam );<br>            } else al.Add(i);<br>        }<br>        return (int[])al.ToArray(typeof(int));<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>