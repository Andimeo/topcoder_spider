            import java.util.*;<br>public class BorelSets {<br>  boolean[] in;<br>  int msk;<br>  public int howMany(int size, String[] subsets) {<br>    in = new boolean[1 &lt;&lt; size];<br>    msk = (1 &lt;&lt; size) - 1;<br>    for (int i = 0; i &lt; subsets.length; i++) {<br>      int m = convert(subsets[i]);<br>      doit(m);<br>    }<br>    return ret;<br>  }<br>  int ret = 0;<br>  void doit(int m) {<br>    if (in[m]) {<br>      return;<br>    }<br>    ret++;<br>    in[m] = true;<br>    doit(((~m) &amp; msk));<br>    for (int i = 0; i &lt; in.length; i++) {<br>      if (in[i]) {<br>        doit((m | i));<br>      }<br>    }<br>  }<br>  int convert(String s) {<br>    int ret = 0;<br>    StringTokenizer tokens = new StringTokenizer(s);<br>    while (tokens.hasMoreTokens()) {<br>      ret |= 1 &lt;&lt; (Integer.parseInt(tokens.nextToken()) - 1);<br>    }<br>    return ret;<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>