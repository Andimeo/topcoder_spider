            #line 102 "TurretDefense.cpp"<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;math.h&gt;<br><br>using namespace std;<br><br>class TurretDefense {<br>  public:<br>  int firstMiss(vector &lt;int&gt; xs, vector &lt;int&gt; ys, vector &lt;int&gt; times) {<br>    <br>    int oldx = 0;<br>    int oldy = 0;<br>    int oldtime = 0;<br>    for (int x=0;x&lt;xs.size();x++)<br>    {<br>      int dist = abs(oldx-xs[x])+abs( oldy-ys[x]);<br>      if (dist&gt;times[x]-oldtime) return x;<br><br>      oldx = xs[x];<br>      oldy = ys[x];<br>      oldtime = times[x];<br>    }<br><br>    return -1;<br>  }<br><br>  <br><br>};<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>