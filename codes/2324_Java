            import java.util.*;<br>public class MoneyRun {<br> int[][] a;<br> int m,n;<br> int max=Integer.MIN_VALUE;<br> public int getMost(String[] grid) {<br>  m=grid.length; n=grid[0].length(); a=new int[m][n];<br>  for (int i=0;i&lt;m;i++)<br>   for (int j=0;j&lt;n;j++) a[i][j]=grid[i].charAt(j)-'0';<br>  vet(0,0,a[0][0],0);<br>  return max;<br> }<br> void vet(int i,int j,int k,int t) {<br>   int p=a[i][j];<br>   a[i][j]=0;<br>   if (i==m-1&amp;&amp;j==n-1) {<br>    if (t==0) vet(0,0,k,1); else {<br>     if (k&gt;max) max=k;<br>    }<br>   } else {<br>    if (i&lt;m-1) vet(i+1,j,k+a[i+1][j],t);<br>    if (j&lt;n-1) vet(i,j+1,k+a[i][j+1],t);<br>   }<br>   a[i][j]=p;<br> }<br> <br> <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>