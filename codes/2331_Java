            import java.util.*;<br>public class PolicePair {<br>  public int[] bestSquad(int[] skillStart, int[] skillEnd) {<br>    int[] num = new int[1001];<br>    int total = 0;<br>    for(int i = 0; i&lt;skillStart.length; i++) {<br>      for(int j = skillStart[i]; j&lt;=skillEnd[i]; j++) {<br>        num[j]++;<br>        total++;<br>      }<br>    }<br>    int bestVal = 0;<br>    int bestSkill = 3000;<br>    for(int skill2 = 2000; skill2 &gt;=2; skill2--) {<br>      int cnt = 0;<br>      for(int i = 1; i&lt;=1000; i++) {<br>        int j = skill2-i;<br>        if(j&gt;=1 &amp;&amp; j&lt;=1000) {<br>          if(i!=j) {<br>            cnt+=Math.min(num[i], num[j]);<br>          } else {<br>            cnt += (num[i]/2)*2;<br>          }<br>        }<br>      }<br>      if(cnt&gt;bestVal) {<br>        bestVal = cnt;<br>        bestSkill = skill2/2;<br>      }<br>    }<br>    <br>    return new int[]{total-bestVal, bestSkill};<br>  }<br> <br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>