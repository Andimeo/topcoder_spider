            using System;<br>using System.Text;<br> <br>public class BridgeSort<br>{<br>  public string sortedHand(string h)<br>  {<br>    char[] aa = new char[] { 'C', 'D', 'H', 'S' };<br>    char[] bb = new char[] { 'T', 'J', 'Q', 'K', 'A' };<br>    <br>    int[] k = new int[h.Length / 2];<br>    <br>    string[] ss = new string[h.Length  / 2];<br>    <br>    for(int i = 0; i &lt; h.Length; i+=2)<br>    {<br>      int a = Array.IndexOf(aa, h[i]) * 100;<br>      int b = Array.IndexOf(bb, h[i+1]);<br>      <br>      if (b == -1)<br>        b = int.Parse("" + h[i+1]);<br>      else<br>        b = 10 + b;<br>      <br>      k[i/2] = a + b;<br>      ss[i/2] = "" + h[i] + h[i+1];<br>    }<br> <br>    Array.Sort(k, ss);<br>    <br>    StringBuilder sb = new StringBuilder(h.Length);<br>    foreach(string s in ss)<br>      sb.Append(s);<br> <br>    return sb.ToString();<br>  }<br>}<br></td>