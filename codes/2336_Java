            import java.util.*;<br>public class BridgeSort {<br>  public String sortedHand(String hand) {<br>    int[] cards = new int[hand.length() / 2];<br>    String suits = "CDHS";<br>    String vals = "23456789TJQKA";<br>    String ret = "";<br>    int n = 0;<br>    for(int i = 0; i &lt; cards.length; i++) {<br>      cards[i] = suits.indexOf(hand.charAt(n++)) * 13;<br>      cards[i] += vals.indexOf(hand.charAt(n++));<br>    }<br>    Arrays.sort(cards);<br>    for(int i = 0; i &lt; cards.length; i++) {<br>      ret += suits.charAt(cards[i] / 13);<br>      ret += vals.charAt(cards[i] % 13); <br>    }<br>    return ret;<br>      <br>    <br>  }<br> <br>  <br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>