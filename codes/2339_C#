            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class TennisSet {<br>    public string firstSet(string[] points) {<br>        int[] g = new int[2];<br>        int s=0;<br>        int[] pt = new int[2];<br>        foreach( string ss in points ) { <br>            foreach ( char c in ss ) {<br>                int w = (c=='S') ? s : 1-s;<br>                int l = 1-w;<br>                pt[w]++;<br>                if ( pt[w]&gt;=4 &amp;&amp; pt[w]-pt[l]&gt;=2 ) {<br>                    g[w]++;<br>                    if (g[w]&gt;=6 &amp;&amp; g[w]-g[l]&gt;=2) {<br>                        return g[0]+"-"+g[1];<br>                    }<br>                    pt = new int[2];<br>                    s = 1-s;<br>                }<br>            }<br>        }<br>        return g[0]+"-"+g[1];<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>