            import java.util.*;<br>public class TennisSet {<br>  public String firstSet(String[] points) {<br>    String k = "";<br>    for ( int i = 0; i &lt; points.length; i++ ) k += points[i];<br>    <br>    int a = 0, b = 0;<br>    int ta = 0, tb = 0;<br>    int player = 0;<br>    for ( int i = 0; i &lt; k.length(); i++ ) {<br>      if ( k.charAt( i ) == 'S' ) {<br>        if ( player == 0 ) {<br>          a++;<br>        } else {<br>          b++;<br>        }<br>      }<br>      else {<br>        if ( player == 0 ) {<br>          b++;<br>        } else {<br>          a++;<br>        }<br>      }<br>      <br>      if ( a &gt;= 4 &amp;&amp; a - b &gt;= 2 ) {<br>        a = b = 0;<br>        player = (player + 1 )% 2;<br>        ta++;<br>      }<br>      else if ( b &gt;= 4 &amp;&amp; b - a &gt;= 2 ) {<br>        a = b = 0;<br>        player = (player + 1 )% 2;<br>        tb++;<br>      }<br>      <br>      if ( ta &gt;= 6 &amp;&amp; ta - tb &gt;= 2 ) {<br>        return ta + "-" + tb;<br>      }<br>      else if ( tb &gt;= 6 &amp;&amp; tb - ta &gt;= 2 ) {<br>        return ta + "-" + tb;<br>      }<br>    }<br>    return ta + "-" + tb;<br>  }<br>  <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>