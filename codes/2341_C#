            using System;<br>using System.Collections;<br>using System.Text;<br> <br>public class ScoringEfficiency<br>{<br>  static void Main(string[] args)<br>  {<br>    ScoringEfficiency test = new ScoringEfficiency();<br>  }<br>  public double getPointsPerShot(string[] gameLog)<br>  {<br>    double totalPoints=0;<br>    double fieldsAttempted=0;<br>    double freeAttempted=0;<br>    foreach (string s in gameLog)<br>    {<br>      if (s == "Made 2 point field goal")<br>      {<br>        totalPoints+=2;<br>        fieldsAttempted++;<br>      }<br>      if (s == "Missed 2 point field goal")<br>      {<br>        fieldsAttempted ++;<br>      }<br>      if (s == "Made 3 point field goal")<br>      {<br>        totalPoints+=3;<br>        fieldsAttempted++;<br>      }<br>      if (s == "Missed 3 point field goal")<br>      {<br>        fieldsAttempted++;<br>      }<br>      if (s == "Made free throw")<br>      {<br>        totalPoints++;<br>        freeAttempted++;<br>      }<br>      if (s == "Missed free throw")<br>      {<br>        freeAttempted++;<br>      }<br>    }<br>    return totalPoints / (fieldsAttempted + 0.5*freeAttempted);<br>  }<br>}<br></td>