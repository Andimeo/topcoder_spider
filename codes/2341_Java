            import java.util.*;<br>public class ScoringEfficiency {<br>  public double getPointsPerShot(String[] gameLog) {<br>    int shots = 0;<br>    int points = 0;<br>    int fts = 0;<br>    <br>    for (int j = 0; j &lt; gameLog.length; j++)<br>    {<br>      String s = gameLog[j];<br>      if (s.indexOf("2") != -1)<br>      {<br>        if (s.indexOf("Made") != -1)<br>        {<br>          points += 2;<br>        }<br>        shots++;<br>      }<br>      else if (s.indexOf("3") != -1)<br>      {<br>        if (s.indexOf("Made") != -1) points += 3;<br>        shots++;<br>      }<br>      else if (s.indexOf("free") != -1)<br>      {<br>        if (s.indexOf("Made") != -1) points++;<br>        fts++;<br>      }<br>    }<br>    double pps = (double)points / ((double)shots + ((double).5 * (double)fts));<br>    return pps;<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>