<td class="problemText" colspan="8" valign="middle" align="left">
            Imports microsoft.visualbasic<br> <br>Public Class ScoringEfficiency<br>        Public Function getPointsPerShot(ByVal gameLog As String()) As Double<br>            Dim x As Integer<br>            Dim points As Integer<br>            Dim fpoints As Integer<br>            Dim shots As Integer<br>            Dim fshots As Integer<br> <br>            points = 0<br>            fpoints = 0<br>            shots = 0<br>            fshots = 0<br> <br>            For x = 0 To gameLog.Length - 1<br>                If gameLog(x) = "Made free throw" Then<br>                    fpoints = fpoints + 1<br>                    fshots = fshots + 1<br>                End If<br>                If gameLog(x) = "Missed free throw" Then fshots = fshots + 1<br>                If Right(gameLog(x), 1) = "l" Then<br>                    If Mid(gameLog(x), 2, 1) = "a" Then<br>                        points = points + CInt(Mid(gameLog(x), 6, 1))<br>                        shots = shots + 1<br>                    Else<br>                        shots = shots + 1<br>                    End If<br>                End If<br>            Next x<br>            '- "Made 2 point field goal"<br>            '- "Missed 2 point field goal"<br>            '- "Made 3 point field goal"<br>            '- "Missed 3 point field goal"<br>            '- "Made free throw"<br>            '- "Missed free throw"<br> <br>            getPointsPerShot = (points + fpoints) / ((shots) + (0.5 * fshots))<br>        End Function<br>    End Class<br></td>