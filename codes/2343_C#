            using System;<br>using System.Collections;<br><br>public class Hangman {<br>  public string guessWord(string feedback, string[] words) {<br>        string guess = "";<br>        foreach (string word in words)<br>        {<br>            if (word.Length == feedback.Length)<br>            {<br>                bool okey = true;<br>                for (int i = 0; i &lt; word.Length; i++)<br>                {<br>                    okey &amp;= (word[i] == feedback[i] || feedback[i] == '-');<br>                }<br>                for (int i = 0; i &lt; feedback.Length; i++)<br>                {<br>                    if (feedback[i] != '-')<br>                    {<br>                        for (int ii = 0; ii &lt; word.Length; ii++)<br>                        {<br>                            if (word[ii] == feedback[i] &amp;&amp; word[ii] != feedback[ii])<br>                            {<br>                                okey = false;<br>                            }<br>                        }<br>                    }<br>                }<br>                if (okey)<br>                {<br>                    if (guess.Length &gt; 0) return "";<br>                    guess = word;<br>                }<br>            }<br>        }<br>        return guess;<br>    }<br>}<br><br><br>// Powered by FileEdit<br></td>