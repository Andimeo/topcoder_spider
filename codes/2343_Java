            import java.util.*;<br>public class Hangman {<br>  public String guessWord(String feedback, String[] words) {<br>    int best=-1;<br>    outer:<br>    for(int i=0; i&lt;words.length; i++){<br>      if(words[i].length()!=feedback.length()) continue;<br>      for(int j=0; j&lt;feedback.length(); j++){<br>        if(feedback.charAt(j)=='-'){<br>          for(int k=0; k&lt;feedback.length(); k++){<br>            if(feedback.charAt(k)==words[i].charAt(j)) continue outer;<br>          }<br>          continue;<br>        }<br>        if(words[i].charAt(j)!=feedback.charAt(j)) continue outer;<br> <br>      }<br>      if(best!=-1) return "";<br>      best=i;<br>    }<br>    if(best!=-1) return words[best];<br>    return "";<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>