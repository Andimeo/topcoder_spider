            import java.util.*;<br>public class AlienMultiplication {<br>  int correct(int pos, int d, int a) {<br>    int x = 1;<br>    for(int i = 0; i&lt;pos; i++) {<br>      x *= 10;<br>    }<br>    return a-((a/x)%10)*x+d*x;<br>  }<br>  boolean tryCorrect(int a0, int b0, int c0, int k, int s) {<br>    long x = a0;<br>    x *= b0;<br>    x -= c0;<br>    if(k==0 || s==18) {<br>      return x%1000000==0;<br>    }<br>    if(s%3==0) {<br>      int trunc = s/3;<br>      long y = 1;<br>      for(int i = 0; i&lt;trunc; i++) {<br>        y *= 10;<br>      }<br>      if(x%y!=0) return false;<br>    }<br>    int a = a0;<br>    int b = b0;<br>    int c = c0;<br>    for(int i = 0; i&lt;10; i++) {<br>    <br>      if(s%3==0) {<br>        a = correct(s/3, i, a);<br>      }<br>      if(s%3==1) {<br>        b = correct(s/3, i, b);<br>      }<br>      if(s%3==2) {<br>        c = correct(s/3, i, c);<br>      }<br>      if(tryCorrect(a, b, c, k-1, s+1)) return true;<br>    }<br>    return tryCorrect(a0, b0, c0, k, s+1);<br>  }<br>  public int getCorrections(int a, int b, int c) {<br>    for(int i = 0; i&lt;=5; i++) {<br>//      System.err.println(i);<br>      if(tryCorrect(a, b, c, i, 0)) return i;<br>    }<br>    return 6;<br>  }<br> <br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>