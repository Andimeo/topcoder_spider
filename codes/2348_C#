            using System;<br>public class BagOfHolding {<br>  public double oddsReachable(int[] sz, int item) {<br>    int vitem = sz[item];<br>    int big = 0;<br>    for (int i =0;i&lt;sz.Length;i++) {<br>      if (sz[i] &gt;= vitem)  <br>        big++;<br>    }<br> <br>    int size = sz.Length;<br>    int small = size - big;<br>    double ret = 0.0;<br>    double prod;<br>    for (int i = 0;i&lt;=small;i++) {<br>      prod = 1.0;<br>      for (int j = 0;j&lt;i;j++)<br>        prod*=(double)(small-j)/(double)(size - j-1);<br>      System.Console.WriteLine(prod);<br>      ret+= prod/(double)size;<br>    }<br>    return ret;<br>  }<br>}<br></td>