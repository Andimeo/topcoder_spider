            #include &lt;iostream&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>using namespace std;<br> <br>class BagOfHolding{<br>  public:<br>  double oddsReachable(vector&lt;int&gt; sizes,int item){<br>    int cnt=0;<br>    int good=0;<br>    vector&lt;int&gt; pe;<br>    for (int i=0; i&lt;sizes.size(); i++) pe.push_back(i);<br>    do{<br>      cnt++;<br>      bool ok=true;<br>      for (int j=sizes.size()-1; j&gt;=0; j--) {<br>        if (pe[j]==item) break;<br>        if (sizes[pe[j]]&gt;=sizes[item]) {ok=false; break;}<br>      }<br>      if (ok) good++;<br>      <br>    }while(next_permutation(pe.begin(), pe.end()));<br> <br>    return (double)good/cnt;<br>  }<br>};<br></td>