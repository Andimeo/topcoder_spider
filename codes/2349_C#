            public class DNASingleMatcher<br>  {<br>    public DNASingleMatcher()<br>    {<br> <br>    }<br> <br>    public int longestMatch(string strand1, string strand2)<br>    {<br>      int longest = 0;<br> <br>      for(int start1 = 0; start1 &lt; strand1.Length; start1++)<br>      {<br>        for(int start2 = 0; start2 &lt; strand2.Length; start2++)<br>        {<br>          int matchLength = 0;<br>          for (int i=0; i &lt; strand1.Length - start1 &amp;&amp; i &lt; strand2.Length - start2; i++)<br>          {<br>            if (strand1[start1 + i] == strand2[start2 + i])<br>            {<br>              matchLength++;<br>            }<br>            else<br>            {<br>              break;<br>            }<br>          }<br>          if (matchLength &gt; longest)<br>          {<br>            longest = matchLength;<br>          }<br>        }<br>      }<br>      return longest;<br>    }<br>  }<br></td>