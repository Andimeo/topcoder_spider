<td class="problemText" colspan="8" valign="middle" align="left">
            #include &lt;string&gt;<br>#include &lt;vector&gt;<br> <br>using namespace std;<br> <br>class DNASingleMatcher<br>{<br>public:<br>  int longestMatch(string s1, string s2)  <br>  {<br>    int j, k, l, count = 0;<br>    for(j = 0; j &lt; s1.length(); j++)<br>    {<br>      for(k = 0; k &lt; s2.length(); k++)<br>      {<br>        for(l = 0; l + j &lt; s1.length() &amp;&amp; l + k &lt; s2.length(); l++)<br>        {<br>          if(s1[l + j] != s2[l + k])<br>            break;<br>        }<br>        if(count &lt; l)<br>          count = l;<br>      }<br>    }<br>    return count;<br>  }<br>};<br></td>