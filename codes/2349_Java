            import java.util.*;<br>public class DNASingleMatcher{<br><br>public int longestMatch(String a, String b){<br><br>    int start = 0;<br>    int curLen = 0;<br>    int maxLen = 0;<br>    for (int i = 0; i &lt; a.length(); i++) {<br>      for (int j = a.length(); j &gt; i; j--) {<br>      if (b.indexOf(a.substring(i, j)) != -1) {<br>          curLen = j - i;<br>          if (curLen &gt; maxLen) {<br>          start = i;<br>          maxLen = curLen;<br>        }<br>        break;<br>      }<br>        }<br>    }<br><br><br>  return maxLen;<br>}<br><br>}<br>// Powered by CodeProcessor<br></td>