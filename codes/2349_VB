            imports microsoft.visualbasic<br>public class DNASingleMatcher<br>public function longestMatch(a as string, b as string) as integer<br>  longestMatch = inboth(a,b,"")<br>end function<br>public function inboth(a as string, b as string, c as string) as integer<br>  dim x,y,z,n as integer<br>  x=0<br>  if (InStr(1,a,c) &gt; 0 and InStr(1,b,c) &gt; 0) or c = "" then<br>    x = inboth(a,b,c+"A")<br>    y = inboth(a,b,c+"C")<br>    z = inboth(a,b,c+"G")<br>    n = inboth(a,b,c+"T")<br>    if x &lt; y then x = y<br>    if x &lt; z then x = z<br>    if x &lt; n then x = n<br>    if x = 0 then x = c.length<br>  else<br>    x = 0<br>  end if<br>  inboth = x<br>end function<br>end class<br></td>