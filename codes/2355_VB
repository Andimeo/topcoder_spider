            Imports Microsoft.VisualBasic<br>Imports System<br>Imports System.Collections<br>Imports System.Math<br> <br>Public Class CantorSet<br>    Public Function removed(ByVal value As String) As Integer<br>        Dim i, j, k, l As Long<br>        Dim a(value.Length - 2) As Integer<br>        Dim b() As Integer<br> <br>        For i = 1 To value.Length - 1<br>            a(i - 1) = CInt(value.Substring(i, 1))<br>        Next<br>        k = 1<br> <br>        Do While k &lt; 1000000<br>            'CHECK 1000000!<br>            a = sMult(a, l)<br>            If l = 1 Then Return k<br>            k += 1<br>        Loop<br>        Return 0<br> <br>    End Function<br> <br>    Private Function sMult(ByVal a() As Integer, ByRef n As Integer) As Integer()<br>        Dim b(a.GetUpperBound(0)) As Integer<br> <br>        Dim i As Integer<br>        Dim j As Integer<br>        For i = a.GetUpperBound(0) To 0 Step -1<br>            b(i) = a(i) * 3 + j<br>            j = b(i) \ 10<br>            b(i) -= 10 * j<br>        Next<br>        n = j<br>        Return b<br>    End Function<br>End Class<br></td>