            using System;<br>public class TVAntenna {<br>  public double minRadius(int[] x, int[]y) {<br>    double ret = 800f;<br>    int size = x.Length;<br>    double dist,max;<br>    for (int i = -200;i&lt;=200;i++)<br>      for (int j = -200;j&lt;=200;j++) {<br>        max = 0f;<br>        for (int k = 0;k&lt;size;k++) {<br>          dist = System.Math.Sqrt((i-x[k])*(i-x[k]) + (j-y[k])*(j-y[k]));<br>          if (dist &gt; max)<br>            max = dist;<br>        }<br>        if (max &lt; ret) {<br>          ret = max;<br>        }<br>      }<br>    return ret;<br>  }<br>}<br></td>