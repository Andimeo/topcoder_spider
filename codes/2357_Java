            import java.util.*;<br> <br>public class TVAntenna {<br> <br>  public static double dist(int x, int y, int x2, int y2) {<br>    return Math.sqrt( (x-x2)*(x-x2) + (y-y2)*(y-y2));<br>}<br>  public static double minRadius(int[] x, int[] y)<br>  {<br>    double min = Integer.MAX_VALUE;<br>    for (int k = -200; k &lt;= 200;k++) {<br>      for (int j = -200; j &lt;= 200;j++) {<br>        <br>        double max = -1;<br>        for (int r = 0; r &lt; x.length;r++) {<br>          int xx = x[r];<br>          int yy = y[r];<br>          double d = dist(k,j,xx,yy);<br>          //System.out.println(d);<br>          if (d &gt; max)<br>            max = d;<br>        }<br>        if (max &lt; min)<br>          min = max;<br>      }<br>    }<br>    return min;<br>  }<br> <br>}<br></td>