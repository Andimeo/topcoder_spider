            using System;<br>using System.Collections;<br>public class Hawaiian<br>{<br>  string haw = "aeiouhklmnpw";<br>  public string[] getWords(string sentence) <br>  {<br>    string[] sp = sentence.Split(' ');<br>    ArrayList al = new ArrayList();<br>    for(int i = 0; i &lt; sp.Length; i++)<br>    {<br>      if(sp[i] == "") continue;<br>      bool good = true;<br>      for(int j = 0; j &lt; sp[i].Length; j++)<br>      {<br>        if(haw.IndexOf(sp[i][j].ToString().ToLower()) &lt; 0) good = false;<br>      }<br>      if(good) al.Add(sp[i]);<br>    }<br>    return (string[])al.ToArray(typeof(string));<br>    <br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>