            import java.util.*;<br> <br>public class Hawaiian<br>{<br>  public String[] getWords(String sentence)<br>  {<br>    StringTokenizer st = new StringTokenizer( sentence );<br>    Vector bag = new Vector();<br>    while (st.hasMoreTokens()) {<br>      String s = st.nextToken();<br>      String u = s.toLowerCase();<br>      boolean yes = true;<br>      for (int i = 0; i &lt; u.length(); i++) {<br>        if ("aeiouhklmnpw".indexOf( u.charAt(i) ) &lt; 0) {<br>          yes = false;<br>          break;<br>        }<br>      }<br>      if (yes) {<br>        bag.add( s );<br>      }<br>    <br>    }<br>    return (String[])bag.toArray( new String[] {} );<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>