            #include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cmath&gt;<br> <br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;utility&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br> <br>using namespace std;<br> <br>class Transpose {<br>public:<br>  int numSwaps(int m,int n)<br>  {<br>    int i,j,x;<br>    vector&lt;int&gt; p;<br>    vector&lt;int&gt; pos;<br>    int ret=0;<br> <br>    p.clear();pos.clear();<br>    pos.resize(m*n);<br>    for(i=0;i&lt;m;i++) {<br>      x=i;<br>      for(j=0;j&lt;n;j++) {<br>  p.push_back(x);<br>  pos[x]=p.size()-1;<br>  x+=m;<br>      }<br>    }<br> <br>    for(i=0;i&lt;p.size();i++) if(p[i]!=i) {<br>      j=pos[i];<br>      p[j]=p[i];pos[p[j]]=j;<br>      p[i]=i;pos[p[i]]=i;<br>      ret++;<br>    }<br>    return ret;<br>  }<br>};<br></td>