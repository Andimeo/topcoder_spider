<td class="problemText" colspan="8" valign="middle" align="left">
            public class Transpose<br>{<br>  public int index(int row, int column, int numRow, int numColumn)<br>  {<br>    return row*numColumn + column;<br>  }<br> <br>  public int[] position(int index, int numRow, int numColumn)<br>  {<br>    int out[] = new int[2];<br>    out[0] = index/numColumn;<br>    out[1] = index%numColumn;<br>    return out;<br>  }<br> <br>  public int numSwaps(int m, int n)<br>  {<br>    boolean visited[] = new boolean[m*n];<br>    for(int i = 0; i &lt; visited.length; i++) visited[i] = false;<br>    <br>    int swaps = 0;<br> <br>    for(int i = 0; i &lt; visited.length; i++)<br>    if(!visited[i])<br>    {<br>      int cycle = 0;<br>      int curr = i;<br>      do<br>      {  <br>      visited[curr] = true;<br>      int current[] = position(curr, m, n);<br>      curr = index(current[1], current[0], n, m);<br>      cycle++;<br>      }while(!visited[curr]);<br>      swaps += cycle-1;<br>    }<br>    return swaps;<br>  }<br>}<br></td>