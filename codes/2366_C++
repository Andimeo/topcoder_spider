<td class="problemText" colspan="8" valign="middle" align="left">
            #include &lt;vector&gt;<br>#include &lt;iostream&gt;<br>#include &lt;stdlib.h&gt;<br>using namespace std;<br> <br>class MagicSquare<br>{<br>public: <br>int missing(vector &lt;int&gt; s)<br>{<br>  int a,b=0,c=0,i;<br>  for (i=a=0;i&lt;3;i++) a+=s[i];<br>  for (i=3;i&lt;6;i++) b+=s[i];<br>  for (i=6;i&lt;9;i++) c+=s[i];<br>  if (b&lt;a) {a=b; b=c; }<br>  if (c&lt;a) a=c;<br>  return b-a-1;<br>}<br>};<br></td>