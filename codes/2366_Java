            import java.util.*;<br> <br>public class MagicSquare{<br>  public int missing(int[] s){<br>    int n1=s[0]+s[1]+s[2];<br>    int n2=s[3]+s[4]+s[5];<br>    int n3=s[6]+s[7]+s[8];<br>    if(n1==n2){<br>      return n1-n3-1;<br>    }<br>    else if(n1==n3){<br>      return n1-n2-1;<br>    }<br>    else<br>      return n2-n1-1;<br>      <br> <br>  }<br>  public static void main(String[] args){<br>    MagicSquare sample = new MagicSquare();<br>  }<br>  //-------------Routin Functions----------------//<br>  private boolean isPrime(long number){for(long i = 2; i &lt; number; i++){if(number % i == 0)return false;}return true;}<br>  private int gcd(int a,int b){for(int c;b&gt;0;c=a,a=b,b=c%b);return a;}<br>  private int lcm(int a,int b){return a/gcd(a,b)*b;}<br>}<br></td>