            using System;<br>using System.Collections;<br>using System.Text.RegularExpressions;<br><br>public class NumericalIntegral <br>{<br>  const int slices = 1000000;<br><br>  public string integrate(double x1, double x2) <br>  {<br>    double sliceSize = (x2 - x1) / slices;<br>    double halfSliceSize = sliceSize / 2.0;<br><br>    double total = 0.0;<br>    for(int i = 0; i &lt; slices; ++i)<br>    {<br>      double x = x1 + (i * sliceSize) + halfSliceSize;<br>      double y = Math.Exp(-x * x);<br>      double thisArea = y * sliceSize;<br>      total += thisArea;<br>    }<br><br>    string ret = string.Format("{0:F5}",total);<br>    return ret;  <br>  }<br>}<br><br><br><br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester [Jay_Slupesky modified] [25-Jan-2004]<br>// Powered by CodeProcessor<br></td>