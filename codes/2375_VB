            Imports Microsoft.VisualBasic<br>Imports System.Math<br>Imports System<br>Imports System.Collections<br>Imports System.Text<br> <br>Public Class NumericalIntegral<br>    Public Function integrate(ByVal x1 As Double, ByVal x2 As Double) As String<br>        Dim ret As Double = integ(x1, x2, trapez(x1, x2))<br>        Return String.Format("{0:0.00000}", ret)<br>    End Function<br> <br>    Public Function integ(ByVal x1 As Double, ByVal x2 As Double, ByVal t As Double) As Double<br>        Dim x As Double = x1 + (x2 - x1) / 2<br>        Dim v1, v2 As Double<br>        v1 = trapez(x1, x)<br>        v2 = trapez(x, x2)<br>        If Abs(v1 + v2 - t) &lt; 0.0000001 Then Return v1 + v2<br>        v1 = integ(x1, x, v1)<br>        v2 = integ(x, x2, v2)<br>        Return v1 + v2<br>    End Function<br> <br>    Public Function trapez(ByVal x1 As Double, ByVal x2 As Double) As Double<br>        Dim f1 As Double = Exp(-x1 * x1)<br>        Dim f2 As Double = Exp(-x2 * x2)<br>        Return (f1 + (f2 - f1) / 2) * Abs(x2 - x1)<br>    End Function<br>End Class<br></td>