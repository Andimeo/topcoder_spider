            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;stdio.h&gt;<br>#include &lt;stack&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;deque&gt;<br>#include &lt;stdlib.h&gt;<br>using namespace std;<br>int min(int a,int b)<br>{if (a&lt;=b) return a; else return b;}<br> <br>int max(int a,int b)<br>{if (a&gt;=b) return a; else return b;}<br> <br>int gcd(int a, int b)<br>{<br>  if (b==0) return a; else<br>  return gcd(b,a%b);<br>}<br>class Hilbert {<br>  public:<br>  int steps(int k, int x, int y) ;<br>};<br>int Hilbert::steps(int k, int x, int y)<br>{<br>  int i,j,bok,ans,nr_cw;<br>  bok=1;<br>  for (i=1; i&lt;k; i++) bok*=2;<br>  ans=0;<br>  while (k&gt;0)<br>  {<br>    nr_cw=0;<br>    //printf("x %d y %d ans %d\n",x,y,ans);<br>    if (x&gt;bok) {nr_cw+=2; x-=bok;}<br>    if (y&gt;bok) {nr_cw+=1; y-=bok;}<br>    if (nr_cw==3) nr_cw=2; else<br>    if (nr_cw==2) nr_cw=3;<br>    ans+=nr_cw*bok*bok;<br>    if (nr_cw==0) {j=x; x=y; y=j; }<br>    if (nr_cw==3) {j=x; x=y; y=j; y=bok+1-y; x=bok+1-x;}<br>    k--; bok/=2;<br>  }<br>  return ans;<br>}<br> <br>// Powered by PopsEdit<br></td>