            #include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;set&gt;<br>#include &lt;iostream&gt;<br>#include &lt;map&gt;<br>#include &lt;functional&gt;<br>#include &lt;deque&gt;<br>#include &lt;sstream&gt;<br>#include &lt;numeric&gt;<br><br>using namespace std;<br><br>class TandemRepeats {<br>  public:<br>  int maxLength(string dna, int k) {<br>    int beast = 0;<br>    for( int s = 0; s &lt; dna.size(); s++ ) {<br>      for( int d = 1; d &lt;= ( dna.size() - s ) / 2; d++ ) {<br>        int dif = 0;<br>        for( int m = 0; m &lt; d; m++ )<br>          if( dna[ s + m ] != dna[ s + d + m ] )<br>            dif++;<br>        if( dif &lt;= k &amp;&amp; d &gt; beast )<br>          beast = d;<br>      }<br>    }<br>    return beast;<br>  }<br>};<br><br><br><br>// Powered by FileEdit<br></td>