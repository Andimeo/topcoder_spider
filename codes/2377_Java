            import java.util.*;<br>public class TandemRepeats {<br>  int match(String a, String c)<br>  {<br>    int s=0;<br>    for (int i=0;i&lt;a.length();i++) <br>    if (a.charAt(i)!=c.charAt(i)) s++;<br>    return s;<br>  }<br>  public int maxLength(String dna, int k) {<br>    for (int i=dna.length(); i&gt;=1; i--)<br>    {<br>      for (int j=0; j +i+i &lt;=dna.length(); j++)<br>      if (match(dna.substring(j, j+i), dna.substring(j+i, j+i+i)) &lt;= k)<br>      return i;<br>    }    <br>    return 0;<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>