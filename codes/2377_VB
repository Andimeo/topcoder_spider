<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>public class TandemRepeats<br>{<br>  public int maxLength(string dna,int k)<br>  {<br>    for(int len=dna.Length/2;len&gt;0;len--)<br>    {<br>      for(int start1 = 0; start1 + 2*len &lt;= dna.Length; start1++)<br>      {<br>        int start2 = start1 + len;<br>          int off = 0;<br>          for(int i=0;i&lt;len;i++)<br>          {<br>            if(dna[start1+i] != dna[start2+i]) off++;<br>          }<br>          if(off &lt;= k) return len;<br>        <br>      }<br>    }<br>    return 0;<br>  }<br>}<br></td>