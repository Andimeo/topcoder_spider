            using System;<br>using System.Text;<br>using System.Collections;<br><br>public class SimpleIO {<br><br>  public string gets(string p, int i, int t)<br>  {<br>    string s = "";<br><br>    while (i != t)<br>    {<br>      i = (i+1) % p.Length;<br>      s += p[i];<br>    }<br>    return s;<br>  }<br><br><br>  public string getl(string p, int i, int t)<br>  {<br>    string s = "";<br><br>    while (i != t || s.Length &lt; p.Length)<br>    {<br>      i = (i+1) % p.Length;<br>      s += p[i];<br>    }<br>    return s;<br>  }<br><br><br>  public int worstCase(string p, int t) <br>  {<br><br>    int n = p.Length;<br><br>    string [] ss = new string[n];<br>    string [] ll = new string[n];<br>    int [] v = new int[n];<br><br>    for(int i = 0; i&lt;n; i++)<br>    {<br>      ss[i] = gets(p, i, t);<br>      ll[i] = getl(p, i, t);<br>    }<br><br>    int worst = 0;<br><br>    for(int j = 0; j&lt;n; j++)<br>    {<br>      int i;<br><br>      for(i = 0; i&lt;n; i++)<br>        if (i != j &amp;&amp; ll[i].StartsWith(ss[j])) break;<br>        <br>      if (i&lt;n)<br>      {<br>        for(i = 0; i&lt;n; i++)<br>          if (i != j &amp;&amp; ll[i].StartsWith(ll[j])) break;<br><br>        if (i&lt;n) return -1;<br><br>        v[j] = ll[j].Length;<br>      } else<br>      v[j] = ss[j].Length;<br><br>      worst = Math.Max(worst, v[j]);<br><br><br>    }<br>    return worst;<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester for c# [12-oct-2003]<br>// Powered by CodeProcessor<br></td>