            using System;<br>using System.Collections;<br>public class Percents {<br>  public int minSamples(string percent) {<br>    int ret = 0;<br>    int p = int.Parse(percent.Substring(0,2)) * 100 + int.Parse(percent.Substring(3,2));<br>    <br>    for (int i = 1; i &lt; 10001; i++) {<br>      for (int j = 0; j &lt;= i; j++) {<br>        double z = ((double)j * 10000.0 / (double)i) + .5;<br>        if (p == (int) z) {<br>          return(i);<br>        }<br>      }<br>    }<br>    <br>    return (ret);<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>