            import java.util.*;<br> <br>public class Percents<br>{<br>    public int minSamples(String percent)<br>    {<br>        <br>        int v = Integer.parseInt(""+percent.charAt(0)+percent.charAt(1)+percent.charAt(3)+percent.charAt(4));<br>        for (int i=1; i &lt;= 10000; i++)<br>        {<br>            int n = (v * i) / 10000;<br>            if (test(n, i, v)) return i;<br>            if (test(n+1, i, v)) return i;<br>            if (test(n-1, i, v)) return i;<br>        }<br>        return 1;<br>    }<br>    <br>    boolean test(int n, int i, int v)<br>    {<br>        double t = ((double)n * 10000) / (double)i;<br>        return (Math.round(t) == v);<br>    }<br> <br>    public static void main(String args[])<br>    {<br>        Percents x = new Percents();<br>     <br>        //Example 0<br>        show(x.minSamples("25.00%"));<br>        show(4);<br>        System.out.println();<br> <br>        //Example 1<br>        show(x.minSamples("66.67%"));<br>        show(3);<br>        System.out.println();<br> <br>        //Example 2<br>        show(x.minSamples("66.66%"));<br>        show(2858);<br>        System.out.println();<br> <br>        //Example 3<br>        show(x.minSamples("71.43%"));<br>        show(7);<br>        System.out.println();<br> <br>        //Example 4<br>        show(x.minSamples("00.00%"));<br>        show(1);<br>        System.out.println();<br> <br>    }<br> <br>    //Prints an int<br>    public static void show(int i)<br>    {<br>        System.out.println(i);<br>    }<br> <br>    //Prints a String<br>    public static void show(String s)<br>    {<br>        System.out.println(s);<br>    }<br>}<br></td>