            // Author:  King Larry Mak<br>// TC Handle:  Larry<br>import java.util.*;<br>public class OddsAndEvens {<br>  public String find(String[] sums, String[] products) {<br>    int N = 0;<br>    for ( int i = 0; i &lt;= 11; i++ )<br>    if ( ( i * ( i - 1 ) / 2 ) == sums.length )  {<br>      N = i;<br>      break;<br>    }<br>    <br>    int[] s = new int[2];<br>    int[] p = new int[2];<br>    <br>    for ( int i = 0; i &lt; sums.length; i++ ) {<br>      if ( sums[i].charAt( 0 ) == 'E' )<br>      s[0]++;<br>      else<br>      s[1]++;<br>    }<br>    for ( int i = 0; i &lt; products.length; i++ ) {<br>      if ( products[i].charAt( 0 ) == 'E' )<br>      p[0]++;<br>      else<br>      p[1]++;<br>    }<br>    <br>    for ( int i = 0; i &lt;= N; i++ ) {<br>      int s2[] = new int[2];<br>      int p2[] = new int[2];<br>      <br>      for ( int j = 0; j &lt; N; j++ )<br>      for ( int k = j + 1; k &lt; N; k++ ) {<br>        int n = ( j &lt; i ) ? 0 : 1;<br>        int n2 = ( k &lt; i ) ? 0 : 1;<br>        <br>        s2[ ( n + n2 ) % 2 ]++;<br>        p2[ ( n * n2 ) % 2 ]++;<br>      }<br>      <br>      if ( s[0] == s2[0] &amp;&amp; s[1] == s2[1] &amp;&amp;<br>      p[0] == p2[0] &amp;&amp; p[1] == p2[1] ) <br>      return "EVEN " + i + " ODD " + (N-i);<br>    }<br>    <br>    return "IMPOSSIBLE";<br>  }<br>  <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>