<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>using System.Collections;<br>public class Cyberline<br>{<br>  public string lastCyberword(string cyberline)<br>  {<br>    string whitespace = ".,()?!&lt;&gt;#&amp;$%_=+;:/~|{}";<br>    foreach(char c in whitespace)<br>      cyberline = cyberline.Replace(c,' ');<br>    string[] ss = cyberline.Split(new char[]{' '});<br>    for(int i=ss.Length-1;i&gt;=0;i--)<br>    {<br>      string s = ss[i].Replace("-","");<br>      if(s != "") return s;<br>    }<br>    return "";<br>  }<br>}<br></td>