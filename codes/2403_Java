            import java.util.*;<br>public class BoxLoader {<br>  public int mostItems(int boxX, int boxY, int boxZ, int itemX, int itemY, int itemZ) {<br>    int max=0;<br>    max=Math.max(max,calc(boxX,boxY,boxZ,itemX,itemY,itemZ));<br>    max=Math.max(max,calc(boxX,boxY,boxZ,itemX,itemZ,itemY));<br>    max=Math.max(max,calc(boxX,boxY,boxZ,itemY,itemX,itemZ));<br>    max=Math.max(max,calc(boxX,boxY,boxZ,itemY,itemZ,itemX));<br>    max=Math.max(max,calc(boxX,boxY,boxZ,itemZ,itemX,itemY));<br>    max=Math.max(max,calc(boxX,boxY,boxZ,itemZ,itemY,itemX));<br>    return max;<br>  }<br>  <br>  public int calc(int a,int b,int c,int d,int e,int f) {<br>//    System.out.print("Trying "+a+"/"+d+" ");<br>//    System.out.print("Trying "+b+"/"+e+" ");<br>//    System.out.println("Trying "+c+"/"+f+" ");<br>    return (a/d)*(b/e)*(c/f);<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>