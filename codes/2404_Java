            // FilterJava<br><br>import java.util.*;<br>public class CutoffRounder {<br>     public int round(String num, String cutoff) {<br>          double n = Double.parseDouble(num);<br>          double c = Double.parseDouble(cutoff);<br>          if( n - c &gt; (int)n )<br>               return (int)n+1;<br>          return (int)n;<br>     }<br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>