<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br> <br>public class GolfScore<br>{<br>  public int tally(int[] parValues, string[] scoreSheet)<br>  {<br>    int iScore = 0;<br> <br>    for(int i = 0; i &lt; parValues.Length; i++)<br>    {<br>      string s = scoreSheet[i];<br>      <br>      if(s == "triple bogey")<br>      {<br>        iScore += parValues[i] + 3;<br>      }<br>      else if (s == "double bogey")<br>      {<br>        iScore += parValues[i] + 2;<br>      }<br>      else if(s == "bogey")<br>      {<br>        iScore += parValues[i] + 1;<br>      }<br>      else if(s == "par")<br>      {<br>        iScore += parValues[i];<br>      }<br>      else if(s == "birdie")<br>      {<br>        iScore += parValues[i] - 1;<br>      }<br>      else if(s == "eagle")<br>      {<br>        iScore += parValues[i] - 2;<br>      }<br>      else if(s == "albatross")<br>      {<br>        iScore += parValues[i] - 3;<br>      }<br>      else if(s == "hole in one")<br>      {<br>        iScore += 1;<br>      }<br>    }<br> <br>    return iScore;<br>  }<br>}<br></td>