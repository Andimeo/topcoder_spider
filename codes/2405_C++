            #include &lt;algorithm&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cmath&gt;<br> <br> <br>using namespace std;<br> <br> <br>#define fo(i,a,b) for(int i = (a); i &lt; (b); i++)<br>#define fos(i,a,b) for(int i = (a); i &lt; (b).size(); i++)<br> <br>class GolfScore<br>{<br>public:<br>  int tally(vector &lt;int&gt; parValues, vector &lt;string&gt; scoreSheet)<br>  {<br>        int result = 0;<br>    for(size_t i=0; i&lt; parValues.size(); i++)<br>    {<br>      string value = scoreSheet[i];<br>      if (value == "hole in one")<br>      {<br>        result ++;<br>      }<br>      else<br>      {<br>        result += parValues[i];<br> <br>        if (value == "triple bogey")<br>        {<br>          result += 3;<br>        }<br>        else if (value == "double bogey")<br>        {<br>          result += 2;<br>        }<br>        else if (value == "bogey")<br>        {<br>          result ++;<br>        }<br>        else if (value == "birdie")<br>        {<br>          result --;<br>        }<br>        else if (value == "eagle")<br>        {<br>          result -= 2;<br>        }<br>        else if (value == "albatross")<br>        {<br>          result -= 3;<br>        }<br>      }<br>    }<br> <br>    return result; <br>  }<br>  <br>};<br> <br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br> <br>// Powered by PopsEdit<br></td>