<td class="problemText" colspan="8" valign="middle" align="left">
            import java.util.*;<br> <br>public class GolfScore<br>{<br>  public int tally(int[] parValues, String[] scoreSheet)<br>  {<br>    int sum= 0;<br>    for(int i=0; i&lt;parValues.length; i++)<br>    {<br>      if(scoreSheet[i].equals("triple bogey")) sum+= parValues[i]+3;<br>      if(scoreSheet[i].equals("double bogey")) sum+= parValues[i]+2;<br>      if(scoreSheet[i].equals("bogey")) sum+= parValues[i]+1;<br>      if(scoreSheet[i].equals("par")) sum+= parValues[i];<br>      if(scoreSheet[i].equals("birdie")) sum+= parValues[i]-1;<br>      if(scoreSheet[i].equals("eagle")) sum+= parValues[i]-2;<br>      if(scoreSheet[i].equals("albatross")) sum+= parValues[i]-3;<br>      if(scoreSheet[i].equals("hole in one")) sum+= 1;<br>    }<br>    return sum;<br> <br>  }<br>}<br></td>