<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>using System.Text;<br>using System.Collections;<br>using System.Collections.Generic;<br>public class MusicalChairs<br>{<br>    public string knockedOut(string[] player, string chairs, int seconds)<br>    {<br>         int n=player.Length;<br>         Dictionary&lt;int,string&gt;dic=new Dictionary&lt;int,string&gt;();<br>         int index=0;<br>         for(int i=0;i&lt;n;++i)<br>         {  <br>            if(chairs[i]=='.')index=i;<br>            dic[(i+seconds)%n]=player[i];<br>         }<br>         return dic[index];<br>    }<br>}<br></td>