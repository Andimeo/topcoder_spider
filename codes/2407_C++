            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br> <br> <br>using namespace std;<br> <br>class MusicalChairs {<br>public:<br>  string knockedOut(vector &lt;string&gt;, string, int);<br>};<br> <br>string MusicalChairs::knockedOut(vector &lt;string&gt; a, string b, int seconds) {<br>  int s = 0;  int n =  b . size ();<br> <br>  for ( int i = 0; i &lt; n ;i ++ )<br>    if ( b[i] == '.' )<br>      s = i;<br>  s -= seconds;<br>  s %= n;<br>  s += n;<br>  s %= n;<br>  return a[s];<br>}<br> <br> <br> <br> <br> <br> <br> <br> <br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>