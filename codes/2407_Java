            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br><br>public class MusicalChairs<br>{<br>  public String knockedOut(String[] p, String c, int s)<br>  {<br>    int[] t = new int[p.length];<br>    s %= p.length;<br>    for(int i = 0; i &lt; t.length; ++i)<br>    {<br>      t[i] = i;<br>      for(int j =  0; j &lt; s; ++j)<br>      {<br>        t[i] = (t[i] + t.length - 1) % t.length;<br>      }<br>    }<br>    System.out.println(Arrays.toString(t));<br>    int index = c.indexOf('.');<br>    return p[t[index]];<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br><br><br>// Powered by FileEdit<br></td>