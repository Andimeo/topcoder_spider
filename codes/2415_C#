            using System;<br>using System.Collections;<br>using System.Text;<br>public class WindowManager {<br>    public string[] screen(int height, int width, string[] windows) {<br>        char[][] result = new char[height][];<br>        for(int i=0;i&lt;result.Length;i++) {<br>            result[i] = new char[width];<br>        }<br>        for(int i=0;i&lt;height;i++) {<br>            for(int j=0;j&lt;width;j++){<br>                result[i][j]=' ';<br>            }<br>        }<br>        foreach (string win in windows) {<br>            string[] foo = win.Split(new char[]{' '});<br>            int voff = Convert.ToInt32(foo[0]);<br>            int hoff = Convert.ToInt32(foo[1]);<br>            int vsz = Convert.ToInt32(foo[2]);<br>            int hsz = Convert.ToInt32(foo[3]);<br>            char fill = foo[4][0];<br>            for(int i=0;i&lt;height;i++) {<br>                for(int j=0;j&lt;width;j++){<br>                    if(b(i,voff,voff+vsz-1)) {<br>                        if (b(j,hoff,hoff+hsz-1)) {<br>                            result[i][j] = fill;<br>                        }<br>                        else if (j==hoff || j== hoff+hsz-1) {<br>                            result[i][j] = '|';<br>                        }<br>                    }<br>                    else if (i == voff || i == voff+vsz-1) {<br>                        if (b(j,hoff,hoff+hsz-1)) {<br>                            result[i][j] = '-';<br>                        }<br>                        else if (j==hoff || j== hoff+hsz-1) {<br>                            result[i][j] = '+';<br>                        }<br><br>                    }<br>                }<br>            }<br>        }<br>        string[] temp = new string[height];<br>        for(int i=0;i&lt;temp.Length;i++) {<br>            temp[i] = new string(result[i]);<br>        }<br>        return temp;<br>    }<br><br>    public bool b(int x, int y, int z) {return x &gt;y  &amp;&amp; x &lt; z;}<br>        <br>   public static void Main(string[] args) {}<br>}<br><br><br>// Powered by FileEdit<br></td>