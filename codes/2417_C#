            using System;<br>using System.Collections;<br>public class GravityBomb {<br>  public string[] aftermath(string[] board) {<br>    // Begin Your Code Here...<br>    int[] count = new int[board[0].Length];<br>    for (int i = 0; i &lt; count.Length; i++)<br>      count[i] = 0;<br>    for (int i = 0; i &lt; board.Length; i++) <br>      for (int j = 0; j &lt; count.Length; j++)<br>        if (board[i][j] == 'X')<br>          count[j]++;<br>    int min = count[0];<br>    for (int i = 1; i &lt; count.Length; i++)<br>      if (count[i] &lt; min)<br>        min = count[i];<br>    for (int i = 0; i &lt; count.Length; i++)<br>      count[i] -= min;<br>    char[,] ret = new char[board.Length,count.Length];<br>    for (int i = board.Length-1; i &gt;= 0; i--) {<br>      for (int j = 0; j &lt; count.Length; j++) {<br>        if (count[j] &gt; 0) {<br>          ret[i,j] = 'X';<br>          count[j]--;<br>        }<br>        else<br>          ret[i,j] = '.';<br>      }<br>    }<br>    string[] ret2 = new string[board.Length];<br>    for (int i = 0; i &lt; ret2.Length; i++) {<br>      ret2[i] = "";<br>      for (int j = 0; j &lt; count.Length; j++)<br>        ret2[i] += ret[i,j];<br>    }<br>    return ret2;<br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>