            #include &lt;stdio.h&gt;<br>#include &lt;stdlib.h&gt;<br>#include &lt;string.h&gt;<br>#include &lt;vector.h&gt;<br>#include &lt;math.h&gt;<br>#include &lt;ctype.h&gt;<br> <br>using namespace std;<br> <br>class GravityBomb {<br>  public:<br>  vector &lt;string&gt; aftermath (vector &lt;string&gt; a) {<br>    int n,m,i,j,k;<br>    n = a.size();<br>    m = a[0].size();<br>    int cnt[100];<br>    for (i = 0; i &lt; m; i++) cnt[i] = 0;<br>    vector &lt;string&gt; ans;<br>    ans.assign(n,"");<br>    for (i = 0; i &lt; n; i++) {<br>      for (j = 0; j &lt; m; j++) {<br>        ans[i] = ans[i] + '.';<br>      }<br>    }<br>    for (i = 0; i &lt; m; i++) {<br>      for (j = 0; j &lt; n; j++) {<br>        if (a[j][i] == 'X') cnt[i]++;<br>      }<br>    }<br>    k = 100;<br>    for (i = 0; i &lt; m; i++) {<br>      if (cnt[i] &lt; k) k = cnt[i];<br>    }<br>    for (i = 0; i &lt; m; i++) {<br>      for (j = 0; j &lt; cnt[i] - k; j++) {<br>        ans[n-j-1][i] = 'X';<br>      }<br>    }<br>    return ans;<br>  }<br>};<br></td>