            // FilterC#<br><br><br>using System;<br>using System.Collections;<br><br>public class ThirtyOne {<br><br>  private int p2(string s)<br>  {<br>    string[] t=s.Split(' ');<br>    if(t[0]==t[1]&amp;&amp;t[1]==t[2]) return 61;<br>    int r=0;<br>    int ac=0;<br>    foreach(string tt in t)<br>    {<br>      if(tt=="A") {ac++; continue;}<br>      if(tt.Length==1&amp;&amp;Char.IsLetter(tt[0]))<br>      {<br>        r+=10;<br>      }<br>      else r+=INT(tt);<br>    }<br><br>    while(ac&gt;0)<br>    {<br>      if(r&lt;=20) r+=11;<br>      else r++;<br>      ac--;<br>    }<br><br>    return r*2;<br>  }<br><br>  public int findWinner(string[] hands) <br>  {<br>    int mp=0;<br>    int mi=-1;<br>    for(int i=0;i&lt;hands.Length;i++)<br>    {<br>      int tp=p2(hands[i]);<br>      if(tp&gt;mp) {mp=tp; mi=i;}<br>    }<br><br>    return mi;<br>  }<br><br><br>  private static ArrayList BitCount(int n){ ArrayList r = new ArrayList(); int idx=0; while(n &gt; 0) { if((n&amp;1)==1) r.Add(idx); n &gt;&gt;= 1; idx++;} return r; }<br>  private static int Gcd(int a, int b) { while(b!=0) {int t=a; a=b; b=t%b;} return a;}<br>  private static int Lcm(int a, int b) { return a*b/Gcd(a,b); }<br>  private static char[] CA(ArrayList ar) { return (char []) ar.ToArray(typeof(char)); }<br>  private static int[] IA(ArrayList ar) { return (int []) ar.ToArray(typeof(int)); }<br>  private static long[] LA(ArrayList ar) { return (long []) ar.ToArray(typeof(long)); }<br>  private static double[] DA(ArrayList ar) { return (double []) ar.ToArray(typeof(double)); }<br>  private static string[] SA(ArrayList ar) { return (string[]) ar.ToArray(typeof(string)); }<br>  private static int INT(Object x) { return Int32.Parse(x.ToString()); }<br>  private static long LNG(Object x) { return Int64.Parse(x.ToString()); }<br>  private static double DBL(Object x) { return Double.Parse(x.ToString()); }<br>  private static int MinI(IEnumerable a) { int m = Int