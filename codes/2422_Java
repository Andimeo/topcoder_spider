            import java.util.*;<br>public class ThirtyOne {<br>    double score(String [] t) {<br>        int tot = 0;<br>        for(int i = 0; i &lt; t.length; i++) {<br>            try {<br>                int v = Integer.parseInt(t[i]);<br>                tot += v;<br>            }<br>            catch(Exception e) {<br>                if(t[i].charAt(0) == 'A')<br>                    tot += 11;<br>                else tot += 10;<br>            }<br>        }<br>        if(t[0].equals(t[1]) &amp;&amp; t[1].equals(t[2]))<br>            return 30.5;<br>        while(tot &gt; 31)<br>            tot -= 10;<br>        return tot;<br>    }<br>    <br>    public int findWinner(String[] hands) {<br>        double best = 0;<br>        int num = -1;<br>        for(int i = 0; i &lt; hands.length; i++) {<br>            String [] t = hands[i].split(" ");<br>            double score = score(t);<br>            if(score &gt; best) {<br>                best = score;<br>                num = i;<br>            }<br>        }<br>        return num;<br>    }<br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>