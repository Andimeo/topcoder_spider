            import java.util.*;<br>public class EigenVector {<br>  int n;<br>  int[][] a;<br>  private int[] fun(int[] x, int b, int s, int l, boolean z) {<br>    if (b == x.length) {<br>      //print(x);print("\n");<br>      int[] y = new int[n];<br>      for (int i = 0; i &lt; n; i++)<br>        for (int j = 0; j &lt; n; j++) y[i] += a[i][j] * x[j];<br>      int kn = 0;<br>      int kd = 0;<br>      for (int i = 0; i &lt; n; i++) {<br>        if (x[i] == 0 ^ y[i] == 0) return null;<br>        if (x[i] != 0) {<br>          if (kn == 0) {<br>            kn = y[i];<br>            kd = x[i];<br>          } else {<br>            int q = x[i] * kn;<br>            if (q % kd != 0 || q / kd != y[i]) return null;<br>          }<br>        }<br>      }<br>      return x;<br>    }<br>    if (b + 1 == x.length) {<br>      if (!z) {<br>        x[b] = s - l;<br>        int[] r = fun(x, b + 1, l, l, false);<br>        if (r != null) return r;<br>      }<br>      x[b] = l - s;<br>      int[] r = fun(x, b + 1, l, l, false);<br>      return r;<br>    }<br>    int m = l - s;<br>    int m1 = z ? 0 : -m;<br>    for (int i = m1; i &lt;= m; i++) {<br>      x[b] = i;<br>      int[] r = fun(x, b + 1, s + Math.abs(i), l, z &amp;&amp; i == 0);<br>      if (r != null) return r;<br>    }<br>    return null;<br>  }<br>  public int[] findEigenVector(String[] A) {<br>    n = A.length;<br>    a = new int[n][n];<br>    for (int i = 0; i &lt; n; i++) {<br>      String[] b = A[i].split(" ");<br>      for (int j = 0; j &lt; n; j++)<br>        a[i][j] = Integer.parseInt(b[j]);<br>    }<br>    for (int i = 1; i &lt;= 9; i++) {<br>      int[] r = fun(new int[n], 0, 0, i, true);<br>      if (r != null) return r;<br>    }<br>    return new int[] {3, 1, 4, 1, 5};<br>  }<br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>