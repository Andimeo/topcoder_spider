            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class MnemonicMemory<br>{<br>  public String getPhrase(String number, String[] dictionary)<br>  {<br>    Arrays.sort(dictionary);<br>    String ret = "";<br>    boolean[] used = new boolean[dictionary.length];<br>    for(int i=0; i&lt;number.length(); i++){<br>      int n = Integer.parseInt(String.valueOf(number.charAt(i)));<br>      boolean found = false;<br>      for(int j=0; j&lt;dictionary.length; j++){<br>        if(dictionary[j].length()==n&amp;&amp;!used[j]){<br>          ret+=dictionary[j]+ " ";<br>          found = true;<br>          used[j]=true;<br>          break;<br>        }<br>      }<br>      <br>      if(!found)<br>        return "";<br>    <br>    }<br>    <br>    return ret.substring(0,ret.length()-1);<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>