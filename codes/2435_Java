            import java.util.*;<br>public class OmahaLow {<br>  <br>  public String low(String sharedCards, String playersCards) {<br>    String sc = sharedCards;<br>    String pc = playersCards;<br>    <br>    int a1, a2;<br>    int b1, b2, b3;<br>    char c[] = new char[5];<br>    <br>    String rv = "99999";<br>    for(a1=0;a1&lt;4;a1++) for(a2=0;a2&lt;4;a2++) {<br>      if(a1==a2) continue;<br>      for(b1=0;b1&lt;5;b1++) for(b2=0;b2&lt;5;b2++) for(b3=0;b3&lt;5;b3++) {<br>        if(b1==b2||b1==b3||b2==b3) continue;<br>        c[0] = pc.charAt(a1);<br>        c[1] = pc.charAt(a2);<br>        c[2] = sc.charAt(b1);<br>        c[3] = sc.charAt(b2);<br>        c[4] = sc.charAt(b3);<br>        boolean good = true;<br>        int i, j;<br>        for(i=0;i&lt;5&amp;&amp;good;i++) for(j=i+1;j&lt;5&amp;&amp;good;j++) {<br>          if(c[i]==c[j]) good = false;<br>        }<br>        if(!good) continue;<br>        for(i=0;i&lt;5&amp;&amp;good;i++) {<br>          if(c[i]&lt;'1'||c[i]&gt;'8') good = false;<br>        }<br>        if(!good) continue;<br>        Arrays.sort(c);<br>        String tstr = "";<br>        for(i=4;i&gt;=0;i--) tstr += c[i];<br>        if(tstr.compareTo(rv)&lt;0) rv = tstr;<br>      }<br>    }<br>    if(rv.equals("99999")) return "";<br>    return rv;<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>