            public class ChangePurse {<br>  public int[] optimalCoins(int[] coinTypes, int value) {<br>    int i,j,k,n;<br>    n=coinTypes.length;<br>    int id[]=new int[n];<br>    for(i=0;i&lt;n;i++) id[i]=i;<br>    for(i=0;i&lt;n-1;i++)<br>      for(j=i+1;j&lt;n;j++)<br>        if(coinTypes[i]&lt;coinTypes[j]){<br>          k=coinTypes[i];<br>          coinTypes[i]=coinTypes[j];<br>          coinTypes[j]=k;<br>          k=id[i]; id[i]=id[j];<br>          id[j]=k;<br>        } <br>    int ans[]=new int[n];<br>    for(i=0;i&lt;n;i++) ans[i]=0;<br>    int s=value;<br>    for(i=0;i&lt;n-1;i++){<br>      if((s+1)%coinTypes[i]==0){<br>        ans[id[i]] = s / coinTypes[i];<br>        s=coinTypes[i]-1;<br>      }<br>    }<br>    ans[id[n-1]]=s;<br>    return ans;<br>  }<br>  <br>  public static void main(String[] args) {<br>    ChangePurse changePurse1 = new ChangePurse();<br>    int a[]=new int[]{1,25,10};<br>    changePurse1.optimalCoins(a,49);<br>  }<br> <br>}<br></td>