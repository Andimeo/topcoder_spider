            using System;<br>using System.Text;<br>using System.Collections;<br><br>public class DigitMultiples {<br><br>    public int longest(string a, string b)<br>    {<br>        int max = 0;<br>        for(int i=0;i&lt;a.Length;i++)<br>            for (int ii = 0; ii &lt; b.Length; ii++)<br>            {<br>                int cur = 0;<br>                while (i + cur &lt; a.Length &amp;&amp; ii + cur &lt; b.Length &amp;&amp; a[i + cur] == b[ii + cur])<br>                {<br>                    cur++;<br>                }<br>                if (cur &gt; max) max = cur;<br>            }<br>        return max;<br>    }<br><br>    public int getLongest(string single, string multiple) {<br>        int ret = 0;<br>        for (int i = 0; i &lt;= 9; i++)<br>        {<br>            StringBuilder sb = new StringBuilder();<br>            for (int ii = 0; ii &lt; single.Length; ii++)<br>            {<br>                int d = (single[ii] - '0') * i;<br>                if (d &lt; 10)<br>                {<br>                    sb.Append(d.ToString());<br>                }<br>                else<br>                {<br>                    sb.Append('x');<br>                }<br>            }<br>            ret = Math.Max(ret,longest(sb.ToString(),multiple));<br>        }<br>        return ret;<br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>