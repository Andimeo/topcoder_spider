            // Author:  King Larry Mak<br>// TC Handle:  Larry<br>import java.util.*;<br>public class DigitMultiples {<br>  public boolean go( String a, String b, int k ) {<br>    char[] a1 = a.toCharArray();<br>    char[] b1 = b.toCharArray();<br>    <br>    for ( int i = 0; i &lt; a1.length; i++ )<br>    a1[i] -= '0';<br>    for ( int i = 0; i &lt; b1.length; i++ )<br>    b1[i] -= '0';<br>    for ( int i = 0; i &lt; a1.length; i++ )<br>    a1[i] *= k;<br>    for ( int i = 0; i &lt; a1.length; i++ )<br>    if ( a1[i] != b1[i] ) return false;<br>    <br>    return true;<br>  }<br>  <br>  public int getLongest(String single, String multiple) {<br>    int m = Math.min( single.length(), multiple.length() );<br>    <br>    int max = 0;<br>    for ( int i = 0; i &lt;= m; i++ ) {<br>      boolean f = false;<br>      for ( int j = 0; j + i &lt;= single.length(); j++ )<br>      for ( int k = 0; k + i &lt;= multiple.length(); k++ )<br>      for ( int l = 0; l &lt; 10; l++ )<br>      f |= go( single.substring( j, j + i ), multiple.substring( k, k + i ), l );<br>      if( f  )<br>      max = Math.max( max, i );<br>    }<br>    return max;<br>  }<br>  <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>