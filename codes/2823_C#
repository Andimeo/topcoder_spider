            using System;<br>using System.Collections;<br><br><br>public class BigCube<br>{<br><br>  public string largestCube(string[] range)<br>  {<br><br>    string result = "";<br>    long[,] r = new long[range.Length,2];<br>    for (long i = 0; i &lt; range.Length; i++)<br>    {<br>      r[i,0] = long.Parse(range[i].Substring(0,range[i].IndexOf("-")));<br>      r[i,1] = long.Parse(range[i].Substring(range[i].IndexOf("-")+1));      <br>    }      <br><br>    for (long i = 100000; i &gt;= 0; i--)<br>    {<br>            long q = i*i*i;<br>      for (int j = 0; j &lt; range.Length; j++)<br>      {<br>                if ((q &lt;= r[j,1])&amp;&amp;(q &gt;= r[j,0]))<br>          return q.ToString();<br>      }<br>    }      <br><br>    return result;<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>