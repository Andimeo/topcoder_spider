            using System;<br>using System.Text;<br>using System.Collections;<br><br>public class Education {<br>    public int minimize(string desire, int[] tests) {<br>        int res;<br>    int sum = 0, ocen = 0;<br>    for (int i = 0; i &lt; tests.Length; i++) sum+= tests[i];<br>    if (desire == "A") ocen = 90; else<br>    if (desire == "B") ocen = 80; else<br>    if (desire == "C") ocen = 70; else<br>    if (desire == "D") ocen = 60;<br>    res = ocen * (tests.Length + 1) - sum;<br>    if (res &gt; 100) return -1;<br>    if (res &lt; 0) return 0;<br>        return res;<br>    }<br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>