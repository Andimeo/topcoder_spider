            import java.util.*;<br>import java.lang.*;<br> <br>public class Education {<br> <br>  public static int minimize(String d, int[] tests)<br>  {<br>    double g = 0;<br>    if (d.equals("A"))<br>      g = 90.0;<br>    if (d.equals("B"))<br>      g = 80.0;<br>    if (d.equals("C"))<br>      g = 70.0;<br>    if (d.equals("D"))  g = 60.0;<br>    int sum = 0;<br>    for (int k =0; k &lt; tests.length;k++)<br>      sum += tests[k];<br>    for (int k = 0; k &lt; 101;k++) {<br>      int t = k + sum;<br>      if (t / (tests.length + 1.0) &gt;= g)<br>        return k;<br>    }<br>    return -1;<br>  }<br> <br>}<br></td>