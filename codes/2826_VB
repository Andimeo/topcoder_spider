            imports microsoft.visualbasic<br> <br>public class Education<br> <br>public function minimize(desire as string, tests as integer()) as integer<br>  dim desireNum as integer  <br> <br>  select case desire<br>    case "A"<br>      desireNum = 90<br>    case "B"<br>      desireNum = 80<br>    case "C"<br>      desireNum = 70<br>    case else <br>      desireNum = 60<br>  end select<br> <br>  dim testsTotal as double = 0.00<br>  dim x as integer = 0<br> <br>  for x = 0 to ubound(tests) <br>    testsTotal += tests(x)<br>  next x<br> <br>  dim grade as double<br>  grade = desireNum * (ubound(tests) + 2) - testsTotal<br>  <br>  if grade &gt; 100 then<br>    return -1<br>  else if grade &lt; 0 then<br>    return 0<br>  end if<br> <br>  return grade<br>end function<br> <br> <br> <br>end class<br></td>