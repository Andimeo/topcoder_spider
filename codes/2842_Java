            import java.util.*;<br>public class Sortitaire {<br>  public int turns(int[] a, int[] b) {<br>    for (int c = 0; c &lt;= b.length; c++) {<br>      int[] t = (int[]) a.clone();<br>      Arrays.sort(b, 0, c);<br>      int j = 0;<br>      for (int i = 0; i &lt; c; i++) {<br>        while (j &lt; t.length &amp;&amp; t[j] &lt;= b[i] &amp;&amp; (j == 0 || t[j - 1] &lt;= t[j])) j++;<br>        if (j &lt; t.length) t[j] = b[i];<br>      }<br>      boolean f = true;<br>      for (; j &lt; t.length; j++)<br>        if (j != 0 &amp;&amp; t[j - 1] &gt; t[j]) f = false;<br>      if (f) return c;<br>    }<br>    return -1;<br>  }<br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>