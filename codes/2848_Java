            import java.util.*;<br>import java.math.*;<br> <br> <br>public class ShoelaceLength<br>{<br>  public double calculate(int startWidth, int endWidth, int spread, int numPairs)<br>  {<br>    double tLength = startWidth;<br>    <br>    double incr = (double)(endWidth - startWidth) / (double)(numPairs - 1);<br>    double newWidth = startWidth;<br>    <br>    for (int i = 0; i &lt; numPairs - 1; i++) {<br>      newWidth += incr;<br>      System.out.println(newWidth);<br>      double l = Math.sqrt(spread*spread + (newWidth-incr/2.0)*(newWidth-incr/2.0));<br>      tLength += 2*l;<br>    }<br>    <br>    return tLength;<br>    <br>  }<br>}<br></td>