            using System;<br> <br>public class Chivalry<br>{<br>  public  string getOrder(string first, string second)<br>  {<br>    string strout = "";<br>    int i =0, j=0;<br>    while (i &lt; first.Length || j &lt; second.Length)<br>    {<br>      if (i == first.Length)<br>      {<br>        strout += second[j];<br>        j++;<br>      }<br>      else if (j == second.Length)<br>      {<br>        strout += first[i];<br>        i++;<br>      }<br>      else if (first[i] == second[j])<br>      {<br>        strout += first[i];<br>        i++;<br>      }<br>      else if (first[i] == 'M' &amp;&amp; second[j] == 'W')<br>      {<br>        strout += "W";<br>        j++;<br>      }<br>      else<br>      {<br>        strout += first[i];<br>        i++;<br>      }<br>    }<br>    return strout;  <br>  }<br>}<br></td>