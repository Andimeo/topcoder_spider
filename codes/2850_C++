            // Libraries<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;vector&gt;<br>#include &lt;queue&gt;<br>#include &lt;string&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;cmath&gt;<br> <br>// Some useful macros<br>#define FOR(i,n) for(int i=0;i&lt;n;i++)<br>#define REP(i,f,t) for(int i=f;i&lt;=t;i++)<br>#define deb(a) cout &lt;&lt; #a &lt;&lt; " : " &lt;&lt; a &lt;&lt; " ";<br>#define debln(a) cout&lt;&lt; #a &lt;&lt;" : " &lt;&lt; a &lt;&lt; endl;<br>#define ALL(v) v.begin(),v.end()<br>#define UNIQUE(v) v.erase(unique(ALL(v)),v.end())<br>#define MIN(c) *min_element(ALL(c))<br>#define MAX(c) *max_element(ALL(c))<br>#define SUM(c) accumulate(ALL(c),0)<br>using namespace std;<br>typedef vector&lt;int&gt; VI;<br> <br>class Chivalry {<br>  public:<br>  string getOrder(string first, string second) {<br>    int i=0,j=0;<br>    string result;<br>    while(i&lt;first.size() &amp;&amp; j&lt;second.size()) {<br>      if(first[i] == 'W') { result+='W'; i++; }<br>      else if(second[j] == 'W') { result += 'W'; j++; }<br>      else if(first[i] == second[j] &amp;&amp; first[i] == 'M') {<br>        result+='M';<br>        i++;<br>      }<br>    }<br>    while(i&lt;first.size()) {<br>      result+=first[i++];<br>    }<br>    while(j&lt;second.size()) { result+=second[j++]; }<br>    return result;<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>