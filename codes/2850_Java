            import java.util.*;<br>public class Chivalry {<br>    public String getOrder(String first, String second) {<br>        String res = "";<br>        int c1 = 0;<br>        int c2 = 0;<br>        while (c1 != first.length() &amp;&amp; c2 != second.length())<br>        {<br>          char ch1 = first.charAt(c1);<br>          char ch2 = second.charAt(c2);<br>          if (ch1 == 'M' &amp;&amp; ch2 == 'W')<br>          {<br>            res = res + "W";<br>            c2++;<br>          }<br>          else<br>          {<br>            if (ch1 == 'W' &amp;&amp; ch2 == 'M')<br>            {<br>              res = res + "W";<br>              c1++;<br>            }<br>            else<br>            {<br>              c1++;<br>              res = res + ch1;<br>            }<br>          }<br>          <br>        }<br>        if (c1 == first.length())<br>        {<br>          res = res + second.substring(c2, second.length());<br>          return res;<br>        }<br>    res = res + first.substring(c1, first.length());<br>    return res;<br>          <br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>