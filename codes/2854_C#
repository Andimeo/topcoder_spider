            public class Aaagmnrs {<br>    public string[] anagrams(string[] phrases) {<br>        string[] res;<br>    int i,j;<br>    int noRem = phrases.Length;<br>    bool []remove = new bool[phrases.Length];<br>    for (i = 1 ; i &lt; phrases.Length ; i++)<br>    {<br>      for (j = 0 ; j &lt; i ; j++)<br>      {<br>        remove[i] = AreAnagrams(phrases[j],phrases[i]);<br>        if (remove[i]) <br>        {<br>          noRem--;<br>          break;<br>        }<br>      }<br>    }<br>    res = new string[noRem];<br>    j = 0;<br>    for (i = 0 ; i &lt; phrases.Length ; i++)<br>    {<br>      if (!remove[i])<br>      {<br>        res[j] = phrases[i];<br>        j++;<br>      }<br>    }<br>        return res;<br>    }<br> <br>  private bool AreAnagrams(string a, string b)<br>  {<br>    int ind;<br>    a = a.ToLower();<br>    b = b.ToLower();<br>    for (int i = 0 ; i &lt; a.Length ; i++)<br>    {<br>      if (a[i]!=' ') <br>      {<br>        ind = b.IndexOf(a[i]);<br>        if (ind&gt;=0)<br>        {<br>          b = b.Remove(ind,1);<br>        }<br>        else<br>        {<br>          return false;<br>        }<br>      }<br>    }<br>    for (int i = 0 ; i &lt; b.Length ; i++)<br>      if (b[i]!=' ') return false;<br>    return true;<br>  }<br>}<br></td>