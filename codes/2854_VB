            Imports System.Collections<br> <br>Public Class Aaagmnrs<br>    Public Function anagrams(ByVal phrases As String()) As String()<br>        Dim i, j As Integer<br>        For i = 1 To phrases.Length - 1<br>            For j = 0 To i - 1<br>                If Not phrases(j) = String.Empty Then<br>                    If isAn(phrases(i), phrases(j)) Then<br>                        phrases(i) = String.Empty<br>                    End If<br>                End If<br>            Next<br>        Next<br> <br>        Dim ret As New ArrayList<br>        For i = 0 To phrases.Length - 1<br>            If Not phrases(i) = String.Empty Then<br>                ret.Add(phrases(i))<br>            End If<br>        Next<br> <br>        Dim rrr(ret.Count - 1) As String<br>        For i = 0 To rrr.Length - 1<br>            rrr(i) = CStr(ret.Item(i))<br>        Next<br>        Return rrr<br> <br>    End Function<br> <br> <br>    Private Function isAn(ByVal a As String, ByVal b As String) As Boolean<br>        'If a.Length &lt;&gt; b.Length Then Return False<br>        Dim i As Integer<br> <br>        Dim t1 As String<br>        t1 = a<br>        t1 = t1.ToLower<br>        Dim a1 As New ArrayList<br>        For i = 0 To t1.Length - 1<br>            If t1.Chars(i) &lt;&gt; " ".Chars(0) Then<br>                a1.Add(t1.Chars(i))<br>            End If<br>        Next<br>        a1.Sort()<br>        t1 = String.Empty<br>        For i = 0 To a1.Count - 1<br>            t1 &amp;= CStr(a1.Item(i))<br>        Next<br> <br>        Dim t2 As String<br>        t2 = b<br>        t2 = t2.ToLower<br>        Dim a2 As New ArrayList<br>        For i = 0 To t2.Length - 1<br>            If t2.Chars(i) &lt;&gt; " ".Chars(0) Then<br>                a2.Add(t2.Chars(i))<br>            End If<br>        Next<br>        a2.Sort()<br>        t2 = String.Empty<br>        For i = 0 To a2.Count - 1<br>            t2 &amp;= CStr(a2.Item(i))<br>        Next<br> <br>        If t1 = t2 Then<br>            Return True<br>        Else<br>            Return False<br>        End If<br> <br>    End Function<br> <br>End Class<br></td>