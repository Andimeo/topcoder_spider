            using System; using System.Collections; using System.Text; public class RegularSeason { public string[] finalStandings(string[] T, int R) {<br>int N = T.Length;<br>String[] names = new string[N];<br>int[] wins = new int[N];<br> <br>for (int i=0; i&lt;N; i++)<br> {<br> String[] ss = T[i].Split(' ');<br> names[i] = ss[0];<br> for (int j=0; j&lt;N; j++) if (j != i)<br>  {<br>  int p = Convert.ToInt32(ss[j+1]);<br>  wins[i] += p * R;<br>  wins[j] += (100-p) * R;<br>  }<br> }<br> <br>for (int i=0; i&lt;N; i++)<br> for (int j=N-2; j&gt;=i; j--)<br>  if (wins[j] &lt; wins[j+1] || (wins[j] == wins[j+1] &amp;&amp; names[j].CompareTo(names[j+1]) &gt; 0))<br>   {<br>   String ts = names[j]; names[j] = names[j+1]; names[j+1] = ts;<br>   int ti = wins[j]; wins[j] = wins[j+1]; wins[j+1] = ti;<br>   }<br> <br>for (int i=0; i&lt;N; i++)<br> names[i] += " " + ((wins[i] + 50) / 100).ToString();<br>return names;<br>}<br> <br> <br> <br> <br> <br>static void WR(Object x, params object[] p) { Console.Write(x.ToString(), p); }<br>static void WL(Object x, params object[] p) { Console.WriteLine(x.ToString(), p); }<br>static void WL() { WL(""); }<br>static void WS(params Object[] c) { foreach (Object x in c) WR(x.ToString() + " "); }<br>static void WS(Array ar, int X) { for (int x=0; x&lt;X; x++) WS(ar.GetValue(x)); WL(); }<br>static void WS(Array ar, int X, int Y) { for (int y=0; y&lt;Y; y++) { for (int x=0; x&lt;X; x++) WS(ar.GetValue(x,y)); WL(); }}<br>}<br></td>