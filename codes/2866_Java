            import java.util.*;<br>public class RegularSeason{<br>    public String[] finalStandings(String[] teams /*teams*/,<br>                                   int      rounds /*rounds*/){<br>        String[] ret = new String[]{};<br>        Team[] t = new Team[teams.length];<br>        for (int i = 0; i &lt; teams.length; i++) {<br>            t[i] = new Team(teams[i].split(" ")[0], 0);<br>        }<br>        <br>        for (int i = 0; i &lt; teams.length; i++) {<br>            String[] probs = teams[i].split(" ");<br>            for (int j = 1; j &lt; probs.length; j++) {<br>                t[i].prob += Integer.parseInt(probs[j]);<br>                if (j - 1 != i) {<br>                    t[j - 1].prob  += (100 - Integer.parseInt(probs[j]));<br>                }<br>            }<br>        }<br>        for (int i = 0; i &lt; t.length; i++) {<br>            t[i].calc(rounds);<br>        }<br> <br>        Arrays.sort(t, new Comparator() {<br>                public int compare(Object o1, Object o2) {<br>                    Team t1 = (Team) o1;<br>                    Team t2 = (Team) o2;<br>                    if (t1.wins &lt; t2.wins) {<br>                        return 1;<br>                    } else if (t1.wins &gt; t2.wins) {<br>                        return -1;<br>                    } else if (t1.prob &lt; t2.prob) {<br>                        return 1;<br>                    } else if (t1.prob &gt; t2.prob) {<br>                        return -1;<br>                    } else {<br>                        return t1.name.compareTo(t2.name);<br>                    }<br>                }<br>            });<br>        ret = new String[t.length];<br>        for (int i = 0; i &lt; t.length; i++) {<br>            ret[i] = t[i].name + " " + t[i].wins;<br>        }<br>        return ret;<br>    }<br> <br>    public class Team {<br>        public String name;<br>        public int wins;<br>        public int prob;<br> <br>        public Team(String name, int prob) {<br>            this.name = name;<br>            this.prob = prob;<br>        }<br> <br>        public void calc(int rounds) {<br>            prob *= rounds;<br>            wins = (prob + 50) / 100;<br>        }<br>    }<br> <br> <br>}<br></td>