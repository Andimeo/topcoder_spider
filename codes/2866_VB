            imports microsoft.visualbasic<br> <br>Public Class RegularSeason<br>    Public Function finalStandings(ByVal teams As String(), ByVal rounds As Integer) As String()<br>        Dim i, j, t(19, 19), max, wins(19) As Integer<br>        Dim tmp As String()<br> <br>        For i = 0 To teams.Length - 1<br>            tmp = teams(i).Split(" ")<br>            teams(i) = tmp(0)<br>            For j = 0 To teams.Length - 1<br>                t(i, j) = Val(tmp(j + 1))<br>            Next<br>        Next<br> <br>        For i = 0 To teams.Length - 1<br>            For j = 0 To teams.Length - 1<br>                If i &lt;&gt; j Then wins(i) += (t(i, j) + 100 - t(j, i)) * rounds<br>            Next<br>        Next<br> <br>        Dim res(teams.Length - 1) As String<br>        For i = 0 To teams.Length - 1<br>            max = 0<br>            For j = 0 To teams.Length - 1<br>                If Fix((wins(j) + 50) / 100) &gt; Fix((wins(max) + 50) / 100) Then<br>                    max = j<br>                ElseIf Fix((wins(j) + 50) / 100) = Fix((wins(max) + 50) / 100) AndAlso (wins(j) &gt; wins(max)) Then<br>                    max = j<br>                ElseIf (wins(j) = wins(max)) AndAlso teams(j) &lt; teams(max) Then<br>                    max = j<br>                End If<br>            Next<br>            res(i) = teams(max) &amp; " " &amp; LTrim(Fix((wins(max) + 50) / 100))<br>            wins(max) = -1<br>        Next<br> <br>        Return res<br>    End Function<br>End Class<br></td>