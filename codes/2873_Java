            import java.util.*;<br>import java.io.*;<br>import java.text.*;<br>import java.util.regex.*;<br>public class ChartError {<br>  double[] vals;<br>  public double minErr(String[] val) {<br>    vals = new double[val.length+1];<br>    for(int i = 0; i &lt; val.length; i++){<br>      vals[i] = Double.parseDouble(val[i]);<br>    }<br>    vals[vals.length-1] = 0;<br>    double ret = Double.MAX_VALUE;<br>    Arrays.sort(vals);<br>    for(int i = 0; i &lt; vals.length-1; i++){<br>      for(int j = 1; j &lt; 16; j++){<br>        double lo = vals[i]/j;<br>        double hi = vals[i+1]/j;<br>        while(hi-lo &gt; 1e-10){<br>          double lower = (2*lo+hi)/3;<br>          double high = (lo+2*hi)/3;<br>          if(getDist(lower) &lt; getDist(high)) hi = high;<br>          else lo = lower;<br>        }<br>        ret = Math.min(ret, getDist((lo+hi)/2));<br>      }<br>    }<br>    return ret;<br>  }<br>  double getDist(double val){<br>    double ret = 0;<br>    for(int i = 0; i &lt; vals.length; i++){<br>      double best = Double.MAX_VALUE;<br>      for(int j = 0; j &lt; 16; j++){<br>        best = Math.min(best, Math.abs(j*val-vals[i]));<br>      }<br>      ret = Math.max(ret, best);<br>    }<br>    return ret;<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>