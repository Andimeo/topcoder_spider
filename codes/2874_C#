            // FilterC#<br><br><br>using System;<br>using System.Collections;<br><br>public class Histogram {<br><br>  public string[] draw(string[] title, int[] v) <br>  {<br>    int mv=MaxI(v);<br>    int nr=mv+1;<br>    int ms=0;<br>    int n=v.Length;<br>    for(int i=1;i&lt;n;i++)<br>    {<br>      int l0=title[i-1].Length;<br>      int l1=title[i].Length;<br>      ms=Math.Max(ms,(l1-1)/2+(l0-(l0-1)/2));<br>    }<br>    int[] xp=new int[n];<br>    xp[0]=(title[0].Length-1)/2;<br>    for(int i=1;i&lt;n;i++)<br>      xp[i]=xp[i-1]+ms+1;<br>    string[] r=new string[nr];<br>    for(int i=0;i&lt;nr-1;i++)<br>    {<br>      int lp=n-1;<br>      for(;lp&gt;=0;lp--)<br>      {<br>        if(v[lp]&gt;=mv-i) break;<br>      }<br>      char[] ca=new char[xp[lp]+1];<br>      for(int j=0;j&lt;ca.Length;j++) ca[j]=' ';<br>      for(int k=0;k&lt;=lp;k++)<br>        if(v[k]&gt;=mv-i) ca[xp[k]]='X';<br>      r[i]=new string(ca);<br>    }<br>    r[nr-1]=title[0];<br>    for(int i=1;i&lt;n;i++)<br>    {<br>      int l0=title[i-1].Length;<br>      int l1=title[i].Length;<br>      int ns=ms-((l1-1)/2+(l0-(l0-1)/2))+1;<br>      for(int j=0;j&lt;ns;j++)<br>        r[nr-1]+=" ";<br>      r[nr-1]+=title[i];<br>    }<br><br>    return r;<br>  }<br><br><br>  private static ArrayList BitCount(int n){ ArrayList r = new ArrayList(); int idx=0; while(n &gt; 0) { if((n&amp;1)==1) r.Add(idx); n &gt;&gt;= 1; idx++;} return r; }<br>  private static int Gcd(int a, int b) { while(b!=0) {int t=a; a=b; b=t%b;} return a;}<br>  private static int Lcm(int a, int b) { return a*b/Gcd(a,b); }<br>  private static char[] CA(ArrayList ar) { return (char []) ar.ToArray(typeof(char)); }<br>  private static int[] IA(ArrayList ar) { return (int []) ar.ToArray(typeof(int)); }<br>  private static long[] LA(ArrayList ar) { return (long []) ar.ToArray(typeof(long)); }<br>  private static double[] DA(ArrayList ar) { return (double []) ar.ToArray(typeof(double)); }