            using System;<br>using System.Collections;<br>using System.Text;<br> <br>public class Mailbox<br>{<br>  static void Main(string[] args)<br>  {<br>    Mailbox test = new Mailbox();<br>  }<br> <br>  bool imp(string collection, string address)<br>  {<br>    foreach (char ch in address)<br>    {<br>      if (ch == ' ')<br>        continue;<br>      int index = collection.IndexOf(ch);<br>      if (index == -1)<br>        return true;<br>      collection = collection.Remove(index, 1);<br>    }<br>    return false;<br>  }<br> <br>  public int impossible(string collection, string[] address)<br>  {<br>    int count=0;<br>    foreach (string addr in address)<br>    {<br>      if (imp(collection, addr))<br>        count++;<br>    }<br>    return count;<br>  }<br>}<br></td>