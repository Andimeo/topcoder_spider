            #include &lt;vector&gt;<br>#include &lt;map&gt;<br>#include &lt;string&gt;<br>#include &lt;string.h&gt;<br> <br>using namespace std;<br> <br>class Mailbox<br>{<br>public:<br>  int impossible(string collection, vector &lt;string&gt; address)<br>  {<br>    int chara[300];<br>    int i,j;<br>    int ans=0;<br>    for (i=0; i&lt;address.size(); i++)<br>    {<br>      memset(chara,0,sizeof(chara));<br>      for (j=0; j&lt;collection.length(); j++)<br>      {<br>        chara[collection[j]]++;<br>      }<br>      for (j=0; j&lt;address[i].length(); j++)<br>      {<br>        if (address[i][j]==' ') continue;<br>        if (chara[address[i][j]]&gt;0) chara[address[i][j]]--;<br>        else break;<br>      }<br>      if (j!=address[i].length())<br>        ans++;<br>    }<br>    return ans;<br>  }<br>};<br></td>