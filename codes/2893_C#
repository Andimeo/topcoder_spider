            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class Uptime <br>{<br>    public string calcUptime(string started, string now) <br>    {<br>    DateTime start = DateTime.Parse(started.Replace("at ", ""));<br>    DateTime n = DateTime.Parse(now.Replace("at ", ""));<br>    TimeSpan diff = n - start;<br>    string res = "";<br>    if ((int)diff.TotalDays &gt; 0)<br>      res += (int)diff.TotalDays + "d ";<br>    if (diff.Hours &gt; 0)<br>      res += diff.Hours + "h ";<br>    if (diff.Minutes &gt; 0)<br>      res += diff.Minutes + "m ";<br>    if (diff.Seconds &gt; 0)<br>      res += diff.Seconds + "s ";<br>        return res.Trim();<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>