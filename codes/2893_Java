            import java.util.*;<br>public class Uptime {<br>  String[] mn = {"Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"};<br>  int getDay(int y, int m, int d) {<br>    y--;<br>    int r = 2 + y * 365 + y / 4 - y / 100 + y / 400 + (m - 1) * 31 + d;<br>    if (m &gt; 11) r--;<br>    if (m &gt; 9) r--;<br>    if (m &gt; 6) r--;<br>    if (m &gt; 4) r--;<br>    y++;<br>    if (m &gt; 2) r -= y % 400 == 0 || y % 4 == 0 &amp;&amp; y % 100 != 0 ? 2 : 3;<br>    return r;<br>  }<br>  long getSec(String s) {<br>    String[] t = s.split("[ :]");<br>    int m = 0;<br>    while (!mn[m].equals(t[1])) m++;<br>    long r = (long) getDay(Integer.parseInt(t[2]), m + 1, Integer.parseInt(t[0])) * 86400;<br>    int h = Integer.parseInt(t[4]);<br>    if (h == 12) h = 0;<br>    if ("PM".equals(t[7])) h += 12;<br>    r += h * 3600;<br>    r += Integer.parseInt(t[5]) * 60 + Integer.parseInt(t[6]);<br>    return r;<br>  }<br>  public String calcUptime(String started, String now) {<br>    long d = getSec(now) - getSec(started);<br>    String s = "";<br>    if (d % 60 != 0) s = (d % 60) + "s " + s;<br>    d /= 60;<br>    if (d % 60 != 0) s = (d % 60) + "m " + s;<br>    d /= 60;<br>    if (d % 24 != 0) s = (d % 24) + "h " + s;<br>    d /= 24;<br>    if (d != 0) s = d + "d " + s;<br>    if (s.length() &gt; 0) s = s.substring(0, s.length() - 1);<br>    return s;<br>  }<br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>