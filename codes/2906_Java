            /*/*///*/*/ Powered by radedit!            /*/*/*/*/*/ ;<br>/*/*//*/*/  import java . util .           /*/*/*/*/*/ ;<br>/*/*//*/*/  import java . lang . reflect . /*/*/*/*/*/ ;<br>public class SensorGrid {<br>   boolean ddd = false ;<br>  public int[] largestRectangle(int width, int height, int[] x, int[] y) {<br>    int i, j, k ;<br>    int[] xx = new int[x.length + 2] ;<br>    int[] yy = new int[y.length + 2] ;<br>    for (i=0; i&lt;x.length; i++) {<br>      xx[i] = x[i] ;<br>      yy[i] = y[i] ;<br>    }<br>    xx[x.length] = -1 ;<br>    xx[x.length+1] = width ;<br>    yy[y.length] = -1 ;<br>    yy[y.length+1] = height ;<br>    int r[] = new int[4] ;<br>    int t[] = new int[4] ;<br>    int higharea = 0 ;<br>    for (int x1 = 0; x1 &lt; xx.length; x1++) {<br>      for (int x2 = 0; x2 &lt; xx.length; x2++) if (xx[x2] &gt; xx[x1]) {<br>  for (int y1=0; y1&lt;yy.length; y1++) {<br>    for (int y2=0; y2&lt;yy.length; y2++) if (yy[y2] &gt; yy[y1]) {<br>      int ht = (yy[y2] - yy[y1] - 1) ;<br>      int wd = (xx[x2] - xx[x1] - 1) ;<br>      if (ht * wd &gt;= higharea) {<br>        boolean okay = true ;<br>        for (i=0; i&lt;xx.length; i++) {<br>    int xt = xx[i] ;<br>    int yt = yy[i] ;<br>    if (xt &lt;= xx[x1] || xt &gt;= xx[x2] ||<br>        yt &lt;= yy[y1] || yt &gt;= yy[y2]) {<br>    } else {<br>      okay = false ;<br>      break ;<br>    }<br>        }<br>        if (okay) {<br>    t[0] = xx[x1] + 1 ;<br>    t[1] = yy[y1] + 1 ;<br>    t[2] = wd ;<br>    t[3] = ht ;<br>    if (ht * wd &gt; higharea ||<br>        (t[1] &lt; r[1] || (t[1] == r[1] &amp;&amp;<br>             (t[0] &lt; r[0] || (t[0] == r[0] &amp;&amp;<br>                  t[2] &gt;= r[2]))))) {<br>      r[0] = t[0] ;<br>      r[1] = t[1] ;<br>      r[2] = t[2] ;<br>      r[3] = t[3] ;<br>      higharea = ht * wd ;<br>    }<br>        }<br>      }<br>    }<br>  }<br>      }<br>    }<br>    return r ;<br>  }<br> <br>  static void pp(Object o) { System.out.println(o) ; }<br>}<br></td