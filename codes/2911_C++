+) {<br>          if (answersWomen[index][k] == answersMen[j][k]) c++;<br>        }<br>        if (c &gt; numAns) {<br>          numAns = c; em = namesMen[j]; n2 = j;<br>        }<br>        else if (c == numAns) {<br>          if (namesMen[j] &lt; em) {em = namesMen[j]; n2 = j;}<br>        }<br>      }<br>      mm[n2] = 1;<br>      cout &lt;&lt; earliest &lt;&lt; "-&gt;" &lt;&lt; em &lt;&lt; "\n";<br>      if (earliest == queryWoman) return em;<br>    }<br>    return "";<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>