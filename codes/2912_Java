            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class UnLinker<br>{<br>  public String clean(String text)<br>  {<br>    int  count=1;<br>    Pattern  p = Pattern.compile("(http\\:\\/\\/www\\.|http\\:\\/\\/|www\\.)[a-zA-Z0-9\\.]+(\\.org|\\.com|\\.edu|\\.info|\\.tv)");<br>    while (true) {<br>    <br>      Matcher m = p.matcher(text);<br>      <br>      if (m.find()) {<br>        text = m.replaceFirst("OMIT"+count);<br>        count++;<br>      } else {<br>        break;<br>      }<br>      <br>//      System.out.println(m.group());<br>    <br>    }<br>      <br>    return text;<br>  }<br>  <br>  public static void main(String[] args)<br>  {<br>    long time;<br>    String answer;<br>    boolean errors = false;<br>    String answerString, desiredAnswerString;<br>    <br>    time = System.currentTimeMillis();<br>    answer = new UnLinker().clean("espihttp://www.tv.org.superwww.cali.comaladocious");<br>    System.out.println("Time: " + (System.currentTimeMillis()-time)/1000.0 + " seconds");<br>    System.out.println("Your answer:");<br>    answerString = "\"" + answer + "\"";<br>    System.out.println("\t" + answerString);<br>    desiredAnswerString = "\"espiOMIT1aladocious\"";<br>    System.out.println("Desired answer:\n\t" + desiredAnswerString);<br>    if (!answerString.equals(desiredAnswerString))<br>    {<br>      errors = true;<br>      System.out.println("DOESN'T MATCH!!!!");<br>    }<br>    else<br>      System.out.println("Match :-)");<br>    System.out.println();<br>    time = System.currentTimeMillis();<br>    answer = new UnLinker().clean("check www.foo.com 4 www.foo.com www.scoopz.com");<br>    System.out.println("Time: " + (System.currentTimeMillis()-time)/1000.0 + " seconds");<br>    System.out.println("Your answer:");<br>    answerString = "\"" + answer + "\"";<br>    System.out.println("\t" + answerString);<br>    desiredAnswerString = "\"check OMIT1 4 