            import java.util.*;<br><br>public class UserName <br>{<br>  public String newMember(String[] existingNames, String newName) <br>  {<br>        int number = 0;<br>        String name = newName;<br><br>        boolean found = false;<br>        do<br>        {<br>            found = false;<br>            for (int i=0; i &lt; existingNames.length; i++)<br>            {<br>                if (existingNames[i].equals(name))<br>                {<br>                    found = true;<br>                    break;<br>                }<br>            }<br><br>            if (found)<br>            {<br>                number++;<br>                name = newName + number;<br>            }<br><br>        } while(found);<br><br>        return name;<br><br>  }<br>  <br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>