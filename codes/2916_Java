            import java.util.*;<br>public class Medici{<br>    public int winner(int[] fame /*fame*/,<br>                      int[] fo /*fortune*/,<br>                      int[] sec /*secrets*/){<br>        int ret = 0;<br> <br>        l1 = fame.length;<br>        l2 = fo.length;<br>        l3 = sec.length;<br>        int min=fame[0];<br>  int max = -1;<br>        for(int i=0;i&lt;l1;i++){<br>      min = fame[i];<br>      min = Math.min(min,fo[i]);<br>      min = Math.min(min,sec[i]);<br>      if(min&gt;max){<br>    max = min;<br>    ret = i;<br>      }else if(min==max){<br>    ret = -1;<br>      }<br>  }<br>        <br>        return ret;<br>    }<br>    int l1,l2,l3;<br> <br> <br>}<br></td>