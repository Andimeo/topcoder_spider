            using System;<br>using System.Collections;<br><br>public class HexagonalSums<br>{<br>  public int minLength(int N) <br>  {<br>    int[] nums = new int[800];<br>    for(int i=1;i&lt;800;i++)<br>    {<br>      nums[i] = i*(2*i-1);<br>    }<br>    ArrayList cur = new ArrayList();<br>    cur.Add(N);<br>    <br>    char [] reach = new char[1000001];<br>    for(int round = 1;round &lt;= 6;round ++)<br>    {<br>      ArrayList next = new ArrayList();<br>      foreach(int n in cur)<br>      {<br>        for(int i=0;i&lt;800;i++)<br>        {<br>          if(n - nums[i] == 0) return round;<br>          if(n - nums[i] &gt; 0) <br>          {<br>            if(reach[n - nums[i]] == 0)<br>            {<br>              next.Add(n - nums[i]);<br>              reach[n - nums[i]] = 'a';<br>            }<br>          }<br>        }<br>      }<br>      cur = next;<br>    }<br>    return -1;<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>