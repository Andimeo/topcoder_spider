            #include &lt;string&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;vector&gt;<br> <br>using namespace std;<br> <br>struct cnt {<br>  string code; int g; int s; int b;<br>};<br> <br>bool operator&lt;(const cnt &amp;a, const cnt &amp;b)<br>{<br>  if (a.g!=b.g) return a.g&gt;b.g;<br>  if (a.s!=b.s) return a.s&gt;b.s;<br>  if (a.b!=b.b) return a.b&gt;b.b;<br>  return a.code&lt;b.code;<br>}<br> <br>vector&lt;cnt&gt; r;<br>void add(string code, int g, int s, int b)<br>{<br>  for (int i=0;i&lt;r.size();i++) {<br>    if (r[i].code==code) {<br>      r[i].g+=g;<br>      r[i].s+=s;<br>      r[i].b+=b;<br>      return;<br>    }<br>  }<br>  cnt c={code,g,s,b};<br>  r.push_back(c);<br> <br>}<br>class MedalTable {<br>public:<br>  vector &lt;string&gt; generate(vector &lt;string&gt; results) {<br>    r.clear();<br>    for (int i=0;i&lt;results.size();i++) {<br>      istringstream str(results[i]);<br>      string g,s,b;<br>      str &gt;&gt; g &gt;&gt; s &gt;&gt; b;<br>      add(g,1,0,0);<br>      add(s,0,1,0);<br>      add(b,0,0,1);<br>    }<br>    sort(r.begin(),r.end());<br>    vector&lt;string&gt; ret;<br>    for (int i=0;i&lt;r.size();i++) {<br>      char buf[1000];<br>      sprintf(buf,"%s %d %d %d",r[i].code.c_str(),r[i].g,r[i].s,r[i].b);<br>      ret.push_back(buf);<br>    }<br>    return ret;<br>  }<br>  <br> <br>};<br> <br> <br> <br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>