            imports microsoft.visualbasic<br> <br>Public Class MedalTable<br>    Public Function generate(ByVal results As String()) As String()<br>        Dim i, j, k, countries, max As Integer<br>        Dim countryName(49) As String<br>        Dim countryMedal(49, 2) As Integer<br>        Dim thisCountry As String<br> <br>        For i = 0 To results.Length - 1<br>            For k = 0 To 2<br>                thisCountry = Mid(results(i), k * 4 + 1, 3)<br>                For j = 0 To countries<br>                    If countryName(j) = thisCountry Then Exit For<br>                Next<br>                If j &gt; countries Then<br>                    countries += 1<br>                    j -= 1<br>                End If<br>                countryName(j) = thisCountry<br>                countryMedal(j, k) += 1<br>            Next<br>        Next<br> <br>        Dim res(countries - 1) As String<br> <br>        For i = 0 To countries - 1<br>            max = -1<br>            For j = 0 To countries - 1<br>                If countryName(j).Length &gt; 0 Then<br>                    For k = 0 To 2<br>                        If max = -1 OrElse countryMedal(j, k) &gt; countryMedal(max, k) Then<br>                            max = j<br>                            Exit For<br>                        ElseIf countryMedal(j, k) &lt; countryMedal(max, k) Then<br>                            Exit For<br>                        End If<br>                    Next<br>                    If k &gt; 2 AndAlso (max = -1 OrElse countryName(j) &lt; countryName(max)) Then max = j<br>                End If<br>            Next<br>            res(i) = countryName(max)<br>            For k = 0 To 2<br>                res(i) &amp;= " " &amp; LTrim(Str(countryMedal(max, k)))<br>            Next k<br>            countryName(max) = ""<br>        Next<br> <br>        Return res<br>    End Function<br>End Class<br></td>