            // Author:  King Larry Mak<br>// TC Handle:  Larry<br>import java.util.*;<br>public class TallPeople {<br>  public int[] getPeople(String[] people) {<br>    int[][] a;<br>    <br>    StringTokenizer st = new StringTokenizer( people[0] );<br>    int y = st.countTokens();<br>    int x = people.length;<br>    a = new int[x][y];<br>    int min = 0, max = 0;<br>    <br>    for ( int i = 0; i &lt; x; i++ ) {<br>      st = new StringTokenizer( people[i] );<br>      <br>      for ( int j = 0; j &lt; y; j++ ) {<br>        a[i][j] = Integer.parseInt( st.nextToken() );<br>      }<br>    }<br>    <br>    int m[] = new int[x];<br>    <br>    for ( int i = 0; i &lt; x; i++ ) {<br>      m[i] = 1000000000;<br>      for ( int j = 0; j &lt; y; j++ ) m[i] = Math.min( a[i][j], m[i] );<br>      min = Math.max( min, m[i] );<br>    }<br>    <br>    max = 1000000;<br>    m = new int[y];<br>    for ( int i = 0; i &lt; y; i++ ) {<br>      m[i] = -1;<br>      for ( int j = 0; j &lt; x; j++ ) m[i] = Math.max( a[j][i], m[i] );<br>      max = Math.min( max, m[i] );<br>    }<br>    <br>    return new int[] { min, max };<br>  }<br>  <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>