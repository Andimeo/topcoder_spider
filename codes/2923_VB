            imports microsoft.visualbasic<br>imports system<br> <br>Public Class TallPeople<br>    Public Function getPeople(ByVal people As String()) As Integer()<br>        Dim i, j, hei(50, 30), R, C, min(50), max(30), ans(1) As Integer<br>        Dim tok As String()<br> <br>        R = people.Length<br>        For i = 0 To R - 1<br>            tok = people(i).Split(" ")<br>            C = tok.Length<br>            For j = 0 To C - 1<br>                hei(i, j) = Val(tok(j))<br>            Next<br>        Next<br> <br>        For i = 0 To R - 1<br>            min(i) = 1001<br>            For j = 0 To C - 1<br>                min(i) = Math.Min(min(i), hei(i, j))<br>            Next<br>            ans(0) = Math.Max(ans(0), min(i))<br>        Next<br> <br>        ans(1) = 1001<br>        For j = 0 To C - 1<br>            For i = 0 To R - 1<br>                max(j) = Math.Max(max(j), hei(i, j))<br>            Next<br>            ans(1) = Math.Min(ans(1), max(j))<br>        Next<br>        Return ans<br>    End Function<br>End Class<br></td>