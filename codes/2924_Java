            // <br>import java.util.*;<br>public class MovingAverages {<br>    public int[] calculate(String[] times, int n) {<br>        int[] res = new int[times.length-n+1];<br>  for(int i=0;i&lt;=times.length-n;i++){<br>      int avg = 0;<br>      for(int z=i;z&lt;n+i;z++){<br>    String g[] = times[z].split(":");<br>    avg+= Integer.parseInt(g[0])*3600+ Integer.parseInt(g[1])*60 + Integer.parseInt(g[2]);<br>      }<br>      res[i] = avg/n;<br>  }<br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>