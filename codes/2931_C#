            using System;<br><br>public class CampLunches<br>{<br>    public int firstMatching(string[] menu1, string[] menu2)<br>    {<br>        int len_1 = menu1.Length;<br>        int len_2 = menu2.Length;<br><br>        int i = 0;<br>        int j = 0;<br><br>        for (int k= 0; k &lt;  len_1 * len_2 / gcd(len_1, len_2); k++)<br>        {<br>            if (menu1[i].Equals(menu2[j]))<br>            {<br>                return k;<br>            }<br><br>            i++; i %= len_1;<br><br>            j++;j %=len_2;<br>        }<br>        return -1;<br>    }<br><br>    public int gcd(int x, int y)<br>    {<br>        return y == 0 ? x : gcd(y, x% y);<br>    }<br><br>}<br></td>