            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class CampLunches<br>{<br>  public int firstMatching(String[] menu1, String[] menu2)<br>  {<br>    int i,j;<br>    int n = menu1.length;<br>    int m = menu2.length;<br>    for(int step=0;step&lt;100000;step++)<br>    {<br>      i = step % n;<br>      j = step % m;<br>      if(menu1[i].equals(menu2[j]))return step;<br>    }<br>    return -1;<br>  }<br>  <br>  public static void main(String[] args)<br>  {}<br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>