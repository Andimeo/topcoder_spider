            // FilterJava<br><br>import java.util.*;<br>public class DrivingDirections {<br>    public String[] reverse(String[] directions) {<br>        String[] ret = new String[directions.length];<br>        String[] locs = new String[directions.length];<br>        boolean[] goRight = new boolean[directions.length-1];<br>        for( int i=0; i&lt;directions.length; i++ ) {<br>            if(directions[i].startsWith("Start on ")) {<br>                locs[i]=directions[i].substring(9);<br>            }<br>            else if(directions[i].startsWith("Turn RIGHT on ")) {<br>                locs[i]=directions[i].substring(14);<br>                goRight[i-1]=true;<br>            }<br>            else if(directions[i].startsWith("Turn LEFT on ")) {<br>                locs[i]=directions[i].substring(13);<br>                goRight[i-1]=false;<br>            }<br>        }<br>        for( int i=0; i&lt;ret.length; i++ ) {<br>            if(i==ret.length-1) {<br>                ret[0]="Start on "+locs[i];<br>            }<br>            else if( goRight[i] ) {<br>                ret[ret.length-1-i] = "Turn LEFT on "+locs[i];<br>            }<br>            else {<br>                ret[ret.length-1-i] = "Turn RIGHT on "+locs[i];<br>            }<br>        }<br>        return ret;<br>    }<br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>