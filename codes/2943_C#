            using System;<br>using System.Text;<br>using System.Collections;<br><br>public class SumOfSquareRoots {<br>    public int[] shortestList(int[] A) {<br>        int[] res;<br><br>    int n = A.Length;<br>    int[] a = new int[n];<br>    int[] c = new int[1001];<br>    int cnt = 0;<br>    <br>    for (int i = 0; i &lt; n; i++)<br>    {<br>      a[i] = 1;<br>      for (int j = 2; j &lt; 40; j++)<br>        if (A[i] % (j*j) == 0)<br>        {<br>          A[i] = A[i] / (j*j);<br>          a[i] *= j;<br>          j--;<br>        }<br>      if (c[A[i]] == 0) cnt++;<br>      c[A[i]] += a[i];<br>    }<br><br>    res = new int[cnt];<br>    cnt = 0;<br>    for (int j = 0; j &lt; 1001; j++) if (c[j] &gt; 0) { res[cnt] = j*c[j]*c[j]; cnt++; };<br><br>    Array.Sort(res);<br><br>        return res;<br>    }<br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>