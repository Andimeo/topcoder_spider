            import java.util.*;<br>public class SumOfSquareRoots {<br>  public int[] shortestList(int[] a) {<br>    int[] c = new int[1000];<br>    for (int i = 0; i &lt; a.length; i++) {<br>      int n = 32;<br>      while (a[i] % (n * n) != 0) n--;<br>      c[a[i] / (n * n)] += n;<br>    }<br>    int k = 0;<br>    for (int i = 0; i &lt; 1000; i++)<br>      if (c[i] != 0) k++;<br>    int[] b = new int[k];<br>    k = 0;<br>    for (int i = 0; i &lt; 1000; i++)<br>      if (c[i] != 0) b[k++] = i * c[i] * c[i];<br>    Arrays.sort(b);<br>    return b;<br>  }<br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>