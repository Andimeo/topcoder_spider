            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;utility&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cctype&gt;<br>#include &lt;climits&gt;<br> <br>using namespace std;<br> <br> <br>typedef vector&lt;int&gt; VI;<br>typedef vector&lt;string&gt; VS;<br>typedef vector&lt;vector&lt;int&gt; &gt; VVI;<br>typedef pair&lt;int,int&gt; VII;<br>typedef pair&lt;string,int&gt; VSI;<br>typedef pair&lt;int,string&gt; VIS;<br>typedef pair&lt;string,string&gt; VSS;<br> <br>#define debug(x) cout &lt;&lt; #x &lt;&lt; " = " &lt;&lt; x &lt;&lt; endl;<br> <br>template &lt;class T&gt;<br>ostream&amp; operator &lt;&lt;(ostream&amp; os, const vector&lt;T&gt;&amp; a) {<br>  for(int i=0;i&lt;(int)a.size();i++) {<br>    os &lt;&lt; a[i] &lt;&lt; endl;<br>  }<br>  os &lt;&lt; endl;<br>  return os;<br>}<br> <br>class Unblur {<br>  public:<br>  vector &lt;string&gt; original(vector &lt;string&gt; b) {<br>    VS r(b.size(), string(b[0].size(),'.'));<br>    for(int i=1;i+1&lt;(int)b.size();i++) {<br>      for(int j=1;j+1&lt;(int)b[0].size();j++) {<br>        int total = b[i-1][j-1] - '0';<br>        for(int di=i-2;di&lt;=i;di++)for(int dj=j-2;dj&lt;=j;dj++) {<br>          if (0&lt;=di&amp;&amp;di&lt;b.size() &amp;&amp; 0&lt;=dj&amp;&amp;dj&lt;b[0].size()) {<br>            total -= (r[di][dj]=='#');<br>          }<br>        }<br>        r[i][j] = total==1?'#':'.';<br>      }<br>    }<br>    cout &lt;&lt; r;<br>    return r;    <br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>