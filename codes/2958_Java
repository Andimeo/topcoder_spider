            import java.util.*;<br> <br>public class BikeRace {<br> <br>  final static int oo = 100100100;<br> <br>  class A implements Comparable {<br>    String name;<br>    double remove;<br>    <br>    A(String x, double y) { name=x; remove=y; }<br>    <br>    int sign(double x) {<br>      if (x&gt;0) return 1;<br>      if (x&lt;0) return -1;<br>      return 0;<br>    }<br>    <br>    public int compareTo(Object oo) {<br>      A o = (A) oo;<br>      if (remove!=o.remove) return sign(remove-o.remove);<br>      return name.compareTo(o.name);<br>    }<br>    <br>  }<br> <br>  String[] result;<br>  int m, n, size;<br>  A[] a;<br> <br>  double loai(int t1, int v1, int t2, int v2) {<br>    double r = t1+1.0*size/v1;<br>    if (r&lt;=t2) return r;<br>    if (v1==v2) return oo;    <br>    if (v1&lt;=v2) return oo;<br>    if (t1&gt;t2) {<br>      r = 1.0*(t1-t2)*v2/(v1-v2) + t1;<br>    } else {<br>      double s = (t2-t1)*v1;<br>      double s2 = size - s;<br>      double t = s2/(v1-v2);<br>      r = t2 + t;<br>    }<br>    return r;<br>  }<br> <br>  public String[] eliminated(int track, String[] name, int[] start, int[] speed) {<br>    size = track;<br>    n = name.length;<br>    boolean[] con = new boolean[n];<br>    Arrays.fill(con, true);<br>    a = new A[n];<br>    for (int i=0; i&lt;n; i++) a[i] = new A(name[i], oo);<br>    do {<br>      double min = oo-1;<br>      int i2=-1, j2=-1;<br>      for (int i=0; i&lt;n; i++)<br>      for (int j=0; j&lt;n; j++)<br>      if (i!=j&amp;&amp;con[i]&amp;&amp;con[j]) {<br>        double tmp = loai(start[i],speed[i],start[j],speed[j]);<br>//        if (i==0&amp;&amp;j==2) pp(""+tmp);<br>        if (tmp&lt;min) { min = tmp; i2=i; j2=j; }<br>      }<br>      <br>      if (min==oo-1) break;<br>      a[j2].remove = min;<br>      con[j2] = false;<br>    } while (true);<br>    <br>    Arrays.sort(a);<br>    <br>    int s = 0;<br>    for (int i=0; i&lt;n; i++)<br>    if (a[i].remove!=oo) s++;<br>    <br>    result = new String[s];<br>    for (int i=0; i&lt;s; i++) result[i] = a[i].name;<br>    return result;<br>  }<br> <br>  public void pp(String s) {<br>    System.out.println(s);<br>  }<br> <br>  <br>}<br> <br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>