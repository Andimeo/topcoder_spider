            Public Class BikeRace<br> <br>        Public Function eliminated(ByVal track As Integer, ByVal name As String(), ByVal start As Integer(), ByVal speed As Integer()) As String()<br>            Dim aElim(name.GetUpperBound(0), name.GetUpperBound(0)) As Double<br>            Dim bEliminated(name.GetUpperBound(0)) As Boolean<br>            Dim aRet As New System.Collections.ArrayList<br> <br>            Dim i, j As Integer<br> <br>            For i = 0 To name.GetUpperBound(0)<br>                For j = 0 To name.GetUpperBound(0)<br>                    If i = j Then<br>                        aElim(i, j) = -1<br>                    Else<br>                        If start(i) &gt; start(j) Then<br>                            If speed(i) &lt;= speed(j) Then<br>                                aElim(i, j) = -1<br>                            Else<br>                                aElim(i, j) = ((start(i) - start(j)) * speed(j)) / (speed(i) - speed(j)) + start(i)<br>                            End If<br>                        Else<br>                            If (track / speed(i)) &lt;= (start(j) - start(i)) Then<br>                                aElim(i, j) = start(i) + (track / speed(i))<br> <br>                            Else<br> <br> <br>                                If speed(i) &gt; speed(j) Then<br>                                    aElim(i, j) = track / speed(i) + start(i)<br>                                    If start(j) &lt; aElim(i, j) Then<br>                                        aElim(i, j) = (track - (start(j) - start(i)) * speed(i)) / (speed(i) - speed(j)) + start(j)<br>                                    End If<br>                                Else<br>                                    aElim(i, j) = -1<br>                                End If<br>                            End If<br>                        End If<br> <br>                    End If<br>                Next<br>            Next<br> <br>            Dim dMin As Double = 1<br>            Dim k As Integer<br>            Dim jElim As Integer<br> <br>            Do While dMin &lt; 9999999<br>                dMin = 9999999<br>                jElim = 0<br>                For i = 0 To aElim.GetUpperBound(0)<br>                    For j = 0 To aElim.GetUpperBound(0)<br>                        If aElim(i, j) &gt;= 0 Then<br>                            If aElim(i, j) &lt; dMin Or (aElim(i, j) = dMin And name(j) &lt; name(jElim)) Then<br>                                dMin = aElim(i, j)<br>                                jElim = j<br>                            End If<br>                        End If<br>                    Next<br>                Next<br> <br>                If dMin &lt; 9999999 Then<br>                    aRet.Add(name(jElim))<br>                    For i = 0 To aElim.GetUpperBound(0)<br>                        aElim(i, jElim) = -1<br>                        aElim(jElim, i) = -1<br>                    Next<br>                End If<br>            Loop<br> <br>            Return aRet.ToArray(GetType(String))<br> <br>        End Function<br> <br>    End Class<br></td>