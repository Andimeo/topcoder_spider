            #include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cmath&gt;<br>#include &lt;queue&gt;<br>using namespace std;<br>typedef vector&lt;int&gt; VI;  typedef vector&lt;vector&lt;int&gt; &gt; VVI;<br>typedef vector&lt;string&gt; VS;  typedef vector&lt;vector&lt;string&gt; &gt; VVS;<br>typedef signed long long i64;  typedef unsigned long long u64;<br><br>double memo[1000][1000];<br>double p;<br>double doit(int a, int b) {<br>  if( a &gt;= 1000 || b &gt;= 1000 ) return 0.0;<br>  double &amp;ret = memo[a][b];<br>  if( ret != -1.0 ) return ret;<br>  if( a &gt;= 15 &amp;&amp; a-b &gt;= 2 ) return ret = 1.0;<br>  if( b &gt;= 15 &amp;&amp; b-a &gt;= 2 ) return ret = 0.0;<br>  return ret = p*doit(a+1, b) + (1-p)*(1-doit(b+1, a));<br>}<br><br>class Volleyball {<br>public:<br>double win(int sScore, int rScore, double probWinServe) {<br>  p = probWinServe;<br>  for( int i = 0; i &lt; 1000; i++ )<br>  for( int j = 0; j &lt; 1000; j++ )<br>    memo[i][j] = -1.0;<br>  return doit(sScore, rScore);<br>}<br>};<br><br><br>// Powered by FileEdit<br></td>