            import java.util.*;<br>public class Volleyball {<br>  double[][] a = new double[120][120];<br>  double fun(int i, int j, double p) {<br>    if (a[i][j] &gt; -0.1) return a[i][j];<br>    a[i][j] = p * fun(i + 1, j, p) + (1 - p) * (1 - fun(j + 1, i, p));<br>    return a[i][j];<br>  }<br>  public double win(int s1, int r1, double p) {<br>    double q0 = 1 / (3 - 2 * p);<br>    double q1 = p + (1 - p) * (1 - q0);<br>    double q2 = p * q0;<br>    for (int i = 0; i &lt; 120; i++) Arrays.fill(a[i], -1);<br>    for (int i = 0; i &lt; 120; i++)<br>      for (int j = 0; j &lt; 120; j++)<br>        if (i &gt;= 15 || j &gt;= 15)<br>          if (i == j) a[i][j] = q0;<br>          else if (i == j + 1) a[i][j] = q1;<br>          else if (i + 1 == j) a[i][j] = q2;<br>          else if (i &gt;= j + 2) a[i][j] = 1;<br>          else if (i + 2 &lt;= j) a[i][j] = 0;<br>    return fun(s1, r1, p);<br>  }<br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>