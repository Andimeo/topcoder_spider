            import java.util.*;<br>public class Diving {<br>  public String needed(String difficulty, String need, String ratings) {<br>    int d = (int) Math.round(Double.parseDouble(difficulty) * 10);<br>    int n = (int) Math.round(Double.parseDouble(need) * 100);<br>    int k = 0;<br>    int[] r = new int[5];<br>    String[] s = ratings.split(" ");<br>    for (int i = 0; i &lt; 5; i++)<br>      if (s[i].charAt(0) == '?') k = i;<br>      else r[i] = (int) Math.round(Double.parseDouble(s[i]) * 10);<br>    for (int i = 0; i &lt;= 100; i += 5) {<br>      int[] a = (int[]) r.clone();<br>      a[k] = i;<br>      Arrays.sort(a);<br>      int t = 0;<br>      for (int j = 1; j &lt; 4; j++) t += a[j];<br>      t *= d;<br>      if (t &gt;= n) return i / 10 + "." + i % 10;<br>    }<br>    return "-1.0";<br>  }<br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>