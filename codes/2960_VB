            Public Class Diving<br>        Public Function needed(ByVal difficulty As String, ByVal need As String, ByVal ratings As String) As String<br>            Dim aSpaceSep() As Char = {" "}<br>            Dim aRatings(4) As Integer<br>            Dim aRat() As String = ratings.Split(aSpaceSep)<br>            Dim i As Integer<br>            Dim iMissing As Integer<br>            For i = 0 To 4<br>                If aRat(i).StartsWith("?") Then<br>                    iMissing = i<br>                Else<br>                    aRatings(i) = 10 * (CDbl(aRat(i)))<br>                End If<br>            Next<br> <br>            Dim iTryScore As Integer<br> <br>            For i = 0 To 100 Step 5<br>                aRatings(iMissing) = i<br> <br> <br>                iTryScore = iScore(CDbl(difficulty) * 10, aRatings)<br>                If iTryScore &gt;= CDbl(need) * 100 Then<br>                    Return Microsoft.VisualBasic.Format(i / 10, "0.0")<br>                End If<br>            Next<br> <br>            Return "-1.0"<br> <br>        End Function<br> <br>        Private Function iScore(ByVal difficulty As Integer, ByVal ratings() As Integer) As Integer<br>            Dim i As Integer<br>            Dim iMin As Integer = 100<br>            Dim iMax As Integer = 0<br>            Dim iSum As Integer = 0<br>            For i = 0 To ratings.GetUpperBound(0)<br>                If ratings(i) &gt; iMax Then iMax = ratings(i)<br>                If ratings(i) &lt; iMin Then iMin = ratings(i)<br>                iSum += ratings(i)<br>            Next<br> <br>            iSum -= (iMin + iMax)<br>            Return iSum * difficulty<br>        End Function<br>    End Class<br></td>