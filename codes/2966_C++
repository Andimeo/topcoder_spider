            using namespace std;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;stdio.h&gt;<br>#include &lt;stdlib.h&gt;<br> <br>class MitchellMovement {<br>public:<br>    vector &lt;int&gt; teams(int N, int round, int board) {<br>  int sn = (board - 1) / 3;<br>  round--;<br>  int table = (N + N + sn - round) % N;<br>  int nspair = table;<br>  int ewpair = (N + N + table - round) % N;<br>  if ((N % 2 == 0) &amp;&amp; round &gt;= N / 2) {<br>      ewpair = (N + ewpair - 1) % N;<br>  }<br>  vector&lt;int&gt; result;<br>  result.push_back( nspair+1 );<br>  result.push_back( ewpair+1 );<br>  return result;<br>    }<br>};<br> <br> <br>// Powered by FileEdit<br></td>