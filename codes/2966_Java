            import java.util.*;<br>import java.math.*;<br><br>public class MitchellMovement {<br>    public int[] teams(int N, int round, int board) {<br>  round--;<br>  board--;<br><br>        int ewshift = round;<br>  if (N % 2 == 0 &amp;&amp; round &gt;= N/2) ewshift++;<br>  int boardshift = (-round+N) % N;<br>  int boardorig = board / 3;<br>  for (int i = 0; i &lt; N; i++){<br>      if ((boardorig + boardshift) % N != i) continue;<br>      return new int[]{i+1, (i-ewshift+N)%N+1};<br>  }<br>  return new int[]{0, 0};<br>    }<br><br>    public static void main(String[] args) {<br>        MitchellMovement temp = new MitchellMovement();<br>    }<br>}<br><br><br>// Powered by FileEdit<br></td>