            Public Class TheThe<br>    Public Function check(ByVal lines As String()) As Integer<br>        Dim sNext, sLine As String<br>        Dim sFirst, sLast As String<br>        Dim iStart, iEnd As Integer<br>        Dim iLine As Integer<br> <br>        For iLine = 0 To lines.Length - 2<br>            sLine = lines(iLine)<br>            sNext = lines(iLine + 1)<br> <br>            iStart = sLine.LastIndexOf(" ")<br>            If iStart = -1 Then<br>                sFirst = sLine<br>            Else<br>                sFirst = sLine.Substring(iStart + 1)<br>            End If<br> <br>            iEnd = sNext.IndexOf(" ")<br>            If iEnd = -1 Then<br>                sLast = sNext<br>            Else<br>                sLast = sNext.Substring(0, iEnd)<br>            End If<br> <br>            If sFirst = sLast Then Return iLine<br>        Next<br> <br>        Return -1<br>    End Function<br>End Class<br></td>