            import java.util.*;<br>public class NumberChanger {<br>  String a, b;<br>  int n, r = 1001001001;<br>  int[] hv;<br>  boolean[] chua;<br>  <br>  int cp() {<br>    boolean[] marked = new boolean[n];<br>    int r = 0;<br>    for (int i=0; i&lt;n; i++)<br>    if (!marked[i]) {<br>      int c = 1, x = i;<br>      marked[x] = true;<br>      do {<br>        x = hv[x];<br>        if (marked[x]) break;<br>        c++;<br>        marked[x] = true;<br>      } while (true);<br>      r+=c-1;<br>    }<br>    return r;<br>  }<br>  <br>  void xemxet() {<br>    int t = 0;<br>    for (int i=0; i&lt;n; i++) {<br>      char c1 = a.charAt(hv[i]);<br>      char c2 = b.charAt(i);<br>      t += Math.abs(c1-c2);<br>    }<br>    t += cp();<br>    if (t&lt;r) r = t;<br>  }<br>  <br>  void dq(int i) {<br>    for (int j=0; j&lt;n; j++)<br>    if (chua[j]) {<br>      chua[j] = false;<br>      hv[i] = j;<br>      if (i+1&lt;n) dq(i+1); else xemxet();<br>      chua[j] = true;<br>    }<br>  }<br>  <br>  public int transform(String start, String finish) {<br>    a = start;<br>    b = finish;<br>    n = a.length();<br>    <br>    chua = new boolean[n];<br>    hv = new int[n];<br>    Arrays.fill(chua, true);<br>    dq(0);<br>    return r;<br>  }<br>  <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>