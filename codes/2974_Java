            /*  OhmSakthi  */<br>/*  OhmSakthi  */<br>import java.util.*;<br> <br>public class GeneticCrossover {<br>  double[]dpx; int[] depx; double[] dppx;<br> <br>  public double cross(String p1a, String p1b, String p2a, String p2b, int[] dom, int[] rec, int[] dependencies) {<br>    double[]dp =  new double[dom.length];<br>    for(int i=0; i&lt;dp.length;i++){<br>      char a, b,c,d;<br>      a = p1a.charAt(i); b=p1b.charAt(i); c=p2a.charAt(i); d=p2b.charAt(i);<br>      boolean au,bu,cu,du;<br>      au = ((a&gt;='A')&amp;&amp;(a&lt;='Z')); bu = ((b&gt;='A')&amp;&amp;(b&lt;='Z')); cu = ((c&gt;='A')&amp;&amp;(c&lt;='Z')); du = ((d&gt;='A')&amp;&amp;(d&lt;='Z'));<br>      if( (au&amp;&amp;bu) || (cu&amp;&amp;du) ) {dp[i] = 1.0; continue;}<br>      if( (!au&amp;&amp;!bu) &amp;&amp; (!cu&amp;&amp;!du) ) {dp[i] = 0.0; continue;}<br>      if(au||bu){<br>        if(cu||du){ dp[i] = 0.75; }else{ dp[i] = 0.5;}<br>      }else{ if(cu||du){ dp[i] = 0.5; }else{ dp[i] = 0.0;} }<br>    }<br>    double dpp[] = new double [dp.length];<br>    fillp(dp, dependencies, dpp) ;<br>    double sum = 0;<br>    for(int i=0; i&lt;dpp.length;i++){<br>      sum += dpp[i]*dom[i] + (1-dpp[i])*rec[i];<br>    }<br>    return sum;<br>  }<br> <br>  void fillp(double[]dp, int[] dep, double[] dpp){<br>    dpx = dp; depx=dep; dppx=dpp;<br>    for(int i=0; i&lt;dp.length;i++){<br>      if(dep[i]==-1) dpp[i] = dp[i];<br>    }<br>    for(int i=0; i&lt;dp.length;i++){<br>      dpp[i] = fillup(i);<br>    }<br>  }<br> <br>  double fillup(int ind){<br>    int dep = depx[ind];<br>    if(dep==-1)return dpx[ind];<br>    else{<br>      return dpx[ind]*fillup(dep);<br>    }<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>