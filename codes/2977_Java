            public class Boxing {<br>public int maxCredit(int[] a, int[] b, int[] c, int[] d, int[] e) {<br>  int hits[][]=new int[5][];<br>  hits[0]=a;<br>  hits[1]=b;<br>  hits[2]=c;<br>  hits[3]=d;<br>  hits[4]=e;<br>  int mr[]=new int[5];<br>  for(int i=0;i&lt;5;i++) mr[i]=-1;<br>  int pos[]=new int[5];<br>  for(int i=0;i&lt;5;i++) pos[i]=0;<br>  int res=0;<br>  for(int i=0;i&lt;=200000;i++) {<br>    for(int j=0;j&lt;5;j++) {<br>      if(pos[j]&gt;=hits[j].length) continue;<br>      if(hits[j][pos[j]]&lt;=i) {<br>        mr[j]=hits[j][pos[j]];<br>        pos[j]++;<br>      }<br>    }<br>    int num_diff=0;<br>    for(int j=0;j&lt;5;j++) {<br>      if(mr[j]==-1) continue;<br>      if(i-mr[j]&lt;=1000) {<br>        num_diff++;<br>      }<br>    }<br>    if(num_diff&gt;=3) {<br>      res++;<br>      for(int j=0;j&lt;5;j++) mr[j]=-1;  <br>    }<br>  }<br>  return res;<br>}<br> <br>}<br></td>