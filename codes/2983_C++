            #include &lt;iostream.h&gt;<br>#include &lt;stdio.h&gt;<br>#include &lt;stdlib.h&gt;<br>#include &lt;ctype.h&gt;<br>#include &lt;math.h&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;algorithm&gt;<br> <br>using namespace std;<br> <br>vector&lt;string&gt; parseWords(string a)<br>{<br>  vector&lt;string&gt; ret; string word=""; int i=0;<br> <br>  for(i=0;i&lt;a.size();i++)<br>  {<br>    if(a[i]==' ')<br>    {<br>      if(word!="") ret.push_back(word); word="";<br>    }<br>    else word+=a[i];<br>  }<br> <br>  if(word!="") ret.push_back(word); return ret;<br>}<br> <br>class CultureShock<br>{<br>  public:<br> <br>   string translate(string text)<br>  {<br>    vector&lt;string&gt; temp=parseWords(text); int i=0;<br> <br>    for(i=0;i&lt;temp.size();i++)<br>    {<br>      if(temp[i]=="ZEE") temp[i]="ZED";<br>    }<br> <br>    string ret=temp[0];<br> <br>    for(i=1;i&lt;temp.size();i++)<br>    {<br>      ret+=" "; ret+=temp[i];<br>    }<br> <br>    return ret;<br>  }<br>};<br></td>