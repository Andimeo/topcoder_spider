            // Warning: no successful challenges against me yet.<br>// Note: I'm now automatically removing unused routines.<br><br>#include &lt;algorithm&gt;<br>using namespace std;<br><br>#line 3 "RockStar.cc"<br><br>class RockStar {<br>  public:<br>  int getNumSongs(int ff, int fs, int sf, int ss) {<br>    // !FDI<br>    if(ff == 0 &amp;&amp; fs == 0) return ss + (sf ? 1 : 0);<br>    else if(fs == 0) return ff;<br>    else if(sf &lt; fs) return ff + ss + (sf+1) + sf;<br>    else return ff + ss + fs + fs;<br>    }<br>  };<br><br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>