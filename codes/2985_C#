            using System;<br>using System.Collections;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class TournamentRanker<br>{<br>  int[] won;<br>  <br>  int GI(string n, string[] names){<br>    for (int i=0;i&lt;names.Length;i++)<br>      if(names[i]==n) return i;<br>    return -1;<br>  }<br>  bool gr(int a, int b, string[] names, string[] lostTo){<br>    if (won[a]&lt;won[b]) return false;<br>    else if (won[b]&lt;won[a]) return true;<br>    else return gr(GI(lostTo[a],names),GI(lostTo[b],names),names,lostTo) ;<br>  }<br>  public string[] rankTeams(string[] names, string[] lostTo)<br>  {<br>  won = new int[names.Length];<br>  for (int j=0;j&lt;names.Length;j++){<br>  won[j]=0;<br>  for (int i=0;i&lt;names.Length;i++)<br>    {  if (lostTo[i] == names[j]) won[j]++;}<br>    }<br>    {}<br>  int[] ordn = new int[names.Length];<br>  string[] result = new string[names.Length];<br>  for (int i=0;i&lt;names.Length;i++){ ordn[i]=i;}<br>  <br>  bool f;<br>  do {<br>  f=false;  <br>  for(int i=0;i&lt;names.Length-1;i++)<br>    if (gr(ordn[i+1],ordn[i],names,lostTo)){<br>      f=true;<br>      int s = ordn[i];<br>      ordn[i]=ordn[i+1];<br>      ordn[i+1]=s;<br>    }<br>  } while (f);<br>  <br>  for (int i=0;i&lt;names.Length;i++){ result[i]=names[ordn[i]];}<br>  return result;<br>    <br>  }<br>  <br>  }<br></td>