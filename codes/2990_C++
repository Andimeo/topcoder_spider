            using namespace std;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;stdio.h&gt;<br>#include &lt;stdlib.h&gt;<br> <br>class Rank {<br>public:<br>    vector &lt;double&gt; rank(vector &lt;int&gt; a) {<br>  vector&lt;double&gt; result;<br>  int n = a.size();<br>  for (int i = 0; i &lt; n; i++)<br>      result.push_back( 1 );<br>  for (int i = 0; i &lt; n; i++)<br>      for (int j = 0; j &lt; n; j++)<br>    if (j != i)<br>        if (a[j] == a[i])<br>      result[i] += 0.5;<br>        else if (a[j] &gt; a[i])<br>      result[i] += 1;<br>  return result;<br>    }<br>};<br> <br> <br>// Powered by FileEdit<br></td>