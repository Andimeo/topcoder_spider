            import java.util.*;<br>public class grafixClick {<br>    public int[] checkSaveButton(int[] mouseRows, int[] mouseCols) {<br>        List l=new ArrayList();<br>        for (int i = 0; i &lt; mouseCols.length; i++) {<br>            if(mouseRows[i]&gt;=20 &amp;&amp; mouseRows[i]&lt;=39 &amp;&amp; mouseCols[i]&gt;=50 &amp;&amp; mouseCols[i]&lt;=99)<br>                l.add(new Integer(i));<br>        }<br>        Integer[] r=(Integer[]) l.toArray(new Integer[l.size()]);<br>        int[] res=new int[r.length];<br>        for (int i = 0; i &lt; res.length; i++) {<br>            res[i]=r[i].intValue();<br>        }<br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>