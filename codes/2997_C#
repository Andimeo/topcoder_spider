            using System;<br>using System.Text;<br>using System.Collections;<br><br>public class grafixCorrupt {<br>    public int selectWord(string[] dictionary, string candidate) {<br>        int max = 0;<br>    int res = 0;<br>    for (int i = 0; i &lt; dictionary.Length; i++)<br>    {<br>      string d = dictionary[i];<br>      int cnt = 0;<br>      for (int k = 0; k &lt; d.Length; k++)<br>        if (d[k] == candidate[k]) cnt++;<br>      if (cnt &gt;= 1)<br>      {<br>        if (cnt &gt; max)<br>        {<br>          max = cnt;<br>          res = i;<br>        }<br>      }<br>    }<br>    if (max == 0) return -1; else return res;<br>    }<br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>