            Imports Microsoft.VisualBasic<br>Imports System<br>Imports System.Collections<br> <br>Public Class grafixMask<br>    Dim t(399, 599) As Byte<br>    Dim contR As Integer() = {-1, 0, 1, 0}<br>    Dim contC As Integer() = {0, -1, 0, 1}<br> <br>    Private Function floodFill(ByVal r1 As Integer, ByVal c1 As Integer) As Integer<br>        Dim r, c, i, area As Integer<br>        Dim qr As New Queue()<br>        Dim qc As New Queue()<br> <br>        qr.Enqueue(r1)<br>        qc.Enqueue(c1)<br> <br>        While qr.Count &gt; 0<br>            r = qr.Dequeue()<br>            c = qc.Dequeue()<br>            If t(r, c) = 0 Then<br>                area += 1<br>                t(r, c) = 1<br>                For i = 0 To 3<br>                    If r + contR(i) &gt;= 0 AndAlso r + contR(i) &lt;= 399 AndAlso c + contC(i) &gt;= 0 AndAlso c + contC(i) &lt;= 599 AndAlso t(r + contR(i), c + contC(i)) = 0 Then<br>                        qr.Enqueue(r + contR(i))<br>                        qc.Enqueue(c + contC(i))<br>                    End If<br>                Next i<br>            End If<br>        End While<br> <br>        Return area<br>    End Function<br> <br>    Public Function sortedAreas(ByVal rectangles As String()) As Integer()<br>        Dim i, r, c, r1, r2, c1, c2, area As Integer<br>        Dim tok As String()<br>        Dim retList As New ArrayList()<br>        Dim ret As Integer()<br> <br>        For i = 0 To rectangles.Length - 1<br>            tok = rectangles(i).Split(" ")<br>            r1 = Val(tok(0))<br>            c1 = Val(tok(1))<br>            r2 = Val(tok(2))<br>            c2 = Val(tok(3))<br>            For r = r1 To r2<br>                For c = c1 To c2<br>                    t(r, c) = 1<br>                Next c<br>            Next r<br>        Next i<br> <br>        For r = 0 To 399<br>            For c = 0 To 599<br>                If t(r, c) = 0 Then<br>                    area = floodFill(r, c)<br>                    retList.Add(area)<br>                End If<br>            Next c<br>        Next r<br> <br>        ret = retList.ToArray(GetType(Integer))<br>        ret.Sort(ret)<br>        Return ret<br>    End Function<br>End Class<br> <br>Module MainModule<br>    Sub Main()<br>        Dim Errors As Boolean = False<br>        Dim Answer, DesiredAnswer As Integer()<br>        Dim Obj As grafixMask<br>        Dim Same As Boolean<br>        Dim i As Integer<br>        Obj = New grafixMask()<br>        Answer = Obj.sortedAreas(New String() {"0 292 399 307"})<br>        DesiredAnswer = New Integer() {116800, 116800}<br>        Console.WriteLine("Your Answer:")<br>        Console.Write("{ ")<br>        If (Answer.Length &gt; 0) Then<br>            Console.Write(Answer(0))<br>            For i = 1 To Answer.Length - 1<br>                Console.Write("  , " &amp; Answer(i))<br>            Next<br>            Console.WriteLine(" }")<br>        Else<br>            Console.WriteLine("}")<br>        End If<br>        Console.WriteLine("Desired Answer:")<br>        Console.Write("{ ")<br>        Same = (Answer.Length = DesiredAnswer.Length)<br>        If (Answer.Length &gt; 0) Then<br>            Console.Write(DesiredAnswer(0))<br>            If Answer(0) &lt;&gt; DesiredAnswer(0) Then<br>                Same = False<br>            End If<br>            For i = 1 To DesiredAnswer.Length - 1<br>                Console.Write("  , " &amp; DesiredAnswer(i))<br>                If Answer(i) &lt;&gt; DesiredAnswer(i) Then<br>                    Same = False<br>                End If<br>            Next<br>            Console.WriteLine(" }")<br>        Else<br>            Console.WriteLine("}")<br>        End If<br>        If Same Then<br>            Console.WriteLine("Match :-)")<br>        Else<br>            Console.WriteLine("DOESN'T MATCH!!!!")<br>            Errors = True<br>        End If<br>        Console.WriteLine()<br>        Obj = New grafixMask()<br>        Answer = Obj.sortedAreas(New String() {"48 192 35