            // Author:  King Larry Mak<br>// TC Handle:  Larry<br>import java.util.*;<br>public class PlayGame {<br>  public int saveCreatures(int[] you, int[] computer) {<br>    boolean used[] = new boolean[ computer.length ];<br>    <br>    Arrays.sort( computer );<br>    Arrays.sort( you );<br>    int sum = 0;<br>    int max = computer.length - 1;<br>    <br>    for ( int i = you.length - 1; i &gt;= 0; i-- ) {<br>      for ( int j = max; j &gt;= 0; j-- ) {<br>        if ( you[i] &gt; computer[j] ) {<br>          max = j - 1;<br>          sum += you[i];<br>          break;<br>        }<br>      }<br>    }<br>    return sum;<br>  }<br>  <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>