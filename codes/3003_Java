            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class WinningRecord<br>{<br>  public int[] getBestAndWorst(String games)<br>  {<br>    double best = 0;<br>    double worst = 1;<br>    <br>    int sum = 0;<br>    int[] res = new int[2];<br>    <br>    for (int i = 0; i &lt; games.length(); i++) {<br>      char c = games.charAt(i);<br>      if (c == 'W') {<br>        sum++;<br>      }<br>      <br>      if (i &gt;= 2) {<br>        double a = sum / (double) (i + 1);<br>        if (a &lt;= worst) {<br>          worst = a;<br>          res[1] = i + 1;<br>        }<br>        if (a &gt;= best) {<br>          best = a;<br>          res[0] = i + 1;<br>        }<br>      }<br>    }<br>    return res;<br>  }<br>  <br>  public static void main(String[] args)<br>  {<br>    long time;<br>    int[] answer;<br>    boolean errors = false;<br>    int[] desiredAnswer;<br>    <br>    boolean same;<br>    time = System.currentTimeMillis();<br>    answer = new WinningRecord().getBestAndWorst("WWWWWLLLLLLLLLLLLLLL");<br>    System.out.println("Time: " + (System.currentTimeMillis()-time)/1000.0 + " seconds");<br>    desiredAnswer = new int[]{ 5,  20 };<br>    System.out.println("Your answer:");<br>    if (answer.length &gt; 0)<br>    {<br>      System.out.print("\t{ " + answer[0]);<br>      for (int i=1; i&lt;answer.length; i++)<br>        System.out.print(", " + answer[i]);<br>      System.out.println(" }");<br>    }<br>    else<br>      System.out.println("\t{ }");<br>    System.out.println("Desired answer:");<br>    if (desiredAnswer.length &gt; 0)<br>    {<br>      System.out.print("\t{ " + desiredAnswer[0]);<br>      for (int i=1; i&lt;desiredAnswer.length; i++)<br>        System.out.print(", " + desiredAnswer[i]);<br>      System.out.println(" }");<br>    }<br>    else<br>      System.out.println("\t{ }");<br>    same = desiredAnswer.length == answer.length;<br>    for (int i=0; i&lt