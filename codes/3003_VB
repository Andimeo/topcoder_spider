            Imports Microsoft.VisualBasic<br>Imports System<br>Imports System.Collections<br> <br>Public Class WinningRecord<br>    Public Function getBestAndWorst(ByVal games As String) As Integer()<br>        Dim i, gamesConsidered, gamesWon, bestcount, worstcount As Integer<br>        Dim best, worst, perc As Double<br> <br>        worst = 2<br>        For i = 0 To games.length - 1<br>            gamesConsidered += 1<br>            If games.chars(i) = "W" Then gamesWon += 1<br>            If gamesConsidered &gt;= 3 Then<br>                perc = gamesWon / gamesConsidered<br>                If perc &gt;= best Then<br>                    best = perc<br>                    bestcount = gamesConsidered<br>                End If<br>                If perc &lt;= worst Then<br>                    worst = perc<br>                    worstcount = gamesConsidered<br>                End If<br>            End If<br>        Next i<br> <br>        Return New Integer() {bestcount, worstcount}<br>    End Function<br>End Class<br> <br>Module MainModule<br>    Sub Main()<br>        Dim Errors As Boolean = False<br>        Dim Answer, DesiredAnswer As Integer()<br>        Dim Obj As WinningRecord<br>        Dim Same As Boolean<br>        Dim i As Integer<br>        Obj = New WinningRecord()<br>        Answer = Obj.getBestAndWorst("WWWWWLLLLLLLLLLLLLLL")<br>        DesiredAnswer = New Integer() {5, 20}<br>        Console.WriteLine("Your Answer:")<br>        Console.Write("{ ")<br>        If (Answer.Length &gt; 0) Then<br>            Console.Write(Answer(0))<br>            For i = 1 To Answer.Length - 1<br>                Console.Write("  , " &amp; Answer(i))<br>            Next<br>            Console.WriteLine(" }")<br>        Else<br>            Console.WriteLine("}")<br>        End If<br>        Console.WriteLine("Desired Answer:")<br>        Console.Write("{ ")<br>        Same = (Answer.Length = DesiredAnswer.Length)<br>        If (Answer.Length &gt; 0) Then<br>            Consol