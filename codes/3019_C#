<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>using System.Collections;<br> <br>public class bloggoHyphenate<br>{<br>  public string[] correct(string[] dict,string[] cand)<br>  {<br>    int i,j;<br>    string[] res=new string[cand.Length];<br>    for (i=0; i&lt;res.Length; i++)<br>    {<br>      j=0;<br>      while (j&lt;dict.Length &amp;&amp; string.Compare(dict[j].Replace(" ",""),cand[i].Replace("-",""))!=0) j++;<br>      if (j==dict.Length) res[i]=cand[i].Replace("-","");<br>      else<br>      {<br>        int pos=cand[i].IndexOf('-');<br>        int k,c,rr;<br>        c=0;<br>        rr=-2;<br>        for (k=0; k&lt;dict[j].Length; k++)<br>          if (dict[j][k]==' ')<br>            if (k-c==pos){ rr=-1; break; }<br>            else<br>            {<br>              if (rr==-2 || Math.Abs(k-c-pos)&lt;Math.Abs(rr-pos)) rr=k-c;<br>              c++;<br>            }<br>        if (rr==-1) res[i]="correct";<br>        else if (rr==-2) res[i]=cand[i].Replace("-","");<br>        else res[i]=cand[i].Replace("-","").Insert(rr,"-");<br>      }<br>    }<br>    return res;<br>  }<br>}<br></td>