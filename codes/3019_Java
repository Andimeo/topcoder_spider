            public class bloggoHyphenate {<br>public String[] correct(String[] dictionary, String[] candidates) {<br>    for(int i=candidates.length;i--&gt;0;) {<br>  String s = candidates[i];<br>  int hpos = s.indexOf('-');<br>  String s2 = s.replaceAll("-", "");<br>  for(int j=dictionary.length;j--&gt;0;) {<br>      String s3 = dictionary[j].replaceAll(" ", "");<br>      if(!s3.equals(s2)) continue;<br>      String z = dictionary[j];<br>      int best = -1, score = 10000000;<br>      while(true) {<br>    int p = z.indexOf(' ');<br>    if(p&lt;0) break;<br>    int sc = Math.abs(p-hpos)*2 + (hpos&lt;p ? 1 : 0);<br>    if(sc&lt;score) { score=sc; best=p; }<br>    z = z.substring(0,p) + z.substring(p+1);<br>      }<br>      if(best&gt;0) s2 = s2.substring(0,best) + "-" + s2.substring(best);<br>      candidates[i] = s2.equals(s) ? "correct" : s2;<br>  }<br>    }<br>    return candidates;<br>}}<br></td>