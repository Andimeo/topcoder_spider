            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class bloggoShortcuts<br>{<br>  public String expand(String text)<br>  {<br>    String result = "";<br>    <br>    boolean mode = false;<br>    for (int i=0;i&lt;text.length();i++)<br>    {<br>      if (text.charAt(i) == '_')<br>      {<br>        result = result + (mode ? "&lt;/i&gt;" : "&lt;i&gt;");<br>        mode = !mode;<br>      } else if (text.charAt(i) == '*')<br>      {<br>        result = result + (mode ? "&lt;/b&gt;" : "&lt;b&gt;");<br>        mode = !mode;<br>      } else result = result + text.charAt(i);<br>    }<br>  <br>    return result;<br>  }<br>  <br>  public static void main(String[] args)<br>  {<br>    long time;<br>    String answer;<br>    boolean errors = false;<br>    String desiredAnswer;<br>    <br>    time = System.currentTimeMillis();<br>    answer = new bloggoShortcuts().expand("You _should_ see the new walrus at the zoo!");<br>    System.out.println("Time: " + (System.currentTimeMillis()-time)/1000.0 + " seconds");<br>    desiredAnswer = "You &lt;i&gt;should&lt;/i&gt; see the new walrus at the zoo!";<br>    System.out.println("Your answer:");<br>    System.out.println("\t\"" + answer + "\"");<br>    System.out.println("Desired answer:");<br>    System.out.println("\t\"" + desiredAnswer + "\"");<br>    if (!answer.equals(desiredAnswer))<br>    {<br>      errors = true;<br>      System.out.println("DOESN'T MATCH!!!!");<br>    }<br>    else<br>      System.out.println("Match :-)");<br>    System.out.println();<br>    time = System.currentTimeMillis();<br>    answer = new bloggoShortcuts().expand("Move it from *Accounts Payable* to *Receiving*.");<br>    System.out.println("Time: " + (System.currentTimeMillis()-time)/1000.0 + " seconds");<br>    desiredAnswer = "Move it from &lt;b&gt;Accounts Payable&lt;/b&gt; to &lt;b&gt;Receiving&lt;/b&gt;.";<br>    System.out.println("Your answer:");<br>    System.out.println("\t\"" + answer + "\"");<br>    System.out.println("Desired answer:");<br>    System.out.println("\t\"" + desiredAnswer + "\"");<br>    if (!answer.equals(desiredAnswer))<br>    {<br>      errors = true;<br>      System.out.println("DOESN'T MATCH!!!!");<br>    }<br>    else<br>      System.out.println("Match :-)");<br>    System.out.println();<br>    time = System.currentTimeMillis();<br>    answer = new bloggoShortcuts().expand("I saw _Chelydra serpentina_ in *Centennial Park*.");<br>    System.out.println("Time: " + (System.currentTimeMillis()-time)/1000.0 + " seconds");<br>    desiredAnswer = "I saw &lt;i&gt;Chelydra serpentina&lt;/i&gt; in &lt;b&gt;Centennial Park&lt;/b&gt;.";<br>    System.out.println("Your answer:");<br>    System.out.println("\t\"" + answer + "\"");<br>    System.out.println("Desired answer:");<br>    System.out.println("\t\"" + desiredAnswer + "\"");<br>    if (!answer.equals(desiredAnswer))<br>    {<br>      errors = true;<br>      System.out.println("DOESN'T MATCH!!!!");<br>    }<br>    else<br>      System.out.println("Match :-)");<br>    System.out.println();<br>    time = System.currentTimeMillis();<br>    answer = new bloggoShortcuts().expand(" _ _ __  _ yabba dabba _   *  dooooo  * ****");<br>    System.out.println("Time: " + (System.currentTimeMillis()-time)/1000.0 + " seconds");<br>    desiredAnswer = " &lt;i&gt; &lt;/i&gt; &lt;i&gt;&lt;/i&gt;  &lt;i&gt; yabba dabba &lt;/i&gt;   &lt;b&gt;  dooooo  &lt;/b&gt; &lt;b&gt;&lt;/b&gt;&lt;b&gt;&lt;/b&gt;";<br>    System.out.println("Your answer:");<br>    System.out.println("\t\"" + answer + "\"");<br>    System.out.println("Desired answer:");<br>    System.out.println("\t\"" + desiredAnswer + "\"");<br>    if (!answer.equals(desiredAnswer))<br>    {<br>      errors = true;<br>      System.out.println("DOESN'T MATCH!!!!");<br>    }<br>    else<br>      System.out.println("Match :-)");<br>    System.out.println();<br>    time = System.currentTimeMillis();<br>    answer = new bloggoShortcuts().expand("_now_I_know_*my*_ABC_next_time_*sing*it_with_me");<br>    System.out.println("Time: " + (System.currentTimeMillis()-time)/1000.0 + " seconds");<br>    desiredAnswer = "&lt;i&gt;now&lt;/i&gt;I&lt;i&gt;know&lt;/i&gt;&lt;b&gt;my&lt;/b&gt;&lt;i&gt;ABC&lt;/i&gt;next&lt;i&gt;time&lt;/i&gt;&lt;b&gt;sing&lt;/b&gt;it&lt;i&gt;with&lt;/i&gt;me";<br>    System.out.println("Your answer:");<br>    System.out.println("\t\"" + answer + "\"");<br>    System.out.println("Desired answer:");<br>    System.out.println("\t\"" + desiredAnswer + "\"");<br>    if (!answer.equals(desiredAnswer))<br>    {<br>      errors = true;<br>      System.out.println("DOESN'T MATCH!!!!");<br>    }<br>    else<br>      System.out.println("Match :-)");<br>    System.out.println();<br>    <br>    if (errors)<br>      System.out.println("Some of the test cases had errors :-(");<br>    else<br>      System.out.println("You're a stud (at least on the test data)! :-D ");<br>  }<br>}<br> <br>//Powered by [KawigiEdit]<br></td>