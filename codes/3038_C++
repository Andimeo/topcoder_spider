;<br>ld inf=1e150;<br><br>ld go(int i,int j,int t)<br>{<br>  if(i==4)<br>    if(t==0)<br>      return 0;<br>    else<br>      return -inf;<br>  int nexti=i,nextj=j+1;<br>  if(nexti&lt;nextj)<br>  {<br>    nexti++;nextj=0;<br>  }<br>  tcache::iterator it=ch.find(piii(pii(i,j),t));<br>  if(it!=ch.end())return it-&gt;second;<br>  ld&amp; r=ch[piii(pii(i,j),t)];<br>  r=-inf;<br>  for(int val=((i==j)?1:-10);val&lt;=10;val++)<br>  {<br>    r&gt;?=oc[i][j]*val+go(nexti,nextj,t-val*((i!=j)?2:1));<br>  }<br>  return r;<br>}<br><br>class BioScore<br>{<br>public:<br>  double maxAvg(vector &lt;string&gt; inp)<br>  {<br>    int i,j,k,n;<br>    n=inp.size();<br>    rep(i,inp.size())<br>      for(j=i+1;j&lt;inp.size();j++)<br>        rep(k,inp[0].size())<br>        {<br>          int p=let.find(inp[i][k]);<br>          int s=let.find(inp[j][k]);<br>          if(p&lt;s)swap(p,s);<br>          oc[p][s]++;<br>        }<br>    rep(i,4)cout&lt;&lt;vtostr(oc[i])&lt;&lt;endl;<br>    return go(0,0,0)/(n*(n-1)/2);<br>  }<br>};<br><br><br>// Powered by FileEdit<br></td>