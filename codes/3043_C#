            using System;<br>using System.Collections;<br><br>public class Generators <br>{<br>  public int[] find(int p) <br>  {<br>    ArrayList ret = new ArrayList();<br>    for(int i=1;i&lt;p;i++)<br>    {<br>      if(isgen(i,p)) ret.Add(i);<br>    }<br>    return (int[])ret.ToArray(typeof(int));<br>  }<br>  bool isgen(int num, int prime)<br>  {<br>    Hashtable found = new Hashtable();<br>    int acc = 1;<br>    for(int i=1;i&lt;prime;i++)<br>    {<br>      if(!found.Contains(acc)) found.Add(acc,null);<br>      acc *= num;<br>      acc %= prime;<br>    }<br>    return found.Count == prime - 1;<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>