            import java.util.*;<br>import java.util.regex.*;<br> <br>public class Generators {<br>    public int[] find(int p) {<br>        int[] rv = new int[1500];<br>  int ind = 0;<br>  for (int i=2; i &lt; p; i++) {<br>      boolean [] ba = new boolean[p];<br>      int cnt = 0;<br>      int tv = i;<br>      for (int j=0; j &lt; p-1; j++) {<br>    if (ba[tv] == false) {<br>        cnt++;<br>        ba[tv] = true;<br>    }<br>    tv = (tv * i) % p;<br>      }<br>      if (cnt == p-1) {<br>    rv[ind++] = i;<br>      }<br>  }<br>  int [] rrv = new int[ind];<br>  System.arraycopy(rv, 0, rrv, 0, ind);<br>  <br>        return rrv;<br>    }<br> <br>}<br> <br>// Powered by CodeProcessor<br></td>