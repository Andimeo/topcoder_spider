            #include&lt;set&gt;<br>#include&lt;algorithm&gt;<br>#include&lt;iostream&gt;<br>#include&lt;sstream&gt;<br>#include&lt;string&gt;<br>#include&lt;vector&gt;<br> <br>using namespace std;<br>#define FOR(i,a,b) for(int i=(a);i&lt;=(b);++i)<br>#define MP make_pair<br>#define SIZE(t) ((signed)(t).size())<br>template&lt;class t&gt;inline t gcd(t a,t b){a=abs(a),b=abs(b);for(;;){if(!b)return a;a%=b;if(!a)return b;b%=a;}}<br> <br>struct FractionCounting {<br> <br>public: int howMany(int w, int x, int y, int z) {<br> <br>  set&lt;pair&lt;int,int&gt; &gt;se;<br>  FOR(p,w,x) FOR(q,y,z) {<br>    int pp = p , qq = q;<br>    int g = gcd(p,q);<br>    pp /= g; qq /= g;<br>    se.insert(MP(pp,qq));<br>  }<br>  return SIZE(se);<br>}};<br></td>