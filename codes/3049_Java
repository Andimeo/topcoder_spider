            import java.util.*;<br> <br>public class FractionCounting {<br>  public int howMany(int w, int x, int y, int z) {<br>    Set&lt;String&gt; s = new TreeSet&lt;String&gt;();<br>    for (int i = w; i &lt;= x; i++) {<br>      for (int j = y; j &lt;= z; j++) {<br>        int gcdd = gcd(i,j);<br>        int num = i/gcdd;<br>        int den = j/gcdd;<br>        s.add(num+","+den);<br>      }<br>    }<br>    return s.size();<br>  }<br>  public int gcd(int a, int b) {<br>    if (a == 0) return b;<br>    return gcd(b%a, a);<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>