            using System;<br>using System.Collections;<br>using math = System.Math;<br> <br>public class FractionSplit<br>{<br>  public string[] getSum(int n, int d)<br>  {<br>    long a, b;<br>    a = n;<br>    b = d;<br>    ArrayList s = new ArrayList();<br>    while(a&gt;0)<br>    {<br>      long c = b/a;<br>      if (c*a &lt; b) c++;<br>      long x = a*c-b;<br>      long y = b*c;<br>      a = x;<br>      b = y;<br>      s.Add( "1/"+c.ToString());<br> <br>    }<br> <br>    string[] res = new string[s.Count];<br>    for(int i=0;i&lt;res.Length;i++)<br>      res[i] = (string)s[i];<br>    return res;<br>      <br>  }<br>}<br></td>