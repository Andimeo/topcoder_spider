            import java.util.*;<br> <br>public class FractionSplit<br>{<br>    public String[] getSum(int n, int d)<br>    {<br>        List ret = new ArrayList();<br>        while (n &gt; 0)<br>        {<br>            int x = (d+n-1)/n;<br>            ret.add("1/"+x);<br>            n = n*x - d;<br>            d = d * x;<br>        }<br>        return toStringArray(ret);<br>    }<br> <br>    //Converts a Collection of String in an String[]<br>    public static String[] toStringArray(Collection c)<br>    {<br>        String ret[] = new String[c.size()];<br>        Iterator it = c.iterator();<br>        <br>        for (int i = 0; i&lt;ret.length; i++)<br>        {<br>            ret[i] = "" + it.next();<br>        }<br>        return ret;<br>    }<br> <br>}<br></td>