            import java.util.*;<br> <br>public class TaglishTranslator<br>{<br>  public String translate(String x)<br>  {<br>      String subject = null;<br>      String verb = null;<br>      String object = null;<br>      boolean the = false;<br>      boolean will = false;<br>    boolean ed = false;<br>    boolean to = false;<br>    boolean the2 = false;<br>    <br>    StringTokenizer t = new StringTokenizer(x, " \t\n\r");<br>    subject = t.nextToken();<br>    if (subject.equalsIgnoreCase("The"))<br>    {<br>        the = true;<br>      subject = t.nextToken();<br>    }  <br>    <br>    verb = t.nextToken();<br>    if (verb.equalsIgnoreCase("Will"))<br>    {<br>        will = true;<br>        verb = t.nextToken();<br>    }<br>    else if (verb.length() &gt;= 2 &amp;&amp; verb.substring(verb.length() - 2).equalsIgnoreCase("ed"))<br>    {<br>        ed = true;<br>    }<br>    <br>    object = t.hasMoreTokens() ? t.nextToken() : null;<br>    if (object != null)<br>    {<br>       if (object.equalsIgnoreCase("to"))<br>       {<br>           to = true;<br>           object = t.nextToken();<br>       }<br>       <br>       if (object.equalsIgnoreCase("the"))<br>       {<br>           the2 = true;<br>           object = t.nextToken();<br>       }<br>    }<br>    <br>    if (ed)<br>        verb = "nag" + verb.substring(0, verb.length() - 2);<br>    else if (will)<br>      verb = "mag" + fs(verb, true) + fs(verb, true) + fs(verb, false);<br>    else<br>        verb = "nag" + fs(verb, true) + fs(verb, true) + fs(verb, false);<br>    <br>    String result = verb + " " + (the ? "ang " : "si ") + subject;<br>    if (object != null)<br>    {<br>        result += " ";<br>        if (!the2 &amp;&amp; !to)<br>            result += "ni ";<br>        else if (the2 &amp;&amp; !to)<br>            result += "ng ";<br>        else if (the2 &amp;&amp; to)<br>            result += "sa ";<br>        else<br>            result += "kay ";<br>            result +=  object;<br>    }<br>    <br>    return result;<br>  }<br>  <br>  protected static String fs(String x, boolean f)<br>  {<br>      int i = 0;<br>     for (i = 0; i &lt; x.length() &amp;&amp; Character.isLetter(x.charAt(i)) &amp;&amp; "AEIOU".indexOf("" + x.charAt(i)) &lt; 0 &amp;&amp; "aeiou".indexOf("" + x.charAt(i)) &lt; 0; i++);<br>     if (f)<br>     {<br>         if (i &lt; x.length())<br>           return x.substring(0, i + 1);<br>         else<br>             return x;<br>     }<br>     else<br>         return i &lt; x.length() - 1 ? x.substring(i + 1) : ""; <br>  }<br>}<br></td>