            #include &lt;iostream&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>using namespace std;<br>typedef vector&lt;int&gt; VI; typedef vector&lt;VI&gt; VVI;<br>typedef vector&lt;string&gt; VS; typedef vector&lt;VS&gt; VVS;<br>typedef long long i64; typedef unsigned long long u64;<br><br>int a, b;<br>int u[7][7];<br><br>int doit(int x, int y) {<br>  if( u[y][x] ) return 0;<br>  if( x == 0 || y == 0 || x == a || y == b ) return 1;<br>  int ret = 0;<br>  u[y][x] = 1;<br>  ret += doit(x+1, y);<br>  ret += doit(x-1, y);<br>  ret += doit(x, y-1);<br>  ret += doit(x, y+1);<br>  u[y][x] = 0;<br>  return ret;<br>}<br><br>class RectangleDivision {<br>public:<br>int count(int A, int B) {<br>  a = A; b = B;<br>  int i, j, k, x, y, z, n;<br>  int ret = 0;<br><br>  for( i = 1; i &lt; a; i++ ) {<br>    memset(u, 0, sizeof(u));<br>    u[0][i] = 1;<br>    ret += doit(i, 1);<br>  }<br>  for( i = 1; i &lt; b; i++ ) {<br>    memset(u, 0, sizeof(u));<br>    u[i][0] = 1;<br>    ret += doit(1, i);<br>  }<br>  return ret;<br>}<br>};<br><br><br>// Powered by FileEdit<br></td>