            using System;<br>using System.Collections;<br><br>public class HiddenNumbers <br>{<br>  public string[] findAll(string[] text) <br>  {<br>    string res = "";<br>    char[] temp = new char[50*50];<br>    for(int i=0;i&lt;50*50;i++) temp[i] = ' ';<br>    foreach(string s in text)<br>    {<br>      res += s;<br>    }<br>    for(int i=0;i&lt;res.Length;i++)<br>    {<br>      if(char.IsDigit(res[i]))<br>      {<br>        temp[i] = res[i];<br>      }<br>      else<br>      {<br>        temp[i] = ' ';<br>      }<br>    }<br>    res = new string(temp);<br>    ArrayList ret = new ArrayList();<br>    string[] nums = res.Split(new char[]{' '});<br>    foreach(string s in nums)<br>    {<br>      if(s.Length &gt; 0)<br>      {<br>        ret.Add(s);<br>      }<br>    }<br>    ret.Sort(new Comparer());<br>    string[] realret = new string[(ret.Count + 1)/ 2];<br>    int start = (ret.Count)/2;<br>    for(int i=start;i&lt;ret.Count;i++)<br>    {<br>      realret[i - start] = (string)ret[i];<br>    }<br>    return realret;<br>  }<br>  public class Comparer : IComparer<br>  {<br>    #region IComparer Members<br><br>    public int Compare(object x, object y)<br>    {<br>      string a = x as string;<br>      string b = y as string;<br>      ulong aa = ulong.Parse(a);<br>      ulong bb = ulong.Parse(b);<br>      if(aa != bb)<br>      {<br>        return aa.CompareTo(bb);<br>      }<br>      return a.Length.CompareTo(b.Length);<br>    }<br><br>    #endregion<br><br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>