            import java.util.*;<br>public class HiddenNumbers implements Comparator {<br>  public int compare(Object a, Object b) {<br>    String s = (String) a;<br>    String t = (String) b;<br>    long m = Long.parseLong(s);<br>    long n = Long.parseLong(t);<br>    if (m != n) return m &lt; n ? -1 : 1;<br>    else return s.length() - t.length();<br>  }<br>  public String[] findAll(String[] text) {<br>    String s = "";<br>    for (int i = 0; i &lt; text.length; i++) s += text[i];<br>    StringTokenizer t = new StringTokenizer(s, "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz ");<br>    ArrayList l = new ArrayList();<br>    while (t.hasMoreTokens()) l.add(t.nextToken());<br>    Collections.sort(l, this);<br>    int n = l.size();<br>    int m = (n + 1) / 2;<br>    String[] r = new String[m];<br>    int j = 0;<br>    for (int i = n - m; i &lt; n; i++)<br>      r[j++] = (String) l.get(i);<br>    return r;<br>  }<br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>