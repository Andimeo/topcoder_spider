            import java.util.*;<br>public class BusinessPlan {<br>  public int howLong(int[] expense, int[] revenue, int[] ptime, int C, int D) {<br>    long[] money = new long[2000000];<br>    money[0] = C;<br>    for (int t=0; t &lt; money.length-200000; ++t) {<br>      if (money[t] &gt;= D) return t;<br>      for (int i=0; i &lt; expense.length; ++i) {<br>        if (expense[i] &lt;= money[t]) {<br>          money[t+ptime[i]] = Math.max(money[t+ptime[i]],money[t]+revenue[i]-expense[i]);<br>        }<br>      }<br>    }<br>    return -1;<br>  }<br>  <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>