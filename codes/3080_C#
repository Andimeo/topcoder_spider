            using System;<br>using System.Text;<br>using System.Collections;<br><br>public class Cafeteria {<br>    public string latestTime(int[] offset, int[] walkingTime, int[] drivingTime) {<br>        string res;<br>    <br>    int n = offset.Length;<br><br>    int max = 0;<br>    for (int i = 0; i &lt; n; i++)<br>    {<br>      for (int h = 1; h &lt;= 14; h++)<br>        for (int m = 0; m &lt;= 5; m++)<br>        {<br>                    int cs = h*60 + m*10 + offset[i];<br>          if (cs + drivingTime[i] &gt; 14*60 + 30) continue;<br>          cs -= walkingTime[i];<br>          if (cs &gt; max) max = cs;<br>        }<br><br>    }<br><br>    int mm = max % 60, hh = max / 60;<br>    if (hh &gt;= 13) res = (hh-12).ToString("00") + ":" + mm.ToString("00"); else<br>      res = hh.ToString("00") + ":" + mm.ToString("00");<br>        return res;<br>    }<br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>