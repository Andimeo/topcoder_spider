            import java.util.*;<br>public class Cafeteria<br>{<br>    public String latestTime(int[] offset, int[] walkingTime, int[] drivingTime)<br>    {<br>    int endtime = 14*60+30;<br>    int latest = 0;<br>    for (int i=0;i&lt;offset.length;i++) {<br>      int leave = endtime - drivingTime[i];<br>      leave = (leave-offset[i]) / 10 * 10 + offset[i];<br>      leave -= walkingTime[i];<br>      if (leave &gt; latest) latest=leave;<br>    }<br>    String min = "" + latest%60;<br>    if (latest%60&lt;10) min = "0"+min;<br>    int hour = latest / 60;<br>    if (hour &gt; 12) hour -= 12;<br>        return "" + (hour&lt;10?"0":"") + hour + ":" + min;<br>    }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>