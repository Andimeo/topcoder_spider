<td class="problemText" colspan="8" valign="middle" align="left">
            Public Class Cafeteria<br>        Public Function latestTime(ByVal offset As Integer(), ByVal walkingTime As Integer(), ByVal drivingTime As Integer()) As String<br>            Dim i, j, k As Integer<br>            Dim iLatest As Integer = 0<br> <br>            For i = 0 To offset.GetUpperBound(0)<br>                j = offset(i) + drivingTime(i)<br>                Do While j &lt;= 860<br>                    j += 10<br>                Loop<br>                k = j - drivingTime(i) - walkingTime(i)<br>                If k &gt; iLatest Then iLatest = k<br>            Next<br> <br>            If iLatest &gt;= 780 Then iLatest -= 720<br> <br>            Return Microsoft.VisualBasic.Format(iLatest \ 60, "00") &amp; ":" &amp; Microsoft.VisualBasic.Format(iLatest Mod 60, "00")<br> <br> <br>        End Function<br> <br>    End Class<br></td>