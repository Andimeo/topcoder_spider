            #include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;map&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>using namespace std;<br> <br>#define ll long long<br>#define df double<br>#define vi vector&lt;int&gt;<br>#define vs vector&lt;string&gt;<br>#define fo(i,n) for(i=0;i&lt;(n);i++)<br>#define fir(n) fo(i,n)<br>#define fjr(n) fo(j,n)<br>#define fkr(n) fo(k,n)<br>#define flr(n) fo(l,n)<br>#define fi fir(n)<br>#define fj fjr(n)<br>#define fk fkr(n)<br>#define fl flr(n)<br>#define pb(x) push_back(x)<br>#define sz size()<br>#define ins(x) insert(x)<br>#define cs c_str()<br>#define srt(x) sort(x.begin(),x.end())<br>#define mp(x,y) make_pair(x,y)<br> <br> <br> <br>vs tk(string f, char *d) {<br>vs ret;<br>char *buf,*s;<br>buf=(char *) malloc(f.sz+1);<br>strcpy(buf,f.cs);<br>for(s=strtok(buf,d);s &amp;&amp; *s;s=strtok(0,d)) {<br>  ret.pb(s);<br>}<br>free(buf);<br>return ret;<br>}<br> <br>vi tki(string f, char *d) {<br>vi ret;<br>vs g;<br>g=tk(f,d);<br>int i;<br>fir(g.sz) ret.pb(atoi(g[i].cs));<br>return ret;<br>}<br> <br>void print(vs x) { int i; fir(x.sz) printf("[%s] ",x[i].cs); puts(""); return; }<br>void print(vi x) { int i; fir(x.sz) printf("%d ",x[i]); puts(""); return; }<br> <br>int ret;<br>#define r(x) ((x)*(x)*(x))<br> <br>int cr(int n) {<br>if(!n) return 0;<br>int i=0,j;<br>for(j=1024;j&gt;=1;j/=2) {<br>if(r(i+j)&lt;=n) i+=j;<br>}<br>return i;<br>}<br> <br>class CubeSum { public: int count(int n) {<br>int a,b,c,d;<br> <br>fo(a,30) printf("%d %d\n",a,cr(a));<br> <br>for(a=0;a*a*a&lt;=n;a++) <br>for(b=0;b&lt;=a;b++)<br>for(c=0;c&lt;=b;c++) {<br>d=cr(n-r(a)-r(b)-r(c));<br> <br>//for(d=0;d&lt;=c;d++) <br>if(d&lt;=c &amp;&amp; r(a)+r(b)+r(c)+r(d)==n) ret++;<br>}<br>return ret;<br> <br>}};<br></td>