            #include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cmath&gt;<br>#include &lt;queue&gt;<br>using namespace std;<br>typedef vector&lt;int&gt; VI;  typedef vector&lt;vector&lt;int&gt; &gt; VVI;<br>typedef vector&lt;string&gt; VS;  typedef vector&lt;vector&lt;string&gt; &gt; VVS;<br>typedef signed long long i64;  typedef unsigned long long u64;<br><br>class ParameterSubstitution {<br>public:<br>string processParams(string a, vector &lt;string&gt; b) {<br>  int i, j, k, x, y, z, n;<br>  string ret;<br><br>  for( i = 0; i &lt; a.size(); i++ ) {<br>    if( a[i] != '$' || i == a.size()-1 || a[i+1] == '0' ) {<br>      ret += a[i];<br>      continue;<br>    }<br>    x = y = 0;<br>    for( j = i+1; j &lt; a.size(); j++ ) {<br>      if( !isdigit(a[j]) ) break;<br>      y = x*10 + a[j]-'0';<br>      if( y &gt; b.size() ) break;<br>      x = y;<br>    }<br>    if( !x ) {<br>      ret += a[i];<br>      continue;<br>    } else {<br>      i = j-1;<br>      ret += b[x-1];<br>    }<br>  }<br>  return ret;<br>}<br>};<br><br><br>// Powered by FileEdit<br></td>