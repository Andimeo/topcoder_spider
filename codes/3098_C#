            public class ComboBoxKeystrokes<br>  {<br>    int[,] edge = new int[100, 100];<br>    int[,] dist = new int[100, 100];<br> <br>    public int minimumKeystrokes(string[] elements)<br>    {<br>      string aa = "qwertyuiopasdfghjklzxcvbnm";<br>      string AA = "QWERTYUIOPASDFGHJKLZXCVBNM";<br>      int i,j,k,n;<br>      n = elements.Length;<br>      for(i=0;i&lt;elements.Length;i++)<br>        for(j=0;j&lt;aa.Length;j++)<br>        {<br>          edge[i,j] = i;<br>          for(k=i+1;k&lt;elements.Length;k++)<br>            if ((elements[k][0] == aa[j])||(elements[k][0] == AA[j]))<br>            {<br>              edge[i,j] = k;<br>              break;<br>            }<br>          if (edge[i,j] == i)<br>            for(k=0;k&lt;i;k++)<br>              if ((elements[k][0] == aa[j])||(elements[k][0] == AA[j]))<br>              {<br>                edge[i,j] = k;<br>                break;<br>              }<br> <br>        }<br> <br>      for(i=0;i&lt;n;i++)<br>        for(j=0;j&lt;n;j++)<br>          dist[i,j] = 1&lt;&lt;20;<br>      for(i=0;i&lt;n;i++)<br>        for(j=0;j&lt;aa.Length;j++)<br>          dist[i, edge[i,j]] = 1;<br>      for(i=0;i&lt;n;i++)<br>        dist[i,i] = 0;<br> <br>      for(k=0;k&lt;n;k++)<br>        for(i=0;i&lt;n;i++)<br>          for(j=0;j&lt;n;j++)<br>            if (dist[i,k] + dist[k,j] &lt; dist[i,j])<br>              dist[i,j] = dist[i,k] + dist[k,j];<br>      k = 0;<br>      for(i=0;i&lt;n;i++)<br>      {<br>        for(j=0;j&lt;n;j++)<br>          if (dist[i,j] &gt; k) k = dist[i,j];<br>      }<br>      return k;<br>    }<br>  }<br></td>