            import java.util.*;<br>public class ComboBoxKeystrokes {<br>  public int minimumKeystrokes(String[] e) {<br>    int INF = 10000;<br>    int n = e.length;<br>    int[][] d = new int[n][n];<br>    for (int i = 0; i &lt; n; i++) {<br>      Arrays.fill(d[i], INF);<br>      d[i][i] = 0;<br>      for (int j = 0; j &lt; 26; j++)<br>        for (int k = 1; k &lt; n; k++)<br>          if ((char) ('A' + j) == Character.toUpperCase(e[(i + k) % n].charAt(0))) {<br>            d[i][(i + k) % n] = 1;<br>            break;<br>          }<br>    }<br>    for (int k = 0; k &lt; n; k++)<br>      for (int i = 0; i &lt; n; i++)<br>        for (int j = 0; j &lt; n; j++)<br>          d[i][j] = Math.min(d[i][j], d[i][k] + d[k][j]);<br>    int r = 0;<br>    for (int i = 0; i &lt; n; i++)<br>      for (int j = 0; j &lt; n; j++)<br>        r = Math.max(r, d[i][j]);<br>    return r;<br>  }<br>}<br></td>