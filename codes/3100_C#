            #include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;cmath&gt;<br>#include &lt;iostream&gt;<br> <br>using namespace std;<br> <br>class AccessLevel {<br>  public:<br>  string canAccess(vector &lt;int&gt; rights, int minPermission) {<br>    string ret;<br>    <br>    ret = "";<br>    for(int i=0;i&lt;rights.size();i++)<br>    if(rights[i]&gt;=minPermission)<br>    ret+="A";<br>    else<br>    ret+="D";<br>    return ret;<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>