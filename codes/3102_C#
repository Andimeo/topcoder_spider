            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class FolderSize <br>{<br>  public int[] calculateWaste(string[] files, int folderCount, int clusterSize) <br>  {<br>    int[] res = new int[folderCount];<br>    int i;<br>    Array.Clear(res,0,res.Length);<br>    for(i=0;i&lt;files.Length;i++)<br>    {<br>      string[] buf=files[i].Split(" ".ToCharArray());<br>      int k=Int32.Parse(buf[0]),p=Int32.Parse(buf[1]);<br>      if(p%clusterSize!=0)res[k]+=clusterSize-p%clusterSize;<br>    }<br>    return res;<br>  }<br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>