            import java.util.*;<br>public class FolderSize {<br>  public int[] calculateWaste(String[] files, int folderCount, int clusterSize) {<br>    int[] ret = new int[folderCount];<br>    for(int i = 0; i &lt; files.length; i++){<br>      StringTokenizer st = new StringTokenizer(files[i]);<br>      int folder = Integer.parseInt(st.nextToken());<br>      int space = Integer.parseInt(st.nextToken())%clusterSize;<br>      if(space != 0)ret[folder] += clusterSize-space;<br>    }<br>    return ret;<br>  }<br> <br> <br>// ##################### NO POINT LOOKING BELOW, TESTCODE ONLY ####################<br> <br> <br>/** begin cut - don't modify this line*/<br>  public static void main(String[] a) {<br>    new FolderSize().runTestCase(0);<br>    new FolderSize().runTestCase(1);<br>    new FolderSize().runTestCase(2);<br>    new FolderSize().runTestCase(3);<br>  }<br> <br>  public void runTestCase(int nbr) {<br>    switch(nbr) {<br>      case 0 : {<br>        checkOutput(calculateWaste(new String[] {"0 55", "0 47", "1 86"}, 3, 50), new int[] { 48,  14,  0 }, 0); break;<br>      }<br>      case 1 : {<br>        checkOutput(calculateWaste(new String[] {"0 123", "2 456", "4 789", "6 012", "8 345"}, 10, 98), new int[] { 73,  0,  34,  0,  93,  0,  86,  0,  47,  0 }, 1); break;<br>      }<br>      case 2 : {<br>        checkOutput(calculateWaste(new String[] {}, 5, 100), new int[] { 0,  0,  0,  0,  0 }, 2); break;<br>      }<br>      case 3 : {<br>        checkOutput(calculateWaste(new String[] {"0 93842", "1 493784", "2 43212", "3 99327", "4 456209", "5 947243", "6 59348", "7 58237", "8 5834", "9 492384", "0 58342", "3 538432", "6 1432", "9 453983", "2 4321", "4 583729", "6 6974", "8 9864", "4 43211", "8 38437"}, 10, 22485), new int[] { 27696,  886,  19922,  14306,  18616,  19612,  44671,  9218,  35805,  20488 }, 3); break;<br>      }<br>    }<br>  }<br>  final void checkOutput(int mine, int them, int nbr) {<br>    boolean success = (mine==them);<br>    StringBuffer out = ne