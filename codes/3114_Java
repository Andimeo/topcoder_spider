            public class PayBill {<br>  public int[] whoPaid(int[] meals, int totalMoney) {<br>    int n = meals.length;<br>    <br>    char p[][] = new char[n+1][totalMoney+1];<br>    for (int i = 0; i &lt;= totalMoney; i++)<br>      p[0][i] = 'n';<br>    p[0][0] = 'y';<br>    <br>    for (int i = 0; i &lt; n; i++) {<br>      for (int j = 0; j &lt;= totalMoney; j++)<br>        p[i+1][j] = 'n';<br>      <br>      for (int j = 0; j &lt;= totalMoney; j++)<br>        if (p[i][j] == 'y') {<br>          p[i+1][j] = 'y';<br>          int k = j + meals[i];<br>          if (k &lt;= totalMoney)<br>            p[i+1][k] = 'y';<br>        }<br>    }<br>    <br>    int result[] = new int[n]; <br>    <br>    int left = totalMoney;<br>    <br>    int paid = 0;<br>    <br>    for (int i = n-1; i &gt;= 0; i--) {<br>      if (meals[i] &gt; left)<br>        result[i] = 0;<br>      else<br>      if (p[i][left] == 'y')<br>        result[i] = 0;<br>      else {<br>        result[i] = 1;<br>        left -= meals[i];<br>      }<br>      <br>      if (result[i] == 1) paid++;<br>    }<br>    <br>    int h[] = new int[paid];<br>    int j = 0;<br>    for (int i = 0; i &lt; n; i++)<br>      if (result[i] == 1)<br>        h[j++] = i;    <br>    <br>    return h;<br>  }<br>}<br></td>