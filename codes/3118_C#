            using System;<br>using System.Collections;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class HealthFood<br>{<br>  int[] p, c, f, cal;<br>  public int[] selectMeals(int[] protein, int[] carbs, int[] fat, string[] dietPlans)<br>  {<br>    p = protein; c = carbs; f = fat;<br>    cal = new int[p.Length];<br>    for (int i = 0; i &lt; p.Length; ++i) cal[i] = 9*f[i] + 5*(p[i]+c[i]);<br>    int[] res = new int[dietPlans.Length];<br>    for (int i = 0; i &lt; dietPlans.Length; ++i) res[i] = meal(dietPlans[i]);<br>    return res;<br>  }<br>  <br>  int meal(string plan){<br>    int best = 0;<br>    for (int i = 1; i &lt; p.Length; ++i){<br>      if (better(i, best, plan)) best = i;<br>    }<br>    return best;<br>  }<br>  <br>  bool better(int a, int b, string plan){<br>    char ch;<br>    for (int i = 0; i &lt; plan.Length; ++i){<br>      ch = plan[i];<br>      if (ch == 'C' &amp;&amp; c[a] &gt; c[b]) return true;<br>      if (ch == 'C' &amp;&amp; c[a] &lt; c[b]) return false;<br>      if (ch == 'c' &amp;&amp; c[a] &lt; c[b]) return true;<br>      if (ch == 'c' &amp;&amp; c[a] &gt; c[b]) return false;<br>      if (ch == 'P' &amp;&amp; p[a] &gt; p[b]) return true;<br>      if (ch == 'P' &amp;&amp; p[a] &lt; p[b]) return false;<br>      if (ch == 'p' &amp;&amp; p[a] &lt; p[b]) return true;<br>      if (ch == 'p' &amp;&amp; p[a] &gt; p[b]) return false;<br>      if (ch == 'F' &amp;&amp; f[a] &gt; f[b]) return true;<br>      if (ch == 'F' &amp;&amp; f[a] &lt; f[b]) return false;<br>      if (ch == 'f' &amp;&amp; f[a] &lt; f[b]) return true;<br>      if (ch == 'f' &amp;&amp; f[a] &gt; f[b]) return false;<br>      if (ch == 'T' &amp;&amp; cal[a] &gt; cal[b]) return true;<br>      if (ch == 'T' &amp;&amp; cal[a] &lt; cal[b]) return false;<br>      if (ch == 't' &amp;&amp; cal[a] &lt; cal[b]) return true;<br>      if (ch == 't' &amp;&amp; cal[a] &gt; cal[b]) return false;<br>    }<br>    return false;<br>  }<br>}<br>//Powered by [KawigiEdit]<br></td>