            #include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cmath&gt;<br>#include &lt;queue&gt;<br>using namespace std;<br>typedef vector&lt;int&gt; VI;  typedef vector&lt;vector&lt;int&gt; &gt; VVI;<br>typedef vector&lt;string&gt; VS;  typedef vector&lt;vector&lt;string&gt; &gt; VVS;<br>typedef signed long long i64;  typedef unsigned long long u64;<br><br>VI a, b, c, d;<br>VI p;<br>string s;<br><br>bool pcmp(const int &amp;x, const int &amp;y) {<br>  for( int i = 0; i &lt; s.size(); i++ ) {<br>    if( s[i] == 'P' &amp;&amp; a[x] != a[y] ) return a[x] &gt; a[y];<br>    if( s[i] == 'p' &amp;&amp; a[x] != a[y] ) return a[x] &lt; a[y];<br>    if( s[i] == 'C' &amp;&amp; b[x] != b[y] ) return b[x] &gt; b[y];<br>    if( s[i] == 'c' &amp;&amp; b[x] != b[y] ) return b[x] &lt; b[y];<br>    if( s[i] == 'F' &amp;&amp; c[x] != c[y] ) return c[x] &gt; c[y];<br>    if( s[i] == 'f' &amp;&amp; c[x] != c[y] ) return c[x] &lt; c[y];<br>    if( s[i] == 'T' &amp;&amp; d[x] != d[y] ) return d[x] &gt; d[y];<br>    if( s[i] == 't' &amp;&amp; d[x] != d[y] ) return d[x] &lt; d[y];<br>  }<br>  return x &lt; y;<br>}<br><br>class HealthFood {<br>public:<br>vector &lt;int&gt; selectMeals(vector &lt;int&gt; A, vector &lt;int&gt; B, vector &lt;int&gt; C, vector &lt;string&gt; e) {<br>  a = A; b = B; c = C;<br>  int i, j, k, x, y, z, n;<br>  vector &lt;int&gt; ret;<br><br>  for( i = 0; i &lt; a.size(); i++ )<br>    d.push_back(a[i]*5 + b[i]*5 + c[i]*9);<br>  for( i = 0; i &lt; e.size(); i++ ) {<br>    s = e[i];<br>    p.clear();<br>    for( j = 0; j &lt; a.size(); j++ ) p.push_back(j);<br>    sort(p.begin(), p.end(), pcmp);<br>    ret.push_back(p[0]);<br>  }<br>  return ret;<br>}<br>};<br><br><br>// Powered by FileEdit<br></td>