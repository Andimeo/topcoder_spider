            Imports Microsoft.VisualBasic<br>Imports System<br>Imports System.Collections<br> <br>Public Class HealthFood<br>    Dim p, c, f, t As Integer()<br> <br>    Private Sub selectFrom(ByVal criteria As String, ByRef avail As Boolean())<br>        Dim i, best As Integer<br> <br>        For i = 0 To avail.Length - 1<br>            If avail(i) = True Then<br>                best = i<br>                Exit For<br>            End If<br>        Next<br>        For i = 0 To avail.Length - 1<br>            If avail(i) = True Then<br>                Select Case criteria<br>                    Case "p" : If p(i) &lt; p(best) Then best = i<br>                    Case "P" : If p(i) &gt; p(best) Then best = i<br>                    Case "c" : If c(i) &lt; c(best) Then best = i<br>                    Case "C" : If c(i) &gt; c(best) Then best = i<br>                    Case "f" : If f(i) &lt; f(best) Then best = i<br>                    Case "F" : If f(i) &gt; f(best) Then best = i<br>                    Case "t" : If t(i) &lt; t(best) Then best = i<br>                    Case "T" : If t(i) &gt; t(best) Then best = i<br>                End Select<br>            End If<br>        Next<br>        For i = 0 To avail.Length - 1<br>            If avail(i) = True Then<br>                Select Case criteria<br>                    Case "p" : If p(i) &lt;&gt; p(best) Then avail(i) = False<br>                    Case "P" : If p(i) &lt;&gt; p(best) Then avail(i) = False<br>                    Case "c" : If c(i) &lt;&gt; c(best) Then avail(i) = False<br>                    Case "C" : If c(i) &lt;&gt; c(best) Then avail(i) = False<br>                    Case "f" : If f(i) &lt;&gt; f(best) Then avail(i) = False<br>                    Case "F" : If f(i) &lt;&gt; f(best) Then avail(i) = False<br>                    Case "t" : If t(i) &lt;&gt; t(best) Then avail(i) = False<br>                    Case "T" : If t(i) &lt;&gt; t(best) Then avail(i) = False<br>                End Select<br>            End If<br>        Next<br>    End Sub<br>    Public Function selectMeals(ByVal protein As Integer(), ByVal carbs As Integer(), ByVal fat As Integer(), ByVal dietPlans As String()) As Integer()<br>        Dim i, j As Integer<br>        Dim cal(protein.Length - 1), ret(dietPlans.Length - 1) As Integer<br>        Dim available(protein.Length - 1) As Boolean<br> <br>        p = protein<br>        c = carbs<br>        f = fat<br>        t = cal<br> <br>        For i = 0 To protein.Length - 1<br>            cal(i) = fat(i) * 9 + protein(i) * 5 + carbs(i) * 5<br>        Next<br>        For i = 0 To ret.Length - 1<br>            For j = 0 To available.Length - 1<br>                available(j) = True<br>            Next<br>            For j = 0 To dietPlans(i).Length - 1<br>                selectFrom(dietPlans(i).Chars(j), available)<br>            Next<br>            For j = 0 To available.Length - 1<br>                If available(j) = True Then<br>                    ret(i) = j<br>                    Exit For<br>                End If<br>            Next<br>        Next<br>        Return ret<br>    End Function<br>End Class<br></td>