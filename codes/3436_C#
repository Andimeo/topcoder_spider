            using System;<br>using System.Collections;<br>using System.Text;<br> <br>public class NthFraction<br>{<br>    ArrayList al1=new ArrayList();<br>    ArrayList al2=new ArrayList();<br> <br>    void gener(int m,int a1,int b1,int a2,int b2)<br>    {<br>        if (b1+b2&gt;m) return;<br>        gener(m,a1,b1,a1+a2,b1+b2);<br>        al1.Add(a1+a2);<br>        al2.Add(b1+b2);<br>        gener(m,a1+a2,b1+b2,a2,b2);<br>    }<br> <br>    public string getFraction(int n,int m)<br>    {<br>        al1.Add(0);<br>        al2.Add(1);<br>        gener(m,0,1,1,1);<br>        int i,j,a,b;<br>        i=n/al1.Count;<br>        j=n%al1.Count;<br>        a=(int)al1[j];<br>        b=(int)al2[j];<br>        a+=b*i;<br>        return a.ToString()+"/"+b.ToString();<br>    }<br>}<br></td>