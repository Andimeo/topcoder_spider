            using System;<br> <br>/// &lt;summary&gt;<br>  /// Summary description for TextCompressor.<br>  /// &lt;/summary&gt;<br>  public class TextCompressor<br>  {<br>    public TextCompressor()<br>    {<br>      //<br>      // TODO: Add constructor logic here<br>      //<br>    }<br> <br>    string result="";<br>    int length=0;<br>    public string longestRepeat(string sourceText)<br>    {<br>      for (int i=0;i&lt;sourceText.Length;i++)<br>      {<br>        for (int j=i+1;j&lt;sourceText.Length;j++)<br>        {          <br>          if (sourceText.Substring(j).IndexOf(sourceText.Substring(i,j-i))!=-1)<br>          {<br>            if (sourceText.Substring(i,j-i).Length&gt;length)<br>            {<br>              result=sourceText.Substring(i,j-i);<br>              length=result.Length;<br>            }<br>          }<br>        }<br>      }<br>      return result;<br>    }<br>  }<br></td>