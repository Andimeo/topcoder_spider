            import java.util.*;<br>public class TextCompressor {<br>    public String longestRepeat(String sourceText) {<br>      int len = sourceText.length();<br>        String res = null;<br>        for (int i = 0; i &lt; len; i++)<br>          for (int j = i + 1; j &lt; len; j++)<br>          {<br>            String sub = sourceText.substring(i, j);<br>            String remaining = sourceText.substring(j);<br>            if (remaining.indexOf(sub) != -1)<br>            {<br>              if (res == null)<br>              {<br>                res = sub;<br>              }<br>              else<br>              {<br>                if (sub.length() &gt; res.length())<br>                  res = sub;<br>              }<br>            }<br>          }<br>        if (res == null) res ="";<br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>