            using System;<br>using System.Collections;<br>using System.Text;<br> <br>public class OneArmedBandit<br>{<br>    string[] ws;<br> <br>    private double getp(string str)<br>    {<br>        double res=1;<br>        for (int i=0; i&lt;str.Length; i++)<br>            if (str[i]!='-')<br>            {<br>                int c=0;<br>                for (int j=0; j&lt;ws[i].Length; j++)<br>                    if (ws[i][j]==str[i]) c++;<br>                res*=(1.0*c)/ws[i].Length;<br>            }<br>        return res;<br>    }<br> <br>    public double progressiveJackpot(string[] wheels,string jackpot,string[] payoff)<br>    {<br>        ws=wheels;<br>        double s=0,p;<br>        foreach (string st in payoff)<br>        {<br>            string[] ss=st.Split(' ');<br>            p=getp(ss[0]);<br>            s+=p*int.Parse(ss[1]);<br>        }<br>        if (s.CompareTo(1.0) &gt;= 0) return 0.0;<br>        p=getp(jackpot);<br>        if (p.CompareTo(0.0) == 0) return -1.0;<br>        return (1.0-s)/p;<br>    }<br>}<br></td>