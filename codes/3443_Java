            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class OneArmedBandit<br>{<br>  public double progressiveJackpot(String[] wheels, String jackpotLine, String[] payoffTable)<br>  {<br>    double payout = 0;<br>    for (int i=0; i&lt;payoffTable.length; i++)<br>    {<br>      String[] parts = payoffTable[i].split(" ");<br>      payout += Integer.parseInt(parts[1])*prob(parts[0], wheels);<br>    }<br>    if (payout &gt;= 1)<br>      return 0;<br>    else if (prob(jackpotLine, wheels) == 0)<br>      return -1;<br>    else<br>      return (1-payout)/prob(jackpotLine, wheels);<br>  }<br>  <br>  double prob(String line, String[] wheels)<br>  {<br>    double ret = 1;<br>    for (int i=0; i&lt;line.length(); i++)<br>    {<br>      if (line.charAt(i) != '-')<br>      {<br>        int total = 0;<br>        for (int j=0; j&lt;wheels[i].length(); j++)<br>          if (line.charAt(i) == wheels[i].charAt(j))<br>            total++;<br>        ret *= (double)total/wheels[i].length();<br>      }<br>    }<br>    return ret;<br>  }  <br> <br>}<br>//Powered by [KawigiEdit]<br></td>