            #include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;iostream&gt;<br>#include &lt;cctype&gt;<br> <br>using namespace std;<br> <br>int s,ans;<br>string seq;<br> <br>int go(int ct,int last) {<br>  int bil,baru,i;<br> <br>  if(ct==s) {<br>    ans++;<br>    return 0;<br>  }<br>  baru=-1; bil=0;<br>  for(i=ct;i&lt;s;i++) {<br>    bil=bil*10+seq[i]-'0';<br>    if(bil&gt;1000000) break;<br>    if(bil!=baru &amp;&amp; bil&gt;=last &amp;&amp; bil&gt;0) {<br>      go(i+1,bil);<br>      baru=bil;<br>    }<br>  }<br>  return 0;<br>}<br> <br>class  DeserializeSequence {<br>public:<br>int howMany (string str) {<br>  s=str.size();<br>  seq=str;<br>  ans=0;<br>  go (0,-1);<br>  return ans;<br>}<br> <br>};<br></td>