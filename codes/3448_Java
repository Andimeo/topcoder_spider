            import java.util.*;<br> <br>public class DeserializeSequence<br>{<br>    HashMap hmNumFor;<br> <br>    private int numFor(String s, int iMax)<br>    {<br>  String sKey = s + ";" + iMax;<br>  Integer ii = (Integer)hmNumFor.get(sKey);<br>  if (ii==null)<br>  {<br>          int iResult = 0;<br>      if (iMax&gt;0 &amp;&amp; s.length()&lt;8 &amp;&amp; Integer.parseInt(s)&lt;=iMax) iResult++;<br>      for (int i=1; i&lt;s.length(); i++)<br>      {<br>    if (i&gt;7 &amp;&amp; s.charAt(s.length()-i)!='0') break;<br>    int iLastNum = Integer.parseInt(s.substring(s.length()-i));<br>    if (iLastNum&gt;iMax) break;<br>    String sNext = s.substring(0,s.length()-i);<br>    iResult += numFor(sNext, iLastNum);<br>      }<br>      ii = new Integer(iResult);<br>      hmNumFor.put(sKey,ii);<br>  }<br>  return ii.intValue();<br>    }<br> <br>    public int howMany(String str)<br>    {<br>  hmNumFor = new HashMap();<br> <br>  str = str.replaceAll("^0*","");<br>  int res = numFor(str, 1000000);<br>  return res;<br>    }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>