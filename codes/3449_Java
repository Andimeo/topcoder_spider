            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>public class Triptych {<br>  public int minCost(String[] fares, int[] destinations) {<br>    int[][] g = new int[fares.length][fares.length];<br>    for(int i = 0; i &lt; g.length; i++){<br>      Arrays.fill(g[i],Integer.MAX_VALUE/3);<br>      g[i][i] = 0;<br>    }<br>    for(int i = 0; i &lt; fares.length; i++){<br>      for(int j = 0; j &lt; fares[i].length(); j++){<br>        char c = fares[i].charAt(j);<br>        if(c != '-') g[i][j] = 10*(c-48);<br>      }<br>    }<br>    for(int k = 0; k &lt; g.length; k++){<br>      for(int i = 0; i &lt; g.length; i++){<br>        for(int j = 0; j &lt; g.length; j++){<br>          g[i][j] = Math.min(g[i][j], g[i][k]+g[k][j]);<br>        }<br>      }<br>    }<br>    int[][][] h = new int[g.length][g.length][g.length];<br>    for(int i = 0; i &lt; h.length; i++){<br>      for(int j = 0; j &lt; h.length; j++){<br>        for(int k = j; k &lt; h.length; k++){<br>          h[i][j][k] = Integer.MAX_VALUE/3;<br>          for(int n = 0; n &lt; g.length; n++){<br>            h[i][j][k] = Math.min(h[i][j][k], g[i][n]+g[n][j]+g[n][k]);<br>          }<br>          h[i][k][j] = h[i][j][k];<br>        }<br>      }<br>    }<br>    int ret = Integer.MAX_VALUE/3;<br>    int[][] test = new int[3][];<br>    test[0] = new int[] {destinations[0], destinations[1], destinations[2]};<br>    test[1] = new int[] {destinations[1], destinations[0], destinations[2]};<br>    test[2] = new int[] {destinations[2], destinations[1], destinations[0]};<br>    for(int i = 0; i &lt; test.length; i++){<br>      for(int n = 0; n &lt; g.length; n++){<br>        ret = Math.min(ret, g[0][n]+g[n][test[i][0]]+h[n][test[i][1]][test[i][2]]);<br>      }<br>    }<br>    return ret == Integer.MAX_VALUE/3 ? -1 : ret;<br>  }<br> <br> <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>