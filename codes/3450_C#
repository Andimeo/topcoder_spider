            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br>using System.Collections.Specialized;<br><br>public class GroceryBagger {<br>    public GroceryBagger() {<br>       // Life is Good !!<br>    }<br>    public int minimumBags(int strength, string[] itemType) {<br>        Hashtable count = new Hashtable();<br>        foreach (string item in itemType)<br>        {<br>            if (count.ContainsKey(item))<br>            {<br>                count[item] = (int)count[item] + 1;<br>            }<br>            else<br>            {<br>                count[item] = 1;<br>            }<br>        }<br>        int res = 0;<br>        foreach (string item in count.Keys)<br>        {<br>            int N = (int)count[item];<br>            res += N / strength;<br>            if (N % strength != 0) res++;<br>        }<br>        return res;<br>    }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>