            import java.util.*;<br> <br>public class GroceryBagger {<br>  public int minimumBags(int strength, String[] itemType) {<br>    HashMap map = new HashMap();<br>    int idx = 0;<br>    for (int i = 0; i &lt; itemType.length; i++) {<br>      String s = itemType[i];<br>      if (!map.containsKey(s)) {<br>        map.put(s, new Integer(idx)); idx++;<br>      }<br>    }<br>    int[] a = new int[idx];<br>    for (int i = 0; i &lt; itemType.length; i++) {<br>      int n = ((Integer) map.get(itemType[i])).intValue();<br>      a[n]++;<br>    }<br>    int ret = 0;<br>    for (int i = 0; i &lt; a.length; i++) {<br>      ret += (a[i] + strength - 1) / strength;<br>    }<br>    return ret;<br>  }<br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>