<td class="problemText" colspan="8" valign="middle" align="left">
            Public Class GroceryBagger<br>    Public Function minimumBags(ByVal strength As Integer, ByVal itemType As String()) As Integer<br>        Dim i, j, count, bags, ret As Integer<br>        Dim stored(itemType.Length - 1) As Boolean<br> <br>        For i = 0 To itemType.Length - 1<br>            If Not stored(i) Then<br>                count = 0<br>                For j = i To itemType.Length - 1<br>                    If itemType(i) = itemType(j) Then<br>                        stored(j) = True<br>                        count += 1<br>                    End If<br>                Next<br>                bags = count \ strength<br>                If count Mod strength &gt; 0 Then bags += 1<br>                ret += bags<br>            End If<br>        Next<br>        Return ret<br>    End Function<br>End Class<br></td>