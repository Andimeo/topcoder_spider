            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class InsideOut<br>{<br>    public string unscramble(string line)<br>    {<br>        int mid = line.Length / 2;<br><br>        StringBuilder sb = new StringBuilder( line.Length );<br>        for ( int i=mid-1; i &gt;=0; --i)<br>            sb.Append( line[i] );<br><br>        for ( int j=line.Length-1; j &gt;=mid; --j )<br>            sb.Append( line[j] );<br>        return sb.ToString();<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>