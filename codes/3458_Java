            import java.util.*;<br>public class PrimeAnagrams {<br>  boolean[] pr = new boolean[1000000];<br>  int n;  <br>  String s;<br>  int[] used = new int[8];<br>  int[] p = new int[8];<br>  int[] ans = new int[3];<br>  long min = Long.MAX_VALUE;<br> <br>  void check(String x, String y, String z) {<br>    if (x.charAt(0) == '0') return;<br>    if (y.charAt(0) == '0') return;<br>    if (z.charAt(0) == '0') return;<br>    int X = Integer.parseInt(x);<br>    int Y = Integer.parseInt(y);<br>    int Z = Integer.parseInt(z);<br>    if (pr[X] &amp;&amp; pr[Y] &amp;&amp; pr[Z] &amp;&amp; min &gt; X * Y * Z) {<br>      min = X * Y * Z;<br>      ans[0] = X; ans[1] = Y; ans[2] = Z;<br>    } <br>  }<br> <br>  void rec(int k) {<br>    if (k == n) {<br>      String t = "";<br>      int i, j;<br>      for (i = 0; i &lt; n; i++) t += s.charAt(p[i]);<br>      for (i = 1; i &lt; n; i++)<br>        for (j = i + 1; j &lt; n; j++) {<br>          String x = t.substring(0, i);<br>          String y = t.substring(i, j);<br>          String z = t.substring(j);<br>          check(x, y, z);<br>        }<br>      return;<br>    }<br>    for (int i = 0; i &lt; n; i++)<br>      if (used[i] == 0) {<br>        used[i] = 1;<br>        p[k] = i;  <br>        rec(k + 1);<br>        used[i] = 0;<br>      }<br>  }<br> <br>  public int[] primes(String anagram) {<br>    int i, j, k; s = anagram;<br>    n = anagram.length();<br>    for (i = 2; i &lt; 1000000; i++) {<br>      pr[i] = true;<br>      for (j = 2; j * j &lt;= i; j++) if (i % j == 0) { pr[i] = false; break; }<br>    } <br>    rec(0);<br>    Arrays.sort(ans);  <br>    if (min &lt; Long.MAX_VALUE) return ans;<br>    return new int[0];<br>  }<br>}<br></td>