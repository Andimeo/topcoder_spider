            // Includes<br>#include &lt;algorithm&gt;<br>#include &lt;cmath&gt;<br>#include &lt;iostream&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;string&gt;<br>#include &lt;sstream&gt;<br>#include &lt;vector&gt;<br>using namespace std;<br> <br>// Real code<br>class MusicLicenses  {<br>  public:<br>  int audit(string log) {<br>    for (int maxt = 0; maxt &lt; log.size(); maxt++) {<br>      vector&lt;int&gt; first(26,1001);<br>      vector&lt;int&gt; last(26,-1);<br>      int i;<br>    <br>      for (i = 0; i &lt;= maxt; i++) {<br>        int j = log[i] - 'A';<br>        first[j] &lt;?= i;<br>        last[j] &gt;?= i;<br>      }<br>    <br>      for (i = 0; i &lt;= maxt; i++) {<br>        int c = 0;<br>        for (int j = 0; j &lt; 26; j++)<br>        if (first[j] &lt;= i &amp;&amp; last[j] &gt;= i) c++;<br>        if (c &gt; 3) return maxt;<br>      }<br>    }<br>    return -1;<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>