            import java.util.*;<br><br>/** <br> * @author Roman Elizarov<br> */<br>public class SantaClaus {<br>  public int[] exchange(String[] value) {<br>    int n = value.length;<br>    int[][] v = new int[n][n];<br>    for (int i = 0; i &lt; n; i++) {<br>      for (int j = 0; j &lt; n; j++) {<br>        v[i][j] = value[i].charAt(j) - 'A' + 1;<br>      }<br>    }<br>    int[][] d = new int[n][n];<br>    int[][] c = new int[n][n];<br>    int[][] b = new int[n][n];<br>    for (int i = 0; i &lt; n; i++) {<br>      for (int j = 0; j &lt; n; j++) {<br>        d[i][j] = v[i][j] - v[i][i];<br>        c[i][j] = d[i][j];<br>      }<br>    }<br>    for (int len = 1; len &lt;= n; len++) {<br>      int maxi = 0;<br>      for (int i = 0; i &lt; n; i++) {<br>        maxi = Math.max(maxi, c[i][i]);<br>      }<br>      if (maxi &gt; 0)<br>        return new int[] {len, maxi};<br>      for (int i = 0; i &lt; n; i++) {<br>        for (int j= 0; j &lt; n; j++) {<br>          b[i][j] = c[i][j];<br>        }<br>      }<br>      for (int i = 0; i &lt; n; i++) {<br>        for (int j = 0; j &lt; n; j++) {<br>          for (int k = 0; k &lt; n; k++) {<br>            b[i][k] = Math.max(b[i][k], c[i][j] + d[j][k]);<br>          }<br>        }<br>      }<br>      for (int i = 0; i &lt; n; i++) {<br>        for (int j= 0; j &lt; n; j++) {<br>          c[i][j] = b[i][j];<br>        }<br>      }<br>    }<br>    return new int[] {0, 0};<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>