<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>using System.Collections;<br>using System.Text;<br> <br>public class CalendarHTML<br>{<br>    public string[] makeTable(int days,int start)<br>    {<br>        ArrayList al=new ArrayList();<br>        al.Add("&lt;table&gt;");<br>        al.Add("&lt;tr&gt;&lt;td&gt;S&lt;/td&gt;&lt;td&gt;M&lt;/td&gt;&lt;td&gt;T&lt;/td&gt;&lt;td&gt;W&lt;/td&gt;&lt;td&gt;T&lt;/td&gt;&lt;td&gt;F&lt;/td&gt;&lt;td&gt;S&lt;/td&gt;&lt;/tr&gt;");<br>        int i=1,p=start;<br>        while (i&lt;=days)<br>        {<br>            StringBuilder sb=new StringBuilder();<br>            sb.Append("&lt;tr&gt;");<br>            for (int j=0; j&lt;p; j++) sb.Append("&lt;td&gt;&lt;/td&gt;");<br>            for (int j=7-p; j&gt;0; j--)<br>            {<br>                if (i&gt;days) sb.Append("&lt;td&gt;&lt;/td&gt;");<br>                else sb.Append("&lt;td&gt;"+i.ToString()+"&lt;/td&gt;");<br>                ++i;<br>            }<br>            sb.Append("&lt;/tr&gt;");<br>            al.Add(sb.ToString());<br>            p=0;<br>        }<br>        al.Add("&lt;/table&gt;");<br>        string[] res=new string[al.Count];<br>        al.CopyTo(res);<br>        return res;<br>    }<br>}<br></td>