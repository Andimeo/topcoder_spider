            import java.util.*;<br>public class CalendarHTML {<br>public String[] makeTable(int days, int start) {<br>    List a = new ArrayList();<br>    int d = 1;<br>    a.add("&lt;table&gt;");<br>    a.add("&lt;tr&gt;&lt;td&gt;S&lt;/td&gt;&lt;td&gt;M&lt;/td&gt;&lt;td&gt;T&lt;/td&gt;&lt;td&gt;W&lt;/td&gt;&lt;td&gt;T&lt;/td&gt;&lt;td&gt;F&lt;/td&gt;&lt;td&gt;S&lt;/td&gt;&lt;/tr&gt;");<br>    while(d &lt;= days) {<br>  String s = "&lt;tr&gt;";<br>  for(int i=7;i--&gt;0;) {<br>      s += "&lt;td&gt;";<br>      if(start&lt;=0 &amp;&amp; d &lt;= days) s += d++;<br>      else { start--; }<br>      s += "&lt;/td&gt;";<br>  }<br>  s += "&lt;/tr&gt;";<br>  a.add(s);<br>    }<br>    a.add("&lt;/table&gt;");<br>    return (String[])a.toArray(new String[]{});<br>}}<br></td>