            #line 83 "Fairness.cs"<br><br>using System;<br>using System.Text;<br>using System.Collections;<br><br>public class Fairness<br>{<br>  protected double getval(string n)<br>  {<br>    int sum = 0;<br>    foreach(char c in n)<br>    {<br>      sum += (int) (c - 'B');<br>    }<br>    return ((double) sum) / ((double) n.Length);<br>  }<br><br>  protected void swap(string[] a, int i, int j)<br>  {<br>    string t = a[i];<br>    a[i] = a[j];<br>    a[j] = t;<br>  }<br><br>  public string[] fairSort(string[] names)<br>  {<br>    int i;<br>    int j;<br>    string[] ret = new string[names.Length];  <br>    <br>    for(i = 0; i &lt; names.Length; ++ i)<br>    {<br>      ret[i] = names[i];<br>      for(j = i - 1; j &gt;= 0; -- j)<br>      {<br>        if(getval(ret[j + 1]) &lt; getval(ret[j]))<br>        {<br>          swap(ret, j, j + 1);<br>        }<br>      }<br>    }<br>    return ret;<br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>