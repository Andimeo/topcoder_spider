            import java.util.*;<br>public class Fairness implements Comparator {<br>    public String[] fairSort(String[] names) {<br>        Arrays.sort(names,this);<br>        return names;<br>    }<br>    private double getValue(Object o) {<br>        int t=0;<br>        for (int j=0;j&lt;((String)o).length();j++) {<br>            t+=((String)o).charAt(j)-'A'+1;<br>        }<br>        return (double)t/(double)((String)o).length();<br>    }<br>    public int compare(Object o1, Object o2) {<br>        if (getValue(o1)-getValue(o2)&gt;1e-8) return 1;<br>        if (getValue(o1)-getValue(o2)&lt;-1e-8) return -1;<br>        return 0;<br>    }<br>}<br></td>