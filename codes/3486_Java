<td class="problemText" colspan="8" valign="middle" align="left">
            import java.util.*;<br> <br>public class TestScores<br>{<br>    public double weightedScore(double[] questions, int testScore)<br>    {<br>        int len = questions.length;<br>        double s[] = new double[len+1];<br>        s[0]=1;<br>        for (int i=0; i &lt; len; i++)<br>        {<br>            double s2[] = new double[len+1];<br>            for (int j=0; j &lt; len; j++)<br>            {<br>                s2[j] += s[j] * (1-questions[i]);<br>                s2[j+1] += s[j] * questions[i];<br>            }<br>            s=s2;<br>        }<br>        double m = 0;<br>        double sd = 0;<br>        for (int i=0; i &lt;= len; i++)<br>        {<br>            m += s[i] * i;<br>        }<br>        for (int i=0; i &lt;= len; i++)<br>        {<br>            sd += Math.pow(i - m, 2) * s[i];<br>        }<br>        sd = Math.sqrt(sd);<br> <br>        double dif = (testScore - m) / sd;<br>        return 1000 + 300 * dif;<br>    }<br>}<br></td>