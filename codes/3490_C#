            using System;<br>using System.Collections;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class CmpdWords<br>{<br> <br>    public int ambiguous(string[] dictionary)<br>    {<br>        Hashtable a = new Hashtable();<br>        foreach (string s in dictionary) {<br>            a.Add(s, 1);<br>        }<br>        foreach (string s1 in dictionary) {<br>            foreach (string s2 in dictionary) {<br>                if (s1 != s2) {<br>                    int x;<br>                    string s = s1 + s2;<br>                    if (a.ContainsKey(s))<br>                        x = (int)a[s];<br>                    else<br>                        x = 0;<br>                    a[s] = x + 1;<br>                }<br>            }<br>        }<br>        int res = 0;<br>        foreach (DictionaryEntry de in a) {<br>            if (((int)de.Value) &gt; 1) {<br>                res++;<br>            }<br>        }<br>        return res;<br>    }<br> <br>}<br></td>