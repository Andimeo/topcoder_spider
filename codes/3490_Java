            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import static java.lang.Math.*;<br>import static java.util.Arrays.*;<br>public class CmpdWords {<br>  public int ambiguous(String[] dictionary) {<br>    HashSet map = new HashSet(asList(dictionary));<br>    HashSet back = new HashSet();<br>    int ret = 0;<br>    for(int i = 0; i &lt; dictionary.length; i++){<br>      for(int j = 0; j &lt; dictionary.length; j++){<br>        if(i==j)continue;<br>        String s = dictionary[i]+dictionary[j];<br>        if(!map.add(s)){<br>          if(back.add(s)) ret++;<br>        }<br>      }<br>    }<br>    return ret;<br>  }<br> <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>