            // Includes<br>#include &lt;algorithm&gt;<br>#include &lt;cmath&gt;<br>#include &lt;iostream&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;string&gt;<br>#include &lt;sstream&gt;<br>#include &lt;vector&gt;<br>using namespace std;<br> <br>// Real code<br>vector&lt;int&gt; bags;<br>class BagsOfGold  {<br>  public:<br>  map&lt;int,int&gt; optimal;<br>  int solve(int i1, int i2) {<br>    int key = i1*1000 + i2;<br>    if (optimal.count(key)) return optimal[key];<br>    <br>    if (i1 == i2) return bags[i1];<br>    int v1 = bags[i1] - solve(i1+1,i2);<br>    int v2 = bags[i2] - solve(i1, i2-1);<br>    v1 &gt;?= v2;<br>    optimal[key] = v1;<br>    return v1;<br>  }<br>  <br>  int netGain(vector &lt;int&gt; _bags) {<br>    bags = _bags;<br>    return solve(0, bags.size()-1);<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>