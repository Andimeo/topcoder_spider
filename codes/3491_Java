            import java.util.*;<br>public class BagsOfGold {<br>    public int netGain(int[] bags) {<br>  int n = bags.length;<br>  int[][] f = new int[n][n];<br>  for (int i=0; i &lt; n; i++) {<br>    Arrays.fill(f[i], 0);<br>  }<br>  for (int i=0; i &lt; n; i++) {<br>    f[i][i] = bags[i];<br>  }<br>  for (int l=1; l &lt; n; l++) {<br>    for (int i=0; i+l &lt; n; i++) {<br>      int j = i+l;<br>      f[i][j] = Math.max(bags[i] - f[i+1][j], bags[j] - f[i][j-1]);<br>    }<br>  }<br>  return f[0][n-1];<br>    }<br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>