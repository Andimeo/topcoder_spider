            #include &lt;algorithm&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br><br>using namespace std;<br><br>class Inversions { <br>   public: <br>   vector &lt;int&gt; createExample(int n, int inv) { <br>      vector&lt;int&gt; x;<br>      for( int i = 1; i &lt;= n; ++i ) x.push_back( i );<br>      int maxinversions = n * (n-1) / 2;<br>      <br>      vector&lt;int&gt; ret;<br><br>      if( inv &gt; maxinversions ) return ret;<br><br>      for( int i = 0; i &lt; n-1; ++i ) {<br>         for( int j = 0; j &lt; n-i; ++j ) {<br>            int invleft = (n-i-1)*(n-i-2)/2;<br>            if( invleft &gt;= inv - j ) {<br>               inv -= j;<br>               ret.push_back( x[j] );<br>               x.erase( x.begin() + j );<br>               break;<br>            }<br>         }<br>      }<br>      ret.push_back( x[0] );<br><br>      return ret;<br>   } <br><br>   <br> <br>}; <br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>