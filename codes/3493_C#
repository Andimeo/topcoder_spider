            using System;<br>using System.Collections;<br> <br>public class ExperimentalAnalyzer {<br>  public int[] getPredictors(string[] data) {<br>    int[][] A = new int[data.Length][];<br>    int i, j, m, k;<br>    string[] values;<br>    ArrayList R = new ArrayList();<br>    bool ok;<br> <br>    for (m = i = 0; i &lt; data.Length; i++) {<br>      values = data[i].Split(' ');<br>      m = values.Length;<br>      A[i] = new int[m];<br>      for (j = 0; j &lt; values.Length; j++)<br>        A[i][j] = Convert.ToInt32(values[j]);<br>    }<br> <br>    ok = false;<br>    for (i = 1; i &lt; data.Length; i++) {<br>      if (A[i][0] != A[0][0]) {<br>        ok = true;<br>        break;<br>      }<br>    }<br>    if (!ok) {<br>      return new int[] {};<br>    }<br> <br>    for (j = 1; j &lt; m; j++) {<br>      for (i = 0; i &lt; data.Length; i++) {<br>        ok = true;<br>        for (k = 0; ok &amp;&amp; (k &lt; data.Length); k++) {<br>          if (A[k][j] &lt;= A[i][j]) {<br>            ok = ok &amp;&amp; (A[k][0] == A[i][0]);<br>          }<br>          else {<br>            ok = ok &amp;&amp; (A[k][0] != A[i][0]);<br>          }<br>        }<br> <br>        if (ok) {<br>          R.Add(j);<br>          break;<br>        }<br>      }<br>    }<br> <br>    int[] result = new int[R.Count];<br>    for (i = 0; i &lt; R.Count; i++)<br>      result[i] = (int)R[i];<br> <br>    return result;<br>  }<br> <br>  public static void Main() {<br>    Console.WriteLine(new ExperimentalAnalyzer().getPredictors(new string[] {<br>????"0 10 20 20 0",<br>  "1 20 30 17 98765",<br>  "0 10 30 29 1234567",<br>  "1 20 40 10 42"}      <br>      ));<br>//    Console.ReadLine();<br>  }<br>}<br></td>