            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class ExperimentalAnalyzer<br>{<br>  public int[] getPredictors(String[] data)<br>  {<br>    String split[] = data[0].split(" ");<br>    int var_num = split.length - 1;<br>    int max[][] = new int[2][split.length];<br>    int min[][] = new int[2][split.length];<br>    Arrays.fill(max[0], -1);<br>    Arrays.fill(max[1], -1);<br>    Arrays.fill(min[0], -1);<br>    Arrays.fill(min[1], -1);<br>    <br>    for(int i=0;i&lt;data.length;i++)<br>    {<br>      String temp[] = data[i].split(" ");<br>      for(int j=1;j&lt;temp.length;j++)<br>      {<br>        if(min[Integer.parseInt(temp[0])][j] == -1 ||<br>          Integer.parseInt(temp[j])&lt;min[Integer.parseInt(temp[0])][j])<br>        min[Integer.parseInt(temp[0])][j] = Integer.parseInt(temp[j]);<br>        if(max[Integer.parseInt(temp[0])][j] == -1 ||<br>          Integer.parseInt(temp[j])&gt;max[Integer.parseInt(temp[0])][j])<br>        max[Integer.parseInt(temp[0])][j] = Integer.parseInt(temp[j]);<br>      }<br>    }<br>    <br>    int ret[], ret_size = 0, temp_ret[] = new int[split.length];<br>    for(int i=1;i&lt;=var_num;i++)<br>    {<br>      if(min[0][i] == -1 || max[0][i] == -1 || min[1][i] == -1 || max[1][i] == -1)<br>        continue;<br>      if(min[0][i] &gt; max[1][i] || max[0][i] &lt; min[1][i])<br>      {<br>        temp_ret[ret_size] = i;<br>        ret_size ++;<br>      }<br>    }<br>    ret = new int[ret_size];<br>    for(int i=0;i&lt;ret_size;i++)<br>      ret[i] = temp_ret[i];<br>    return ret;<br>  }<br>}<br> <br>//Powered by [KawigiEdit]<br></td>