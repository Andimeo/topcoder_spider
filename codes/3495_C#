<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>using System.Collections;<br>using System.Text;<br> <br> <br>public class MovingAvg<br>{<br>    public double difference(int k, double[] data)<br>    {<br>        double result = 0;<br> <br>        double min = 0;<br>        double max = 0;<br> <br>        for (int i = 0; i &lt; k; i++)<br>        {<br>            min = min + data[i];<br>            max = max + data[i];<br>        }<br> <br>        for (int i = 0; i &lt;= data.Length - k; i++)<br>        {<br>            double temp = 0;<br>            for (int j = i; j &lt; i + k; j++)<br>            {<br>                temp = temp + data[j];<br>            }<br> <br>            if (temp &gt; max)<br>            {<br>                max = temp;<br>            }<br> <br>            if (temp &lt; min)<br>            {<br>                min = temp;<br>            }<br>        }<br> <br>        result = (max - min) / k;<br>        return result;<br>    } <br>   <br>    <br>}<br></td>