<td class="problemText" colspan="8" valign="middle" align="left">
            public class MovingAvg<br>  {<br> <br>  <br>  public static double difference(int k, double[] data)<br>    {<br>      double ret=0;<br>      double savg=0,lavg=0,curavg=0;<br> <br>      for(int j=0;j&lt;k;j++)<br> <br>        curavg+=data[j];<br> <br>      curavg=savg=lavg=curavg/k;<br> <br>      for(int i=1;i+k &lt;= data.length;i++)<br>        {<br> <br>//        curavg=(curavg*(i-1)+data[i-1])/i;<br>                //curavg=0;<br>        //for(int j=i;j&lt;i+k;j++)<br> <br>        //  curavg+=data[j];<br> <br>        curavg=(curavg*k-data[i-1]+data[i+k-1])/k;<br> <br>        //curavg=curavg/k;<br> <br>        if(curavg&lt;savg)<br>          savg=curavg;<br>        else<br>                if(curavg &gt; lavg)<br>                  lavg=curavg;<br> <br>        }<br> <br>           ret=lavg-savg;<br> <br>           return ret;<br>    }<br> <br>  }<br></td>