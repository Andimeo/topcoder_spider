            using System;<br>using System.Collections;<br>public class Predicting {<br>  public double avgError(double[] data) {<br>    int n = data.Length;<br>    double res = double.MaxValue;<br><br>    for (int i1 = -10; i1 &lt;= 10; ++i1)<br>    for (int i2 = -10; i2 &lt;= 10; ++i2)<br>    for (int i3 = -10; i3 &lt;= 10; ++i3)<br>    for (int i4 = -10; i4 &lt;= 10; ++i4)<br>    {<br>      int i5 = 10 - i1 - i2 - i3 - i4;<br>      if (i5 &gt;= -10 &amp;&amp; i5 &lt;= 10)<br>      {<br>        double cur = 0;<br>        for (int i = 0; i &lt; n - 5; ++i)<br>          cur += Math.Abs(data[i] * i1 + data[i + 1] * i2 + data[i + 2] * i3 + data[i + 3] * i4 + data[i + 4] * i5 - data[i + 5] * 10);<br>        if (cur &lt; res)<br>          res = cur;<br>      }<br>    }<br><br>    return res / (n - 5) / 10;<br>  }<br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>