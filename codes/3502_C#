            using System;<br>using System.Collections;<br>using System.Collections.Specialized;<br> <br>public class TimeAnalysis{<br> Hashtable h=new Hashtable();<br> public string time(string[] v, string s){<br>  string[] term=s.Split('+');<br>  for (int i=0;i&lt;v.Length;i++) {<br>   string[] st=v[i].Split(' ');<br>   h[st[0]]=double.Parse(st[1]);<br>  }<br>  double t=0;<br>  for (int i=0;i&lt;term.Length;i++) t+=calc(term[i]);<br>  return result(t);<br> }<br> string[] st={"nanoseconds","microseconds","milliseconds","seconds","minutes","hours","days","years"}; <br> double[] duration={1,1,1,1,1,1,1,1};  <br> string result(double t) {<br>  for (int i=1;i&lt;=3;i++) <br>   duration[i]=duration[i-1]*1000;<br>   duration[4]=duration[3]*60;<br>   duration[5]=duration[4]*60;<br>   duration[6]=duration[5]*24;<br>   duration[7]=duration[6]*365;<br>   for (int i=st.Length-1;i&gt;=0;i--)<br>    if (t&gt;=duration[i]) return (int) (t/duration[i])+" "+st[i]; return "error";<br> }<br> double calc(string s) {<br>  string[] ss=s.Split('*');<br>  double x=1;<br>  for (int i=0;i&lt;ss.Length;i++) x*=cal(ss[i]);<br>  return x;<br> }<br> <br> double val(string s) {<br>  if (h[s]!=null) return (double) h[s];<br>  return double.Parse(s);<br> }<br>  <br> double cal(string s) {<br>  if (h[s]!=null) return (double) h[s];<br>  if (s[0]=='l') return Math.Log(val(""+s[3]))/Math.Log(2);<br>  if (s[1]=='^') return Math.Pow(val(""+s[0]),val(""+s[2]));<br>  return 0;<br> }<br>  <br>}<br>//hilfiger's editor<br></td>