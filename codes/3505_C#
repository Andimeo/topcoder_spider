            using System;<br>using System.Collections;<br>using System.Collections.Specialized;<br><br>public class DictionarySorter{<br> public string[] sort(string[] words){<br>  int n=words.Length;<br>  for (int i=0;i&lt;n;i++)<br>   for (int j=i+1;j&lt;n;j++) if (greater(words[i],words[j])) {<br>    string s=words[i]; words[i]=words[j]; words[j]=s;<br>   }<br>  return words;<br> }<br> bool greater(string s1, string s2) {<br>  s1=s1.Replace("/ae","*");<br>  s1=s1.Replace("/th","?");<br>  s2=s2.Replace("/ae","*");<br>  s2=s2.Replace("/th","?");<br>  int[] a1=get(s1);<br>  int[] a2=get(s2);<br>  return greater(a1,a2);<br> }<br> int[] get(string s) {<br>  int[] a=new int[s.Length];<br>  for (int i=0;i&lt;s.Length;i++) if (s[i]=='*') a[i]=2*((int) 'a')+1;<br>  else if (s[i]=='?') a[i]=2*((int) 't')+1; else a[i]=((int) s[i])*2;<br>  return a;<br> }<br> bool greater(int[] a, int[] b) {<br>  for (int i=0;i&lt;Math.Min(a.Length,b.Length);i++) <br>   if (a[i]&gt;b[i]) return true; else if (a[i]&lt;b[i]) return false;<br>  if (a.Length&gt;b.Length) return true;<br>  return false;<br> }<br>}<br>//hilfiger's editor<br></td>