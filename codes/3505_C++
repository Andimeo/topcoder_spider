            #include &lt;sstream&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;cmath&gt;<br>#include &lt;iostream&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br> <br>using namespace std;<br> <br>bool strless(const string &amp;s1, const string &amp;s2) {<br>  for (const char *p1 = s1.c_str(), *p2 = s2.c_str(); *p1 || *p2; ++p1, ++p2) {<br>    double v1 = *p1, v2 = *p2;<br>    if (*p1 == '/') v1 = (*++p1 == 'a' ? 'a'+0.5 : 't'+0.5), ++p1;<br>    if (*p2 == '/') v2 = (*++p2 == 'a' ? 'a'+0.5 : 't'+0.5), ++p2;<br>    if (v1 != v2) return v1 &lt; v2;<br>  }  <br>  return false;<br>}<br> <br>struct DictionarySorter {<br>  vector &lt;string&gt; sort(vector &lt;string&gt; words) {<br>    std::sort(words.begin(), words.end(), strless);<br>    return words;<br>  }<br>};<br></td>