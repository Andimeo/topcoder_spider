            import java.util.*;<br>public class DictionarySorter {<br>public String[] sort(String[] words) {<br>for (int i = 0; i &lt; words.length; i++) {<br>StringBuffer sb = new StringBuffer();<br>for (int j = 0; j &lt; words[i].length(); j++) {<br>if (words[i].charAt(j)=='a') sb.append('a');<br>else if (words[i].charAt(j)=='/') {<br>if (words[i].charAt(j+1)=='a') {<br>sb.append('b');<br>j+=2;<br>}<br>else {<br>sb.append('v');<br>j+=2;<br>}<br>}<br>else if (words[i].charAt(j)&lt;='t') sb.append(((char)(words[i].charAt(j)+1)));<br>else sb.append(((char)(words[i].charAt(j)+2)));<br>}<br>words[i] = new String(sb);<br>}<br>Arrays.sort(words);<br>for (int i = 0; i &lt; words.length; i++) {<br>StringBuffer sb = new StringBuffer();<br>for (int j = 0; j &lt; words[i].length(); j++) {<br>if (words[i].charAt(j)=='a') sb.append('a');<br>else if (words[i].charAt(j)=='b') sb.append("/ae");<br>else if (words[i].charAt(j)&lt;='u') sb.append(((char)(words[i].charAt(j)-1)));<br>else if (words[i].charAt(j)=='v') sb.append("/th");<br>else sb.append(((char)(words[i].charAt(j)-2)));<br>}<br>words[i] = new String(sb);<br>}<br>return words;<br>}<br>}<br></td>