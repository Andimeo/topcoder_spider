            using System;<br>using System.Text;<br>using System.Collections;<br><br>public class ImageEnlarger {<br>    public string[] enlarge(string[] image, int factor) {<br>        string[] res;<br><br>    int n = image[0].Length, m = image.Length;<br><br>    res = new string[m*factor];<br><br>    for (int j = 0; j &lt; m; j++)<br>      for (int i = 0; i &lt; n; i++)<br>      {<br>        for (int cj = 0; cj &lt; factor; cj++)<br>          for (int ci = 0; ci &lt; factor; ci++)<br>            res[j*factor+cj] += image[j][i];<br>      }<br><br>        return res;<br>    }<br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>