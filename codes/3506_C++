            #include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;iostream&gt;<br>#include &lt;algorithm&gt;<br>using namespace std;<br>#define V vector<br>#define tD typedef<br>tD long long ll;<br>tD V&lt;int&gt; vi;<br>tD V&lt;vi&gt; vii;<br>tD V&lt;double&gt; vd;<br>tD V&lt;vd&gt; vdd;<br>tD V&lt;ll&gt; vll;<br>tD V&lt;string&gt; vs;<br>#define prr make_pair<br>#define fr(x,y) for(int x=0;x&lt;(y); ++x)<br>#define fi(n) fr(i,n)<br>#define fj(n) fr(j,n)<br>#define fk(n) fr(k,n)<br>#define pb push_back<br>#define sz size()<br> <br>class ImageEnlarger {<br>   public:<br>   vector &lt;string&gt; enlarge(vector &lt;string&gt; image, int factor) {<br>    vs ans(image.sz*factor,string(image[0].sz*factor,' '));<br>    fi(image.sz) {<br>      fj(image[i].sz) {<br>        for(int k=i*factor; k&lt;(i+1)*factor; ++k) {<br>          for(int l=j*factor; l&lt;(j+1)*factor; ++l) {<br>            ans[k][l]=image[i][j];<br>          }<br>        }<br>      }<br>    }<br>    return ans;<br>   }<br>};<br> <br>// Powered by PopsEdit<br> <br>// Powered by PopsEdit<br></td>