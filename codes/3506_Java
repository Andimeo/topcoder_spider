            import java.util.*;<br>public class ImageEnlarger {<br>    public String[] enlarge(String[] mm, int f) {<br>        int m=mm.length;<br>        int n=mm[0].length();<br>        String[] res=new String[m*f];<br>        if(f!=0){<br>            for(int i=0;i&lt;m*f;i++){<br>                res[i]="";<br>                for(int j=0;j&lt;n*f;j++)res[i]=res[i]+mm[i/f].charAt(j/f);<br>            }<br>        }<br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>