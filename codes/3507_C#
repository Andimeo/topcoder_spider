<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>public class TimeOfPossession {<br>public string getTime(string[] ts) {<br>long ts1=0;<br>bool aHasPos=false;<br>long lastTime=0;<br>for(int i=0;i&lt;ts.Length;i++) {<br>string[] sx=ts[i].Split(' ');<br>string tm=sx[1];<br>long secs=Int32.Parse(tm.Split(':')[0])*60+Int32.Parse(tm.Split(':')[1]);<br>if(aHasPos)<br> ts1+=secs-lastTime;<br>lastTime=secs;<br>if(sx[0]=="A")<br> aHasPos=true;<br>if(sx[0]=="B")<br> aHasPos=false;<br>if(sx[0]=="SWITCH")<br> aHasPos=!aHasPos;<br>}<br>return ts1/60+":"+(ts1%60).ToString("00");<br>}<br>}<br></td>