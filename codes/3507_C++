            #include&lt;vector&gt;<br>#include&lt;string&gt;<br>#include&lt;iostream&gt;<br>#include&lt;cstdlib&gt;<br>#include&lt;cstring&gt;<br>#include&lt;sstream&gt;<br>using namespace std;<br><br>class TimeOfPossession<br>{ <br>public: <br>  string getTime(vector &lt;string&gt; times) <br>    { <br>    string ret;<br>    char ch[100];<br>    int i;<br>    string now="NONE";<br>    int prevTime=0;<br>    int totalA=0;<br>    for(i=0;i&lt;times.size();i++)<br>    {<br>      strcpy(ch,times[i].c_str());<br>      char* str=strtok(ch," ");<br>      char* smin=strtok(NULL,":");<br>      char* ssec=strtok(NULL,": ");<br>      int min=atoi(smin);<br>      int sec=atoi(ssec);<br>      int totalSec=min*60+sec;<br>      string str1=str;<br>      string prevStr=now;<br>      int newTime=totalSec;<br>      if(str1=="A")<br>      {<br>        now="A";<br>      }<br>      else if(str1=="B")<br>      {<br>        now="B";<br>      }<br>      else if(str1=="SWITCH")<br>      {<br>        if(now=="A")<br>        {<br>          now="B";<br>        }<br>        else<br>        {<br>          now="A";<br>        }<br>      }<br>      if(prevStr=="A")<br>      {<br>        totalA+=(newTime-prevTime);<br>        <br>      }<br>      prevTime=newTime;<br>    }<br>    int amin=totalA/60;<br>    int asec=totalA%60;<br>    ostringstream ostr;<br>    ostr&lt;&lt;amin&lt;&lt;":";<br>    ostr.width(2);<br>    ostr.fill('0');<br>    ostr&lt;&lt;asec;<br>    ret=ostr.str();<br>    <br>    return ret;<br><br>    } <br>    <br> <br>    }; <br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>