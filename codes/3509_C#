            using System;<br>using System.Collections;<br> <br>public class ChessKnight<br>{<br>    public double probability(int x,int y,int n)<br>    {<br>        --x;<br>        --y;<br>        int[] dx={-2,-1,1,2,2,1,-1,-2};<br>        int[] dy={1,2,2,1,-1,-2,-2,-1};<br>        double[,] a=new double[8,8];<br>        int i,j;<br>        for (i=0; i&lt;8; i++) for (j=0; j&lt;8; j++) a[i,j]=0;<br>        a[x,y]=1;<br>        while (n--&gt;0)<br>        {<br>            double[,] next=new double[8,8];<br>            for (i=0; i&lt;8; i++) for (j=0; j&lt;8; j++) next[i,j]=0;<br>            for (i=0; i&lt;8; i++) <br>                for (j=0; j&lt;8; j++)<br>                    for (int k=0; k&lt;8; k++)<br>                    {<br>                        int i1=i+dx[k];<br>                        int j1=j+dy[k];<br>                        if (0&lt;=i1 &amp;&amp; i1&lt;8 &amp;&amp; 0&lt;=j1 &amp;&amp; j1&lt;8) next[i1,j1]+=a[i,j]/8.0;<br>                    }<br>            a=next;<br>        }<br>        double sum=0;<br>        for (i=0; i&lt;8; i++) for (j=0; j&lt;8; j++) sum+=a[i,j];<br>        return sum;<br>    }<br>}<br></td>