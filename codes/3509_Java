            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>public class ChessKnight {<br>  int[] dx = {-2,-2,-1,-1,1,1,2,2};<br>  int[] dy = {-1,1,-2,2,-2,2,-1,1};<br>  double[][][] memo;<br>  public double probability(int x, int y, int n) {<br>    memo = new double[8][8][n+1];<br>    for(int i = 0; i &lt; memo.length; i++){<br>      for(int j = 0; j &lt; memo[i].length; j++){<br>        Arrays.fill(memo[i][j],-1);<br>      }<br>    }<br>    return doit(x-1, y-1, n);<br>  }<br>  double doit(int x, int y, int n){<br>    if(x &lt; 0 || y &lt; 0 || x &gt;= 8 || y &gt;= 8) return 0;<br>    if(n == 0) return 1;<br>    if(memo[x][y][n] != -1) return memo[x][y][n];<br>    double ret = 0;<br>    for(int i = 0; i &lt; dx.length; i++){<br>      ret += doit(x+dx[i], y+dy[i], n-1);<br>    }<br>    ret /= 8;<br>    memo[x][y][n] = ret;<br>    return ret;<br>  }<br> <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>