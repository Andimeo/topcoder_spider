            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>public class NestedRandomness {<br>  public double probability(int N, int nestings, int target) {<br>    memo = new double[N+1][nestings+1];<br>    for(int i = 0; i &lt; memo.length; i++){<br>      Arrays.fill(memo[i],-1);<br>    }<br>    return doit(N, nestings, target);<br>  }<br>  double[][] memo;<br>  double doit(int N, int nestings, int target){<br>    if(nestings == 0){<br>      if(N == target) return 1;<br>      else return 0;<br>    }<br>    if(memo[N][nestings] != -1) return memo[N][nestings];<br>    double ret = 0;<br>    for(int i = 0; i &lt; N; i++){<br>      ret += doit(i, nestings-1, target)/N;<br>    }<br>    memo[N][nestings] = ret;<br>    return ret;<br>  }<br> <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>