            using System;<br> <br>public class TreePlanting<br>{<br> <br>  int[] addv(int[] v, int x, int d)<br>  {<br>    for (int i = 2; (i &lt; 70) &amp; (x &gt; 1); i++)<br>      while ((x &gt; 1) &amp; (x % i == 0))<br>      {<br>        x /= i;<br>        v[i] += d;<br>      }<br>    <br>    return v;<br>  }<br> <br>  public long countArrangements(int total, int fancy)<br>  {<br>    int n = total - fancy + 1;<br>    int k = fancy;<br>    <br>    int[] v = new int[70];<br>    <br>    for (int i = 0; i &lt; 70; i++)<br>      v[i] = 0;<br>    <br>    long res = 0;<br>    <br>    if (n - k &gt;= 0)<br>    {<br>      if (k &gt; n - k)<br>        k = n - k;<br>        <br>      for (int i = n - k + 1; i &lt;= n; i++)<br>        v = addv(v, i, 1);<br>        <br>      for (int i = 2; i &lt;= k; i++)<br>        v = addv(v, i, -1);<br>        <br>      res = 1;<br>      for (int i = 0; i &lt; 70; i++)<br>        for (int j = 0; j &lt; v[i]; j++)<br>          res *= i;<br>    }<br>    <br>    return res;    <br>  }<br>  [STAThread]<br>  static void Main(string[] args)<br>  {<br>    TreePlanting obj = new TreePlanting();<br>    long r = obj.countArrangements(60, 15);<br>  }<br>}<br></td>