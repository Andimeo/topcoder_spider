            public class DeadCode {<br>    <br>    public int deadCount(String[] code) {<br>        // S = { 0 }<br>        int n = code.length;<br>        boolean[] ar1 = new boolean[n]; // false by default<br>        ar1[0] = true;<br>        boolean[] ar2 = new boolean[n]; // false by default<br>        int[][] aCode = new int[n][];<br>        for(int i=0; i&lt;code.length; i++) {<br>            String s = code[i];<br>            if (s.equals("RETURN")) {<br>                aCode[i] = new int[0];<br>                ar2[i] = true;<br>            } else {<br>                String[] tmp = s.split(" "); // IF 1 ELSE 2<br>                aCode[i] = new int[2];<br>                aCode[i][0] = Integer.parseInt(tmp[1]);<br>                aCode[i][1] = Integer.parseInt(tmp[3]);<br>            }<br>        }<br>        // Go ahead.<br>        for(int i=0; i&lt;n; i++) {<br>            for(int j=0; j&lt;n; j++) {<br>                if (ar1[j]) {<br>                    for(int k=0; k&lt;aCode[j].length; k++) {<br>                        ar1[aCode[j][k]] = true;<br>                    }<br>                }<br>            }<br>        }<br>        for(int i=0; i&lt;n; i++) {<br>            for(int j=0; j&lt;n; j++) {<br>                for(int k=0; k&lt;aCode[j].length; k++) {<br>                    int index = aCode[j][k];<br>                    if (ar2[index]) {<br>                        ar2[j] = true;<br>                    }<br>                }<br>            }<br>        }<br>        int count = 0;<br>        for(int i=0; i&lt;n; i++) {<br>            if (!ar1[i] || !ar2[i])<br>                count++;<br>        }<br>        return count;<br>    }<br>    <br>    /** Creates a new instance of DeadCode */<br>    public DeadCode() {<br>    }<br>}<br></td>