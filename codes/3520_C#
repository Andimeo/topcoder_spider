            using System;<br>using System.Collections;<br> <br>public class ClientsList<br>{<br>  class Name: IComparable{<br>    public string First, Last;<br>    public override string ToString() { return First + " " + Last; }<br>    public int CompareTo(object o){<br>      Name n = (Name)o;<br>      int i = Last.CompareTo(n.Last);<br>      if (i == 0) return First.CompareTo(n.First);<br>      return i;<br>    }<br>  }<br>  public string[] dataCleanup(string[] names)<br>  {<br>    Name[] ns = new Name[names.Length];<br>    for (int i = 0; i &lt; names.Length; ++i){<br>      string[] ss = names[i].Split();<br>      ns[i] = new Name();<br>      if (names[i].IndexOf(',') &gt;= 0){<br>        ns[i].First = ss[1];<br>        ns[i].Last = ss[0].Substring(0, ss[0].Length-1);<br>      }else{<br>        ns[i].First = ss[0];<br>        ns[i].Last = ss[1];<br>      }<br>    }<br>    Array.Sort(ns);<br>    string[] res = new string[ns.Length];<br>    for (int i = 0; i &lt; ns.Length; ++i){<br>      res[i] = ns[i].ToString();<br>    }<br>    return res;<br>  }<br>}<br>//Powered by [KawigiEdit]<br></td>