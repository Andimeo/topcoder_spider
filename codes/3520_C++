            #include &lt;string&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;vector&gt;<br> <br>using namespace std;<br> <br>class ClientsList {<br>public:<br>  vector &lt;string&gt; dataCleanup(vector &lt;string&gt; names) {<br>    vector&lt;pair&lt;string,string&gt; &gt; v;<br>    for (int i=0;i&lt;names.size();i++) {<br>      istringstream str(names[i]);<br>      string s1,s2;<br>      str &gt;&gt; s1 &gt;&gt; s2;<br>      if (s1[s1.size()-1]==',') {<br>        v.push_back(make_pair(s1.substr(0,s1.size()-1),s2));<br>      } else {<br>        v.push_back(make_pair(s2,s1));<br>      }<br>    }<br>    sort(v.begin(),v.end());<br>    vector&lt;string&gt; ret;<br>    for (int i=0;i&lt;v.size();i++) {<br>      ret.push_back(v[i].second+" "+v[i].first);<br>    }<br>    return ret;<br>  }<br>  <br> <br>};<br> <br> <br> <br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>