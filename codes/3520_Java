            import java.util.Arrays;<br> <br>public class ClientsList {<br> <br>  public String[] dataCleanup(String[] names) {<br>    for (int i = 0; i &lt; names.length; i++) {<br>      if (names[i].indexOf(',') &gt;= 0) {<br>        String[] parts = names[i].split(", ");<br>        names[i] = parts[0] + " " + parts[1];<br>      } else {<br>        String[] parts = names[i].split(" ");<br>        names[i] = parts[1] + " " + parts[0];<br>      }<br>    }<br>    Arrays.sort(names);<br>    for (int i = 0; i &lt; names.length; i++) {<br>      String[] parts = names[i].split(" ");<br>      names[i] = parts[1] + " " + parts[0];<br>    }<br>    return names;<br>  }<br> <br>}<br></td>