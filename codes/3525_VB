            Public Class ContractWork<br>    Public Function minimumCost(ByVal costs As String(), ByVal numTasks As Integer) As Integer<br>        Dim mas(costs.GetUpperBound(0), numTasks - 1) As Integer<br>        Dim i, j As Long<br>        Dim sep() As Char = " "<br>        For i = 0 To costs.GetUpperBound(0)<br>            Dim s() As String<br>            s = costs(i).Split(sep)<br>            For j = 0 To s.GetUpperBound(0)<br>                mas(i, j) = Microsoft.VisualBasic.Val(s(j))<br>            Next<br>        Next<br>        Dim tdyn(1, costs.Length - 1), maxdyn(1, costs.Length - 1), dyn(1, costs.Length - 1) As Long<br>        Dim n, z As Long<br>        n = costs.GetUpperBound(0)<br>        For i = 0 To n<br>            For j = 0 To 1<br>                maxdyn(j, i) = Long.MaxValue<br>            Next<br>        Next<br>        Dim k, g, mz As Long<br>        For k = 0 To numTasks - 1<br>            tdyn = maxdyn.Clone<br>            For i = 0 To n<br>                For j = 0 To n<br>                    For z = 0 To 1<br>                        If (z = 0) Or (i &lt;&gt; j) Then<br>                            g = dyn(z, j) + mas(i, k)<br>                            If i = j Then<br>                                mz = 1<br>                            Else<br>                                mz = 0<br>                            End If<br>                            If tdyn(mz, i) &gt; g Then tdyn(mz, i) = g<br>                        End If<br>                    Next<br>                Next<br>            Next<br>            dyn = tdyn.Clone<br>        Next<br>        Dim ans As Long = Long.MaxValue<br>        For i = 0 To n<br>            For j = 0 To 1<br>                If dyn(j, i) &lt; ans Then ans = dyn(j, i)<br>            Next<br>        Next<br>        Return ans<br>    End Function<br>End Class<br></td>