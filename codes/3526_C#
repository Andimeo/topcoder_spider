            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class MultiRead<br>{<br>  public int minCycles(string trace, int procs)<br>  {<br>    int dur = 0, c = 0;<br>    int n = trace.Length;<br>    for (int i=0; i&lt;n; ++i) {<br>      char ch = trace[i];<br>      if (ch == 'W') {<br>        dur++;<br>        if (c &gt; 0) dur++;<br>        c=0;<br>      }<br>      else {<br>        c++;<br>        if (c == procs) {<br>          dur++;<br>          c=0;<br>        }<br>      }<br>    }<br>    if (c&gt;0) dur++;<br>    return dur;<br>  }<br> <br> <br>}<br></td>