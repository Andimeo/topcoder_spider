            #include&lt;string&gt;<br>using namespace std;<br>class MultiRead<br>  {<br>    public:<br>      int minCycles(string,int);<br>  }mr;<br>int MultiRead::minCycles(string trace,int procs)<br>  {<br>    int x,y,l,j=0,c=0;<br>    l=trace.length();<br>    for(y=0;y&lt;l;y++)<br>       {<br>          if(trace[y]=='R')<br>           {<br>             j++;<br>             <br>             if(j==procs)<br>               {<br>                 c++;<br>               }<br>             x=y+1;<br>             if((j&lt;procs) &amp;&amp; ((trace[x]=='\0') || (trace[x]=='W')))<br>               {<br>                 c++;<br>       <br>               }<br>             if(j==procs)<br>               j=0;<br>           }<br>         else if(trace[y]=='W')<br>                {<br>                  c++;<br>       <br>                  j=0;<br>                }  <br>       }<br> <br>   return c;<br> }<br></td>