<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>using System.Collections;<br> <br>public class Pronunciation<br>{<br>  public bool isVowel(char c)<br>  {<br>      return c == 'a' || c == 'e' || c == 'i' || c == 'o' || c == 'u' ||<br>             c == 'A' || c == 'E' || c == 'I' || c == 'O' || c == 'U';<br>  }<br>  <br>  public bool canPronounce(string word)<br>  {<br>    int cc = 0;<br>    int cv = 0;<br>    char last = ' ';<br>    foreach (char z in word) {<br>        char c = char.ToLower(z);<br>        if (isVowel(c)) {<br>            cc = 0;<br>            ++cv;<br>            if (cv == 2 &amp;&amp; last != c) {<br>                return false;<br>            } else {<br>                cv = 1;<br>            }<br>        } else {<br>            cv = 0;<br>            ++cc;<br>            if (cc == 3) {<br>                return false;<br>            }<br>          <br>        }<br>        last = c;<br>    }<br>    return true;<br>  }<br>  <br>  public string canPronounce(string[] words)<br>  {<br>      foreach (string w in words) {<br>          if (!canPronounce(w)) {<br>              return w;<br>          }<br>      }<br>      return "";<br>  }<br>  <br> <br>}<br> <br>//Powered by [KawigiEdit]<br></td>