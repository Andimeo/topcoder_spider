            public class Pronunciation {<br>  boolean vocal(char c) {<br>    return c == 'a' || c == 'e' || c == 'i' || c == 'o' || c == 'u';<br>  }<br>  boolean okay(String s) {<br>    for (int i = 2; i &lt; s.length(); i++) {<br>      if (!vocal(s.charAt(i)) &amp;&amp; !vocal(s.charAt(i - 1)) &amp;&amp; !vocal(s.charAt(i - 2)))<br>        return false;<br>    }<br>    for (int i = 1; i &lt; s.length(); i++) {<br>      if (vocal(s.charAt(i)) &amp;&amp; vocal(s.charAt(i - 1)) &amp;&amp; s.charAt(i) != s.charAt(i - 1))<br>        return false;<br>    }<br>    return true;<br>  }<br>  public String canPronounce(String[] words) {<br>    for (int i = 0; i &lt; words.length; i++) {<br>      if (!okay(words[i].toLowerCase()))<br>        return words[i];<br>    }<br>    return "";<br>  }<br>}<br></td>