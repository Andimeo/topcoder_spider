            public class HingedDoor{<br>  public int numSwings(int a, int r){<br>    double n=a;<br>    double rr=r;<br>    int i=0;<br>    while(n&gt;5.0){<br>      n = n * (1/rr);  <br>      i++;<br>    }<br>    return i;<br>  }<br>}<br></td>