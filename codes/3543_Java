            // Powered by radeclipse!<br>import java.util.* ;<br>public class ATaleOfThreeCities {<br>  double md(int[] ax, int[] ay, int bx[], int[] by) {<br>    double ret = 1e99 ;<br>    for (int i=0; i&lt;ax.length; i++)<br>      for (int j=0; j&lt;bx.length; j++) {<br>  double t = Math.sqrt((ax[i]-bx[j])*(ax[i]-bx[j]) +<br>           (ay[i]-by[j])*(ay[i]-by[j])) ;<br>  ret = Math.min(ret, t) ;<br>      }<br>    return ret ;<br>  }<br>  public double connect(int[] ax, int[] ay, int[] bx, int[] by, int[] cx, int[] cy) {<br>    double mab = md(ax, ay, bx, by) ;<br>    double mbc = md(cx, cy, bx, by) ;<br>    double mac = md(ax, ay, cx, cy) ;<br>    return Math.min(Math.min(mab+mbc,mbc+mac),mab+mac) ;<br>  }<br>}<br></td>