            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class BitStrings {<br>    public int maxStrings(string[] list, int numZeroes, int numOnes) <br>  {<br>    int[] ones = new int[list.Length];<br>    int[] zeros = new int[list.Length];<br>    int max = 0;<br><br>    for(int i = 0; i &lt; list.Length; i++)<br>    {<br>      count(list[i], out ones[i], out zeros[i]);<br>    }<br><br>    for(int i = 0; i &lt; (1 &lt;&lt; list.Length); i++)<br>    {<br>      int o = numOnes, z = numZeroes;<br>      int c = 0;<br><br>      for(int j = 0; j &lt; list.Length; j++)<br>      {<br>        if ((i &amp; (1 &lt;&lt; j)) != 0)<br>        {<br>          o -= ones[j];<br>          z -= zeros[j];<br>          c++;<br>        }<br>      }<br><br>      if (o &gt;= 0 &amp;&amp; z &gt;= 0 &amp;&amp; c &gt; max)<br>      {<br>        max = c;<br>      }<br>    }<br><br>    return max;<br>    }<br><br>  private void count(string s, out int ones, out int zeros)<br>  {<br>    zeros = 0;<br>    ones = 0;<br><br>    for(int i=0; i &lt; s.Length; i++)<br>    {<br>      if (s[i] == '0')<br>      {<br>        zeros++;<br>      }<br>      else<br>      {<br>        ones++;<br>      }<br>    }<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>