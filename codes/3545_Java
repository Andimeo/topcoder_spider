            public class BitStrings {<br> <br>    public int maxStrings(String[] list, int numZeroes, int numOnes)<br>    {<br>        l=list;<br>        mayor=Integer.MIN_VALUE;<br>        probar(0,numZeroes,numOnes,0);<br>        return mayor;<br>    }<br>    String[] l;<br>    int mayor;<br>    public int[] numOnes(String s)<br>    {<br>        int[] res=new int[2];<br>        for(int i=0;i&lt;s.length();i++)<br>        {<br>            if(s.charAt(i)=='0')<br>                res[0]++;<br>            else<br>                res[1]++;<br>        }<br>        return res;<br>    }<br>    private void probar(int i,int z,int o,int num) {<br>        <br>        if(i==l.length)<br>        {<br>            if(mayor&lt;num)<br>                mayor=num;<br>            return;<br>        }<br>        int[] n = numOnes(l[i]);<br>        if((n[0]&lt;=z)&amp;&amp;(n[1]&lt;=o))<br>        {<br>            probar(i+1,z-n[0],o-n[1],num+1);<br>            <br>        }<br>            probar(i+1,z,o,num);<br>    }<br> <br> <br>}<br></td>