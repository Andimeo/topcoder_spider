            using System;<br>using System.Text;<br>using System.Collections;<br><br>public class MatchCounting <br>{<br>  int nn;<br>  bool ok(long res)<br>  {<br>    int left = nn;<br>    while (res&gt;0)<br>    {<br>      switch (res%10)<br>      {    <br>        case 8: left-=7; break;<br>        case 0:<br>        case 6:<br>        case 9: left-=6; break;<br>        case 2:<br>        case 3:<br>        case 5: left-=5; break;<br>        case 4:<br>        case 7: left-=4; break;<br>        case 1: left-=2; break;<br>      }<br>      if (left&lt;0) return false;<br>      res/=10;<br>    }<br>    return true;<br>  }<br>  public long count(int n) <br>  {<br>    nn=n;<br>    long res=1;<br>    int m = n;<br>    long add=1;<br><br>    while (m&gt;=4*8) <br>    {<br>      if (res==1) res=8;<br>      else<br>      {<br>        res*=10;<br>        res+=8;      <br>      }<br>      add*=10;<br>      m-=7;<br>    }<br>    while (ok(res)) res+=add;<br>    return res;<br>  }<br><br><br><br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester4C# modified by zsacul<br>// Powered by CodeProcessor<br></td>