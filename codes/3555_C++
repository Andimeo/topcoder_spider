            #include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;iostream&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;sstream&gt;<br>using namespace std;<br>#define V vector<br>#define tD typedef<br>tD long long ll;<br>tD V&lt;int&gt; vi;<br>tD V&lt;vi&gt; vii;<br>tD V&lt;double&gt; vd;<br>tD V&lt;vd&gt; vdd;<br>tD V&lt;ll&gt; vll;<br>tD V&lt;string&gt; vs;<br>#define prr make_pair<br>#define fr(x,y) for(int x=0;x&lt;(y); ++x)<br>#define fi(n) fr(i,n)<br>#define fj(n) fr(j,n)<br>#define fk(n) fr(k,n)<br>#define pb push_back<br>#define sz size()<br> <br>class MatchCounting {<br>   public:<br>   long long count(int n) {<br>    vector&lt;int&gt; bad;<br>    while(n&gt;=0) {<br>      bad.push_back(8); n-=7;<br>    }<br>    // lead: 1 (+5) , 2 (+2) , 6 (+1)<br>    // others: 0<br>    if(n&lt;-5) {<br>      bad[0]=1;<br>      n+=5;<br>    } else if (n&lt;-2) {<br>      bad[0]=2;<br>      n+=2;<br>    } else if(n&lt;-1) {<br>      bad[0]=6;<br>      n+=6;<br>    }<br>    for(int i=1; i&lt;bad.sz; ++i) {<br>      if(n&lt;-1) {<br>        ++n;<br>        bad[i]=0;<br>      }<br>    }<br>    ostringstream o;<br>    fi(bad.sz) {<br>      o&lt;&lt;bad[i];<br>    }<br>    istringstream in(o.str());<br>    long long ans;<br>    in&gt;&gt;ans;<br>    return ans;<br>   }<br>};<br> <br>// Powered by PopsEdit<br> <br>// Powered by PopsEdit<br></td>