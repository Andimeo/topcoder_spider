            import java.util.*;<br> <br>public class MatchCounting {<br>  public long count(int n) {<br>    int m = 1;<br>    for (;;) {<br>      if (((m - 1) * 7 &lt;= n) &amp;&amp; (m * 7 &gt; n)) break;<br>      m++;<br>    }<br>    int[] c = new int[] {6, 2, 5, 5, 4, 5, 6, 4, 7, 6};<br>    if (m == 1) {<br>      for (int i = 1; i &lt;= 9; i++) {<br>        if (c[i] &gt; n) return i;<br>      }<br>    }<br>    String ret = "";<br>    for (int i = 1; i &lt;= 9; i++) {<br>      if (n - c[i] &lt; (m - 1) * 7) {<br>        ret += i;<br>        m--;<br>        n -= c[i];<br>        break;<br>      }<br>    }<br>    <br>    for (int i = m; i &gt;= 1; i--) {<br>      <br>      for (int d = 0; d &lt;= 9; d++) {<br>        if (n - c[d] &lt; (i - 1) * 7) {<br>          ret += d;<br>          n -= c[d];<br>          break;<br>        }<br>      }<br>    }<br>    <br>    return Long.parseLong(ret);<br>  }<br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>