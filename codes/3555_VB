            Public Class MatchCounting<br>        Public Function count(ByVal n As Integer) As Long<br>            Dim i12, i44, i25, i06, i87, i66 As Integer<br>            Dim iRem As Integer<br>            Dim iSmallest As Long = Long.MaxValue<br>            Dim sNum As String<br>            Dim i As Integer<br>            Dim lNum As Long<br> <br>            If n = 1 Then Return 1<br>            'If n = 2 Then Return 2<br>            Dim iTarget As Integer<br>            Dim iRem1, iRem2, iRem3, iRem4 As Integer<br> <br>            For iTarget = n + 1 To n + 5<br> <br> <br> <br>                For i12 = 0 To iTarget \ 2<br>                    iRem = iTarget - 2 * i12<br>                    For i44 = 0 To iRem \ 4<br>                        iRem1 = iRem - 4 * i44<br>                        For i25 = 0 To iRem1 \ 5<br>                            iRem2 = iRem1 - 5 * i25<br>                            For i06 = 0 To iRem2 \ 6<br>                                iRem3 = iRem2 - 6 * i06<br>                                For i66 = 0 To iRem3 \ 6<br>                                    iRem4 = iRem3 - 6 * i66<br>                                    If iRem4 Mod 7 = 0 Then<br>                                        i87 = iRem4 \ 7<br>                                        If i44 &gt; 0 Or i25 &gt; 0 Or i12 &gt; 0 Or i87 &gt; 0 Or i66 &gt; 0 Then<br>                                            sNum = New String("1", i12)<br>                                            sNum = sNum &amp; New String("2", i25)<br>                                            sNum = sNum &amp; New String("4", i44)<br>                                            sNum = sNum &amp; New String("6", i66)<br>                                            sNum = sNum &amp; New String("8", i87)<br>                                            sNum = sNum.Substring(0, 1) &amp; New String("0", i06) &amp; sNum.Substring(1)<br> <br>                                            If sNum.Length &lt;= 19 Then<br>                                                If sNum.Length = 19 Then<br>                                                    If sNum.Substring(0, 1) &lt; 9 Or sNum.Substring(1, 1) &lt;= 2 Then<br>                                                        lNum = CLng(sNum)<br>                                                        If lNum &lt; iSmallest Then iSmallest = lNum<br>                                                    End If<br>                                                Else<br>                                                    lNum = CLng(sNum)<br>                                                    If lNum &lt; iSmallest Then iSmallest = lNum<br>                                                End If<br>                                            End If<br>                                        End If<br>                                    End If<br>                                Next<br>                            Next<br>                        Next<br>                    Next<br>                Next<br>            Next<br> <br>            Return iSmallest<br> <br>        End Function<br>    End Class<br></td>