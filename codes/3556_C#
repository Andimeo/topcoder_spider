            using System;<br>using System.Collections;<br> <br>public class Barbecue<br>{<br>  public int eliminate(int n, int[] voter, int[] excluded)<br>  {<br>    int [] totalVotes=new int[21];<br>    int [] votesCast=new int[21];<br>    for (int i=0; i&lt;21; i++)<br>    {<br>      totalVotes[i]=0;<br>      votesCast[i]=0;<br>    }<br>    for (int i=0; i&lt;voter.Length; i++)<br>    {<br>      totalVotes[excluded[i]]++;<br>      votesCast[voter[i]]++;<br>    }<br>    int max=0;<br>    int maxIndex=0;<br>    int vc=0;<br>    for (int i=0; i&lt;21; i++)<br>    {<br>      if (totalVotes[i]&gt;max)<br>      {<br>        maxIndex=i;<br>        max=totalVotes[i];<br>        vc=votesCast[i];<br>      }<br>      else if (totalVotes[i]==max)<br>      {<br>        if (votesCast[i]&gt;vc)<br>        {<br>          maxIndex=i;<br>          max=totalVotes[i];<br>          vc=votesCast[i];<br>        }<br>      }<br>    }<br>    return maxIndex;<br>  }<br>}<br> <br> <br>// Powered by TCGen for C#<br></td>