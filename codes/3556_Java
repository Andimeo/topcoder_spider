            import java.util.*;<br>public class Barbecue<br>{<br>    public int eliminate(int n, int[] v, int[] e)<br>    {<br>       int[] arr = new int[n];<br>    int[] vote = new int[n];<br>    for(int i = 0; i &lt; e.length; i ++)<br>     arr[e[i]]++;    <br>   for(int i = 0; i &lt; v.length; i ++)<br>      vote[v[i]]++;<br>  int max = 0;<br>  int ret = 0;<br>  for(int i = 0 ;i &lt; arr.length; i++)<br>  {<br>    if(arr[i] &gt; max)<br>    {<br>      max = arr[i];<br>      ret = i;<br>    }<br>    if(arr[i] == max)<br>      if(vote[i] &gt; vote[ret])<br>        ret = i;<br>    <br>  }<br>  return ret;<br>   <br>    }<br>}<br></td>