            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;iostream&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;stdio.h&gt;<br>#include &lt;stdlib.h&gt;<br>using namespace std;<br> <br>class DiceChecker {<br>  public: vector&lt;int&gt; badValues( vector&lt;int&gt; values ) {<br>    int i,j;<br>    vector&lt;int&gt; rv;<br>    for( i=1; i&lt;=6; i++ ){<br>      int ct=0;<br>      for( j=0; j&lt;values.size(); j++ ){<br>        if( values[j]==i ){<br>          ct++;<br>        }<br>      }<br>      if( ct*4 &gt; values.size() || ct*10 &lt; values.size() ){<br>        rv.push_back(i);<br>      }<br>    }<br>    return rv;<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>