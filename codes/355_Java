            public class DiceChecker {<br>  public int[] badValues(int[] val) {<br>    int[] r = new int[6];<br>    for (int x =0 ; x &lt; val.length; x++) {<br>      r[val[x]-1]++;<br>    }<br>    int t = 0;<br>    for (int x = 0; x &lt; 6; x++) {<br>      if ((float)r[x] / (float)val.length &lt; .1 || (float)r[x] / (float)val.length &gt; 0.25) t++;<br>    }<br>    int[] ret = new int[t];<br>    int c = 0;<br>    for (int x = 0; x &lt; 6; x++) {<br>      if ((float)r[x] / (float)val.length &lt; .1 || (float)r[x] / (float)val.length &gt; 0.25) { ret[c] = x+1; c++;}<br>    }<br>    return ret;<br>  }<br>}<br></td>