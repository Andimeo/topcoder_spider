<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br> <br>public class InequalityChecker<br>{<br>  public int[] getDifferences(int n)<br>  {<br>    int bigS = 0;<br>    int littleS = 0;<br> <br>    for(int i = 1; i &lt;= n; i++)<br>    {<br>      bigS += i * i * i;<br>    }<br> <br>    for(int i = 1; i &lt; n; i++)<br>    {<br>      littleS += i * i * i;<br>    }<br> <br>    int n4 = n * n * n * n;<br> <br>    int numerator = 2 * bigS + 2 * littleS - n4;<br>    int denominator = 4;<br> <br>    if(numerator % 2 == 0)<br>    {<br>      numerator /= 2;<br>      denominator /= 2;<br>    }<br> <br>    if(numerator % 2 == 0)<br>    {<br>      numerator /= 2;<br>      denominator /= 2;<br>    }<br> <br>    int[] result = new int[2];<br>    result[0] = numerator;<br>    result[1] = denominator;<br> <br>    return result;<br>  }<br>}<br></td>