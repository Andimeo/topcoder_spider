            import java.util.*;<br> <br>public class InequalityChecker<br>{<br>    private int s(int n)<br>    {<br>  int iResult = 0;<br>      for (int i=1; i&lt;=n-1; i++) iResult += i*i*i;<br>  return iResult;<br>    }<br> <br>    public int[] getDifferences(int n)<br>    {<br>  int num = 2*(s(n+1)+s(n)) - n*n*n*n;<br>  int denom = 4;<br>  if (num%4==0) { num/=4; denom=1; }<br>  else if (num%2==0) { num/=2; denom=2; }<br>  int[] res = new int[] {num, denom};<br>  return res;<br>    }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>