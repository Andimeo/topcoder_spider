            #include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cmath&gt;<br>#include &lt;queue&gt;<br>using namespace std;<br>typedef vector&lt;int&gt; VI;  typedef vector&lt;vector&lt;int&gt; &gt; VVI;<br>typedef vector&lt;string&gt; VS;  typedef vector&lt;vector&lt;string&gt; &gt; VVS;<br>typedef signed long long i64;  typedef unsigned long long u64;<br><br>class SortEstimate {<br>public:<br>double howMany(int c, int time) {<br>  int i, j, k, x, y, z, n;<br>  double ret;<br>  double lo = 0, hi = 1e20;<br><br>  for( i = 0; i &lt; 500; i++ ) {<br>    ret = (hi+lo)/2;<br>    if( ret*log(ret)/log(2.0)*c &lt;= time ) lo = ret; else hi = ret;<br>  }<br>  ret = (hi+lo)/2;<br>  return ret;<br>}<br>};<br><br><br>// Powered by FileEdit<br></td>