            // Powered by radeclipse!<br>import java.util.* ;<br>public class SortEstimate {<br>  int cc = 0 ;<br>  double f(double x) {<br>    return cc * x * Math.log(x) / Math.log(2) ;<br>  }<br>  public double howMany(int c, int time) {<br>    double low = 1 ;<br>    cc = c ;<br>    while (f(low) &lt; time)<br>      low *= 2 ;<br>    double high = low ;<br>    low = 1 ;<br>    for (int i=0; i&lt;50; i++) {<br>      double m = (high + low) / 2 ;<br>      if (f(m) &lt; time)<br>  low = m ;<br>      else<br>  high = m ;<br>    }<br>    return low ;<br>  }<br>}<br></td>