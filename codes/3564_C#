            using System;<br>public class PascalCount {<br> <br>public int f (int x, int v) {<br>  int ret = 0;<br>  while (x % v == 0) {<br>    x = x / v;<br>    ret++;<br>  };<br>  return ret;<br>}<br> <br>public int howMany(int i, int d) {<br>  int ret = 0;<br>  int c2 = 0;<br>  int c3 = 0;<br>  int c5 = 0;<br>  for (int j = 1; j &lt; i; j++) {<br>    c2 += f(i + 1 - j, 2);<br>    c3 += f(i + 1 - j, 3);<br>    c5 += f(i + 1 - j, 5);<br>    c2 -= f(j, 2);<br>    c3 -= f(j, 3);<br>    c5 -= f(j, 5);<br>    if (d == 2 &amp;&amp; c2 &gt; 0) ret++;<br>    if (d == 3 &amp;&amp; c3 &gt; 0) ret++;<br>    if (d == 4 &amp;&amp; c2 &gt; 1) ret++;<br>    if (d == 5 &amp;&amp; c5 &gt; 0) ret++;<br>    if (d == 6 &amp;&amp; c2 &gt; 0 &amp;&amp; c3 &gt; 0) ret++;<br>  };<br>  return ret;<br>}<br> <br>}<br></td>