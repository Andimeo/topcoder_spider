            #include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cmath&gt;<br>#include &lt;queue&gt;<br>using namespace std;<br>typedef vector&lt;int&gt; VI;  typedef vector&lt;vector&lt;int&gt; &gt; VVI;<br>typedef vector&lt;string&gt; VS;  typedef vector&lt;vector&lt;string&gt; &gt; VVS;<br>typedef signed long long i64;  typedef unsigned long long u64;<br><br>int f2[5000001];<br>int f3[5000001];<br>int f5[5000001];<br><br>class PascalCount {<br>public:<br>int howMany(int i, int d) {<br>  int j, k, x, y, z, n;<br>  int ret = 0;<br><br>  for( j = 1; j &lt;= 5000000; j++ ) f2[j] = j/2 + f2[j/2];<br>  for( j = 1; j &lt;= 5000000; j++ ) f3[j] = j/3 + f3[j/3];<br>  for( j = 1; j &lt;= 5000000; j++ ) f5[j] = j/5 + f5[j/5];<br>  for( j = 0; j &lt;= i; j++ ) {<br>    int n2 = f2[i] - f2[j] - f2[i-j];<br>    int n3 = f3[i] - f3[j] - f3[i-j];<br>    int n5 = f5[i] - f5[j] - f5[i-j];<br>    if( d == 2 &amp;&amp; n2 ) ret++;<br>    if( d == 3 &amp;&amp; n3 ) ret++;<br>    if( d == 4 &amp;&amp; n2 &gt;= 2 ) ret++;<br>    if( d == 5 &amp;&amp; n5 ) ret++;<br>    if( d == 6 &amp;&amp; n2 &amp;&amp; n3 ) ret++;<br>  }<br>  return ret;<br>}<br>};<br><br><br>// Powered by FileEdit<br></td>