            // Powered by radeclipse!<br>import java.util.* ;<br>public class PascalCount {<br>  int fact(int v, int d) {<br>    int r = 0 ;<br>    if (v == 0)<br>      throw new Error("Empty v") ;<br>    int den = d ;<br>    while (v % den == 0) {<br>      r++ ;<br>      den *= d ;<br>    }<br>    return r ;<br>  }<br>  public int howMany(int i, int d) {<br>    int v = 1 ;<br>    int c2 = 0 ;<br>    int c3 = 0 ;<br>    int c5 = 0 ;<br>    int r = 0 ;<br>    int d2 = fact(d, 2) ;<br>    int d3 = fact(d, 3) ;<br>    int d5 = fact(d, 5) ;<br>    for (int j=0; j&lt;i; j++) {<br>      int den = j + 1 ;<br>      int n = i + 1 - den ;<br>      c2 += fact(n, 2) - fact(den, 2) ;<br>      c3 += fact(n, 3) - fact(den, 3) ;<br>      c5 += fact(n, 5) - fact(den, 5) ;<br>      if (c2 &gt;= d2 &amp;&amp; c3 &gt;= d3 &amp;&amp; c5 &gt;= d5) {<br>  r++ ;<br>      }<br>    }<br>    return r ;<br>  }<br>}<br></td>