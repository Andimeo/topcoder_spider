            /*<br>*/<br> <br>import java.util.*;<br> <br>public class Checker {<br>  boolean validname(String s) {<br>    for(int i = 0; i &lt; s.length(); i++) {<br>      char c = s.charAt(i);<br>      if(!((c &gt;= 'A' &amp;&amp; c &lt;= 'Z') || c == '-')) return false;<br>    }<br>    return true;<br>  }<br> <br>  public boolean CheckData(String[] data) {<br>    HashSet l1 = new HashSet();<br>    HashSet l2 = new HashSet();<br> <br>    int n = data.length;<br>    if(n &lt; 2 || n &gt; 20) return false;<br>    for(int i = 0; i &lt; n; i++)<br>      if(data[i].length() &gt; 40) return false;<br> <br>    for(int i = 0; i &lt; n; i++) {<br>      StringTokenizer st = new StringTokenizer(data[i], " ", true);<br>      if(st.countTokens() != 5) return false;<br>      String x = st.nextToken();<br>      if(!validname(x)) return false;<br>      if(!st.nextToken().equals(" ")) return false;<br>      if(!st.nextToken().equals("LOVES")) return false;<br>      if(!st.nextToken().equals(" ")) return false;<br>      String y = st.nextToken();<br>      if(!validname(y)) return false;<br>      if(x.equals(y)) return false;<br>      l1.add(x);<br>      l2.add(y);<br>    }<br> <br>    for(Iterator it = l2.iterator(); it.hasNext();) {<br>      String x = (String)it.next();<br>      if(!l1.contains(x)) return false;<br>    }<br> <br>    return true;<br>  }<br>  <br>  public static void main(String[] args) {<br>    Checker temp = new Checker();<br>    //System.out.println(temp.CheckData(???));<br>  }<br>}<br> <br> <br>// Powered by FileEdit<br></td>