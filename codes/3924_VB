            Public Class CommunicableDisease<br>        Private aWouldSpread(,) As Boolean<br>        Public Function findSource(ByVal contact As String(), ByVal results As String) As String<br>            ReDim aWouldSpread(contact.GetUpperBound(0), contact.GetUpperBound(0))<br> <br>            Dim i, j, k As Integer<br>            For i = 0 To contact.GetUpperBound(0)<br>                aWouldSpread(i, i) = True<br>            Next<br> <br>            Dim aHasX(contact.GetUpperBound(0)) As Boolean<br> <br>            For i = 0 To contact.GetUpperBound(0)<br>                For j = 0 To contact(0).Split(" ").GetUpperBound(0)<br>                    For k = 0 To contact.GetUpperBound(0)<br>                        aHasX(k) = aWouldSpread(i, k)<br>                    Next<br> <br>                    For k = 0 To contact.GetUpperBound(0)<br>                        If aHasX(k) Then aWouldSpread(i, contact(k).Split(" ")(j)) = True<br>                    Next<br>                Next<br>            Next<br> <br>            Dim aRet(contact.GetUpperBound(0)) As String<br>            For i = 0 To contact.GetUpperBound(0)<br>                aRet(i) = "I"<br>            Next<br> <br>            For i = 0 To contact.GetUpperBound(0)<br>                If results.Substring(i, 1) = "N" Then<br>                    For j = 0 To contact.GetUpperBound(0)<br>                        If aWouldSpread(j, i) Then aRet(j) = "N"<br>                    Next<br>                End If<br>            Next<br> <br>            Dim iSuspectSources As Integer<br> <br>            For i = 0 To contact.GetUpperBound(0)<br>                If results.Substring(i, 1) = "C" Then<br>                    iSuspectSources = 0<br>                    For j = 0 To contact.GetUpperBound(0)<br>                        If aWouldSpread(j, i) And aRet(j) &lt;&gt; "N" Then<br>                            iSuspectSources += 1<br>                        End If<br>                    Next<br>                    If iSuspectSources = 0 Then Return "INVALID"<br>                    If iSuspectSources = 1 Then<br>                        For j = 0 To contact.GetUpperBound(0)<br>                            If aWouldSpread(j, i) And aRet(j) &lt;&gt; "N" Then aRet(j) = "C"<br>                        Next<br>                    End If<br>                End If<br>            Next<br> <br>            Dim sRet As String = ""<br>            For i = 0 To contact.GetUpperBound(0)<br>                sRet &amp;= aRet(i)<br>            Next<br> <br>            Return sRet<br> <br> <br>        End Function<br>    End Class<br></td>