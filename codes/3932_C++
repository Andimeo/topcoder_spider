            #line 2 "Buoyancy.cpp"<br>#include &lt;algorithm&gt;<br><br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br><br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cctype&gt;<br><br>using namespace std;<br><br>#define REP(i,N) for(int i=0;i&lt;(N);i++)<br>#define FOR(i,L,U) for(int i=(L);i&lt;=(U);i++)<br>#define FORD(i,U,L) for(int i=(U);i&gt;=(L);i--)<br>#define FOREACH(it,c) for(typeof((c).begin()) it=(c).begin(); it!=(c).end(); ++it)<br>#define CLEAR(a) memset((a),0,sizeof(a))<br><br>typedef vector&lt;int&gt; VI;<br>typedef vector&lt; vector&lt;int&gt; &gt; VVI;<br>typedef vector&lt;string&gt; VS;<br>typedef vector&lt; vector&lt;string&gt; &gt; VVS;<br>typedef pair&lt;int,int&gt; PII;<br>typedef map&lt;string,int&gt; MSI;<br><br>VS split(const string &amp;s, const string &amp;delim="") {<br>  string t="";<br>  VS res;<br>  for(unsigned int i=0; i&lt;s.size(); i++) {<br>    if (delim.find(s[i]) != string::npos) {<br>      if (!t.empty()) res.push_back(t);<br>      t="";<br>    } else {<br>      t+=s[i];<br>    }<br>  }<br>  if (!t.empty()) res.push_back(t);<br>  return res;<br>}<br><br>class Buoyancy {<br>  public:<br>  double waterLevel(double wid, double len, double waterHt, double objWt, double objHt) {<br>    double V = objHt;<br>    double S = wid * len;<br><br>    double lo = waterHt;<br>    double hi = waterHt + objHt;<br><br>    double rho = objWt / V;<br>    int floats = (objWt &lt; objHt);<br>    <br>    REP(step,10000) {<br>      double cur = (lo+hi) / 2.0;<br>      double water = wid * len * cur;<br>      if (floats) {<br>        double sub = min( cur, rho * double(objHt) );<br>        water -= sub;<br>      } else {<br>        double sub = min( cur, double(objHt) );<br>        water -= sub;<br>      }<br><br>      if (water &gt; wid * len * waterHt) hi = cur; else lo = cur;<br>    }<br><br>    return lo;<br>  }<br>};<br><br><br>// Powered by FileEdit<br></td>