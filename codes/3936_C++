            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;string.h&gt;<br>#include &lt;stdio.h&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>using namespace std;<br> <br>class DiskCut {<br>  public:<br>  double minCost(int a, vector &lt;int&gt; b) {<br>    sort( b.begin(), b.end() );<br>    multiset&lt; int &gt; q;<br>    for( int i = 0; i &lt; ( int )b.size(); i++ ) q.insert( b[i] );<br>    double cost = a;<br>    while( q.size() &gt; 1 )<br>    {<br>      int x = *q.begin(); q.erase( q.begin() );<br>      int y = *q.begin(); q.erase( q.begin() );<br>      q.insert( x + y );<br>      cost += ( x + y ) * a / 100.0;<br>    }<br>    return cost;<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>