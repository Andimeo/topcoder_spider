            import java.util.*;<br>public class DiskCut {<br>    public double minCost(int area, int[] percent) {<br>        double res = 0;<br><br>        Arrays.sort(percent);<br><br>        ArrayList al = new ArrayList();<br><br><br><br>        for (int i = 0; i &lt; percent.length; i++)<br>            al.add(new Integer(percent[i]));<br><br>        while (al.size()&gt;1) {<br>            Collections.sort(al);<br>            int l = ((Integer)al.get(0)).intValue();<br>            int r = ((Integer)al.get(1)).intValue();<br>            al.remove(0); al.remove(0);<br>            al.add(new Integer(l+r));<br>            res=res+l+r;<br>        }<br><br>        return area*(res+100)/100;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>