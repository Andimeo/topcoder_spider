            import java.util.*;<br>import java.lang.*;<br>import java.awt.*;<br> <br>public class AutoMarket<br>{<br>  public int maxSet(int[] cost, int[] features, int[] fixTimes)<br>  {<br>    int n = cost.length;<br>    for (int i = 0; i &lt; n; i++)<br>    {<br>      for (int j = n - 1; j &gt; 0; j--)<br>      {<br>        if (cost[j] &gt; cost[j - 1])<br>        {<br>          int temp = cost[j];<br>          cost[j] = cost[j - 1];<br>          cost[j - 1] = temp;<br>          temp = features[j];<br>          features[j] = features[j - 1];<br>          features[j - 1] = temp;<br>          temp = fixTimes[j];<br>          fixTimes[j] = fixTimes[j - 1];<br>          fixTimes[j - 1] = temp;<br>        }<br>      }<br>    }<br>    <br>    int length[] = new int[n];<br>    for (int i = 0; i &lt; n; i++) length[i] = 1;<br>    for (int i = 0; i &lt; n; i++)<br>    {<br>      for (int j = i + 1; j &lt; n; j++)<br>      {<br>        if (cost[i] &gt; cost[j] &amp;&amp; features[i] &lt; features[j] &amp;&amp; fixTimes[i] &gt; fixTimes[j])<br>        {<br>          if (length[i] + 1 &gt; length[j]) length[j] = length[i] + 1;  <br>        }<br>      }<br>    }<br>    int max = -1;<br>    for (int i = 0; i &lt; n; i++)<br>    {<br>      if (length[i] &gt; max) max = length[i];  <br>    }<br>    return max;<br>  }<br> <br>  <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>