            import java.util.*;<br>public class LargeSignTest {<br>  double doit(int N, int i) {<br>    return lfact[N] - lfact[i] - lfact[N - i];<br>  }<br>  double[] lfact = new double[1000100];<br>  public String pvalue(int N, int K) {<br>    for (int i = 1; i &lt; lfact.length; i++) {<br>      lfact[i] = lfact[i - 1] + Math.log(i);<br>    }<br>    if (K * 2 == N) {<br>      return "100.0%";<br>    }<br>    double t = 0;<br>    for (int i = 0; i &lt;= Math.min(K, N - K); i++) {<br>      t += Math.exp(doit(N, i) - (N - 1) * Math.log(2));<br>    }<br>    t *= 1000;<br>    long r = Math.round(t);<br>    String ret = "." + (r % 10) + "%";<br>    r /= 10;<br>    ret = r + ret;<br>    return ret;<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>