<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>using System.Collections;<br>using math = System.Math;<br> <br> <br>public class CakeDivision<br>{<br>  double minRatio(double x, double y, int n)<br>  {<br>    if (n==1)<br>    {<br>      if (x&gt;y) return x/y;<br>      return y/x;<br>    }<br>    if (n==2) return math.Min(math.Max(x/2/y,y/x*2), math.Max(y/2/x, x/y*2));<br>    int i;<br>    double res = 1e20;<br>    for(i=1;i*2&lt;=n;i++)<br>    {<br>      double v = math.Max(minRatio(i*x/n,y,i), minRatio((n-i)*x/n,y,n-i));<br>      if (v &lt; res) res = v;<br>      v = math.Max(minRatio(x, i*y/n,i), minRatio(x, (n-i)*y/n,n-i));<br>      if (v &lt; res) res = v;<br>    }<br>    return res;<br>  }<br>public   double ratio(int length, int width, int pieces)<br>  {<br>    return minRatio(length, width, pieces);<br>  }<br>}<br></td>