<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>public class ProgramSchedule<br>{<br>   public int schedule(int[] A, int[] B)<br>   {<br>      int max = 0;<br>      int setup_time = 0;<br>      <br>      for(int x = 0; x &lt; A.Length; x++) {<br>         int hold = 0;<br>         for(int y = 0; y &lt; B.Length; y++) {<br>            if(B[y] &gt; B[hold])<br>               hold = y;<br>         }<br> <br>         setup_time += A[hold];<br>         if(max &lt; setup_time + B[hold])<br>            max = setup_time + B[hold];<br>         B[hold] = 0;<br>         A[hold] = 0;<br> <br>      }<br> <br>      return max;<br>   }<br> <br> <br>}<br></td>