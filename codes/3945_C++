            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>using namespace std;<br>class ProgramSchedule {<br>  public:<br>  int schedule(vector &lt;int&gt; A, vector &lt;int&gt; B) {<br>    int i, j;<br>    for (i = 0; i &lt; A.size(); i++) {<br>      for (j = i + 1; j &lt; A.size(); j++) {<br>        if (B[i] &lt; B[j]) {<br>          int tmp;<br>          tmp = B[i];<br>          B[i] = B[j];<br>          B[j] = tmp;<br>          tmp = A[i];<br>          A[i] = A[j];<br>          A[j] = tmp;<br>        }<br>      }<br>    }<br>    int max = 0;<br>    int sum = 0;<br>    for (i = 0; i &lt; A.size(); i++) {<br>      sum += A[i];<br>      int m = B[i] + sum;<br>      if (m &gt; max) max = m;<br>    }<br>    return max;<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>