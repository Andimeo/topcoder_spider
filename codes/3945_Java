<td class="problemText" colspan="8" valign="middle" align="left">
            import java.util.*;<br> <br>public class ProgramSchedule<br>{<br>  public int schedule(int[] A, int[] B)<br>  {<br>    int tmp;<br>    for(int i=0; i&lt;B.length; i++)<br>      for(int j=i+1; j&lt;B.length; j++)<br>        if(B[j]&gt;B[i])<br>        {<br>          tmp=B[i]; B[i]=B[j]; B[j]=tmp;<br>          tmp=A[i]; A[i]=A[j]; A[j]=tmp;<br>        }<br>    int curT=0;<br>    int allT=0;<br>    for(int i=0; i&lt;A.length; i++)<br>    {<br>      curT+=A[i];<br>      if(curT+B[i]&gt;allT) allT=curT+B[i];<br>    }<br>    return allT;<br> <br>  }<br>}<br></td>