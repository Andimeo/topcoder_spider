            #include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;string&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br> <br>using namespace std;<br> <br>class Stitch {<br>public:<br>  vector &lt;string&gt; stitch(vector &lt;string&gt;, vector &lt;string&gt;, int);<br>};<br> <br>vector &lt;string&gt; Stitch::stitch(vector &lt;string&gt; A, vector &lt;string&gt; B, int overlap) {<br>  vector&lt;string&gt; ret;<br>  <br>  for(int i=0; i&lt;A.size(); i++)<br>  {<br>    string s;<br>    s += A[i].substr(0,A[i].length()-overlap);<br>    <br>    for(int j=0; j&lt;overlap; j++)<br>    {<br>      double x=((overlap-j)*((int)A[i][j+A[i].length()-overlap])+(j+1)*B[i][j]);<br>      x /= (overlap+1);<br>      <br>      int a = (int)(x+0.5+1e-9);<br>      s += (char)a;<br>    }<br>    <br>    s += B[i].substr(overlap,-1);<br>    <br>    ret.push_back(s);<br>  }  <br>  <br>  return ret;<br>  <br>}<br> <br> <br>//Powered by [KawigiEdit]<br></td>