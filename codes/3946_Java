            import java.util.*;<br>import java.util.regex.*;<br> <br>public class Stitch {<br>    int N;<br>    public String[] stitch(String[] A, String[] B, int overlap) { <br>  N = A.length;<br>        String[] rv = new String[N];<br>  for (int i=0; i&lt;N; i++) {<br>      rv[i] = A[i].substring(0, A[i].length()-overlap);<br>      for (int j=0; j &lt; overlap; j++) {<br>    int ca = A[i].charAt(A[i].length()-overlap+j);<br>    int cb = B[i].charAt(j);<br>    int c = ((overlap+1-(j+1))*ca + ((1+j)*cb) + (overlap+1)/2)/(overlap+1);<br>    rv[i] = rv[i] + (char)c;<br>      }<br>      rv[i] += B[i].substring(overlap);<br>  }<br>        return rv;<br>    }<br> <br>}<br> <br>// Powered by CodeProcessor<br></td>