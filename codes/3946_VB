            Public Class Stitch<br>        Public Function stitch(ByVal A As String(), ByVal B As String(), ByVal overlap As Integer) As String()<br>            Dim i, j, k As Integer<br>            Dim l As Double<br>            Dim m As Integer<br>            Dim aRet(A.GetUpperBound(0)) As String<br>            For i = 0 To A.GetUpperBound(0)<br>                aRet(i) &amp;= A(i).Substring(0, A(i).Length - overlap)<br> <br>                For j = 1 To overlap<br>                    k = (Microsoft.VisualBasic.Asc(A(i).Substring(A(i).Length - overlap + j - 1)) * (overlap - j + 1)) + Microsoft.VisualBasic.Asc(B(i).Substring(j - 1)) * (j)<br>                    l = CDbl(k) / (overlap + 1)<br>                    m = System.Math.Round(l)<br>                    If l - m = 0.5 Then m += 1<br>                    aRet(i) &amp;= Microsoft.VisualBasic.Chr(m)<br>                Next<br> <br>                aRet(i) &amp;= B(i).Substring(overlap)<br>            Next<br> <br>            Return aRet<br>        End Function<br>    End Class<br></td>