            #include &lt;iostream&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;sstream&gt;<br>#include &lt;cmath&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;utility&gt;<br>#include &lt;functional&gt;<br>#include &lt;queue&gt;<br>#include &lt;list&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cctype&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br>#include &lt;complex&gt;<br><br>using namespace std;<br><br>typedef  complex&lt;double&gt; tComp;<br>typedef pair&lt;int,int&gt; PII;<br>typedef vector&lt;int&gt; VI;<br>typedef vector&lt;PII&gt; VPII;<br>typedef vector&lt;VI&gt; VVI;<br>typedef vector&lt;VPII&gt; VVPII;<br>typedef map&lt;string,int&gt; MSI;<br>typedef map&lt;int,string&gt; MIS;<br>typedef map&lt;int,VI&gt; MIVI;<br>typedef priority_queue&lt;int,VI,greater&lt;int&gt; &gt; PQI;<br>typedef set&lt;int&gt; SI;<br>typedef set&lt;string&gt; SS;<br><br>const double eps = 1e-9;<br><br>class Shouting {public:double shout(vector &lt;int&gt; x, vector &lt;int&gt; y); };<br><br>double Shouting::shout(vector &lt;int&gt; x, vector &lt;int&gt; y)<br>{<br>    int n = x.size();<br>    double adj[64][64];<br>    for (int i = 0; i &lt; n; i++)<br>        for (int j = 0; j &lt; n; j++)<br>            adj[i][j] = double(x[i] - x[j]) * double(x[i] - x[j]) + double(y[i] - y[j]) * double(y[i] - y[j]);<br>    <br>    for (int k = 0; k &lt; n; k++)<br>        for (int i = 0; i &lt; n; i++)<br>            for (int j = 0; j &lt; n; j++)<br>                adj[i][j] &lt;?= adj[i][k] &gt;? adj[k][j];<br><br>    double res = 0.0;<br>    for (int i = 0; i &lt; n; i++)<br>        for (int j = 0; j &lt; n; j++)<br>            res &gt;?= adj[i][j];<br>    return sqrt(res);<br>}<br><br><br>// Powered by FileEdit<br></td>