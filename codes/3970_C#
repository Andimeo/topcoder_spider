            using System;<br>using System.Collections;<br>public class AutoLoan {<br>  public double interestRate(double price, double monthlyPayment, int loanTerm) {<br>    double left = 0;<br>    double right = 1;<br><br>    while ((right - left) &gt; right * 1e-15)<br>    {<br>      double middle = (left + right) / 2;<br>      if (middle == left || middle == right)<br>        break;<br><br>      double p = price;<br><br>      for (int i = 0; i &lt; loanTerm; ++i)<br>      {<br>        p = (p * (1 + middle)) - monthlyPayment;<br>      }<br><br>      if (p &lt;= 0)<br>        left = middle;<br>      else<br>        right = middle;<br>    }<br><br>    return left * 1200;<br>  }<br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>