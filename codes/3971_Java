            public class StockHistory<br>{<br>  public  int maximumEarnings(int initialInvestment, int monthlyContribution, String[] stockPrices)<br>  {<br>    double maxPercentGain = 0;<br>    double totalEarnings = 0;<br>    String[] s = stockPrices[0].split(" ");<br>    <br>    int[][] sp = new int[stockPrices.length][ s.length];<br>    for(int i = 0; i &lt; stockPrices.length; i++)<br>    {<br>      s = stockPrices[i].split(" ");<br>      for(int j = 0; j &lt; s.length; j++)<br>        sp[i][j] = Integer.parseInt(s[j]);<br>      <br>    }<br>    <br>    for(int i = 0; i &lt; stockPrices.length; i++)<br>    {<br>      for(int j = 0; j &lt; s.length; j++)<br>        System.out.print(sp[i][j] + " ");<br> <br>      System.out.println("");<br>    }<br>    <br>    for(int i = stockPrices.length - 2; i &gt;= 0; i--)<br>    {<br>      for( int j = 0; j &lt; s.length; j++)<br>      {<br>        if(  ((sp[stockPrices.length-1][j] - sp[i][j] ) / (float)sp[i][j] )  &gt; maxPercentGain)<br>        {<br>          <br>          maxPercentGain = (sp[stockPrices.length-1][j] - sp[i][j] ) / (float)sp[i][j];<br>          System.out.println("maxPercent = " + maxPercentGain + " " + i + " " + j );<br>        }<br>      }<br>      if( i != 0)<br>        totalEarnings = totalEarnings + maxPercentGain * monthlyContribution;<br>    }<br>    totalEarnings = totalEarnings + maxPercentGain * initialInvestment;<br> <br>    return (int)( totalEarnings + 0.5) ;<br>  }<br>}<br></td>