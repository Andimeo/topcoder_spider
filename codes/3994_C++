            #include &lt;string&gt;<br>#include &lt;vector&gt;<br> <br>using namespace std;<br> <br>class PipeCuts {<br>  public:<br>  double probability(vector &lt;int&gt; v, int L) {<br>    <br>    double tot = 0;<br>    sort(v.begin(), v.end());<br>    for (int i = 0; i &lt; v.size(); i++)<br>      for (int j = i+1; j &lt; v.size(); j++)<br>        if (v[i] &gt; L || v[j] - v[i] &gt; L || 100 - v[j] &gt; L)<br>    tot++;<br>    return 2 * tot / v.size() / (v.size() - 1);<br> <br>  }<br>};<br> <br> <br>// Powered by FileEdit<br></td>