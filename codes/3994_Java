            import java.util.*;<br> <br>public class PipeCuts<br>{<br>    public double probability(int[] w, int L)<br>    {<br>        double n = 0;<br>        double t = 0;<br>        Arrays.sort(w);<br>        for (int i=0; i &lt; w.length; i++)<br>        {<br>            for (int j = i+1; j &lt; w.length; j++)<br>            {<br>                int v1 = w[i];<br>                int v2 = w[j]-w[i];<br>                int v3 = 100-w[j];<br>                if (v1&gt;L||v3&gt;L||v2&gt;L) n++;<br>                t++;<br>            }<br>        }<br>        <br>        return n/t;<br>    }<br> <br>}<br></td>