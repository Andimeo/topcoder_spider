<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br> <br>public class ComboLength<br>{<br>  public int howLong(string moves)<br>  {<br>    int maxLength = 0;<br>    int currLength = 0;<br>    char curr = ' ';<br> <br>    for(int i = 0; i &lt; moves.Length; i++)<br>    {<br>      if(moves[i] != curr)<br>      {<br>        curr = moves[i];<br>        currLength = 1;<br>      }<br>      else<br>      {<br>        currLength++;<br>      }<br> <br>      maxLength = Math.Max(maxLength, currLength);<br>    }<br> <br>    return maxLength;<br>  }<br>}<br></td>