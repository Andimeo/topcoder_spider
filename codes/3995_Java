            import java.util.regex.Pattern; <br>public class ComboLength <br>{ <br>  public static void main(String[] args)  <br>  {  <br>     ComboLength test = new ComboLength();  <br>    System.out.println(test.howLong("AAABBBBB"));<br>    System.out.println(test.howLong("BBAABBAABBAABBAA"));<br>    System.out.println(test.howLong("AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA"));<br> <br>  }  <br>  public int howLong(String moves)<br>  {  <br>    String regex="A+";<br>    Pattern pat = Pattern.compile (regex);<br>    String[] a;<br>    CharSequence cs=moves;<br>    a=pat.split(cs);<br>    int n=a.length;<br>    int amax=0;<br>    for (int i=0;i&lt;n;i++) <br>    {<br>      //System.out.println(a[i]);<br>      if (a[i].length()&gt;amax)<br>        amax=a[i].length();<br>    } <br>    regex="B+";<br>    pat=Pattern.compile(regex);<br>    a=pat.split(cs);<br>    int bmax=0;<br>    n=a.length;<br>    for (int i=0;i&lt;n;i++) <br>    {<br>      if (a[i].length()&gt;bmax)<br>        bmax=a[i].length();<br>    }<br>    //System.out.println(amax);<br>     //System.out.println(bmax);<br>    return Math.max(amax,bmax);<br>  } <br>}<br></td>