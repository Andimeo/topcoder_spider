            import java.util.*;<br>public class FastSpider {<br>  public int findTime(double spiderSpeed, double manSpeed) {<br>    double timeinc = 0.00001;<br>    double fraction = 0;<br>    double time = 0;<br>    double length = 1;<br>    double maninc;<br>    double spiderinc;<br>    while(time &lt; 3600.6){<br>      maninc = manSpeed*timeinc;<br>      spiderinc = spiderSpeed*timeinc;<br>      time+=timeinc;<br>      length+=maninc;<br>      fraction+=spiderinc/length;<br>      if(fraction &gt; 0.99999){<br>        break;<br>      }<br>      if(time &gt; 10 &amp;&amp; timeinc &lt; 0.00005)<br>      timeinc = 0.0001;<br>      if(time &gt; 100 &amp;&amp; timeinc &lt; 0.0005)<br>      timeinc = 0.001;<br>      if(time &gt; 1000 &amp;&amp; timeinc &lt; 0.005)<br>        timeinc = 0.01;<br>    }<br>    if(fraction &lt; 0.9999)<br>    return -1;<br>    int t = (int)time;<br>    if(time-t &gt; 0.5)<br>    t++;<br>    return t;<br>    <br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>