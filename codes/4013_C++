            #include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cmath&gt;<br>#include &lt;queue&gt;<br>using namespace std;<br>typedef vector&lt;int&gt; VI;  typedef vector&lt;vector&lt;int&gt; &gt; VVI;<br>typedef vector&lt;string&gt; VS;  typedef vector&lt;vector&lt;string&gt; &gt; VVS;<br>typedef signed long long i64;  typedef unsigned long long u64;<br><br>class OptimalTax {<br>public:<br>double optimalIncome(vector &lt;int&gt; a, vector &lt;int&gt; b, int c) {<br>  int i, j, k, x, y, z, n;<br>  double ret, mn = 0, mx=1e10;<br>  <br>  for( i = 0; i &lt; a.size(); i++ ) if( i != c ) {<br>    x = b[i]-b[c];<br>    y = a[c]-a[i];<br>    if( x == 0 ) {<br>      if( y &gt;= 0 ) return -1.0;<br>    } else if( x &gt; 0 ) {<br>      mn &gt;?= y*100.0/x;<br>    } else if( x &lt; 0 ) {<br>      mx &lt;?= y*100.0/x;<br>    }<br>  }<br>  if( mn+1e-9 &gt; mx ) return -1.0;<br>  return mn;<br>}<br>};<br><br><br>// Powered by FileEdit<br></td>