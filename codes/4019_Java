            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class NoisySensor<br>{<br>  public static int []a=new int[3];<br>  public int[] medianFilter(int[] data)<br>  {  <br>    int n=data.length;<br>    int[] ret=new int[n];<br>    ret[0]=data[0];<br>    ret[n-1]=data[n-1];<br>    for(int i=1;i&lt;=n-2;i++)<br>    {<br>      a[0]=data[i-1];<br>      a[1]=data[i];<br>      a[2]=data[i+1];<br>      ret[i]=get();<br>    }<br>    return ret;<br>  } <br>  public static int get()<br>  {<br>    Arrays.sort(a);<br>    return a[1];<br>  }<br>  <br>  <br>}<br> <br>//Powered by [KawigiEdit]<br></td>