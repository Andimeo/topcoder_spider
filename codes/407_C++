            #line 2 "Chooser.cpp"<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br> <br>class Chooser {<br>  public:<br>  int numSets(vector&lt;int&gt; easy, vector&lt;int&gt; middle, vector&lt;int&gt; <br>hard) {<br>    int count = 0;<br>    for (int x = 0; x &lt; easy.size(); x++) {<br>      for (int y = 0; y &lt; middle.size(); y++) {<br>        for (int z = 0; z &lt; hard.size(); z++) {<br>          int t =easy[x] + middle[y] + hard[z] ;<br>          if (t &gt;= 60 &amp;&amp; t &lt;= 75)<br>            count++;<br>        }<br>      }<br>    }<br>    return count;<br>  }<br>};<br> <br> <br> <br>// Powered by FileEdit<br></td>