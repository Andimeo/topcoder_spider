            import java.awt.geom.*;<br>import java.io.*;<br>import java.math.*;<br>import java.util.*;<br> <br>public class Chooser {<br>  public int numSets(int[] easy, int[] middle, int[] hard) {<br>    int i,j,k;<br>    int rval;<br>    rval = 0;<br>    for(i=0; i&lt;easy.length; i++)<br>    for(j=0; j&lt;middle.length; j++)<br>    for(k=0; k&lt;hard.length; k++) {<br>      int t = easy[i]+middle[j]+hard[k];<br>      if(t&gt;=60 &amp;&amp; t&lt;=75) rval++;<br>    }<br>    return (int)rval;<br>  }<br>}<br></td>