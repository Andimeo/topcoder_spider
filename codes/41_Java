            public class Syntax<br>{<br>  public boolean match(String input)<br>  {<br>    java.util.Stack stack = new java.util.Stack();<br>    for (int x = 0; x &lt; input.length(); x++)<br>    {<br>      if (input.charAt(x) == '{' || input.charAt(x) == '[' || input.charAt(x) == '(')<br>      {<br>        String newString = "" + input.charAt(x);<br>        stack.push(newString);<br>      }<br>      else if (input.charAt(x) == '}')<br>      {<br>        if (stack.size() == 0)<br>        {<br>          return false;<br>        } <br>        String currString = (String) stack.pop();<br>        char currChar = currString.charAt(0);<br>        if (currChar != '{')<br>        {<br>          return false;<br>        }<br>      }<br>      else if (input.charAt(x) == ')')<br>      {<br>        if (stack.size() == 0)<br>        {<br>          return false;<br>        } <br>        String currString = (String) stack.pop();<br>        char currChar = currString.charAt(0);<br>        if (currChar != '(')<br>        {<br>          return false;<br>        }<br>      }<br>      else if (input.charAt(x) == ']')<br>      {<br>        if (stack.size() == 0)<br>        {<br>          return false;<br>        } <br>        String currString = (String) stack.pop();<br>        char currChar = currString.charAt(0);<br>        if (currChar != '[')<br>        {<br>          return false;<br>        }<br>      }<br>    }<br>    return true;<br>  }<br>}<br></td>