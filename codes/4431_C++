            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;sstream&gt;<br>#include &lt;cmath&gt;<br>#include &lt;iostream&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstdio&gt;<br> <br>using namespace std;<br> <br>class CircleOrder<br>{<br>public:<br>  int pos( string a, int i )<br>  {<br>    int j;<br>    <br>    for (j = (i + 1) % a.length(); i != j &amp;&amp; a[j] != a[i] - 'a' + 'A'; j = (j + 1) % a.length())<br>      if ('a' &lt;= a[j] &amp;&amp; a[j] &lt;= 'z' &amp;&amp; i != j)<br>        break;<br>    if (a[j] == a[i] - 'a' + 'A')<br>      return 1;<br>    for (j = (i + a.length() - 1) % a.length(); i != j &amp;&amp; a[j] != a[i] - 'a' + 'A'; j = (j + a.length() - 1) % a.length())<br>      if ('a' &lt;= a[j] &amp;&amp; a[j] &lt;= 'z')<br>        break;<br>    return a[j] == a[i] - 'a' + 'A';<br>  };<br>  int get( string s, char t )<br>  {<br>    for (int i = 0; i &lt; s.length(); i++)<br>      if (s[i] == t - 'a' + 'A')<br>        return i;<br>    return -1;<br>  };<br>  string firstOrder( string s )<br>  {<br>    string ans;<br>    int i, j, k, l, n;<br>    <br>    n = s.length();<br>    for (i = 0; i &lt; n / 2; i++)<br>    {<br>      for (j = 0, k = -1; j &lt; n; j++)<br>        if ('a' &lt;= s[j] &amp;&amp; s[j] &lt;= 'z' &amp;&amp; (k == -1 || s[k] &gt; s[j]))<br>          if (pos(s, j))<br>            k = j;<br>      if (k == -1)<br>        return "NONE";<br>      ans += s[k];<br>      l = get(s, s[k]);<br>      s[l] = s[k], s[k] = '#';<br>    }<br>    return ans;<br>  };<br>};<br></td>