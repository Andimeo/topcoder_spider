            using System;<br>public class PerforatedSheet {<br> <br>public double[] getCenterOfMass(int sheetWidth, int sheetHeight, int[] x, int[] y, int[] width, int[] height) {<br> <br>decimal sumX = 1.0M * sheetWidth * sheetHeight * sheetWidth / 2;<br>decimal sumY = 1.0M * sheetWidth * sheetHeight * sheetHeight / 2;<br>decimal weight = 1.0M * sheetWidth * sheetHeight;<br>for (int i = 0; i &lt; x.Length; i++) {<br>  Console.Out.WriteLine(sumX + " " + sumY + " " + weight);<br>  sumX -= 1.0M * width[i] * height[i] * (x[i] + 1.0M * width[i] / 2.0M);<br>  sumY -= 1.0M * width[i] * height[i] * (y[i] + 1.0M * height[i] / 2.0M);<br>  weight -= 1.0M * width[i] * height[i];<br>  Console.Out.WriteLine(sumX + " " + sumY + " " + weight);<br>}<br>if (weight == 0)<br>  return new double[0];<br>return new double[]{(double)(sumX / weight), (double)(sumY / weight)};<br>}<br> <br> <br>}<br></td>