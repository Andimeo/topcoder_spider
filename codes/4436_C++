            #include &lt;algorithm&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cmath&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;string&gt;<br>#include &lt;utility&gt;<br>#include &lt;vector&gt;<br>using namespace std;<br>#define REP(i,n) for(int _n=n, i=0;i&lt;_n;++i)<br>typedef long long LL;<br>template&lt;class T&gt; inline int size(const T&amp;c) { return c.size(); }<br> <br>struct PerforatedSheet {<br> <br>  // MAIN<br>  vector &lt;double&gt; getCenterOfMass(int sheetWidth, int sheetHeight, vector &lt;int&gt; x, vector &lt;int&gt; y, vector &lt;int&gt; width, vector &lt;int&gt; height) {<br>    LL mass = LL(sheetWidth) * LL(sheetHeight);<br>    LL totalX = mass * sheetWidth;<br>    LL totalY = mass * sheetHeight;<br>    REP(i,size(x)) {<br>      LL x1 = x[i];<br>      LL y1 = y[i];<br>      LL x2 = x1 + width[i];<br>      LL y2 = y1 + height[i];<br>      LL m = (x2-x1)*(y2-y1);<br>      mass -= m;<br>      totalX -= (x1+x2)*m;<br>      totalY -= (y1+y2)*m;<br>    }<br>    vector&lt;double&gt; res;<br>    if(mass==0) return res;<br>    res.push_back(double(totalX)/mass/2);<br>    res.push_back(double(totalY)/mass/2);<br>    return res;<br>  }<br> <br>};<br> <br>// Powered by FileEdit<br> <br>// Powered by TomekAI<br>// Powered by TZTester 1.01 [25-Feb-2003]<br> <br>// Powered by CodeProcessor<br></td>