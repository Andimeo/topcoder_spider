            using System;<br><br>public class PolygonDecomposition {<br>  const int MODULO = 1000000000;<br><br>  public int howMany(int n, int k) {<br>    if (k &gt; n - 2)<br>      return -1;<br><br>    long[,] a = new long[n + 1, k + 1];<br>    long[,] b = new long[n + 1, k + 1];<br><br>    for (int i = 3; i &lt;= n; ++i)<br>      for (int j = 1; j &lt;= k; ++j)<br>      {<br>        if (i == 3)<br>        {<br>          if (j == 1)<br>            b[i, j] = 1;<br>          else<br>            b[i, j] = 0;<br>        }<br>        else <br>        {<br>          b[i, j] = (a[i - 1, j - 1] + a[i - 1, j]) % MODULO;<br>        }<br><br>        a[i, j] = b[i, j];<br><br>        for (int l = 2; l &lt;= i - 2; ++l)<br>        {<br>          for (int m = 1; m &lt;= j - 1; ++m) {<br>            a[i, j] = (a[i, j] + b[l + 1, m] * a[i - l + 1, j - m]) % MODULO;<br>          }<br>        }<br>      }<br><br>    return (int) a[n, k];<br>  }<br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>