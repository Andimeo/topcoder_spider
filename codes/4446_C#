<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br> <br>public class Synchro<br>{<br>    static public double resynch(int[] watch)<br>    {<br>        int a, b, c, gcd, i, j, n = watch.Length;<br>        double result, t;<br>        int[] x = new int[n];<br>        Array.Sort(watch);<br>        if (watch[0] == watch[n - 1])<br>            return (0);<br>        for (i = 1; i &lt; n; i++)<br>            x[i] = watch[i] - watch[0];<br>        j = 1;<br>        while (x[j] == 0)<br>            j++;<br>        gcd = x[j];<br>        for (i = j + 1; i &lt; n; i++)<br>        {<br>            a = x[i];<br>            b = gcd;<br>            while (a % b != 0)<br>            {<br>                c = a % b;<br>                a = b;<br>                b = c;<br>            }<br>            gcd = b;<br>            if (gcd == 1) break;<br>        }<br>        t = 60.0 / gcd;<br>        result = watch[0] * t % 60;<br>        return (result);<br>    }<br>}<br></td>