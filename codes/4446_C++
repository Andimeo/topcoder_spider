            #include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;math.h&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;stdio.h&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;stdlib.h&gt;<br>#include &lt;string.h&gt;<br>#include &lt;memory.h&gt;<br>#include &lt;iostream&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;queue&gt;<br> <br>using namespace std;<br> <br>#define vc vector<br>#define pb push_back<br>#define iss istringstream<br>#define oss ostringstream<br>#define PQ priority_queue<br> <br> <br>struct fac{<br>    int a,b;<br>}s[10000];<br>inline int gcd(int a,int b)<br>{<br>    if (b==0) return a;<br>    return gcd(b,a%b);<br>}<br>inline int lcm(int a,int b)<br>{<br>    return a*b/gcd(a,b);<br>}<br>class Synchro<br>{<br>    public:<br>    double resynch(vector &lt;int&gt; w)<br>    {<br>        int n=0;<br>        int i,j,m;<br>        m=w.size();<br>        for (i=0;i&lt;m;i++)<br>        for (j=0;j&lt;m;j++)<br>        if (w[i]!=w[j])<br>        {<br>            n++;<br>            s[n].a=60;<br>            s[n].b=abs(w[i]-w[j]);<br>            int W=gcd(s[n].a,s[n].b);<br>            s[n].a/=W;<br>            s[n].b/=W;<br>        }<br>        if (n==0) return 0.0;<br>        int A=1,B=s[1].b;<br>        for (i=1;i&lt;=n;i++)<br>        {<br>            A=lcm(s[i].a,A);<br>            B=gcd(s[i].b,B);<br>        }<br>        B*=60;<br>        A*=w[1];<br>        int W=gcd(A,B);<br>        A/=W;<br>        B/=W;<br>        A%=B;<br>        printf("%d %d\n",A,B);<br>        return (double)A*60/B;<br>    }<br>};<br></td>