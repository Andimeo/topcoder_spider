            import java.util.*;<br>import java.math.*;<br>import java.io.*;<br><br>import static java.lang.System.*;<br>import static java.lang.Math.*;<br><br>// I'm shifting to Java, inspect it harder to see the fault.<br>public class Synchro {<br>    static int gcd(int a, int b) {<br>        while (b != 0) {<br>            int temp = a % b;<br>            a = b;<br>            b = temp;<br>        }<br>        return a;<br>    }<br><br>    public double resynch(int[] watch) {<br>        List&lt;Integer&gt; list = new ArrayList&lt;Integer&gt;();<br>        for (int i = 0; i &lt; watch.length; ++i) {<br>            for (int j = i + 1; j &lt; watch.length; ++j) {<br>                list.add(abs(watch[i] - watch[j]));<br>            }<br>        }<br>        int g = 0;<br>        for (int each : list) {<br>            g = gcd(each, g);<br>        }<br>        if (g == 0) {<br>            return 0.0;<br>        }<br>        double res = 60.0 / g * watch[0];<br>        while (res &gt;= 60.0) {<br>            res -= 60.0;<br>        }<br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>