            using System;<br> <br>public class MassiveNumbers<br>{<br>  public string getLargest(string numberA, string numberB)<br>  {<br>    string[] aParts = numberA.Split(new char[]{'^'});<br>    string[] bParts = numberB.Split(new char[]{'^'});<br>    <br>    double a = Convert.ToDouble(aParts[1]) * Math.Log(Convert.ToDouble(aParts[0]));<br>    double b = Convert.ToDouble(bParts[1]) * Math.Log(Convert.ToDouble(bParts[0]));<br> <br>    if(a &gt; b)<br>    {<br>      return numberA;<br>    }<br>    return numberB;<br>  }<br>}<br></td>