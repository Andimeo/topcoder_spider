            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;stdio.h&gt;<br>#include &lt;stdlib.h&gt;<br>#include &lt;math.h&gt;<br> <br>template&lt;class T&gt; void prvec(vector&lt;T&gt; v) {<br>  for (int i=0; i&lt;v.size(); i++) {<br>    cout &lt;&lt; v[i] &lt;&lt; ":";<br>  }<br>  cout &lt;&lt; endl;<br>}<br> <br>class Intervals {<br>public:<br>  vector&lt;string&gt; partition(vector&lt;string&gt; intervals) {<br>    int ni = intervals.size();<br>    int mins[ni], maxs[ni];<br>    int intindex[11000];<br>    for (int i=0; i&lt;ni; i++) {<br>      mins[i]=atoi(intervals[i].c_str());<br>      maxs[i]=atoi(intervals[i].substr(intervals[i].find(":",0)+1).c_str());<br>      cout &lt;&lt; intervals[i] &lt;&lt; " " &lt;&lt; mins[i] &lt;&lt; "-&gt;" &lt;&lt; maxs[i] &lt;&lt; endl;<br>    }<br>    for (int i=0; i&lt;10000; i++) {<br>      intindex[i]=0;<br>      for (int j=0; j&lt;ni; j++) {<br>        if (mins[j]&lt;=i &amp;&amp; i&lt;=maxs[j]) intindex[i]|=(1&lt;&lt;j);<br>      }<br>    }<br>    intindex[10000]=0;<br>    int start=0,end=0,cur=0,idex=-1;<br>    char buf[80];<br>    vector&lt;string&gt; ret;<br>    for (int i=0; i&lt;10001; i++) {<br>      if (intindex[i]==idex) end=i;<br>      else {<br>        if (idex&gt;0) {<br>          sprintf(buf,"%d:%d",start,end);<br>          string s = buf;<br>          ret.push_back(s);<br>        }<br>          start = end = i;<br>          idex = intindex[i];<br>      }<br>    }<br>    return ret;<br>    <br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>