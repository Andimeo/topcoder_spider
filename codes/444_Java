            import java.util.*;<br>public class Intervals {<br>  public String[] partition(String[] intervals) {<br>    ArrayList res = new ArrayList();<br>    int id[] = new int[10000];<br>    for (int a = 0; a &lt; intervals.length; a++){<br>      StringTokenizer st = new StringTokenizer (intervals[a], ":");<br>      int lo = Integer.parseInt (st.nextToken());<br>      int hi = Integer.parseInt (st.nextToken());<br>      int val = (1 &lt;&lt; a);<br>      for (int b = lo; b &lt;= hi; b++){<br>        id[b] |= val;<br>      }<br>    }<br>    <br>    int here = 0;<br>    while (true){<br>      if (here &gt;= 10000) break;<br>      while (here &lt; 10000 &amp;&amp; id[here] == 0) here++;<br>      if (here &gt;= 10000) break;<br>      int there = here;<br>      while (there &lt; 10000 &amp;&amp; (id[there] == id[here])) there++;<br>      there--;<br>      res.add ("" + here + ":" + there);<br>      here = there + 1;<br>    }<br>    return (String[]) res.toArray (new String[0]);    <br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>