            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cctype&gt;<br>#include &lt;queue&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;utility&gt;<br>#include &lt;numeric&gt;<br> <br>using namespace std;<br> <br>long double pa[30000], pb[30000];<br>long double cb[30000];<br> <br>class DiceThrows {<br>  public:<br>  double winProbability(int a, vector &lt;int&gt; sa, int b, vector &lt;int&gt; sb) {<br>    <br>    pa[0] = pb[0] = 1;<br>    for (int i = 0; i &lt; a; i++)<br>      for (int j = 20000; j &gt;= 0; j--)<br>        {<br>    for (int k = 0; k &lt; 6; k++)<br>      pa[j+sa[k]] += pa[j] / 6;<br>    pa[j] = 0;<br>        }<br>    for (int i = 0; i &lt; b; i++)<br>      for (int j = 20000; j &gt;= 0; j--)<br>        {<br>    for (int k = 0; k &lt; 6; k++)<br>      pb[j+sb[k]] += pb[j] / 6;<br>    pb[j] = 0;<br>        }<br>    long double cum = 0;<br>    for (int j = 0; j &lt; 25000; j++)<br>      {<br>        cb[j] = cum;<br>        cum += pb[j];<br>      }<br>    long double ans = 0;<br>    for (int j = 0; j &lt; 25000; j++)<br>      ans += pa[j] * cb[j];<br>    return (double) ans;<br>    <br>  }<br>};<br> <br> <br>// Powered by FileEdit<br></td>