            // Powered by radeclipse!<br>import java.util.* ;<br>public class DiceThrows {<br>  double[] calcval(int nthrows, int[] dice, int n) {<br>    double[] r = new double[n] ;<br>    r[0] = 1 ;<br>    for (int i=0; i&lt;nthrows; i++) {<br>      for (int j=n-1; j&gt;=0; j--) {<br>  double s = 0 ;<br>  for (int k=0; k&lt;6; k++) {<br>    if (j - dice[k] &gt;= 0)<br>      s += r[j-dice[k]] ;<br>  }<br>  r[j] = s / 6 ;<br>      }<br>    }<br>    return r ;<br>  }<br>  public double winProbability(int numDiceA, int[] sidesA, int numDiceB, int[] sidesB) {<br>    int n = Math.max(numDiceA, numDiceB) * 100 + 1 ;<br>    double a[] = calcval(numDiceA, sidesA, n) ;<br>    double b[] = calcval(numDiceB, sidesB, n) ;<br>    double s = 0 ;<br>    double r = 0 ;<br>    for (int i=0; i&lt;n; i++) {<br>      r += b[i] * (s + a[i]) ;<br>      s += a[i] ;<br>    }<br>    return 1 - r ;<br>  }<br>}<br></td>