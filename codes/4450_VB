<td class="problemText" colspan="8" valign="middle" align="left">
            Public Class DiceThrows<br>        Public Function winProbability(ByVal numDiceA As Integer, ByVal sidesA As Integer(), ByVal numDiceB As Integer, ByVal sidesB As Integer()) As Double<br>            Dim dATotals(20000, 1) As Double<br>            Dim i, j, k, l As Integer<br>            dATotals(0, 0) = 1<br>            dATotals(0, 1) = 1<br>            Dim iMax As Integer<br>            Dim iNumDice As Integer<br> <br>            For l = 0 To 1<br>                If l = 0 Then<br>                    iNumDice = numDiceA<br>                Else<br>                    iNumDice = numDiceB<br>                End If<br>                For i = 1 To iNumDice<br>                    If l = 0 Then<br>                        For k = 100 * i To 0 Step -1<br>                            dATotals(k, l) = 0<br>                            For j = 0 To 5<br>                                If k &gt;= sidesA(j) Then dATotals(k, l) += dATotals(k - sidesA(j), l) / 6<br>                            Next<br>                        Next<br>                    Else<br>                        For k = 100 * i To 0 Step -1<br>                            dATotals(k, l) = 0<br>                            For j = 0 To 5<br>                                If k &gt;= sidesB(j) Then dATotals(k, l) += dATotals(k - sidesB(j), l) / 6<br>                            Next<br>                        Next<br>                    End If<br>                Next<br>            Next<br> <br>            Dim dTot As Double = 0<br>            Dim dSmallTot As Double<br> <br>            For i = 0 To dATotals.GetUpperBound(0)<br>                dSmallTot = 0<br>                For j = i + 1 To dATotals.GetUpperBound(0)<br>                    dSmallTot += dATotals(j, 0)<br>                Next<br>                dTot += dATotals(i, 1) * dSmallTot<br>            Next<br> <br>            Return dTot<br> <br>        End Function<br>    End Class<br></td>