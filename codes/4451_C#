            using System;<br>  public class NumberSplit<br>  {<br>    public int longestSequence(int start)<br>    {<br>      int[] ar = new int[200000];<br>      int i;<br>      for(i = 0; i &lt; 10; i++)ar[i] = 1;<br>      for(i = 10; i &lt;= start; i++) {<br>        int de1 = 10;<br>        while(i/de1 != 0) {<br>          int c1 = i/de1;<br>          int d1 = i%de1;<br>          ar[i] = Math.Max(ar[i],ar[c1*d1]+1);<br>          int de2 = 10;<br>          while(d1/de2 != 0) <br>          {<br>            int c2 = d1/de2;<br>            int d2 = d1%de2;<br>            ar[i] = Math.Max(ar[i],ar[c1*c2*d2]+1);<br>            int de3 = 10;<br>            while(d2/de3 != 0) <br>            {<br>              int c3 = d2/de3;<br>              int d3 = d2%de3;<br>              ar[i] = Math.Max(ar[i],ar[c1*c2*c3*d3]+1);<br>              int de4 = 10;<br>              while(d3/de4 != 0) <br>              {<br>                int c4 = d3/de4;<br>                int d4 = d3%de4;<br>                ar[i] = Math.Max(ar[i],ar[c1*c2*c3*c4*d4]+1);<br>                de4 *= 10;<br>              }<br>              de3 *= 10;<br>            }<br>            de2 *= 10;<br>          }<br>          de1 *= 10;<br>        }<br>      }<br>      return ar[start];<br>    }<br>  }<br></td>