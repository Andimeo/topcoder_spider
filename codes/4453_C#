            using System;<br>using System.Collections;<br>public class TopFive {<br>  public double findProbability(string[] results, string[] accuracies, int points) {<br>    double[] prob = new double[5];<br>    prob[0] = 1;<br>    for (int i = 1; i &lt; 5; ++i)<br>      prob[i] = 0;<br><br>    for (int id = 0; id &lt; results.Length; ++id)<br>    {<br>      string[] res = results[id].Split(' ');<br>      string[] acc = accuracies[id].Split(' ');<br><br>      int[] r = new int[3];<br>      for (int i = 0; i &lt; 3; ++i)<br>        r[i] = int.Parse(res[i]);<br><br>      int[] a = new int[3];<br>      for (int i = 0; i &lt; 3; ++i)<br>        a[i] = int.Parse(acc[i]);<br><br>      double this_prob = 0;<br>      for (int g0 = 0; g0 &lt;= 1; ++g0)<br>        for (int g1 = 0; g1 &lt;= 1; ++g1)<br>          for (int g2 = 0; g2 &lt;= 1; ++g2)<br>          {<br>            if (g0 * r[0] + g1 * r[1] + g2 * r[2] &gt;= points)<br>              this_prob += (<br>                ((100 - a[0]) + g0 * (2 * a[0] - 100)) * <br>                ((100 - a[1]) + g1 * (2 * a[1] - 100)) * <br>                ((100 - a[2]) + g2 * (2 * a[2] - 100)) + <br>                0.0) / 1000000.0;<br>          }<br><br>      for (int i = 4; i &gt;= 0; --i)<br>      {<br>        if (i == 0)<br>          prob[i] = (1 - this_prob) * prob[i];<br>        else<br>          prob[i] = this_prob * prob[i - 1] + (1 - this_prob) * prob[i];<br>      }<br>    }<br><br>    double ret = 0;<br>    for (int i = 0; i &lt; 5; ++i)<br>      ret += prob[i];<br><br>    return ret;<br>}<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>