            // Powered by radeclipse!<br>import java.util.* ;<br>public class TopFive {<br>int[] s2ia(String s) {<br>  StringTokenizer t=new StringTokenizer(s.trim());<br>  int[]r=new int[t.countTokens()];<br>  int i=0;<br>  while(t.hasMoreTokens())try{<br>     r[i++]=Integer.parseInt((String)t.nextToken());<br>  }catch(Exception e){};<br>  return r;<br>}<br>  public double findProbability(String[] results, String[] accuracies, int points) {<br>    double[] above = new double[7] ;<br>    above[0] = 1 ;<br>    for (int i=0; i&lt;results.length; i++) {<br>      double wp = 0 ;<br>      int[] p = s2ia(results[i]) ;<br>      int[] a = s2ia(accuracies[i]) ;<br>      for (int b=0; b&lt;8; b++) {<br>  int hp = 0 ;<br>  double tp = 1.0 ;<br>  for (int bb=0; bb&lt;3; bb++) {<br>    if (0 != (b &amp; (1 &lt;&lt; bb))) {<br>      hp += p[bb] ;<br>      tp *= a[bb]/100.0 ;<br>    } else {<br>      tp *= (100-a[bb]) / 100.0 ;<br>    }<br>  }<br>  if (hp &gt;= points) {<br>    wp += tp ;<br>  }<br>      }<br>      for (int j=6; j&gt;0; j--) {<br>  above[j] = above[j-1] * wp + above[j] * (1-wp) ;<br>      }<br>      above[0] = above[0] * (1 - wp) ;<br>    }<br>    double s = 0.0 ;<br>    for (int i=0; i&lt;=4; i++) {<br>      s += above[i] ;<br>    }<br>    return s ;<br>  }<br>}<br></td>