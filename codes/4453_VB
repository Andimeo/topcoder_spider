            Public Class TopFive<br>        Public Function findProbability(ByVal results As String(), ByVal accuracies As String(), ByVal points As Integer) As Double<br>            If results.GetUpperBound(0) &lt; 4 Then Return 1<br>            Dim dBeatOdds(results.GetUpperBound(0)) As Double<br>            Dim i, j, k, l, m As Integer<br>            Dim sRes() As String<br>            Dim sAcc() As String<br>            Dim iScore As Integer<br>            Dim dProb As Double<br>            Dim d As Double<br> <br>            For i = 0 To results.GetUpperBound(0)<br>                sRes = results(i).Split(" ")<br>                sAcc = accuracies(i).Split(" ")<br>                For j = 0 To 1<br>                    For k = 0 To 1<br>                        For l = 0 To 1<br>                            iScore = 0<br>                            dProb = 1<br>                            If j = 1 Then dProb *= CDbl(sAcc(0)) / 100<br>                            If j = 0 Then dProb *= 1 - (CDbl(sAcc(0)) / 100)<br>                            If k = 1 Then dProb *= CDbl(sAcc(1)) / 100<br>                            If k = 0 Then dProb *= 1 - (CDbl(sAcc(1)) / 100)<br>                            If l = 1 Then dProb *= CDbl(sAcc(2)) / 100<br>                            If l = 0 Then dProb *= 1 - (CDbl(sAcc(2)) / 100)<br>                            If j = 1 Then iScore += CInt(sRes(0))<br>                            If k = 1 Then iScore += CInt(sRes(1))<br>                            If l = 1 Then iScore += CInt(sRes(2))<br>                            If iScore &gt;= points Then<br>                                dBeatOdds(i) += dProb<br>                            End If<br>                        Next<br>                    Next<br>                Next<br>            Next<br> <br>            dProb = 0<br>            For i = 0 To results.GetUpperBound(0) - 3<br>                For j = i + 1 To results.GetUpperBound(0) - 2<br>                    For k = j + 1 To results.GetUpperBound(0) - 1<br>                        For l = k + 1 To results.GetUpperBound(0)<br>                            d = 1<br>                            For m = 0 To results.GetUpperBound(0)<br>                                If m &lt;&gt; i And m &lt;&gt; j And m &lt;&gt; k And m &lt;&gt; l Then<br>                                    d *= (1 - dBeatOdds(m))<br>                                Else<br>                                    d *= dBeatOdds(m)<br>                                End If<br>                            Next<br>                            dProb += d<br>                        Next<br>                    Next<br>                Next<br>            Next<br> <br>            For j = 0 To results.GetUpperBound(0) - 2<br>                For k = j + 1 To results.GetUpperBound(0) - 1<br>                    For l = k + 1 To results.GetUpperBound(0)<br>                        d = 1<br>                        For m = 0 To results.GetUpperBound(0)<br>                            If m &lt;&gt; j And m &lt;&gt; k And m &lt;&gt; l Then<br>                                d *= (1 - dBeatOdds(m))<br>                            Else<br>                                d *= dBeatOdds(m)<br>                            End If<br>                        Next<br>                        dProb += d<br>                    Next<br>                Next<br>            Next<br> <br>            For k = 0 To results.GetUpperBound(0) - 1<br>                For l = k + 1 To results.GetUpperBound(0)<br>                    d = 1<br>                    For m = 0 To results.GetUpperBound(0)<br>                        If m &lt;&gt; k And m &lt;&gt; l Then<br>                            d *= (1 - dBeatOdds(m))<br>                        Else<br>                            d *= dBeatOdds(m)<br>                        End If<br>                    Next<br>                    dProb += d<br>                Next<br>            Next<br> <br>            For l = 0 To results.GetUpperBound(0)<br>                d = 1<br>                For m = 0 To results.GetUpperBound(0)<br>                    If m &lt;&gt; l Then<br>                        d *= (1 - dBeatOdds(m))<br>                    Else<br>                        d *= dBeatOdds(m)<br>                    End If<br>                Next<br>                dProb += d<br>            Next<br> <br>            d = 1<br>            For m = 0 To results.GetUpperBound(0)<br>                d *= (1 - dBeatOdds(m))<br>            Next<br>            dProb += d<br> <br>            Return dProb<br>        End Function<br>    End Class<br></td>