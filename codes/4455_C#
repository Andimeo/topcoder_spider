            using System;<br>using System.Collections;<br>public class TravellingByTrain {<br>  public string earliestArrival(string[] timeTable) {<br>    string at="09:00";<br>    int day=1;<br>    for (int i=0; i&lt;timeTable.Length; ++i)<br>    {<br>      string tt=timeTable[i];<br>      string[] times=tt.Split(' ');<br>      string nat="00:00";<br>      int nday=1000;<br>      for (int j=0; j&lt;times.Length; ++j)<br>      {<br>        string t=times[j];<br>        string[] a=t.Split('-');<br>        string nnat=a[1];<br>        int nnday=day;<br>        if (a[0].CompareTo(at)&lt;0) ++nnday;<br>        if (a[0].CompareTo(a[1])&gt;=0) ++nnday;<br>        if (nnday&lt;nday || ((nnday==nday) &amp;&amp; (nnat.CompareTo(nat)&lt;0)))<br>        {<br>          nday=nnday;<br>          nat=nnat;<br>        }<br>      }<br>      at=nat;<br>      day=nday;<br>    }<br>    return at+", day "+day;<br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>