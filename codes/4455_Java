            import java.util.*;<br>import java.util.regex.*;<br> <br>public class TravellingByTrain {<br>    int N;<br>    public String earliestArrival(String[] timeTable) { <br>  N = timeTable.length;<br>  int t = 9*60;<br>  for (int i=0; i&lt;N; i++) {<br>      String [] sa = timeTable[i].split(" +");<br>      int tid = t % 1440;<br>      int d = t/1440;<br>      int mint = 1000000000;<br>      for (int j=0; j &lt; sa.length; j++) {<br>    int s = Integer.parseInt( sa[j].substring(0, 2) ) * 60 + Integer.parseInt( sa[j].substring(3, 5) );<br>    int e = Integer.parseInt( sa[j].substring(6, 8) ) * 60 + Integer.parseInt( sa[j].substring(9, 11) );<br>    if (e &lt;= s) {<br>        e += 1440;<br>    }<br>    int tmpt;<br>    if (s &gt;= tid) {<br>        tmpt = d*1440+e;<br>    } else {<br>        tmpt = d*1440+e+1440;<br>    }<br>    mint = Math.min(mint, tmpt);<br>      }<br>      t = mint;<br>  }<br>    <br>  int d = t/1440;<br>  int h = (t-d*1440)/60;<br>  int m = t%60;<br>  <br>        return i2s(h, 2) + ":" + i2s(m, 2) + ", day " + (d+1);<br>    }<br> <br>    public String i2s(int i, int minWidth) {<br>  return i2s(i, minWidth, '0');<br>    }<br> <br>    public String i2s(int i, int minWidth, char padding) {<br>  String s = i + "";<br>  for (int j=minWidth - s.length(); j&gt;0; j--) {<br>      s = padding + s;<br>  }<br>  return s;<br>    }<br> <br>}<br> <br>// Powered by CodeProcessor<br></td>