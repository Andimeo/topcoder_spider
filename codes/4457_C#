            using System;<br>using System.Collections;<br>public class MailArchiving {<br>  public int minSelections(string[] destFolders) {<br>    int n=destFolders.Length;<br>    Hashtable t = new Hashtable();<br>    int m=0;<br>    for (int i=0; i&lt;n; ++i)<br>    {<br>      if (!t.ContainsKey(destFolders[i]))<br>        t[destFolders[i]]=m++;<br>    }<br>    int[] dest=new int[n];<br>    for (int i=0; i&lt;n; ++i)<br>      dest[i]=(int) t[destFolders[i]];<br>    int[][] best=new int[n][];<br>    for (int i=0; i&lt;n; ++i)<br>      best[i]=new int[n];<br>    for (int len=1; len&lt;=n; ++len)<br>    {<br>      for (int i=0; i&lt;=n-len; ++i)<br>      {<br>        int j=i+len-1;<br>        int b=len;<br>        for (int k=i; k&lt;j; ++k)<br>          if (best[i][k]+best[k+1][j]&lt;b)<br>            b=best[i][k]+best[k+1][j];<br>        if (dest[i]==dest[j] &amp;&amp; (len&gt;1))<br>        {<br>          int what=dest[i];<br>          for (int l=j; l&gt;i &amp;&amp; dest[l]==what; --l)<br>          {<br>            for (int s=l-1; s&gt;=i; --s)<br>              if (dest[s]==what)<br>              {<br>                int aa;<br>                if (s==l-1)<br>                  aa=0;<br>                else<br>                  aa=best[s+1][l-1];<br>                if (best[i][s]+aa&lt;b)<br>                  b=best[i][s]+aa;<br>              }<br>          }<br>        }<br>        best[i][j]=b;<br>      }<br>    }<br>    return best[0][n-1];<br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>