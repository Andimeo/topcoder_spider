<td class="problemText" colspan="8" valign="middle" align="left">
            #include &lt;vector&gt;<br>#include &lt;string&gt;<br> <br>using namespace std;<br> <br>#define FOR(i,a,b) for(i=a; i&lt;=b; i++)<br>#define REP(i,n) for(i=0; i&lt;n; i++)<br>#define FIL(a) memset(&amp;a,0,sizeof(a))<br>#define VI vector&lt;int&gt;<br>#define VS vector&lt;string&gt;<br> <br>struct MailArchiving<br>{<br>  int n;<br>  bool eq[50][50];<br>  int dp[50][50];<br>  <br>  int minSelections(VS dest)<br>  {<br>    int i,j,k,p,t;<br>    n=dest.size();<br>    REP(i,n) REP(j,n) eq[i][j]=dest[i]==dest[j];<br>    REP(i,n) dp[i][i]=1;<br>    FOR(k,2,n)<br>    {<br>      i=0; j=k-1;<br>      while(j&lt;n)<br>      {<br>        dp[i][j]=1+dp[i+1][j];<br>        FOR(p,i+1,j) if(eq[i][p])<br>        {<br>          t=dp[p][j];<br>          if(p-1&gt;=i+1) t+=dp[i+1][p-1];<br>          dp[i][j]&lt;?=t;<br>        }<br>        ++i; ++j;<br>      }<br>    }<br>    return dp[0][n-1];<br>  }<br>};<br></td>