            using System; using System.Collections; using System.Text; using System.Text.RegularExpressions;<br>public class HammingNumbers<br>   {<br>   public int getNumber(int[] factors, int n)<br>      {<br>      int F = factors.Length;<br>      long[] X = new long[n+n+500];<br>      X[0] = 1;<br>      int[] pos = new int[F];<br>      for (int i=1; i &lt;= n-1; i++)<br>         {<br>         long min=long.MaxValue; int minKey=-1; for (int j=0; j&lt;F; j++) { {long cur2 = factors[j] * X[pos[j]]; if (cur2 &lt; min) { min = cur2; minKey = j; }} }<br>         int k=minKey;<br>         X[i] = factors[k] * X[pos[k]];<br>         long cur = X[i];<br>         if (cur &gt; 2147483647)<br>            return -1;<br>         for (int w=0; w &lt; F; w++)<br>            while (factors[w] * X[pos[w]] &lt;= cur)<br>               pos[w]++;<br>         }<br>      return (int)X[n-1];<br>      }<br><br><br>} // TopC#der<br><br><br>// Powered by FileEdit<br></td>