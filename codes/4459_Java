            import java.util.*;<br>public class HammingNumbers {<br>  public int getNumber(int[] factors, int n) {<br>    int m=factors.length;<br>    long[] res = new long[n];<br>    res[0] = 1;<br>    int[] index =new int[m];<br>    for (int i=1;i&lt;n;i++) {<br>      long min = 2147483648L;<br>      int minj=-1;<br>      for (int j=0;j&lt;factors.length;j++) {<br>        if (factors[j]*res[index[j]]&lt;min) {<br>          min = factors[j]*res[index[j]];<br>          minj=j;<br>        }<br>      }<br>      if (minj&lt;0) return -1;<br>      else {<br>        res[i]=min;<br>        for (int j=0;j&lt;factors.length;j++) {<br>          if (factors[j]*res[index[j]]==min) index[j]++;<br>        }<br>        <br>      }<br>    }<br>    return (int)res[n-1];<br>  }<br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>