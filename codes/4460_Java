            import java.util.*;<br>public class ComputerExpert<br>{<br>    public String[] operate(String[] facts, String[] rules)<br>    {<br>        Set factSet = new TreeSet();<br>        for (int i = 0; i &lt; facts.length; i++)<br>        {<br>            factSet.add(facts[i]);<br>        }<br> <br>        for (int m = 0; m &lt; rules.length; m++)<br>        {<br>            for (int i = 0; i &lt; rules.length; i++)<br>            {<br>                String[] beforeAfter = rules[i].split(" -&gt; ");<br>                String before = beforeAfter[0];<br>                String after = beforeAfter[1];<br> <br>                boolean ANDS_true = true;<br>                ;<br>                String[] ANDS = before.split(",");<br>                for (int j = 0; j &lt; ANDS.length; j++)<br>                {<br>                    boolean ORS_true = false;<br>                    String[] ORS = ANDS[j].split("/");<br>                    for (int k = 0; k &lt; ORS.length; k++)<br>                    {<br>                        if (factSet.contains(ORS[k]))<br>                        {<br>                            //this OR TRUE<br>                            ORS_true = true;<br>                            break;<br>                        }<br>                    }<br>                    if (!ORS_true)<br>                    {<br>                        ANDS_true = false;<br>                        break;<br>                    }<br>                }<br>                if (ANDS_true)<br>                {<br>                    factSet.add(after);<br>                }<br>            }<br>        }<br> <br>        for (int i = 0; i &lt; facts.length; i++)<br>        {<br>            factSet.remove(facts[i]);<br>        }<br> <br>        return (String[])factSet.toArray(new String[] {<br>        });<br> <br>    }<br> <br>}<br></td>