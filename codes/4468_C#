            using System;<br>using System.Collections;<br> <br>public class CardRemover<br>{<br>  bool poss(string a, string b)<br>  {<br>    int i, j;<br>    <br>    int x=0;<br>    <br>    for(i=0; i&lt;3; i++)<br>      for(j=0; j&lt;3; j++)<br>        if(a[i]==b[j])<br>          x++;<br>    <br>    if(x&gt;=2)<br>      return true;<br>    return false;<br>  }<br>  <br>  public int calculate(string[] cards)<br>  {<br>    int i,j,k,l;<br>    l=cards.Length;<br> <br>    int[,] rm=new int[l,l];<br>    <br>    <br>    for(i=0; i&lt;l; i++)<br>      rm[i,i]=0;<br>    <br>    for(i=0; i&lt;l-1; i++)<br>      rm[i,i+1]=0;<br>    <br>    for(i=2; i&lt;l; i++)<br>    {<br>      for(j=0; j+i&lt;l; j++)<br>      {<br>        int max=0;<br>        <br>        bool pos=poss(cards[j], cards[j+i]);<br> <br>        for(k=j+1; k&lt;=j+i-1; k++)<br>        {<br>          if(pos &amp;&amp; rm[j,k]==k-j-1 &amp;&amp; rm[k,j+i]==j+i-k-1 &amp;&amp; rm[j,k]+rm[k,j+i]+1&gt;max)<br>            max=rm[j,k]+rm[k,j+i]+1;<br>           else<br>          if(rm[j,k]+rm[k,j+i]&gt;max)<br>            max=rm[j,k]+rm[k,j+i];<br>        }<br>        <br>        rm[j,j+i]=max;<br>      }<br>    }<br>    <br>    return rm[0,l-1];<br>  }<br>}<br> <br> <br>// Powered by FileEdit<br></td>