            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class CalcTest {<br>    public string[] uniform(string[] numbers) {<br>        string[] res = new string[numbers.Length];<br>        for (int i = 0; i &lt; numbers.Length; i++)<br>        {<br>            res[i] = "";<br>            for (int j = 0; j &lt; numbers[i].Length; j++)<br>            {<br>                if (Char.IsDigit(numbers[i][j]))<br>                {<br>                    res[i] += numbers[i][j];<br>                }<br>                else if (numbers[i][j] != ' ')<br>                {<br>                    res[i] += ".";<br>                }<br>            }<br>        }<br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>