            #line 109 "CalcTest.cpp"<br>#include &lt;algorithm&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br><br>using namespace std;<br><br>/* ------------------------------------------------------ */<br>/* ------------ PROBLEM SOLUTION BEGINS HERE ------------ */<br>/* ------------------------------------------------------ */<br><br>class CalcTest<br>{<br>public: <br> vector &lt;string&gt; uniform(vector &lt;string&gt; numbers) <br> { <br>    vector &lt;string&gt; ret;<br>    for (int i=0;i&lt;numbers.size();i++)<br>    {<br>  string tmp = "";<br>  for (int j=0;j&lt;numbers[i].size();j++)<br>      if (numbers[i][j]== ' ') <br>    ;<br>      else if (!isdigit(numbers[i][j])) tmp += '.';<br>      else tmp += numbers[i][j];<br>  ret.push_back(tmp);<br>    }  <br>    return ret;<br> } <br> <br> <br>}; <br><br><br><br><br><br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>