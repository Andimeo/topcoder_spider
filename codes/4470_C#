            using System;<br>using System.Collections;<br>public class PiCalculator {<br>  const string PI = "3.1415926535897932384626433832790";<br><br>  public string calculate(int precision) {<br>    string head = PI.Substring(0, precision+2);<br>    string tail = PI.Substring(precision+2);<br>    if (tail[0] &gt;= '5')<br>    {<br>      int i;<br>      for (i = head.Length-1; head[i] == '9'; --i)<br>        head = head.Substring(0, i) + '0' + head.Substring(i+1);<br>      head = head.Substring(0, i) + (char) (head[i]+1) + head.Substring(i+1);<br>    }<br>    return head;<br>  }<br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>