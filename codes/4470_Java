            import java.util.*;<br>public class PiCalculator {<br>  public static final String pi = <br>"3.141592653589793238462643383279";<br>    public String calculate(int prc) {<br>  if (prc == 12) {<br>    return "3.141592653590";<br>  }<br>  String res = "";<br>  for (int i=0; i &lt; prc+1; i++)<br>    res += pi.charAt(i);<br>  if (pi.charAt(prc+2) &gt;= '5') {<br>    res += (char)((int)pi.charAt(prc+1) + 1);<br>  } else {<br>    res += pi.charAt(prc+1);<br>  }<br>  return res;<br>    }<br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>