            using System;<br><br>public class UnsealTheSafe {<br><br>long [,]mem = new long[16,64];<br>bool [,] was = new bool[16,64];<br>long rec(int d, int n) {<br>if(n == 1) return 1;<br>if(was[d,n]) return mem[d,n];<br>long ret = 0;<br>if(d == 1) { ret = rec(2, n-1) + rec(4, n-1);}<br>if(d == 2) { ret = rec(1, n-1) + rec(3, n-1) + rec(5, n-1); }<br>if(d == 3) { ret = rec(2, n-1) + rec(6, n-1);}<br>if(d == 4) { ret = rec(1, n-1) + rec(5, n-1) + rec(7,n-1); }<br>if(d == 5) { ret = rec(2, n-1) + rec(4, n-1) + rec(6, n-1) + rec(8,n-1); }<br>if(d == 6) { ret = rec(3, n-1) + rec(5, n-1) + rec(9,n-1); }<br>if(d == 7) { ret = rec(4, n-1) + rec(8, n-1) + rec(0, n-1); }<br>if(d == 8) { ret = rec(5, n-1) + rec(7, n-1) + rec(9,n-1); }<br>if(d == 9) { ret = rec(6, n-1) + rec(8, n-1);}<br>if(d == 0) { ret = rec(7, n-1); }<br>was[d,n] = true;<br>mem[d,n]= ret;<br>return ret;<br>}<br>public long countPasswords(int n) {<br>long ret = 0;<br>for(int i = 0; i &lt; 10; i++) ret += rec(i, n);<br>return ret;<br>}<br><br>}<br></td>