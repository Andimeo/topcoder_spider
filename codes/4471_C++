            #include &lt;cstdio&gt;<br>class UnsealTheSafe{<br>public:<br>  long long countPasswords(int N){<br>    long long d[50][20];<br>    int i;<br>    for(i=0; i&lt;=9; i++){<br>      d[1][i]=1;<br>    }<br>    for(i=2; i&lt;=N; i++){<br>      d[i][0]=d[i-1][7];<br>      d[i][1]=d[i-1][2]+d[i-1][4];<br>      d[i][2]=d[i-1][1]+d[i-1][3]+d[i-1][5];<br>      d[i][3]=d[i-1][2]+d[i-1][6];<br>      d[i][4]=d[i-1][1]+d[i-1][5]+d[i-1][7];<br>      d[i][5]=d[i-1][2]+d[i-1][4]+d[i-1][6]+d[i-1][8];<br>      d[i][6]=d[i-1][3]+d[i-1][5]+d[i-1][9];<br>      d[i][7]=d[i-1][4]+d[i-1][8]+d[i-1][0];<br>      d[i][8]=d[i-1][5]+d[i-1][7]+d[i-1][9];<br>      d[i][9]=d[i-1][6]+d[i-1][8];<br>      d[i][0]=d[i-1][7];<br>    }<br>    long long ans=0;<br>    for(i=0; i&lt;=9; i++){<br>      ans+=d[N][i];<br>    }<br>    return ans;<br>  }<br>};<br></td>