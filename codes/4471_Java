            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class UnsealTheSafe<br>{<br>  public long countPasswords(int N)<br>  {<br>  boolean a[][]=new boolean[11][11];<br>  a[1][4]=true;<br>  a[1][2]=true;<br>  a[2][5]=true;<br>  a[2][1]=true;<br>  a[2][3]=true;<br>  a[3][2]=true;<br>  a[3][6]=true;<br>  a[4][7]=true;<br>  a[4][1]=true;<br>  a[4][5]=true;<br>  a[5][4]=true;<br>  a[5][6]=true;<br>  a[5][2]=true;<br>  a[5][8]=true;<br>  a[6][3]=true;<br>  a[6][5]=true;<br>  a[6][9]=true;<br>  a[7][4]=true;<br>  a[7][8]=true;<br>  a[8][7]=true;<br>  a[8][9]=true;<br>  a[8][5]=true;<br>  a[9][6]=true;<br>  a[9][8]=true;<br>  a[0][7]=true;<br>  a[7][0]=true;<br>  long res[][]=new long[31][11];<br>  for (int i=0;i&lt;10;i++){<br>    res[1][i]=1;<br>  }<br>  <br>  for (int i=2;i&lt;=N;i++){<br>    for (int j=0;j&lt;10;j++)for (int k=0;k&lt;10;k++){<br>      if (a[k][j])res[i][j]+=res[i-1][k];    <br>    }  <br>  }<br>  long Res=0;<br>  for (int i=0;i&lt;10;i++)Res+=res[N][i];<br>  return Res;<br>  <br>  <br>    <br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>