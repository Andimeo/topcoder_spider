            import java.util.*;<br>public class CalcButton {<br>    public String getDigits(String[] sequence) {<br>  String s = "";<br>  for (int i=0; i &lt; sequence.length; i++)<br>    s += sequence[i];<br>  char ba='0';<br>  char bb='0';<br>  char bc='0';<br>  int n = s.length();<br>  if (n &lt; 3) return "000";<br>  int[] f = new int[n];<br>  int best = n+1;<br>  for (char a='0'; a &lt;= '9'; a++) {<br>    for (char b='0'; b &lt;= '9'; b++) {<br>      for (char c='0'; c &lt;= '9'; c++) {<br>        f[0] = 1;<br>        f[1] = 2;<br>        f[2] = 3;<br>        if ((s.charAt(2) == c) &amp;&amp; (s.charAt(1) == b) &amp;&amp; (s.charAt(0) == a)) f[2] = 1;<br>        for (int i=3; i &lt; n; i++) {<br>          f[i] = f[i-1] + 1;<br>          if ((s.charAt(i) == c) &amp;&amp; (s.charAt(i-1) == b) &amp;&amp; (s.charAt(i-2) == a) &amp;&amp; (f[i] &gt; f[i-3] + 1)) {<br>            f[i] = f[i-3] + 1;<br>          }<br>        }<br>        if (f[n-1] &lt; best) {<br>          best = f[n-1];<br>          ba = a; bb = b; bc = c;<br>        }<br>      }<br>    }<br>  }<br>  return Character.toString(ba)+bb+bc;<br>    }<br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>