            Public Class CalcButton<br>        Public Function getDigits(ByVal sequence As String()) As String<br>            Dim sBig As String = ""<br>            Dim i As Integer<br>            For i = 0 To sequence.GetUpperBound(0)<br>                sBig = sBig &amp; sequence(i)<br>            Next<br> <br>            Dim iBestCount As Integer<br>            Dim sBestVal As String = "000"<br>            Dim sSeq As String<br>            Dim j As Integer<br>            Dim iCount As Integer<br> <br>            For i = 0 To 999<br>                'If i = 777 Then Stop<br>                sSeq = Microsoft.VisualBasic.Format(i, "000")<br>                iCount = 0<br> <br>                j = 0<br>                Do While j &lt;= sBig.Length - 3<br>                    If sBig.Substring(j, 3) = sSeq Then<br>                        iCount += 1<br>                        j += 3<br>                    Else<br>                        j += 1<br>                    End If<br>                Loop<br>                If iCount &gt; iBestCount Then<br>                    sBestVal = sSeq<br>                    iBestCount = iCount<br>                End If<br> <br>            Next<br>            Return sBestVal<br>        End Function<br>    End Class<br></td>