            #line 54 "PluCodeGenerator.cc"<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;sstream&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;iostream&gt;<br>#include &lt;cmath&gt;<br>#include &lt;ctype.h&gt;<br>using namespace std;<br> <br>class PluCodeGenerator {<br>public:<br>  int isInValid(int N){<br>    vector&lt;int&gt; v;<br>    while (N!=0) {<br>      v.push_back(N%10);<br>      N/=10;<br>    }<br>    for (int i=0; i+2&lt;v.size(); i++){<br>      if (v[i] == v[i+1] &amp;&amp; v[i] == v[i+2]) return 1;<br>    }<br>    return 0;<br>  }<br>  int invalidInThousand(int N){<br>    vector&lt;int&gt; v;<br>    while (N!=0) {<br>      v.push_back(N%10);<br>      N/=10;<br>    }<br>    for (int i=0; i+2&lt;v.size(); i++){<br>      if (v[i] == v[i+1] &amp;&amp; v[i] == v[i+2]) return 1000;<br>    }<br>    if (v.size() == 0) return 9;<br>    if (v.size() &gt;= 2 &amp;&amp;  v[0] == v[1]) return 109;<br>    return 19;<br>  }<br>  int countInvalidCodes(int N) {<br>    int ret = 0;<br>    int j;<br>    for (j=0; j&lt;N/1000; j++) {<br>      ret += invalidInThousand(j);<br>    }<br>    for (int i=j*1000; i&lt;N; i++) ret+= isInValid(i);<br>    return ret;<br>  }<br>};<br> <br> <br>// Powered by FileEdit<br></td>