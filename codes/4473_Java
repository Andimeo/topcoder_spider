            import java.util.*;<br>import java.util.regex.*;<br> <br>public class PluCodeGenerator {<br>    char [] ca;<br>    int N;<br>    public int countInvalidCodes(int n) {<br>  if (n &lt; 110)<br>      return 0;<br>  n--;<br>  ca = (n+"").toCharArray();<br>  N = ca.length;<br>  for (int i=0; i &lt; N; i++)<br>      ca[i] -= '0';<br>  int rv = 0;<br>  for (int j=0; j &lt; N; j++) {<br>      int lim;<br>      if (j == 0) {<br>    for (int k=1; k &lt;=ca[0] ; k++) {<br>        rv += countit(0, k, 1, k &lt; ca[0]);<br>    }<br>      } else {<br>    for (int k=1; k &lt; 10; k++) {<br>        rv += countit(0, k, j+1, true);<br>    }<br>      }<br>  }<br> <br>  return rv;<br>    }<br> <br>    int [][][][] memo = new int[11][11][50][2];<br> <br>    int countit(int p2, int p1, int pos, boolean countany) {<br>  if (pos == N)<br>      return 0;<br> <br>  if (memo[p2][p1][pos][countany? 0 : 1] &gt; 0)<br>      return memo[p2][p1][pos][countany? 0 : 1]-1;<br> <br>  int rv = 0;<br>  int lim = (countany ? 9 : ca[pos]);<br>  for (int i=0; i &lt;=lim ; i++) {<br>      if (p1 == p2 &amp;&amp; i == p1) {<br>    if (N-1-pos == 0 || i &lt; lim || countany) {<br>        rv += pow10(N-1-pos);<br>    } else {<br>        int toa = 0;<br>        for (int k=pos+1; k &lt; N; k++) {<br>      toa = toa * 10 + ca[k];<br>        }<br>        rv += toa + 1;<br>    }<br>      } else {<br>    rv += countit(p1, i, pos+1, countany || (i &lt; lim));<br>      }<br>  }<br>  memo[p2][p1][pos][countany? 0 : 1] = rv+1;<br>  return rv;<br>    }<br> <br>    int pow10(int k) {<br>  int rv = 1;<br>  for (int i=0; i &lt; k; i++) rv *= 10;<br>  return rv;<br>    }<br> <br>}<br> <br>// Powered by CodeProcessor<br></td>