         aMem(499) = 61144991<br>            aMem(500) = 62190730<br> <br>            Dim iStart As Integer = (N - 1) \ 2000000<br> <br>            Dim i, j, iLast, iTwo As Integer<br>            Dim bInvalid As Boolean<br>            Dim s As String<br>            Dim cLast As Char<br>            Dim c() As Char<br>            For i = iStart * 2000000 + 1 To N - 1<br>                bInvalid = False<br>                s = CStr(i)<br>                c = s.ToCharArray<br>                cLast = "a"<br>                iTwo = 0<br>                For j = 0 To c.GetUpperBound(0)<br>                    If cLast = c(j) Then<br>                        If iTwo = 1 Then bInvalid = True<br>                        iTwo = 1<br>                    Else<br>                        iTwo = 0<br>                        cLast = c(j)<br>                    End If<br>                Next<br>                If bInvalid Then iCount += 1<br>            Next<br> <br>            Return iCount + aMem(iStart)<br> <br>        End Function<br>    End Class<br></td>