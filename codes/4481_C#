            using System;<br>using System.Collections;<br> <br> <br> <br>public class DivisibilityCriteria<br>{<br>   static int[] res;<br> <br>   public static void Main(string[] a)<br>   {<br>      int[] r = multipliers(6,7);<br> <br>   }<br>   public static int[] multipliers(int N, int P)<br>   {<br>      res= new int[N];<br>      res[N-1]=1;<br>      long testN=1;<br> <br>      for (int i=N-2;i&gt;=0;i--)<br>      {<br>         int tmp=sumF();<br>         testN = 10*testN+1;<br>         res[i]=(int)((testN-tmp)%P);<br>      }<br>      return res;<br>   }<br>   private static int sumF()<br>   {<br>      int r=0;<br>      for(int i=0;i&lt;res.Length;i++)<br>      {<br>         r+=res[i];<br>      }<br>      return r;<br>   }<br>}<br></td>