            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;sstream&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;cmath&gt;<br>using namespace std;<br> <br>class DivisibilityCriteria {<br>  public:<br>  vector &lt;int&gt; multipliers(int N, int P) {<br>    vector&lt;int&gt; ret,ret2;<br>    ret.push_back(1);<br>    int cislo=1;<br>    for (int i=1;i&lt;N;i++) {<br>      cislo*=10;<br>      cislo%=P;<br>      ret.push_back(cislo);<br>    }<br>    for (int i=N-1;i&gt;=0;i--) {<br>      ret2.push_back(ret[i]);  <br>    }<br>    return ret2;<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>