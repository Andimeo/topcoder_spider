            using System; using System.Collections; using System.Text; using System.Text.RegularExpressions;<br>public class WordCompositionGame<br>   {<br>   public string score(string[] A, string[] B, string[] C)<br>      {<br>      return String.Format("{0}/{1}/{2}", F(A,B,C), F(B,C,A), F(C,A,B));<br>      }<br>   int F(String[] A, String[] B, String[] C)<br>      {<br>      int tot=0;<br>      foreach (String s in A)<br>         {<br>         tot += 3;<br>         if (Array.IndexOf(B, s) &gt;= 0) tot--;<br>         if (Array.IndexOf(C, s) &gt;= 0) tot--;<br>         }<br>      return tot;<br>      }<br><br><br>} // TopC#der<br><br><br>// Powered by FileEdit<br></td>