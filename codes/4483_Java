            // Author:  King Larry Mak<br>// TC Handle:  Larry<br>import java.util.*;<br>public class WordCompositionGame {<br>  int score( String k, String[] b, String[] c ) {<br>    int found = 3;<br>    for ( int i = 0; i &lt; b.length; i++ ) {<br>      if ( b[i].equals( k ) ) found--;<br>    }<br>    for ( int i = 0; i &lt; c.length; i++ ) {<br>      if ( c[i].equals( k ) ) found--;<br>    }<br>    return found;<br>  }<br>  <br>  public String score(String[] listA, String[] listB, String[] listC) {<br>    int a = 0, b = 0, c = 0;<br>    <br>    for ( int i = 0; i &lt; listA.length; i++ ) {<br>      a += score( listA[i], listB, listC );<br>    }<br>    for ( int i = 0; i &lt; listB.length; i++ ) {<br>      b += score( listB[i], listA, listC );<br>    }<br>    for ( int i = 0; i &lt; listC.length; i++ ) {<br>      c += score( listC[i], listB, listA );<br>    }<br>    <br>    return a + "/" + b + "/" + c;<br>  }<br>  <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>