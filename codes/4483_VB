            Public Class WordCompositionGame<br>        Public Function score(ByVal listA As String(), ByVal listB As String(), ByVal listC As String()) As String<br>            Dim oHash As New System.Collections.Hashtable<br>            Dim i, j, k As Integer<br>            Dim iScore(2) As Integer<br>            Dim sList() As String<br> <br>            For i = 0 To 2<br>                If i = 0 Then sList = listA<br>                If i = 1 Then sList = listB<br>                If i = 2 Then sList = listC<br>                For j = 0 To sList.GetUpperBound(0)<br>                    If Not oHash.Contains(sList(j)) Then<br>                        oHash.Add(sList(j), 0)<br>                    End If<br>                    oHash(sList(j)) += 1<br>                Next<br>            Next<br> <br>            For i = 0 To 2<br>                If i = 0 Then sList = listA<br>                If i = 1 Then sList = listB<br>                If i = 2 Then sList = listC<br>                For j = 0 To sList.GetUpperBound(0)<br>                    iScore(i) += (4 - oHash(sList(j)))<br>                Next<br>            Next<br> <br>            Return iScore(0) &amp; "/" &amp; iScore(1) &amp; "/" &amp; iScore(2)<br>        End Function<br>    End Class<br></td>