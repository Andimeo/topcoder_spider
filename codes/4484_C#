            using System;<br>using System.Collections;<br> <br>public class Cards<br>{<br>  public string[] deal(int numPlayers, string deck)<br>  {<br>    string[] ret = new string[numPlayers];<br>    for (int i=0; i&lt;numPlayers; i++)<br>    {<br>      ret[i]="";<br>    }<br>    int curPlayer=0;<br>    for (int i=0; i&lt;deck.Length-(deck.Length % numPlayers); i++)<br>    {<br>      ret[curPlayer]+=deck[i];<br>      curPlayer++;<br>      if (curPlayer==numPlayers) curPlayer=0;<br>    }<br>    return ret;<br>  }<br>}<br> <br> <br>// Powered by TCGen for C#<br></td>