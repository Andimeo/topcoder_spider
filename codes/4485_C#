            using System;<br>using System.Collections;<br><br>public class ReportAccess <br>{<br>  public string[] whoCanSee(string[] userNames, string[] allowedData, string[] reportData) <br>  {<br>    ArrayList canView = new ArrayList();<br>    for(int i=0;i&lt;userNames.Length;i++)<br>    {<br>      bool ok = true;<br>      string[] allowed = allowedData[i].Split(new char[]{' '});<br>      for(int j=0;j&lt;reportData.Length;j++)<br>      {<br>        bool meOk = false;<br>        for(int k=0;k&lt;allowed.Length;k++)<br>        {<br>          if(allowed[k] == reportData[j]) meOk = true;<br>        }<br>        if(!meOk) ok = false;<br>      }<br>      if(ok) canView.Add(userNames[i]);<br>    }<br>    canView.Sort();<br>    return (string[])canView.ToArray(typeof(string));<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>