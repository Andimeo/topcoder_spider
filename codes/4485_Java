            // Powered by radeclipse!<br>import java.util.* ;<br>public class ReportAccess {<br>String[] s2sa(String s) {<br>  StringTokenizer t=new StringTokenizer(s.trim());<br>  String[]r=new String[t.countTokens()];<br>  int i=0;<br>  while(t.hasMoreTokens())try{<br>     r[i++]=t.nextToken();<br>  }catch(Exception e){};<br>  return r;<br>}<br>  public String[] whoCanSee(String[] userNames, String[] allowedData, String[] reportData) {<br>    List r = new ArrayList() ;<br>    for (int i=0; i&lt;userNames.length; i++) {<br>      boolean good = true ;<br>      String[] allowed = s2sa(allowedData[i]) ;<br>      for (int j=0; j&lt;reportData.length; j++) {<br>  boolean okay = false ;<br>  for (int k=0; k&lt;allowed.length; k++) {<br>    if (reportData[j].equals(allowed[k])) {<br>      okay = true ;<br>      break ;<br>    }<br>  }<br>  if (!okay) {<br>    good = false ;<br>    break ;<br>  }<br>      }<br>      if (good)<br>  r.add(userNames[i]) ;<br>    }<br>    String[] rr = (String[])r.toArray(new String[0]) ;<br>    Arrays.sort(rr) ;<br>    return rr ;<br>  }<br>}<br></td>