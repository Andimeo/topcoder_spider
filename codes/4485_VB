<td class="problemText" colspan="8" valign="middle" align="left">
            Public Class ReportAccess<br>        Public Function whoCanSee(ByVal userNames As String(), ByVal allowedData As String(), ByVal reportData As String()) As String()<br>            Dim aAns As New System.Collections.ArrayList<br>            Dim i, j, k As Integer<br>            Dim sCanSee() As String<br>            Dim bCanSee As Boolean<br>            Dim bCanSeeAll As Boolean<br> <br>            For i = 0 To userNames.GetUpperBound(0)<br>                sCanSee = allowedData(i).Split(" ")<br>                bCanSeeAll = True<br>                For k = 0 To reportData.GetUpperBound(0)<br>                    bCanSee = False<br>                    For j = 0 To sCanSee.GetUpperBound(0)<br>                        If sCanSee(j) = reportData(k) Then bCanSee = True<br>                    Next<br>                    If Not bCanSee Then bCanSeeAll = False<br>                Next<br>                If bCanSeeAll Then aAns.Add(userNames(i))<br>            Next<br> <br>            aAns.Sort()<br> <br>            Return aAns.ToArray(GetType(String))<br>        End Function<br>    End Class<br></td>