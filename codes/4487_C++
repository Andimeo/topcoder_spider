            // Powered by photosynthesis<br>// FilterC++<br> <br>#line 5 "Flush.cpp"<br> <br>#include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;iterator&gt;<br>#include &lt;numeric&gt;<br>#include &lt;sstream&gt;<br>#include &lt;utility&gt;<br>#include &lt;map&gt;<br> <br>#define fu(i,m,n) for(typeof(m) i=m; i!=(n); i++)<br>#define fd(i,n) for(int i=(n)-1; i&gt;=0; i--)<br>#define fa(i,x) fu(i,(x).begin(),(x).end())<br> <br>#define i64 long long<br>using namespace std;<br> <br>double bi(double a, double b) {<br>  if(b&lt;0 || b&gt;a) return 0;<br>  if(b==0) return 1;<br>  return bi(a-1,b-1)*a/b;<br>}  <br> <br>struct Flush { <br>  double size(vector &lt;int&gt; s, int n) { <br>    double ret=0;<br>    double y=0;<br>    fu(a,0,s[0]+1)<br>    fu(b,0,s[1]+1)<br>    fu(c,0,s[2]+1)<br>    fu(d,0,s[3]+1)<br>    if(a+b+c+d==n){<br>      double t= 1.0*bi(s[0],a)*bi(s[1],b)*bi(s[2],c)*bi(s[3],d);<br>      ret += max(max(a,b),max(c,d))*t;<br>      y+=t;<br>    }<br>    return ret/y;<br> <br> <br>  } <br> <br>  <br> <br>}; <br> <br> <br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>