            using System;<br>using System.Collections;<br><br>public class Musical {<br>  public string loser(int n, double time) {<br>    double[,] dist = new double[n, n-1];<br>    double max = double.MinValue;<br>    char loser = '@';<br>    time /= 10.0;<br>    for (int i = 0; i &lt; n; i++)<br>    {<br>      double startPos = ((double)i)/n;<br>      double pos = startPos - time;<br>      while (pos &lt; 0) pos += 1;<br>      double maxDist = double.MaxValue;<br>      for (int j = 0; j &lt; n-1; j++)<br>      {<br>        double chairPos = ((double)j)/(n-1);<br>        <br>        double d1 = pos-chairPos;<br>        double d2 = chairPos-pos;<br>        while (d1 &lt; 0) d1 += 1.0;<br>        while (d2 &lt; 0) d2 += 1.0;<br><br>        dist[i,j] = Math.Min(d1, d2);<br>        if (dist[i,j] &lt; maxDist) maxDist = dist[i,j];<br>      }<br>      if (maxDist &gt; max)<br>      {<br>        max = maxDist;<br>        loser = (char)('A'+i);<br>      }<br>    }<br><br>    return "" + loser;<br><br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>