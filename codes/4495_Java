            import java.util.*;<br>public class Musical {<br><br>    double EPS = 1e-10;<br><br>    public String loser(int n, double time) {<br><br>        double R = 1.0;<br>        double chStep = 2 * Math.PI / (n - 1);<br>        double[] x = new double[n - 1];<br>        double[] y = new double[n - 1];<br>        for (int i = 0; i &lt; n-1; i++) {<br>            double ang = i * chStep;<br>            x[i] = R * Math.cos(ang);<br>            y[i] = R * Math.sin(ang);<br>        }<br>        while (time &gt; 10.0 - EPS) time -= 10.0;<br>        double[] xM = new double[n];<br>        double[] yM = new double[n];<br>        double step = 2 * Math.PI / n;<br>        for (int i = 0; i &lt; n; i++) {<br>            double ang = i * step - 2 * Math.PI * time / 10;<br>            xM[i] = R * Math.cos(ang);<br>            yM[i] = R * Math.sin(ang);<br>        }<br><br>        double max = Double.MIN_VALUE;<br>        int ans = 0;<br>        for (int i = 0; i &lt; n; i++) {<br>            double min = Double.MAX_VALUE;<br>            for (int j = 0; j &lt; n-1; j++) {<br>                double d = Math.hypot(xM[i] - x[j], yM[i] - y[j]);<br>                min = Math.min(min, d);<br>            }<br>            if (min &gt; max) {<br>                max = min;<br>                ans = i;<br>            }<br>        }<br><br>        String res = "" + (char) ('A' + ans);<br><br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>