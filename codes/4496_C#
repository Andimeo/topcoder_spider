            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class Elections <br>{<br>    public int visit(string[] likelihoods) <br>  {<br>    int ind = 0;<br>    double best = 100;<br>    for (int i = 0; i &lt; likelihoods.Length; i++)<br>    {<br>      double one = 0;<br>      for (int j = 0; j &lt; likelihoods[i].Length; j++)<br>        if (likelihoods[i][j] == '1')<br>          one++;<br>      double d = one / (double)likelihoods[i].Length ;<br>      if (d &lt; best)<br>      {<br>        best = d;<br>        ind = i;<br>      }<br>    }<br><br>    return ind;<br>    }<br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>