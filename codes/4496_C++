            #include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;map&gt;<br>#include &lt;algorithm&gt;<br>using namespace std;<br><br>#define FORI(X)  for (i = 0; i &lt; X.size(); i++)<br>#define FORJ(X)  for (int j = 0; j &lt; X.size(); j++)<br>#define RFORI(X)  for (i = X.size() - 1; i &gt;= 0; i--)<br><br>class Elections<br>{<br>  public:<br>  int visit(vector &lt;string&gt; lk)<br>  {<br>    int ret = 0;<br>    int i, ct;<br>    double best = 99;<br><br>    FORI(lk) {<br>      ct = 0;<br>      FORJ(lk[i])<br>        if (lk[i][j] == '1')<br>          ct++;<br>      if (best - (double)ct / lk[i].size() &gt;= 1e-9) {<br>        ret = i;<br>        best = (double)ct / lk[i].size();<br>      }<br>    }<br><br>    return ret;<br>  }<br>};<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>