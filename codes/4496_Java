            import java.util.*;<br>public class Elections {<br>  public int visit(String[] l) {<br>        double[] p = new double[l.length];<br>        double[] num1 = new double[l.length];<br>        double[] num2 = new double[l.length];<br>        <br>        for (int i = 0; i &lt; l.length; i++) {<br>            for (int j = 0; j &lt; l[i].length(); j++) {<br>                if (l[i].charAt(j) == '1')<br>                    num1[i]++;<br>                else<br>                    num2[i]++;<br>            }<br>        }<br>        int min = 0;<br>        for (int i = 0; i &lt; p.length; i++) {<br>            p[i] = num1[i] / (num1[i] + num2[i]);<br>            if (p[i] &lt; p[min])<br>                min = i;<br>        }<br>        <br>        return min;<br>        <br>        <br>    <br>  }<br>}<br> <br> <br>// Powered by FileEdit<br></td>