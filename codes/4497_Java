            import java.util.List;<br>import java.util.ArrayList;<br>import java.util.Collections;<br>import java.util.Iterator;<br> <br>public class BlackWhitePlane {<br> <br>    public double area(String[] circles) {<br>        final List&lt;C&gt; li = new ArrayList&lt;C&gt;();<br>        for (String s : circles) {<br>            final String[] sp = s.split("[ ]");<br>            li.add(new C(Integer.valueOf(sp[0]),Integer.valueOf(sp[1]),Integer.valueOf(sp[2])));<br>        }<br>        Collections.sort(li);<br>        int wa = 0;<br>        int ws = 0;<br>        final List&lt;C&gt; done = new ArrayList&lt;C&gt;();<br>        for (Iterator&lt;C&gt; iterator = li.iterator(); iterator.hasNext();) {<br>            final C c =  iterator.next();<br>            boolean white = true;<br>            for (int i = 0; i &lt; done.size(); i++) {<br>                final C oth = done.get(i);<br>                final int dist = (c.x - oth.x) * (c.x - oth.x) + (c.y - oth.y) * (c.y - oth.y);<br>                if (dist &lt;= oth.getR() * oth.getR()) {<br>                    white = !white;<br>                }<br>            }<br>            if (white) {<br>                wa += c.getR() * c.getR();<br>            } else {<br>                ws += c.getR() * c.getR();<br>            }<br>            done.add(c);<br>        }<br>        return Math.PI * (double) (wa - ws);<br>    }<br> <br>    private class C implements Comparable&lt;C&gt; {<br>        int x;<br>        int y;<br>        int r;<br> <br>        public C(int x, int y, int r) {<br>            this.x = x;<br>            this.y = y;<br>            this.r = r;<br>        }<br> <br>        public int getX() {<br>            return x;<br>        }<br> <br>        public int getY() {<br>            return y;<br>        }<br> <br>        public int getR() {<br>            return r;<br>        }<br> <br>        public int compareTo(C c) {<br>            return c.r - this.r;<br>        }<br>    }<br> <br>}<br></td>