            import java.util.*;<br><br>/** <br> * @author Roman Elizarov<br> */<br>public class Bingo {<br>  public String winner(String[] card, String[] calls) {<br>    String[][] a = new String[6][5];<br>    for (int i = 0; i &lt; 6; i++) {<br>      for (int j = 0; j &lt; 5; j++)<br>        a[i][j] = card[i].substring(3 * j, 3 * j + 3).trim();<br>    }<br>    String[][] b = new String[5][5];<br>    for (int i = 0; i &lt; 5; i++) {<br>      for (int j = 0; j &lt; 5; j++)<br>        b[i][j] = a[0][j] + a[i + 1][j];<br>    }<br>    int[][] f = new int[5][5];<br>    for (int i = 0; i &lt; 5; i++)<br>      Arrays.fill(f[i], -1);<br>    f[2][2] = Integer.MAX_VALUE;<br>    int[] best = new int[5];<br>    int[] chk = new int[5];<br>    Arrays.fill(best, -1);<br>    for (int k = 0; k &lt; calls.length; k++) {<br>      String s = calls[k];<br>      if (s.equals("BINGO"))<br>        return "YOU LOSE";<br>      for (int i = 0; i &lt; 5; i++)<br>        for (int j = 0; j &lt; 5; j++)<br>          if (s.equals(b[i][j]))<br>            f[i][j] = k;<br>      for (int i = 0; i &lt; 5; i++) {<br>        for (int j = 0; j &lt; 5; j++)<br>          chk[j] = f[i][j];<br>        check(best, chk);<br>      }<br>      for (int i = 0; i &lt; 5; i++) {<br>        for (int j = 0; j &lt; 5; j++)<br>          chk[j] = f[j][i];<br>        check(best, chk);<br>      }<br>      for (int j = 0; j &lt; 5; j++)<br>        chk[j] = f[j][j];<br>      check(best, chk);<br>      for (int j = 0; j &lt; 5; j++)<br>        chk[j] = f[j][4 - j];<br>      check(best, chk);<br>      if (best[0] &gt;= 0)<br>        break;<br>    }<br>    StringBuilder res = new StringBuilder();<br>    for (int i = 0; i &lt; 5; i++) {<br>      if (best[i] == Integer.MAX_VALUE)<br>        break;<br>      if (i &gt; 0)<br>        res.append(",");<br>      res.append(calls[best[i]]);<br>    }<br>    return res.toString();<br>  }<br><br>  private void check(int[] best, int[] chk) {<br>    Arrays.sort(chk);<br>    if (chk[0] &lt; 0)<br>      return;<br>    if (best[0] &lt; 0 || best[0] &gt; chk[0])<br>      System.arraycopy(chk, 0, best, 0, 5);<br><br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>