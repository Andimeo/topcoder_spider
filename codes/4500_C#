            using System;<br>using System.Collections;<br> <br>public class BlackjackWinner<br>{<br>  public int winnings(int bet, int dealer, int dealerBlackjack, int player, int blackjack)<br>  {<br>    if (blackjack==1 &amp;&amp; dealerBlackjack==0) return bet+(int)(bet/2);<br>    if (player&gt;21) return -bet;<br>    if (dealer&gt;21) return bet;<br>    if (dealer&gt;player) return -bet;    <br>    if (player&gt;dealer) <br>    {<br>      return bet;<br>    }<br>    if (dealerBlackjack==blackjack) return 0;<br>    if (dealerBlackjack==1) return -bet;<br>    return 0;<br>  }<br>}<br> <br> <br>// Powered by TCGen for C#<br></td>