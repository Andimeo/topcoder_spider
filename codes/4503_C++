            #include &lt;iostream&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#define FOR(i,n) for(int i=0;i&lt;n;i++)<br>#define debln(a) cout&lt;&lt; #a &lt;&lt;" : " &lt;&lt; a &lt;&lt; endl;<br>using namespace std;<br> <br>class ArrayHash {<br>  public:<br>  int getHash(vector &lt;string&gt; input) {<br>    int res = 0;<br>    FOR(i,input.size()) {<br>      FOR(j,input[i].size()) res+=(input[i][j]-'A' + i + j);<br>    }<br>    return res;<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>