            using System; using System.Collections; using System.Text; using System.Text.RegularExpressions;<br>public class PrintScheduler<br>   {<br>   public string getOutput(string[] threads, string[] slices)<br>      {<br>      StringBuilder sb = new StringBuilder();<br>      int N=threads.Length;<br>      int[] pos = new int[N];<br>      foreach (String sl in slices)<br>         {<br>         int i=INT(sl.Split(' ')[0]);<br>         int ct=INT(sl.Split(' ')[1]);<br>         for (int t=0; t &lt; ct; t++)<br>            {<br>            sb.Append(threads[i][pos[i]]);<br>            pos[i] = (pos[i]+1) % threads[i].Length;<br>            }<br>         }<br>      return sb.ToString();<br>      }<br><br><br>// Library begins here<br><br>static int    INT   (Object o) { return Convert.ToInt32(o.ToString()); }<br><br>} // TopC#der<br><br><br>// Powered by FileEdit<br></td>