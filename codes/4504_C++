            // You will never see how this line looked after 4 challenges failed!<br>#include &lt;algorithm&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>using namespace std;<br><br>#define LS &lt;<br>#define Size(x) (int(x.size()))<br><br>// All macros with parameters "k,a,b" run the "k" variable in range [a,b)<br>#define FOR(k,a,b) for(typeof(a) k=(a); k LS (b); ++k)<br><br>#ifdef floyd<br>FOR(k,0,100) FOR(i,0,100) FOR(j,0,100) w0[i][j] &lt;?= w0[i][k] + w0[k][j];<br>#endif<br><br>#line 3 "PrintScheduler.cc"<br><br>class PrintScheduler {<br>  public:<br>  string getOutput(vector &lt;string&gt; threads, vector &lt;string&gt; slices) {<br>    // !FDI<br>    int pos[60];<br>    int N = Size(threads);<br>    FOR(k,0,N) pos[k] = 0;<br>    string res;<br>    FOR(l,0,Size(slices)) {<br>      int a, b;<br>      sscanf(slices[l].c_str(), "%d %d", &amp;a, &amp;b);<br>      FOR(m,0,b) {<br>        res += threads[a][pos[a]];<br>        pos[a]++;<br>        pos[a] %= Size(threads[a]);<br>        }<br>      }<br>    return res;<br>    }<br>  };<br><br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>