            import java.util.*;<br> <br>public class PrintScheduler {<br>  public String getOutput(String[] threads, String[] slices) {<br>    int[] idx = new int[threads.length];<br>    String ret = "";<br>    for (int i = 0; i &lt; slices.length; i++) {<br>      StringTokenizer st = new StringTokenizer(slices[i]);<br>      int th = Integer.parseInt(st.nextToken());<br>      int t = Integer.parseInt(st.nextToken());<br>      for (int j = 0; j &lt; t; j++) {<br>        ret += threads[th].charAt(idx[th]);<br>        idx[th]++;<br>        if (idx[th] == threads[th].length()) {<br>          idx[th] = 0;<br>        }<br>      }<br>    }<br>    return ret;<br>  }<br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>