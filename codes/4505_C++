<td class="problemText" colspan="8" valign="middle" align="left">
            #include&lt;string&gt;<br>#include&lt;vector&gt;<br>#include&lt;math.h&gt;<br> <br>using namespace std;<br> <br> <br> <br>class RedundantStrings<br>{<br>  public:<br>  <br>  int howMany(int n)<br>  {<br>    int i;<br>    int count=0;<br>    int ret=0;<br>    <br>    if(n==1)<br>      return 0;<br>    <br>    for(i=1;i&lt;n;i++)<br>    {<br>      if(n%i==0)<br>      {  <br>        count=howMany(i);<br>        count=pow(2,i)-count;<br>        ret=ret+count;<br>      }<br>    }<br>    return ret;    <br>  }<br>};<br></td>