            using System;<br>using System.Collections;<br>public class Polyline {<br>  public double length(int a, int b, int x0, int y0, int x1, int y1) {<br>    double res = double.MaxValue;<br><br>    for (int dx = 0; dx &lt;= 1; ++dx)<br>      for (int dy = 0; dy &lt;= 1; ++dy)<br>      {<br>        int cx1 = x1;<br>        int cy1 = y1;<br>        for (int i = 0; i &lt; 2; ++i)<br>        {<br>          if (dx == 0)<br>          {<br>            cx1 = 2 * (-i * a) - cx1;<br>          }<br>          else<br>          {<br>            cx1 = 2 * ((i + 1) * a) - cx1;<br>          }<br>          if (dy == 0)<br>          {<br>            cy1 = 2 * (-i * b) - cy1;<br>          }<br>          else<br>          {<br>            cy1 = 2 * ((i + 1) * b) - cy1;<br>          }<br>        }<br><br>        double cur = Math.Sqrt((double) (x0 - cx1) * (double) (x0 - cx1) + (double) (y0 - cy1) * (double) (y0 - cy1));<br>        if (cur &lt; res)<br>          res = cur;<br>      }<br><br>    return res;<br>  }<br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>