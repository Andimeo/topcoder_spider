            using System;<br>using System.Collections;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class ComputationalComplexity<br>{<br>  public int fastestAlgo(int[] constant, int[] power, int[] log, int N)<br>  {<br>    int best=0;<br>    double d=Double.MaxValue;<br>    for (int i=0; i&lt;log.Length; ++i) {<br>      if (constant[i] * Math.Pow(N, power[i]) * Math.Pow(Math.Log(N), log[i]) &lt; d) {<br>        d = constant[i] * Math.Pow(N, power[i]) * Math.Pow(Math.Log(N), log[i]);<br>        best = i;<br>      }<br>    }<br>    return best;<br>  }<br>  <br>  #region Testing<br>  #endregion<br>}<br> <br>//Powered by [KawigiEdit]<br></td>