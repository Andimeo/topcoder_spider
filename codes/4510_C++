            #include &lt;stdio.h&gt;<br>#include &lt;stdlib.h&gt;<br>#include &lt;string.h&gt;<br>#include &lt;math.h&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;stack&gt;<br>#include &lt;queue&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;iterator&gt;<br>#include &lt;iostream&gt;<br>#include &lt;functional&gt;<br>#include &lt;sstream&gt;<br><br>using namespace std;<br><br>class node<br>{<br>public:<br>  double x;<br>  int pos;<br>};<br><br>vector&lt;node&gt; arr;<br>node tmp;<br><br>bool cmp( node a, node b)<br>{<br>  if(a.x != b.x)<br>    return a.x &lt; b.x;<br>  return a.pos &lt; b.pos;<br>}<br>class ComputationalComplexity <br>{<br>  public:<br>  int fastestAlgo(vector &lt;int&gt; con, vector &lt;int&gt; pwr, vector &lt;int&gt; lp, int N) <br>  {<br>    int i;<br>    int ret;<br><br>    arr.resize(0);<br><br>    for(i=0;i&lt;con.size();i++)<br>    {<br>      tmp.pos = i;<br>      tmp.x = con[i] * pow( (double)N , (double) pwr[i] ) * pow( log((double)N) , (double)lp[i] );<br><br>      arr.push_back(tmp);<br><br>    }<br><br>    sort(arr.begin() , arr.end(),cmp);<br><br>    ret = arr[0].pos;<br><br>    return ret;<br>  }<br>    <br><br>  <br><br>};<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>