            import java.util.*;<br>public class ComputationalComplexity {<br>  public int fastestAlgo(int[] constant, int[] power, int[] logPower, int N) {<br>    double best = constant[0] * Math.pow(N,power[0]) * Math.pow(Math.log(N),logPower[0]);<br>    int ind = 0;<br>    for(int x = 1; x &lt; constant.length; x++)<br>    {<br>      double test = constant[x] * Math.pow(N,power[x]) * Math.pow(Math.log(N),logPower[x]);<br>      if(test &lt; best)<br>      {<br>        ind = x;<br>        best = test;<br>      }<br>    }<br>    return ind;<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>