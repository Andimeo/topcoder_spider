            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class TaxTable {<br>    public int income(int taxAmount) {<br>        if (taxAmount &lt;= 1000) return -1;<br><br>        double [] per = {.25, .28, .33, .35};<br>        int [] low = {100000, 117250, 178650, 319100};<br>        int [] high = {117250, 178650, 319100, Int32.MaxValue};<br>        double [] sub = {6525.00, 10042.50, 18975.00, 25357.00};<br><br>        for (int i = 0; i &lt; per.Length; i++)<br>        {<br>            double x = (double)taxAmount + sub[i];<br>            double y = x / per[i];<br>            if (y &gt;= low[i] &amp;&amp; y &lt; high[i])<br>            {<br>                return (int)(Math.Round(y));<br>            }<br>        }<br>        return -1;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>