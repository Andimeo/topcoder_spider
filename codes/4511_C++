            #include &lt;vector&gt;<br>#include &lt;cmath&gt;<br>#include &lt;iostream&gt;<br>using namespace std;<br>class TaxTable<br>{<br>public:<br>int income (int taxMount)<br>{<br>     double total;<br>     total=taxMount+6525;<br>     total/=.25;<br>     if (total&gt;=100000 &amp;&amp; total&lt;117250) return (int)(total+.5);<br>     total=taxMount+10042.5;<br>     total/=.28;<br>     if (total&gt;=117250 &amp;&amp; total&lt;178650) return (int)(total+.5);<br>     total=taxMount+18975;<br>     total/=.33;<br>     if (total&gt;=178650 &amp;&amp; total&lt;319100) return (int)(total+.5);<br>     total=taxMount+25357;<br>     total/=.35;<br>     if (total&gt;=319100) return (int)(total+.5);<br>     return -1;<br>     }<br>     };<br></td>