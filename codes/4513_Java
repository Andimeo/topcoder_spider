            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class SysFailure<br>{<br>  boolean[][] visited = new boolean[50][50];<br>  int compsize[] = new int[2500];<br>  <br>  public int dfs(int x, int y, char[][] mat)<br>  {<br>    <br>    if(x &lt; 0 || x &gt;= mat.length || y &lt; 0 || y &gt;= mat[0].length) return 0;<br>    if(mat[x][y]=='0')return 0;<br>    if(visited[x][y]) return 0;<br>    visited[x][y] = true;<br>    int ret = 1;<br>    ret+= dfs(x-1, y-1, mat);<br>    ret+= dfs(x-1, y, mat);<br>    ret+= dfs(x-1, y+1, mat);<br>    ret+= dfs(x+1, y-1, mat);<br>    ret+= dfs(x+1, y, mat);<br>    ret+= dfs(x+1, y+1, mat);<br>    ret+= dfs(x, y-1, mat);<br>    ret+= dfs(x, y+1, mat);<br>    return ret;<br>        <br>  }<br>  <br>  public int maxVal(char[][] mat)<br>  {<br>    int max = 0;<br>    for(int i = 0; i &lt; mat.length; i++)<br>    {<br>      for(int j = 0; j &lt; mat[i].length; j++)<br>      {<br>        if(!visited[i][j])<br>        {<br>          int x = dfs(i, j, mat);<br>          //System.out.println(i + " " + j + " " + x);<br>          if(x &gt; max) max = x;<br>        }<br>      }<br>    }<br>    <br>    for(int i = 0; i &lt; mat.length; i++)<br>    {<br>      for(int j = 0; j &lt; mat[i].length; j++)<br>      {<br>        visited[i][j] = false;<br>      }<br>    }<br>    <br>    return max;<br>  }<br>  <br>  public int critical(int n, String[] state)<br>  {<br>    int ret = 0;<br>    char mat[][] = new char[state.length][state[0].length()];<br>    for(int i = 0; i &lt; state.length ;i++)<br>      for(int j = 0; j &lt; state[0].length(); j++)<br>        mat[i][j] = state[i].charAt(j);<br>    int x = maxVal(mat);<br>    if(x &lt; n) return -1;  <br>    for(int i = 0; i &lt; mat.length; i++)<br>    {<br>      for(int j = 0; j &lt; mat[0].length;j ++)<br>      {<br>        if(mat[i][j]=='1')<br>        {<br>          mat[i][j] = '0';<br>          int xb = maxVal(mat);<br>          if(xb &lt; n) ret++;<br>          mat[i][j] = '1';          <br>        }<br>      }<br>    }  <br>    return ret;        <br>  }<br>  <br>  <br>}<br> <br>//Powered by [KawigiEdit]<br></td>