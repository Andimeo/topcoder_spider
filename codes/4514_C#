            using System;<br>using System.Collections;<br>using System.Text;<br> <br>public class Dating<br>{<br>    public string dates(string circle, int k)<br>    {<br>        StringBuilder ret = new StringBuilder();<br>        int N = circle.Length;<br>        char[] person = circle.ToCharArray();<br>        int cntCap=0,cntMin=0;<br>        foreach (char ch in person)<br>        {<br>            if (ch &lt; 'a')<br>                cntCap++;<br>            else<br>                cntMin++;<br>        }<br>        int num = Math.Min(cntCap, cntMin);<br>        bool[] bOut = new bool[N];<br>        int iPos = -1;<br>        for (int pair = 0; pair &lt; num; pair++)<br>        {<br>            for (int step = 0; step &lt; k; step++)<br>            {<br>                iPos++;<br>                iPos %= N;<br>                while (bOut[iPos])<br>                {<br>                    iPos++;<br>                    iPos %= N;<br>                }<br>            }<br>            char chooser = person[iPos];<br>            bOut[iPos] = true;<br>            char choosen = 'z';<br>            int cPos = -1;<br>            for (int i = 0; i &lt; N; i++)<br>            {<br>                if (bOut[i]) continue;<br>                if ((chooser - 'a' + 1) * (person[i] - 'a' + 1) &gt; 0) continue;<br>                if ((cPos == -1) || (person[i] &lt; choosen))<br>                {<br>                    cPos = i;<br>                    choosen = person[i];<br>                }<br>            }<br>            bOut[cPos] = true;<br>            if (ret.Length &gt; 1)<br>                ret.Append(' ');<br>            ret.Append(chooser);<br>            ret.Append(choosen);<br>        }<br>        return ret.ToString();<br>    }<br>}<br> <br>// Powered by PopsEdit<br></td>