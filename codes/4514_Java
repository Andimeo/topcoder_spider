            import java.util.*;<br>import java.util.regex.*;<br> <br> <br>public class Dating {<br> <br> <br>  public String dates(String circle, int k) {<br>    int pos = 0;<br>    String ret = "";<br>    while (!done(circle)) {<br>      System.out.printf("Circle %s pos %d\n", circle, pos);<br>      for (int i = 1; i &lt; k; i++) { <br>        pos++;<br>        pos = pos % circle.length();<br>      }<br>      char picker = circle.charAt(pos);<br>      char mate = mate(picker,circle);<br>      int mateIndex = circle.indexOf(mate);<br>      if (mateIndex &lt; pos) {<br>        pos--;<br>      }<br>      ret += "" + picker + mate + " ";<br>      circle = circle.replace(""+picker, "").replace(""+mate,"");<br>    }<br>    <br>    return ret.trim();<br>  }<br>  <br>  boolean done(String circle) {<br>    return circle.length() == 0 || <br>    circle.equals(circle.toUpperCase()) || circle.equals(circle.toLowerCase());<br>  }<br>  <br>  char mate(char c, String circle) {<br>    char best = (char) ('z' + 1);<br>    for (char m : circle.toCharArray()) {<br>      if (Character.isUpperCase(m) != Character.isUpperCase(c)<br>          &amp;&amp; m &lt; best) {<br>        best = m;<br>      }<br>    }<br>    return best;<br>  }<br> <br> <br> <br>}<br> <br> <br> <br> <br> <br>// Powered by CodeProcessor<br></td>