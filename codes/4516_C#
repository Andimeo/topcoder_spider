            using System;<br>using System.Collections.Generic;<br><br>public class Popularity {<br>  public string[] sort(string[] reply) {<br>        Dictionary&lt;string, int&gt; count = new Dictionary&lt;string, int&gt;();<br>        foreach (string x in reply)<br>        {<br>            string[] y = x.Split(' ');<br>            if (!count.ContainsKey(y[0]))<br>                count[y[0]] = 1;<br>            else<br>                ++count[y[0]];<br>        }<br>        int[] r = new int[reply.Length];<br>        for (int i = 0; i &lt; reply.Length; ++i)<br>            r[i] = i;<br>        Array.Sort(r, delegate(int a, int b)<br>                          {<br>                              int z = -(count[reply[a].Split(' ')[0]] - count[reply[b].Split(' ')[0]]);<br>                              if (z != 0)<br>                                  return z;<br>                              return a - b;<br>                          });<br>        string[] res = new string[reply.Length];<br>        for (int i = 0; i &lt; reply.Length; ++i)<br>            res[i] = reply[r[i]];<br>      return res;<br>  }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>