            import java.io.*;<br>import java.util.*;<br><br>public class Popularity {<br>  public String[] sort(String[] reply) {<br>    int n = reply.length;<br>    HashMap&lt;String, Integer&gt; cnt = new HashMap&lt;String, Integer&gt;();<br>    for (String s : reply) {<br>      String[] a = s.split(" ");<br>      int num = (cnt.containsKey(a[0])) ? cnt.get(a[0]) : 0;<br>      cnt.put(a[0], num + 1);<br>    }<br>    for (int i = 0; i &lt; n; i++) {<br>      for (int j = 0; j &lt; n - 1; j++) {<br>        String a = reply[j].split(" ")[0];<br>        String b = reply[j + 1].split(" ")[0];<br>        int aa = cnt.get(a);<br>        int bb = cnt.get(b);<br>        if (aa == bb)<br>          continue;<br>        if (aa &lt; bb) {<br>          a = reply[j];<br>          reply[j] = reply[j + 1];<br>          reply[j + 1] = a;<br>        }<br>      }<br>    }<br>    return reply;<br>  }<br>}<br></td>