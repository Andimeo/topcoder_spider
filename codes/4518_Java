            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class OneFight<br>{<br>  public int monsterKilling(int[] life, int[] damage, int yourDamage)<br>  {<br>    best = Integer.MAX_VALUE;<br>    searchAll(life, damage, yourDamage, new boolean[life.length], 0);<br>    return best+1;<br>  }<br>  int best;<br>  public void searchAll(int[] life, int[] damage, int yourDamage, boolean[] used, int sofar)<br>  {<br>    boolean done = true;<br>    int totalDamage = 0;<br>    for (int i=0; i&lt;used.length; i++)<br>    {<br>      if (!used[i])<br>      {<br>        done = false;<br>        totalDamage += damage[i];<br>      }<br>    }<br>    if (done)<br>    {<br>      best = Math.min(best, sofar);<br>    }<br>    else<br>      for (int i=0; i&lt;used.length; i++)<br>        if (!used[i])<br>        {<br>          used[i] = true;<br>          searchAll(life, damage, yourDamage, used, sofar + totalDamage*((life[i]+yourDamage-1)/yourDamage));<br>          used[i] = false;<br>        }<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>