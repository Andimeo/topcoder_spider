            using System;<br>using System.Collections;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class Speller<br>{<br>  public int value(string number)<br>  {<br>    int ans = 0;  <br>    int diff = int.MaxValue;<br>    int[] d = new int[100];<br>    <br>    for (int i = 1; i &lt;= 99; i++)<br>    {<br>      string s = Spell(i);<br>      if (s.Length != number.Length)<br>      {<br>        d[i] = int.MaxValue;<br>         continue;      <br>      }<br>      for (int j = 0; j &lt; s.Length; j++)<br>        if (s[j] != number[j]) d[i]++;<br>      <br>    }<br>    <br>    for (int i = 1; i &lt;= 99; i++)<br>      if (d[i] &lt; diff) <br>      {<br>        diff = d[i];<br>        ans = i;<br>      }<br>    <br>    int c = 0;<br>    for (int i = 1; i &lt;= 99; i++)<br>      if (d[i] == diff) c++;<br>    if (c &gt; 1) return -1;<br>    <br>    return ans;<br>  }<br>  <br>  string Spell(int k)<br>  {<br>    string[] ONES = new string[9]{"one", "two", "three", "four", "five", "six", "seven", "eight", "nine"};<br>    string[] TEEN = new string[10]{"ten", "eleven", "twelve", "thirteen", "fourteen", "fifteen", "sixteen" ,"seventeen", "eighteen","nineteen"};<br>    string[] TENS = new string[8]{"twenty", "thirty", "forty", "fifty", "sixty", "seventy", "eighty", "ninety"};<br>    if (k &lt; 10) return ONES[k - 1];<br>    if (k &lt; 20) return TEEN[k - 10];<br>    if (k % 10 == 0) return TENS[k / 10 - 2];<br>    return TENS[k / 10 - 2] + "-" + ONES[k % 10 - 1];<br>  }<br>}<br> <br>//Powered by [KawigiEdit]<br></td>