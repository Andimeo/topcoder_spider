            #include &lt;string&gt;<br> <br>using namespace std;<br> <br> <br>string a[20] = {"zero", "one", "two", "three", "four", "five", "six", "seven", "eight", "nine",<br>                "ten", "eleven", "twelve", "thirteen", "fourteen", "fifteen", "sixteen", "seventeen", "eighteen", "nineteen"};<br>string des[10] = {"twenty", "thirty", "forty", "fifty", "sixty", "seventy", "eighty", "ninety"};<br>string cis[100];<br> <br>class Speller<br>{<br> <br>public:<br> <br>void robCisla(){<br>  for (int i=0; i&lt;20; i++)<br>    cis[i] = a[i];<br>  for (int i=20; i&lt;100; i++){<br> <br>    if (i % 10 == 0) cis[i] = des[i/10-2];<br>    else {<br>      cis[i] = des[i/10-2] + "-"+a[i % 10];<br>    };<br> <br>  };<br>};<br> <br>int dist(string a, string b){<br>  if (a.length() != b.length()) return 999999;<br>  int p = 0;<br>  for (int i=0; i&lt;a.length(); i++)<br>    if (a[i] != b[i])p++;<br>  return p;  <br>};<br> <br>int value(string number){<br>  int naj = 99999, k = 1, c = -1;<br> <br>  robCisla();<br> <br>  for (int i=1; i&lt;100; i++){<br>    int d = dist(number, cis[i]);<br> <br>    if (d &lt; naj){<br>      naj = d; k = 1;<br>      c = i;<br>    }else if (d == naj) k++;<br> <br>  };<br> <br>  if (k &gt; 1 || naj &gt; 1000) return -1;<br>  return c;<br>};<br> <br>};<br></td>