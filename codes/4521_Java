            import java.util.*;<br>public class Speller {<br>  String[] ones = new String[]{"zero", "one", "two", "three", "four", "five", "six", "seven", "eight", "nine"};<br>  String[] teen = new String[]{"ten", "eleven", "twelve", "thirteen", "fourteen", "fifteen", "sixteen", "seventeen", "eighteen", "nineteen"};<br>  String[] tens = new String[]{"twenty", "thirty", "forty", "fifty", "sixty", "seventy", "eighty", "ninety"};<br>  public int value(String S) {<br>    int result = -1;<br>    int minDist = 1000;<br>    String s = "";<br>    int dist = -1;<br>    for (int a=1;a&lt;100;a++) {<br>      s = figure(a);<br>      dist = findDist(s,S);<br>      if (dist == minDist) {<br>        result = -1;<br>      } else if (dist &lt; minDist) {<br>        minDist = dist;<br>        result = a;<br>      }<br>    }<br>    if (minDist == 1000) return -1;<br>    return result;<br>  }<br>  public String figure(int N) {<br>    if (N &lt; 10) return ones[N];<br>    else if (N &lt; 20) return teen[N-10];<br>    else if (N % 10 == 0) return tens[(N/10)-2];<br>    else return tens[(N/10)-2] + "-" + ones[N%10];<br>  }<br>  public int findDist(String a, String b) {<br>    if (a.length() != b.length()) {<br>      return Integer.MAX_VALUE;  <br>    }<br>    int result = 0;<br>    char ca = ' '; <br>    char cb = ' ';<br>    for (int c=0;c&lt;a.length();c++) {<br>      ca = a.charAt(c);<br>      cb = b.charAt(c);<br>      if (ca!=cb) {<br>        result++;  <br>      }<br>    }<br>    return result;<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>