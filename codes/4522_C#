            using System;<br>using System.Collections;<br> <br>public class FracCount<br>  {<br>    public int position(int n, int d)<br>    {<br>      int ans=0;<br>      for (int i=2;i&lt;=d-1;i++)<br>      {<br>        for (int j=1;j&lt;=i-1;j++)<br>        {<br>          if (ir(j,i)==true)<br>            ans++;<br>        }<br>      }<br> <br>      for (int j=1;j&lt;=n-1;j++)<br>      {<br>        if (ir(j,d)==true)<br>          ans++;<br>      }<br>      ans++;<br>      return(ans);<br>    }<br>    public bool ir(int n, int d)<br>    {<br>      for (int i=2;i&lt;=n;i++)<br>      {<br>        if (n%i==0)<br>        {<br>          if (d%i==0)<br>            return(false);<br>        }<br>      }<br>      return(true);<br>    }<br>  }<br></td>