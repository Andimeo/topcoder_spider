            #include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;cmath&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;sstream&gt;<br>#include &lt;numeric&gt;<br>#include &lt;iostream&gt;<br>#include &lt;algorithm&gt;<br> <br>using namespace std;<br> <br>class FracCount {<br>  int gcd (int a, int b)<br>  {<br>    if (b == 0) return a;<br>    else return gcd (b, a % b);<br>  }<br>  <br>  public:<br>  int position(int num, int den) {<br>    int cnt = 0;<br>    for (int d = 2; d &lt;= 1000; ++d)<br>    {<br>      for (int n = 1; n &lt; d; ++n)<br>      {<br>        if (gcd (d, n) == 1) cnt++;<br>        if (d == den &amp;&amp; n == num) return cnt;<br>      }<br>    }<br>    return -1;<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>