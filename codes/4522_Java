<td class="problemText" colspan="8" valign="middle" align="left">
            public class FracCount<br>{<br>  public int position(int numerator, int denominator)<br>  {<br>    int ret=0;<br>    for(int d=2 ;  ; d++)<br>      for(int n=1 ;n&lt;d ; n++)<br>      {<br>        if(gcd(n,d) ==1) ret++;<br>        if(d==denominator &amp;&amp; n==numerator) return ret;<br>      }<br> <br>  <br>  }<br> <br>  public int gcd(int n, int d)<br>   {<br>   if ( d == 0 )<br>      {<br>      return 1;<br>      }<br>   int r = n % d;<br>   return( r == 0 ) ? d : gcd( d , r );<br>   }<br> <br> <br>}<br></td>