            using System;<br>using System.Collections;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class DivToZero<br>{<br>  public string lastTwo(int num, int factor)<br>  {<br>    num = (num / 100) * 100;<br>    for (int i=0; i&lt;100; i++) <br>    {<br>      int num0 = num + i;<br>      if (num0 % factor == 0) <br>      {<br>        if (i&lt;10)<br>          return "0" + i.ToString();<br>        return i.ToString();<br>      }<br>    }<br> <br>    return null;<br>  }<br> <br>}<br> <br>//Powered by [KawigiEdit]<br></td>