            import java.util.ArrayList;<br> <br>public class SortBooks<br>{<br>  boolean ist(String s)<br>  {<br>    s=s.trim();<br>    while (s.indexOf("  ")&gt;=0)<br>      s=s.replaceAll("  ", " ");<br>    String[] tmp=s.split(" ");<br>    if (tmp.length&gt;3)<br>      return true;<br>    <br>    s=" "+s.toLowerCase()+" ";<br>    if (s.indexOf(" the ")&gt;=0) return true;<br>    if (s.indexOf(" of ")&gt;=0) return true;<br>    if (s.indexOf(" and ")&gt;=0) return true;<br>    <br>    return false;<br>  }<br>  <br>  public int[] checkManually(String[] field1, String[] field2)<br>  {<br>    ArrayList&lt;Integer&gt; res=new ArrayList&lt;Integer&gt;();<br>    for (int a=0; a&lt;field1.length; a++) {<br>      if (ist(field1[a])==ist(field2[a]))<br>        res.add(a);<br>    }<br>    int[] out=new int[res.size()];<br>    int pos=0;<br>    for(int i:res)<br>      out[pos++]=i;<br>    return out;<br>  }<br>}<br></td>