            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class CompanyName <br>{<br>    public string shortestProposal(string[] proposals) <br>    {<br>        string res = "";<br>    string vowel = "aAeEiIoOuU";<br>    int min = int.MaxValue;<br>    foreach (string s in proposals)<br>    {<br>      int cnt = 0;<br>      bool isPrevVowel = false;<br>      foreach (char c in s)<br>      {<br>        bool isVowel = (vowel.IndexOf(c) != -1);<br>        if (isVowel)<br>        {<br>          if (!isPrevVowel)<br>            cnt++;<br>          isPrevVowel = true;<br>        }<br>        else<br>        {<br>          if (isPrevVowel)<br>            cnt--;<br>          isPrevVowel = false;<br>          cnt += 2;<br>        }<br>      }<br><br>      if (cnt &lt; min)<br>      {<br>        res = s;<br>        min = cnt;<br><br>      }<br><br>    }<br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>