            public class CompanyName<br>{<br>  public String shortestProposal(String[] pro)<br>  {<br>    int sum=0;<br>    int best=100000;<br>    int index=-1;<br>    String temp="";<br>    char c;<br>    int length=0;<br>    int con=0,vo=0;<br>    String a="AEIOUaeiou";<br>    for(int i=0;i&lt;pro.length;i++)<br>    {<br>      temp=pro[i];<br>      length=0;<br>      con=0;<br>      vo=0;<br>      sum=0;<br>      for(int j=0;j&lt;temp.length();j++)<br>      {<br>        c=temp.charAt(j);<br>        if((a.indexOf(c)==-1))<br>        {<br>          con=1;<br>          if(vo==0)<br>          {<br>            length++;<br>          }<br>          else<br>          {<br>            sum++;<br>            length++;<br>            vo=0;<br>          }<br>        }<br>        else<br>        {<br>          vo=1;<br>          if(con==1)<br>          {<br>            sum=sum+ 2*length -1;<br>            length=0;<br>            con=0;<br> <br>          }<br>        }<br> <br> <br>      }<br>      if(con==1)<br>      {<br>        sum=sum+ 2*length -1;<br>      }<br>      else if(vo==1)<br>      {<br>          sum++;<br>      }<br> <br>      if(sum&lt;best)<br>      {<br>        best=sum;<br>        index=i;<br>      }<br>    }<br> <br>    return pro[index];<br>  }<br> <br>}<br></td>