<td class="problemText" colspan="8" valign="middle" align="left">
            #include&lt;iostream&gt;<br>#include&lt;vector&gt;<br>#include&lt;string&gt;<br>#include&lt;cmath&gt;<br>using namespace std;<br>int value(string s)<br>{<br> if(s == "black") return 0;<br> if(s == "brown") return 1;<br> if(s == "red") return 2;<br> if(s == "orange") return 3;<br> if(s == "yellow") return 4;<br> if(s == "green") return 5;<br> if(s == "blue") return 6;<br> if(s == "violet") return 7;<br> if(s == "grey") return 8;<br> if(s == "white") return 9;<br>}<br> <br>class ColorCode<br>{<br> public:<br> long long getOhms(vector&lt;string&gt; code)<br> {<br>  long long ret;<br>  ret = value(code[0]);<br>  ret = ret * 10 + value(code[1]);<br>  ret *= pow(10 , (float)value(code[2]));<br>  <br>  return ret;<br> }<br>};<br></td>