<td class="problemText" colspan="8" valign="middle" align="left">
            public class ColorCode {<br> <br>  public long getOhms(String[] code) {<br>    String value = "";<br>    String x = "";<br>    long output = 0;<br>    <br>    for (int i = 0; i &lt; 3; i++) {<br>      if (code[i].compareTo("black") == 0)<br>        x = "0";<br>      else if (code[i].compareTo("brown") == 0)<br>        x = "1";<br>      else if (code[i].compareTo("red") == 0)<br>        x = "2";<br>      else if (code[i].compareTo("orange") == 0)<br>        x = "3";<br>      else if (code[i].compareTo("yellow") == 0)<br>        x = "4";<br>      else if (code[i].compareTo("green") == 0)<br>        x = "5";<br>      else if (code[i].compareTo("blue") == 0)<br>        x = "6";<br>      else if (code[i].compareTo("violet") == 0)<br>        x = "7";<br>      else if (code[i].compareTo("grey") == 0)<br>        x = "8";<br>      else if (code[i].compareTo("white") == 0)<br>        x = "9";<br>      <br>      if (i != 2)<br>        value = value + x;<br>      else<br>        output = (long) (new Double(value).doubleValue() * Math.pow(10, new Double(x).doubleValue()));<br>    }<br>    <br>    return output;<br>  }<br> <br>}<br></td>