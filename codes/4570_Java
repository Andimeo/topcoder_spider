            import java.util.*;<br>import java.io.*;<br>import java.text.*;<br>import java.math.*;<br>import static java.lang.Math.*;<br>import static java.util.Arrays.*;<br>public class LanguageRecognition {<br>  public int whichLanguage(String[] languages, String text) {<br>    double best = Double.MAX_VALUE;<br>    int ret = -1;<br>    double[][] look = new double[languages.length][26];<br>    for(int i = 0; i &lt; languages.length; i++){<br>      look[i] = get(languages[i]);<br>    }<br>    double[] t = get(text);<br>    for(int i = 0; i &lt; look.length; i++){<br>      double sum = 0;<br>      for(int j = 0; j &lt; 26; j++){<br>        double tmp = (look[i][j]-t[j]);<br>        sum += tmp*tmp;<br>      }<br>      if(sum + 1e-9 &lt; best){<br>        best = sum;<br>        ret = i;<br>      }<br>    }<br>    return ret;<br>  }<br>  double[] get(String s){<br>    double[] ret = new double[26];<br>    s = s.toUpperCase();<br>    int tot = 0;<br>    for(int i = 0; i &lt; s.length(); i++){<br>      char c = s.charAt(i);<br>      if(c &lt;= 'Z' &amp;&amp; c &gt;= 'A'){<br>        ret[c-'A']++;<br>        tot++;<br>      }<br>    }<br>    for(int i = 0; i &lt; ret.length; i++){<br>      ret[i] /= tot;<br>    }<br>    return ret;<br>  }<br> <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>