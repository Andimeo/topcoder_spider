            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class TVWatching {<br>  int[] s, e;<br><br>  int s2t(string s)<br>  {<br>    int h = int.Parse(s.Substring(0, 2));<br>    int m = int.Parse(s.Substring(3, 2));<br><br>    if (h == 12) h = 0;<br><br>    int min = h*60 + m;<br>    <br>    if (s.Substring(5, 2) == "PM") min += 12*60;<br><br>    return min;<br>  }<br><br>  void inp(string[] p)<br>  {<br>    s = new int[p.Length];<br>    e = new int[p.Length];<br><br>    for (int i=0; i &lt; p.Length; i++)<br>    {<br>      s[i] = s2t(p[i].Substring(0, 7));<br>      e[i] = s2t(p[i].Substring(10, 7));<br>    }<br>  }<br><br>/*  public int test(int t0, int t1)<br>  {<br>    int[] d = new int[24*60+1];<br><br>    int cnt = 0;<br>    for (int i=0; i &lt; s.Length; i++)<br>      if (s[i] &lt; e[i] &amp;&amp; s[i] &gt;= t0 &amp;&amp; e[i] &lt;= t1)<br>      {<br>        d[e[i]] = Math.Max(d[e[i]-1], d[s[i]]+e[i]-s[i]]);<br>      }<br><br>    return cnt;<br>  }*/<br><br>  public int test(int t0, int t1)<br>  {<br>    int[] d = new int[24*60+1];<br><br>    for (int i=1; i &lt; 24*60; i++)<br>    {<br>      d[i] = d[i-1];<br><br>      for (int j=0; j &lt; e.Length; j++)<br>        if (s[j] &lt; e[j] &amp;&amp; s[j] &gt;= t0 &amp;&amp; e[j] &lt;= t1 &amp;&amp; e[j] == i)<br>          d[i] = Math.Max(d[i], d[s[j]] + i - s[j]);<br>    }<br><br>    return d[24*60-1];<br>  }<br><br>  public int mostMinutes(string[] programs) <br>  {<br>    inp(programs);<br><br>    Array.Sort(e, s);<br>    int max = test(0, 24*60);<br><br>    for (int i=0; i &lt; s.Length; i++)<br>    {<br>      if (s[i] == e[i]) return 24*60;<br><br>      if (s[i] &gt; e[i])<br>      {<br>        int v = test(e[i], s[i]) + e[i] + 24*60 - s[i];<br>        if (v &gt; max) max = v;<br><br>      }<br>    }<br><br>    return max;<br>    }<br><br>}<br><br><br><br>// Powered by F