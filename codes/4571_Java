            import java.util.*;<br>public class TVWatching {<br>  public int get_time(String tm) {<br>    int h = 10 * (tm.charAt(0) - '0') + (tm.charAt(1) - '0');<br>    int m = 10 * (tm.charAt(3) - '0') + (tm.charAt(4) - '0');<br>    if (h == 12) h = 0;<br>    if (tm.charAt(5) == 'A')<br>      return 60 * h + m;<br>    else<br>      return 60 * (h + 12) + m;<br>  }<br><br>  public boolean ints(int a, int b, int c, int d) {<br>    int l = Math.max(a, c);<br>    int r = Math.min(b, d);<br>    return (l &lt; r);<br>  }<br><br>    public int mostMinutes(String[] programs) {<br>  int n = programs.length;<br>  int[] st = new int[n];<br>  int[] fn = new int[n];<br>  for (int i=0; i &lt; n; i++) {<br>    StringTokenizer stt = new StringTokenizer(programs[i], " -");<br>    String lt = stt.nextToken();<br>    String rt = stt.nextToken();<br>    st[i] = get_time(lt);<br>    fn[i] = get_time(rt);<br>    if (fn[i] &lt;= st[i]) {<br>      fn[i] += 24 * 60;<br>    }<br>  }<br>  int ret = 0;<br>  for (int nst=0; nst &lt; n; nst++) {<br>    int cnt = 0;<br>    int[] pst = new int[50];<br>    int[] pfn = new int[50];<br>    for (int i=0; i &lt; n; i++) {<br>      int tst = st[i];<br>      int tfn = fn[i];<br>      if (tst &lt; st[nst]) {<br>        tst += 24 * 60;<br>        tfn += 24 * 60;<br>      }<br>      if ((!ints(st[nst], fn[nst], tst, tfn)) &amp;&amp;<br>         (!ints(st[nst] + 24 * 60, fn[nst] + 24 * 60, tst, tfn))) {<br>        pst[cnt] = tst;<br>        pfn[cnt++] = tfn;<br>      }<br>    }<br>    for (int i=0; i+1 &lt; cnt; i++)<br>      for (int j=0; j+1 &lt; cnt; j++)<br>        if (pst[j] &gt; pst[j+1]) {<br>          int s = pst[j]; pst[j] = pst[j+1]; pst[j+1] = s;<br>          s = pfn[j]; pfn[j] = pfn[j+1]; pfn[j+1] = s;<br>        }<br>    int[] f = new int[50];<br>    int maxf = 0;<br>    for (int i=0; i &lt; cnt; i++) {<br>      f[i] = pfn[i] - pst[i];<br>      for (int j=0; j &lt; i; j++)<br>        if ((pfn[j] &lt;= pst[i]) &amp;&amp; (f[i] &lt; f[j] + pfn[i] - pst[i]))<br>          f[i] = f[j] + pfn[i] - pst[i];<br>      if (f[i] &gt; maxf) maxf = f[i];<br>    }<br>    if (maxf + fn[nst] - st[nst] &gt; ret)<br>      ret = maxf + fn[nst] - st[nst];<br>  }<br>  return ret;<br>    }<br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>