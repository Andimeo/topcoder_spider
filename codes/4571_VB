            Public Class TVWatching<br>        Public Function mostMinutes(ByVal programs As String()) As Integer<br>            Dim iPrograms(100, 1) As Integer<br> <br>            Dim i, j, k As Integer<br> <br>            Dim aTimes As New System.Collections.SortedList<br> <br>            For i = 0 To programs.GetUpperBound(0)<br>                iPrograms(i, 0) = iTimeInt(programs(i).Substring(0, 7))<br>                iPrograms(i, 1) = iTimeInt(programs(i).Substring(10, 7))<br>                If iPrograms(i, 1) &lt;= iPrograms(i, 0) Then iPrograms(i, 1) += 1440<br>                If Not aTimes.ContainsKey(iPrograms(i, 0)) Then<br>                    aTimes.Add(iPrograms(i, 0), iPrograms(i, 0))<br>                End If<br>                If Not aTimes.ContainsKey(iPrograms(i, 1)) Then<br>                    aTimes.Add(iPrograms(i, 1), iPrograms(i, 1))<br>                End If<br>            Next<br> <br>            Dim aMaxMins(aTimes.Count, aTimes.Count) As Integer<br>            Dim iMax As Integer<br> <br>            For i = 0 To programs.GetUpperBound(0)<br>                For j = 0 To aTimes.Count - 1<br>                    For k = 0 To aTimes.Count - 1<br>                        If aTimes.GetByIndex(j) &lt;= iPrograms(i, 0) And aTimes.GetByIndex(k) &gt;= iPrograms(i, 1) Then<br>                            iMax = aMaxMins(j, aTimes.IndexOfKey(iPrograms(i, 0))) + aMaxMins(aTimes.IndexOfKey(iPrograms(i, 1)), k) + (iPrograms(i, 1) - iPrograms(i, 0))<br>                            If iMax &gt; aMaxMins(j, k) Then aMaxMins(j, k) = iMax<br>                        End If<br>                    Next<br>                Next<br>            Next<br> <br>            iMax = 0<br> <br>            For j = 0 To aTimes.Count - 1<br>                For k = 0 To aTimes.Count - 1<br>                    If aTimes.GetByIndex(k) - aTimes.GetByIndex(j) &lt;= 1440 Then<br>                        If aMaxMins(j, k) &gt; iMax Then iMax = aMaxMins(j, k)<br>                    End If<br>                Next<br>            Next<br> <br>            Return iMax<br> <br>        End Function<br> <br>        Private Function iTimeInt(ByVal sTime As String) As Integer<br>            Dim iTime As Integer<br>            If sTime.Substring(5, 2) = "PM" Then iTime = 720<br>            If sTime.Substring(0, 2) &lt;&gt; "12" Then<br>                iTime += CInt(sTime.Substring(0, 2)) * 60<br>            End If<br>            Return iTime + CInt(sTime.Substring(3, 2))<br>        End Function<br>    End Class<br></td>