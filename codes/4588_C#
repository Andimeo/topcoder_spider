<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>public class Grader<br>{<br>  public int[] grade( int[] predictedGrades, int[] actualGrades )<br>  {<br>    int[] diff = new int[7];<br> <br>    for( int i = 0; i &lt; 7; i++ ) diff[i] = 0;<br> <br>    for( int i = 0; i &lt; predictedGrades.Length; i++ )<br>    {<br>      int curdiff = predictedGrades[i] - actualGrades[i];<br>      if( curdiff &lt; 0 ) curdiff = -curdiff;<br>    <br>      diff[ curdiff ]++;<br>    }<br> <br>    for( int i = 0; i &lt; 7; i++ )<br>    {<br>      double percent = diff[i] / (double)predictedGrades.Length * 100.00;<br>      diff[i] = (int)percent;<br>    }<br> <br>    return diff;<br>  }<br>}<br></td>