            using System;<br>public class Acronyms<br>  {<br>    string s;<br>    int l;<br>    public string acronize(string[] document)<br>    {<br>      s = document[0];<br>      for(int i=1;i&lt;document.Length;i++)<br>        s += " "+document[i];<br>      l = s.Length;<br> <br>      string[,] store = new string[l,l];<br>      for(int i=0;i&lt;l;i++)<br>        for(int j=0;j&lt;l;j++)<br>          store[i,j] = "";<br>      for(int i=0;i&lt;l;i++)<br>        for(int j=i+1;j&lt;l;j++)<br>          store[i,j] = is_acro(i,j);<br>      string res="";<br>      for(int i=0;i&lt;l;i++)<br>      {<br>        int ai =i;<br>        for(int j=l-1;j&gt;i;j--)<br>        {<br>          if(store[i,j] != "")<br>          {<br>            res += store[i,j];<br>            i=j;<br>            break;<br>          }<br>        }<br>        if(i==ai)<br>          res+=s[i];        <br>      }<br>      return res;<br>    }<br>    public string is_acro(int begin,int end)<br>    {<br>      if(begin==0||begin==1)<br>        return "";<br>      if(s[begin-1]!=' ')<br>        return "";<br>      if(s[begin-1]==' ' &amp;&amp; s[begin-2]==' ')<br>        return "";<br>      if(!char.IsLetter(s[end]))<br>        return "";<br>      if(!char.IsUpper(s[begin]))<br>        return "";<br>      for(int i=begin;i&lt;end;i++)<br>        if(s[i]==' ' &amp;&amp; s[i+1]==' ')<br>          return "";<br>      string r="";<br>      int n=0;<br>      int prev =0;<br>      for(int i=begin;i&lt;=end;i++)<br>      {<br>        if(char.IsUpper(s[i]))<br>        {<br>          r+=s[i].ToString();<br>          n++;<br>          prev =1;<br>          i++;<br>          while(i&lt;=end &amp;&amp; s[i]!=' ')<br>          {<br>            if(char.IsUpper(s[i]))<br>              r += s[i].ToString();<br>            i++;<br>          }<br>        }<br>        else<br>        {<br>          if(prev ==0)<br>            return "";<br>          else<br>          {<br>            prev =0;<br>            i++;<br>            while(i&lt;=end &amp;&amp; s[i]!=' ')<br>            {<br>              //if(char.IsUpper(s[i]))<br>              //  r += s[i].ToString();<br>              i++;<br>            }<br>          }<br> <br>        }<br>      }<br>      if(n&gt;=2 &amp;&amp; prev==1)<br>        return r;<br>      return "";<br> <br> <br> <br> <br>    }<br> <br>  }<br></td>