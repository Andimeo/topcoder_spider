            import static java.lang.Math.*;<br>import java.util.*;<br> <br>public class OneMoreRectangle<br>{<br>    static class Rect<br>    {<br>        int x1;<br>        int y1;<br>        int x2;<br>        int y2;<br>    }<br>    static public int maxCover(String[] rectangles, int qa, int qb)<br>    {<br>        ArrayList&lt;Integer&gt; T = new ArrayList&lt;Integer&gt;();<br>        ArrayList&lt;Integer&gt; R = new ArrayList&lt;Integer&gt;();<br>        ArrayList&lt;Rect&gt; REC = new ArrayList&lt;Rect&gt;();<br>        for (int i = 0; i &lt; rectangles.length; i++)<br>        {<br>            String[] s = rectangles[i].split("\\s");<br>            Rect t = new Rect();<br>            t.x1 = Integer.parseInt(s[0]);<br>            t.y1 = Integer.parseInt(s[1]);<br>            t.x2 = Integer.parseInt(s[2]);<br>            t.y2 = Integer.parseInt(s[3]);<br>            REC.add(t);<br>            if(!T.contains(t.y2))<br>                T.add(t.y2);<br>            if(!R.contains(t.x2))<br>                R.add(t.x2);<br>        }<br>        int m = 0;<br>        for (int t:T)<br>        {<br>            for (int r:R)<br>            {<br>                int ca = 0;<br>                for (Rect re : REC)<br>                {<br>                    if(t&gt;=re.y2 &amp;&amp; r&gt;=re.x2 &amp;&amp; (t-qa)&lt;=re.y1 &amp;&amp; (r-qb)&lt;=re.x1)<br>                    {<br>                        ca++;<br>                    }<br>                }<br>                int cb = 0;<br>                for (Rect re : REC)<br>                {<br>                    if(t&gt;=re.y2 &amp;&amp; r&gt;=re.x2 &amp;&amp; (t-qb)&lt;=re.y1 &amp;&amp; (r-qa)&lt;=re.x1)<br>                    {<br>                        cb++;<br>                    }<br>                }<br>                m = max(m,max(ca,cb)); <br>            }<br>        }<br>        return m;<br>    }<br> <br>}<br></td>