            using System;<br>using System.Collections;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class TriangleType<br>{<br>  public string type(int a, int b, int c)<br>  {<br>    int[] d = new int[3]{a,b,c};<br>    Array.Sort(d);<br>    if (d[0] + d[1] &lt;= d[2]) return "IMPOSSIBLE";<br>    long x = d[0] * d[0] + d[1] * d[1] - d[2] * d[2];<br>    if (x == 0) return "RIGHT";<br>    if (x &lt; 0) return "OBTUSE";<br>    return "ACUTE";<br>  }<br>  <br>  <br>}<br> <br>//Powered by [KawigiEdit]<br></td>