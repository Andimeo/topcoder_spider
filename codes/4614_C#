            using System;<br>using System.Collections;<br>public class ChatExit {<br>  public int[] leaveOrder(string[] numSeen) {<br>    int n = numSeen.Length;<br>    int[,] a = new int[n,n];<br><br>    for (int i = 0; i &lt; n; ++i) {<br>      string[] s = numSeen[i].Split(' ');<br>      for (int j = 0; j &lt; n; ++j)<br>        a[i,j]=int.Parse(s[j]);<br>    }<br><br>    bool[] alive = new bool[n];<br><br>    for (int i = 0; i &lt; n; ++i)<br>      alive[i] = true;<br><br>    int[] lo = new int[n];<br><br>    for (int i = 0; i &lt; n; ++i) {<br>      int k = -1;<br>      for (int j = 0; j &lt; n; ++j)<br>        if (alive[j]) {<br>          int aa = -1;<br>          bool ok = true;<br>          for (int l = 0; l &lt; n; ++l) <br>            if (l != j &amp;&amp; alive[l])<br>              if (aa&lt;0) {<br>                aa = a[l,j];<br>              } else {<br>                if (aa != a[l,j]) {<br>                  ok = false;<br>                  break;<br>                }<br>              }<br>          if (!ok) continue;<br>          for (int ll = 0; ll &lt; n; ++ll)<br>            if (ll != j &amp;&amp; alive[ll]) {<br>              for (int l = 0; l &lt; n; ++l)<br>                if (l != j &amp;&amp; alive[l] &amp;&amp; l != ll)<br>                  if (a[l,ll]&lt;a[j,ll]) {<br>                    ok = false;<br>                    break;<br>                  }<br>              if (!ok) break;<br>            }<br>          if (!ok) continue;<br>          k = j;<br>          break;<br>        }<br>      if (k &lt; 0)<br>        return new int[]{};<br>      lo[i] = k;<br>      alive[k] = false;<br>    }<br><br>    return lo;<br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>