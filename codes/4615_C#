            using System;<br>using System.Collections;<br> <br>  public class FieldPairParse<br>  {<br>    public FieldPairParse()<br>    {<br>    }<br>    <br>    public int[] getPairs(string[] data)<br>    {<br>      return (new Main()).Execute(data);<br>    }<br>  }<br> <br>  public class Main<br>  {<br>    public Main()<br>    {<br>      me = this;<br>    }<br>    <br>    public static Main me;<br> <br>    public int[] dats;<br>    <br>    public int[] Execute(string[] data)<br>    {<br>      int[] ret = new int[0];<br>      dats = new int[data[0].Length];<br>      foreach(string tmp in data)<br>      {<br>        char[] chs = tmp.ToCharArray();<br>        for(int i=0;i&lt;=chs.Length-1;i++)<br>        {<br>          if (chs[i]==' ') dats[i]++;<br>        }<br>      }<br>      int t = 0;<br>      ArrayList al = new ArrayList();<br>      for(int i=0;i&lt;=dats.Length-1;i++)<br>      {<br>        if (dats[i]==data.Length)<br>        {<br>          t++;<br>        }<br>        else<br>        {<br>          if (t&gt;0)<br>          {<br>            al.Add(Convert.ToInt32(t));<br>            t=0;<br>          }<br>        }<br>      }<br>      if ((al.Count%2)!=0)<br>      {<br>                ret = new int[al.Count];                <br>        int i = 0;<br>        foreach(int tmp in al)<br>        {<br>          ret[i] = tmp;<br>          i++;<br>        }<br>      }<br>      return ret;<br> <br>    }<br>  }<br></td>