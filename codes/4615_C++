            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;math.h&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;time.h&gt;<br>#include &lt;ctype.h&gt;<br> <br>using namespace std;<br> <br> <br>class FieldPairParse {<br>public:<br>  vector &lt;int&gt; getPairs(vector &lt;string&gt; data)<br>  {<br>    vector&lt;int&gt; result;<br>    int run=0;<br>    for(int j=0;;j++)<br>    {          <br>      <br>      int i;<br>      for(i=0;i&lt;data.size();i++)<br>      {<br>        <br>        if(j==data[i].size())<br>        {<br>          if(result.size()&amp;1)<br>            return result;<br>          result.clear();<br>          return result;<br> <br>        }<br>        if(data[i][j]!=' ')<br>          break;<br>      }<br>      if(i==data.size())<br>      {<br>        run++;<br>      }<br>      else if(run!=0)<br>      {<br>        result.push_back(run);<br>        run=0;<br>      }<br>    }<br>    <br>    return result;<br>  }<br>};<br></td>