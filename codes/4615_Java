            import java.util.ArrayList;<br><br>public class FieldPairParse {<br>    public int[] getPairs(String[] d) {<br>        int[] res = new int[]{};<br>        ArrayList l = new ArrayList();<br>        int current = 0;<br>        for (int i = 0; i &lt; d[0].length(); i++) {<br>            boolean spaces = true;<br>            <br>            for (int j = 0; j &lt; d.length; j++) {<br>                if(d[j].charAt(i) != ' '){<br>                    spaces = false;<br>                    break;<br>                }<br>            }<br>            <br>            if(spaces){<br>                current++;<br>            }else{<br>                if(current &gt; 0 ){<br>                    l.add(""+current);<br>                    current = 0;<br>                }<br>            }<br>            <br>        }<br>        <br>        if(l.size() % 2 != 0){<br>           res = new int[l.size()];<br>           for (int i = 0; i &lt; res.length; i++) {<br>            res[i] = Integer.parseInt((String)l.get(i));<br>        }<br>        }<br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>