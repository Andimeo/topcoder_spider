            // Powered by radeclipse!<br>import java.util.* ;<br>public class TableSeating {<br>  public double getExpected(int numTables, int[] probs) {<br>    double canseat[] = new double[4096] ;<br>    for (int i=(1&lt;&lt;numTables)-1; i&gt;=0; i--) {<br>      double ss2 = 0 ;<br>      int nnn = 0 ;<br>      for (int siz=0; siz&lt;probs.length; siz++) {<br>  int len = siz + 1 ;<br>  int custmask = (1 &lt;&lt; len) - 1 ;<br>  int nn = 0 ;<br>  double ss = 0 ;<br>  for (int j=0; j&lt;numTables; j++) {<br>    int msk = custmask &lt;&lt; j ;<br>    if (msk &lt; (1 &lt;&lt; numTables) &amp;&amp; (i &amp; msk) == 0) {<br>      ss += len + canseat[i|msk] ;<br>      nn++ ;<br>    }<br>  }<br>  if (nn &gt; 0) {<br>    ss2 += (ss / nn) * probs[siz] ;<br>  }<br>  nnn += probs[siz] ;<br>      }<br>      if (nnn &gt; 0)<br>  canseat[i] = ss2 / nnn ;<br>    }<br>    return canseat[0] ;<br>  }<br>}<br></td>