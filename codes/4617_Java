<td class="problemText" colspan="8" valign="middle" align="left">
            public class BalancedGame {<br>  public int result(String[] conflicts, int p, int q) {<br>    int result = -1;<br>    int N = conflicts.length;<br>    for (int i = 0; i &lt; conflicts.length; i++) {<br>      int win = (int) Math.ceil( (N-1) * (double) p / 100.0);<br>      int lose = (int) Math.ceil( (N-1) * (double) q / 100.0);<br>      int numWin = 0, numLose = 0;<br>      for (int j = 0; j &lt; conflicts[i].length(); j++) {<br>        char c = conflicts[i].charAt(j);<br>        if (c == 'W') numWin++;<br>        else if (c == 'L') numLose++;<br>      }<br>      if (numWin &gt;= win &amp;&amp; numLose &gt;= lose) continue;<br>      else {<br>        result = i; break;<br>      }<br>    }<br>    return result;<br>  }<br>}<br></td>