            #include &lt;iostream&gt;<br>#include &lt;vector&gt;<br><br>using namespace std;<br><br><br>class NumericalSequence {<br>public:<br>   int makePalindrome( vector &lt;int&gt; sequence ) { // caret here<br>      int a=0, b=(int)sequence.size()-1;<br>      int ret = 0;<br>      <br>      while ( a &lt; b ) {<br>         if ( sequence[a] &lt; sequence[b] ) {<br>            sequence[a+1] += sequence[a];<br>            ++a; ++ret;<br>         }<br>         else if ( sequence[a] &gt; sequence[b] ) {<br>            sequence[b-1] += sequence[b];<br>            --b; ++ret;<br>         }<br>         else {<br>            ++a, --b;<br>         }<br>      }<br>      return ret;<br>   }<br><br>   <br> <br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by moj 2.0 [modified TZTester]<br>// Powered by CodeProcessor<br></td>