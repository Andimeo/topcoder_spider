            // Powered by radeclipse!<br>import java.util.* ;<br>public class PieSharing {<br>  int s(int[] pieces) {<br>    // can't pick the first piece<br>    int n = pieces.length; <br>    int[][] v = new int[n][n] ;<br>    for (int i=1; i&lt;n; i++) {<br>      v[i][1] = pieces[i] ;<br>      for (int j=2; j+1&lt;n; j++) {<br>  for (int k=1; k+1&lt;i; k++) {<br>    v[i][j] = Math.max(v[i][j], pieces[i]+v[k][j-1]) ;<br>  }<br>      }<br>    }<br>    int t = n/3 ;<br>    int r = 0 ;<br>    for (int i=0; i&lt;n; i++)<br>      r = Math.max(r, v[i][t]) ;<br>    return r ;<br>  }<br>  public int share(int[] pieces) {<br>    int r = s(pieces) ;<br>    int fp = pieces[0] ;<br>    for (int i=0; i+1&lt;pieces.length; i++)<br>      pieces[i] = pieces[i+1] ;<br>    pieces[pieces.length-1] = fp ;<br>    r = Math.max(r, s(pieces)) ;<br>    return r ;<br>  }<br>}<br></td>