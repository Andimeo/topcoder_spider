            using System.Collections;<br>public class KthElement {<br>  int X(int a){<br>    int r=0;<br>    while (a&gt;0) {r+=a%2;a/=2;};<br>    return r;<br>  }<br>  public int find(int A, int B, int K) {<br>    int x=0;<br>    ArrayList list=new ArrayList();<br>    for (int i=1;i&lt;=K;i++)<br>    {<br>      list.Add(x);<br>      x=A*X(x)+B;<br>      if (list.Contains(x)){<br>        int ind=list.IndexOf((object)x);<br>        return (int)list[ind+(K-i) % (i-ind)];<br>      }<br>    }<br>    return x;<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>