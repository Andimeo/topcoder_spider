            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class KthElement<br>{<br>    public int find(int A, int B, int K)<br>  {<br>    int[] hash = new int [1000000];<br>    int prev = 0;<br>    for(int i = 1 ; i &lt;=K ; i ++)<br>    {<br>      prev = A * ones (prev) + B ;<br>      if( Arrays.binarySearch(hash,prev)&gt;=0)<br>      {<br>        int p = Arrays.binarySearch(hash,prev) + 1;<br>        int more = K - i;<br>        int excess = more % (i-p);<br>        return hash[p+excess-1];<br>      }<br>      hash[i-1] = prev;<br>      //System.out.print( prev + " " );<br>    }<br>    return prev;<br>  }<br>  <br>  public static int ones( int n )<br>  {<br>    int ret = 0;<br>    while(n!=0)<br>    {<br>      if(n%2==1)<br>      {<br>        ret++;<br>      }<br>      n = (n-(n%2))/2;<br>    }<br>    return ret;<br>  }<br>  <br>  <br>}<br> <br>//Powered by [KawigiEdit]<br></td>