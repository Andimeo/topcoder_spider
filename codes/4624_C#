            using System;<br>using System.Collections;<br><br>public class SMS <br>{<br>  string m;<br>  public string compress(string originalMessage) <br>  {<br>    m = originalMessage;<br>    ArrayList output = new ArrayList();<br>    for(int i=0;i&lt;m.Length;i++)<br>    {<br>      if(left(i) &amp;&amp; right(i))<br>      {<br><br>      }<br>      else<br>      {<br>        output.Add(m[i]);<br>      }<br>    }<br>    return new string((char[])output.ToArray(typeof(char)));<br>  }<br>  bool left(int i)<br>  {<br>    if(!isvowel(m[i])) return false;<br>    i--;<br>    while(i &gt;= 0)<br>    {<br>      if(m[i] == ' ') return false;<br>      if(!isvowel(m[i])) return true;<br>      i--;<br>    }<br>    return false;<br>  }<br>  bool right(int i)<br>  {<br>    if(!isvowel(m[i])) return false;<br>     i++;<br>     while(i &lt; m.Length)<br>     {<br>       if(m[i] == ' ') return false;<br>       if(!isvowel(m[i])) return true;<br>       i++;<br>     }<br>     return false;<br>   }<br>  bool isvowel(char c)<br>  {<br>    c = char.ToLower(c);<br>    return c == 'a' || c == 'e' || c == 'i' || c == 'o' || c == 'u';<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>