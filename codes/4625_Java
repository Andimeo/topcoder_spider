            // Powered by radeclipse!<br>import java.util.* ;<br>public class MusicCompilation {<br>  public String[] makeCompilation(String[] artists) {<br>    int n = artists.length ;<br>    int siz[] = new int[n] ;<br>    int tot = 0 ;<br>    Arrays.sort(artists) ;<br>    for (int i=0; i&lt;n; i++) {<br>      String[] f = artists[i].split(" ") ;<br>      artists[i] = f[0] ;<br>      siz[i] = Integer.parseInt(f[1]) ;<br>      tot += siz[i] ;<br>    }<br>    String[] r = new String[tot] ;<br>    int pp = 0 ;<br>    for (int i=0; i&lt;n; i++)<br>      if (siz[i] &gt; 1)<br>  r[pp++] = artists[i] ;<br>    for (int i=0; i&lt;n; i++) {<br>      int m = siz[i] ;<br>      if (siz[i] &gt; 1)<br>  m -= 2 ;<br>      for (int j=0; j&lt;m; j++)<br>  r[pp++] = artists[i] ;<br>    }<br>    for (int i=0; i&lt;n; i++)<br>      if (siz[i] &gt; 1)<br>  r[pp++] = artists[i] ;<br>    return r ;<br>  }<br>}<br></td>