            using System;<br>using System.Collections;<br><br>public class TheCoderMan {<br>  public double evaluateMovie(string yourEvaluations, string[] friendsEvaluations, int maximalDeviation) {<br>    int cnt = 0, sum = 0;<br>    foreach(string eval in friendsEvaluations)<br>    {<br>      if (eval[eval.Length-1] == 'X') continue;<br>      int cmncnt = 0, cmnsum = 0;<br>      for (int i = 0; i &lt; eval.Length-1; i++)<br>      {<br>        if (eval[i]=='X' || yourEvaluations[i]=='X') continue;<br>        int e1 = eval[i]-'0';<br>        int e2 = yourEvaluations[i]-'0';<br>        cmnsum += Math.Abs(e1-e2);<br>        cmncnt++;<br>      }<br>      if (cmncnt == 0) continue;<br>      if (cmnsum &gt; maximalDeviation*cmncnt) continue;<br>      int cm = eval[eval.Length-1]-'0';<br>      sum += cm;<br>      cnt++;<br>    }<br>    if (cnt == 0) return -1;<br>    return (double)sum/cnt;<br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>