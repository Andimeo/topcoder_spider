            import java.util.*;<br> <br>public class TheCoderMan {<br>  public double evaluateMovie(String yourEvaluations, String[] friendsEvaluations, int maximalDeviation) {<br>    int count = 0;<br>    double sum = 0.0;<br>    for (int i = 0; i &lt; friendsEvaluations.length; ++i) {<br>      String eval = friendsEvaluations[i];<br>      if (eval.charAt(eval.length() - 1) == 'X') continue;<br>      <br>      int count2 = 0;<br>      int sum2 = 0;<br>      for (int j = 0; j &lt; yourEvaluations.length() - 1; ++j) {<br>        if (eval.charAt(j) == 'X' || yourEvaluations.charAt(j) == 'X') continue;<br>        int a1 = eval.charAt(j) - '0';<br>        int a2 = yourEvaluations.charAt(j) - '0';<br>        sum2 += Math.abs(a1 - a2);<br>        count2++;<br>      }<br>      if (count2 == 0) continue;<br>      if (sum2 &gt; maximalDeviation * count2) continue;<br>      count++;<br>      sum+=eval.charAt(eval.length() - 1) - '0';<br>    }<br>    <br>    if (count == 0) return -1;<br>    return sum / count;<br>  }<br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>