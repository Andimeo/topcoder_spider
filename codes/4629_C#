            using System;<br>using System.Collections;<br>public class FibonacciSum {<br>  public int howMany(int n) {<br>    int[] fib = new int[100000];<br>    int num = 1;<br>    int a = 0;<br>    int b = 1;<br>    fib[0] = 1;<br>    for (; a + b &lt;= n;)<br>    {<br>      int c = a + b;<br>      fib[num++] = c;<br>      a = b;<br>      b = c;<br>    }<br><br>    int res = 0;<br><br>    for (int i = num - 1; i &gt;= 0; --i)<br>      if (n &gt;= fib[i]) <br>      {<br>        n -= fib[i];<br>        ++res;<br>      }<br><br>    return res;<br>  }<br><br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>