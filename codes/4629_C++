            #include &lt;cstdio&gt;<br>#include &lt;iostream&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;stack&gt;<br>#include &lt;list&gt;<br>#include &lt;queue&gt;<br>#include &lt;deque&gt;<br>#include &lt;cctype&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iterator&gt;<br>using namespace std;<br>typedef long long LL;<br> <br> <br>typedef vector &lt;int &gt; VI;<br>#define REP(i,n) for (int i=0; i&lt;n; ++i)<br>#define FOREACH(it,var) for(__typeof((var).begin()) it=(var.begin()); it!=(var).end(); ++it)<br>#define FOR(var,pocz,koniec) for (int var=pocz; var&lt;=koniec; ++var)<br> <br>#define N 1000<br>LL f[N];<br> <br>    class FibonacciSum<br>        { <br>        public: <br>        int howMany(int n){<br>              f[0]=f[1]=1LL;<br>              FOR(i,2,N-1) f[i]=f[i-1]+f[i-2];<br>              int i=0;<br>              while (f[i]&lt;n) i++;<br>              int ile=0;<br>              while (n){<br>                if (f[i]&lt;=n) {ile++; n-=f[i];}<br>                else i--;<br>              } <br>              return ile;<br>            } <br>        <br> <br>         }; <br> <br>    <br> <br> <br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>