            import java.util.*;<br>public class FibonacciSum {<br>    public int howMany(int n) {<br>        int[] f = new int[1000];<br>        f[0] = 1;<br>        f[1] = 1;<br>        int num =0;<br>        while (n &gt; 0) {<br>            int i = 1;<br>            while (f[i] &lt;= n) {<br>                f[i+1] = f[i]+f[i-1];<br>                i++;<br>            }<br>            i--;<br>            num++;<br>            n-=f[i];<br>        }<br>        return num;<br>    }<br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>