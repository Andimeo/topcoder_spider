            Imports System<br>Imports System.Math<br>Imports Microsoft.VisualBasic<br>Public Class FibonacciSum<br>    Dim fibs() As Integer<br>    Dim fibc As Integer<br>    Public Function howMany(ByVal n As Integer) As Integer<br>        Dim i As Integer<br>        fibc = 0<br>        ReDim fibs(30)<br>        getFib(1, 1)<br>        fibc = fibc - 1<br>        howMany = CountF(n)<br>        'Console.WriteLine(fibc)<br>    End Function<br>    Public Function CountF(ByVal n As Integer) As Integer<br>        Dim i As Integer<br> <br>        For i = fibc To 0 Step -1<br>            If fibs(i) = n Then<br>                CountF = 1<br>                Exit Function<br>            End If<br>            If fibs(i) &lt; n Then<br>                n = n - fibs(i)<br>                CountF = 1 + CountF(n)<br>                Exit Function<br>            End If<br>        Next<br>    End Function<br>    Public Sub getFib(ByVal x1 As Integer, ByVal x2 As Integer)<br>        fibs(fibc) = x2<br>        fibc += 1<br>        If x2 &lt; 1000000 Then<br>            getFib(x2, x1 + x2)<br>        End If<br>    End Sub<br>End Class<br></td>