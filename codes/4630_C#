            using System;<br>using System.Collections;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class RectangleError<br>{<br>  public double bottomRange(double topMin, double topMax, double leftMin, double leftMax, double rightMin, double rightMax)<br>  {<br>    double y1 = Math.Max(leftMax - rightMin, rightMax - leftMin);<br>    double y2 = Math.Min(leftMax - rightMin, rightMax - leftMin);<br>    if (leftMin &lt; rightMax &amp;&amp; rightMin &lt; leftMax) y2 = 0;<br>    return Math.Sqrt(topMax*topMax + y1*y1) - Math.Sqrt(topMin*topMin + y2*y2);<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit]<br></td>