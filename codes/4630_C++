            #include &lt;stdio.h&gt;<br>#include &lt;vector&gt;<br>#include &lt;map&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;string.h&gt;<br>#include &lt;ctype.h&gt;<br>#include &lt;string&gt;<br>#include &lt;math.h&gt;<br> <br>using namespace std;<br> <br>typedef vector&lt;int&gt; VI;<br>typedef vector&lt;string&gt; VS;<br>typedef vector&lt;VI&gt; VII;<br> <br>class RectangleError {<br>  public:<br>  double bottomRange(double topMin, double topMax, double leftMin, double leftMax, double rightMin, double rightMax) {<br> <br>    double max = hypot(topMax, leftMin-rightMax) &gt;? hypot(topMax, leftMax-rightMin);<br> <br>    double y;<br>    if(rightMin &gt;= leftMax)<br>      y = rightMin - leftMax;<br>    else<br>      if(leftMin &gt;= rightMax)<br>        y = rightMax - leftMin;<br>      else<br>        y = 0;<br> <br>    double min = hypot(topMin, y);<br>    return max-min;<br>  }<br>};<br> <br> <br>// Powered by FileEdit<br></td>