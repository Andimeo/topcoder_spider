            Public Class RectangleError<br>        Public Function bottomRange(ByVal topMin As Double, ByVal topMax As Double, ByVal leftMin As Double, ByVal leftMax As Double, ByVal rightMin As Double, ByVal rightMax As Double) As Double<br>            Dim i, j, k, l As Long<br> <br>            Dim iMin, iMax As Integer<br> <br>            Dim dMinLen, dMaxLen As Double<br>            dMinLen = 0<br>            dMaxLen = 0<br> <br>            If leftMin &gt;= rightMax Then<br>                dMinLen = System.Math.Sqrt(topMin ^ 2 + (leftMin - rightMax) ^ 2)<br>            End If<br> <br>            If rightMin &gt;= leftMax Then<br>                dMinLen = System.Math.Sqrt(topMin ^ 2 + (rightMin - leftMax) ^ 2)<br>            End If<br> <br>            If dMinLen = 0 Then<br>                dMinLen = topMin<br>            End If<br> <br>            dMaxLen = System.Math.Abs(rightMax - leftMin)<br>            dMaxLen = System.Math.Max(System.Math.Abs(rightMax - leftMax), dMaxLen)<br>            dMaxLen = System.Math.Max(System.Math.Abs(rightMin - leftMin), dMaxLen)<br>            dMaxLen = System.Math.Max(System.Math.Abs(rightMin - leftMax), dMaxLen)<br> <br>            dMaxLen = System.Math.Sqrt(topMax ^ 2 + dMaxLen ^ 2)<br> <br>            Return dMaxLen - dMinLen<br> <br>        End Function<br>    End Class<br></td>