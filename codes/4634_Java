            import java.util.*;<br>import java.util.regex.*;<br> <br> <br>public class TestingCar {<br> <br> <br>  public double maximalSpeed(String[] restrictions, int duration, int acceleration) {<br>    int[] bestSpeed = new int[1001];<br>    int[] maxSpeed = new int[1001];<br>    int[] restriction = new int[1001];<br>    <br>    Arrays.fill(maxSpeed, 1000000);<br>    Arrays.fill(bestSpeed, 1000000);<br>    Arrays.fill(restriction, 1000000);<br>    <br>    for (String s : restrictions) {<br>      String[] ss = s.split(" ");<br>      int speed = Integer.parseInt(ss[0]);<br>      int start = Integer.parseInt(ss[1]);<br>      int end = Math.min(start + Integer.parseInt(ss[2]), duration);<br>      for (int i = start; i &lt;= end; i++) {<br>        maxSpeed[i] = Math.min(maxSpeed[i], speed);<br>      }<br>      for (int i = start; i &lt; end; i++) {<br>        restriction[i] = Math.min(restriction[i], speed);<br>      }<br>    }<br>    maxSpeed[0] = 0;<br>    <br>    for (int i = 0; i &lt;= 1000; i++) {<br>      int best = 1000000;<br>      for (int j = 0; j &lt;= 1000; j++) {<br>        best = Math.min(best, acceleration * Math.abs(j - i) + maxSpeed[j]);<br>      }<br>      bestSpeed[i] = best;<br>    }<br> <br> <br> <br> <br>    double best = 0;<br>    for (int i = 0; i &lt; duration; i++) {<br>      int k = bestSpeed[i];<br>      int j = bestSpeed[i + 1];<br>      best = Math.max(<br>          Math.min(restriction[i], Math.max(k, j) + ((double)(acceleration - Math.abs(k - j))) / 2), <br>          best);<br> <br>    }<br>    best = Math.max(best, bestSpeed[duration]);<br>    return best;<br>  }<br> <br> <br>  <br>}<br> <br> <br> <br> <br> <br>// Powered by CodeProcessor<br></td>