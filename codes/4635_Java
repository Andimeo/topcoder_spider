            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import static java.lang.Math.*;<br>import static java.util.Arrays.*;<br>public class PartyGame {<br>  char[] map;<br>  int n;<br>  public double numberOfMoves(String[] field, int N) {<br>    n = N;<br>    String s = "";<br>    for(int i = 0; i &lt; field.length; i++){<br>      s += field[i];<br>    }<br>    map = s.toCharArray();<br>    memo = new double[map.length+1];<br>    return doit(0);<br>  }<br>  double[] memo;<br>  double doit(int at){<br>    if(at &gt;= map.length-1) return 0;<br>    if(map[at] == 'R') return -1;<br>    if(memo[at] != 0) return memo[at];<br>    int nogos = 0;<br>    double sum = 0;<br>    for(int i = 1; i &lt;= n; i++){<br>      double res = doit(at+i);<br>      if(res == -1) nogos++;<br>      else sum += 1+res;<br>    }<br>    double ret;<br>    if(nogos == n) ret = -1;<br>    else ret = (sum+nogos)/(n-nogos);<br>    memo[at] = ret;<br>    return ret;<br>  }<br> <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>