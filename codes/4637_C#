            using System;<br>using System.Collections;<br><br>public class Task : IComparable<br>{<br>  int h, m;<br>  public string name;<br><br>  public Task(int h, int m, string name)<br>  {<br>    this.h = h;<br>    this.m = m;<br>    this.name = name;<br>  }<br><br>  public int CompareTo(object obj)<br>  {<br>    Task t = (Task)obj;<br>    if (this.h &lt; t.h) return -1;<br>    if (this.h &gt; t.h) return 1;<br>    return this.m - t.m;<br>  }<br>}<br><br>public class DayPlanner {<br>  public string getEnds(string[] tasks) {<br>    ArrayList al = new ArrayList();<br>    foreach(string s in tasks)<br>    {<br>      string h = s.Substring(0,2);<br>      string m = s.Substring(3,2);<br>      string task = s.Substring(6);<br>      al.Add(new Task(int.Parse(h), int.Parse(m), task));<br>    }<br>    al.Sort();<br>    Task t1 = (Task)al[0], t2 = (Task)al[al.Count-1];<br>    return t1.name + "-" + t2.name;<br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>