            public class DayPlanner<br> <br>public Function getEnds(tasks as String()) as String<br> <br>  dim i as integer<br>        dim mint as long<br>        dim tm(49) as long<br>       dim hr as string<br>       dim mn as string<br>       dim hr1 as long<br>  for i=0 to tasks.length -1<br>     hr=tasks(i).Substring(0,2)<br>           mn=tasks(i).Substring(3,2)<br>     <br>     hr1=Cint(hr)*60<br>           hr1=hr1+Cint(mn)<br>           tm(i)=Cint(hr1)<br>       next i<br>       <br>       dim min as long<br>       dim max as long<br>  dim mini as long<br>       dim maxi as long<br>      min=1439<br>      max=0<br>      for i=0 to tasks.length-1<br>        if min&gt;Cint(tm(i)) then               <br>             min=Cint(tm(i))<br>      mini=i<br>        end if<br> <br>  if max&lt;Cint(tm(i)) then<br>             max=Cint(tm(i))<br>             maxi=i<br>        end if<br>   next i<br>dim rstr as string<br>rstr=tasks(mini).Substring(6,(tasks(mini).length-6)) &amp;"-"<br>rstr=rstr &amp; tasks(maxi).Substring(6,(tasks(maxi).length-6))<br> <br>getEnds=rstr<br>end function<br>end class<br></td>