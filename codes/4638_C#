            using System;<br>using System.Collections;<br>public class RecipeFraction {<br>  public double getFraction(string[] r, string[] ing) {<br>    int a=0;<br>    for(int i=0;i&lt;r.Length;i++)<br>    {<br>      string[] t=r[i].Split();<br>      a+=int.Parse(t[0]);<br>    }<br>    int b=0;<br>    for(int i=0;i&lt;ing.Length;i++)<br>    {<br>      for(int j=0;j&lt;r.Length;j++)<br>      {<br>        string[] t=r[j].Split();<br>        if(t[1]==ing[i])<br>          b+=int.Parse(t[0]);<br>      }<br>    }<br>    return (double)b/(double)a;<br><br><br><br><br>  }<br><br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003], modified to support C#<br>// Powered by CodeProcessor<br></td>