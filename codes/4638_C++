            #include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;string&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br> <br>using namespace std;<br> <br>#define rep(i,n) for(int i=0; i&lt;(n); i++)<br>typedef vector&lt;int&gt; vi;<br> <br>class RecipeFraction {<br>public:<br>  double getFraction(vector &lt;string&gt;, vector &lt;string&gt;);<br>};<br> <br>double RecipeFraction::getFraction(vector &lt;string&gt; recipe, vector &lt;string&gt; ingredients) {<br>  int tot = 0;<br>  int occ = 0;<br>  <br>  rep(i,recipe.size())<br>  {<br>    istringstream st(recipe[i]);<br>    string s;<br>    int a;<br>    st &gt;&gt; a &gt;&gt; s;<br>    tot += a;<br>    rep(j,ingredients.size()) if (ingredients[j]==s) {<br>      occ += a;<br>      break;<br>    }<br>  }<br>  <br>  return ((double)occ)/tot;<br>}<br> <br> <br>//Powered by [KawigiEdit]<br></td>