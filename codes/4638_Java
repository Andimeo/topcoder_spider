            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class RecipeFraction<br>{<br>  public double getFraction(String[] recipe, String[] ingredients)<br>  {<br>    double total=0.0, used=0.0;<br>    for (int i=0; i&lt;recipe.length; i++) {<br>      String[] sp = recipe[i].split(" ");<br>      double item = Double.parseDouble(sp[0]);<br>      total += item;<br>      for (int j=0; j&lt;ingredients.length; j++) if (ingredients[j].equals(sp[1])) { used+=item; break; }<br>    }<br>    return used/total;<br>  }<br>}<br></td>