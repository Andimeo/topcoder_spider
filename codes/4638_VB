            Public Class RecipeFraction<br>        Public Function getFraction(ByVal recipe As String(), ByVal ingredients As String()) As Double<br>            Dim i, j, k, l As Long<br> <br>            Dim iTot As Integer<br>            Dim dRet As Double = 0<br> <br>            For i = 0 To recipe.GetUpperBound(0)<br>                iTot += CInt(recipe(i).Split(" ")(0))<br>            Next<br> <br>            Dim s As String<br> <br>            For i = 0 To recipe.GetUpperBound(0)<br>                s = recipe(i).Split(" ")(1)<br>                For j = 0 To ingredients.GetUpperBound(0)<br>                    If s = ingredients(j) Then<br>                        dRet += CInt(recipe(i).Split(" ")(0)) / iTot<br>                    End If<br>                Next<br>            Next<br> <br>            Return dRet<br> <br>        End Function<br>    End Class<br></td>