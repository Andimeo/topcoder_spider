            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br><br>#define Fu(i,min,ma,inc) for(int i=min;i&lt;ma;i+= inc)<br>#define Fd(i,ma,min,dec) for(int i=ma;i&gt;=min;i-= dec)<br>#define sz size()<br>#define pb push_back<br>#define vi vector&lt; int &gt;<br>#define vvi vector&lt; vector&lt; int &gt; &gt;<br>#define vs vector&lt; string &gt;<br>#define vvs vector &lt; vs &gt;<br>#define vd vector&lt; double &gt;<br>#define vvd vector&lt; vd &gt;<br>#define ld long double<br>#define ll long long<br>#define vb vector&lt; bool &gt;<br>#define bn begin()<br>#define en end()<br><br>using namespace std;<br><br>class ProseFlip {<br>public:<br><br>vector &lt;string&gt; getFlip(vector &lt;string&gt; t) {<br>  vs ans(t[0].sz,"");<br>  Fu(i,0,t[0].sz,1){<br>    Fu(j,0,t.sz,1){<br>      ans[i]+=t[j][i];<br>    }<br>  }<br>  return ans;<br>}<br><br>};<br><br>//Powered by [LSEdit] 1.1<br></td>