            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;iostream&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;numeric&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;queue&gt;<br>#include &lt;cmath&gt;<br>#include &lt;algorithm&gt;<br><br>using namespace std;<br>typedef pair&lt;int,int&gt; ip;<br><br>class ModeProbability {<br>public:<br>  double getProb(vector &lt;int&gt; probs, int n, int value) {<br>    double fac[20];<br>    fac[0]=1;<br>    for (int i=1;i&lt;20;i++){<br>      fac[i]=fac[i-1]*i;<br>    }<br>    if (value) swap(probs[0],probs[value]);<br>    while (probs.size()&lt;5) probs.push_back(0);<br>    double sumpr=0.0;<br>    for (int a=0;a&lt;=15;a++){<br>      for (int b=0;b&lt;a;b++){<br>  for (int c=0;c&lt;a;c++){<br>    for (int d=0;d&lt;a;d++){<br>      int e = n-a-b-c-d;<br>      if (e&lt;0) break;<br>      if (e&gt;=a) continue;<br>      double f=1.0;<br>      for (int i=0;i&lt;a;i++)<br>        f*=probs[0]/100.0;<br>      for (int i=0;i&lt;b;i++)<br>        f*=probs[1]/100.0;<br>      for (int i=0;i&lt;c;i++)<br>        f*=probs[2]/100.0;<br>      for (int i=0;i&lt;d;i++)<br>        f*=probs[3]/100.0;<br>      for (int i=0;i&lt;e;i++)<br>        f*=probs[4]/100.0;<br>      f*=fac[n];<br>      f/=fac[a];<br>      f/=fac[b];<br>      f/=fac[c];<br>      f/=fac[d];<br>      f/=fac[e];<br>      sumpr+=f;<br>    }<br>  }<br>      }<br>    }<br>    return sumpr;<br>  }<br>};<br><br><br>// Powered by FileEdit<br></td>