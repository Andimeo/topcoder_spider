            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class SplitSubgraphs<br>{<br>    bool[,] a = new bool[128, 128];<br>    public int howMany(string[] graph)<br>    {<br>        int i, j;<br>        int n = graph.Length;<br>        for (i = 0;i&lt;n;i++)<br>            for (j = 0;j&lt;n;j++)<br>                a[i,j ]= (graph[i][j] =='1');<br> <br>        int nn = 1&lt;&lt;n;<br>        bool[] aa = new bool[nn];<br>        bool[] bb = new bool[nn];<br>        for (i=0;i&lt;nn;i++)<br>        {<br>            aa[i]=true;<br>            bb[i]=true;<br>            for (j = 0; j &lt; n; j++)<br>                for (int k = j + 1; k &lt; n; k++)<br>                    if ((i &amp; (1 &lt;&lt; j)) &gt; 0 &amp;&amp; (i &amp; (1 &lt;&lt; k)) &gt; 0)<br>                    {<br>                        if (a[j, k] == false) { aa[i] = false; }<br>                        if (a[j, k] == true) { bb[i] = false; }<br>                    }<br>            if (i == 0) aa[i] = false;<br>            if (i == 0) bb[i] = false;<br>        }<br>        List&lt;int&gt; A = new List&lt;int&gt;();<br> <br>        for (i = 0; i &lt; nn; i++) if (aa[i]) A.Add(i);<br>        List&lt;int&gt; B = new List&lt;int&gt;();<br> <br>        for (i = 0; i &lt; nn; i++) if (bb[i]) B.Add(i);<br> <br>        bool[] ok = new bool[nn];<br>        foreach(int ii in A)<br>            foreach(int jj in B)<br>                if ((ii &amp; jj) == 0)<br>                {<br>                    ok[ii + jj] = true;<br>                }<br>        int ans = 0;<br>        for (i = 0; i &lt; nn; i++) if (ok[i]) ans++;<br>        return ans;<br>    }}<br></td>