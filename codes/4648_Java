            import java.util.*;<br>import java.math.*;<br>import java.io.*;<br>import java.text.*;<br>import java.util.regex.*;<br>import static java.util.Arrays.*;<br>import static java.lang.Math.*;<br>import static java.lang.Integer.parseInt;<br>public class SplitSubgraphs {<br>  public int howMany(String[] graph) {<br>    g = new int[graph.length];<br>    for(int i = 0; i &lt; graph.length; i++){<br>      for(int j = 0; j &lt; graph[i].length(); j++){<br>        if(graph[i].charAt(j) == '1') g[i] |= (1&lt;&lt;j);<br>      }<br>    }<br>    int ret = 0;<br>    for(int i = 0; i &lt; (1&lt;&lt;g.length); i++){<br>      if(split(0, 0, i)) ret++;<br>    }<br>    return ret;<br>  }<br>  int[] g;<br>  boolean split(int at, int a, int b){<br>    if(at == g.length) return a != 0 &amp;&amp; b != 0;<br>    if(((1&lt;&lt;at)&amp;b)==0) return split(at+1, a, b);<br>    if( ((g[at] &amp; b) == (b ^ (1&lt;&lt;at))) &amp;&amp; a != 0){<br>      return split(at+1, a, b);<br>    }<br>    int precb = ((1&lt;&lt;at)-1) &amp; b;<br>    if( (g[at] &amp; precb) != precb){<br>      if( (g[at] &amp; a) != 0) return false;<br>      return split(at+1, a|(1&lt;&lt;at), b ^ (1&lt;&lt;at));<br>    }<br>    if((g[at] &amp; a) != 0) return split(at+1, a, b);<br>    return split(at+1, a|(1&lt;&lt;at), b ^ (1&lt;&lt;at)) || split(at+1, a, b);<br>  }<br> <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>