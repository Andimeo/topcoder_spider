            import java.util.*;<br>public class TheTournament {<br>  public String findLeader (String[] m) {<br>  <br>    String[] names = new String[m.length*2];<br>    int c = 0;<br>    for(int i = 0; i &lt; m.length; i++) {<br>      StringTokenizer tok = new StringTokenizer(m[i], " ");<br>      String a = tok.nextToken();<br>      String emp = tok.nextToken();<br>      String b = tok.nextToken();<br>      boolean found = false;<br>      boolean found2 = false;<br>      for(int j = 0; j &lt; c; j++) {<br>        if(a.equals(names[j])) found = true;<br>        if(b.equals(names[j])) found2 = true;<br>      }<br>      if(!found) names[c++] = a;<br>      if(!found2) names[c++] = b;<br>    }<br>    <br>    String[] allN = new String[c];<br>    for(int i = 0; i &lt; c; i++) allN[i] = names[i];<br>    Arrays.sort(allN);<br>    <br>    int[] wins = new int[c];<br>    int[] total = new int[c];<br>    for(int i = 0; i &lt; c; i++) {wins[i] = 0; total[i]=0;}<br>    <br>    for(int i = 0; i &lt; m.length; i++) {<br>      StringTokenizer t = new StringTokenizer(m[i], " ");<br>      int p1 = 0; int p2 = 0;<br>      String a = t.nextToken();<br>      String emp = t.nextToken();<br>      String b = t.nextToken();<br>      for(int j = 0; j &lt; c; j++) {<br>        if(allN[j].equals(a)) p1 = j;<br>        if(allN[j].equals(b)) p2 = j;<br>      }<br>      wins[p1]++; total[p1]++; total[p2]++;<br>    }<br>    <br>    double a = 0.0;<br>    String ret = "";<br>    for(int i = 0; i &lt; c; i++) {<br>      if(total[i] &gt; 0) {<br>        if((double)wins[i] / (double)total[i] &gt; a) {<br>          a = (double)wins[i] / (double)total[i];<br>          ret = allN[i];<br>        }<br>      }<br>    }<br>    return ret;<br>  }<br>}<br></td>