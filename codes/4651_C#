            using System;<br>using System.Collections;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class SpreadsheetColumn<br>{<br>  public string getLabel(int column)<br>  {<br>    if(column&lt;=26)<br>      return new string((char)(column+'A'-1),1);<br>    column -=27;<br>    char c1 = (char)(column/26);<br>    char c2 = (char)(column%26);<br>    return new string(new char[]{(char)(c1+'A'),(char)(c2+'A')});<br>  }<br> <br>}<br> <br>//Powered by [KawigiEdit]<br></td>