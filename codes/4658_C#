            using System;<br>using System.Collections;<br> <br>public class CountriesRanklist<br>{<br>  class C : IComparable<br>  {<br>    public string c;<br>    public int score;<br>    public int n;<br>    public int apos, bpos;<br>    <br>    public int CompareTo(object o)<br>    {<br>      C qq = o as C;<br>      <br>      return string.CompareOrdinal(this.c, qq.c);<br>    }<br>  }<br>  <br>  public string[] findPlaces(string[] knownPoints)<br>  {<br>    C[] q = new C[100];<br>    int qs=0;<br>    int low=1000000;<br>    <br>    foreach(string s in knownPoints)<br>    {<br>      string[] ss = s.Split(' ');<br>      <br>      int i;<br> <br>      for(i=0; i&lt;qs; i++)<br>        if(q[i].c.CompareTo(ss[0])==0)<br>          break;<br>      if(i==qs)<br>      {<br>        q[qs]=new C();<br>        q[qs].c=ss[0];<br>        q[qs].score=0;<br>        q[qs].n=0;<br>        qs++;<br>      }<br>      <br>      q[i].n++;<br>      q[i].score+=int.Parse(ss[2]);<br>      <br>      if(int.Parse(ss[2])&lt;low)<br>        low=int.Parse(ss[2]);<br>    }<br>    <br>    low--;<br>    <br>    Array.Sort(q, 0, qs);<br>    <br>    for(int i=0; i&lt;qs; i++)<br>    {<br>      int num=1;<br>      <br>      for(int j=0; j&lt;qs; j++)<br>        if(i!=j)<br>          if(q[i].score+(4-q[i].n)*low &lt; q[j].score)<br>            num++;<br>      <br>      q[i].apos=num;<br>      <br>      num=1;<br> <br>      for(int j=0; j&lt;qs; j++)<br>        if(i!=j)<br>          if(q[i].score &lt; q[j].score+(4-q[j].n)*low)<br>            num++;<br>      <br>      q[i].bpos=num;<br>    }<br>    <br>    string[] aa = new string[qs];<br>    <br>    for(int i=0; i&lt;qs; i++)<br>      aa[i] = q[i].c+" "+q[i].apos+" "+q[i].bpos;<br>    <br>    return aa;<br>  }<br>}<br> <br> <br>// Powered by FileEdit<br></td>