            import java.util.*;<br> <br>public class CountriesRanklist {<br>    public String[] findPlaces(String[] knownPoints) {<br>        int n = 0;<br>        String[] ctr = new String[knownPoints.length];<br>        int[] sum = new int[knownPoints.length];<br>        int[] ptp = new int[knownPoints.length];<br>        int[] ltsc = new int[knownPoints.length];<br>        int[] gtsc = new int[knownPoints.length];<br>        int min = Integer.MAX_VALUE;<br>        for (int i = 0; i &lt; knownPoints.length; i++) {<br>            Scanner sc = new Scanner(knownPoints[i]);<br>            String country = sc.next();<br>            String name = sc.next();<br>            int points = sc.nextInt();<br>            if (points &lt; min) {<br>                min = points;<br>            }<br>            int k = -1;<br>            for (int j = 0; j &lt; n; j++) {<br>                if (ctr[j].equals(country)) {<br>                    k = j;<br>                    break;<br>                }<br>            }<br>            if (k == -1) {<br>                n++;<br>                ctr[n - 1] = country;<br>                k = n - 1;<br>            }<br>            sum[k] += points;<br>            ptp[k]++;<br>        }<br> <br>        for (int i = 0; i &lt; n; i++) {<br>            ltsc[i] = sum[i];<br>            gtsc[i] = sum[i] + (min - 1) * (4 - ptp[i]);<br>        }<br>        int[] bp = new int[n];<br>        int[] wp = new int[n];<br>        List&lt;String&gt; ans = new ArrayList&lt;String&gt;();<br>        for (int i = 0; i &lt; n; i++) {<br>            for (int j = 0; j &lt; n; j++) {<br>                if (i != j &amp;&amp; ltsc[j] &gt; gtsc[i]) {<br>                    bp[i]++;<br>                }<br>                if (i != j &amp;&amp; gtsc[j] &gt; ltsc[i]) {<br>                    wp[i]++;<br>                }<br>            }<br>            ans.add(ctr[i] + " " + (bp[i] + 1) + " " + (wp[i] + 1));<br>        }<br>        String[] r = ans.toArray(new String[n]);<br>        Arrays.sort(r);<br>        return r;<br>    }<br> <br> <br>}<br> <br> <br>// Powered by PopsEdit<br> <br>// Powered by CodeProcessor<br></td>