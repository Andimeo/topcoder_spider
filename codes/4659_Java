            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class SequenceOfNumbers<br>{<br>  public String[] rearrange(String[] sequence)<br>  {<br>    int n = sequence.length;<br>    int[] ret = new int[n];<br>    for(int i = 0 ; i &lt; n ; i ++)<br>    {<br>      ret[i] = Integer.parseInt( sequence[i] );<br>    }<br>    Arrays.sort(ret);<br>    String[] r = new String[n];<br>    for(int i = 0 ; i &lt; n ; i ++)<br>    {<br>      r[i]=""+ret[i];<br>    }<br>    return r;<br>  }<br>  <br>  <br>}<br> <br>//Powered by [KawigiEdit]<br></td>