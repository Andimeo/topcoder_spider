            using System;<br>using System.Collections;<br><br>public class PrimeStatistics <br>{<br>  public int mostCommonRemainder(int lowerBound, int upperBound, int modulo) <br>  {<br>    ArrayList primes = new ArrayList();<br>    primes.Add(2);<br>    for(int i=3;i&lt;=upperBound;i+=2)<br>    {<br>      bool prime = true;<br>      for(int j=3;j*j&lt;=i;j++)<br>      {<br>        if(i%j == 0) <br>        {<br>          prime = false;<br>          break;<br>        }<br>      }<br>      if(prime) primes.Add(i);<br>    }<br>    int[] freq = new int[modulo];<br>    foreach(int i in primes)<br>    {<br>      if(lowerBound &lt;= i &amp;&amp; i &lt;= upperBound)<br>      {<br>        freq[i % modulo] ++;<br>      }<br>    }<br>    int best = 0;<br>    for(int i=1;i&lt;modulo;i++)<br>    {<br>      if(freq[best] &lt; freq[i]) best = i;<br>    }<br>    return best;<br><br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>