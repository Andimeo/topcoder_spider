            #include&lt;iostream&gt;<br>#include&lt;vector&gt;<br>#include&lt;string&gt;<br>using namespace std;<br> <br>int p[40000];<br> <br>class PrimeStatistics{<br>public:<br>  int mostCommonRemainder(int l, int u, int m)<br>  {<br>    int i,j,k,s[1000];<br>    memset(s,0,sizeof(s));<br>    if(p[0]!=2){<br>      i=1;<br>      p[0]=2;<br>      for(j=3;j&lt;201000;j+=2){<br>        for(k=0;k&lt;i;k++) if(j%p[k]==0) break;<br>        if(k==i) p[i++]=j;<br>      }<br>    }<br>    k=0;<br>    for(i=0;p[i]&lt;=u;i++){<br>      if(p[i]&lt;l) continue;<br>      s[p[i]%m]++;<br>    }<br>    for(i=1;i&lt;m;i++) if(s[k]&lt;s[i]) k=i;<br>    return k;<br>  }<br>};<br></td>