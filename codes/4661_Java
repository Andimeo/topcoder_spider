            // Powered by radeclipse!<br>import java.util.* ;<br>public class PrimeStatistics {<br>boolean isprime(long a) {<br>   if (a &lt; 2) return false ;<br>   if (a &lt; 4) return true ;<br>   if (a % 2 == 0) return false ;<br>   long b ;<br>   for (b=3; b*b&lt;=a; b+=2)<br>      if (a % b == 0)<br>         return false ;<br>   return true ;<br>}<br>  public int mostCommonRemainder(int lowerBound, int upperBound, int modulo) {<br>    int modset[] = new int[1000] ;<br>    for (int i=lowerBound; i&lt;=upperBound; i++)<br>      if (isprime(i))<br>  modset[i%modulo]++ ;<br>    int r = 0 ;<br>    for (int i=1; i&lt;modulo; i++)<br>      if (modset[i] &gt; modset[r])<br>  r = i ;<br>    return r ;<br>  }<br>}<br></td>