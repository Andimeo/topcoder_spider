<td class="problemText" colspan="8" valign="middle" align="left">
            Public Class PrimeStatistics<br>        Public Function mostCommonRemainder(ByVal lowerBound As Integer, ByVal upperBound As Integer, ByVal modulo As Integer) As Integer<br>            Dim i, j, k, l As Long<br> <br>            Dim aMod(modulo) As Integer<br>            Dim iBest As Integer<br>            Dim iBestIdx As Integer<br>            Dim bPrime As Boolean<br> <br>            If lowerBound = 1 Then lowerBound += 1<br> <br>            For i = lowerBound To upperBound<br>                bPrime = True<br>                For j = 2 To System.Math.Floor(System.Math.Sqrt(i))<br>                    If i Mod j = 0 Then<br>                        bPrime = False<br>                        Exit For<br>                    End If<br>                Next<br>                If bPrime Then aMod(i Mod modulo) += 1<br>            Next<br> <br>            iBest = -1<br> <br>            For i = 0 To modulo - 1<br>                If aMod(i) &gt; iBest Then<br>                    iBest = aMod(i)<br>                    iBestIdx = i<br>                End If<br>            Next<br> <br>            Return iBestIdx<br> <br>        End Function<br>    End Class<br></td>