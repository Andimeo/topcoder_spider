            using System;<br>using System.Collections;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class DigitFiller<br>{<br>    public long howMany(string k, int num)<br>    {<br>        int n = k.Length;<br>        long[,] d = new long[n + 1, num];<br>        d[0, 0] = 1;<br>        for (int i = 0; i &lt; k.Length; i++)<br>        {<br>            for (int j = 0; j &lt; num; j++)<br>                if (d[i, j] &gt; 0)<br>                {<br>                    if (k[i] == 'X')<br>                    {<br>                        for (int q = 0; q &lt; 10; q++)<br>                        {<br>                            d[i + 1, (j * 10 + q) % num] += d[i, j];<br>                        }<br>                    }<br>                    else<br>                    {<br>                        d[i + 1, (j * 10 + (k[i] - '0')) % num] += d[i, j];<br>                    }<br>                }<br>        }<br> <br>        return d[n, 0];<br>    }<br>}<br> <br>//Powered by [Zigger]<br></td>