            #include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cmath&gt;<br>#include &lt;queue&gt;<br>using namespace std;<br>typedef vector&lt;int&gt; VI;  typedef vector&lt;vector&lt;int&gt; &gt; VVI;<br>typedef vector&lt;string&gt; VS;  typedef vector&lt;vector&lt;string&gt; &gt; VVS;<br>typedef signed long long i64;  typedef unsigned long long u64;<br><br>i64 n[10001], n2[10001];<br><br>class DigitFiller {<br>public:<br>long long howMany(string s, int num) {<br>  int i, j, k, x, y, z;<br>  long long ret;<br>  n[0] = 1;<br>  for( i = 0; i &lt; s.size(); i++ ) {<br>    memset(n2, 0, sizeof(n2));<br>    for( j = 0; j &lt; num; j++ )<br>    for( k = 0; k &lt; 10; k++ ) {<br>      if( isdigit(s[i]) &amp;&amp; s[i]-'0' != k ) continue;<br>      n2[((j*10)+k)%num] += n[j];<br>    }<br>    memcpy(n, n2, sizeof(n));<br>  }<br>  return n[0];<br>}<br>};<br><br><br>// Powered by FileEdit<br></td>