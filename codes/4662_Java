            import java.util.*;<br>import java.util.regex.*;<br>import java.io.*;<br><br>public class DigitFiller {<br>  public long howMany(String k, int num) {<br>    ut = System.out;<br>    long[] dp = new long[num];<br>    dp[0] = 1;<br>    int base = 1;<br>    for (int i=k.length()-1; i&gt;=0; --i) {<br>      char c = k.charAt(i);<br>      long[] ny = new long[num];<br>      if (c=='X') {<br>        for (int x=0; x&lt;10; ++x) {<br>          for (int j=0; j&lt;num; ++j) {<br>            ny[(j+x*base)%num] += dp[j];<br>          }<br>        }<br>      } else {<br>        int e = (c-'0')*base;<br>        for (int j=0; j&lt;num; ++j) {<br>          ny[(j+e)%num] = dp[j];<br>        }<br>      }<br>      dp = ny;<br>      base = (10*base)%num;<br>    }<br>    return dp[0];<br>  }<br><br>  PrintStream ut;<br>  void print(Object... a) {<br>    for (Object o:a) {<br>      ut.print(o+" ");<br>    }<br>    ut.println();<br>  }<br><br>  <br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>