            Imports Microsoft.VisualBasic<br>Imports System<br>Imports System.Collections<br> <br>Public Class AlphabetCount<br>    Dim dp(,) As Integer<br>    Public Function count(ByVal grid As String(), ByVal length As Integer) As Integer<br>        Dim i, j, k, x, y, w, h As Integer<br>        Dim mycount, res As Long<br> <br>        h = grid.Length<br>        w = grid(0).Length<br>        ReDim dp(h - 1, w - 1)<br>        For i = 0 To h - 1<br>            For j = 0 To w - 1<br>                If grid(i).Chars(j) = Chr(Asc("A") + length - 1) Then dp(i, j) = 1<br>            Next<br>        Next<br>        For k = length - 1 To 1 Step -1<br>            For i = 0 To h - 1<br>                For j = 0 To w - 1<br>                    If grid(i).Chars(j) = Chr(Asc("A") + k - 1) Then<br>                        mycount = 0<br>                        For x = -1 To 1<br>                            For y = -1 To 1<br>                                If i + x &gt;= 0 AndAlso i + x &lt; h AndAlso j + y &gt;= 0 AndAlso j + y &lt; w AndAlso grid(i + x).Chars(j + y) = Chr(Asc("A") + k) Then mycount += dp(i + x, j + y)<br>                                If mycount &gt;= 1000000000 Then<br>                                    mycount = 1000000000<br>                                    Exit For<br>                                End If<br>                            Next<br>                            If mycount &gt;= 1000000000 Then<br>                                mycount = 1000000000<br>                                Exit For<br>                            End If<br>                        Next<br>                        dp(i, j) = mycount<br>                    End If<br>                Next<br>            Next<br>        Next<br>        For i = 0 To grid.Length - 1<br>            For j = 0 To grid(0).Length - 1<br>                If grid(i).Chars(j) = "A" Then<br>                    res += dp(i, j)<br>                    If res &gt;= 1000000000 Then Return 1000000000<br>                End If<br>            Next<br>        Next<br>        Return CInt(res)<br>    End Function<br>End Class<br></td>