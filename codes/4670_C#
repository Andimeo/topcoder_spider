            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class ListeningIn {<br>  const string UNMATCHED = "UNMATCHED";<br><br>    public string probableMatch(string typed, string phrase) {<br><br>    StringBuilder res = new StringBuilder();<br><br>    int i=0, j = 0;<br>    for (j = 0; j &lt; phrase.Length; j++)<br>      if (i &lt; typed.Length &amp;&amp; phrase[j] == typed[i])<br>        i++;<br>      else<br>        res.Append(phrase[j]);<br><br>    if (i != typed.Length) return UNMATCHED;<br><br><br>        return res.ToString();<br>    }<br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>