            public class ListeningIn {<br> <br>  public String probableMatch(String typed, String phrase) {<br>    int x = 0;<br>    String s = "";<br>    for (int i = 0; i &lt; phrase.length(); i++) {<br>      char c1 = phrase.charAt(i);<br>      char c2 = '#';<br>      if (x &gt;= typed.length()) {<br>        c2 = '#';<br>      } else {<br>        c2 = typed.charAt(x);<br>      }<br>      if (c1 == c2) {<br>        x++;<br>      } else {<br>        s = s + c1;<br>      }<br>    }<br>    if (x == typed.length()) {<br>      return s;<br>    }<br>    return "UNMATCHED";<br>    <br>  }<br> <br>}<br></td>