            import java.util.*;<br>public class CheatABit<br>{<br>public int enterCodes(int[] ratings, int yourRating)<br>{<br>  Vector vec1 = new Vector();<br>  int len = ratings.length, a=0, b=0, min =0;<br>  for( int i=0; i&lt;len; i++ )<br>    vec1.add( new Integer(ratings[i]) );<br>  Vector vec2 = new Vector();<br>  Vector vec3 = new Vector();<br>  int n = yourRating;<br>  for( int i=0;i&lt;=len;i++)<br>  {<br>    int ccount = 0;<br>    vec1.insertElementAt( new Integer(n), i );<br>    int j = 0, k=1;<br>    vec2.clear();<br>    while( vec2.size() != vec1.size() )<br>    {<br>      vec2.add(vec1.elementAt(j));<br>      j++;<br>    }<br>    while( vec2.size() &gt; 1 )<br>    {<br>      j = 0; k=1;<br>      while( k &lt; vec2.size() )<br>      {<br>        a = Integer.parseInt( (vec2.elementAt(j)).toString() );<br>        b = Integer.parseInt( (vec2.elementAt(k)).toString() );<br>        if( a==n &amp;&amp; a&lt;b )<br>        {<br>          ccount++;<br>          vec3.add(new Integer(n));<br>        }<br>        else if( b==n &amp;&amp; a&gt;b )<br>        {<br>          ccount++;<br>          vec3.add(new Integer(n));<br>        }<br>        else if( a&lt;b )<br>          vec3.add( new Integer(b) );<br>        else<br>          vec3.add( new Integer(a) );<br>        j += 2;k += 2;<br>      }<br>      if( vec2.size() % 2 == 1 )<br>        vec3.add( vec2.lastElement() );<br>      vec2.clear();<br>      while( vec3.size() != 0 )<br>        vec2.add(vec3.remove(0));<br>    }<br>    if( min &gt; ccount || i==0 )<br>      min = ccount;<br>    vec1.remove(i);<br>  }<br>  return min;<br>}<br>}<br></td>