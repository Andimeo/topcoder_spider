            using System; using System.Collections; using System.Text; using System.Text.RegularExpressions;<br>public class Decipher<br>   {<br>   public string[] decipher(string[] encoded, string frequencyOrder)<br>      {<br>      String usedS = "";<br>      int[] ct = new int[26];<br>      foreach (String s in encoded)<br>         foreach (char c in s) if (c != ' ')<br>            {<br>            if (ct[(int)c-65] == 0)<br>               usedS += c;<br>            ct[(int)c-65]++;<br>            }<br>      char[] used = usedS.ToCharArray();<br>      Array.Sort(used);<br>      for (int i=0; i &lt; used.Length-1; i++)<br>         for (int j=used.Length-2; j &gt;= i; j--)<br>            if (ct[(int)used[j]-65] &lt; ct[(int)used[j+1]-65])<br>               {<br>               char t = used[j]; used[j] = used[j+1]; used[j+1] = t;<br>               }<br>      char[] map = new char[26];<br>      for (int i=0; i &lt; used.Length; i++)<br>         map[(int)used[i]-65] = frequencyOrder[i];<br>      for (int i=0; i &lt; encoded.Length; i++)<br>         {<br>         String s = "";<br>         foreach (char c in encoded[i])<br>            if (c == ' ')<br>               s += c;<br>            else<br>               s += map[(int)c - 65];<br>         encoded[i] = s;<br>         }<br>      return encoded;<br>      }<br><br><br>} // TopC#der<br><br><br>// Powered by FileEdit<br></td>