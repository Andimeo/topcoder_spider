            import java.util.*;<br>import java.io.*;<br>import java.text.*;<br>import java.math.*;<br>import static java.lang.Math.*;<br>import static java.util.Arrays.*;<br>public class Decipher {<br>  public String[] decipher(String[] encoded, String frequencyOrder) {<br>    char[] convert = new char[26];<br>    int[] freq = new int[26];<br>    for(int i = 0; i &lt; encoded.length; i++){<br>      for(int j = 0; j &lt; encoded[i].length(); j++){<br>        char c = encoded[i].charAt(j);<br>        if(c == ' ') continue;<br>        freq[c-'A']++;<br>      }<br>    }<br>    for(int i = 0; i &lt; frequencyOrder.length(); i++){<br>      int best = 0;<br>      int who = 0;<br>      for(int j = 0; j &lt; freq.length; j++){<br>        if(freq[j] &gt; best){<br>          best = freq[j];<br>          who = j;<br>        }<br>      }<br>      convert[who] = frequencyOrder.charAt(i);<br>      freq[who] = -1;<br>    }<br>    String[] ret = new String[encoded.length];<br>    for(int i = 0; i &lt; ret.length; i++){<br>      ret[i] = "";<br>      for(int j = 0; j &lt; encoded[i].length(); j++){<br>        char c = encoded[i].charAt(j);<br>        if(c == ' ') ret[i] += " ";<br>        else ret[i] += convert[c-'A'];<br>      }<br>    }<br>    return ret;<br>  }<br> <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>