            #include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;sstream&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt; <br>#include &lt;cctype&gt; <br>using namespace std;<br>class ObjectPacking {<br>  public:<br>  int smallBox(int ow, int oh, vector &lt;int&gt; bw, vector &lt;int&gt; bh) {<br>    int ar=9999999;<br>    for(int i=0;i&lt;bw.size();i++){<br>      if(bw[i]&gt;=ow &amp;&amp; bh[i]&gt;=oh &amp;&amp; bh[i]*bw[i]&lt;ar)<br>      ar=bh[i]*bw[i];<br>      if(bw[i]&gt;=oh &amp;&amp; bh[i]&gt;=ow &amp;&amp; bh[i]*bw[i]&lt;ar)ar=bh[i]*bw[i];<br>    }<br>    if(ar==9999999)return -1;<br>    return ar;<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>