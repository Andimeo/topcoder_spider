<td class="problemText" colspan="8" valign="middle" align="left">
            public class ObjectPacking<br>{<br>    public int smallBox (int objWidth, int objLength, int[] boxWidth, int[] boxLength)<br>    {<br>        int lowestArea = Integer.MAX_VALUE;<br>        for (int i = 0 ; i &lt; boxWidth.length ; i++)<br>            if ((boxWidth [i] &gt;= objWidth &amp;&amp; boxLength [i] &gt;= objLength) ||<br>                    (boxLength [i] &gt;= objWidth &amp;&amp; boxWidth [i] &gt;= objLength))<br>                lowestArea = Math.min (lowestArea, boxWidth [i] * boxLength [i]);<br>        if (lowestArea == Integer.MAX_VALUE)<br>            return -1;<br>        return lowestArea;<br>    }<br>}<br></td>