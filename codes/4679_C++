            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class ValetParking {<br>public:<br>  int minMoves(int, int, int, int);<br>};<br> <br>int ValetParking::minMoves(int emptyRow, int emptyCol, int cusRow, int cusCol) {<br>  int sx = emptyRow, sy = emptyCol;<br>  int x = cusRow, y = cusCol;<br>  int cnt = 0;<br>  while(x || y) {<br>    int t1= 1000000, t2 = 1000000;<br>    if (x) {<br>      t1 = abs((x-1)-sx)+abs(y-sy);<br>      if (sx&gt;x &amp;&amp; sy  == y)<br>        t1 += 2;<br>    }<br>    if (y) {<br>      t2 = abs((y-1)-sy)+abs(x-sx);<br>      if (sy&gt;y &amp;&amp; sx == x)<br>        t2 += 2;<br>    }<br>    sx = x;<br>    sy = y;<br>    if (t1 &lt; t2 || t1 == t2 &amp;&amp; x &gt; y) {<br>      cnt += t1;<br>      x--;<br>    }<br>    else {<br>      cnt += t2;<br>      y--;<br>    }<br>    cnt++;<br>  }<br>  return cnt;<br>}<br></td>