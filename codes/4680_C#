            using System; using System.Collections; using System.Text; using System.Text.RegularExpressions;<br>public class VideoEncode<br>   {<br>   public double bitrate(string length, int des)<br>      {<br>      String[] ss = length.Split(':');<br>      double secs = INT(ss[0])*3600 + INT(ss[1])*60 + INT(ss[2]);<br>      double kbits = des * 1048576.0 * 8.0 / 1000.0;<br>      return kbits/secs;<br>      }<br><br><br>// Library begins here<br><br>static int    INT   (Object o) { return Convert.ToInt32(o.ToString()); }<br><br>} // TopC#der<br><br><br>// Powered by FileEdit<br></td>