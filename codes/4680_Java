            import java.util.*;<br>import java.math.*;<br>import java.util.regex.*;<br>import java.text.*;<br>public class VideoEncode {<br>    public double bitrate(String length, int desiredSize) {<br>        String[] tokens = length.split(":");<br>        int h = Integer.parseInt(tokens[0]);<br>        int m = Integer.parseInt(tokens[1]);<br>        int s = Integer.parseInt(tokens[2]);<br>        <br>        s += m * 60 + h * 60 * 60;<br>        <br>        return 1.0 * desiredSize * 1048576 * 8 / s / 1000;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>