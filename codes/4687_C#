            using System;<br>using System.Collections;<br>public class CliqueCount {<br>  bool[,] g;<br>  int n;<br>  bool[] take;<br>  bool[] good;<br><br>  void rec(int start, int mask)<br>  {<br>    if (start &gt;= n)<br>    {<br>      good[mask] = true;<br>      return;<br>    }<br><br>    take[start] = false;<br>    rec(start + 1, mask);<br><br>    for (int i = 0; i &lt; start; ++i)<br>      if (take[i] &amp;&amp; !g[i, start])<br>        return;<br><br>    take[start] = true;<br>    rec(start + 1, mask | (1 &lt;&lt; start));<br>  }<br><br>  public int countCliques(string[] graph) {<br>    n = graph.Length;<br>    g = new bool[n, n];<br>    for (int i = 0; i &lt; n; ++i)<br>      for (int j = 0; j &lt; n; ++j)<br>        if (graph[i][j] != '0')<br>          g[i, j] = true;<br>        else<br>          g[i, j] = false;<br><br>    take = new bool[n];<br>    good = new bool[1 &lt;&lt; n];<br><br>    for (int i = 0; i &lt; (1 &lt;&lt; n); ++i)<br>      good[i] = false;<br><br>    rec(0, 0);<br><br>    int res = 0;<br><br>    for (int i = 0; i &lt; (1 &lt;&lt; n); ++i)<br>      if (good[i])<br>      {<br>        bool ok = true;<br><br>        for (int k = 0; k &lt; n; ++k)<br>          if (((i &amp; (1 &lt;&lt; k)) == 0) &amp;&amp; good[i | (1 &lt;&lt; k)])<br>          {<br>            ok = false;<br>            break;<br>          }<br><br>        if (ok) ++res;<br>      }<br><br>    return res;<br>  }<br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>