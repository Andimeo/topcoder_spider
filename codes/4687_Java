            import java.util.*;<br>public class CliqueCount {<br>    boolean[][] edges;<br>    int count=0;<br>    boolean[] seen;<br>    int n;<br>    <br>  public int countCliques(String[] graph) {<br>    n = graph.length;<br>        edges = new boolean[n][n];<br>        for (int i=0; i &lt; n; ++i) {<br>            for (int j=0; j &lt; n; ++j) {<br>                edges[i][j] = graph[i].charAt(j) == '1';<br>            }<br>        }<br>        seen = new boolean[1&lt;&lt;n];<br>        dfs(0);<br>        return count;<br>  }<br>  <br> public void dfs(int mask) {<br>     if (seen[mask]) return;<br>     seen[mask] = true;<br>     boolean max = true;<br>     for (int i=0; i &lt; n; ++i) {<br>         if ((mask &amp; (1&lt;&lt;i)) != 0) continue;<br>         boolean clique = true;<br>         for (int j=0; j &lt; n; ++j) {<br>             if ((mask &amp; (1&lt;&lt;j)) == 0) continue;<br>             clique &amp;= edges[i][j];<br>         }<br>         if (clique) {<br>             max = false;<br>             dfs(mask + (1&lt;&lt;i));<br>         }<br>     }<br>     if (max) ++count;<br> }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>