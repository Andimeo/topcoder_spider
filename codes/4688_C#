            using System;<br> <br>public class GridGenerator<br>{<br>  public int generate(int[] row, int[] col)<br>  {<br>    int m = row.Length, n = col.Length;<br>    int[,] grid = new int[m,n];<br>    for (int i = 0; i &lt; m; i++) grid[i, 0] = row[i];<br>    for (int j = 0; j &lt; n; j++) grid[0, j] = col[j];<br>    for (int i = 1; i &lt; m; i++)<br>      for (int j = 1; j &lt; n; j++)<br>        grid[i, j] = grid[i - 1, j - 1] + grid[i - 1, j] + grid[i, j - 1];<br>    return grid[m - 1, n - 1];<br>  }<br> <br>}<br> <br>//Powered by [KawigiEdit]<br></td>