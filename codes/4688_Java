            import java.util.*;<br>public class GridGenerator {<br>  public int generate(int[] row, int[] col) {<br>    int n = row.length;;<br>    int[][] p = new int[n][n];<br>    for (int i = 0; i &lt; n; ++i) {<br>      p[0][i] = row[i];<br>      p[i][0] = col[i];<br>    }<br>  <br>    for (int i = 1; i &lt; n; ++i) {<br>      for (int j = 1; j &lt; n; ++j) {<br>        p[i][j] = p[i][j - 1] + p[i - 1][j - 1] + p[i - 1][j];<br>      }<br>    }<br>  <br>    return p[n - 1][n - 1];<br>  }<br>}<br> <br> <br>// Powered by FileEdit<br></td>