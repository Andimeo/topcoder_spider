<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br> <br>public class GraphLabel<br>{<br>  private int n = -1;<br>  private int[] v = null;<br>  private bool[] used = null;<br>  int ans = int.MaxValue;<br>  char[][] g = null;<br> <br>  public int adjacentDifference(string[] graph)<br>  {<br>    g = new char[graph.Length][];<br>    for (int i = 0; i &lt; graph.Length; i++)<br>      g[i] = graph[i].ToCharArray();<br>    n = graph.Length;<br>    v = new int[graph.Length];<br>    used = new bool[graph.Length];<br>    search(0);<br>    return ans;<br>  }<br> <br>  private void search(int depth)<br>  {<br>    if (depth &gt;= n)<br>    {<br>      int curr = int.MinValue;<br>      for (int i=0; i&lt;n; i++)<br>        for (int j=i+1; j&lt;n; j++)<br>          if (g[i][j] == '1')<br>          {<br>            int now = Math.Abs(v[i] - v[j]);<br>            if (now &gt; curr) curr = now;<br>          }<br>      if (curr &lt; ans) ans = curr;<br>    }<br>    else<br>    {<br>      for (int i = 0; i &lt; n; i++)<br>        if (!used[i])<br>        {<br>          used[i] = true;<br>          v[depth] = i;<br>          search(depth + 1);<br>          used[i] = false;<br>        }<br>    }<br>  }<br>}<br> <br>//Powered by [KawigiEdit]<br></td>