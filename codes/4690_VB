            Imports System<br>Imports Microsoft.VisualBasic<br>Public Class GraphLabel<br>    Dim Tmn As Integer<br>    Dim grb(,) As Boolean<br>    Dim used() As Boolean<br>    Public Function adjacentDifference(ByVal graph As String()) As Integer<br>        Dim sz As Integer, i As Integer, j As Integer<br>        sz = graph.GetUpperBound(0)<br> <br>        ReDim grb(sz, sz)<br>        Dim nums() As Integer<br>        ReDim nums(sz)<br>        ReDim used(sz)<br>        Tmn = 10000<br>        For i = 0 To sz<br>            For j = 0 To sz<br>                grb(i, j) = (graph(i).Chars(j) = "1")<br>            Next<br>            nums(i) = -1<br>        Next<br> <br>        Comb(nums, sz)<br>        adjacentDifference = Tmn<br>    End Function<br> <br>    Function Comb(ByVal nums() As Integer, ByVal lft As Integer)<br>        Dim sz As Integer, t As Integer<br>        sz = nums.GetUpperBound(0)<br>        Dim i As Integer<br>        For i = 0 To sz<br>            If nums(sz - lft) = -1 Then<br>                If Not used(i) Then<br>                    used(i) = True<br>                    nums(sz - lft) = i<br>                    If lft = 0 Then<br>                        t = ScoreGraph(nums)<br>                        If t &lt; Tmn Then Tmn = t<br>                    Else<br>                        Comb(nums, lft - 1)<br> <br>                    End If<br> <br>                    nums(sz - lft) = -1<br>                    used(i) = False<br>                End If<br>            End If<br>        Next<br>    End Function<br> <br>    Public Function ScoreGraph(ByVal nums() As Integer) As Integer<br>        Dim sz As Integer<br>        sz = grb.GetUpperBound(0)<br>        Dim i As Integer, j As Integer, mx As Integer, t As Integer<br>        mx = -1<br>        For i = 0 To sz<br>            For j = i + 1 To sz<br>                If grb(i, j) Then<br>                    t = Math.Abs(nums(i) - nums(j))<br>                    If t &gt; mx Then<br>                        mx = t<br> <br>                    End If<br> <br>                End If<br>            Next<br>        Next<br>        If mx = 0 Then Stop<br>        ScoreGraph = mx<br>    End Function<br> <br>End Class<br></td>