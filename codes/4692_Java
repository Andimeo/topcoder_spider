            import java.util.*;<br>public class ImageRepeat {<br>    public int largestSize(String[] imageA, String[] imageB) {<br>        int na = imageA.length;<br>        int ma = imageA[0].length();<br>        int n = imageB.length;<br>        int m = imageB[0].length();<br>        int[][] f = new int[n][m];<br>        int[][] g = new int[n][m];<br>        int res = 0;<br>        for (int dx=-50; dx&lt;50; dx++)<br>            for (int dy=-50; dy&lt;50; dy++) {<br>                for (int x=0; x &lt; n; x++)<br>                    for (int y=0; y &lt; m; y++)<br>                        if ((x + dx &lt; 0) || (x + dx &gt;= na) || (y + dy &lt; 0) || (y + dy &gt;= ma)) {<br>                            f[x][y] = 0;<br>                        } else<br>                            if (imageA[x+dx].charAt(y+dy) == imageB[x].charAt(y)) {<br>                                f[x][y] = 1;<br>                            } else {<br>                                f[x][y] = 0;<br>                            }<br>                for (int x=0; x &lt; n; x++)<br>                    for (int y=0; y &lt; m; y++)<br>                        if (f[x][y] == 0) {<br>                            g[x][y] = 0;<br>                        } else<br>                        if ((x == 0) || (y == 0)) {<br>                            g[x][y] = 1;<br>                        } else {<br>                            g[x][y] = Math.min(Math.min(g[x-1][y], g[x][y-1]), g[x-1][y-1])+1;<br>                        }<br>                for (int x=0; x &lt; n; x++)<br>                    for (int y=0; y &lt; m; y++)<br>                        if (g[x][y] &gt; res)<br>                            res = g[x][y];<br>            }<br>        return res;<br>    }<br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>