<td class="problemText" colspan="8" valign="middle" align="left">
            Public Class ImageRepeat<br>        Public Function largestSize(ByVal imageA As String(), ByVal imageB As String()) As Integer<br>            Dim i, j, k, l As Long<br>            Dim iHighA, iLenA As Integer<br>            Dim iHighB, iLenB As Integer<br>            Dim iHigh, iLen As Integer<br>            iHighA = imageA.GetUpperBound(0)<br>            iLenA = imageA(0).Length - 1<br>            iHighB = imageB.GetUpperBound(0)<br>            iLenB = imageB(0).Length - 1<br>            iHigh = System.Math.Min(iHighA, iHighB)<br>            iLen = System.Math.Min(iLenA, iLenB)<br> <br>            Dim iSize As Integer<br>            Dim iMax As Integer = System.Math.Min(iHigh, iLen) + 1<br>            iSize = (iMax + 1) \ 2<br> <br>            Dim oHash As System.Collections.Hashtable<br>            Dim s As String<br>            Dim iMinFound, iMaxFound As Integer<br>            Dim bDone As Boolean<br>            iMinFound = 0<br>            iMaxFound = iMax + 1<br> <br>            bDone = False<br> <br>            Do While Not bDone<br>                oHash = New System.Collections.Hashtable<br> <br>                For i = 0 To iHighA - iSize + 1<br>                    For j = 0 To iLenA - iSize + 1<br>                        s = ""<br>                        For k = i To i + iSize - 1<br>                            s &amp;= imageA(k).Substring(j, iSize)<br>                        Next<br>                        If Not oHash.Contains(s) Then<br>                            oHash.Add(s, Nothing)<br>                        End If<br>                    Next<br>                Next<br> <br>                Dim bFound As Boolean = False<br> <br>                For i = 0 To iHighB - iSize + 1<br>                    For j = 0 To iLenB - iSize + 1<br>                        s = ""<br>                        For k = i To i + iSize - 1<br>                            s &amp;= imageB(k).Substring(j, iSize)<br>                        Next<br>                        If oHash.Contains(s) Then<br>                            bFound = True<br>                            iMinFound = iSize<br>                            Exit For<br>                        End If<br>                    Next<br>                    If bFound Then Exit For<br> <br>                Next<br>                If Not bFound Then iMaxFound = iSize<br> <br>                If iMinFound + 1 = iMaxFound Then Return iMinFound<br>                iSize = (iMinFound + iMaxFound + 1) \ 2<br>            Loop<br> <br>        End Function<br>    End Class<br></td>