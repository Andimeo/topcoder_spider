            #include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br> <br>using namespace std;<br> <br>typedef long long int64;<br>typedef vector&lt;int&gt; vi;<br>#define FOR(i,n) for (int i=0; i&lt;(n); i++)<br> <br>class CompanyMessages {<br>public:<br>  vi ans[1&lt;&lt;15];<br>  int cnt[1&lt;&lt;15];<br>  vector &lt;int&gt; getRank(vector &lt;string&gt; m) {<br>    int n=m.size();<br>    FOR(i,1&lt;&lt;n) {<br>      if (!i) continue;<br>      cnt[i]=10000;<br>      FOR(j,n) if (i&amp;(1&lt;&lt;j)) {<br>        int prop=cnt[i-(1&lt;&lt;j)];<br>        FOR(k,n) if (i&amp;(1&lt;&lt;k)) prop+=m[k][j]-'0';<br>        if (prop&lt;cnt[i]) {<br>          cnt[i]=prop;<br>          ans[i].clear();<br>          ans[i].push_back(j);<br>          FOR(k,ans[i-(1&lt;&lt;j)].size()) ans[i].push_back(ans[i-(1&lt;&lt;j)][k]);<br>        }<br>      }<br>    }<br>    return ans[(1&lt;&lt;n)-1];<br>  }<br>};<br> <br> <br>// Powered by PopsEdit<br></td>