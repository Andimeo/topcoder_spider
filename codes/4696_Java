            import java.util.*;<br>public class CompanyMessages {<br>  public int[] getRank(String[] messages) {<br>    int n = messages.length;<br>    int[] score = new int[1 &lt;&lt; n];<br>    int[] back = new int[1 &lt;&lt; n];<br>    int[][] msg = new int[n][n];<br>    for (int i=0; i&lt;n; i++) for (int j=0; j&lt;n; j++) msg[i][j] = messages[i].charAt(j)-'0';<br>    Arrays.fill(score, Integer.MAX_VALUE);<br>    score[0] = 0;<br>    Arrays.fill(back, n+1);<br>    for (int mask=0; mask&lt;(1&lt;&lt;n); mask++) {<br>      for (int j=0; j&lt;n; j++) if (((1&lt;&lt;j)&amp;mask) != 0) {<br>        int curr = score[mask - (1&lt;&lt;j)];<br>        for (int k=0; k&lt;n; k++) if ((j != k) &amp;&amp; ((1&lt;&lt;k)&amp;mask) != 0) {<br>          curr += msg[k][j];<br>        }<br>        if (curr == score[mask]) back[mask] = Math.min(j, back[mask]);<br>        if (curr &lt; score[mask]) back[mask] = j;<br>        score[mask] = Math.min(score[mask], curr);  <br>      }<br>      //System.out.println(mask+" "+score[mask]+" "+back[mask]);<br>    }<br>    int[] result = new int[n];<br>    int mask = (1&lt;&lt;n) - 1;<br>    for (int i=0; i&lt;n; i++) {<br>      result[i] = back[mask];<br>      mask -= (1 &lt;&lt; back[mask]);<br>    }<br>    return result;<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>