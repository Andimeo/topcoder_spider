            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class ProductSet {<br>  bool[] ss;<br>    public int howMany(int[] lows, int[] highs) {<br>      ss = new bool[1000100];<br>      int i, j, k, cnt=0;<br>      for (i=0; i&lt;1000100; i++) ss[i]=false;<br>      for (i=lows[0]; i&lt;=highs[0]; i++) for (j=lows[1]; j&lt;=highs[1]; j++) for (k=lows[2]; k&lt;=highs[2]; k++)<br>        if (ss[i*j*k]==false)<br>        {<br>          ss[i*j*k]=true;<br>          cnt++;<br>        }<br>      return cnt;<br>    }<br>};<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>