            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class ProductSet {<br>public:<br>  int howMany(vector &lt;int&gt;, vector &lt;int&gt;);<br>};<br> <br>int ProductSet::howMany(vector &lt;int&gt; lows, vector &lt;int&gt; highs) {<br>  set&lt;int&gt; result;<br>  for (int i = lows[0]; i &lt;= highs[0]; i++)<br>    for (int j = lows[1]; j &lt;= highs[1]; j++)<br>      for (int k = lows[2]; k &lt;= highs[2]; k++)<br>        result.insert(i * j * k);<br>  return result.size();<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>