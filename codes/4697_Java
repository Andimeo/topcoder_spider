            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class ProductSet<br>{<br>  public int howMany(int[] l, int[] h)<br>  {<br>    int r = 0;<br>    Set&lt;Integer&gt; set = new HashSet&lt;Integer&gt;();<br>    for(int i=l[0];i&lt;=h[0];i++)<br>    for(int j=l[1];j&lt;=h[1];j++)<br>    for(int k=l[2];k&lt;=h[2];k++){<br>      set.add(i*j*k);<br>    }<br>    return set.size();<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>