            import java.util.*;<br><br>/** <br> * @author Roman Elizarov<br> */<br>public class EncodingTrees {<br>  private long[] c;<br><br>  public String getCode(int N, int index) {<br>    c = new long[N + 1];<br>    c[0] = 1;<br>    for (int i = 1; i &lt;= N; i++) {<br>      for (int j = 0; j &lt; i; j++)<br>        c[i] += c[j] * c[i - j - 1];<br>    }<br>    if (index &gt; c[N])<br>      return "";<br>    return code(N, index, 'a');<br>  }<br><br>  String code(int n, int index, char ofs) {<br>    if (n == 0)<br>      return "";<br>    int i = 0;<br>    while (index &gt; c[i] * c[n - i - 1]) {<br>      index -= c[i] * c[n - i - 1];<br>      i++;<br>    }<br>    int k = (int)(c[n - i - 1]);<br>    int l = (index - 1) / k + 1;<br>    int r = (index - 1) % k + 1;<br>    return "" + (char)(ofs + i) + code(i, l, ofs) + code(n - i - 1, r, (char)(ofs + 1 + i));<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>