<td class="problemText" colspan="8" valign="middle" align="left">
            public class MissileTarget<br>  {<br>    public MissileTarget()<br>    {<br>    }<br> <br>    public int[] bestFit(int[] x, int[] y)<br>    {<br>      double X,Y;<br>      int xs =0,ys=0;<br>      for (int i=0; i&lt; x.Length; i++)<br>      {<br>        xs += x[i];<br>        ys += y[i];<br>      }<br>      X = ((double)xs/(double)x.Length);<br>      Y = ((double)ys/(double)x.Length);<br> <br>      int invertX = (X&lt;0 ? -1 : 1);<br>      int invertY = (Y&lt;0 ? -1 : 1);<br> <br>      int XRes = (int)System.Math.Round(System.Math.Abs(X)) * invertX;<br>      int YRes = (int)System.Math.Round(System.Math.Abs(Y)) * invertY;<br> <br>      return new int[] { XRes, YRes };                        <br>    }<br>  }<br></td>