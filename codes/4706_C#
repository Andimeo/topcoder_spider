            using System;<br>using System.Collections;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class StockQuotes<br>{<br>  public string[] report(string[] quotes)<br>  {<br>    int[] bids = new int[11];<br>    int[] asks = new int[11];<br>    int[] count = new int[11];<br>    int[] sum = new int[11];<br>    for (int i = 0; i &lt; 11; ++i){ bids[i] = -1; asks[i] = int.MaxValue; count[i] = 0; }<br>    foreach (string s in quotes)<br>    {<br>      string[] ss = s.Split();<br>      int ex = int.Parse(ss[0]);<br>      count[ex]++;<br>      int bid = int.Parse(ss[1]);<br>      int ask = int.Parse(ss[2]);<br>      bool ch = false;<br>      if (bid != bids[ex])<br>      {<br>        bids[ex] = bid;<br>        for (int i = 0; i &lt; 10; ++i)<br>        {<br>          if (bids[i] &gt; bid) bid = bids[i];<br>        }<br>        if (bid != bids[10])<br>        {<br>          bids[10] = bid;<br>          ch = true;<br>        }<br>      }<br>      if (ask != asks[ex])<br>      {<br>        asks[ex] = ask;<br>        for (int i = 0; i &lt; 10; ++i)<br>        {<br>          if (asks[i] &lt; ask) ask = asks[i];<br>        }<br>        if (ask != asks[10])<br>        {<br>          asks[10] = ask;<br>          ch = true;<br>        }<br>      }<br>      if (ch)<br>      {<br>        count[10]++;<br>        sum[10] += asks[10] - bids[10];<br>      }<br>      sum[ex] += asks[ex] - bids[ex];<br>    }<br>    ArrayList res = new ArrayList();<br>    for (int i = 0; i &lt; 11; ++i)<br>    {<br>      if (count[i] == 0) continue;<br>      res.Add(i + " " + count[i] + " " + format(sum[i] / (double)count[i]));<br>    }<br>    return (string[])res.ToArray(typeof(string));<br>  }<br>  <br>  string format(double d)<br>  {<br>    return d.ToString("0.00");<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit]<br></td>