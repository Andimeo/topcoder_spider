            #include &lt;algorithm&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br><br>using namespace std;<br><br>class StockQuotes { <br>   public: <br>   vector &lt;string&gt; report(vector &lt;string&gt; quotes) { <br>      vector&lt; pair&lt;int,int&gt; &gt; exch( 11, pair&lt;int,int&gt;(0,0) );<br>      vector&lt;int&gt; bid( 11, -1 );<br>      vector&lt;int&gt; ask( 11, -1 );<br>      for( vector&lt;string&gt;::iterator it = quotes.begin(); it != quotes.end(); ++it ) {<br>         istringstream iss( *it );<br>         int e, b, a;<br>         iss &gt;&gt; e &gt;&gt; b &gt;&gt; a;<br>         bid[e] = b; ask[e] = a;<br>         <br>         exch[e].first++;<br>         exch[e].second += ask[e] - bid[e];<br><br>         int bb = 0, aa = 10000;<br>         for( int i = 0; i &lt; 10; ++i ) {<br>            if( bid[i] == -1 ) continue;<br>            bb &gt;?= bid[i];<br>            aa &lt;?= ask[i];<br>         }<br>         if( bb != bid[10] || aa != ask[10] ) {<br>            bid[10] = bb; <br>            ask[10] = aa;<br>            exch[10].first++;<br>            exch[10].second += ask[10] - bid[10];<br>         }<br>      }<br>      vector&lt;string&gt; ret;<br>      for( int i = 0; i &lt;= 10; ++i ) {<br>         if( exch[i].first == 0 ) continue;<br>         char buff[100];<br>         sprintf( buff, "%d %d %.2lf", i, exch[i].first, exch[i].second / (double)exch[i].first + 1e-12 );<br>         ret.push_back( string( buff ) ); <br>      }<br>      return ret;<br>   } <br><br><br><br><br><br>   <br><br>}; <br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>