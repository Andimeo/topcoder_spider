            import java.util.*;<br> <br>public class Recipe {<br>    int gcd(int x, int y) {<br>        while (y != 0) {<br>            int t = x % y; x = y; y = t;<br>        }<br>        return x;<br>    }<br> <br>    public String[] fix(String[] recipe, String[] mixingBowl) {<br>        int n = recipe.length;<br>        String[] names = new String[n];<br>        int[] rq = new int[n];<br>        int[] aq = new int[n];<br>        for (int i = 0; i &lt; n; i++) {<br>            Scanner sc = new Scanner(recipe[i]);<br>            int nn = sc.nextInt();<br>            String ss = sc.next();<br>            if (ss.equals("Tbsp")) {<br>                nn *= 3;<br>            } else if (ss.equals("cups")) {<br>                nn *= 48;<br>            }<br>            rq[i] = nn;<br>            names[i] = sc.next();<br>        }<br> <br>        for (int i = 0; i &lt; mixingBowl.length; i++) {<br>            Scanner sc = new Scanner(mixingBowl[i]);<br>            int nn = sc.nextInt();<br>            String ss = sc.next();<br>            if (ss.equals("Tbsp")) {<br>                nn *= 3;<br>            } else if (ss.equals("cups")) {<br>                nn *= 48;<br>            }<br>            String name = sc.next();<br>            for (int j = 0; j &lt; n; j++) {<br>                if (names[j].equals(name)) {<br>                    aq[j] = nn;<br>                }<br>            }<br>        }<br> <br>        int d = 0;<br>        for (int i = 0; i &lt; n; i++) {<br>            d = gcd(d, rq[i]);<br>        }<br>        int[] pp = new int[n];<br>        for (int i = 0; i &lt; n; i++) {<br>            pp[i] = rq[i] / d;<br>        }<br>        while (true) {<br>            boolean ok = true;<br>            for (int i = 0; i &lt; n; i++) {<br>                if (pp[i] * d &lt; aq[i]) {<br>                    ok = false;<br>                    break;<br>                }<br>            }<br>            if (ok) {<br>                break;<br>            }<br>            d++;<br>        }<br> <br>        List&lt;String&gt; l = new LinkedList&lt;String&gt;();<br>        for (int i = 0; i &lt; n; i++) {<br>            if (pp[i] * d &gt; aq[i]) {<br>                int t = pp[i] * d - aq[i];<br>                int cups = t / 48;<br>                t = t % 48;<br>                int tbls = t / 3;<br>                t = t % 3;<br>                int tsps = t;<br>                String s = "";<br>                if (cups != 0) {<br>                    s = s + cups + " cups ";<br>                }<br>                if (tbls != 0) {<br>                    s = s + tbls + " Tbsp ";<br>                }<br>                if (tsps != 0) {<br>                    s = s + tsps + " tsp ";<br>                }<br>                s = s + names[i];<br>                l.add(s);<br>            }<br>        }<br>        String[] r = l.toArray(new String[l.size()]);<br>        return r;<br>    }<br> <br> <br>}<br> <br> <br>// Powered by PopsEdit<br> <br>// Powered by CodeProcessor<br></td>