            using System;<br>using System.Collections;<br>public class FloorTiling {<br>  public int tile(int size, int offset, int width, int height) {<br>    int coffs = 0;<br>    int res = width * height;<br><br>    for (int i = 0; i &lt; height; ++i)<br>    {<br>      int j = coffs;<br>      while (j + size &lt;= width)<br>      {<br>        res -= size;<br>        j += size;<br>      }<br>      coffs += offset;<br>      while (coffs &gt;= size) coffs -= size;<br>    }<br><br>    return res;<br>  }<br><br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>