<td class="problemText" colspan="8" valign="middle" align="left">
            Public Class FloorTiling<br>        Public Function tile(ByVal size As Integer, ByVal offset As Integer, ByVal width As Integer, ByVal height As Integer) As Integer<br>            Dim i, j, k, l As Long<br> <br>            Dim iLen As Integer = 0<br>            Dim iHeight As Integer = 0<br>            Dim iOffset As Integer = 0<br> <br>            Do While iHeight &lt; height<br>                iOffset = iOffset Mod size<br>                iLen += ((width - iOffset) \ size) * size<br>                iOffset += offset<br>                iHeight += 1<br>            Loop<br> <br>            Return (width * height) - iLen<br> <br>        End Function<br>    End Class<br></td>