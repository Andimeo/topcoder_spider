            import java.util.*;<br>public class ClassScores<br>{<br>    public int[] findMode (int[] scores)<br>    {<br>        int counts[] = new int [101];<br>        for (int i = 0 ; i &lt; scores.length ; i++)<br>            counts [scores [i]]++;<br>        int best = -1;<br>        for (int i = 0 ; i &lt;= 100 ; i++)<br>            best = Math.max (best, counts [i]);<br>        Vector ret = new Vector ();<br>        for (int i = 0 ; i &lt; scores.length ; i++)<br>        {<br>            if (counts [scores [i]] != best)<br>                continue;<br>            Integer n = new Integer (scores [i]);<br>            if (!ret.contains (n))<br>                ret.add (n);<br>        }<br> <br>        int r[] = new int [ret.size ()];<br>        for (int i = 0 ; i &lt; ret.size () ; i++)<br>            r [i] = ((Integer) ret.get (i)).intValue ();<br> <br>        Arrays.sort(r);<br>        return r;<br>    }<br>}<br></td>