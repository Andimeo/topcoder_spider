            import java.util.*;<br>import java.util.regex.*;<br>import java.io.*;<br><br>public class GridGame {<br>  <br>  Map&lt;BitSet,Integer&gt; map = new HashMap&lt;BitSet,Integer&gt;();<br>  <br>  int win(BitSet bs) {<br>    int a = 0;<br>    for (int p=0; p&lt;16; ++p) {<br>      if (bs.get(p)) continue;<br>      if (p&gt;=4 &amp;&amp; bs.get(p-4)) continue;<br>      if (p+4&lt;16 &amp;&amp; bs.get(p+4)) continue;<br>      if ((p%4)&gt;0 &amp;&amp; bs.get(p-1)) continue;<br>      if ((p%4)&lt;3 &amp;&amp; bs.get(p+1)) continue;<br>      BitSet copy = (BitSet)bs.clone();<br>      copy.set(p);<br>      if (win(copy)==0) ++a;<br>    }<br>    return a;<br>  }<br>  <br>  public int winningMoves(String[] grid) {<br>    ut = System.out;<br>    BitSet bs = new BitSet();<br>    for (int x=0; x&lt;4; ++x) {<br>      for (int y=0; y&lt;4; ++y) {<br>        if (grid[x].charAt(y)=='X') bs.set(4*x+y);<br>      }<br>    }<br>    return win(bs);<br>  }<br><br>  PrintStream ut;<br>  void print(Object... a) {<br>    for (Object o:a) {<br>      ut.print(o+" ");<br>    }<br>    ut.println();<br>  }<br><br>  <br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>