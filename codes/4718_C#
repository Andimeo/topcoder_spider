            using System;<br>using System.Collections;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class SuperString<br>{<br>  public string goodnessSubstring(string[] superstring)<br>  {<br>    int[] fr;<br>    string s = string.Join("", superstring);<br>    string res = "ZZ";<br>    int g = 0;<br>    for (int i = 0; i &lt; s.Length; i++){<br>      fr = new int[26];<br>      int mg = 0, ml = 0;<br>      int cg = 0;<br>      for (int j = i; j &lt; s.Length; j++){<br>        int id = s[j] - 'A';<br>        fr[id]++;<br>        if (fr[id] == 1) cg++;<br>        if (fr[id] == 2) cg--;<br>        if (cg &gt; mg){ mg = cg; ml = j-i+1; }<br>      }<br>      if (mg &gt; g){ g = mg; res = s.Substring(i, ml); }<br>      else if (mg == g){<br>        string s2 = s.Substring(i, ml);<br>        if (s2.CompareTo(res) &lt; 0) res = s2;<br>      }<br>    }<br>    return res;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit]<br></td>