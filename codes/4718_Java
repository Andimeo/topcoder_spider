            import java.util.*;<br> <br>public class SuperString {<br> <br>  final static int oo = 100100100;<br>  String s, result;<br>  <br>  void solvePro() {<br>    int max = -1;<br>    for (int i=0; i&lt;s.length(); i++) {<br>      int[] count = new int[26];<br>      for (int j=i; j&lt;s.length(); j++) {<br>        char c = s.charAt(j);<br>        count[c-'A']++;<br>        if (count[c-'A']==1) {<br>          int sl = 0;<br>          for (int u=0; u&lt;26; u++) if (count[u]==1) sl++;<br>          if (sl&gt;max) {<br>            max = sl;<br>            result = s.substring(i,j+1);<br>          } else<br>          if (sl==max&amp;&amp;result.compareTo(s.substring(i,j+1))&gt;0) result = s.substring(i,j+1);<br>        }<br>      }<br>    }<br>  }<br>  <br>  public String goodnessSubstring(String[] superstring) {<br>    s = "";<br>    for (int i=0; i&lt;superstring.length; i++) s+=superstring[i];<br>    solvePro();<br>    return result;<br>  }<br> <br>  void pp(String s) {<br>    System.out.println(s);<br>  }<br>  <br>  void pp(int s) {<br>    System.out.println(s);<br>  }<br>  <br>}<br> <br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>