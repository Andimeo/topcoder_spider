            using System;<br><br>public class Airways {<br>  const double EPS = 1e-12;<br><br><br>  public double distance(int n, int east, int north) {<br>    double alpha = 2 * Math.PI / n;<br><br>    double res = double.MaxValue;<br><br>    for (int i = 0; i &lt; n; ++i)<br>      for (int j = i + 1; j &lt; n; ++j)<br>        if (2 * (j - i) != n)<br>        {<br>          double a1 = Math.Cos(i * alpha);<br>          double b1 = Math.Cos(j * alpha);<br>          double c1 = east;<br>          double a2 = Math.Sin(i * alpha);<br>          double b2 = Math.Sin(j * alpha);<br>          double c2 = north;<br><br>          double z = a1 * b2 - a2 * b1;<br>          double x = (c1 * b2 - c2 * b1) / z;<br>          double y = (a1 * c2 - a2 * c1) / z;<br><br>          if (x &gt; -EPS &amp;&amp; y &gt; -EPS &amp;&amp; x + y &lt; res)<br>          {<br>            res = x + y;<br>          }<br>        }<br><br>    return res;<br>  }<br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>