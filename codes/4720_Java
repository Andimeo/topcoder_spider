            // Powered by radeclipse!<br>import java.util.* ;<br>public class Airways {<br>  public double distance(int n, int east, int north) {<br>    double ang1 = Math.atan2(north, east) + 2 * Math.PI ;<br>    double low = 2 * Math.PI / n ;<br>    double a = 0 ;<br>    while (a + low &lt; ang1)<br>      a += low ;<br>    double anga = ang1 - a ;<br>    double angb = a + low - ang1 ;<br>    double angc = Math.PI - low ;<br>    double s1 = Math.sqrt(east*(long)east + north*(long)north) ;<br>    double s2 = 0 ;<br>    if (angc != 0)<br>      s2 = s1 * Math.sin(anga) / Math.sin(angc) ;<br>    double s3 = 0 ;<br>    if (angc != 0)<br>      s3 = s1 * Math.sin(angb) / Math.sin(angc) ;<br>    System.out.println("" + s1 + " " + s2 + " " + s3) ;<br>    return Math.abs(s2) + Math.abs(s3) ;<br>  }<br>}<br></td>