            Public Class Airways<br> <br>        Public Function distance(ByVal n As Integer, ByVal east As Integer, ByVal north As Integer) As Double<br> <br>            Dim Angle As Double = System.Math.Atan2(north, east)<br>            <br>            System.Console.WriteLine(CStr(Angle))<br> <br>            Dim theta As Double = 2 * System.Math.PI / n<br>            <br>            System.Console.WriteLine("Angle = " &amp; CStr(Angle) &amp; " Theta = " &amp; CStr(Theta))<br> <br>            Dim First_Dir As Integer = System.Math.Round(Angle / theta)<br>            If First_Dir &gt; Angle/theta then First_dir -= 1<br> <br>            If First_Dir &lt; 0 Then First_Dir += n<br> <br>            Dim First_Angle As Double = First_Dir * theta<br> <br>            Dim Next_Dir As Integer = First_Dir + 1<br> <br>            If Next_Dir = n Then Next_Dir = 0<br> <br>            Dim Next_Angle As Double = Next_Dir * theta<br> <br>            System.Console.WriteLine("Point is between edge " &amp; CStr(First_Dir) &amp; " and edge " &amp; CStr(Next_Dir))<br> <br>            Dim new_east As Double = east * System.Math.Cos(First_Angle) + north * System.Math.Sin(First_Angle)<br>            Dim new_north As Double = -1 * east * System.Math.Sin(First_Angle) + north * System.Math.Cos(First_Angle)<br> <br>            System.Console.WriteLine("New Point new_east = " &amp; CStr(new_east) &amp; " new_north = " &amp; CStr(new_north))<br> <br>            Dim alpha As Double = new_north / System.Math.Sin(theta)<br>      Dim new_theta As Double = System.Math.Atan2(new_north,new_east)<br>      Dim new_dist As Double = (new_north^2+new_east^2)^(1/2)<br>      <br>      System.Console.WriteLine("new_theta = " &amp; CStr(new_theta) &amp; " new_dist = " &amp; CStr(new_dist))<br>      <br>      System.Console.WriteLine("alpha = " &amp; CStr(alpha))<br> <br>            Dim x As Double = new_east - alpha * System.Math.Cos(theta)<br>            <br>            System.Console.WriteLine("x = " &amp; CStr(x))<br> <br>            Return x + alpha<br> <br>        End Function<br> <br>    End Class<br></td>