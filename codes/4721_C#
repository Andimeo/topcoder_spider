            using System;<br> <br>public class HairCuts<br>{<br>    private int[] e;<br>    private int last;<br> <br>    public double maxCut(string[] enter, string lastExit)<br>    {<br>        e = new int[enter.Length];<br>        for (int i = 0; i &lt; enter.Length; i++)<br>        {<br>            e[i] = ParseTime(enter[i]);<br>        }<br>        Array.Sort(e);<br>        last = ParseTime(lastExit);<br>        double low = 5, high = 24*60;<br>        while (low &lt; high)<br>        {<br>            if (Math.Abs(high - low) &lt;= 1e-13)<br>            {<br>                break;<br>            }<br>            double mid = (high + low)/2;<br>            double now = GetLast(mid);<br>            if (now &gt;= last)<br>            {<br>                high = mid;<br>            }<br>            else<br>            {<br>                low = mid;<br>            }<br>        }<br>        double mmid = (high + low)/2;<br>        if (GetLast(mmid) &gt; last + 1e-9) return -1;<br>        return mmid;<br>    }<br> <br>    private double GetLast(double mid)<br>    {<br>        double now = 0;<br>        for (int i = 0; i &lt; e.Length; i++)<br>        {<br>            if (e[i] &gt; now)<br>            {<br>                now = e[i];<br>            }<br>            now += mid;<br>        }<br>        return now;<br>    }<br> <br>    private int ParseTime(string s)<br>    {<br>        int h = (s[0] - '0')*10 + s[1] - '0';<br>        int m = (s[3] - '0')*10 + s[4] - '0';<br>        if (h &lt; 9)<br>        {<br>            h += 12;<br>        }<br>        return h*60 + m;<br>    }<br>}<br> <br>//Powered by [Zigger]<br></td>