            import java.util.*;<br> <br>public class HairCuts<br>{<br>  public double maxCut(String[] enter, String lastExit)<br>  {<br>    int N = enter.length;<br>    int[] m = new int[N];<br>    for (int i = 0; i &lt; enter.length; i++)<br>    {<br>      String[] z = enter[i].split(":");<br>      int hh = Integer.parseInt(z[0]);<br>      int mm = Integer.parseInt(z[1]);<br>      if (hh &lt; 5)<br>        hh += 12;<br>      <br>      m[i] = hh * 60 + mm;<br>    }<br>    for (int i = 0; i &lt; N; i++)<br>    for (int j = i + 1; j &lt; N; j++)<br>      if (m[j] &lt; m[i])<br>      {<br>        int t = m[i]; m[i] = m[j]; m[j] = t;<br>      }<br>    <br>    String[] z = lastExit.split(":");<br>    int last = Integer.parseInt(z[0]) * 60 + Integer.parseInt(z[1]);<br>    for (int i = 0; i &lt; m.length; i++)<br>      while (last &lt;= m[i])<br>        last += 12 * 60;<br>    <br>    double[] d = new double[N];<br>    <br>    double l = 0, r = 100000, w = 0;<br>    while (Math.abs(r - l) &gt; 1e-12)<br>    {<br>      w = (l + r) / 2;      <br>      for (int i = 0; i &lt; N; i++)<br>      {<br>        double start = i == 0 || m[i] &gt; d[i-1] ? m[i] : d[i-1];<br>        d[i] = start + w;<br>      }<br>      <br>      if (d[N-1] &gt; last)<br>        r = w;<br>      else<br>        l = w;<br>    }<br>    <br>    w = (l + r) / 2;<br>    <br>    return w + 1e-9 &lt; 5 ? -1 : w;<br>  }<br> <br>}<br>//Powered by [KawigiEdit]<br></td>