            public class VariableAddition <br>{<br>  public int add(String eq, String[] vars)<br>  {<br>    String[] val=eq.split("\\+");<br>    int ln=val.length;<br>    <br>    int vln=vars.length;<br>    for (int i = 0; i &lt; vln; i++) <br>    {<br>      String q[]=vars[i].split("\\s");<br>       for (int j = 0; j &lt;ln ; j++) <br>       {<br>         if(val[j].equals(q[0]))<br>         {<br>           val[j]=q[1];<br>         }<br>       }<br>    }<br>    int ret=0;<br>  <br>    for (int i = 0; i &lt; ln; i++) <br>   <br>      ret+=Integer.parseInt(val[i]);<br>    <br>    <br>    return ret;<br>    <br>  }<br>}<br></td>