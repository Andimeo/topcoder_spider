            import java.util.*;<br>import java.io.*;<br>import java.math.*;<br>import static java.lang.Math.*;<br> <br>public class PlayingCubes {<br>  String[] cubes;<br>  int num;<br>  boolean canwe(String w, boolean b[])<br>  {<br>    boolean ret = false;<br>    if (w.length()==0)<br>    return true;<br>    for (int i=0; i&lt;num &amp;&amp; !ret; i++) {<br>      if (!b[i]) {<br>        if (cubes[i].indexOf(w.charAt(0))!=-1) {<br>          b[i] = true;<br>          if (canwe(w.substring(1), b)) {<br>            ret = true;<br>          }<br>          b[i] = false;<br>        }<br>      }<br>    }<br>    return ret;<br>  }<br>  public int[] composeWords(String[] _cubes, String[] words) {<br>    ArrayList&lt;Integer&gt; ret = new ArrayList();<br>    cubes = _cubes;<br>    num = cubes.length;<br>    try {<br>      for (int i=0; i&lt;words.length; i++) {<br>        boolean bf[] = new boolean[num];<br>        if (canwe(words[i], bf)) {<br>          ret.add(i);<br>        }<br>      }<br>      <br>    } catch (Exception e) {<br>      System.out.println(e);<br>      e.printStackTrace(System.out);<br>    }<br>    int reta[] = new int[ret.size()];<br>    for (int i=0; i&lt;ret.size(); i++) {<br>      reta[i] = ret.get(i);<br>    }<br>    return reta;<br>  }<br> <br> <br> <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>