            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class TippingWaiters<br>{<br>  public int possiblePayments(int bill, int cash)<br>  {<br>    int temp = bill;<br>    int res =0;<br>    while(temp%5!=0) temp++;<br>    if(temp==bill) temp+=5;<br>    while(temp&lt;=cash &amp;&amp; per(temp,bill)&lt;=0.1) {<br>      if(per(temp,bill)&gt;=0.05) res++;<br>      temp+=5;<br>    }<br>    return res;<br>  }<br>  double per(int a,int b) {<br>    return (double)(a-b)/(double)a;<br>  }<br>}<br> <br>//Powered by [KawigiEdit]<br></td>