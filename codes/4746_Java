            public class IsingModel {<br>    public int energy(String[] spins) {<br>      char[][] grid = new char[spins.length][spins[0].length()];<br>      for (int i = 0; i &lt; spins.length; i++) {<br>        for (int j = 0; j &lt; spins[i].length(); j++) {<br>          grid[i][j] = spins[i].charAt(j);<br>        }<br>      }<br>      int result = 0;<br>      for (int i = 0; i &lt; spins.length - 1; i++) {<br>        for (int j = 0; j &lt; spins[i].length(); j++) {<br>          if (grid[i][j] == grid[i+1][j]) {<br>            result -= 1;<br>          } else {<br>            result += 1;<br>          }<br>        }<br>      }<br>      for (int i = 0; i &lt; spins.length; i++) {<br>        for (int j = 0; j &lt; spins[i].length() - 1; j++) {<br>          if (grid[i][j] == grid[i][j + 1]) {<br>            result -= 1;<br>          } else {<br>            result += 1;<br>          }<br>        }<br>      }<br>        return result;<br>    }<br>}<br></td>