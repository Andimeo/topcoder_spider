            using System;<br><br>public class PackingShapes {<br>  double EPS = 1e-12;<br>  public string[] tryToFit(int width, int height, string[] shapes) {<br>    string[] res = new string[shapes.Length];<br><br>    for (int i = 0; i &lt; shapes.Length; ++i)<br>    {<br>      string[] s = shapes[i].Split(' ');<br><br>      bool ok = false;<br><br>      if (s.Length == 2)<br>      {<br>        int r = int.Parse(s[1]);<br>        ok = (2 * r &lt;= width &amp;&amp; 2 * r &lt;= height);<br>      }<br>      else<br>      {<br>        int a = int.Parse(s[1]);<br>        int b = int.Parse(s[2]);<br><br>        if ((a &lt;= width &amp;&amp; b &lt;= height) || (a &lt;= height &amp;&amp; b &lt;= width))<br>        {<br>          ok = true;<br>        } <br>        else <br>        {<br>          double d = Math.Sqrt(a * a + b * b);<br>          if (d &gt; height + EPS)<br>          {<br>            double alpha = Math.Asin(height / d);<br>            double beta = Math.Asin(a / d);<br>            double gamma = alpha - beta;<br>            if (gamma &gt; EPS)<br>            {<br>              double w = b * Math.Cos(gamma) + a * Math.Sin(gamma);<br>              if (w &lt; width + EPS)<br>                ok = true;<br>            }<br>            beta = Math.Asin(b / d);<br>            gamma = alpha - beta;<br>            if (gamma &gt; EPS)<br>            {<br>              double w = a * Math.Cos(gamma) + b * Math.Sin(gamma);<br>              if (w &lt; width + EPS)<br>                ok = true;<br>            }<br>          }<br>          if (d &gt; width + EPS)<br>          {<br>            double alpha = Math.Asin(width / d);<br>            double beta = Math.Asin(a / d);<br>            double gamma = alpha - beta;<br>            if (gamma &gt; EPS)<br>            {<br>              double w = b * Math.Cos(gamma) + a * Math.Sin(gamma);<br>              if (w &lt; height + EPS)<br>                ok = true;<br>            }<br>            beta = Math.Asin(b / d);<br>            gamma = alpha - beta;<br>            if (gamma &gt; EPS)<br>            {<br>              double w = a * Math.Cos(gamma) + b * Math.Sin(gamma);<br>              if (w &lt; height + EPS)<br>                ok = true;<br>            }<br>          }<br>        }<br>      }<br><br>      if (ok)<br>        res[i] = "YES";<br>      else<br>        res[i] = "NO";<br>    }<br><br>    return res;<br>  }<br><br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>