            using System;<br>using System.Collections;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class CheapestFlights<br>{<br>    int[,] a;<br>    int[] qty;<br>    int n;<br>    double ans = 1e50;<br>    int endLoc;<br>    <br>    private void rec(int v, int step, int spend) {<br>        if (step &lt;= 0) {<br>            if (v == endLoc) {<br>                ans = Math.Min(ans, spend / 1024.0);<br>            }<br>        }<br>        else {<br>            for (int i = 0; i &lt; n; i++)<br>                if (i != v) {<br>                    int t = spend + (a[v, i] &lt;&lt; qty[i]);<br>                    qty[i]--;<br>                    rec(i, step - 1, t);<br>                    qty[i]++;<br>                }<br>        }<br>    }<br>    <br>    public double getLowest(string[] prices, int startLocation, int endLocation, int num) {<br>        n = prices.Length;<br>        a = new int[n, n];<br>        for (int i = 0; i &lt; n; i++)<br>            for (int j = 0; j &lt; n; j++)<br>                a[i, j] = prices[i][j] - '0';<br>                <br>        qty = new int[n];<br>        for (int i = 0; i &lt; n; i++)<br>            qty[i] = 10;<br>            <br>        endLoc = endLocation;<br>        <br>        rec(startLocation, num, 0);<br>        <br>        return ans;<br>    }<br> <br>    [STAThread]<br>    static void Main(string[] args)<br>    {<br>        CheapestFlights obj = new CheapestFlights();<br>    }<br>}<br></td>