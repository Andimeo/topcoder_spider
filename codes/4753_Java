            import java.util.*;<br>public class CheapestFlights {<br><br>    private double res;<br>    private int n;<br>    private int[][] d;<br>    private int[] cnt;<br><br>    private void rec(int cur, int b, int len, double p) {<br>        if (len &lt;= 0) {<br>            if (cur == b) res = Math.min(res, p);<br>            return;<br>        }<br><br>        for (int i = 0; i &lt; n; i++)<br>            if (i != cur) {<br>                cnt[i]++;<br>                rec(i, b, len - 1, p + d[cur][i] / 1.0 / (1 &lt;&lt; (cnt[i] - 1)));<br>                cnt[i]--;<br>            }<br>    }<br><br>    public double getLowest(String[] prices, int startLocation, int endLocation, int num) {<br><br>        n = prices.length;<br>        d = new int[n][n];<br>        cnt = new int[n];<br>        Arrays.fill(cnt, 0);<br>        for (int i = 0; i &lt; n; i++) {<br>            for (int j = 0; j &lt; n; j++)<br>                d[i][j] = prices[i].charAt(j) - '0';<br>        }<br>        res = Double.MAX_VALUE;<br>        rec(startLocation, endLocation, num, 0.0);<br><br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>