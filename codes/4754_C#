            public class BuyingCheap<br>{<br>  public int thirdBestPrice(int[] prices) <br>  {<br>    System.Collections.ArrayList lijst = new System.Collections.ArrayList();<br>    for(int i = 0; i &lt; prices.Length; i++) <br>    {<br>      if(lijst.IndexOf(prices[i]) &lt; 0) <br>      {<br>        lijst.Add(prices[i]);<br>      }<br>    }<br>    lijst.Sort();<br>    if(lijst.Count &lt; 3) <br>    {<br>      return -1;<br>    }<br>    else <br>    {<br>      return int.Parse(lijst[2].ToString());<br>    }<br>  }<br>}<br></td>