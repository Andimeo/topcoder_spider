            #include &lt;algorithm&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>using namespace std;<br><br>class BuyingCheap {<br>public:<br>  int thirdBestPrice(vector &lt;int&gt; v) {<br>    int l = v.size();<br>    sort(v.begin(), v.end());<br>    int t = unique(&amp;v[0], &amp;v[l]) - &amp;v[0];<br>    if (t &lt; 3) return -1;<br>    else return v[2];<br>  }<br><br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>