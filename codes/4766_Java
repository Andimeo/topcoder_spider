            import java.util.regex.*;<br>import java.util.*;<br>public class GridPointsOnCircle {<br>  public int countPoints(int rSquare) <br>  {<br>  int out= 0 ;<br>  for(int i=0;i*i&lt;rSquare;i++)<br>    {<br>    int diff= rSquare - i*i;<br>    int sqrt = (int) (Math.sqrt(diff) + 0.001);<br> <br>    if(sqrt*sqrt == diff)<br>      out++;<br> <br> <br> <br> <br>    }<br> <br> <br> <br>  return 4*out;<br>  }<br> <br>  //int dx[]={0,-1,0,1},dy[]={1,0,-1,0};<br>  //int dx[]={0,1,1,1,0,-1,-1,-1},dy[]={1, 1,0,-1,-1,-1,0,1};<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>