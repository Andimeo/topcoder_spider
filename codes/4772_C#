<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br> <br>public class Party<br>{<br>  public static double averageNames(int n, int[] personA, int[] personB)<br>  {<br>    int i, j, sum = 0;<br>    bool[,] know = new bool[100, 100];<br>    for (i = 0; i &lt; personA.Length; i++)<br>    {<br>      know[personA[i], personB[i]] = true;<br>      know[personB[i], personA[i]] = true;<br>      for (j = 0; j &lt; n; j++)<br>        if (j != personA[i] &amp;&amp; j != personB[i])<br>        {<br>          if (know[personA[i], j])<br>            know[personB[i], j] = true;<br>          else<br>          if (know[personB[i], j])<br>            know[personA[i], j] = true;<br>        }<br>    }<br>    for (i = 0; i &lt; n; i++)<br>      for (j = 0; j &lt; n; j++)<br>        if (i != j &amp;&amp; know[i, j])<br>          sum++;<br>    return((double) sum / (double) n);<br>  }<br>}<br></td>