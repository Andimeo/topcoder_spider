            import java.util.*;<br>public class Party {<br>  public double averageNames(int n, int []A, int []B){<br>    Map&lt;Integer, Set&lt;Integer&gt; &gt; m = new HashMap();<br>    for (int i=0;i&lt;n;i++) m.put(i, new TreeSet&lt;Integer&gt;());<br>    for (int i=0;i&lt;A.length;i++){<br>       Set&lt;Integer&gt; sa = m.get(A[i]);<br>       Set&lt;Integer&gt; sb = m.get(B[i]);<br>       sa.addAll(sb);<br>       sb.addAll(sa);<br>       sa.add(B[i]);<br>       sb.add(A[i]);<br>    }<br>    double total = 0;<br>    for (int i=0;i&lt;n;i++){<br>             m.get(i).remove(i);<br>       total += m.get(i).size();<br>    }<br>    return total / (n*1.0);<br>   <br>   }<br>  }<br></td>