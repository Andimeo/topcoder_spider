            using System;<br>using System.Collections;<br><br>public class DVDPlayer {<br>  public string[] findMovies(string[] moviesWatched) {<br>    Hashtable movie = new Hashtable();<br><br>    string inDVD = "";<br><br>    for (int i = 0; i &lt; moviesWatched.Length; ++i)<br>    {<br>      string cur = moviesWatched[i];<br><br>      if (!movie.ContainsKey(cur))<br>        movie[cur] = cur;<br><br>      if (inDVD != "")<br>        movie[inDVD] = (string) movie[cur];<br>      movie[cur] = "";<br>      inDVD = cur;<br>    }<br><br>    ArrayList res = new ArrayList();<br><br>    foreach (DictionaryEntry e in movie)<br>    {<br>      string what = (string) e.Key;<br>      string where = (string) e.Value;<br><br>      if (what.CompareTo(where) != 0 &amp;&amp; where != "")<br>        res.Add(what + " is inside " + where + "'s case");<br>    }<br><br>    string[] r = new string[res.Count];<br>    res.CopyTo(r);<br>    Array.Sort(r);<br><br>    return r;<br>  }<br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>