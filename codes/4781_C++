            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;map&gt;<br>using namespace std;<br> <br>class DVDPlayer {<br>  public:<br>  vector &lt;string&gt; findMovies(vector &lt;string&gt; m) {<br>    map&lt;string,string&gt; mtc;<br>    for(int i=0; i&lt;m.size(); i++) {<br>      mtc[m[i]] = m[i];<br>    }<br>    mtc[m[0]] = "";<br>    string current = m[0];<br>    for(int i=1; i&lt;m.size(); i++) {<br>      mtc[current] = mtc[m[i]];<br>      mtc[m[i]] = "";<br>      current = m[i];<br>    }<br>    <br>    vector&lt;string&gt; r;<br>    for(map&lt;string,string&gt;::iterator it=mtc.begin(); it!=mtc.end(); it++) if (it-&gt;first != it-&gt;second &amp;&amp; it-&gt;second != "") {<br>      r.push_back( it-&gt;first + " is inside " + it-&gt;second + "'s case" );<br>    }<br>    sort(r.begin(), r.end());<br>    <br>    return r;<br>    <br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>