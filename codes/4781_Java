            import java.util.*;<br> <br>public class DVDPlayer {<br>    public String[] findMovies(String[] moviesWatched) {<br>        HashMap map = new HashMap();<br>        int n = moviesWatched.length;<br>        for (int i = 0; i &lt; n; i++) {<br>            String watched = moviesWatched[i];<br>            String where;<br>            if (map.containsKey(watched)) {<br>                where = (String) map.get(watched);<br>                map.remove(watched);<br>            } else {<br>                where = watched;<br>            }<br>            if (i &gt; 0) {<br>                map.put(moviesWatched[i - 1], where);<br>            }<br>        }<br> <br>        int m = 0;<br>        for (Iterator it = map.keySet().iterator(); it.hasNext();) {<br>            String mv = (String) it.next();<br>            String bx = (String) map.get(mv);<br>            if (!mv.equals(bx)) {<br>                m++;<br>            }<br>        }<br>        String[] r = new String[m];<br>        int i = 0;<br>        for (Iterator it = map.keySet().iterator(); it.hasNext();) {<br>            String mv = (String) it.next();<br>            String bx = (String) map.get(mv);<br>            if (!mv.equals(bx)) {<br>                r[i++] = mv + " is inside " + bx + "'s case";<br>            }<br>        }<br>        Arrays.sort(r);<br>        return r;<br>    }<br> <br>}<br> <br> <br>// Powered by PopsEdit<br> <br>// Powered by CodeProcessor<br></td>