            Imports Microsoft.VisualBasic<br>Imports System<br>Imports System.Collections<br>Imports System.Text<br> <br>Public Class DVDPlayer<br>    Public Function findMovies(ByVal moviesWatched As String()) As String()<br> <br>        Dim i As Integer<br>        Dim N As Integer = moviesWatched.Length - 1<br>        Dim src(N) As String<br>        For i = 0 To N<br>            If src.IndexOf(src, moviesWatched(i)) = -1 Then<br>                src(i) = moviesWatched(i)<br>            End If<br>        Next<br> <br>        Dim mw(src.Length - 1) As String<br>        For i = 0 To src.Length - 1<br>            mw(i) = src(i)<br>        Next<br> <br>        Dim DVD As String = String.Empty<br>        Dim i1, i2 As Integer<br>        Dim tmp As String<br>        For i = 0 To N<br>            i1 = moviesWatched.IndexOf(mw, moviesWatched(i))<br>            tmp = DVD<br>            DVD = mw(i1)<br>            mw(i1) = tmp<br>        Next<br> <br>        Dim al As New ArrayList<br>        For i = 0 To N<br>            If mw(i) &lt;&gt; src(i) And mw(i) &lt;&gt; String.Empty Then<br>                al.Add(mw(i) &amp; " is inside " &amp; src(i) &amp; "'s case")<br>            End If<br>        Next<br>        al.Sort()<br>        Dim ret(al.Count - 1) As String<br>        For i = 0 To al.Count - 1<br>            ret(i) = al.Item(i).ToString<br>        Next<br> <br>        Return ret<br> <br>    End Function<br>End Class<br></td>