            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cctype&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;queue&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;utility&gt;<br>#include &lt;numeric&gt;<br>#include &lt;complex&gt;<br><br>using namespace std;<br><br>class NearBirthdays {<br>  public:<br>  double probability(int p, int k, int d) {<br>    <br>    int w = k+1;<br>    if (d - p*w &lt; 0) return 1;<br>    double ans = 1;<br>    for (int i = 0; i &lt; p-1; i++)<br>      ans *= (d-p*w+p-1 - i) / (double) d;<br>    return 1 - ans;<br>    <br>  }<br>};<br><br><br>// Powered by FileEdit<br></td>