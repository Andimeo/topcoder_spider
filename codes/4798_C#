            using System;<br><br>public class MatchCounts {<br>  public int howMany(string[] ability) {<br>    return rec(ability, 0, new bool[6]);<br>  }<br><br>  int rec(string[] ability, int at, bool[] can)<br>  {<br>    if (at &gt;= ability.Length)<br>      return 1;<br>    int res = 0;<br>    foreach (char ch in ability[at])<br>      if (!can[ch - '0'])<br>      {<br>        can[ch - '0'] = true;<br>        res += rec(ability, at + 1, can);<br>        can[ch - '0'] = false;<br>      }<br>    return res;<br>  }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>