            #include &lt;vector&gt;<br>#include &lt;string&gt;<br>using namespace std;<br><br>vector&lt;string&gt; a;<br>int b[10];<br>int ret;<br>void gao(int i) {<br>  if (i == a.size()) {<br>    ret++;<br>  } else {<br>    for (int j = 0; j &lt; a[i].size(); ++j) {<br>      if (b[a[i][j]-'0']) {<br>        b[a[i][j]-'0'] = 0;<br>        gao(i+1);<br>        b[a[i][j]-'0'] = 1;<br>      }<br>    }<br>  }<br>}<br>class MatchCounts{<br>public:<br>  int howMany(vector&lt;string&gt; ability){<br>    a = ability;<br>    for (int i = 0; i &lt; 10; ++i) b[i] = 1;<br>    ret = 0;<br>    gao(0);<br>    return ret;<br>    <br>  }<br>};<br></td>