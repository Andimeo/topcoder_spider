            import java.util.*;<br> <br>public class MatchCounts {<br>    int n;<br>    boolean[][] can;<br>    int ans;<br>    boolean[] u;<br> <br>    void bt(int x) {<br>        if (x == n) {<br>            ans++;<br>            return;<br>        }<br>        for (int i = 0; i &lt; 6; i++) {<br>            if (!u[i] &amp;&amp; can[x][i]) {<br>                u[i] = true;<br>                bt(x + 1);<br>                u[i] = false;<br>            }<br>        }<br>    }<br> <br>    public int howMany(String[] ability) {<br>        n = ability.length;<br>        can = new boolean[n][6];<br>        for (int i = 0; i &lt; n; i++) {<br>            for (int j = 0; j &lt; ability[i].length(); j++) {<br>                can[i][ability[i].charAt(j) - '0'] = true;<br>            }<br>        }<br>        u = new boolean[6];<br>        ans = 0;<br>        bt(0);<br>        return ans;<br>    }<br> <br> <br>}<br> <br> <br>// Powered by PopsEdit<br> <br>// Powered by CodeProcessor<br></td>