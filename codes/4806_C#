            using System;<br>using System.Text ;<br>using System.Collections ;<br> <br>public class SandwichBar<br>{<br>  Hashtable h ;<br>  bool check( string order )<br>  {<br>    string[] o = order.Split(' ') ;<br>    foreach( string s in o )<br>      if( !h.Contains( s ) )<br>        return false ;<br>    return true ;<br>  }<br>  public int whichOrder(string[] available, string[] orders)<br>  {<br>    h = new Hashtable() ;<br>    foreach( string s in available )<br>    {<br>      if( !h.Contains(s) )<br>        h.Add(s,1) ;<br>    }<br>    for( int i=0 ; i&lt;orders.Length ; i++ )<br>      if( check(orders[i]) )<br>        return i ;<br>    return -1 ;<br>    <br>  }<br>  <br>}<br></td>