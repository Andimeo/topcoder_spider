            using System;<br>using System.Collections;<br>public class ExploringEuropa {<br>  <br>  public int travelTime(string surface, int delay) {<br>    <br>    int minDist = 0;<br>    int dist1 = 0;<br>    int firstV = 0;<br><br>    for (int i = 0; i &lt; surface.Length; i++)<br>      if (surface[i] == 'V')<br>      {<br>        if (dist1 == 0)<br>        {<br>          firstV = i;<br>          dist1 = delay * 5 + firstV;<br>          minDist = dist1;<br>        }<br>        else<br>        {<br>          int distFirstV = i - firstV;<br>          if ( distFirstV &lt;= delay)<br>            minDist = Math.Min(minDist, dist1 - distFirstV);<br>          else if (distFirstV &lt;= delay * 2)<br>            minDist = Math.Min(minDist, dist1 - delay + 3 * (distFirstV - delay));<br>        }<br>      }<br>    <br>    return minDist;<br>  }<br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>