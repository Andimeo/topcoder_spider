            import java.util.*;<br> <br>public class BedroomFloor {<br>    public long numberOfSticks(int x1, int y1, int x2, int y2) {<br>        long[] c = new long[6];<br>        int s1 = 0;<br>        int s2 = 0;<br>        for (int x = x1; x &lt; x2; x++) {<br>            int z = x / 5;<br>            if (z % 2 == 0) s1++;<br>            else s2++;<br>        }<br>        for (int y = 0; y &lt; y2; y += 5) {<br>            int t = Math.max(y, y1);<br>            int b = Math.min(y + 5, y2);<br>            if (b &gt; t) {<br>                int l = b - t;<br>                if (((y / 5) % 2) == 0) {<br>                    c[l] += s2;<br>                } else {<br>                    c[l] += s1;<br>                }<br>            }<br>        }<br> <br>        s1 = 0;<br>        s2 = 0;<br>        for (int y = y1; y &lt; y2; y++) {<br>            int z = y / 5;<br>            if (z % 2 == 0) s1++;<br>            else s2++;<br>        }<br>        for (int x = 0; x &lt; x2; x += 5) {<br>            int t = Math.max(x, x1);<br>            int b = Math.min(x + 5, x2);<br>            if (b &gt; t) {<br>                int l = b - t;<br>                if (((x / 5) % 2) == 0) {<br>                    c[l] += s1;<br>                } else {<br>                    c[l] += s2;<br>                }<br>            }<br>        }<br> <br>        System.out.println(Arrays.toString(c));<br> <br>        long res = c[5];<br>        res += c[4];<br>        c[1] -= Math.min(c[1], c[4]);<br>        res += c[3];<br>        long t = c[3];<br>        long y = Math.min(t, c[2]);<br>        c[2] -= y;<br>        t -= y;<br>        c[1] -= Math.min(t * 2, c[1]);<br> <br>        t = (c[2] + 1) / 2;<br>        res += t;<br>        c[1] -= Math.min(t * 5 - c[2] * 2, c[1]);<br> <br>        res += (c[1] + 4) / 5;<br> <br>        return res;<br>    }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>