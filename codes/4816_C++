            #include &lt;algorithm&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;ctime&gt;<br>#include &lt;map&gt;<br>using namespace std;<br> <br>bool same(int base, int a, int b){<br>  int atA = 1, atB = 1;<br>  for(int i = 0; i &lt; base + 1; i++){<br>    if(atA != atB) return false;<br>    atA = (atA * base) % a;<br>    atB = (atB * base) % b;<br>  }<br>  return true;<br>}<br> <br>class DivisibilityRules { public:<br> <br> <br>int similar(int nb, int div){<br>  int ret = 0;<br>  for(int i = 2; i &lt; nb; i ++){<br>    if(same(nb, div, i)) ret++;<br>  }<br>  <br>  return ret;<br>}<br> <br> <br>};<br> <br> <br>//Powered by [KawigiEdit] + aussie<br></td>