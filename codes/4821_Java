            public class SwimmersDelight {<br>  public int longestJump (int [] x, int [] y) {<br>    double only1 = Math.max (x[0], 10-x[0]);<br>    double only2 = Math.max (x[1], 10-x[1]);<br>    double dist  = Math.sqrt((x[0] - x[1]) * (x[0] - x[1])  +  (y[0] - y[1]) * (y[0] - y[1]));<br>    double all12 = Math.max (x[0], Math.max (dist, 10-x[1]));<br>    double all21 = Math.max (x[1], Math.max (dist, 10-x[0]));<br> <br>    double md = Math.min (only1, Math.min (only2, Math.min (all12, all21)));<br>    return (int) Math.round (md);<br>  }<br>}<br></td>