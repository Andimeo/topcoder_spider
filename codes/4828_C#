            using System;<br> <br>public class AirlinerSeats<br>{<br>  public string[] mostAisleSeats(int width, int seats)<br>  {<br>    char[] chs = new char[width];<br>    while(3*(width - seats)&lt;width)<br>    {<br>      seats--;<br>      chs[--width] = 'S';<br>    }<br>    for(int i=1; i&lt;=width; i++)<br>    {<br>      if(seats==0)<br>        chs[width-i] = '.';<br>      else<br>      {<br>        if((i%3)==2)<br>          chs[width-i] = '.';<br>        else<br>        {<br>          chs[width-i] = 'S';<br>          seats--;<br>        }<br>      }<br>    }<br>    string str = new string(chs);<br>    if(str.Length&lt;=50)<br>      return new string[]{str};<br>    if(str.Length&lt;=100)<br>      return new string[]{str.Substring(0,50), str.Substring(50)};<br>    return new string[]{str.Substring(0,50), str.Substring(str.Length-50, 50)};<br>  }<br>}<br></td>