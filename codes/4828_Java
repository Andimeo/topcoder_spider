            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import static java.lang.Math.*;<br>import static java.util.Arrays.*;<br>public class AirlinerSeats {<br>  public String[] mostAisleSeats(int width, int seatsn) {<br>    char[] seats = new char[width];<br>    int ail = width-seatsn;<br>    int at = 0;<br>    boolean lead = false;<br>    while(ail*2 &gt; seatsn){<br>      lead = true;<br>      seats[at++] = '.';<br>      ail--;<br>    }<br>    if(!lead){<br>      while(ail*2 &lt; seatsn){<br>        seats[--width] = 'S';<br>        seatsn--;<br>      }<br>    }<br>    if(ail*2 &lt; seatsn){<br>      seats[at++] = 'S';<br>      seatsn--;<br>    }<br>    while(at &lt; width){<br>      seats[at++] = 'S';<br>      seats[at++] = '.';<br>      seats[at++] = 'S';<br>    }<br>    String s = new String(seats);<br>    if(seats.length &lt;= 50) return new String[] {s};<br>    if(seats.length &lt;= 100) return new String[] {s.substring(0,50),s.substring(50)};<br>    return new String[] {s.substring(0,50), s.substring(s.length()-50)};<br>  }<br> <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>