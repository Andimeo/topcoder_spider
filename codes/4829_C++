            #include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;cstdio&gt;<br>using namespace std;<br> <br>#define V(x) vector&lt;x &gt;<br>#define vi vector&lt;int&gt;<br>#define vd vector&lt;double&gt;<br>#define vs vector&lt;string&gt;<br> <br>#define fr(i,a,b) for((i)=(a);(i)&lt;(b);(i)++)<br>#define fo(i,n) fr(i,0,n)<br>#define fir(n) fo(i,n)<br>#define fjr(n) fo(j,n)<br>#define fkr(n) fo(k,n)<br>#define flr(n) fo(l,n)<br>#define fi fir(n)<br>#define fj fjr(n)<br>#define fk fkr(n)<br>#define fl flr(n)<br> <br> <br>#define ll long long<br>#define df double<br>#define pb(x) push_back(x)<br>#define cs c_str()<br>#define sz size()<br> <br> <br>int u[60];<br> <br>int f[60][60];<br> <br>class XORing { public: ll findSubset(vs g, vi y) {<br>int n=g.sz,m=g[0].sz;<br>int i,j,k,c,r;<br> <br>fi fjr(m) f[i][j]=((g[i][j]=='1')?1:0);<br> <br>for(c=0;c&lt;m;c++) fo(r,n) if(!u[r] &amp;&amp; f[r][c]) {<br>u[r]=1;<br>fi if(i!=r &amp;&amp; f[i][c]) { fjr(m) f[i][j]^=f[r][j]; y[i]^=y[r]; }<br>break;<br>}<br> <br>fi { fjr(m) if(f[i][j]) break; if(j==m &amp;&amp; y[i]) return 0; }<br> <br>ll ret=m;<br> <br>fi if(u[i]) ret--;<br> <br>ret=1LL&lt;&lt;(ret);<br>return ret;<br>}};<br></td>