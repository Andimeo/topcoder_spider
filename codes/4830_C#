            #include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;queue&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;math.h&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;numeric&gt;<br>using namespace std;<br>typedef vector&lt;int&gt; vi;<br>typedef vector&lt;double&gt; vd;<br>typedef vector&lt;string&gt; vs;<br>typedef vector&lt;vector&lt;int&gt; &gt; vvi;<br>typedef vector&lt;vector&lt;string&gt; &gt; vvs;<br>#define fri(i, n) for(i = 0; i &lt; (n); ++i)<br>#define frc(c, b, n) for (c = (b); c &lt; (n); ++c)<br>#define rep(i, b, e) for (i = (b); i &lt; (e); ++i)<br>#define brep(i, e, b) for (i = (e); i &gt; (b); --i)<br><br>class AccessChanger {<br>public:<br>  vector &lt;string&gt; convert(vector &lt;string&gt; program) {<br>    string s;<br>    for (int i = 0; i &lt; program.size(); ++i) {<br>      s = "";<br>      for (int j = 0; j &lt; program[i].size(); ++j) {<br>        if (program[i].substr(j, 2) == "//") {<br>          s += program[i].substr(j);<br>          break;<br>        }<br>        if (program[i].substr(j, 2) == "-&gt;") {<br>          s += ".";<br>          j++;<br>        }<br>        else<br>          s += program[i][j];<br>      }<br>      program[i] = s;<br>    }<br>    return program;<br>  }<br><br><br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>