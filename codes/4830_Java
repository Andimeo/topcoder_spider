            import java.util.*;<br>public class AccessChanger {<br>    public String[] convert(String[] program) {<br>        for (int x=0; x&lt;program.length; x++) {<br>            String s = program[x];<br>            String ss = "";<br>            if (s.indexOf("//") != -1) {<br>                ss = s.substring(s.indexOf("//"));<br>                s = s.substring(0, s.indexOf("//"));<br>            }<br>            while (s.indexOf("-&gt;") != -1)<br>                s = s.replaceAll("-&gt;",".");<br>            program[x] = s + ss;<br>        }<br>        return program;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>