            Public Class AccessChanger<br> <br>    Public Function convert(ByVal program As String()) As String()<br> <br>        Dim x, y, z As Integer<br>        Dim a, b, c As String<br>        Dim change As Boolean<br> <br>        For x = 0 To program.GetUpperBound(0)<br>            For y = 1 To microsoft.visualbasic.Len(program(x)) - 1<br>                If microsoft.visualbasic.Mid$(program(x), y, 2) = "-&gt;" Then<br>                    program(x) = microsoft.visualbasic.Left(program(x), y - 1) + "." + microsoft.visualbasic.Right(program(x), microsoft.visualbasic.Len(program(x)) - y - 1)<br>                ElseIf microsoft.visualbasic.Mid$(program(x), y, 2) = "//" Then<br>                    y = microsoft.visualbasic.Len(program(x)) - 1<br>                End If<br>            Next y<br>        Next x<br>        Return program<br>    End Function<br> <br>End Class<br></td>