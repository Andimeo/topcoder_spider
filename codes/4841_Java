            import java.util.*;<br>import java.lang.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>import static java.lang.String.*;<br>import static java.lang.System.*;<br>import static java.lang.Math.*;<br>import static java.util.Arrays.*;<br>import static java.lang.Double.*;<br>import static java.lang.Integer.*;<br>import static java.lang.Long.*;<br> <br>class Ds {<br>  public int test;<br>}<br> <br>class Cmp implements Comparator {<br>  public int compare(Object o1, Object o2) {<br>    Ds a = (Ds)o1;<br>    Ds b = (Ds)o2;<br>    return a.test - b.test;<br>  }<br>}<br> <br>public class CoinPiles {<br>  static public int i, j, k;<br>  static Scanner cin = new Scanner(in);<br> <br>  static final int INF = 0x7fffffff;<br>  static final long INFL = 0x7fffffffffffffffL;<br>  static final double EPS = 1e-8;<br>  public static int organize(int[] sizes) {<br>    int res, pren, tot, pre, preh, len = sizes.length;<br>    sort(sizes);<br>    pre = sizes[0];<br>    preh = 1;<br>    tot = 1;<br>    res = 1;<br>    pren = 0;<br>    for (i = 1; i &lt; len; i++) {<br>      if (sizes[i] == sizes[i-1]) {<br>        pren++;<br>      } else pren = 0;<br>      if (sizes[i] &gt; pre &amp;&amp; i - tot + 1 - pren &gt; preh) {<br>        tot += preh + 1;<br>        pre = sizes[i];<br>        preh = preh + 1;<br>        res ++;<br>      }<br>    }<br>    return res;<br>  }<br>  public static void main(String[] argv) {<br>    out.printf("%d %d %d\n", i, j, k);<br>    String s = String.format("%d %d", i, j);<br>    out.println(s);<br>  }<br>}<br> <br>//Written by King_Bette<br></td>