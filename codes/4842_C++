            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br>#define FOR(i, n) for(int i=0; i&lt;(n); i++)<br> <br>using namespace std;<br> <br>class SimpleDuplicateRemover {<br>public:<br>  vector &lt;int&gt; process(vector &lt;int&gt; s) {<br>    map&lt;int,int&gt; m;<br>    FOR(i, s.size()) {<br>      m[s[i]]++;<br>    }<br>    vector&lt;int&gt; ret;<br>    FOR(i, s.size()) {<br>      m[s[i]]--;<br>      if (!m[s[i]])<br>        ret.push_back(s[i]);<br>    }<br>    return ret;<br>  }<br>};<br></td>