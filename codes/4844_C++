<td class="problemText" colspan="8" valign="middle" align="left">
            #include&lt;iostream&gt;<br>#include&lt;vector&gt;<br>using namespace std;<br>int f[50],ma,i,j,sum,n;<br>class InsertSort{<br>public:<br>int calcMinimalCost(vector&lt;int&gt; x){<br>   n=x.size();<br>   sum=0;<br>   for(i=0;i&lt;n;i++){<br>      sum+=x[i];<br>      f[i]=x[i];<br>      for(j=0;j&lt;i;j++)<br>         if(x[j]&lt;=x[i]&amp;&amp;f[j]+x[i]&gt;f[i])<br>            f[i]=f[j]+x[i];<br>      if(f[i]&gt;ma)<br>         ma=f[i];<br>   }<br>   return sum-ma;<br>}<br>};<br></td>