            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br><br>public class InsertSort<br>{<br>  public int calcMinimalCost(int[] theArray)<br>  {<br>    int[] n = new int[theArray.length];<br>    <br>    int sum =0;<br>    for(int c1=0; c1&lt;theArray.length; c1++){<br>      sum += theArray[c1];<br>    }<br>    <br>    for(int c1=theArray.length-1; c1&gt;=0; c1--){<br>      int max = theArray[c1];<br>      <br>      for(int c2=c1+1; c2&lt;theArray.length; c2++){<br>        if(theArray[c1] &lt;= theArray[c2]){<br>          if(theArray[c1] + n[c2] &gt; max)<br>            max = theArray[c1] + n[c2];<br>        }<br>      }<br>      <br>      <br>      n[c1] = max;<br>    }<br>    <br>    int max = n[0];<br>    for(int c1=0; c1&lt;n.length; c1++){<br>      if(n[c1] &gt; max)<br>        max = n[c1];<br>    }<br>    return sum - max;<br>  <br>    <br>  }<br>  <br><br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>