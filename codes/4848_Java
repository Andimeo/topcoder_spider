            import java.util.*;<br> <br>public class JumpyNum {<br>  <br>  final static int oo = 100100100;<br>  final static double eps = 1e-9;<br>  int m, n;<br>  int result;<br>  int[][] sl;<br>  int[][][] d;<br>  String s;<br>  <br>  void init() {<br>    sl = new int[11][10];<br>    for (int i=0; i&lt;10; i++) sl[1][i] = 1;<br>    for (int i=2; i&lt;11; i++)<br>    for (int j=0; j&lt;10; j++) {<br>      sl[i][j] = 0;<br>      for (int k=0; k&lt;10; k++)<br>      if (Math.abs(j-k)&gt;=2) sl[i][j]+=sl[i-1][k];<br>    }<br>  }<br>  <br>  int dem(int scs) {<br>    if (scs==1) return 9;<br>    int r = 0;<br>    for (int i=1; i&lt;10; i++)<br>    for (int j=0; j&lt;10; j++)<br>    if (Math.abs(i-j)&gt;=2) r += sl[scs-1][j];<br>    return r;<br>  }<br>  <br>  int dp(int p, int last, int less) {<br>    if (d[p][last][less]!=-1) return d[p][last][less];<br>    <br>    int r = 0;<br>    if (p==s.length()-1) return less;<br>    for (int i=0; i&lt;10; i++)<br>    if (Math.abs(i-last)&gt;=2) {<br>      if (less==0&amp;&amp;i&gt;s.charAt(p+1)-'0') break;<br>      int less2;<br>      if (less==1) less2 = 1; else<br>      if (i&lt;s.charAt(p+1)-'0') less2 = 1; else less2 = 0;<br>      r += dp(p+1, i, less2);<br>    }<br>    return (d[p][last][less] = r);<br>  }<br>  <br>  int count(int ct) {<br>    s = Integer.toString(ct);<br>    int r = 0;<br>    for (int i=1; i&lt;s.length(); i++) r+= dem(i);<br>    <br>    d = new int[11][10][2];<br>    for (int i=0; i&lt;11; i++)<br>    for (int j=0; j&lt;10; j++) Arrays.fill(d[i][j], -1);<br>    <br>    for (int i=1; i&lt;=s.charAt(0)-'0'; i++) {<br>      if (i&lt;s.charAt(0)-'0') r += dp(0, i, 1);<br>      else r += dp(0, i, 0);<br>    }<br>    return r;<br>  }<br> <br>  public int howMany(int low, int high) {<br>    init();<br>    result = count(high+1) - count(low);<br>    return result;<br>  }<br> <br>}<br> <br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>