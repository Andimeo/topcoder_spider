            import java.util.ArrayList;<br>public class Matrix {<br>  private static String [][] data = { { "a", "b", "c" } ,<br>                                      { "d", "e", "f" } };<br>  public String[][] rotatePos(String data[][]) {<br>    String tmp[][] = new String[data[0].length][data.length];<br>    int row, col;<br>    for(row = 0; row &lt; data.length; row++)<br>      for(col = 0; col &lt; data[row].length; col++)<br>        tmp[col][data.length - row - 1] = data[row][col];<br>    return tmp;<br>  }<br>  public String[][] rotateNeg(String data[][]) {<br>    String tmp[][] = new String[data[0].length][data.length];<br>    int row, col;<br>    for(row = 0; row &lt; data.length; row++)<br>      for(col = 0; col &lt; data[row].length; col++)<br>        tmp[data[row].length - col - 1][row] = data[row][col];<br>    return tmp;<br>  }<br>  public String[][] hshiftPos(String data[][]) {<br>    String tmp[][] = new String[data.length][data[0].length];<br>    int row, col;<br>    for(row = 0; row &lt; data.length; row++)<br>      for(col = 0; col &lt; data[row].length; col++)<br>        tmp[row][(col + 1) % data[row].length] = data[row][col];<br>    return tmp;<br>  }<br>  public String[][] hshiftNeg(String data[][]) {<br>    String tmp[][] = new String[data.length][data[0].length];<br>    int row, col;<br>    for(row = 0; row &lt; data.length; row++)<br>      for(col = 0; col &lt; data[row].length; col++)<br>        tmp[row][(col + data[row].length - 1) % data[row].length] = data[row][col];<br>    return tmp;<br>  }<br>  public String[][] vshiftPos(String data[][]) {<br>    String tmp[][] = new String[data.length][data[0].length];<br>    int row, col;<br>    for(row = 0; row &lt; data.length; row++)<br>      for(col = 0; col &lt; data[row].length; col++)<br>        tmp[(row + data.length - 1) % data.length][col] = data[row][col];<br>    return tmp;<br>  }<br>  public String[][] vshiftNeg(String data[][]) {<br>    String tmp[][] = new String[data.length][data[0].length];<br>    int row, col;<br>    for(row = 0; row &lt; data.length; row++)<br>      for(col = 0; col &lt; data[row].length; col++)<br>        tmp[(row + 1) % data.length][col] = data[row][col];<br>    return tmp;<br>  }<br>  public ArrayList juggle(ArrayList commands) {<br>    int com;<br>    for(com = 0; com &lt; commands.size(); com++) {<br>      if(((Character)commands.get(com)).charValue() == 't') {<br>        String tmp[][] = new String[data[0].length][data.length];<br>        int row, col;<br>        for(row = 0; row &lt; data.length; row++)<br>          for(col = 0; col &lt; data[row].length; col++)<br>            tmp[col][row] = data[row][col];<br>        data = tmp;<br>      } else if(((Character)commands.get(com)).charValue() == 'r') {<br>        com++;<br>        int num = ((Integer)commands.get(com)).intValue();<br>        int i;<br>        if(num &gt; 0) {<br>          for(i = 0; i &lt; num; i++)<br>            data = rotatePos(data);<br>        } else {<br>          for(i = 0; i &lt; -num; i++)<br>            data = rotateNeg(data);<br>        }<br>      } else if(((Character)commands.get(com)).charValue() == 'h') {<br>        com++;<br>        int num = ((Integer)commands.get(com)).intValue();<br>        int i;<br>        if(num &gt; 0) {<br>          for(i = 0; i &lt; num; i++)<br>            data = hshiftPos(data);<br>        } else {<br>          for(i = 0; i &lt; -num; i++)<br>            data = hshiftNeg(data);<br>        }<br>      } else if(((Character)commands.get(com)).charValue() =='v') {<br>        com++;<br>        int num = ((Integer)commands.get(com)).intValue();<br>        int i;<br>        if(num &gt; 0) {<br>          for(i = 0; i &lt; num; i++)<br>            data = vshiftPos(data);<br>        } else {<br>          for(i = 0; i &lt; -num; i++)<br>            data = vshiftNeg(data);<br>        }<br>      }<br>    }<br>    ArrayList list = new ArrayList();<br>    int row, col;<br>    for(row = 0; row &lt; data.length; row++) {<br>      ArrayList rowlist = new ArrayList();<br>      for(col = 0; col &lt; data[row].length; col++)<br>        rowlist.add(data[row][col]);<br>      list.add(rowlist);<br>    }<br>    return list;<br>  }<br>}<br></td>