            import java.util.*;<br>public class TopPilot {<br>  public int addFlights(String[] destinations) {<br>    HashSet n = new HashSet();<br>    HashMap m = new HashMap();<br>    for (int i = 0; i &lt; destinations.length; i++) {<br>      n.add(new Integer(i));<br>      HashSet a = new HashSet();<br>      StringTokenizer st = new StringTokenizer(destinations[i], ", ");<br>      while (st.hasMoreTokens()) {<br>        a.add(new Integer(st.nextToken()));<br>      }<br>      n.addAll(a);<br>      m.put(new Integer(i), a);<br>    }<br>    int N = n.size();<br>    int[] ns = new int[N];<br>    int q = 0;<br>    for (Iterator i = n.iterator(); i.hasNext(); ) {<br>      ns[q] = ((Integer) i.next()).intValue();<br>      q++;<br>    }<br>    boolean[][] a = new boolean[N][N];<br>    for (int i = 0; i &lt; N; i++) {<br>      HashSet b = (HashSet) m.get(new Integer(ns[i]));<br>      if (b == null) continue;<br>      for (Iterator j = b.iterator(); j.hasNext(); ) {<br>        int x = ((Integer) j.next()).intValue();<br>        for (int k = 0; k &lt; N; k++)<br>          if (ns[k] == x) a[i][k] = true;<br>      }<br>    }<br>    for (int k = 0; k &lt; N; k++)<br>      for (int i = 0; i &lt; N; i++)<br>        for (int j = 0; j &lt; N; j++)<br>          a[i][j] = a[i][j] || a[i][k] &amp;&amp; a[k][j];<br>    int c = 0;<br>    for (int i = 0; i &lt; N; i++)<br>      for (int j = 0; j &lt; N; j++)<br>        if (i != j &amp;&amp; a[i][j]) c++;<br>    return c;<br>  }<br>  static void print(Object o) {<br>    System.out.println(o);<br>  }<br>  static void print(int o) {<br>    System.out.println(o);<br>  }<br>  static void print(long o) {<br>    System.out.println(o);<br>  }<br>  static void print(boolean o) {<br>    System.out.println(o);<br>  }<br>  static void print(Object[] o) {<br>    System.out.print("[");<br>    for (int i = 0; i &lt; o.length; i++)<br>      System.out.print(o[i] + ", ");<br>    System.out.println("]");<br>  }<br>  static void print(int[] o) {<br>    System.out.print("[");<br>    for (int i = 0; i &lt; o.length; i++)<br>      System.out.print(o[i] + ", ");<br>    System.out.println("]");<br>  }<br>  static void print(long[] o) {<br>    System.out.print("[");<br>    for (int i = 0; i &lt; o.length; i++)<br>      System.out.print(o[i] + ", ");<br>    System.out.println("]");<br>  }<br>  static void print(boolean[] o) {<br>    System.out.print("[");<br>    for (int i = 0; i &lt; o.length; i++)<br>      System.out.print(o[i] + ", ");<br>    System.out.println("]");<br>  }<br>}<br></td>