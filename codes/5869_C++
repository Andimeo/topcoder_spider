<td class="problemText" colspan="8" valign="middle" align="left">
            #include &lt;vector&gt;<br> <br>using namespace std;<br> <br>class BinarySearchable<br>  {<br>  public:<br>  int howMany(vector &lt;int&gt; sequence)<br>    {<br>      int count = 0;<br>      int N = sequence.size();<br>      for (int i=0; i&lt;N; ++i)<br>      {<br>        bool find = true;<br>        for (int j=0; j&lt;N; ++j)<br>          if ((sequence[j] &gt; sequence[i] &amp;&amp; j &lt; i) || (sequence[j] &lt; sequence[i] &amp;&amp; j &gt; i))<br>          {<br>            find = false;<br>            break;<br>          }<br> <br>        if (find)<br>          count++;<br>      }<br>      return count;<br>    }<br>    <br>  };<br></td>