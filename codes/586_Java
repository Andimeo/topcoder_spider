            import java.util.*;<br>public class Traffic {<br>  public int time(int[] lights, int speed) {<br>    int t = 0;<br>    for (int i = 0; i &lt; lights.length; i++) {<br>      int j = (300 / speed + t) % (lights[i] * 4);<br>      if (j &gt;= lights[i] * 2) {<br>        t = ((300 / speed + t) / (lights[i] * 4) + 1) * lights[i] * 4;<br>      } else {<br>        t += 300 / speed;<br>      }<br>      print(t);<br>    }<br>    t += 300 / speed;<br>    return t / 2;<br>  }<br>  static void print(Object o) {<br>    System.out.println(o);<br>  }<br>  static void print(int o) {<br>    System.out.println(o);<br>  }<br>  static void print(long o) {<br>    System.out.println(o);<br>  }<br>  static void print(boolean o) {<br>    System.out.println(o);<br>  }<br>  static void print(Object[] o) {<br>    System.out.print("[");<br>    for (int i = 0; i &lt; o.length; i++)<br>      System.out.print(o[i] + ", ");<br>    System.out.println("]");<br>  }<br>  static void print(int[] o) {<br>    System.out.print("[");<br>    for (int i = 0; i &lt; o.length; i++)<br>      System.out.print(o[i] + ", ");<br>    System.out.println("]");<br>  }<br>  static void print(long[] o) {<br>    System.out.print("[");<br>    for (int i = 0; i &lt; o.length; i++)<br>      System.out.print(o[i] + ", ");<br>    System.out.println("]");<br>  }<br>  static void print(boolean[] o) {<br>    System.out.print("[");<br>    for (int i = 0; i &lt; o.length; i++)<br>      System.out.print(o[i] + ", ");<br>    System.out.println("]");<br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>