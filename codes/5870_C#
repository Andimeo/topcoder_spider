            using System;<br><br>public class RogersPhenomenon {<br>  public int countTriggers(int[] set1, int[] set2) {<br>    int sum1 = 0;<br>    foreach (int i in set1)<br>      sum1 += i;<br>    int sum2 = 0;<br>    foreach (int i in set2)<br>      sum2 += i;<br>    int res = 0;<br>    if (set1.Length &gt; 1)<br>    foreach (int i in set1)<br>    {<br>      int p = sum1 - i;<br>      int q = sum2 + i;<br>      if (p * (set1.Length) &gt; (sum1) * (set1.Length - 1) &amp;&amp; q * (set2.Length) &gt; (sum2) * (set2.Length + 1))<br>        ++res;<br>    }<br>    if (set2.Length &gt; 1)<br>    foreach (int i in set2)<br>    {<br>      int p = sum1 + i;<br>      int q = sum2 - i;<br>      if (p * (set1.Length) &gt; (sum1) * (set1.Length + 1) &amp;&amp; q * (set2.Length) &gt; (sum2) * (set2.Length - 1))<br>        ++res;<br>    }<br>    return res;<br>  }<br><br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>