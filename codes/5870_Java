            import java.util.*;<br> <br>public class RogersPhenomenon {<br>    public int countTriggers(int[] set1, int[] set2) {<br>        int n = set1.length;<br>        int m = set2.length;<br> <br>        int sum1 = 0;<br>        for (int i = 0; i &lt; n; i++) {<br>            sum1 += set1[i];<br>        }<br>        int sum2 = 0;<br>        for (int i = 0; i &lt; m; i++) {<br>            sum2 += set2[i];<br>        }<br> <br>        int r = 0;<br>        for (int i = 0; i &lt; n; i++) {<br>            if (set1[i] * n &lt; sum1 &amp;&amp; set1[i] * m &gt; sum2) {<br>                r++;<br>            }<br>        }<br>        for (int i = 0; i &lt; m; i++) {<br>            if (set2[i] * m &lt; sum2 &amp;&amp; set2[i] * n &gt; sum1) {<br>                r++;<br>            }<br>        }<br> <br>        return r;<br>    }<br> <br> <br>}<br> <br> <br>// Powered by PopsEdit<br> <br>// Powered by CodeProcessor<br></td>