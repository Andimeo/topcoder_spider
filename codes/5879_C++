            #include &lt;iostream&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#define FOR(i,n) for(int i=0;i&lt;n;i++)<br>#define REP(i,f,t) for(int i=f;i&lt;=t;i++)<br>#define debln(a) cout&lt;&lt; #a &lt;&lt;" : " &lt;&lt; a &lt;&lt; endl;<br>using namespace std;<br> <br>class AimToTen {<br>  public:<br>  int need(vector &lt;int&gt; marks) {<br>    double res = 0;<br>    FOR(i,marks.size()) res+=marks[i];<br>    int i = 0;<br>    int all = marks.size();<br>    while(res/all&lt;9.5) { i++; res+=10; all++; }<br>    return i;<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>