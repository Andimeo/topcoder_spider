            public class AimToTen{<br>  public static int need(int [] a){<br>    <br>    int mat = a.length;<br>    double promedio = 0.0,total=0.0;<br>    int contador = 0;<br>    for(int i=0;i&lt;a.length;i++){<br>      total +=a[i];  <br>      <br>    }<br>    promedio=total/mat;<br>    if(promedio&gt;=9.5){<br>      return 0;<br>    }<br>    else{<br>      while(promedio&lt;9.5){<br>        total += 10;<br>        mat++;<br>        contador++;<br>        promedio=total/mat;  <br>      }<br>    }<br>    <br>    return contador;<br>  }<br>}<br></td>