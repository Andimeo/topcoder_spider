            import java.util.*;<br>import java.util.regex.*;<br> <br> <br>public class PalindromeMaker {<br> <br> <br>  public String make(String baseString) {<br>    int[] counts = new int[26];<br>    for (char c : baseString.toCharArray()) {<br>      counts[c - 'A']++;<br>    }<br>    int t = -1;<br>    for (int i = 0; i &lt; 26; i++) {<br>      if (counts[i] % 2 != 0) {<br>        if (t != -1) {<br>          return "";<br>        }<br>        t = i;<br>      }<br>    }<br>    String s = "";<br>    String e = "";<br>    for (int i = 0; i &lt; 26; i++) {<br>      while (counts[i] &gt; 1) {<br>        s += (char) ('A' + i);<br>        e = "" + (char) ('A' + i) + e;<br>        counts[i] -= 2;<br>      }<br>    }<br>    if (t != -1) {<br>      return s + (char) ('A' + t) + e;<br>    }<br>    return s + e;<br>  }<br> <br> <br> <br>}<br> <br> <br> <br> <br> <br>// Powered by CodeProcessor<br></td>