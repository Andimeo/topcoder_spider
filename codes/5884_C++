            #include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;algorithm&gt;<br> <br>using namespace std;<br> <br>class HammingDistance{<br>public:<br>  int minDistance(vector &lt;string&gt; numbers){<br>    int n=0;<br>    bool r=true;<br>    int num=0;<br>    for(int i=0;i&lt;numbers.size();i++){<br>      for(int j=i+1;j&lt;numbers.size();j++){<br>        num=0;<br>        for(int k=0;k&lt;numbers[0].size();k++){<br>          if(numbers[i][k]!=numbers[j][k])num++;<br>        }<br>        if(r){<br>          r=false;<br>          n=num;<br>        }<br>        if(num&lt;n)n=num;<br>        if(!n)return 0;<br>      }<br>    }<br>    return n;<br>  }<br>};<br></td>