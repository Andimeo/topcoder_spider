            import java.util.*;<br>public class HammingDistance <br>{<br>  public int minDistance(String[] numbers) <br>  {<br>    int min = 100;<br>    for(int i = 0; i &lt; numbers.length; i++)<br>      for(int j = i+1; j &lt; numbers.length; j++)<br>      {<br>        int count = 0;<br>        for(int k = 0; k &lt; numbers[i].length(); k++)<br>        {<br>          if(numbers[i].charAt(k) != numbers[j].charAt(k))<br>            count++;<br>        }<br>        min = Math.min(min, count);  <br>      }<br> <br>    return min;<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>