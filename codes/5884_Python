            public class HammingDistance<br>  public function minDistance(l as string()) as integer<br>  dim num as integer()<br>  redim num(l.length)<br>  dim min as integer<br>  min = 9999999<br>  dim tmp, i, j, k, n as integer<br>  for i = 0 to l.length()  - 1<br>    for j = i + 1 to  l.length() - 1<br>      tmp = HD(l(i), l(j))    <br>      if tmp &lt; min then min = tmp<br>    next j<br>  next i<br>  return min<br>  end function<br>  public function HD(l1 as string, l2 as string) as integer<br>    dim i,j as integer<br>    j = 0<br>    for i = 0 to l1.length() - 1<br>      if l1.substring(i,1) &lt;&gt; l2.substring(i,1) then j += 1<br>    next i<br>    return j<br>  end function<br>end class<br></td>