            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class FewestFactors {<br>  int[] d;<br>  bool[] u;<br>  int res;<br>  int bestc = int.MaxValue;<br><br>  void rec(int k, int v)<br>  {<br>    if (k == d.Length)<br>    {<br>      int c = 0;<br>      for (int i=1; i &lt;= v; i++)<br>        if (v%i == 0) c++;<br><br>      if (c &lt; bestc || (c == bestc &amp;&amp; v &lt; res))<br>      {<br>        bestc = c;<br>        res = v;<br>      }<br>    }<br><br>    for (int i=0; i &lt; d.Length; i++)<br>      if (!u[i])<br>      {<br>        u[i] = true;<br>        rec(k+1, v*10 + d[i]);<br>        u[i] = false;<br>      }<br>  }<br>    <br><br>    public int number(int[] d) {<br>    this.d = d;<br>    u = new bool[d.Length];<br><br>    rec(0, 0);<br><br>        return res;<br>    }<br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>