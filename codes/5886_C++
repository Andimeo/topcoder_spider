            // Cry as challange phase is passing by<br><br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br><br>#include &lt;cmath&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br><br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br><br>using namespace std;<br><br>#define FORC(it,v) for( __typeof((v).begin()) it = (v).begin(); it != (v).end(); ++it )<br><br>const int inf = 2147483647;<br><br>int fact( int x )<br>{<br>    int sum = 0;<br>    for( int i = 1; i &lt;= x; ++i )<br>        sum += ( x%i == 0 );<br>    return sum;<br>}<br><br>class FewestFactors {<br>public:<br>   int number( vector &lt;int&gt; V ) {<br>       int mini = inf;<br>       int sol = 0;<br><br>       sort( V.begin(), V.end() );<br>       do {<br>           int x = 0;<br>           FORC( it, V ) x = x*10 + *it;<br>           int tmp = fact( x );<br>           if( tmp &lt; mini ) { mini = tmp; sol = x; }<br>       } while( next_permutation( V.begin(), V.end() ) );<br><br>       return sol;<br>   }<br><br><br>   <br> <br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by moj 3.0 beta 7 [modified TZTester]<br>// Powered by CodeProcessor<br></td>