            import java.util.*;<br>public class FewestFactors {<br>    public int number(int[] digits) {<br>        int n = digits.length;<br>        int best = 1000000000;<br>        int bestret = -1;<br>        Arrays.sort(digits);<br>        do {<br>            int num = 0;<br>            for (int i=0; i&lt;n; i++) {<br>                num = 10*num + digits[i];<br>            }<br>            int f = 0;<br>            for (int i=1; i&lt;=num; i++) {<br>                if (num % i == 0) {<br>                    f++;<br>                }<br>            }<br>            if (f &lt; best) {<br>                best = f;<br>                bestret = num;<br>            }<br>        } while (nextPermutation(digits));<br>        return bestret;<br>    }<br> <br>    boolean nextPermutation(int[] a) {<br>        for (int i=a.length-2; i&gt;=0; i--) if (a[i]&lt;a[i+1]) {<br>            reverse(a, i+1, a.length);<br>            for (int j=i+1; ; j++) if (a[i]&lt;a[j]) {<br>                swap(a, i, j);<br>                return true;<br>            }<br>        }<br>        return false;<br>    }<br> <br>    void swap(int[] a, int i, int j) {<br>        int t = a[i];<br>        a[i] = a[j];<br>        a[j] = t;<br>    }<br> <br>    void reverse(int[] a, int b, int e) {<br>        while (b &lt; e) {<br>            --e;<br>            swap(a, b, e);<br>            ++b;<br>        }<br>    }<br>}<br>// Powered by [Googly Bot]<br></td>