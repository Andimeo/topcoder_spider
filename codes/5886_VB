            imports System<br>imports Microsoft.VisualBasic<br>imports System.Collections<br>imports System.Text<br> <br> <br>Public Class FewestFactors<br> <br>    Dim iMin As Integer<br>    Dim iBest As Integer<br> <br>    Public Function number(ByVal digits() As Integer) As Integer<br>        iMin = 10000000<br>        iBest = 0<br>        permute(digits, 0)<br>        Return iBest<br> <br>    End Function<br> <br>    Private Function cf(ByVal v As Integer) As Integer<br>        Dim i As Integer<br>        Dim t As Integer<br> <br>        For i = 1 To v<br>            If (v Mod i) = 0 Then<br>                t = t + 1<br>            End If<br>        Next<br> <br>        Return t<br>    End Function<br> <br>    Private Sub permute(ByVal digits() As Integer, ByVal iP As Integer)<br>        Dim i As Integer<br>        Dim iV As Integer<br>        Dim iC As Integer<br> <br>        If iP = digits.Length - 1 Then<br>            iV = 0<br>            For i = digits.Length - 1 To 0 Step -1<br>                iV = iV &amp; digits(i)<br>            Next<br> <br>            iC = cf(iV)<br>            If iC &lt; iMin Or (iC = iMin And iV &lt; iBest) Then<br>                iMin = iC<br>                iBest = iV<br>            End If<br> <br>            Return<br>        End If<br> <br>        For i = iP To digits.Length - 1<br>            Swap(digits(iP), digits(i))<br>            permute(digits, iP + 1)<br>            Swap(digits(iP), digits(i))<br>        Next<br>    End Sub<br> <br>    Private Sub Swap(ByRef i1 As Integer, ByRef i2 As Integer)<br>        Dim s As Integer<br>        s = i1<br>        i1 = i2<br>        i2 = s<br>    End Sub<br> <br>End Class<br></td>