            import java.util.*;<br>public class DyckwordUniformer {<br><br>    char[] c;<br><br>    public boolean valid(int l, int r) {<br>        int q = 0;<br>        for (int i = l; i &lt;= r; i++) {<br>            if (c[i] == 'X') q++; else<br>            if (c[i] == 'Y') q--;<br>            if (q &lt; 0) break;<br>        }<br>        return q == 0;<br>    }<br><br>    public String f(int l, int r) {<br>        if (l &gt; r) {<br>            return "";<br>        }<br><br>        List&lt;String&gt; strings = new ArrayList&lt;String&gt;();<br><br>        int last = l;<br>        int q = 0;<br>        for (int i = l; i &lt;= r; i++) {<br>            if (c[i] == 'X') q++; else<br>            if (c[i] == 'Y') q--;<br>            if (q == 0) {<br>                if ((i &lt; r) || ((i == r) &amp;&amp; (strings.size() &gt; 0))) {<br>                    // last..i = valid<br>                    strings.add(f(last, i));<br>                    last = i + 1;<br>                }<br>            }<br>        }<br><br>        if (strings.size() == 0) {<br>            strings.add("X" + f(l + 1, r - 1) + "Y");<br>        }<br><br>        Collections.sort(strings);<br>        String result = "";<br>        for (String p : strings)<br>            result += p;<br>        return result;<br>    }<br><br>    public String uniform(String dyckword) {<br><br>        c = dyckword.toCharArray();<br>        String res = "";<br>        if (valid(0, c.length - 1)) {<br>            res = f(0, c.length - 1);<br>        }<br><br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>