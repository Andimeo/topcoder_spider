            import java.util.*;<br> <br>public class ExtraBall {<br>  public double expectedPayout(int[] card, int[] balls, String[] patterns, int[] prizes) {<br>    boolean[] matched = new boolean[card.length];<br>    for (int i = 0; i &lt; balls.length; i++) {<br>      for (int j = 0; j &lt; card.length; j++) {<br>        if (balls[i] == card[j]) {<br>          matched[j] = true;<br>        }<br>      }<br>    }<br>    boolean[] won = new boolean[patterns.length];<br>    for (int i = 0; i &lt; patterns.length; i++) {<br>      char[] c = patterns[i].toCharArray();<br>      boolean good = true;<br>      for (int j = 0; j &lt; c.length; j++) {<br>        if (c[j] == 'X' &amp;&amp; !(matched[j])) {<br>          good = false;<br>          break;<br>        }<br>      }<br>      if (good) {<br>        won[i] = true;<br>      }<br>    }<br>    double tot = 0;<br>    for (int i = 0; i &lt; patterns.length; i++) {<br>      if (won[i]) continue;<br>      char[] c = patterns[i].toCharArray();<br>      int missing = 0;<br>      for (int j = 0; j &lt; c.length; j++) {<br>        if (c[j] == 'X' &amp;&amp; !(matched[j])) {<br>          missing++;<br>          if (missing &gt; 1) break;<br>        }<br>      }<br>      if (missing == 1) {<br>        tot += prizes[i];<br>      }<br>    }<br>    return tot / (double)(75 - balls.length);<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>