            import java.util.*;<br><br>public class FallingCoconuts<br>{<br>  public String[] harvest(int[] drops)<br>  {<br>    int[] H = new int[201];<br>    for (int d : drops)<br>    {<br>      d += 50;<br>      while (H[d+1] &lt; H[d]) d++;<br>      while (H[d-1] &lt; H[d]) d--;<br>      H[d]++;<br>    }<br>    int min = 201;<br>    int max = 0;<br>    int h = 0;<br>    for (int i = 0; i &lt; H.length; i++)<br>    {<br>      if (H[i] &gt; 0)<br>      {<br>        min = Math.min(i, min);<br>        max = Math.max(i, max);<br>      }<br>      h = Math.max(h, H[i]);<br>    }<br>    char[][] M = new char[h][max - min + 1];<br>    for (int i = 0; i &lt; h; i++)<br>    {<br>      for (int j = min; j &lt;= max; j++)<br>      {<br>        M[i][j-min] = (H[j] &gt; i) ? 'O' : '-';<br>      }<br>    }<br>    String[] ret = new String[M.length];<br>    for (int i = 0; i &lt; M.length; i++)<br>      ret[i] = new String(M[i]);<br>    return ret;<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>