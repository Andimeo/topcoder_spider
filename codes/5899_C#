            using System;<br>using System.Text;<br>using System.Collections;<br>using System.Text.RegularExpressions;<br> <br>public class RectangleGroups<br>{<br>  public string maximalIndexed(string[] rectangles)<br>  {<br>    Hashtable g = new Hashtable();<br> <br>    for (int i = 0; i &lt; rectangles.Length; i++)<br>    {<br>      string[] p = rectangles[i].Split(" ".ToCharArray());<br> <br>      if (!g.ContainsKey(p[0]))<br>        g[p[0]] = 0;<br> <br>      int v = (int)g[p[0]];<br> <br>      v += Convert.ToInt32(p[1]) * Convert.ToInt32(p[2]);<br> <br>      g[p[0]] = v;<br>    }<br> <br>    int max = -1;<br>    string winner = "";<br> <br>    foreach (string a in g.Keys)<br>    {<br>      if ((int)g[a] &gt; max || (int)g[a] == max &amp;&amp; String.Compare(a,winner) &lt; 1)<br>      {<br>        winner = a;<br>        max = (int)g[a];<br>      }<br>    }<br> <br>    string ret = winner + " " + max;<br> <br>    return ret;<br>  }<br> <br>}<br></td>