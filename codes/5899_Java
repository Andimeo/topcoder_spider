            import java.util.*;<br>public class RectangleGroups<br>{<br>  public String maximalIndexed(String[] rect)<br>  {<br>    int[] g = new int[26];<br><br><br>    for (int i=0; i&lt;26; i++) g[i] = 0;<br>    for (int i=0; i&lt;rect.length; i++)<br>    {<br>      String[] s = rect[i].split(" ");<br>      int x = Integer.parseInt (s[1]);<br>      int y = Integer.parseInt (s[2]);<br><br>      g[s[0].charAt(0)-'A'] += x*y;<br>    }<br><br>    int max = 0, id =0;<br><br>    for (int i=0; i&lt;26; i++)<br>      if (g[i] &gt; max)<br>      {<br>        max = g[i];<br>        id = i;<br>      }<br><br>    String res = "";<br>    res += (char)(id+'A') + " " + g[id];<br><br>    return res;<br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>