            using System;<br>using System.Collections;<br>using System.Collections.Specialized;<br>using System.Collections.Generic;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class SymbolFrequency<br>{<br>  public double language(string[] f, string[] text)<br>  {<br>    double r = double.MaxValue;<br>    int n = f.Length;<br>    <br>    int m = 0;<br>    double[] a = new double[26];<br>    for (int i = 0; i &lt; text.Length; i++)<br>      for (int j = 0; j &lt; text[i].Length; j++) <br>        if (text[i][j] &gt;= 'a' &amp;&amp; text[i][j] &lt;= 'z') <br>        {<br>          a[text[i][j] - 'a']++;<br>          m++;<br>        }<br>      <br>    for (int i = 0; i &lt; n; i++)<br>    {<br>      string s = f[i];<br>      double[] e = new double[26];<br>      <br>      for (int j = 0; j &lt; s.Length; j += 3)<br>      {<br>        int c = s[j] - 'a';<br>        int k = (s[j + 1] - '0') * 10 + (s[j + 2] - '0');<br>        e[c] = k;<br>      }<br>      <br>      double sum = 0.0;<br>      for (int j = 0; j &lt; 26; j++)<br>        sum += Math.Pow(e[j] * m / 100.0 - a[j], 2);<br>      <br>        <br>      r = Math.Min(r, sum);<br>    }<br>    return r;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>