<td class="problemText" colspan="8" valign="middle" align="left">
            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cstdlib&gt;<br> <br>using namespace std;<br> <br>#define SQR(X) ((X) * (X))<br> <br>typedef unsigned char byte;<br> <br>int Pr[256], Num[256], Len;<br>double Res, Cur;<br> <br>class SymbolFrequency<br>{<br>public:<br>  double language(vector &lt;string&gt; fr, vector &lt;string&gt; s)<br>  {<br>    int i, j;<br> <br>    Res = 1e20, Len = 0;<br>    memset(Num, 0, sizeof(Num));<br>    for (i = 0; i &lt; s.size(); i++)<br>      for (j = 0; j &lt; s[i].length(); j++)<br>        if (s[i][j] != ' ' &amp;&amp; s[i][j] != '\n' &amp;&amp; s[i][j] != '\r')<br>          Len++, Num[(byte)s[i][j]]++;<br>    for (i = 0; i &lt; fr.size(); i++)<br>    {<br>      memset(Pr, 0, sizeof(Pr));<br>      for (j = 0; j &lt; fr[i].length(); j += 3)<br>        Pr[(byte)fr[i][j]] = (fr[i][j + 1] - '0') * 10 + (fr[i][j + 2] - '0');<br>      Cur = 0;<br>      for (j = 0; j &lt; 256; j++)<br>        Cur += SQR(Len * Pr[j] * 1e-2 - Num[j]);<br>      if (Cur &lt; Res)<br>        Res = Cur;<br>    }    <br>    return Res;<br>  }<br>};<br></td>