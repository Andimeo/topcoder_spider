<td class="problemText" colspan="8" valign="middle" align="left">
            public class SymbolFrequency<br>{<br>public double language(String[]frq,String[] txt)<br>{<br>double res = Double.POSITIVE_INFINITY;<br>double[][] prob = new double[frq.length][26];<br>for (int i=0;i&lt;frq.length;i++)<br>{<br>for (int j=0;j&lt;frq[i].length();j++)<br>{<br>int l = frq[i].charAt(j++)-'a';<br>prob[i][l] = ((frq[i].charAt(j++)-'0')*10.0+(frq[i].charAt(j)-'0'))/100.0;<br>}<br>}<br>int[] count = new int[26];<br>int len = 0;<br>for (int i=0;i&lt;txt.length;i++)<br>{<br>for (int j=0;j&lt;txt[i].length();j++)<br>{<br>if (txt[i].charAt(j)&gt;='a' &amp;&amp; txt[i].charAt(j)&lt;='z')<br>{<br>len++;<br>count[txt[i].charAt(j)-'a']++;<br>}<br>}<br>}<br>for (int i=0;i&lt;frq.length;i++)<br>{<br>double d = 0.0;<br>for (int j=0;j&lt;26;j++)<br>{<br>double dif = Math.abs(count[j]-prob[i][j]*len);<br>d+=dif*dif;<br>}<br>res=Math.min(res,d);<br>}<br>return res;<br>}<br>}<br></td>