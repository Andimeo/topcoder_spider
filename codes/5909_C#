            using System;<br>using System.Collections;<br><br>public class Horoscope <br>{<br>  string ret = "";<br><br>  int rr;<br>  int ww;<br>  int[,,] lut = new int[2501,51,51];<br>  public int maxGoodDays(string[] predictions, int R, int W) <br>  {<br>    rr = R;<br>    ww = W;<br>    for(int i=0;i&lt;2501;i++) for(int j=0;j&lt;51;j++) for(int k=0;k&lt;51;k++) lut[i,j,k] = -1;<br>    foreach(string s in predictions) ret += s;<br>    return max(ret.Length-1, rr, ww);<br>  }<br>  int max(int at,int r,int w)<br>  {<br>    if(at == -1) return 0;<br>    if(lut[at,r,w] != -1) return lut[at,r,w];<br>    <br>    int best = 0;<br>    if(ret[at] == 'G')<br>    {<br>      if(r &gt; 0)<br>      {<br>        best = Math.Max(best, max(at-1,r-1,ww)+1);<br>      }<br>      if(w &gt; 0)<br>      {<br>        best = Math.Max(best, max(at-1,rr,w-1));<br>      }<br>    }<br>    else<br>    {<br>      if(r &gt; 0)<br>      {<br>        best = Math.Max(best, max(at-1,r-1,ww));<br>      }<br>      if(w &gt; 0)<br>      {<br>        best = Math.Max(best, max(at-1,rr,w-1)+1);<br>      }<br>    }<br>    lut[at,r,w] = best;<br>    return best;<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>