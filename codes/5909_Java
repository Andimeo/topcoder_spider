            import java.util.*;<br>public class Horoscope {<br>    public int maxGoodDays(String[] predictions, int R, int W) {<br>        String p = "";<br>        for (int i = 0; i &lt; predictions.length; i++) p += predictions[i];<br>        int[][] max = new int[2][p.length()+1];<br>        max[0][0] = 0;<br>        max[1][0] = 0;<br>        for (int i = 1; i &lt;= p.length(); i++) {<br>            int num = 0;<br>            for (int j = 1; j &lt;= R &amp;&amp; i - j &gt;= 0; j++) {<br>                if (p.charAt(i - j) == 'G') {<br>                    num++;<br>                }<br>                max[0][i] = Math.max(max[1][i-j] + num, max[0][i]);<br>            }<br>            num = 0;<br>            for (int j = 1; j &lt;= W &amp;&amp; i - j &gt;= 0; j++) {<br>                if (p.charAt(i - j) == 'B') {<br>                    num++;<br>                }<br>                max[1][i] = Math.max(max[0][i-j] + num, max[1][i]);<br>            }<br>        }<br>        return Math.max(max[1][p.length()], max[0][p.length()]);<br>    }<br>    <br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>