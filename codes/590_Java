            import java.util.*;<br> <br>public class Resort {<br>  Map m;<br>  String[] rs;<br>  String[] answer = {"GREEN CIRCLE","BLUE SQUARE","BLACK DIAMOND"};<br>  <br>  int doit( String s ){<br>    if( m.containsKey( s ) ){<br>      return ((Integer)m.get( s )).intValue();<br>    }<br>    int i;<br>    int rv=0;<br>    String diff = "EMH";<br>    boolean found = false;<br>    for( i=0; i&lt;rs.length; i++ ){<br>      StringTokenizer st = new StringTokenizer( rs[i], ",:" );<br>      if( st.nextToken().equals( s ) ){<br>        found = true;<br>        int me = diff.indexOf( st.nextToken().charAt(0) );<br>        rv = 3;<br>        while( st.hasMoreTokens() ){<br>          rv = Math.min( rv, doit( st.nextToken() ) );<br>        }<br>        if( rv==3 ){<br>          rv = me;<br>        }else{<br>          rv = Math.max( rv, me );<br>        }<br>      }<br>    }<br>    if( !found ){<br>      System.out.println( "UHOH" );<br>    }<br>    m.put( s, new Integer(rv) );<br>    return rv;<br>  }<br>  <br>  void doAll(){<br>    int i;<br>    for( i=0; i&lt;rs.length; i++ ){<br>      doit( rs[i] );<br>    }<br>  }<br>  <br>  public String classify( String[] runs, String classify ) {<br>    rs = runs;<br>    m = new HashMap();<br>    return answer[doit( classify )];<br>  }<br> <br>  int ipi( StringTokenizer st ){ return Integer.parseInt( st.nextToken() ); }<br>}<br> <br>// Powered by PopsEdit<br></td>