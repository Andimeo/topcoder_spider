            import java.util.*;<br> <br>public class FallingBall {<br>    public int howMany(String[] cells, int n) {<br>        int m = cells.length;<br>        int[] x = new int[m];<br>        int[] y = new int[m];<br>        int[] u = new int[n];<br>        Arrays.fill(u, -1);<br> <br>        for (int i = 0; i &lt; m; i++) {<br>            Scanner sc = new Scanner(cells[i]);<br>            x[i] = sc.nextInt();<br>            y[i] = sc.nextInt();<br>            u[x[i]] = y[i];<br>        }<br> <br>        for (int i = 0; i &lt; m; i++) {<br>            for (int j = 0; j &lt; m; j++) {<br>                if (x[i] == x[j] &amp;&amp; y[i] != y[j]) {<br>                    return 0;<br>                }<br>            }<br>        }<br> <br>        int[][] a = new int[n][n];<br>        a[0][0] = 1;<br>        for (int i = 1; i &lt; n; i++) {<br>            for (int j = 0; j &lt;= i; j++) {<br>                a[i][j] = a[i - 1][j];<br>                if (j &gt; 0) {<br>                    a[i][j] += a[i - 1][j - 1];<br>                }<br>                if (u[i] != -1 &amp;&amp; u[i] != j) {<br>                    a[i][j] = 0;<br>                }<br>            }<br>        }<br> <br>        int r = 0;<br>        for (int i = 0; i &lt; n; i++) {<br>            r += a[n - 1][i];<br>        }<br>        return r;<br>    }<br> <br> <br>}<br> <br> <br>// Powered by PopsEdit<br> <br>// Powered by CodeProcessor<br></td>