            using System;<br> <br>public class ObjectFall {<br>  struct Precka : IComparable {<br>    public int y, x1, x2;<br> <br>    public Precka(int y, int x1, int x2) {<br>      this.y = y;<br>      this.x1 = x1;<br>      this.x2 = x2;<br>    }<br> <br>    public int CompareTo(object obj) {<br>      // TODO:  Add Precka.CompareTo implementation<br>      Precka p1 = (Precka)obj;<br>      return p1.y.CompareTo(this.y);<br>    }<br>  }<br> <br>  public int howLong(int x, int y, string[] O) {<br>    int n = O.Length;<br>    int time = y;<br>    Precka[] P = new Precka[n];<br>    for (int i = 0; i &lt; n; ++i) {<br>      string[] o = O[i].Split();<br>      int py = int.Parse(o[0]);<br>      int px1 = int.Parse(o[1]);<br>      int px2 = int.Parse(o[2]);<br>      P[i] = new Precka(py, px1, px2);<br>    }<br>    Array.Sort(P);<br> <br>    for (int i = 0; i &lt; n; ++i) {<br>      Precka p = P[i];<br>      if (p.y &gt; y) continue;<br>      if (x &gt;= p.x1 &amp;&amp; x &lt;= p.x2) {<br>        time += 5;<br>        x = p.x2;<br>      }<br>    }<br> <br>    return time;<br>  }<br>}<br></td>