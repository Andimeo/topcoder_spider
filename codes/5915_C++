            #include&lt;vector&gt;<br>#include&lt;string&gt;<br>#include&lt;algorithm&gt;<br>#include&lt;sstream&gt;<br>#include&lt;iostream&gt;<br>using namespace std;<br> <br>struct Ob<br>{<br>  int y,x1,x2;<br>};<br> <br>Ob t[60];<br> <br>bool cmp(Ob a,Ob b) {return a.y&gt;b.y;}<br> <br>struct ObjectFall<br>{<br>  int howLong(int x,int y,vector&lt;string&gt; v)<br>  {<br>    for(int i=0;i&lt;v.size();i++)<br>    {<br>      istringstream input(v[i]);<br>      input&gt;&gt;t[i].y&gt;&gt;t[i].x1&gt;&gt;t[i].x2;<br>    }<br>    sort(t,t+v.size(),cmp);<br>    int ret=0;<br>    for(int i=0;i&lt;v.size();i++)<br>      if(y&gt;=t[i].y&amp;&amp;t[i].x1&lt;=x&amp;&amp;x&lt;=t[i].x2)<br>      {<br>        ret+=y-t[i].y+5;<br>        cout&lt;&lt;ret&lt;&lt;endl;<br>        x=t[i].x2;<br>        y=t[i].y;<br>      }<br>    ret+=y;<br>    cout&lt;&lt;ret&lt;&lt;endl;<br>    return ret;<br>  }<br>};<br></td>