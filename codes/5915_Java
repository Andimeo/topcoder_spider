            import java.util.*;<br> <br>public class ObjectFall<br>{<br>public int howLong(int x, int y, String[] ob)<br>{<br>  int x1[]=new int[ob.length];<br>  int x2[]=new int[ob.length];<br>  int yy[]=new int[ob.length];<br>  int c=0;<br>  for(String v:ob)<br>  {<br>    String []t=v.split(" ");<br>    yy[c]=Integer.parseInt(t[0]);<br>    x1[c]=Integer.parseInt(t[1]);<br>    x2[c]=Integer.parseInt(t[2]);<br>    c++;<br>  }  <br>  int res=0;<br>  for(int i=y;i&gt;0;i--,res++)<br>  {<br>    for(int j=0;j&lt;c;j++)<br>    {<br>      if(yy[j]==i&amp;&amp;x&gt;=x1[j]&amp;&amp;x&lt;=x2[j])<br>      {<br>        res+=5;<br>        x=x2[j];<br>        j=c;<br>      }<br>    }<br>  }<br>  return res;<br>}<br>}<br></td>