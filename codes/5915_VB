            Public Class ObjectFall<br>    Public Function howLong(ByVal x As Integer, ByVal y As Integer, ByVal obstacles As String()) As Integer<br>        obstacles.Sort(obstacles)<br> <br>        Dim i, j As Integer<br>        Dim N As Integer = obstacles.Length<br>        Dim yo(), x1(), x2() As Integer<br>        ReDim yo(N - 1), x1(N - 1), x2(N - 1)<br>        For i = N - 1 To 0 Step -1<br>            Dim tmpstr As String() = obstacles(i).Split(" ")<br>            yo(i) = Integer.Parse(tmpstr(0))<br>            x1(i) = Integer.Parse(tmpstr(1))<br>            x2(i) = Integer.Parse(tmpstr(2))<br> <br>        Next<br>        Dim cury As Integer = y<br>        Dim result As Integer = 0<br>        For i = N - 1 To 0 Step -1<br>            If yo(i) &lt;= cury AndAlso x &gt;= x1(i) AndAlso x &lt;= x2(i) Then<br>                result += 5<br>                x = x2(i)<br>            End If<br>        Next<br> <br>        result += y<br>        Return result<br> <br>    End Function<br>End Class<br></td>