            import java.util.*;<br> <br>public class BallBouncing {<br>    public int bounces(int rows, int columns, int startrow, int startcol, String[] holes) {<br>        int n = holes.length;<br>        int[] x = new int[n];<br>        int[] y = new int[n];<br>        for (int i = 0; i &lt; n; i++) {<br>            Scanner sc = new Scanner(holes[i]);<br>            x[i] = sc.nextInt();<br>            y[i] = sc.nextInt();<br>        }<br> <br>        int cx = startrow;<br>        int cy = startcol;<br>        int dx = 1;<br>        int dy = 1;<br>        int ans = 0;<br>        while (true) {<br>//            System.out.println(cx + " " + cy + " " + dx + " " + dy);<br>            for (int i = 0; i &lt; n; i++) {<br>                if ((x[i] - cx) * dx == (y[i] - cy) * dy &amp;&amp; (x[i] - cx) * dx &gt;= 0) {<br>                    return ans;<br>                }<br>            }<br> <br>            int leftx;<br>            int lefty;<br>            if (dx == 1) {<br>                leftx = rows - cx;<br>            } else {<br>                leftx = cx + 1;<br>            }<br>            if (dy == 1) {<br>                lefty = columns - cy;<br>            } else {<br>                lefty = cy + 1;<br>            }<br>            if (leftx &lt; lefty) {<br>                cx += leftx * dx;<br>                cy += leftx * dy;<br>                dx = -dx;<br>            } else if (leftx &gt; lefty) {<br>                cx += lefty * dx;<br>                cy += lefty * dy;<br>                dy = -dy;<br>            } else {<br>                cx += lefty * dx;<br>                cy += lefty * dy;<br>                dx = -dx;<br>                dy = -dy;<br>            }<br>            if (cx &gt;= rows) {<br>                cx = 2 * rows - cx - 1;<br>            }<br>            if (cx &lt; 0) {<br>                cx = -cx - 1;<br>            }<br>            if (cy &gt;= columns) {<br>                cy = 2 * columns - cy - 1;<br>            }<br>            if (cy &lt; 0) {<br>                cy = -cy - 1;<br>            }<br>            ans++;<br>            if (ans &gt; 4000000) {<br>                return -1;<br>            }<br>        }<br>    }<br> <br> <br>}<br> <br> <br>// Powered by PopsEdit<br> <br>// Powered by CodeProcessor<br></td>