            import java.util.*;<br> <br>public class OneMorePoint {<br>    public String interiorPoint(String[] rectangles) {<br>        int n = rectangles.length;<br>        int[] x1 = new int[n];<br>        int[] x2 = new int[n];<br>        int[] y1 = new int[n];<br>        int[] y2 = new int[n];<br>        int[] x = new int[2 * n];<br>        int[] y = new int[2 * n];<br> <br>        for (int i = 0; i &lt; n; i++) {<br>            Scanner sc = new Scanner(rectangles[i]);<br>            x1[i] = sc.nextInt() * 2;<br>            y1[i] = sc.nextInt() * 2;<br>            x2[i] = sc.nextInt() * 2;<br>            y2[i] = sc.nextInt() * 2;<br>            x[2 * i] = x1[i];<br>            x[2 * i + 1] = x2[i];<br>            y[2 * i] = y1[i];<br>            y[2 * i + 1] = y2[i];<br>        }<br> <br>        Arrays.sort(x);<br>        Arrays.sort(y);<br> <br>        for (int i = 0; i &lt; 2 * n - 1; i++) {<br>            for (int j = 0; j &lt; 2 * n - 1; j++) {<br>                int xx = (x[i] + x[i + 1]) / 2;<br>                int yy = (y[j] + y[j + 1]) / 2;<br> <br>                boolean ins = false;<br>                boolean xm = false;<br>                boolean xp = false;<br>                boolean ym = false;<br>                boolean yp = false;<br> <br>                for (int k = 0; k &lt; n; k++) {<br>                    int xd = x2[k] &lt; xx ? -1 : x1[k] &lt;= xx ? 0 : 1;<br>                    int yd = y2[k] &lt; yy ? -1 : y1[k] &lt;= yy ? 0 : 1;<br>                    int xid = x2[k] &lt;= xx ? -1 : x1[k] &lt; xx ? 0 : 1;<br>                    int yid = y2[k] &lt;= yy ? -1 : y1[k] &lt; yy ? 0 : 1;<br> <br>                    if (xd == 0 &amp;&amp; yd == 0) {<br>                        ins = true;<br>                        break;<br>                    } else if (xid == 0 &amp;&amp; yd &gt; 0) {<br>                        yp = true;<br>                    } else if (xid == 0 &amp;&amp; yd &lt; 0) {<br>                        ym = true;<br>                    } else if (yid == 0 &amp;&amp; xd &gt; 0) {<br>                        xp = true;<br>                    } else if (yid == 0 &amp;&amp; xd &lt; 0) {<br>                        xm = true;<br>                    }<br>                }<br>                if (!ins &amp;&amp; xm &amp;&amp; xp &amp;&amp; ym &amp;&amp; yp) {<br>                    return "YES";<br>                }<br>            }<br>        }<br>        return "NO";<br>    }<br> <br>}<br> <br> <br>// Powered by PopsEdit<br> <br>// Powered by CodeProcessor<br></td>