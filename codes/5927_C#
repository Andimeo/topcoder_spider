            using System;<br>public class TestCurve<br>  {<br>    public TestCurve()<br>    {<br>      //<br>      // TODO: Add constructor logic here<br>      //<br>    }<br>    static public int[] determineGrades(int[] scores)<br>    {<br>      int highest=scores[0];<br>      for(int i =1;i&lt;scores.Length;i++)<br>        highest=scores[i]&gt;highest?scores[i]:highest;<br>      for(int i =0;i&lt;scores.Length;i++)<br>      {<br>        double dummy = (scores[i]*100)/highest; <br>        scores[i] = (int) dummy;<br>      }  <br>      return(scores);<br>    }}<br></td>