            #include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;cctype&gt;<br> <br>using namespace std;<br> <br>class TestCurve<br>{<br>public: vector&lt;int&gt;determineGrades(vector&lt;int&gt; grade)<br>    {<br>      int i,len=grade.size();<br>      int max=-1,index=0;<br>      for(i=0;i&lt;len;i++)<br>        if(grade[i]&gt;max)<br>        {<br>          max=grade[i];<br>          index=i;<br>        }<br>      float pro= 100.0/(float)max;<br>      for(i=0;i&lt;len;i++)<br>      {<br>        if(i==index)<br>          grade[i]=100;<br>        else <br>        grade[i]=int(pro*(float)grade[i]);<br>      <br>      }<br>      return grade;<br>    <br>    }<br> <br>};<br></td>