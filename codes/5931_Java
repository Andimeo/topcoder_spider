            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class FowlRoad<br>{<br>  public int crossings(int roadY, int[] bobX, int[] bobY)<br>  {<br>    int cross = 0;<br>    boolean less = bobY[0] &lt;= roadY;   <br>    for (int y : bobY) {<br>      if (y == roadY) continue; <br>      <br>      boolean now = y &lt;= roadY;<br>      if (now != less) {<br>        cross++;<br>      }<br>      less = now;<br>    }<br>    return cross;<br>  }<br>  <br> <br>}<br> <br>//Powered by [KawigiEdit]<br></td>