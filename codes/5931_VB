            Public Class FowlRoad<br>    Public Function crossings(ByVal roadY As Integer, ByVal bobX As Integer(), ByVal bobY As Integer()) As Integer<br>        Dim i As Integer<br>        Dim count As Integer = 0<br>        Dim last, now As Integer<br>        last = bobY(0)<br>        For i = 1 To bobY.Length - 1<br>            now = bobY(i)<br>            If (now &gt; roadY And last &lt; roadY) Or (now &lt; roadY And last &gt; roadY) Then<br>                count += 1<br>                last = now<br>            ElseIf now &lt;&gt; roadY Then<br>                last = now<br>            End If<br>        Next<br>        Return count<br>    End Function<br>End Class<br></td>