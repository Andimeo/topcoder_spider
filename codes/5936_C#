<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br> <br>public class GridCut <br>{<br>  public int cutLength(int w, int h, int n) <br>  {<br>    n = Math.Min(n,w*h-n);<br>    if (n == 0)<br>      return 0;<br>    int ret = Math.Min(h+1,w+1);<br>    for (int i = 1; i &lt; w; i++) <br>      for (int j = 1; j &lt; h; j++) <br>        if (i+j-1 &lt;= n &amp;&amp; n &lt;= i*j) <br>          ret = Math.Min(ret,i+j);<br>    if (n % w == 0 &amp;&amp; ret &gt; w)<br>      ret = w;<br>    if (n % h == 0 &amp;&amp; ret &gt; h) <br>      ret = h;<br>    return ret;<br>  }<br>}<br></td>