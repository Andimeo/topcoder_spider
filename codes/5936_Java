            import java.util.*; <br>public class GridCut {<br>static boolean debugging = false ; void exit(int code){if(debugging)System.exit(code);}<br> <br>public int cutLength(int width, int height, int n) <br>{ <br>int i , j , k , l , m , count ;<br> if ( n == 0 ) return 0 ;<br> if ( n &gt; width*height/2 ) return cutLength ( width , height , width*height-n ) ;<br> count = 1000000 ;<br> for ( i = 1 ; i &lt;=width ; i ++ )<br>    for ( j = 1 ; j &lt;= height ; j ++ )<br>       {<br>          <br>       if ( i &lt; width &amp;&amp; j &lt; height )<br>          {<br>          if ( i * j &gt;= n &amp;&amp; i+j &lt; count ) count = i + j ;<br>          }<br>       else if ( i == width )<br>          {<br>         if ( i*j &gt; n &amp;&amp; i+1 &lt; count ) count = i+1 ;<br>         if ( i*j == n &amp;&amp; i &lt; count ) count = i ;<br>           }<br>       else if ( j == height)<br>          {<br>         if ( i*j &gt; n &amp;&amp; j+1 &lt; count ) count = j+1 ;<br>         if ( i*j == n &amp;&amp; j &lt; count ) count =j ;<br>          }<br>       }<br> <br>           <br> <br> <br>return count ;<br>} <br>} <br> <br> <br>// Powered by FileEdit<br></td>