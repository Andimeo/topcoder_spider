            using System;<br> <br>public class FunctionDependency<br>{<br>  public string[] scriptingOrder(string[] f, string[] d)<br>  {<br>    int n = f.Length;<br>    string[] ret = new string[n];<br>    for (int i = 0; i &lt; n; ++i)<br>    {<br>      int choose = -1;<br>      for (int j = 0; j &lt; n; ++j) if (f[j].Length &gt; 0)<br>      {<br>        string[] dep = d[j].Split(new char[] {' '});<br>        bool can = true;<br>        if (d[j].Length &gt; 0) for (int k = 0; k &lt; dep.Length; ++k) if (f[Int32.Parse(dep[k])].Length &gt; 0) can =false;<br>        if (can)<br>        {<br>          if (choose == -1 || f[j].CompareTo(f[choose]) &lt; 0) choose = j;<br>        }<br>      }<br>      ret[i] = f[choose];<br>      f[choose] = "";<br>    }<br>    return ret;<br>  }<br>}<br></td>