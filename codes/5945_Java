            import java.util.*;<br> <br>public class VolumeDiscount {<br>    public int bestDeal(String[] priceList, int quantity) {<br>        int n = priceList.length;<br>        int[] q = new int[n];<br>        int[] p = new int[n];<br> <br>        for (int i = 0; i &lt; n; i++) {<br>            Scanner sc = new Scanner(priceList[i]);<br>            q[i] = sc.nextInt();<br>            p[i] = sc.nextInt();<br>        }<br> <br>        int[] pp = new int[2000];<br>        boolean[] can = new boolean[2000];<br>        can[0] = true;<br> <br>        for (int i = 0; i &lt; n; i++) {<br>            for (int j = 0; j &lt; 2000; j++) {<br>                if (can[j] &amp;&amp; j + q[i] &lt; 2000) {<br>                    if (!can[j + q[i]] || pp[j + q[i]] &gt; pp[j] + p[i]) {<br>                        can[j + q[i]] = true;<br>                        pp[j + q[i]] = pp[j] + p[i];<br>                    }<br>                }<br>            }<br>        }<br> <br>        int best = Integer.MAX_VALUE;<br>        for (int i = quantity; i &lt; 2000; i++) {<br>            if (can[i] &amp;&amp; pp[i] &lt; best) {<br>                best = pp[i];<br>            }<br>        }<br> <br>        return best;     <br>    }<br> <br> <br>}<br> <br> <br>// Powered by PopsEdit<br> <br>// Powered by CodeProcessor<br></td>