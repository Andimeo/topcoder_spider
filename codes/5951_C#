            using System;<br>using System.Collections;<br> <br>public class Distincter<br>{<br>  public int disperse(int[] sequence, int K)<br>  {<br>    int[] x = new int[1200];<br>    int[] y = new int[1200];<br>    <br>    int i,j,k,l;<br>    <br>    foreach(int num in sequence)<br>      x[num+100]++;<br>    <br>    int mam=0;<br> <br>    foreach(int num in x)<br>      if(num!=0)<br>        mam++;<br>    <br>    K-=mam;<br>    <br>    int cost=0;<br>    <br>    while(K--&gt;0)<br>    {<br>      int bb=1000;<br>      int kto=0;<br>      int smer=0;<br>      <br>      for(i=0; i&lt;1200; i++)<br>        if(x[i]&gt;=2)<br>      {<br>        l=0;<br>        j=i;<br>        <br>        while(x[j]!=0)<br>        {<br>          j++;<br>          <br>          if(y[j]&lt;0)<br>            l--;<br>           else<br>            l++;<br>        }<br>        <br>        if(l&lt;bb)<br>        {<br>          bb=l;<br>          kto=i;<br>          smer=1;<br>        }<br>        <br>        <br>        l=0;<br>        j=i;<br>        <br>        while(x[j]!=0)<br>        {<br>          if(y[j]&gt;0)<br>            l--;<br>           else<br>            l++;<br>          <br>          j--;<br>        }<br>        <br>        if(l&lt;bb)<br>        {<br>          bb=l;<br>          kto=i;<br>          smer=-1;<br>        }<br>      }<br>      <br>      cost+=bb;<br>      <br>      if(smer&gt;0)<br>      {<br>        j=kto;<br>        x[j]--;<br>        <br>        while(x[j]!=0)<br>        {<br>          j++;<br>          y[j]++;<br>        }<br>        <br>        x[j]++;<br>      }<br>       else<br>      {<br>        j=kto;<br>        x[j]--;<br>        <br>        while(x[j]!=0)<br>        {<br>          y[j]--;<br>          j--;<br>        }<br>        <br>        x[j]++;<br>      }<br>    }<br>    <br>    return cost;<br>  }<br>}<br> <br> <br>// Powered by FileEdit<br></td>