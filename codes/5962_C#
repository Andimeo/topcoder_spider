            using System;<br><br>public class BrokenClock {<br>  public int fewestClicks(string clockTime, string currentTime) {<br>    int h1 = int.Parse(clockTime.Substring(0, 2));<br>    int m1 = int.Parse(clockTime.Substring(3, 2));<br>    int h2 = int.Parse(currentTime.Substring(0, 2));<br>    int m2 = int.Parse(currentTime.Substring(3, 2));<br>    int res = ((h2 - h1) % 24 + 24) % 24;<br>    h1 = (h1 + res) % 24;<br>    m1 = (m1 + res) % 60;<br>    res += ((m2 - m1) % 60 + 60) % 60;<br>    return res;<br>  }<br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>