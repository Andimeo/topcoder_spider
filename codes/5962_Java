            import java.util.*;<br> <br>public class BrokenClock {<br>    public int fewestClicks(String clockTime, String currentTime) {<br>        StringTokenizer st = new StringTokenizer(clockTime, ":");<br>        int h1 = Integer.parseInt(st.nextToken());<br>        int m1 = Integer.parseInt(st.nextToken());<br>        st = new StringTokenizer(currentTime, ":");<br>        int h2 = Integer.parseInt(st.nextToken());<br>        int m2 = Integer.parseInt(st.nextToken());<br> <br>        int r = 0;<br>        while (h1 != h2) {<br>            r++;<br>            h1 = (h1 + 1) % 24;<br>            m1 = (m1 + 1) % 60;<br>        }<br>        while (m1 != m2) {<br>            r++;<br>            m1 = (m1 + 1) % 60;<br>        }<br>        return r;<br>    }<br> <br> <br>}<br> <br> <br>// Powered by PopsEdit<br> <br>// Powered by CodeProcessor<br></td>