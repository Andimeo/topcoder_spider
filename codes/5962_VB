            Public Class BrokenClock<br>  Public Function fewestClicks(clockTime As String, currentTime As String) as Integer<br>  <br>    Dim a1() As String = clockTime.Split(":")<br>    Dim a2() As String = currentTime.Split(":")<br>    <br>    Dim aClockTime() As Integer = new Integer() {CInt(a1(0)), CInt(a1(1))}<br>    Dim aCurrentTime() As Integer = new Integer() {CInt(a2(0)), CInt(a2(1))}<br>    Dim nPress As Integer = 0 <br> <br>    While aClockTime(0) &lt;&gt; aCurrentTime(0)<br>      aClockTime(0) += 1<br>      aClockTime(1) += 1<br>      if aClockTime(0) &gt; 23 Then <br>        aClockTime(0) = 0 <br>      End If<br>      if aClockTime(1) &gt; 59 Then <br>        aClockTime(1) = 0 <br>      End if<br>      nPress += 1<br>    End While<br>    <br>    ' Minutes <br>    While aClockTime(1) &lt;&gt; aCurrentTime(1)<br>      aClockTime(1) += 1<br>      if aClockTime(1) &gt; 59 Then <br>        aClockTime(1) = 0 <br>      End if<br>      nPress += 1<br>    End While<br>        <br>            <br>    Return nPress<br>  <br>  End Function <br>End Class<br></td>