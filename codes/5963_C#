            using System;<br>using System.Collections;<br>public class FactorialGCD {<br>  public int factGCD(int a, int b) {<br>      if(a==0) a=1;<br> <br>      int gcd=1;<br>      double eps=0.0001;<br> <br>      int[] chislo=new int[50];<br>      int[] count = new int[50];<br> <br>      int pos=0;<br> <br>      for(int i=2;i&lt;Math.Sqrt(b)+eps;i++)<br>      {<br>        while(b%i==0)<br>        {<br>          if(chislo[pos]==i)<br>          {<br>            count[pos]++;<br>          }<br>          else<br>          {<br>            pos++;<br>            chislo[pos]=i;<br>            count[pos]=1;<br>          }<br> <br>          b/=i;<br>        }<br>      }<br> <br>      if(b!=1)<br>      {<br>        pos++;<br>        chislo[pos]=b;<br>        count[pos]=1;<br>      }<br> <br>      for(int j=1;j&lt;=pos;j++)<br>      {<br>        int min=Math.Min(count[j],Count(a,chislo[j]));<br> <br>        for(int q=1;q&lt;=min;q++)<br>          gcd*=chislo[j];<br>      }<br> <br>      return gcd;  <br>    }<br>    <br>    int Count(int a,int b)<br>    {<br>      int cnt=0;<br>      while(a&gt;=b)<br>      {<br>        cnt+=a/b;<br>        a/=b;<br>      }<br> <br>      return cnt;<br>    }<br>}<br> <br>// Powered by PopsEdit<br></td>