            class FactorialGCD<br>{ <br>public: <br>  int factGCD(int a, int b) <br>  { <br>    long long i, j, k, b0 = b, d = 1;<br>    for ( i = 2; i * i &lt;= b0; ++i )<br>      for ( j = i; !(b % i); j += i )<br>      {<br>        k = j;<br>        while ( !(b % i + k % i) )<br>        {<br>          b /= i;        <br>          k /= i;<br>          d = j &lt;= a ? d * i : d;<br>        }      <br>      }<br>      return b &lt;= a ? d * b : d;<br>  } <br>}; <br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>