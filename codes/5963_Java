            public class FactorialGCD{<br>  public int factGCD(int a, int b){<br>    if (a &lt;= 1 || b == 1) return 1;<br>    int[] primes = new int[47000];<br>    for (int j = 0; j &lt; 47000; j++) primes[j] = 0;<br>    int i = 2, otherPrime, c = b;<br>    while (c &gt; 1 &amp;&amp; i &lt; 47000){<br>      while (c % i == 0){<br>        primes[i]++;<br>        c /= i;<br>      }<br>      i++;<br>    }<br>    <br>    int gcd = 1;<br>    <br>    for (i = 2; i &lt; 47000; i++){<br>      if (primes[i] != 0){<br>        long x = i, p = i, z = a;<br>        int count = 0, minCount = primes[i];<br>        while (x &lt;= z){<br>          count += z / x;<br>          x *= p;<br>        }<br>        if (count &lt; minCount) minCount = count;<br>        for (; minCount &gt; 0; minCount--) gcd *= i;       <br>      }<br>    }<br>    <br>    if (c != 1){<br>      long x = c, p = c, z = a;<br>      int count = 0, minCount = 1;<br>      while (x &lt;= z){<br>        count += z / x;<br>        x *= p;<br>      }<br>      if (count &lt; minCount) minCount = count;<br>      for (; minCount &gt; 0; minCount--) gcd *= c;<br>    }<br>    <br>    return gcd;<br>  }<br>}<br></td>