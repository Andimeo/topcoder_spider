            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;cmath&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>using namespace std;<br> <br>class PowerSupply <br>{<br>  public:<br>  int maxProfit(vector &lt;int&gt; x, vector &lt;int&gt; y, int d) <br>  {<br>    int n=x.size(),i,w=0,t[50],j;<br>    for(i=0;i&lt;n;i++) t[i]=x[i];<br>    sort(t,t+n);<br>    for(i=0;i&lt;n;i++)<br>    {<br>      for(j=i;j&lt;n &amp;&amp; t[j]-t[i]&lt;=2*d;j++);<br>      w&gt;?=j-i;<br>    }<br>    for(i=0;i&lt;n;i++) t[i]=y[i];<br>    sort(t,t+n);<br>    for(i=0;i&lt;n;i++)<br>    {<br>      for(j=i;j&lt;n &amp;&amp; t[j]-t[i]&lt;=2*d;j++);<br>      w&gt;?=j-i;<br>    }<br>    for(i=0;i&lt;n;i++) t[i]=x[i]+y[i];<br>    sort(t,t+n);<br>    for(i=0;i&lt;n;i++)<br>    {<br>      for(j=i;j&lt;n &amp;&amp; t[j]-t[i]&lt;=4.0*d/sqrt(2);j++);<br>      w&gt;?=j-i;<br>    }<br>    for(i=0;i&lt;n;i++) t[i]=x[i]-y[i];<br>    sort(t,t+n);<br>    for(i=0;i&lt;n;i++)<br>    {<br>      for(j=i;j&lt;n &amp;&amp; t[j]-t[i]&lt;=4.0*d/sqrt(2);j++);<br>      w&gt;?=j-i;<br>    }<br>    return w;<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>