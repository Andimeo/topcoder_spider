            imports microsoft.visualbasic<br>imports system.math<br> <br>public class PowerSupply<br>public function maxProfit(x as integer(), y as integer(), d as integer) as integer<br>dim poziom_n1(50) as integer<br>dim pion_n1(50) as integer<br>dim diag1_n1(50) as integer<br>dim diag2_n1(50) as integer<br>dim poziom_n2(50) as integer<br>dim pion_n2(50) as integer<br>dim diag1_n2(50) as integer<br>dim diag2_n2(50) as integer<br>dim max as integer<br>dim n as integer<br>dim i as integer<br>dim j as integer<br>dim c as double<br> <br>  n=ubound(x)<br>  max=0<br>  d=d*2<br>  c=sqrt(2*d^2)<br>  <br>  for i=0 to n<br>    poziom_n1(i)=1<br>    pion_n1(i)=1<br>    diag1_n1(i)=1<br>    diag2_n1(i)=1<br>    poziom_n2(i)=1<br>    pion_n2(i)=1<br>    diag1_n2(i)=1<br>    diag2_n2(i)=1<br>  next i<br>  <br>  ' w pionie<br>  for i=0 to n-1<br>    for j=i+1 to n<br>      if ((x(i)-x(j))&lt;=d) and ((x(i)-x(j))&gt;=0) then<br>        pion_n1(i)=pion_n1(i)+1<br>        pion_n2(j)=pion_n2(j)+1<br>      end if<br>      if ((x(j)-x(i))&lt;=d) and ((x(j)-x(i))&gt;=0) then<br>        pion_n2(i)=pion_n2(i)+1<br>        pion_n1(j)=pion_n1(j)+1<br>      end if<br>      if ((y(i)-y(j))&lt;=d) and ((y(i)-y(j))&gt;=0) then<br>        poziom_n1(i)+=1<br>        poziom_n2(j)+=1<br>      end if<br>      if ((y(j)-y(i))&lt;=d) and ((y(j)-y(i))&gt;=0) then<br>        poziom_n2(i)+=1<br>        poziom_n1(j)+=1<br>      end if<br>      if (((x(i)-y(i))-(x(j)-y(j)))&lt;=c) and (((x(i)-y(i))-(x(j)-y(j)))&gt;=0) then<br>        diag1_n1(i)+=1<br>        diag1_n2(j)+=1<br>      end if<br>      if (((x(j)-y(j))-(x(i)-y(i)))&lt;=c) and (((x(j)-y(j))-(x(i)-y(i)))&gt;=0) then<br>        diag1_n2(i)+=1<br>        diag1_n1(j)+=1<br>      end if<br>      if (((x(i)+y(i))-(x(j)+y(j)))&lt;=c) and (((x(i)+y(i))-(x(j)+y(j)))&gt;=0) then<br>        diag2_n1(i)+=1<br>        diag2_n2(j)+=1<br>      end if<br>      if (((x(j)+y(j))-(x(i)+y(i)))&lt;=c) and (((x(j)+y(j))-(x(i)+y(i)))&gt;=0) then<br>        diag2_n2(i)+=1<br>        diag2_n1(j)+=1<br>      end if<br> <br>    next j<br>  next i<br>  for i=0 to n<br>    if poziom_n1(i)&gt;max then max=poziom_n1(i)<br>    if pion_n1(i)&gt;max then max=pion_n1(i)<br>    if diag1_n1(i)&gt;max then max=diag1_n1(i)<br>    if diag2_n1(i)&gt;max then max=diag2_n1(i)<br>    if poziom_n2(i)&gt;max then max=poziom_n2(i)<br>    if pion_n2(i)&gt;max then max=pion_n2(i)<br>    if diag1_n2(i)&gt;max then max=diag1_n2(i)<br>    if diag2_n2(i)&gt;max then max=diag2_n2(i)<br>  next i<br>  return max<br>end function<br> <br>end class<br></td>