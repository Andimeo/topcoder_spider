            import java.util.*;<br> <br>public class MooresLaw {<br> <br>  int compare (double x, double y) {<br>    final double EPS = 1e-9;<br> <br>    if (x &gt; y - EPS &amp;&amp; x &lt; y + EPS) return 0;<br> <br>    double min = Math.min(x * (1.0 - EPS),<br>                x * (1.0 + EPS));<br>    double max = Math.max(x * (1.0 - EPS),<br>                x * (1.0 + EPS));<br> <br>    if (max &lt; y) return -1;<br>    if (min &gt; y) return 1;<br>    return 0;<br>  }<br> <br>    public double shortestComputationTime(int y) {<br>        printf ("shortestComputationTime\n");<br> <br>    int years = y;<br> <br>    double minStart = 0, maxStart = years;<br> <br>    while (compare (minStart, maxStart) &lt; 0) {<br>      double start1 = minStart + (maxStart - minStart) / 3;<br>      double start2 = minStart + (maxStart - minStart) / 3 + (maxStart - minStart) / 3;<br> <br>      double wait1 = calc (start1, years);<br>      double wait2 = calc (start2, years);<br> <br>      if (wait2 &lt; wait1) {<br>        minStart = start1;<br>      } else {<br>        maxStart = start2;<br>      }<br>    }<br>    return calc (minStart, years);<br>    }<br> <br>  double calc (double start, int years) {<br>    return start + (double)years * Math.pow (2, -start / 1.5);<br>  }<br> <br> <br>    static void printf (String f, Object... a) {<br>    }<br> <br> <br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>