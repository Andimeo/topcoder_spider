            #include &lt;iostream&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>using namespace std;<br>class CustomerStatistics {<br>  public:<br>    vector&lt;string&gt; reportDuplicates(vector&lt;string&gt; customerNames) {<br>      int i, j, k, flag, count;<br>      vector&lt;string&gt; answer;<br>      string ans;<br>      for(i = 0;i &lt; customerNames.size();i++) {<br>        count = 0;<br>        for(j = i+1;j &lt; customerNames.size();j++) {<br>          if(customerNames[i] == customerNames[j]) {<br>            flag = 1;<br>            for(k = 0;k &lt; i;k++)<br>                                                        if(customerNames[i] == customerNames[k]) {<br>                                                                flag = 0;<br>                                                                break;<br>                                                        }<br>            if(flag) <br>              count++;<br>          }<br>        }<br>        if(count &gt; 0) {<br>          count++;<br>          ans = customerNames[i];<br>          ans.push_back(' ');<br>          string arr;<br>          int z;<br>          while(count) {<br>            z = count % 10;<br>            arr.push_back(z + '0');<br>            count = count / 10;<br>          }<br>          for(z = arr.size() - 1;z &gt;= 0;z--) {<br>            ans.push_back(arr[z]);<br>          }<br>          answer.push_back(ans);<br>        }<br>      }<br>      for(i = 0;i &lt; answer.size();i++) {<br>        for(j = i+1;j &lt; answer.size();j++) {<br>          k = 0;<br>          while((answer[i][k] == answer[j][k]) &amp;&amp; k &lt; answer[i].size() &amp;&amp; k &lt; answer[j].size()) k++;<br>            if(answer[i][k] &gt; answer[j][k]) {<br>              string temp = answer[i];<br>              answer[i] = answer[j];<br>              answer[j] = temp;<br>            }<br>        }<br>      }<br>      return answer;<br>    }<br>};<br></td>