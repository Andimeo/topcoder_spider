<td class="problemText" colspan="8" valign="middle" align="left">
            import java.util.Iterator;<br>import java.util.Map;<br>import java.util.TreeMap;<br>import java.util.Map.Entry;<br> <br>/*<br> * Created on Feb 4, 2006<br> *<br> */<br>/**<br> * @author Srikar<br> *<br> */<br>public class CustomerStatistics <br>{<br>  public String[] reportDuplicates(String[] customerNames)<br>  {<br>    TreeMap map = new TreeMap();<br>    int num = 0 ;<br>    for ( int i = 0 ; i &lt; customerNames.length ; i ++ )<br>    {<br>      String cn = customerNames[i];<br>      Integer cnt = (Integer) map.get(cn);<br>      if ( cnt == null )<br>      {<br>        cnt = new Integer(1);<br>      }<br>      else<br>      {<br>        int ct = cnt.intValue() ;<br>        if ( ct == 1 )<br>          num ++;<br>        ct ++;<br>        cnt = new Integer(ct);<br>      }<br>      map.put(cn,cnt);<br>    }<br>    Iterator iter = map.entrySet().iterator();<br>    String [] arr = new String[num];<br>    int tmp = 0 ;<br>    while ( iter.hasNext())<br>    {<br>      Map.Entry ent = (Entry) iter.next();<br>      int count = ((Integer)ent.getValue()).intValue();<br>      if ( count &gt; 1 )<br>      {<br>        arr[tmp ] = ((String)ent.getKey())  + " " + count ;<br>        tmp ++;<br>      }<br>    }<br>    return arr;<br>  }<br>}<br></td>