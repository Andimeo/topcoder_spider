            using System;<br><br>public class CompletingBrackets {<br>  public string complete(string text) {<br>    int balance = 0;<br>    string res = text;<br><br>    for (int i = 0; i &lt; text.Length; ++i)<br>    {<br>      if (text[i] == '[')<br>        ++balance;<br>      else<br>        --balance;<br>      if (balance &lt; 0)<br>      {<br>        ++balance;<br>        res = "[" + res;<br>      }<br>    }<br><br>    for (int i = 0; i &lt; balance; ++i)<br>      res = res + "]";<br><br>    return res;<br>  }<br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>