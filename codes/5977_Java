<td class="problemText" colspan="8" valign="middle" align="left">
            import java.util.*;<br><br>public class CompletingBrackets<br>{<br>  public String complete(String s)<br>  {<br>    int o = 0, c = 0;<br>    String res = "";<br><br>    for (int i=0; i &lt; s.length(); i++)<br>    {<br>      if (s.charAt(i) == ']' ) c++; else o++;<br><br>      if ( c &gt; o )<br>      {<br>        res = "[" + res;<br>        c--;<br>      }<br><br>      res += s.charAt(i) + "";<br>    }<br><br>    for (int i=1; i&lt;=o-c; i++) res += "]";<br><br>    return res;<br>  }<br>}<br><br>// Powered by FileEdit<br><br><br>// Powered by FileEdit<br></td>