            #include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;queue&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;math.h&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;numeric&gt;<br>using namespace std;<br>typedef vector&lt;int&gt; vi;<br>typedef vector&lt;double&gt; vd;<br>typedef vector&lt;string&gt; vs;<br>typedef vector&lt;vector&lt;int&gt; &gt; vvi;<br>typedef vector&lt;vector&lt;string&gt; &gt; vvs;<br><br>class SpeedTyper {<br>public:<br>  string lettersToPractice(string l, vector &lt;int&gt; t) {<br>    int mid = t[0];<br>    for (int i = 1; i &lt; t.size(); ++i)<br>      mid += t[i] - t[i - 1];<br>    mid /= t.size();<br>    string res;<br>    if (t[0] &gt; mid)<br>      res += l[0];<br>    for (int j = 1; j &lt; t.size(); ++j) {<br>      if (t[j] - t[j - 1] &gt; mid)<br>        res += l[j];<br>    }<br>    return res;<br>  }<br><br>  <br><br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>