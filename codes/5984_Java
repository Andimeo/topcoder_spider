            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class IntegerGenerator<br>{<br>  public String nextInteger(int[] a, String c)<br>  {<br>    boolean [] all = new boolean[10];<br>    for (int i = 0; i &lt; a.length; i++) all[a[i]] = true;<br>    if (c.charAt(0) == '0') return "INVALID INPUT";<br>    for (int i = 0; i &lt; c.length(); i++) if (!all[c.charAt(i)-'0']) return "INVALID INPUT";<br>    <br>    int f=0;<br>    for (int i = 0; i &lt;= 9; i++) if (all[i]) {f = i; break;}<br>    String ret = "";<br>    for (int i=c.length()-1; i&gt;= 0; i--)<br>    {<br>      int k = c.charAt(i)-'0';<br>      int j;<br>      for (j = k+1; j &lt;= 9; j++) if (all[j]) break;<br>      if (j &lt;= 9) return c.substring(0, i)+j+ret;<br>      ret = f+ret;<br>    }<br>    for (int i = 1; i &lt;= 9; i++) if (all[i]) {f = i; break;}<br>    return f+ret;<br>  }<br>}<br> <br>//Powered by [KawigiEdit]<br></td>