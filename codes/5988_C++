            #include &lt;cmath&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;string&gt;<br>#include &lt;cstring&gt;<br>#include &lt;vector&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br><br>using namespace std;<br><br>typedef vector&lt;int&gt; VI; <br>typedef vector&lt;VI&gt; VVI; <br>typedef vector&lt;string&gt; VS;<br>typedef vector&lt;VS&gt; VVS;<br><br>#define min(x, y)(((x)&lt;(y))?(x):(y))<br>#define max(x, y)(((x)&lt;(y))?(y):(x))<br><br>#define REP(i, n) for(i=0; i&lt;(n); ++i)<br><br>class UniqueDigits  { <br>public: <br>  bool is(int n) {<br>    set&lt;int&gt; s;<br>    while(n&gt;0) {<br>      if(s.find(n%10)!=s.end()) return false;<br>      s.insert(n%10);<br>      n=n/10;<br>    }<br>    return true;<br>  }<br>        int count(int n)  { <br>               int i, j, k;<br>               int ret=0;<br>         for(i = 1; i &lt; n; i++) <br>           if(is(i)) ret++;<br>               return ret;<br>          } <br> <br>        }; <br><br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>