            import java.util.*;<br><br>public class UniqueDigits {<br>    public int count(int n) {<br>    int rval = 0;<br>    <br>    for(int i = 1; i &lt; n; i++) {<br>      String s = Integer.toString(i);<br>      Set&lt;Integer&gt; digits = new HashSet&lt;Integer&gt;();<br>      boolean allDiff = true;<br>      <br>      for(int j = 0; j &lt; s.length(); j++) {<br>        int x = Integer.parseInt("" + s.charAt(j));<br>        <br>        if (digits.contains(x)) {<br>          allDiff = false;<br>          break;<br>        }<br>        <br>        digits.add(x);<br>      }<br>      <br>      if (allDiff) rval++;<br>    }<br>    <br>    return rval;<br>    }<br><br>    <br><br><br>};<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>