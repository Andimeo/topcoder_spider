<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>using System.Collections.Generic;<br>using System.Text;<br> <br>public class KidsWordGame<br>{<br>    public static bool GoodMove(string A, string B)<br>    {<br>        if (B == A || B.Length &lt;= A.Length) return false;<br>        return ((B.Substring(1) == A) || (B.Substring(0, B.Length - 1) == A));<br>    }<br> <br>    public static int getCheater(string[] first, string[] second, string[] third)<br>    {<br>        string S = first[0];<br>        for (int i = 0; i &lt; first.Length; i++)<br>        {<br>            if (second.Length == i) return -1;<br>            if (!GoodMove(S, second[i])) return 1;<br>            S = second[i];<br>            if (third.Length == i) return -1;<br>            if (!GoodMove(S, third[i])) return 2;<br>            S = third[i];<br>            if (first.Length == i + 1) return -1;<br>            if (!GoodMove(S, first[i + 1])) return 3;<br>            S = first[i + 1];<br>        }<br>        return -1;<br>    }<br>}<br></td>