            public class EnclosingRectangle{<br>public int smallestArea(string[] xxx,string[] yyy){<br>int i,j,k,m,z,best;<br>int res=0;<br>int lx=xxx.Length;<br>int ly=yyy.Length;<br>string[] xx=new string[lx];<br>string[] yy=new string[ly];<br>int[] x=new int[200];<br>int[] y=new int[200];<br>int[] a=new int[300];<br>int n=0;<br>for (i=0;i&lt;lx;i++){<br>  xx[0]=xx[0]+""+xxx[i];<br>  }<br>for (i=0;i&lt;ly;i++){<br>  yy[0]=yy[0]+""+yyy[i];<br>  }<br>xx[0]=" "+xx[0]+" ";<br>yy[0]=" "+yy[0]+" ";<br>for (i=0;i&lt;1;i++){<br>  m=xx[i].Length;<br>  k=0;<br>  for (j=0;j&lt;m-1;j++){<br>    if (xx[i][j]!=' ') {k=10*k+xx[i][j]-'0';}<br>    if ((xx[i][j]!=' ')&amp;&amp;(xx[i][j+1]==' ')){<br>      x[n]=k;<br>      n++;<br>      k=0;<br>      }<br>    }<br>  }<br>n=0;<br>for (i=0;i&lt;1;i++){<br>  m=yy[i].Length;<br>  k=0;<br>  for (j=0;j&lt;m-1;j++){<br>    if (yy[i][j]!=' ') {k=10*k+yy[i][j]-'0';}<br>    if ((yy[i][j]!=' ')&amp;&amp;(yy[i][j+1]==' ')){<br>      y[n]=k;<br>      n++;<br>      k=0;<br>      }<br>    }<br>  }<br>m=n/2;<br>res=1000000000;<br>for (i=0;i&lt;n;i++){<br>  for (j=i+1;j&lt;n;j++){<br>    if (y[i]&gt;y[j]){<br>      y[199]=y[i]; y[i]=y[j]; y[j]=y[199];<br>      x[199]=x[i]; x[i]=x[j]; x[j]=x[199];<br>      }<br>    }<br>  }<br>for (k=0;k&lt;300;k++){<br>  a[k]=1000000000;<br>  }<br>for (i=0;i&lt;n;i++){<br>  for (j=0;j&lt;n;j++){<br>    if (x[i]&lt;=x[j]) {<br>      for (k=0;k&lt;n;k++){<br>        a[k]=1000000000;<br>        }<br>      z=0;<br>      for (k=0;k&lt;n;k++){<br>        if ((x[k]&gt;=x[i])&amp;&amp;(x[k]&lt;=x[j])){<br>          a[z]=y[k];<br>          z++;  <br>          }<br>        }<br>      best=200000;<br>      for (k=0;k&lt;z;k++){<br>        if (a[k+m-1]-a[k]&lt;best) {best=a[k+m-1]-a[k];}<br>        }<br>      if (best&lt;200000) {<br>        if ((x[j]-x[i]+2)*(best+2)&lt;res) {res=(x[j]-x[i]+2)*(best+2);}<br>        }<br>      }<br>    }<br>  }<br>return res;<br>}<br>}<br></td>