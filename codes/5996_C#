            public class PaperRockScisQuals<br>{<br>  public int whoPassed( string[] players )<br>  {<br>    int[] scores = new int[players.Length];<br>    int current;<br>    <br>    for( int i = 0; i &lt; players.Length; i++ )<br>    {<br>      for( int j = i+1; j &lt; players.Length; j++ )<br>      {<br>        current = 0;<br>        for( int c = 0; c &lt; 5; c++ )<br>        {<br>          string choice = players[i][c].ToString() + (string)players[j][c].ToString();<br>          switch(choice)<br>          {<br>            case "PR":<br>            case "RS":<br>            case "SP":<br>              current++;<br>              break;<br>            case "SR":<br>            case "PS":<br>            case "RP":<br>              current--;<br>              break;<br>          }<br>        }<br>        <br>        if( current &gt; 0 ) scores[i] += 10;<br>        else if( current &lt; 0 ) scores[j] += 10;<br>        else { scores[i] += 5; scores[j] += 5; }<br>      }<br>    }<br>    <br>    int champ = 0;<br>    for( int i = 1; i &lt; scores.Length; i++ )<br>    {<br>      if( scores[i] &gt; scores[champ] )<br>        champ = i;<br>    }<br>    <br>    return champ;<br>  }<br>}<br></td>