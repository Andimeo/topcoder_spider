            #include&lt;iostream&gt;<br>#include&lt;vector&gt;<br>#include&lt;string&gt;<br>#include&lt;cmath&gt;<br>#include&lt;algorithm&gt;<br> <br>using namespace std;<br> <br>class PaperRockScisQuals {<br>public: int whoPassed(vector&lt;string&gt; p){<br>        vector&lt;double&gt; po(p.size());<br>        for(int i=0;i&lt;p.size();i++){<br>                for(int j=i+1;j&lt;p.size();j++){<br>                        double c1=0,c2=0;<br>                        for(int k=0;k&lt;5;k++){<br>                                if((p[i][k]=='P' &amp;&amp; p[j][k]=='R')||(p[i][k]=='R' &amp;&amp; p[j][k]=='S')||(p[i][k]=='S' &amp;&amp; p[j][k]=='P'))<br>                                                 c1+=1;<br>                                else if((p[j][k]=='P' &amp;&amp; p[i][k]=='R')||(p[j][k]=='R' &amp;&amp; p[i][k]=='S')||(p[j][k]=='S' &amp;&amp; p[i][k]=='P'))<br>                                     c2+=1;<br>                                else {<br>                                     c1+=0.5;<br>                                     c2+=0.5;<br>                                }<br>                        }<br>                        if(c1&gt;c2) po[i]+=1;<br>                        else if(c2&gt;c1) po[j]+=1;<br>                        else {  po[i]+=0.5; po[j]+=0.5; }<br>                }<br>        }<br>        //for(int i=0;i&lt;p.size();i++) cout&lt;&lt;po[i]&lt;&lt;endl;<br>        int c=0;<br>        double s=0;<br>        for(int i=0;i&lt;p.size();i++)<br>                if(po[i]&gt;s){<br>                             s=po[i];<br>                             c=i;<br>                }<br>        <br>        return c;<br>}<br>};<br></td>