            public class PaperRockScisQuals {<br>    public int whoPassed(String[] p) {<br>        int l=p.length,s[]=new int[l];<br>        for(int i=0;i&lt;l;i++)<br>            for(int j=0;j&lt;l;j++) {<br>                int h=0,g=0;<br>                if(i==j)<br>                    continue;<br>                for(int k=0;k&lt;5;k++) {<br>                    if(p[i].charAt(k)=='P' &amp;&amp; p[j].charAt(k)=='R' ||<br>                       p[i].charAt(k)=='R' &amp;&amp; p[j].charAt(k)=='S' ||<br>                       p[i].charAt(k)=='S' &amp;&amp; p[j].charAt(k)=='P')<br>                        h+=2;<br>                    else if(p[i].charAt(k)==p[j].charAt(k)) {<br>                        h+=1;<br>                        g+=1;<br>                    }<br>                    else<br>                        g+=2;<br>                }<br>                if(h&gt;g)<br>                    s[i]+=2;<br>                else if(h==g)<br>                    s[i]+=1;<br>            }<br>        int m=-1,b=0;<br>        for(int i=0;i&lt;l;i++)<br>            if(s[i]&gt;m) {<br>                m=s[i];<br>                b=i;<br>            }<br>        return b;<br>    }<br>}<br></td>