            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class StraightArray<br>{<br>  int dem(int[] nums,int k)<br>  {<br>    int ret=0;<br>    for (int i=1;i&lt;5;i++)<br>    {<br>      int gt=k+i;<br>      boolean kt=false;<br>      for (int j=0;j&lt;nums.length;j++)<br>        if (nums[j]==gt)<br>        {<br>          kt=true;<br>          break;<br>        }<br>      if (kt==false) ret++;<br>    }<br>    return ret;<br>  }<br>  public int howManyMore(int[] nums)<br>  {<br>    int ret=-1;<br>    for (int i=0;i&lt;nums.length;i++)<br>    {<br>      int gt=dem(nums,nums[i]);<br>      if (gt&lt;ret || ret==-1) ret=gt;<br>    }<br>    return ret;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>