            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class TextProcessor <br>{<br>  public string collectLetters(string text)<br>  {<br>    string ret = "";<br><br>    foreach ( char c in text )<br>      if ( char.IsLetter(c) )<br>        ret += char.ToLower(c);<br><br>    char[] tmp = ret.ToCharArray();<br>    Array.Sort&lt;char&gt;(tmp);<br>    return new string(tmp);<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>