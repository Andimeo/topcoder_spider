            import java.util.*;<br>import static java.lang.System.*;<br>public class TextProcessor {<br>  public String collectLetters(String text) {<br>    String kq = "";<br>    int[] a = new int[256];<br>    for (int i = 0; i &lt; text.length(); i++) {<br>      char c = text.charAt(i);<br>      if (Character.isLetter(c)) {<br>        a[ Character.toLowerCase(c)] ++;<br>      }<br>    }<br>    for (int i = 0; i &lt; 256; i++) {<br>      for (int j = 0; j &lt; a[i]; j++) {<br>        kq += (char) i;<br>        <br>      }<br>    }<br>    return kq;<br>  }<br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>