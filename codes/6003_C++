            #include &lt;iostream&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#define FOR(i,n) for(int i=0;i&lt;n;i++)<br>#define REP(i,f,t) for(int i=f;i&lt;=t;i++)<br>#define debln(a) cout&lt;&lt; #a &lt;&lt;" : " &lt;&lt; a &lt;&lt; endl;<br>using namespace std;<br> <br>class DiagonalDisproportion {<br>  public:<br>  int getDisproportion(vector &lt;string&gt; matrix) {<br>    int s1(0),s2(0);<br>    FOR(i,matrix.size()) {<br>      s1 += matrix[i][i];<br>      s2 += matrix[i][matrix[i].size()-i-1];<br>    }<br>    return s1-s2;<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>