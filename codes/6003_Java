            public class DiagonalDisproportion<br>{<br>  public int getDisproportion(String[] matrix)<br>  {<br>    int right=0,left=0;<br>    int length=matrix.length;<br>    for(int k=0;k&lt;length;k++)<br>    {<br>      right+=matrix[k].charAt(k)-48;<br>      left+=matrix[k].charAt(length-1-k)-48;<br>    }<br>    return right-left;<br>  }<br>  public static void main(String[] args)<br>  {<br>    DiagonalDisproportion d=new DiagonalDisproportion();<br>    String test[]=new String[]{"7748297018","8395414567","7006199788","5446757413","2972498628",<br>        "0508396790","9986085827","2386063041","5687189519","7729785238"};<br>    System.out.println(d.getDisproportion(test));<br>  }<br> <br>}<br></td>