            using System;<br>using System.Collections;<br>using System.Text;<br> <br>public class BasketsWithApples<br>{<br>    static void Main(string[] args)<br>    {<br> <br>        int[] t = new int[] { 24, 92, 38, 0, 79, 45 }<br> <br>;<br>        Console.WriteLine(removeExcess(t));<br>    }<br> <br>    public static int removeExcess(int[] apples)<br>    {<br>        int ret = 0;<br>        int i;<br> <br>        Array.Sort(apples);<br> <br>        for (i = 0; i &lt; apples.Length; i++)<br>            ret = Math.Max(ret, apples[i] * (apples.Length - i));<br>        return ret;<br> <br>    }<br>}<br></td>