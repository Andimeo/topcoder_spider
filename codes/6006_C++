            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br> <br>#define FORV(i,v) for(int i=0; i&lt;(int)v.size();i++)<br> <br>using namespace std;<br> <br>class BasketsWithApples {<br>public:<br>  int removeExcess(vector &lt;int&gt;);<br>};<br> <br>int BasketsWithApples::removeExcess(vector &lt;int&gt; apples) {<br> <br>  sort (apples.begin(),apples.end());<br>  int max = 0;<br>  FORV(i,apples)<br>  {<br>    int a =0;<br>    FORV(j,apples){<br>      if (apples[j]&gt;=apples[i]) a+=apples[i];<br>    }<br>    if (max&lt;a)max =a;<br>  }<br>  return max;<br>}<br></td>