            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class BasketsWithApples<br>{<br>  public int removeExcess(int[] apples)<br>  {<br>    int curBest = 0;<br>    for (int i = 0; i &lt; apples.length; i++) <br>    {<br>      // to match<br>      int tempAmount = 0;<br>      for (int j = 0; j &lt; apples.length; j++) {<br>        if (apples[j] &gt;= apples[i]) tempAmount+=apples[i];<br>      }<br>      curBest = Math.max(tempAmount, curBest);<br>            <br>      <br>    }<br>    <br> <br>    return curBest;<br>  }<br>  <br> <br>}<br> <br>//Powered by [KawigiEdit]<br></td>