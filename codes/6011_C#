            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class Truckloads {<br>    public int numTrucks(int numCrates, int loadSize) {<br>        return GetSize(numCrates, loadSize);<br>    }<br><br>  private int GetSize(int numCrates, int loadSize)<br>  {<br>    if(numCrates&lt;=loadSize)<br>      return 1;<br>    return GetSize(numCrates/2, loadSize) + GetSize(numCrates - (numCrates/2), loadSize );<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>