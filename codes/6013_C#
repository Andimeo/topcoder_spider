            using System;<br>using System.Collections;<br>public class TwoEquations {<br>  public string solve(string f, string s) {<br>    int[] a=new int[6];<br>    int[] res=new int[4];<br>    string[] temp=f.Split();<br>    if(temp[0][0]=='(')<br>      a[0]=-(int)(temp[0][2]-'0');<br>    else<br>      a[0]=(int)(temp[0][0]-'0');<br><br>    if(temp[2][0]=='(')<br>      a[1]=-(int)(temp[2][2]-'0');<br>    else<br>      a[1]=(int)(temp[2][0]-'0');<br><br>    if(temp[4][0]=='(')<br>      a[2]=-(int)(temp[4][2]-'0');<br>    else<br>      a[2]=(int)(temp[4][0]-'0');<br><br>    temp=s.Split();<br>    if(temp[0][0]=='(')<br>      a[3]=-(int)(temp[0][2]-'0');<br>    else<br>      a[3]=(int)(temp[0][0]-'0');<br><br>    if(temp[2][0]=='(')<br>      a[4]=-(int)(temp[2][2]-'0');<br>    else<br>      a[4]=(int)(temp[2][0]-'0');<br><br>    if(temp[4][0]=='(')<br>      a[5]=-(int)(temp[4][2]-'0');<br>    else<br>      a[5]=(int)(temp[4][0]-'0');<br><br><br>    if(a[0]*a[4]==a[1]*a[3])<br>    {<br>      if(a[0]==0 &amp;&amp; a[1]==0 &amp;&amp; a[2]!=0)<br>        return "NO SOLUTIONS";<br>      if(a[3]==0 &amp;&amp; a[4]==0 &amp;&amp; a[5]!=0)<br>        return "NO SOLUTIONS";<br>      if(a[0]==0 &amp;&amp; a[1]==0)<br>        return "MULTIPLE SOLUTIONS";<br>      if(a[3]==0 &amp;&amp; a[4]==0)<br>        return "MULTIPLE SOLUTIONS";<br>      if(a[0]==0)<br>      {<br>        if(a[1]*a[5]==a[2]*a[4])<br>          return "MULTIPLE SOLUTIONS";<br>        return "NO SOLUTIONS";<br>      }<br>      if(a[0]*a[5]==a[2]*a[3])<br>        return "MULTIPLE SOLUTIONS";<br>      return "NO SOLUTIONS";<br>    }<br>    res[0]=a[5]*a[1]-a[4]*a[2];<br>    res[2]=-a[5]*a[0]+a[3]*a[2];<br>    res[1]=res[3]=a[3]*a[1]-a[4]*a[0];<br><br>    if(res[1]&lt;0)<br>    {<br>      res[0]=-res[0];<br>      res[1]=-res[1];<br>      res[2]=-res[2];<br>      res[3]=-res[3];<br>    }<br>    for(int i=res[1];i&gt;1;i--)<br>      if(res[0]%i==0 &amp;&amp; res[1]%i==0)<br>      {<br>        res[0]/=i;<br>        res[1]/=i;<br>        break;<br>      }<br>    for(int i=res[3];i&gt;1;i--)<br>      if(res[2]%i==0 &amp;&amp; res[3]%i==0)<br>      {<br>        res[2]/=i;<br>        res[3]/=i;<br>        break;<br>      }<br><br>    string re="X=";<br>    if(res[0]&lt;0)<br>      re+="(";<br>    re+=res[0].ToString();<br>    if(res[0]&lt;0)<br>      re+=")";<br>    re+="/";<br>    if(res[1]&lt;0)<br>      re+="(";<br>    re+=res[1].ToString();<br>    if(res[1]&lt;0)<br>      re+=")";<br>    re+=" Y=";<br>    if(res[2]&lt;0)<br>      re+="(";<br>    re+=res[2].ToString();<br>    if(res[2]&lt;0)<br>      re+=")";<br>    re+="/";<br>    if(res[3]&lt;0)<br>      re+="(";<br>    re+=res[3].ToString();<br>    if(res[3]&lt;0)<br>      re+=")";<br>    return re;<br>  }<br><br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003], modified to support C#<br>// Powered by CodeProcessor<br></td>