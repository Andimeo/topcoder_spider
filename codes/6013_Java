            import java.util.*;<br>public class TwoEquations {<br>  int parseNum(String s) {<br>    if(s.charAt(0)=='(') return Integer.parseInt(s.substring(1, s.length()-1));<br>    else return Integer.parseInt(s);<br>  }<br>  int[] parseStr(String s) {<br>    String[] spl = s.split(" ");<br>    int[] a = new int[]{parseNum(spl[0].substring(0,spl[0].length()-2)), parseNum(spl[2].substring(0,spl[2].length()-2)), parseNum(spl[4])};<br>    return a;<br>  }<br>  String enc(int a) {<br>    if(a&lt;0) return "("+a+")";<br>    return ""+a;<br>  }<br>  String make(int a, int b) {<br>    int gcd = 1;<br>    for(int i = 1; i&lt;=Math.abs(b); i++) {<br>      if(b%i==0 &amp;&amp; a%i==0) gcd = i;<br>    }<br>    if(b&lt;0) gcd = -gcd;<br>    a/=gcd;<br>    b/=gcd;<br>    return enc(a)+"/"+b;<br>  }<br>    public String solve(String first, String second) {<br>    String res;<br>    int[] a1 = parseStr(first);<br>    int[] a2 = parseStr(second);<br>    int det = a1[0]*a2[1]-a1[1]*a2[0];<br>    int det1 = a1[2]*a2[1]-a1[1]*a2[2];<br>    int det2 = a1[0]*a2[2]-a1[2]*a2[0];<br>    if(det==0) {<br>      if(a1[0]==0&amp;&amp;a1[1]==0&amp;&amp;a2[0]==0&amp;&amp;a2[1]==0) {<br>        if(a1[2]==0&amp;&amp;a2[2]==0) return "MULTIPLE SOLUTIONS";<br>        else return "NO SOLUTIONS";<br>      }<br>      if(det1==0&amp;&amp;det2==0) {<br>        return "MULTIPLE SOLUTIONS";<br>      } else {<br>        return "NO SOLUTIONS";<br>      }<br>    }<br>    res = "X="+make(det1,det)+" Y="+make(det2,det);<br>        return res;<br>    }<br>}<br> <br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>