            using System;<br>using System.Collections;<br><br>public class BobTrouble {<br>  Hashtable id;<br><br>  public int minSupers(string[] name, string[] bossName) <br>  {<br>    int n = name.Length;<br>    id = new Hashtable();<br>    bool[] isSuper = new bool[n];<br>    int[] parent = new int[n];<br>    <br>    for (int i = 0; i &lt; n; ++i) <br>    {<br>      id[name[i]] = i;<br>      parent[i] = -1;<br>    }<br><br>    bool anyBob = false;<br><br>    for (int i = 0; i &lt; n; ++i)<br>    {<br>      if (bossName[i].Equals("*"))<br>        continue;<br><br>      if (bossName[i].Equals("BOB")) <br>      {<br>        anyBob = true;<br>        parent[i] = -2;<br>        continue;<br>      }<br><br>      parent[i] = (int) id[bossName[i]];<br><br>      isSuper[parent[i]] = true;<br>    }<br><br>    bool anyGoodBob = false;<br>    <br>    for (int i = 0; i &lt; n; ++i)<br>    {<br>      bool[] visited = new bool[n];<br>      int cur = i;<br>      while (cur &gt;= 0)<br>      {<br>        if (visited[cur])<br>          return -1;<br>        <br>        visited[cur] = true;<br>        cur = parent[cur];<br>      }<br><br>      if (cur == -1 &amp;&amp; name[i].Equals("BOB"))<br>        anyGoodBob = true;<br>    }<br><br>    int numSuper = 0;<br>    for (int i = 0; i &lt; n; ++i)<br>      if (isSuper[i])<br>        ++numSuper;<br><br>    if (!anyBob)<br>      return numSuper;<br><br>    if (!anyGoodBob)<br>      return -1;<br><br>    return numSuper + 1;<br>  }<br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>