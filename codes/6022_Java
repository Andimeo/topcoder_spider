            import java.util.*;<br>public class DigitByDigit {<br>  public double expectedScore(String digits) {<br>    double[][] ex = new double[51][];<br>    ex[1] = new double[]{4.5};<br>    for (int i=2; i&lt;51; i++) {<br>      ex[i] = new double[i];<br>      int p = 0;<br>      for (int j=0; j&lt;10; j++) {<br>        while (p&lt;i-1 &amp;&amp; ex[i-1][p] &lt; j) p++;<br>        for (int k=0; k&lt;p; k++) ex[i][k] += ex[i-1][k];<br>        ex[i][p] += j;<br>        for (int k=p+1; k&lt;i; k++) ex[i][k] += ex[i-1][k-1];        <br>      }<br>      for (int j=0; j&lt;i; j++) ex[i][j] *= 0.1;<br>    }<br>    int k = 0;<br>    for (char c : digits.toCharArray()) if (c=='_') k++;<br>    double result = 0;<br>    double pow = 1;<br>    int u = 0;<br>    for (int i=digits.length()-1; i&gt;=0; i--) {<br>      if (digits.charAt(i)=='_') {result += pow*ex[k][u]; u++;}<br>      else result += pow*(digits.charAt(i)-'0');<br>      pow *= 10;<br>    }<br>    //System.out.println(Arrays.deepToString(ex));<br>    return result;<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>