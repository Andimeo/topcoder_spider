            Imports Microsoft.VisualBasic<br>Imports System<br>Imports System.Collections<br>Imports System.Math<br> <br>Public Class CannonBalls<br>    Public Function fewestPiles(ByVal n As Integer) As Integer<br>        Dim i, j, k, l As Integer<br> <br>        Dim aMinPiles(300000) As Integer<br>        Dim aVals(120) As Integer<br>        Dim aTets(120) As Integer<br>        For i = 1 To 120<br>            aVals(i) = aVals(i - 1) + i<br>            aTets(i) = aTets(i - 1) + aVals(i)<br>        Next<br> <br>        For i = 1 To 300000<br>            aMinPiles(i) = Integer.MaxValue<br>            For j = 1 To 120<br>                If i - aTets(j) &gt;= 0 Then<br>                    aMinPiles(i) = Min(aMinPiles(i), aMinPiles(i - aTets(j)) + 1)<br>                End If<br>            Next<br>        Next<br> <br>        Return aMinPiles(n)<br> <br> <br>    End Function<br>End Class<br></td>