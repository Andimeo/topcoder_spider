            //by &lt;&lt;&lt;Gaisan Dmitrii&gt;&gt;&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;memory.h&gt;<br>#include &lt;ctype.h&gt;<br><br>using namespace std;<br><br>#define REP( i, n ) for( typeof( n ) i = 0; i &lt; ( n ); i++ )<br>#define FOR( i, m, n ) for( typeof( n ) i = ( m ); i &lt; ( n ); i++ )<br>#define FORD( i, n, m ) for( int i = ( n ); i &gt;= m; i-- )<br>#define CLR( x ) memset( ( x ), 0, sizeof( x ) )<br>#define All( v ) (v.begin(), v.end())<br><br>class AccountBalance {<br>  public:<br>      int processTransactions( int res, vector &lt;string&gt; trans ) {<br>    int sz = trans.size();<br>    <br>        REP( i, sz ){<br>        stringstream stm( trans[ i ] );<br>        char c;<br>        int flag, cur;<br>        <br>          stm &gt;&gt; c; stm &gt;&gt; cur;<br>          if( c == 'C' )<br>            flag = 1;<br>          else<br>            flag = -1;<br>          cur *= flag;<br>          res += cur;<br>       }<br>      return ( res );<br>      }<br>};<br><br><br>// Powered by FileEdit<br></td>