            Public Class AccountBalance<br>    Public Function processTransactions(ByVal startingBalance As Integer, ByVal transactions As String()) As Integer<br>        Dim i, j As Integer<br> <br>        Dim trstr As String()<br>        Dim result As Integer = startingBalance<br>        For i = 0 To transactions.Length - 1<br>            trstr = transactions(i).Split(" ")<br>            If trstr(0) = "C" Then<br>                result += Integer.Parse(trstr(1))<br>            Else<br>                result -= Integer.Parse(trstr(1))<br>            End If<br>        Next<br>        Return result<br>    End Function<br>End Class<br></td>