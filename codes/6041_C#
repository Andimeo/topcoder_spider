            using System;<br><br>public class Projections {<br>  public int[] count(string front, string right) {<br>    int n1 = 0;<br>    foreach (char ch in front)<br>      if (ch == 'x')<br>        ++n1;<br>    int n2 = 0;<br>    foreach (char ch in right)<br>      if (ch == 'x')<br>        ++n2;<br>    return new int[]{Math.Max(n1, n2), n1 * n2};<br>  }<br><br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>