            #include &lt;algorithm&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cmath&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;string&gt;<br>#include &lt;utility&gt;<br>#include &lt;vector&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;queue&gt;<br>#include &lt;stack&gt;<br><br>using namespace std;<br><br>class Projections {<br>public:<br>  vector &lt;int&gt; count(string a, string b) {<br>    int i, m, n;<br>    vector&lt;int&gt; ret;<br><br>    for (m = i = 0; i &lt; a.size(); i++)<br>      if (a[i] == 'x') m++;<br><br>    for (n = i = 0; i &lt; b.size(); i++)<br>      if (b[i] == 'x') n++;<br><br>    ret.push_back(max(m, n));<br>    ret.push_back(m*n);<br>    return ret;<br>  }<br>};<br><br><br>// Powered by FileEdit<br></td>