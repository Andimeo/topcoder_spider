            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import static java.lang.Math.*;<br>import static java.util.Arrays.*;<br>public class Projections {<br>  public int[] count(String front, String right) {<br>    int numCol = 0;<br>    int numRow = 0;<br>    for(int i = 0; i &lt; front.length(); i++){<br>      if(front.charAt(i) == 'x') numCol++;<br>    }<br>    for(int i = 0; i &lt; right.length(); i++){<br>      if(right.charAt(i) == 'x') numRow++;<br>    }<br>    return new int[] {max(numCol, numRow), numCol*numRow};<br>  }<br> <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>