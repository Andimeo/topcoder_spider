            using System;<br>using System.Text;<br><br>public class PalindromePartition {<br>  public int partition(string[] s) {<br>    StringBuilder all = new StringBuilder();<br>    foreach (string st in s)<br>      all.Append(st);<br>    string x = all.ToString();<br>    bool[,] isPalin = new bool[x.Length, x.Length];<br>    for (int i = 0; i &lt; x.Length; ++i) <br>    {<br>      isPalin[i, i] = true;<br>      if (i &lt; x.Length - 1)<br>        isPalin[i + 1, i] = true;<br>    }<br>    for (int len = 2; len &lt;= x.Length; ++len)<br>    {<br>      for (int i = 0; i &lt; x.Length - len + 1; ++i)<br>      {<br>        int j = i + len - 1;<br>        isPalin[i, j] = isPalin[i + 1, j - 1] &amp;&amp; x[i] == x[j];<br>      }<br>    }<br>    int[] min = new int[x.Length + 1];<br>    for (int i = 1; i &lt;= x.Length; ++i)<br>    {<br>      int best = int.MaxValue;<br>      for (int j = 0; j &lt; i; ++j)<br>        if (isPalin[j, i - 1])<br>        {<br>          best = Math.Min(best, min[j] + 1);<br>        }<br>      min[i] = best;<br>    }<br>    return min[x.Length];<br>  }<br><br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>