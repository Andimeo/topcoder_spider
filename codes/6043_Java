            public class PalindromePartition {<br>    public int partition(String[] s) {<br>        String d="";<br>        for(String tem:s) d+=tem;<br>        int n=d.length();<br>        boolean a[][]=new boolean [n][n];<br>        for(int i=0;i&lt;n;i++) {<br>            int j1=i,j2=i;<br>            while (d.charAt(j1)==d.charAt(j2)) {<br>                a[j1][j2]=true;<br>                j1--;<br>                j2++;<br>                if (j1&lt;0 || j1==n || j2&lt;0 || j2==n) break;<br>            }<br>            if (i!=n-1) {<br>                j1=i; j2=i+1;<br>                while (d.charAt(j1)==d.charAt(j2)) {<br>                    a[j1][j2]=true;<br>                    j1--;<br>                    j2++;<br>                    if (j1&lt;0 || j1==n || j2&lt;0 || j2==n) break;<br>                }<br>            }<br>        }<br>        int b[]=new int [n+1];<br>        for(int i=0;i&lt;n;i++)<br>            for(int j=i;j&lt;n;j++)<br>                if (a[i][j] &amp;&amp; (b[j+1]==0 || b[j+1]&gt;b[i]+1)) b[j+1]=b[i]+1;<br>        return b[n];        <br>    }<br>}<br></td>