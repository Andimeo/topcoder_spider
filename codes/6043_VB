            imports System<br>imports Microsoft.VisualBasic<br>imports System.Collections<br>imports System.Text<br> <br> <br>Public Class PalindromePartition<br> <br>    Dim memo(2500) As Integer<br>    Dim sA As String<br>    Dim sR As String<br>    Dim isPal(2500, 2500) As Boolean<br> <br>    Public Function partition(ByVal sPal() As String) As Integer<br>        Dim i As Integer<br> <br>        sa = ""<br>        For i = 0 To sPal.Length - 1<br>            sa = sa &amp; sPal(i)<br>        Next<br> <br>        For i = 0 To sA.Length - 1<br>            sR = sR &amp; sA.Chars(sA.Length - 1 - i)<br>        Next<br> <br>        For i = 0 To 2500<br>            memo(i) = 1000000<br>        Next<br>        memo(0) = 0<br> <br>        Dim iMid As Integer<br>        Dim iFC As Integer<br>        Dim iLC As Integer<br> <br>        For iMid = 0 To Len(sA)<br> <br>            iFC = iMid<br>            iLC = iMid<br>            While True<br>                If iFC &lt; 0 Or iLC &gt; Len(sA) - 1 Then<br>                    Exit While<br>                ElseIf sA.Chars(iFC) = sA.Chars(iLC) Then<br>                    isPal(iFC, iLC - iFC + 1) = True<br>                Else<br>                    Exit While<br>                End If<br>                iFC = iFC - 1<br>                iLC = iLC + 1<br>            End While<br> <br>            iFC = iMid<br>            iLC = iMid + 1<br>            While True<br>                If iFC &lt; 0 Or iLC &gt; Len(sA) - 1 Then<br>                    Exit While<br>                ElseIf sA.Chars(iFC) = sA.Chars(iLC) Then<br>                    isPal(iFC, iLC - iFC + 1) = True<br>                Else<br>                    Exit While<br>                End If<br>                iFC = iFC - 1<br>                iLC = iLC + 1<br>            End While<br> <br>        Next<br> <br>        Dim iR As Integer<br>        iR = GetL(Len(sA))<br>        Return iR<br>    End Function<br> <br>    Private Function GetL(ByVal iLeft As Integer) as integer<br>        If memo(iLeft) &lt;&gt; 1000000 Then<br>            Return memo(iLeft)<br>        End If<br> <br>        Dim iBest As Integer<br>        Dim iLen As Integer<br>        Dim iUsed As Integer<br>        Dim iScore As Integer<br> <br>        iBest = 1000000<br>        iUsed = sA.Length - iLeft<br>        For iLen = 1 To iLeft<br>            If isPal(iUsed, iLen) Then<br>                iScore = 1 + GetL(iLeft - iLen)<br>                If iScore &lt; iBest Then iBest = iScore<br>            End If<br>        Next<br> <br>        memo(iLeft) = iBest<br>        Return iBest<br>    End Function<br> <br>End Class<br></td>