            using System;<br>using System.Collections.Generic;<br><br>public class CheapestTabComplete<br>{<br>  public int getFewest(string[] names, string w)<br>  {<br>    int n = w.Length;<br>    int[] result = new int[n + 1];<br>    result[0] = 0;<br>    for (int i = 1; i &lt;= n; i++)<br>      result[i] = int.MaxValue / 2;<br><br>    List&lt;string&gt; matches = new List&lt;string&gt;();<br>    for (int i = 0; i &lt; n; i++)<br>    {<br>      result[i + 1] = Math.Min(result[i + 1], result[i] + 1);<br>      <br>      matches.Clear();<br>      string cur = w.Substring(0, i);<br>      foreach (string name in names)<br>      {<br>        if (name.StartsWith(cur))<br>          matches.Add(name);<br>      }<br>      matches.Sort();<br>      <br>      for (int j = 0; j &lt; matches.Count; j++)<br>      {<br>        string match = matches[j];<br>        if (w.StartsWith(match))<br>        {<br>          int k = match.Length;<br>          result[k] = Math.Min(result[k], result[i] + j + 1);<br>        }<br>      }<br>    }<br><br>    return result[n] + 1;<br>  }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>