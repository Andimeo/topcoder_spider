            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class InverseCollatz {<br>    public string getForm(string s) {<br>        string res;<br>        long a, b;<br>        int n = s.Length;<br>        a = 2;<br>        if (s[n - 1] == 'E') b = 0;<br>        else b = 1;<br>        for (int i = n - 2; i &gt;= 0; i--)<br>        {<br>            if (s[i] == 'E')<br>            {<br>                a *= 2;<br>                b *= 2;<br>            }<br>            else<br>            {<br>                b--;<br>                if (b &lt; 0) b += a;<br>                int j;<br>                for (j = 0; j &lt; 3; j++) if ((b + j * a) % 3 == 0) break;<br>                b = (b + j * a) / 3;<br>                if (a % 3 == 0) a /= 3;<br>            }<br>            //Console.WriteLine(a + " " + b);<br>        }<br>        res = a.ToString() + "k+" + b.ToString();<br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>