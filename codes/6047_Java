            import java.util.*;<br>import java.math.BigInteger;<br> <br>public class InverseCollatz {<br>    public String getForm(String s) {<br>        int n = s.length();<br>        BigInteger TWO = BigInteger.valueOf(2);<br>        BigInteger THREE = BigInteger.valueOf(3);<br>        BigInteger a = BigInteger.valueOf(2);<br>        BigInteger b;<br>        if (s.charAt(n - 1) == 'E') {<br>            b = BigInteger.valueOf(0);<br>        } else {<br>            b = BigInteger.valueOf(1);<br>        }<br>        for (int i = n - 2; i &gt;= 0; i--) {<br>            if (s.charAt(i) == 'E') {<br>                a = a.multiply(TWO);<br>                b = b.multiply(TWO);<br>            } else {<br>                b = b.add(a.subtract(BigInteger.ONE)).multiply(THREE.modInverse(a)).mod(a);<br>            }<br>        }<br>        return a + "k+" + b;<br>    }<br> <br> <br>}<br> <br> <br>// Powered by PopsEdit<br> <br>// Powered by CodeProcessor<br></td>