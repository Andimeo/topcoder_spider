            using namespace std;<br><br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;cstring&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;map&gt;<br><br>#define ALL(xx) (xx).begin(), (xx).end()<br>#define pb push_back<br>#define sz size()<br><br>#define FOR(ii,ss,ff) for(ii=ss;ii&lt;=ff;++ii)<br>#define REP(ii,ff) for(ii=0;ii&lt;ff;++ii)<br>#define VS vector&lt;string&gt;<br>#define VI vector&lt;int&gt;<br>#define SS stringstream<br><br>int T[60][60][60];<br><br>class MaxKTrace<br>{<br>public:<br>int getLargest(vector &lt;string&gt; mat, int K)<br>{<br>    int i, j, k, N( mat.sz ), u, v;<br>    <br>    REP(i,N) REP(j,N)<br>       mat[i][j] -= '0';<br>    <br>    memset( T, 0, sizeof( T ) );<br>    <br>    REP(i,54) REP(j,54) REP(k,54)<br>       T[i][j][k] = -500000;<br>    <br>    T[0][0][0] = 0;<br>    <br>    FOR(i,1,N)<br>       FOR(j,1,N)<br>          FOR(k,1,K)<br>          {<br>               FOR(u,0,i-1)<br>                  FOR(v,0,j-1)<br>                     T[i][j][k] = max( T[i][j][k], T[u][v][k-1] + mat[ i - 1 ][ j - 1 ] );                                                  <br>          }<br>          <br>     int res = 0;<br>     <br>     FOR(i,1,N)<br>        FOR(j,1,N)<br>           res = max( res, T[i][j][K] );  <br>     <br>     return res; <br>    <br>    <br>}<br><br><br> <br>};<br><br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>