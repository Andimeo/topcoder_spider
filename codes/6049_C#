            using System;<br>using System.Collections;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class NumJordanForms<br>{<br>    int M = 179424673;<br>    public int howMany(int[] charPoly, int[] minPoly)<br>    {<br> <br>        long [] count = new long[4001];<br>        count [0] = 1;<br>        long  ret = 1;<br>        for (int k = 1;k&lt;=4000;k++)<br>        {<br>            for (int j = 0;j+k&lt;=4000;j++)<br>            {<br>                count[j+k] = (count[j+k] + count[j])% M;<br>            }<br>           for (int i = 0;i&lt;charPoly.Length; i++)<br>               if (minPoly[i] == k)<br>               {<br>                   ret = ret * count[charPoly[i] - k] % M;<br>               }<br>        }<br>        return (int)ret;<br>    }<br> }<br></td>