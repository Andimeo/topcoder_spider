            #include &lt;iostream&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;map&gt;<br>#include &lt;algorithm&gt;<br>using namespace std;<br> <br>int cnt[4001][4001];<br>class NumJordanForms {<br>public:<br>int NumJordanForms::howMany(vector&lt;int&gt; charPoly, vector&lt;int&gt; minPoly) {<br>  int i,j;<br>  int x,y;<br>  long long ans;<br>  <br>    memset( cnt,0 ,sizeof(cnt) );<br>    for ( i = 0; i &lt;= 4000; i ++)<br>        cnt[i][1] = cnt[0][i] = 1;<br>    for ( i = 1; i &lt;= 4000; i ++)<br>        for ( j = 2; j &lt;= 4000; j ++) {<br>      cnt[i][j] = cnt[i][j-1];<br>      if (i &gt;= j)<br>            cnt[i][j] += cnt[i-j][j];<br>      cnt[i][j] %= 179424673;<br>    }<br>    int len = charPoly.size();<br>  ans = 1;<br>    for ( i = 0; i &lt; len; i++) {<br>    x=charPoly[i] - minPoly[i];<br>    y=minPoly[i];<br>       ans = ans * cnt[x][y] % 179424673;<br>    }<br>  return int(ans);<br>}<br>};<br></td>