<td class="problemText" colspan="8" valign="middle" align="left">
            public class NumJordanForms {<br> <br>    private static final int MOD = 179424673;<br> <br>    private static int[][] mem = new int[4001][4001];<br> <br>    private int recur(int n, int k) {<br>        if (n == 0 || k == 1) return 1;<br>        if (mem[n][k] != 0) {<br>            return mem[n][k];<br>        }<br> <br>        int res = 0;<br>        if (k &gt; n) {<br>            res = recur(n, n);<br>        } else {<br>            res = recur(n, k - 1) + recur(n - k, k);<br>            mem[n][k] = res % MOD;<br>        }<br> <br>        return res % MOD;<br>    }<br> <br>    public int howMany(int[] charPoly, int[] minPoly) {<br>        long res = 1;<br>        for (int i = 0; i &lt; charPoly.length; i++) {<br>            res *= recur(charPoly[i] - minPoly[i], minPoly[i]);<br>            res %= MOD;<br>        }<br>        return (int) res % MOD;<br>    }<br>}<br></td>