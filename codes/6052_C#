            using System;<br><br>public class NumPermutationOrders {<br>  long[] nums;<br><br>  public long howMany(int n) {<br>    nums = new long[n + 1];<br>    for (int i = 0; i &lt;= n; ++i)<br>      nums[i] = 1;<br><br>    for (int i = 2; i &lt;= n; ++i)<br>      if (isPrime(i))<br>      {<br>        for (int j = n; j &gt; 0; --j)<br>        {<br>          int z = i;<br>          while (z &lt;= j)<br>          {<br>            nums[j] += nums[j - z];<br>            z *= i;<br>          }<br>        }<br>      }<br><br>    return nums[n];<br>  }<br><br>  private bool isPrime(int a)<br>  {<br>    for (int j = 2; j * j &lt;= a; ++j)<br>      if (a % j == 0)<br>        return false;<br>    return true;<br>  }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>