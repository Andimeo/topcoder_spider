            #define FR(i,a,b) for (int i = (a); i &lt; (b); i++)<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;stdio.h&gt;<br>#include &lt;stdlib.h&gt;<br>#include &lt;string.h&gt;<br>#include &lt;math.h&gt;<br>#include &lt;ctype.h&gt;<br>#include &lt;setjmp.h&gt;<br>#include &lt;regex.h&gt;<br>#include &lt;utility&gt;<br>#define ll long long<br>#define ld long double<br>#define EPS 1e-9<br>using namespace std;<br> <br>ll dp[1024][1024];<br>struct NumPermutationOrders {<br>int pr[2222], npr;<br>ll ans(int n, int fp=0) {<br> if (!n) return 1;<br> if (n&lt;0) return 0;<br> if (pr[fp] &gt; n) return 1;<br> if (dp[n][fp] != -1) return dp[n][fp];<br> ll x=ans(n,fp+1);<br> for (int i = pr[fp]; i &lt;= n; i *= pr[fp])<br>  x += ans(n-i,fp+1);<br>// printf("ans %i %i = %lli\n", n,fp,x);<br> return dp[n][fp] = x;<br>}<br>long long howMany(int n) {<br>pr[0]=2;<br>npr=1;<br>FR(i,3,2222) {<br> FR(j,2,i) if (!(i%j)) goto sadface;<br> pr[npr++]=i;<br> sadface:;<br>}<br>memset(dp,-1,sizeof(dp));<br>return ans(n);<br> <br>}};<br>// Powered by ted<br></td>