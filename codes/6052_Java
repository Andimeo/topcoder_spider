            import java.util.*;<br>public class NumPermutationOrders {<br>    public long howMany(int n) {<br>      long[] f = new long[n+1];<br>      long[] next = new long[n+1];<br>      <br>      Arrays.fill(f, 0);<br>      f[0] = 1;<br>      <br>      for (int i=2; i &lt;= n; i++) {<br>        int j=2;<br>        while (i % j != 0) j++;<br>                <br>        if (j == i) {          <br>          Arrays.fill(next, 0);<br>          for (j=0; j &lt;= n; j++) {<br>            int x = i;<br>            next[j] = f[j];<br>            while (j &gt;= x) {<br>              next[j] += f[j - x];<br>              x *= i;<br>            }<br>          }<br>          for (j=0; j &lt;= n; j++)<br>            f[j] = next[j];<br>        }        <br>      }<br>      <br>      long res = 0;<br>    for (int j=0; j &lt;= n; j++)<br>      res += f[j];<br>    <br>    return res;<br>    }<br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>