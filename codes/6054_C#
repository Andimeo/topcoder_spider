            using System;<br><br>public class FirstToLast {<br>  const int INF = 2000000000;<br><br>  public int getSmallest(int p, int q) {<br>    long p10 = 1;<br>    int best = INF;<br>    for (int n = 0; n &lt;= 9; ++n)<br>    {<br>      int max = 9;<br>      if (n == 9)<br>        max = 1;<br>      for (long x = 1; x &lt;= max; ++x)<br>      {<br>        long a = (p * p10 - q) * x;<br>        long b = (10 * q - p);<br>        long y = 0;<br><br>        if (b == 0)<br>          if (a == 0)<br>            y = 0;<br>          else<br>            continue;<br>        else<br>          if (a % b == 0)<br>            y = a / b;<br>          else<br>            continue;<br><br>        if (y &gt;= 0 &amp;&amp; y &lt; p10)<br>        {<br>          long cur = x * p10 + y;<br>          if (cur &lt; best)<br>            best = (int) cur;<br>        }<br>      }<br>      p10 *= 10;<br>    }<br><br>    if (best &gt;= INF)<br>      return -1;<br>    else<br>      return best;<br>  }<br><br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>