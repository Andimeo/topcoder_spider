            import java.util.*;<br>import static java.lang.Math.*;<br><br>public class FirstToLast {<br><br>    public int getSmallest(int p, int q) {<br>        long res = Long.MAX_VALUE;<br>        <br>        for (long x = 1; x &lt;= 9; x++) {<br>          long dec = 1;<br>          for (long k = 0; k &lt;= 9; k++) {<br>            long num = x * (p * dec - q);<br>            long den = 10 * q - p;<br>            if (den != 0) {<br>              if (num % den == 0) {<br>                long y = num / den;<br>                if (y &lt; dec &amp;&amp; y &gt;= 0) {<br>                  long tmp = y + x * dec;<br>                  if (tmp &gt; 0 &amp;&amp; res &gt; tmp) {<br>                    res = tmp;<br>                  }<br>                }<br>              }<br>            }<br>            dec *= 10;<br>          }<br>        }<br>        <br>        if (res &gt;= 2000000000)<br>          res = -1;<br>        return (int) res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>