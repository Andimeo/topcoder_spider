            using System;<br><br>public class Snowflakes {<br>  public string[] flareOut(string[] snowflake) {<br>    int n = snowflake.Length;<br><br>    char[][] res = new char[2 * n][];<br>    for (int i = 0; i &lt; 2 * n; ++i)<br>      res[i] = new char[2 * n];<br><br>    for (int i = 0; i &lt; n; ++i)<br>    {<br>      for (int j = 0; j &lt;= i; ++j) <br>      {<br>        res[i + n][j + n] = snowflake[i][j];<br>        res[j + n][i + n] = res[i + n][j + n];<br>      }<br>    }<br><br>    for (int i = 0; i &lt; n; ++i)<br>      for (int j = 0; j &lt; n; ++j)<br>      {<br>        res[i + n][j] = res[i + n][2 * n - 1 - j];<br>      }<br><br>    for (int i = 0; i &lt; n; ++i)<br>      for (int j = 0; j &lt; 2 * n; ++j)<br>      {<br>        res[i][j] = res[2 * n - 1 - i][j];<br>      }<br><br>    string[] r = new string[2 * n];<br>    for (int i = 0; i &lt; r.Length; ++i)<br>      r[i] = new string(res[i]);<br><br>    return r;<br>  }<br><br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>