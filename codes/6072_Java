            import java.util.*;<br>public class Snowflakes {<br>    public String[] flareOut(String[] snowflake) {<br>        int n = snowflake.length;<br>        String[] res = new String[snowflake.length*2];<br>        for(int i = 0; i&lt;res.length; i++) {<br>            String s = "";<br>            for(int j = 0; j&lt;res.length; j++) {<br>                int x = i;<br>                int y = j;<br>                if(x&lt;n) x = 2*n-1-x;<br>                if(y&lt;n) y = 2*n-1-y;<br>                if(x&lt;y) {<br>                    int z = x;<br>                    x =y;<br>                    y = z;<br>                }<br>                x-=n;<br>                y-=n;<br>                s += snowflake[x].charAt(y);<br>            }<br>            res[i] = s;<br>        }<br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>