<td class="problemText" colspan="8" valign="middle" align="left">
            public class FarFromPrimes<br>    {<br>        int IsPrime(int k)<br>        {<br>            int i;<br> <br>            if (k == 1) return 0;<br>            if (k % 2 == 0) return 0;<br>            for (i = 3; i * i &lt;= k; i += 2)<br>                if (k % i == 0) return 0;<br>            return 1;<br>        }<br> <br>        int IsFar(int k)<br>        {<br>            int i;<br> <br>            for (i = k - 10; i &lt;= k + 10; i++)<br>                if (IsPrime(i) != 0) return 0;<br>            return 1;<br>        }<br> <br>        public int count(int A, int B)<br>        {<br>            int i, ret = 0;<br> <br>            for (i = A; i &lt;= B; i++)<br>                if (IsFar(i) != 0) ret++;<br>            return ret;<br>        }<br>    }<br></td>