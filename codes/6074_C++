            #include &lt;iostream&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;vector&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;string&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;sstream&gt;<br>#include &lt;functional&gt;<br>#include &lt;utility&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br><br>using namespace std; <br> <br> class FarFromPrimes { <br> public: <br>    int isprime( int a ) {<br>        if( a &lt; 2 ) return 0;<br>        if( a == 2 ) return 1;<br>        if( a % 2 == 0 ) return 0;<br><br>        for( int i = 3; i * i &lt;= a; i += 2 )<br>            if( a % i == 0 )<br>                return 0;<br>        return 1;<br>    }<br>    int ok( int a ) {<br>        for( int i = a-10; i &lt;= a+10; ++i )<br>            if( isprime( i ) )<br>                return 0;<br>        return 1;<br>    }<br>    int count( int A, int B ) { <br>    int c = 0;<br>     for( int i = A; i &lt;= B; ++i )<br>        c += ok( i );<br>        return c;<br>   } <br>     <br>   <br>  <br> }; <br> <br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>