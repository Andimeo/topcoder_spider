            public class FarFromPrimes<br>{<br>  public boolean isPrime(int k)<br>  {<br>    if(k&lt;=1) return false;<br>    if(k==2) return true;<br>    if(k%2==0) return false;<br>    for(int i=3;i&lt;=Math.sqrt(k)+1;i=i+2)<br>      if(k%i==0) return false;<br>    return true;<br>  }<br>  public boolean isFarPrime(int k)<br>  {<br>    for(int i=k-10;i&lt;=k+10;i++)<br>      if(this.isPrime(i)) return false;<br>    return true;<br>  }<br>  public int count(int A, int B)<br>  {<br>    int result=0;<br>    for(int i=A;i&lt;=B;i++)<br>      if(this.isFarPrime(i)) result++;<br>    return result;<br>  }<br>  public static void main(String[] args)<br>  {<br>    FarFromPrimes f=new FarFromPrimes();<br>    System.out.println(f.count(97001,97691));  <br>  }<br>}<br></td>