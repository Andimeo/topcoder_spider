            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br> <br>public class BigFibonacci<br>{<br>    public int fibNumber(string index, int M)<br>    {<br>        if (M == 1)<br>        {<br>            return 0;<br>        }<br>        long idx = long.Parse(index);<br>        if (idx == 0)<br>        {<br>            return 0;<br>        }<br>        if (idx == 1)<br>        {<br>            return 1;<br>        }<br>        ArrayList list = new ArrayList(1000000);<br>        list.Add(0);<br>        list.Add(1);<br>        int a = 0;<br>        int b = 1;<br>        int temp;<br>        while (true)<br>        {<br>            temp = (a + b) % M;<br>            a = b;<br>            b = temp;<br>            if (b == 1 &amp;&amp; a == 0)<br>            {<br>                break;<br>            }<br>            list.Add(b);<br>        }<br> <br>        int count = list.Count - 1;<br>        idx %= count;<br>        return (int)list[(int) idx];<br>    }<br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>