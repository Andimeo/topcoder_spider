            import java.util.*;<br>import java.util.regex.*;<br>public class BootsExchange {<br>  public int leastAmount(int[] left, int[] right) {<br>    Arrays.sort(left);<br>    Arrays.sort(right);<br>    int c = 0;<br>    <br>    for(int x = 0; x &lt; left.length; x++){<br>      for(int y = 0; y &lt; right.length; y++){<br>        if(left[x] == right[y]){<br>          left[x] = right[y] = -1;<br>          c++;<br>        }  <br>        <br>      }<br>    }<br>    return left.length - c;<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>