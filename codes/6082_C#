<td class="problemText" colspan="8" valign="middle" align="left">
            public class DollSets<br>  {<br>    /// &lt;summary&gt;<br>    /// The main entry point for the application.<br>    /// &lt;/summary&gt;<br>    public DollSets()<br>    {<br>    }<br> <br>    public int maximumQuantity(int[] dollSizes, int K)<br>    {<br>      int total = 0;<br>      System.Array.Sort( dollSizes);<br>      bool[] isIncluded = new bool[ dollSizes.Length ];<br>      for(int i = 0; i &lt; dollSizes.Length ; i ++)<br>        isIncluded[i] = false;<br>      for(int i = 0; i &lt; dollSizes.Length ; i ++)<br>      {<br>        if(isIncluded[i] == true)<br>          continue;<br>        int doll = dollSizes[i];<br>        int pair = doll * K;<br>        for(int j =  i + 1; j &lt; dollSizes.Length &amp;&amp; dollSizes[j] &lt;= pair ; j ++)<br>        {<br>          if(dollSizes[j] == pair &amp;&amp; isIncluded[j] == false)<br>          {<br>            isIncluded[i] = true;<br>            isIncluded[j] =  true;<br>            total ++;<br>            break;<br>          }<br>        }<br>      }<br>      return total;<br>    }<br>    }<br></td>