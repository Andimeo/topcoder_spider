            #include &lt;iostream&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#define FOR(i,n) for(int i=0;i&lt;n;i++)<br>#define REP(i,f,t) for(int i=f;i&lt;=t;i++)<br>#define debln(a) cout&lt;&lt; #a &lt;&lt;" : " &lt;&lt; a &lt;&lt; endl;<br>using namespace std;<br> <br>class DollSets {<br>  public:<br>  int maximumQuantity(vector &lt;int&gt; d, int K) {<br>    sort(d.begin(),d.end());<br>    vector&lt;bool&gt; used(d.size());<br>    int res = 0;<br>    FOR(i,d.size()) {<br>      for(int j=i+1;j&lt;d.size();j++) {<br>        if(d[j]==K*d[i] &amp;&amp; !used[j] &amp;&amp; !used[i]) {<br>          used[i] = used[j] = 1;<br>          cout &lt;&lt; d[j] &lt;&lt; " " &lt;&lt; d[i] &lt;&lt; endl;<br>          res++;<br>        }<br>      }<br>    }<br>    return res;<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>