            public class EratosthenSieve2 <br>{<br>  public int nthElement(int n) <br>  {<br>    int[] a = new int[1001];<br>    for (int j = 2; j &lt;= 10; j++) <br>    {<br>      int k = 0;<br>      for (int i = 1; i &lt; 1001; i++) <br>      {<br>        if (a[i] == 0)<br>          k++;<br>        if (k % j == 0)<br>          a[i] = 1;<br>      }<br>    }<br> <br>    int l = 0;<br>    for (int i = 1; i &lt; 1001; i++) <br>    {<br>      if (a[i] == 0)<br>        l++;<br>      if (l == n)<br>        return i;<br>    }<br>      <br>    return 0;<br>  }<br> <br> <br>}<br></td>