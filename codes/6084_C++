            #include &lt;iostream&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;set&gt;<br>#define FOR(i,n) for(int i=0;i&lt;n;i++)<br>#define REP(i,f,t) for(int i=f;i&lt;=t;i++)<br>#define debln(a) cout&lt;&lt; #a &lt;&lt;" : " &lt;&lt; a &lt;&lt; endl;<br>using namespace std;<br> <br>class EratosthenSieve2 {<br>  public:<br>  int nthElement(int n) {<br>    set&lt;int&gt; s;<br>    FOR(i,1000) s.insert(i+1);<br>    for(int i=2;i&lt;=10;i++) {<br>      int cnt = 0;<br>      vector&lt;int&gt; v;<br>      for(set&lt;int&gt;::iterator it=s.begin(); it!=s.end(); ++it) {<br>        cnt++;<br>        if(cnt==i) {<br>          v.push_back((*it));<br>          cnt=0;<br>        }<br>      }<br>      FOR(j,v.size()) s.erase(v[j]);<br>    }<br>    int ans = 0;<br>    for(set&lt;int&gt;::iterator it=s.begin(); it!=s.end(); ++it) {<br>      ans++;<br>      if(ans==n) return (*it);<br>    }<br>    return 0;<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>