            using System;<br><br>public class NewAlbum {<br>  public int leastAmountOfCDs(int nSongs, int length, int cdCapacity) {<br>    int num = (cdCapacity + 1) / (length + 1);<br>    if (num % 13 == 0)<br>      --num;<br>    int cnt = (nSongs + num - 1) / num;<br>    int rem = nSongs - num * (cnt - 1);<br>    if (rem % 13 == 0)<br>      if (cnt == 1 || rem == num - 1)<br>        ++cnt;<br>    return cnt;<br>  }<br><br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>