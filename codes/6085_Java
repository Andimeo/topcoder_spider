            import java.util.*;<br><br>public class NewAlbum {<br><br>  public int leastAmountOfCDs(int nSongs, int length, int cdCapacity) {<br>    int i, j, k;<br>    int[] d = new int[nSongs + 1];<br>    Arrays.fill(d, 100100100);<br>    d[0] = 0;<br>    for(i = 0; i &lt; nSongs; i++)<br>      for(j = 1; j * (length + 1) - 1 &lt;= cdCapacity &amp;&amp; i + j &lt;= nSongs; j++)<br>        if(j % 13 != 0)  d[i + j] = Math.min(d[i + j], d[i] + 1);<br>    return d[nSongs];<br>  }<br><br>  void p(Object o) {<br>    System.out.println(o);<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>