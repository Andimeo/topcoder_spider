            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class VowelLatin<br>{<br>  public string translate(string word)<br>  {<br>    string v = "", w = "";<br>    foreach (char ch in word) if ("AEIOUaeiou".IndexOf(ch) == -1) w += ch; else v += ch;<br>    return w + v;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>