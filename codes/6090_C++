            #include &lt;algorithm&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cassert&gt;<br>#include &lt;cctype&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;string&gt;<br>#include &lt;stack&gt;<br>#include &lt;vector&gt;<br>#include &lt;set&gt;<br>#include &lt;queue&gt;<br>#include &lt;map&gt;<br>#include &lt;bitset&gt;<br>using namespace std;<br><br>#define FORE(it,c)    for(__typeof((c).begin()) it=(c).begin();it!=(c).end();++it)<br>inline bool IS_VOWEL( char ch) {<br>  ch = tolower( ch);<br>  return ( ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u');<br>}<br><br>class VowelLatin<br>{<br>public:  <br>  string translate(string word) {<br>    string ret, ret2;<br>    FORE( i, word) if( IS_VOWEL( *i))  ret2 += *i; else ret += *i;<br>    return ret + ret2;<br>  }<br>  <br><br>}; <br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>