            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class SeparateConnections<br>{<br>  private boolean not_set(int cur, int i) {<br>    if ((cur &amp; (1&lt;&lt;i)) == 0)<br>      return true;<br>    else<br>      return false;<br>  }<br> <br>  public int howMany(String[] mat)<br>  {<br>    int now, jbfs, n = mat.length, max = 0;<br>    int[] bit = new int[300000];<br>    int[] move = new int[300000];<br>    Arrays.fill(move, 0);<br>    now = 0;<br>    jbfs = 0;<br>    for (int i = 0; i &lt; n; i++)<br>      for (int j = 0; j &lt; n; j++)<br>        if (mat[i].charAt(j) == 'Y') {<br>          int next = 0 | (1&lt;&lt;i);<br>          next = next | (1&lt;&lt;j);<br>          if (move[next] == 0) {<br>            bit[jbfs++] = next;<br>            move[next] = 2;<br>            if (move[next] &gt; max) max = move[next];<br>          }<br>        }<br>    while (now &lt; jbfs) {<br>      int cur = bit[now];<br>      for (int i = 0; i &lt; n; i++)<br>        if (not_set(cur, i))<br>          for (int j = 0; j &lt; n; j++) <br>            if (mat[i].charAt(j) == 'Y' &amp;&amp; not_set(cur, j)) {<br>              int next = cur | (1&lt;&lt;i);<br>              next = next | (1&lt;&lt;j);<br>              if (move[next] == 0) {<br>                bit[jbfs++] = next;<br>                move[next] = move[cur]+2;<br>                if (move[next] &gt; max) max = move[next];<br>              }<br>            }<br>      now++;<br>    }<br> <br>    return max;<br>  }<br>}<br> <br>//Powered by [KawigiEdit]<br></td>