            using System;<br><br>public class JimmyLightning {<br>  const int MAX_TIME = 1010;<br><br>  public int robTheBank(int[] doors, string[] diamonds) {<br>    int n = doors.Length;<br>    for (int i = 1; i &lt; n; ++i)<br>      doors[i] = Math.Min(doors[i - 1], doors[i]);<br>    int m = diamonds.Length;<br>    int[] value = new int[m];<br>    int[] theftTime = new int[m];<br>    int[] roomNumber = new int[m];<br>    for (int i = 0; i &lt; m; ++i)<br>    {<br>      string[] p = diamonds[i].Split(' ');<br>      value[i] = int.Parse(p[0]);<br>      theftTime[i] = int.Parse(p[1]);<br>      roomNumber[i] = int.Parse(p[2]);<br>    }<br><br>    int[] maxValue = new int[MAX_TIME];<br>    for (int i = 1; i &lt; MAX_TIME; ++i)<br>    {<br>      maxValue[i] = maxValue[i - 1];<br>      for (int j = 0; j &lt; m; ++j)<br>        if (doors[roomNumber[j] - 1] &gt; i &amp;&amp; theftTime[j] &lt;= i) <br>          maxValue[i] = Math.Max(maxValue[i], maxValue[i - theftTime[j]] + value[j]);<br>    }<br><br>    return maxValue[MAX_TIME - 1];<br>}<br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>