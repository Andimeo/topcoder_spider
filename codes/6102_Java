            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class JimmyLightning<br>{<br>  public int robTheBank(int[] doors, String[] diamonds)<br>  {<br>    int[] canget = new int[doors[0]];<br>    int[][] vtr = splitInts(diamonds);<br>    for (int i=1; i &lt; doors.length; i++)<br>      doors[i] = Math.min(doors[i], doors[i-1]);<br>    int max = 0;<br>    for (int i=0; i&lt;canget.length; i++)<br>    {<br>      max = Math.max(max, canget[i]);<br>      for (int j=0; j&lt;diamonds.length; j++)<br>      {<br>        if (doors[vtr[j][2]-1] &gt; i+vtr[j][1] &amp;&amp; i+vtr[j][1] &lt; canget.length)<br>        {<br>          canget[i+vtr[j][1]] = Math.max(canget[i+vtr[j][1]], canget[i] + vtr[j][0]);<br>        }<br>      }<br>    }<br>    return max;<br>  }<br>  <br>  int[][] splitInts(String[] arr)<br>  {<br>    int[][] ret = new int[arr.length][];<br>    for (int i=0; i&lt;ret.length; i++)<br>    {<br>      String[] split = arr[i].split(" ");<br>      ret[i] = new int[split.length];<br>      for (int j=0; j&lt;split.length; j++)<br>        ret[i][j] = Integer.parseInt(split[j]);<br>    }  return ret;<br>  }<br>    <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>