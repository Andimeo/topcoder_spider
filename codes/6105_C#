            using System;<br>using System.Collections;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class SlowKeyboard<br>{<br>  public int enterTime(string time)<br>  {<br>        string[] s = time.Split(':');<br>        int mm = int.Parse(s[0]);<br>        int ss = int.Parse(s[1]);<br>        int now = mm * 60 + ss;<br>        for (int i = 4; i &lt; 200; i++)<br>        {<br>            int f = now + i;<br>            string sa = makeString(f);<br>            int t = getTime(sa);<br>            if (t &lt;= i) return i;<br>        }<br>        return -1;<br>  }<br> <br>    private int getTime(string s)<br>    {<br>        int ans = 1;<br>        for (int i = 1; i &lt; s.Length; i++)<br>        {<br>            if (s[i] != s[i - 1]) ans += 2;<br>            ans++;<br>        }<br>        return ans;<br>    }<br> <br>    private string makeString(int f)<br>    {<br>        f = f % 3600;<br>        return Convert.ToString(f / 600) + Convert.ToString((f / 60) % 10) +<br>            Convert.ToString((f % 60) / 10) + Convert.ToString(f % 10);<br>    }<br>}<br> <br>//Powered by [Zigger]<br></td>