            public class SlowKeyboard {<br>  public int enterTime(String time){<br>    int ch=Integer.parseInt(time.split(":")[0]);<br>    int cm=Integer.parseInt(time.split(":")[1]);<br>    <br>    int h,m;<br>    for(int i=4;i&lt;11;i++){<br>      h=ch;<br>      m=cm;<br>      h+=(cm+i)/60;<br>      h%=60;<br>      m=(cm+i)%60;<br>      int t=4;<br>      if(h/10!=h%10)<br>        t+=2;<br>      if(h%10!=m/10)<br>        t+=2;<br>      if(m/10!=m%10)<br>        t+=2;<br>      if(t&lt;=i)<br>        return i;<br>    }<br>    return 10;<br>  }<br>}<br></td>