            import java.util.*;<br>public class PScores {<br>  public int howMany(int[] nexts, int score) {<br>    int N = nexts.length;<br>    int cnt = 0;<br>    boolean seen[] = new boolean[N];<br>    for(int startp = 0; startp &lt; N; startp++) {<br>      Arrays.fill(seen, false);<br>      int curr = startp;<br>      seen[curr] = true;<br>      while(!seen[nexts[curr]]) {<br>        curr = nexts[curr];<br>        seen[curr] = true;<br>      }<br>      if (curr == score) cnt++;<br>      <br>      <br>    }<br>    return cnt;<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>