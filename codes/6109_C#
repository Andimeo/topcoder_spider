            using System;<br>using System.Collections;<br>using System.Text;<br> <br>public class  ThreeCardMonte<br>{<br>    static void Main(string[] args)<br>    {<br> <br>        Console.WriteLine(position("REL"));<br>    }<br> <br>    public static String position(String swaps)<br>    {<br>        int n=0;<br>        for (int i = 0; i &lt; swaps.Length; i++)<br>        {<br>            if (swaps[i] == 'L')<br>            {<br>                if (n &lt;= 0)<br>                {<br>                    n--;<br>                    n %= 2;<br>                }<br>            }<br>            else if (swaps[i] == 'R')<br>            {<br>                if (n &gt;= 0)<br>                {<br>                    n++;<br>                    n %= 2;<br>                }<br>            }<br>            else if (swaps[i] == 'E')<br>            {<br>                n *= -1;<br>            }<br>        }<br>        if (n== -1)<br>            return "L";<br>        else if (n == 0)<br>            return "M";<br>        else<br>            return "R";<br>    }<br>}<br></td>