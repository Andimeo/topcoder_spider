            using System;<br>using System.Collections;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class Aquarium<br>{<br>  public int peaceful(int minSize, int maxSize, int[] fishSizes)<br>  {<br>    Array.Sort(fishSizes, 0, fishSizes.Length);<br> <br>    int ret = 0;<br>    for (int i = minSize; i&lt;= maxSize; i++)<br>    {<br>      bool danger = false;<br>      foreach(int j in fishSizes)<br>      {<br>        if ((i &gt;= j*2 &amp;&amp; i &lt;= j*10) || (j &gt;= i*2 &amp;&amp; j &lt;= i*10))<br>        {<br>          danger = true;<br>          break;<br>        }<br>      }<br> <br>      if (!danger) ret++;<br>    }<br> <br>    return ret;<br>  }<br>}<br> <br>//Powered by [KawigiEdit]<br></td>