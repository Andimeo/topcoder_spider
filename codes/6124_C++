            #include &lt;algorithm&gt;<br>#include &lt;bitset&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;deque&gt;<br>#include &lt;functional&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;iostream&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;numeric&gt;<br>#include &lt;set&gt;<br>#include &lt;sstream&gt;<br>#include &lt;stack&gt;<br>#include &lt;utility&gt;<br>#include &lt;vector&gt;<br>#include &lt;ctime&gt;<br>using namespace std;<br>#define ABS(x) (x&gt;0?x:-x)<br>#define FR(a,l,h) for(a=l; a&lt;=h; a++)<br>#define GMAX(x,y) (x&gt;y?x:y)<br>#define GMIN(x,y) (x&lt;y?x:Y)<br>#define INF 2000000000<br>#define RP(a,h) for(a=0; a&lt;h; a++)<br>#define SZ(a) (int)a.size()<br>typedef vector &lt;int&gt; VI;<br>typedef vector &lt;string&gt; VS;<br><br>bool eat(int v1, int v2)<br>{<br>  return (v1&gt;=v2*2 &amp;&amp; v1&lt;=v2*10);<br>}<br><br>class Aquarium<br>{<br>public:<br>int peaceful(int minSize, int maxSize, vector &lt;int&gt; fish)<br>{<br>  int i, j, ans=0;<br>  FR(i, minSize, maxSize)<br>  {<br>    RP(j, SZ(fish))<br>      if (eat(i, fish[j]) || eat(fish[j], i))<br>        break;<br>    if (j==SZ(fish))<br>      ans++;<br>  }<br>  return ans;<br>}<br>};<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>