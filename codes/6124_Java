            public class Aquarium<br>{<br>  public boolean isSafe(int p,int k)<br>  {<br>    if( p&lt;=10*k &amp;&amp; p&gt;=2*k ) return false;<br>    if( k&gt;=2*p &amp;&amp; k&lt;=10*p ) return false;<br>    return true;<br>  }<br>  public boolean isSafeAll(int p,int[]fishSizes)<br>  {<br>    for(int i=0;i&lt;fishSizes.length;i++)<br>      if(!this.isSafe(p,fishSizes[i])) return false;<br>    return true;<br>  }<br>  public int peaceful(int minSize, int maxSize, int[] fishSizes)<br>  {<br>    int result=0;<br>    for(int k=minSize;k&lt;=maxSize;k++)<br>      if(this.isSafeAll(k,fishSizes)) result++;<br>    return result;<br>  }<br>}<br></td>