            #include &lt;map&gt; <br>#include &lt;set&gt; <br>#include &lt;cmath&gt;<br>#include &lt;vector&gt; <br>#include &lt;string&gt; <br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;algorithm&gt; <br>using namespace std; <br>#define FOR(i,a,b) for(int i=(a); i&lt;(b); ++i) <br>#define FORE(it,x) for(typeof(x.begin()) it=x.begin(); it!=x.end(); ++it) <br>#define SET(x, v) memset(x, v, sizeof (x)) <br>#define sz size() <br>#define cs c_str() <br>#define pb push_back <br>#define mp make_pair<br> <br>typedef long long i64; <br><br>class      ApproximateDivision         { public: <br><br>  double quotient(int a, int b, int terms) {<br>    double ret = 0.0;<br>    i64 t = 1;<br>    for(;t&lt;b;t&lt;&lt;=1);<br>    printf("%d %d %lld\n",a,b,t);<br>    // 1/b -&gt; <br>    i64 c = t - b;<br>    double T= t, C = 1;<br>    while(terms--) {<br>      ret += C / T;<br>      C*= (double)c;<br>      T*= (double)t;<br>    }<br>    ret*= (double) a;<br>    return ret;<br>  }<br><br>};<br></td>