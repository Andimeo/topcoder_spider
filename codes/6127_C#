            using System;<br><br>public class LastStone {<br>  public int numWins(int[] turns, int m, int n) {<br>    bool[] wins = new bool[n + 200];<br>    for (int i = 0; i &lt;= n; ++i)<br>      if (!wins[i])<br>      {<br>        foreach (int turn in turns)<br>          wins[i + turn] = true;<br>      }<br><br>    int res = 0;<br>    for (int i = m; i &lt;= n; ++i)<br>      if (wins[i])<br>        ++res;<br><br>    return res;<br>  }<br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>