<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>using System.Collections;<br>public class RandomizedQuickSort {<br>  public double getExpectedTime(int listSize, int S, int a, int b) {<br>    double[] exp = new double[listSize + 1];<br>    exp[0] = 0.0;<br>    for (int i = 1; i &lt;= listSize; ++i) {<br>      if (i &lt;= S) exp[i] = (double) b*i*i;<br>      else {<br>        exp[i] = (double) a*i;<br>        for (int k = 1; k &lt;= i; ++k) {<br>          exp[i] += exp[k - 1]/(double) i  + (exp[i - k])/ (double) i;<br>        }<br>      }<br>    }<br>    return exp[listSize];<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>