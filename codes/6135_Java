<td class="problemText" colspan="8" valign="middle" align="left">
            public class RandomizedQuickSort {<br>  public double getExpectedTime( int listSize, int S, int a, int b ) {<br>    double time[] = new double[ listSize + 1 ];<br>    for( int i=0; i &lt;= S; ++i ) time[i] = b * i * i;<br>    for( int i=S+1; i &lt;= listSize; ++i ) {<br>      double sum = 0.0;<br>      for( int j=0; j &lt; i; ++j ) {<br>        sum += ( time[ j ] + time[ i -j - 1 ] +  a * i );<br>      }<br>      time[i] = sum / i;<br>    }<br>    return time[ listSize ];<br>  }<br>}<br></td>