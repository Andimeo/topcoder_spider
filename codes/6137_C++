            #include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;utility&gt;<br>#include &lt;queue&gt;<br>#include &lt;map&gt;<br><br>using namespace std;<br><br>class TheXGame{<br>public:<br>  int min(int a,int b){<br>    return a&lt;b?a:b;<br>  }<br><br>  int n,m,a[100],k,r,s;<br>  int firstMove(string board){<br>    n=board.size();<br>    m=0;<br>    int i,j;<br>    for(i=0;i&lt;n;i++){<br>      if (board[i]=='-') m++;<br>    }<br>    if (m&lt;=2) return -1;<br>    m=0;<br>    for(i=0;i&lt;n;){<br>      if (board[i]=='-'){<br>        for(j=i+1;j&lt;n;j++) if (board[j]!='-') break;<br>        a[m++]=j-i;<br>        i=j;<br>      }else{<br>        i++;<br>      }<br>    }<br>    k=0;<br>    int best=100;<br>    for(i=0;i&lt;m;i++) k^=a[i];<br>    for(i=0;i&lt;m;i++){<br>      j=k^a[i];<br>      for(r=0;r&lt;a[i];r++){<br>        for(s=0;s+r&lt;a[i];s++){<br>          if (!(j^s^r)){<br>            best=min(best,a[i]-s-r);<br>          }<br>        }<br>      }<br>    }<br>    if (best==100) best=-1;<br>    return best;<br>  }<br>  <br><br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>