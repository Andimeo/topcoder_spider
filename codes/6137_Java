            public class TheXGame {<br>  <br>    //double[][] probmet = new double[51][2600];<br>    //double[][] probzon = new double[51][2600];<br>  <br>  public boolean win(String board){<br>    int posi = 0;<br>    int n = 0;<br>    int[] many = new int[10];<br>    int pow=1;<br>    for (int i=0;i&lt;10; i++)many[i]=0;<br>    while (posi&lt;board.length()){<br>      if (board.charAt(posi)=='-') n++;<br>      if (board.charAt(posi)=='X'){<br>        int i=0;<br>        while (n!=0){<br>          if (n%2==1){<br>            many[i]++;<br>          }<br>          n/=2;<br>          i++;<br>        }<br>        n=0;<br>      }<br>      posi++;<br>    }<br>        int i=0;<br>        while (n!=0){<br>          if (n%2==1){<br>            many[i]++;<br>          }<br>          n/=2;<br>          i++;<br>        }<br>    for (int i2=0;i2&lt;10;i2++){<br>      if (many[i2]%2==1) return true;<br>    }<br>    return false;<br>  }<br>  public int firstMove(String board){<br>    int posi = 0;<br>    int n = 0;<br>    int nm = 100;<br>    int pow=1;<br>    String tmp="";<br>    while (posi&lt;board.length()){<br>      if (board.charAt(posi)=='-') {<br>        int posi2=posi;<br>        n=0;<br>        tmp="";<br>        while ((posi2&lt;board.length())&amp;&amp;(board.charAt(posi2)=='-')){<br>          tmp = tmp+'X';<br>          n++;<br>          String board2=board.substring(0,posi)+tmp+board.substring(posi2+1);<br>          //System.out.println(board2);<br>          if (board2.indexOf("-")!=-1){<br>          if ((n&lt;nm)&amp;&amp;(!win(board2))){<br>          nm=n;<br>          //System.out.println("win:"+board2);<br>          }}<br>          posi2++;<br>        }<br>      }<br>      posi++;<br>    }<br>    if (nm==100) nm=-1;<br>    return nm;<br>  }<br>}<br></td>