<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>using System.Collections;<br>public class PackingParts {<br>  public int pack(int[] partSizes, int[] boxSizes) {<br>    int i = 0;<br>    int j = 0;<br>    int count = 0;<br>    while(i &lt; partSizes.Length &amp;&amp; j &lt; boxSizes.Length) {<br>      if (partSizes[i]&lt;= boxSizes[j]) {<br>        ++count;<br>        ++i;<br>        ++j;<br>      }<br>      else {<br>        ++j;  <br>      }<br>    }<br>    return count;<br>    <br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>