            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class PackingParts<br>{<br>  public int pack(int[] partSizes, int[] boxSizes)<br>  {<br>    int ret = 0;<br>    int index = 0;<br>    for(int i = 0;i&lt;partSizes.length;i++)<br>    {<br>      for(;index&lt;boxSizes.length;index++)<br>      {<br>        if(boxSizes[index]&gt;=partSizes[i])<br>          {<br>          ret++;<br>          index++;<br>          break;<br>          }<br>      }<br>    }<br>    return ret;<br>  }<br>  }<br></td>