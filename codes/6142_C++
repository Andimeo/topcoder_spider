            #ifdef  WIN32<br>#pragma warning(disable:4786)<br>#define ct cout<br>typedef __int64 int64;<br>#else<br>#define ct if (0) cout<br>typedef long long int64;<br>#endif<br>#include &lt;cctype&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;queue&gt;<br>#include &lt;stdlib.h&gt;<br>#include &lt;stack&gt;<br>#include &lt;stdio.h&gt;<br>#include &lt;math.h&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;algorithm&gt;<br>using namespace std;<br>class MonotoneSEMin {<br>  public:<br>  double min(vector &lt;string&gt; bits) {<br>    vector&lt;int&gt; res;<br>    string bit;<br>    double rres;<br>    int i;<br>    int n;<br>    vector&lt;double&gt; avr;<br>    bit="";<br>    for (i=0;i&lt;bits.size();i++)<br>      bit+=bits[i];<br>    n=bit.size();<br>    res.resize(n);<br>    avr.resize(n);<br>    for (i=0;i&lt;n;i++)<br>      res[i]=bit[i]-'0';<br>    rres=0;<br>    i=0;<br>    while (i&lt;n)<br>    {<br>      int j;<br>      double sum;<br>      double min;<br>      int imin;<br>      sum=0;<br>      for (j=i;j&lt;n;j++)<br>      {<br>        sum+=res[j];<br>        avr[j]=sum/(j-i+1);<br>      }<br>      min=1e+100;imin=i;<br>      for (j=i;j&lt;n;j++)<br>      {<br>        if (min&gt;avr[j])<br>        {<br>          imin=j;<br>          min=avr[j];<br>        }<br>      }<br>      int a,b;<br>      a=0;b=0;<br>      for (j=i;j&lt;=imin;j++)<br>      {<br>        b+=res[j];<br>        a+=1-res[j];<br>      }<br>      rres+=a*b*1.0/(a+b);<br>      i=imin+1;<br>    }<br>    return rres;<br>  }<br>  <br> <br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.1 [28-Sep-2005]<br>// Powered by CodeProcessor<br></td>