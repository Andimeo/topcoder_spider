            public class SkewSymmetric<br>{<br>    public int minChanges(string[] M)<br>    {<br>        int n = M.Length;<br>        int count = 0;<br>        int[,] mat = new int[n,n];<br>        for(int i=0; i&lt;n; i++) {<br>            string[] strArray = M[i].Split(' ');<br>            for(int j=0; j&lt;strArray.Length; j++) {<br>                mat[i,j] = int.Parse(strArray[j]);<br>            }<br>        }<br><br><br>        for (int i = 0; i &lt; n; i++)<br>        {<br>            if (mat[i,i] != 0)<br>            {<br>                count++;<br>            }<br>            for (int j = i + 1; j &lt; n; j++)<br>            {<br>                if (mat[i,j] + mat[j,i] != 0)<br>                {<br>                    count++;<br>                }<br><br>            }<br>        }<br>        return count;<br>    }<br>}<br></td>