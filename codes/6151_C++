            #include &lt;sstream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cmath&gt;<br>using namespace std;<br><br>class SkewSymmetric{<br>public:<br>  int minChanges(vector&lt;string&gt; M){<br>    int n = M.size(),i,j,a[25][25],ans=0;<br>    for(i=0;i&lt;n;i++)<br>    {<br>      istringstream sin(M[i]);<br>      for(j=0;j&lt;n;j++)<br>        sin&gt;&gt;a[i][j];<br>    }<br>    for(i=0;i&lt;n;i++)<br>    {<br>      if(a[i][i]!=0)<br>        a[i][i]=0,ans++;<br>      for(j=i+1;j&lt;n;j++)<br>      {<br>        if(a[i][j]!=-1*a[j][i])<br>          ans++;<br>      }<br>    }<br>    return ans;<br>  }<br>};<br></td>