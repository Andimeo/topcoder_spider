            public class SkewSymmetric {<br>  public int minChanges(String[] M){<br>    int m[][]=new int[M.length][M.length];<br>    for(int i=0;i&lt;M.length;i++){<br>      String t[]=M[i].split(" ");<br>      for(int j=0;j&lt;M.length;j++){<br>        m[i][j]=Integer.parseInt(t[j]);<br>      }<br>    }<br>    <br>    int res=0;<br>    for(int i=0;i&lt;M.length;i++){<br>      for(int j=0;j&lt;M.length;j++){<br>        if(i!=j&amp;&amp;m[i][j]!=-m[j][i])<br>          res++;<br>      }<br>    }<br>    <br>    res=res/2;<br>    for(int i=0;i&lt;m.length;i++)<br>      if(m[i][i]!=0)<br>        res++;<br>    return res;<br>  }<br>}<br></td>