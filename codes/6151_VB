            Public Class SkewSymmetric<br><br>  Public Function minChanges(M As String()) As Integer<br><br>    Dim K(M.Length, M(0).Split(" ").Length) As Integer<br>    Dim x As Integer<br>    For x = 0 to M.Length-1<br>      Dim y as Integer<br>      Dim temp() as String = M(x).Split(" ")<br>      For y = 0 To temp.Length-1<br>        K(x,y) = CInt(temp(y))<br>        Console.WriteLine(x &amp; "," &amp; y &amp; ":" &amp; K(x,Y))<br>      Next y<br>    Next x<br>    <br>    Dim myChanges As Integer = 0<br>    For x = 0 To M.Length-1<br>      Dim y as Integer<br>      For y = x To M.Length-1<br>        If y=x Then<br>          If K(x,y) &lt;&gt; 0 Then myChanges+=1<br>        Else<br>          If K(x,y) &lt;&gt; -(K(y,x)) Then myChanges+=1<br>        End If<br>      Next y<br>    Next x<br>    Return myChanges<br>  <br>  End Function<br><br>End Class<br></td>