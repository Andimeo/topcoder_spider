            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class CyclesInPermutations<br>{<br>  public int maxCycle(int[] board)<br>  {<br>    int best = 0;<br>    for(int i = 0; i &lt; board.Length; i++){<br>      int cur = i;<br>      int tot = 0;<br>      do{<br>        cur = board[cur] - 1;<br>        tot++;<br>      } while(cur != i);<br>      best = Math.Max(tot,best);<br>    }<br>    return best;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>