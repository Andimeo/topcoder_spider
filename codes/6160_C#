            using System;<br>using System.Collections;<br><br>public class FibonacciPositioning {<br>    public double getFPosition(int n) {<br>        double res;<br>    double[] f = new double[1000];<br>    f[1] = f[2] = 1;<br>    for(int i=3; i&lt;f.Length; i++)<br>    {<br>      f[i] = f[i-1] + f[i-2];<br>      if(f[i] &gt;= n) break;<br>    }<br>    for(int i=2; i&lt;f.Length; i++)<br>    {<br>      if(Math.Abs(f[i]-n) &lt; 1e-10) return i;<br>      if(f[i]&lt;n &amp;&amp; f[i+1] &gt; n)<br>      {<br>        res = i + 1.0*(n-f[i])/(f[i+1]-f[i]);<br>        return res;<br>      }<br>    }<br>        return -1.0;<br>    }<br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>