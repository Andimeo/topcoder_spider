            public class MixedUpPrimes {<br>  int low, high;<br>  public int[] findPrimes(int[] X) {<br>    low = Integer.MAX_VALUE;<br>    high = Integer.MIN_VALUE;<br>    go(X);<br>    if (low == Integer.MAX_VALUE || high == Integer.MIN_VALUE) return new int[0];<br>    return new int[]{low,high};<br>  }<br>  public int go(int[] X) {<br>    if (X.length==1 &amp;&amp; isPrime(X[0])) {<br>      low = Math.min(low,X[0]);<br>      high = Math.max(high,X[0]);<br>      return 0;<br>    }<br>    int[] X2 = new int[X.length-1];<br>    int C = -1;<br>    for (int a=0;a&lt;X.length-1;a++) {<br>      for (int b=a+1;b&lt;X.length;b++) {<br>        C = 0;<br>        for (int c=0;c&lt;X.length;c++) {<br>          if (c!=a &amp;&amp; c!=b) {<br>            X2[C]=X[c];<br>            C++;<br>          }<br>        }<br>        X2[C] = X[a]+X[b];<br>        go(X2);<br>        X2[C] = X[a]-X[b];<br>        go(X2);<br>        X2[C] = X[b]-X[a];<br>        go(X2);<br>        X2[C] = X[b]*X[a];<br>        go(X2);<br>        if (X[b] != 0) {<br>          X2[C] = X[a]/X[b];<br>          go(X2);<br>        }<br>        if (X[a] != 0) {<br>          X2[C] = X[b]/X[a];<br>          go(X2);<br>        }<br>      }<br>    }<br>    return 0;<br>  }<br>  public boolean isPrime(int N) {<br>    if (N&lt;2) return false; if (N==2) return true;<br>    if (N%2==0) return false;<br>    for (int a=3;a*a&lt;=N;a+=2) {<br>      if (N%a==0) return false;<br>    }<br>    return true;<br>  }<br>}<br></td>