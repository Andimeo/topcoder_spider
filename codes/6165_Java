            import java.util.*;<br>import java.io.*;<br> <br>public class TrafficMonitor {<br>  PrintStream ut;<br>  int n;<br>  <br>  BitSet[] map;<br> <br>  public int getMin(String[] links) {<br>    ut = System.out;<br>    int ans = 0;<br>    n = links.length;<br>    map = new BitSet[n];<br>    for (int i=0; i&lt;n; ++i) {<br>      map[i] = new BitSet();<br>      for (int j=0; j&lt;n; ++j) {<br>        if (links[i].charAt(j)=='Y') map[i].set(j);<br>      }<br>    }<br>    <br>    while (true) {<br>      int ind = -1;<br>      for (int i=0; i&lt;n; ++i) {<br>        if (map[i].cardinality()==1) {<br>          ind = i;<br>        }<br>      }<br>      if (ind==-1) break;<br>      ++ans;<br>      int a = map[ind].nextSetBit(0);<br>      map[ind].clear();<br>      BitSet bs = map[a];<br>      for(int i=bs.nextSetBit(0); i&gt;=0; i=bs.nextSetBit(i+1)) {<br>        map[i].clear(a);<br>      }<br>      bs.clear();<br>    }<br>    return ans;<br>  }<br> <br>  void print(Object... a) {<br>    for (Object o : a) {<br>      ut.print(o+" ");<br>    }<br>    ut.println();<br>  }<br>  void print(long... a) {<br>    for (long o : a) {<br>      ut.print(o+" ");<br>    }<br>    ut.println();<br>  }<br>  void deep(Object[] o) {<br>    ut.println("deep");<br>    ut.println(Arrays.deepToString(o).replace("], [","],\n["));<br>  }<br>}<br> <br> <br>// Powered by PopsEdit<br></td>