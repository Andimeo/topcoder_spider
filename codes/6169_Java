            import java.util.*;<br>import java.util.regex.*;<br>import java.io.*;<br><br>public class OptimalQueues {<br>  public int minWaitingTime(int[] clientArrivals, int tellerCount, int serviceTime) {<br>    ut = System.out;<br>    Arrays.sort(clientArrivals);<br>    int ind = clientArrivals.length-1;<br>    int ans = 0;<br>    int time = 0;<br>    outer:<br>    while (true) {<br>      time += serviceTime;<br>      for (int i=0; i&lt;tellerCount; ++i) {<br>        ans = Math.max(ans,time+clientArrivals[ind]);<br>        --ind;<br>        if (ind&lt;0) break outer;<br>      }<br>    }<br>    return ans;<br>  }<br><br>  PrintStream ut;<br>  void print(Object... a) {<br>    for (Object o:a) {<br>      ut.print(o+" ");<br>    }<br>    ut.println();<br>  }<br><br>  <br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>