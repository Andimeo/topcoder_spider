            using System;<br><br>public class DivisorInc {<br>  const int INF = 1000000;<br>  <br>  public int countOperations(int N, int M) {<br>    int[] best = new int[M + 1];<br>    best[N] = 0;<br>    for (int i = N + 1; i &lt;= M; ++i)<br>      best[i] = INF;<br>    for (int i = N; i &lt; M; ++i)<br>    {<br>      for (int j = 2; j * j &lt;= i; ++j)<br>        if (i % j == 0)<br>        {<br>          int k = i + j;<br>          if (k &lt;= M)<br>            best[k] = Math.Min(best[k], best[i] + 1);<br>          k = i + i / j;<br>          if (k &lt;= M)<br>            best[k] = Math.Min(best[k], best[i] + 1);<br>        }<br>    }<br>    return best[M] &gt;= INF ? -1 : best[M];<br>  }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>