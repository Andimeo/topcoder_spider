            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import static java.lang.Math.*;<br>import static java.util.Arrays.*;<br>public class DivisorInc {<br>  public int countOperations(int N, int M) {<br>    if(N &gt; M) return -1;<br>    int[] dp = new int[M+1];<br>    fill(dp, Integer.MAX_VALUE);<br>    dp[N] = 0;<br>    for(int i = N; i &lt;= M; i++){<br>      if(dp[i] == Integer.MAX_VALUE)continue;<br>      for(int j = 2; j*j &lt;= i; j++){<br>        if(i%j != 0) continue;<br>        if(i+j &gt; M)continue;<br>        dp[i+j] = min(dp[i+j], dp[i]+1);<br>        if(i+(i/j) &gt; M) continue;<br>        dp[i+(i/j)] = min(dp[i+(i/j)], dp[i]+1);<br>      }<br>    }<br>    return dp[M] == Integer.MAX_VALUE ? -1 : dp[M];<br>  }<br> <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>