<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br><br>public class ProductsMatrix{<br>public long nthElement(int n, int kk){<br>long k=kk;<br>int i;<br>long x=1,y=(long)n*(long)n;<br>long cur,res,z;<br>while (y-x&gt;1) {<br>  cur=(x+y)/2;<br>  res=0;<br>  for (i=1;i&lt;=n;i++){<br>    z=(cur-1)/i;<br>    if (z&gt;n) {z=n;}<br>    res+=z;<br>    }<br>  if (res&lt;k) {x=cur;} else {y=cur;}<br>  }<br>res=0;<br>x--;<br>while (res&lt;k){<br>  x++;<br>  res=0;<br>  for (i=1;i&lt;=n;i++){<br>    z=(x-1)/i;<br>    if (z&gt;n) {z=n;}<br>    res+=z;<br>    }<br>  }<br>res=x-1;<br>return res;<br>}<br>}<br></td>