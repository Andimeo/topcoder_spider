            // Cry as challange phase is passing by<br><br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br><br>#include &lt;cmath&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br><br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br><br>using namespace std;<br><br>#define FORC(it,v) for( __typeof((v).begin()) it = (v).begin(); it != (v).end(); ++it )<br><br>long long n; <br><br>long long bs_cmpf( long long x )<br>{<br>    int in = n;<br>    long long sum = 0;<br><br>    for( int i = 0; i &lt; in; ++i )<br>        sum += min&lt; long long &gt;( in, ( x - 1 ) / ( i + 1 ) );<br><br>    return sum;<br>}<br><br>class ProductsMatrix {<br>public:<br>    long long nthElement( int n, int k ) {<br>        ::n = n;<br>        long long l = 1, r = 10000000000ll;<br><br>        while( l &lt; r ) {<br>            long long mid = ( l + r + 1 ) / 2;<br><br>            if( bs_cmpf( mid ) &gt;= k )<br>                r = mid - 1;<br>            else<br>                l = mid;<br>        }<br><br>        return l;<br>    }<br>};<br><br><br><br><br><br><br><br><br><br>// Powered by FileEdit<br>// Powered by moj 3.0 beta 11 [modified TZTester]<br>// Powered by CodeProcessor<br></td>