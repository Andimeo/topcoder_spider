            import java.util.*;<br> <br>public class FactCount {<br>    public void trcl(boolean[][] r) {<br>  for (int i = 0; i &lt; 26; i++) {<br>      for (int j = 0; j &lt; 26; j++) {<br>    for (int k = 0; k &lt; 26; k++) {<br>        if (r[j][i] &amp;&amp; r[i][k]) {<br>      r[j][k] = true;<br>        }<br>    }<br>      }<br>  }<br>    }<br> <br>    public int minFacts(String[] known) {<br>        boolean[][] r = new boolean[26][26];<br>  for (int i = 0; i &lt; known.length; i++) {<br>      StringTokenizer st = new StringTokenizer(known[i]);<br>      while (st.hasMoreTokens()) {<br>    String s = st.nextToken();<br>    r[s.charAt(0) - 'A'][s.charAt(1) - 'A'] = true;<br>      }<br>  }<br> <br> <br>  trcl(r);<br> <br>  int[] col = new int[26];<br>  int[] csz = new int[27];<br>  int cc = 0;<br>  for (int i = 0; i &lt; 26; i++) {<br>      if (col[i] == 0) {<br>    cc++;<br>    col[i] = cc;<br>    csz[cc] = 1;<br>    for (int j = 0; j &lt; 26; j++) {<br>        if (r[i][j] &amp;&amp; r[j][i]) {<br>      col[j] = cc;<br>      if (i != j) {<br>          csz[cc]++;<br>      }<br>        }<br>    }<br>      }<br>  }<br> <br>  boolean[][] t = new boolean[26][26];<br>  for (int k = 0; k &lt; 26; k++) {<br>      for (int l = 0; l &lt; 26; l++) {<br>    t[k][l] = r[k][l];<br>      }<br>  }<br> <br>  int ans = 0;<br>  for (int i = 0; i &lt; 26; i++) {<br>      for (int j = 0; j &lt; 26; j++) {<br>    if (col[i] != col[j]) {<br>    r[i][j] = false;<br> <br>    boolean[][] p = new boolean[26][26];<br>    for (int k = 0; k &lt; 26; k++) {<br>        for (int l = 0; l &lt; 26; l++) {<br>      p[k][l] = r[k][l];<br>        }<br>    }<br> <br>    trcl(p);<br>    boolean ok = true;<br>    for (int k = 0; k &lt; 26; k++) {<br>        for (int l = 0; l &lt; 26; l++) {<br>      if (t[k][l] != p[k][l]) {<br>          ok = false;<br>      }<br>        }<br>    }<br>    if (!ok) {<br>        r[i][j] = true;<br>    }<br>    if (r[i][j]) {<br>        ans++;<br>        System.out.println("" + ((char)('A' + i)) + ((char)('A' + j)));<br>    }<br>    }<br>      }<br>  }<br> <br>  for (int i = 1; i &lt;= cc; i++) {<br>      if (csz[i] != 1) {<br>    ans += csz[i];<br>      } else {<br>    for (int j = 0; j &lt; 26; j++) {<br>        if (col[j] == i) {<br>      if (r[j][j]) {<br>          ans++;<br>      }<br>        }<br>    }<br>      }<br>  }<br> <br>  return ans;<br>    }<br>}<br> <br> <br> <br>// Powered by PopsEdit<br></td>