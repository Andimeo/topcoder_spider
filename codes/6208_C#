            using System;<br>using System.Collections.Generic;<br>using System.Text;<br><br><br>public class MislabeledWeights<br>{<br>    public int fewest(int[] weights, int testWeight)<br>    {<br>        int len = weights.Length;<br>        for (int n = 1; n &lt;= len; n++)<br>        {<br>            int[] select = new int[n];<br>            for (int i = 0; i &lt; n; i++) select[i] = -1;<br>            int step = 0;<br><br>            while (step &gt; -1)<br>            {<br>                select[step]++;<br>                if (select[step] == len - n + step+1)<br>                {<br>                    step--;<br>                }<br>                else if (step == n-1)<br>                {<br>                    int sum = 0;<br>                    for (int tt = 0; tt &lt; n; tt++)<br>                    {<br>                        sum += weights[select[tt]];<br>                    }<br>                    if (sum &lt;= testWeight + n &amp;&amp; sum &gt;= testWeight - n)<br>                    {<br>                        return n;<br>                    }<br>                }<br>                else<br>                {<br>                    step++;<br>                    select[step] = select[step - 1];<br>                }<br>            }<br>        }<br>        return -1;<br>    }<br>}<br></td>