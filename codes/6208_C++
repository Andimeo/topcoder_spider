            #include&lt;iostream&gt;<br>#include&lt;vector&gt;<br>#include&lt;string&gt;<br>#include&lt;map&gt;<br>#include&lt;set&gt;<br>#include&lt;algorithm&gt;<br>#include&lt;sstream&gt;<br>#include&lt;cstdio&gt;<br>#include&lt;cmath&gt;<br>#include&lt;cstdlib&gt;<br>#include&lt;ctime&gt;<br>using namespace std;<br> <br>template&lt;typename T&gt; inline int sz(const T&amp; c) { return (int)c.size(); }<br> <br>struct MislabeledWeights<br>{<br>  int fewest(vector &lt;int&gt; w, int test)<br>  {<br>    int n = sz(w),ret=2147483647;<br>    for(int i=0;i&lt;(1&lt;&lt;n);i++)<br>    {<br>      int sum=0,cnt=0;<br>      for(int j=0;j&lt;n;j++)<br>        if(i&amp;(1&lt;&lt;j))<br>          sum+=w[j],cnt++;<br>      if(sum-test&gt;=-cnt&amp;&amp;sum-test&lt;=cnt)<br>      {<br>        if(cnt&lt;ret)<br>          ret=cnt;<br>      }<br>    }<br>    if(ret==2147483647) return -1;<br>    return ret;<br>  }<br>};<br></td>