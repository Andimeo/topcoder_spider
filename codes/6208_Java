            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class MislabeledWeights<br>{<br>  public int fewest(int[] a, int s)<br>  {<br>    int n = a.length;<br>    int ans = 100;<br>    int i,j,k, jj;<br>    for(int step=(1&lt;&lt;n)-1;step&gt;0;step--)<br>    {<br>      k = j = 0;<br>      jj = 0;<br>      for(i=0;i&lt;n;i++)if((step&amp;(1&lt;&lt;i))&gt;0) { <br>        k += a[i]; j++;<br>        if(a[i]&gt;1) jj++;<br>      }<br>      if((k-jj)&lt;=s &amp;&amp; s&lt;=(k+j) &amp;&amp; j&lt;ans) ans = j;<br>    }<br>    if(ans==100)ans=-1;<br>    return ans;<br>  }<br>  <br>  public static void main(String[] args)<br>  {}<br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>