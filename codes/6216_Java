            import java.util.*;<br>import java.util.regex.*;<br><br>public class CyclicGame {<br><br>  static final double EPSILON = 1e-13;<br><br>  public double estimateBank(int[] cells) {<br>    int n = cells.length;<br>    double[] a = new double[n];<br>    for (;;) {<br>      boolean updated = false;<br>      for (int i = 0; i &lt; n; ++i) {<br>        double sum = 0;<br>        for (int j = 1; j &lt;= 6; ++j)<br>          sum += cells[(i + j) % n] + a[(i + j) % n];<br>        sum /= 6;<br>        if (sum &gt; a[i] + EPSILON) {<br>          a[i] = sum;<br>          updated = true;<br>        }<br>      }<br>      if (!updated)<br>        break;<br>    }<br>    return a[0];<br>  }<br>}<br></td>