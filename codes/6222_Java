            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import static java.lang.Math.*;<br>import static java.util.Arrays.*;<br>public class EscapingJail {<br>  public int getMaxDistance(String[] chain) {<br>    int[][] g = new int[chain.length][chain.length];<br>    for(int i = 0; i &lt; g.length; i++){<br>      for(int j = 0; j &lt; g.length; j++){<br>        g[i][j] = toi(chain[i].charAt(j));<br>      }<br>    }<br>    for(int k = 0; k &lt; g.length; k++){<br>      for(int i = 0; i &lt; g.length; i++){<br>        for(int j = 0; j &lt; g.length; j++){<br>          g[i][j] = min(g[i][j], g[i][k]+g[k][j]);<br>        }<br>      }<br>    }<br>    int ret = -1;<br>    for(int i = 0; i &lt; g.length; i++){<br>      for(int j = 0; j &lt; g.length; j++){<br>        ret = max(ret, g[i][j]);<br>      }<br>    }<br>    return ret == Integer.MAX_VALUE/2 ? -1 : ret;<br>  }<br>  int toi(char c){<br>    if(c &lt;= '9' &amp;&amp; c &gt;= '0') return c-'0';<br>    if(c &lt;= 'z' &amp;&amp; c &gt;= 'a') return c-'a'+10;<br>    if(c &lt;= 'Z' &amp;&amp; c &gt;= 'A') return c-'A'+36;<br>    return Integer.MAX_VALUE/2;<br>  }<br> <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>