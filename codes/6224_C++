            #include &lt;stdio.h&gt;<br>#include &lt;stdlib.h&gt;<br>#include &lt;string.h&gt;<br>#include &lt;math.h&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;stack&gt;<br>#include &lt;queue&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;iterator&gt;<br>#include &lt;iostream&gt;<br>#include &lt;functional&gt;<br>#include &lt;sstream&gt;<br>#include &lt;numeric&gt;<br><br>#define CLR( x , y ) ( memset( (x) , (y) , sizeof( (x) ) ) )<br>#define EPS 1e-9<br><br>using namespace std;<br><br>class InsertionSortCount<br>{<br>  public:<br>  int countMoves(vector &lt;int&gt; A)<br>  {<br>    int i,j,k,x=0,y=0,z=0;<br>    int ret=0;<br>    for(i=0;i&lt;A.size();i++){<br>      for(j=i+1;j&lt;A.size();j++){<br>        if(A[i]&gt;A[j])ret++;<br>      }<br>    }<br>    <br>    <br>    return ret;<br>  }<br>    <br><br>  <br><br>};<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>