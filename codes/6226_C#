            using System;<br>using System.Collections.Generic;<br>using System.Text;<br>using System.IO;<br>using System.Collections;<br><br>class OrderParser<br>{<br>    public int isnum(String str)<br>    {<br>        int n = 0;<br>        for (int i = 0; i &lt; str.Length; i++)<br>        {<br>            if (!(str[i] &gt;= '0' &amp;&amp; str[i] &lt;= '9'))<br>            {<br>                return -1;<br>            }<br>            n *= 10;<br>            n += str[i] - '0';<br>        }<br>        return n;<br>    }<br>    public int[] orderAmount(string[] items, string[] message)<br>    {<br>        int [] ans = new int[items.Length];<br>        for (int i = 0; i &lt; message.Length; i++)<br>        {<br>            String[] words = message[i].Split(new char[] { ' ' });<br>            int cnt = 0;<br>            for(int j=0;j&lt;words.Length;j++)<br>            {<br>                if (isnum(words[j]) != -1)<br>                {<br>                    cnt = isnum(words[j]);<br>                }<br>                else<br>                {<br>                    if (cnt &gt; 0)<br>                    {<br>                        for (int k = 0; k &lt; items.Length; k++)<br>                        {<br>                            if (items[k] == words[j])<br>                            {<br>                                ans[k] += cnt;<br>                                cnt = 0;<br>                                break;<br>                            }<br>                        }<br>                    }<br>                }<br>            }<br>        }<br>        return ans;<br>    }<br>}<br></td>