            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br><br>public class OrderParser<br>{<br>  public int[] orderAmount(String[] items, String[] mes)<br>  {<br>    HashMap&lt;String, Integer&gt; dict = new HashMap&lt;String, Integer&gt;();<br>    for(String s : items) dict.put(s, 0);<br>    for(String str : mes)<br>    {<br>      String[] s = str.split(" ");<br>      int c = 0;<br>      for(int i = 0; i &lt; s.length; ++i)<br>      {<br>        if(s[i].matches("[0-9]+"))<br>        {<br>          c = Integer.parseInt(s[i]);<br>        }<br>        else if(dict.containsKey(s[i]))<br>        {<br>          dict.put(s[i], dict.get(s[i]) + c);<br>          c = 0;<br>        }<br>      }<br>    }<br>    <br>    int[] ret = new int[items.length];<br>    for(int i = 0; i &lt; items.length; ++i)<br>      ret[i] = dict.get(items[i]);<br>    return ret;<br>  }<br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br><br><br>// Powered by FileEdit<br></td>