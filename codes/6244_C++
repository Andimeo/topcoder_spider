            #include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;set&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>using namespace std;<br> <br>#define V(x) vector&lt;x &gt;<br>#define vi V(int)<br>#define vs V(string)<br>#define vd V(double)<br> <br> <br>#define fr(i,a,b) for((i)=(a);(i)&lt;(b);(i)++)<br>#define fo(i,b) fr(i,0,b)<br>#define fir(n) fo(i,n)<br>#define fjr(n) fo(j,n)<br>#define fkr(n) fo(k,n)<br>#define flr(n) fo(l,n)<br>#define fi fir(n)<br>#define fj fjr(n)<br>#define fk fkr(n)<br> <br>#define pb(x) push_back(x)<br>#define sz size()<br>#define cs c_str()<br> <br>#define df double<br>#define ll long long<br>#define ins(x) insert(x)<br>#define srt(x) sort(x.begin(),x.end())<br> <br> <br> <br>class ModelRailroad { public: ll countTracks(signed N) {<br>ll ret=0;<br> <br>ret+=(N/3)+1;<br> <br>for(ll A=1;2*A&lt;=N;A++) {<br>for(ll BC=0;2*BC+2*A&lt;=N &amp;&amp; BC&lt;=2*(A-1);BC++) {<br>ll mB=0 &gt;? BC-(A-1);<br>ll xB=BC &lt;? (A-1);<br>if(mB&gt;xB) continue;<br>ll nB=xB-mB+1;<br>ll S=N-2*A-2*BC;<br>S/=3;<br>S++;<br>//printf("A=%lld BC=%lld B=[%lld %lld] nB=%lld S=%lld\n",A,BC,mB,xB,nB,S);<br>ret+=nB*S;<br>}<br>}<br> <br>for(ll A=1;2*A&lt;=N;A++) {<br>ll B=A;<br>for(ll C=0;C&lt;=A;C++) if(2*A+2*B+2*C&lt;=N) {<br>ll S=N-2*A-2*B-2*C;<br>S/=3;<br>S++;<br>ret+=S;<br>//printf("A=%lld B=%lld C=%lld S=%lld\n",A,B,C,S);<br>}<br>}<br> <br>return ret;<br>}};<br></td>