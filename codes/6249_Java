            import java.util.*;<br>public class Alignment {<br>    Math M; System S; //duh<br>  public int align(String A, String B, int x) {<br>      int c[][] = new int[A.length()+1][B.length()+1];<br>      for(int b=1;b&lt;=B.length();b++) c[0][b] = x + b;<br>      for(int a=1;a&lt;=A.length();a++) c[a][0] = x + a;<br>      for(int b=1;b&lt;=B.length();b++) {<br>    for(int a=1;a&lt;=A.length();a++) {<br>        int w = -1&gt;&gt;&gt;1;<br>        if(A.charAt(a-1) == B.charAt(b-1)) {<br>      w = c[a-1][b-1];<br>        } else {<br>      for(int s=b;s--&gt;0;) {<br>          w = M.min(w, c[a][s] + (b-s) + x);<br>      }<br>      for(int s=a;s--&gt;0;) {<br>          w = M.min(w, c[s][b] + (a-s) + x);<br>      }<br>        }<br>        c[a][b] = w;<br>    }<br>      }<br>      //      S.out.println(Arrays.deepToString(c).replaceAll("],","\n"));<br>      return c[A.length()][B.length()];<br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>