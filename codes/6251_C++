            //#line 82 "Segments.cpp"<br><br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;queue&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br><br>using namespace std;<br><br>class Segments {<br>  public:<br>  string intersection(vector &lt;int&gt; s1, vector &lt;int&gt; s2) {<br><br>    if (s1[0]&gt;s1[2]) swap(s1[0],s1[2]);<br>    if (s1[1]&gt;s1[3]) swap(s1[1],s1[3]);<br>    if (s2[0]&gt;s2[2]) swap(s2[0],s2[2]);<br>    if (s2[1]&gt;s2[3]) swap(s2[1],s2[3]);<br>    int x1,y1,x2,y2;<br>    x1=(s1[0]&gt;s2[0])?s1[0]:s2[0];<br>    y1=(s1[1]&gt;s2[1])?s1[1]:s2[1];<br>    x2=(s1[2]&lt;s2[2])?s1[2]:s2[2];<br>    y2=(s1[3]&lt;s2[3])?s1[3]:s2[3];<br> <br>    if (x1&gt;x2 || y1&gt;y2) return "NO";<br>    if (x1==x2 &amp;&amp; y1==y2) return "POINT";<br>    return "SEGMENT";<br>  }<br><br><br>  <br><br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>