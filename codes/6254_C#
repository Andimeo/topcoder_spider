            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text; <br><br>public class Hotel<br>{<br>    int[] cache;<br>    int[] a, c;<br>    int n;<br>    int calc(int v)<br>    {<br>        if (v &lt;= 0) return 0;<br>        if (cache[v] &gt; 0) return cache[v] - 1;<br>        int res = 100000000;<br>        int i;<br>        for (i = 0; i &lt; n; i++) res = Math.Min(res, c[i] + calc(v - a[i]));<br>        cache[v] = res + 1;<br>        return res;<br>    }<br>    public int marketCost(int minCustomers, int[] customers, int[] cost)<br>    {<br>        cache = new int[2000];<br>        a = customers;<br>        c = cost;<br>        n = a.Length;<br>        return calc(minCustomers);<br>    }<br><br>    // helper<br>    void cout(object o) { System.Console.WriteLine(o); } <br><br>};<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>