            #include &lt;algorithm&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>using namespace std;<br>#define FORC( V, it ) for( __typeof( (V).begin() ) it = (V).begin(); it != (V).end(); ++it )<br>const int MAXL = 1000;<br>const int MAXN = 50;<br>const int MAXX = 10000;<br>const int INF = 987654321;<br>const double EPS = 1e-9;<br>char buff[MAXL+1];<br><br><br>vector&lt; int &gt; A;<br>vector&lt; int &gt; B;<br><br>int dp[MAXX];<br><br>int rek( int jos ) <br>{<br>    if ( jos &lt; 0 ) return 0;<br>    int &amp;ref = dp[jos];<br>    if ( ref != -1 ) return ref;<br>    if ( jos == 0 ) return ( ref = 0 );<br><br>    ref = INF;<br>    for( int i = 0; i &lt; ( int )A.size(); ++i ) <br>        ref = min( ref, rek( jos - A[i] ) + B[i] );<br>    return ref;<br>        <br>}<br><br>class Hotel {<br>public:<br>    int marketCost( int minCustomers, vector &lt;int&gt; customers, vector &lt;int&gt; cost ) {<br>        A = customers;<br>        B = cost;<br>        memset( dp, -1, sizeof ( dp ) ); <br>        return rek( minCustomers );<br>    }<br>};<br><br><br><br><br><br><br><br><br><br>// Powered by FileEdit<br>// Powered by moj 3.0 beta 11 [modified TZTester]<br>// Powered by CodeProcessor<br></td>