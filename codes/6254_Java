            /**<br> @author Jack Lindamood<br>*/<br>public class Hotel{<br>  public int marketCost(int minCustomers, int[] customers, int[] cost) {<br>        int[] minCost=new int[20000];<br>        minCost[0]=0;<br>        for (int i=1;i&lt;minCost.length;i++){<br>            int thisMin=minCost[i-1]+cost[0];<br>            for (int j=0;j&lt;cost.length;j++){<br>                int prev=i-customers[j];<br>                if (prev&lt;0)<br>                    prev=0;<br>                thisMin=Math.min(thisMin,minCost[prev]+cost[j]);<br>            }<br>            minCost[i]=thisMin;<br>        }<br>        int min2=minCost[minCustomers];<br>        for (int i=minCustomers;i&lt;minCost.length;i++){<br>            min2=Math.min(min2,minCost[i]);<br>        }<br>        return min2;<br>    }<br>    }<br></td>