            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class FussySleeper <br>{<br>    public int[] nextTime(int[] ct, int el) <br>    {<br>    do<br>    {<br>      ct[1] = (ct[1] + 1) % 60;<br>      if ( ct[1] == 0 )<br>        ct[0] = (ct[0] + 1) % 24;<br>    } while ( int.Parse(ct[0].ToString("D2") + ct[1].ToString("D2")) % el != 0 );<br>    return ct;<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>