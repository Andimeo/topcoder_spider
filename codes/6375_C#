<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>using System.Collections;<br> <br>public class NounReform<br>{<br>  public string[] makePlural(string[] nouns)<br>  {<br>    string [] ret = new string[ nouns.Length ];<br>    for( int i = 0 ; i &lt; ret.Length ; ++i )<br>      ret[i] = tonoun(nouns[i]);<br>    return ret;<br>  }<br>  string tonoun(string s)<br>  {<br>    string [] x = new string[]{ "s", "z", "x", "ch" ,"sh" };<br>    foreach( string k in x )<br>      if( s.EndsWith(k) )<br>        return s + "es";<br> <br>    string [] y = {"ay", "ey", "iy", "oy" , "uy" };<br>    foreach( string k in y )<br>      if( s.EndsWith(k) )<br>        return s + "s";<br> <br>    if( s.EndsWith("y"))<br>      return s.Substring(0,s.Length-1) + "ies";<br> <br>    return s + "s";<br>  }<br>}<br></td>