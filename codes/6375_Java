            import java.util.*;<br>import java.util.regex.*;<br>public class NounReform {<br>  public String[] makePlural(String[] n) {<br>    for(int x = 0; x &lt; n.length; x++){<br>      if(n[x].endsWith("s") || n[x].endsWith("z") || n[x].endsWith("x") || n[x].endsWith("ch") || n[x].endsWith("sh")){<br>        n[x] += "es";<br>      }<br>      else if(n[x].endsWith("ay") || n[x].endsWith("ey") || n[x].endsWith("iy") || n[x].endsWith("oy") || n[x].endsWith("uy")){<br>        n[x] += "s";<br>      }<br>      else if(n[x].endsWith("y")){<br>        n[x] = n[x].substring(0, n[x].length() - 1) + "ies";<br>      }<br>      else{<br>        n[x] += "s";<br>      }<br>    }<br>    <br>    return n;<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>