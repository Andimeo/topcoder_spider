            //be the MAN !!!!<br>//challenge it !!!!<br><br>import java.util.*;<br><br>public class TourCounting {<br><br>    private int n;<br><br>    public int countTours(String[] g, int k, int m) {<br>        n = g.length;<br>        int[][] a = new int[2*n][2*n];<br>        for (int i = 0; i &lt; n; i++) {<br>            for (int j = 0; j &lt; n; j++) {<br>                a[i][j] = g[i].charAt(j) == 'Y' ? 1 : 0;<br>            }<br>        }<br>        for (int i = 0; i &lt; n; i++) {<br>            a[i][i + n] = 1;<br>            a[i + n][i + n] = 1;<br>        }<br><br>        int[][] b = matPow(a, k, m);<br><br>        int res = 0;<br>        for (int i = 0; i &lt; n; i++) {<br>            res += b[i][i + n];<br>            res %= m;<br>            System.out.println("" + i + ": " + b[i][i + n]);<br>        }<br>        res = res - n;<br>        while (res &lt; 0) res += m;<br>        return res;<br>    }<br><br>    private int[][] matPow(int[][] a, int k, int m) {<br>        if (k == 0) {<br>            int[][] b = new int[2*n][2*n];<br>            for (int i = 0; i &lt; 2*n; i++){<br>                b[i][i] = 1;<br>            }<br>            return b;<br>        } else if (k % 2 == 0) {<br>            int[][] b = matPow(a, k / 2, m);<br>            return matMul(b, b, m);<br>        } else {<br>            int[][] b = matPow(a, k - 1, m);<br>            return matMul(b, a, m);<br>        }<br>    }<br><br>    private int[][] matMul(int[][] a, int[][] b, int m) {<br>        int[][] c = new int[2*n][2*n];<br>        for (int i = 0; i &lt; 2*n; i++) {<br>            for (int j = 0; j &lt; 2*n; j++) {<br>                for (int k = 0; k &lt; 2*n; k++) {<br>                    long t = (((long)a[i][k]) * b[k][j] + c[i][j]) % m;<br>                    c[i][j] = (int) t;<br>                }<br>            }<br>        }<br>        return c;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>