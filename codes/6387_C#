            using System.Collections;public class XBallGame<br>{<br>  public string[] newStatistics(string[] placeboard)<br>  {<br>    string[] ret = new string[placeboard.Length];<br>    for(int i=0;i&lt;placeboard.Length; i++)<br>    {<br>      ret[i] = placeboard[i];<br>      ArrayList ar = new ArrayList();<br>      for (int j=0; j&lt;placeboard.Length; j++)<br>      {<br>        if (j!=i)<br>        {<br>          if (placeboard[i].Split("-"[0])[0] == placeboard[j].Split("-"[0])[0])<br>            ar.Add(placeboard[j].Split("-"[0])[1]);<br>        }<br>      }<br>      ar.Sort();<br>      for (int j=0; j&lt;ar.Count; j++)<br>        ret[i] += ","+ar[j] ;<br>    }<br>    return ret;<br>  }<br>}<br></td>