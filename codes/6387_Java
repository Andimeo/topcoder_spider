            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class XBallGame<br>{<br>  public String[] newStatistics(String[] board)<br>  {<br>    String[] result = new String[board.length];<br>    ArrayList&lt;String&gt; list = new ArrayList();<br>    for(int a=0; a&lt;board.length; a++){<br>      result[a] = ""+board[a];<br>      String name = board[a].split("-")[0]+"-";<br>      for(int b=0; b&lt;board.length; b++){<br>        if(b!=a){<br>          if(board[b].startsWith(name)){<br>            list.add(board[b].split("-")[1]);<br>          }<br>        }<br>      }<br>      Collections.sort(list);<br>      for(int b=0; b&lt;list.size(); b++){<br>        result[a] += ","+list.get(b);<br>      }<br>      list = new ArrayList();<br>    }<br>    return result;<br>  }<br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>