            using System;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class ExtraordinarilyLarge<br>{<br>    int fac(int x)<br>    {<br>        int res = 1;<br>        for (int i = 1; i &lt;=    x; i++)<br>        {<br>            res *= i;<br>        }<br>        return res;<br>    }<br>    public string compare(string x, string y)<br>    {<br>        int xx = 0; int yy = 0;<br>        while (x[x.Length - 1] == '!') { x = x.Substring(0, x.Length - 1); xx++; }<br>        while (y[y.Length - 1] == '!') { y = y.Substring(0, y.Length - 1); yy++; }<br>        int X = Convert.ToInt32(x);<br>        int Y = Convert.ToInt32(y);<br>        while (X &lt;= 12 &amp;&amp; xx &gt; 0) { X = fac(X); xx--; }<br>        while (Y &lt;= 12 &amp;&amp; yy &gt; 0) { Y = fac(Y); yy--; }<br>        if (xx == yy)<br>        {<br>            if (X &lt; Y) return "x&lt;y";<br>            if (X == Y) return "x=y";<br>            return "x&gt;y";<br>        }<br>        if (xx &lt; yy)<br>        {<br>            return "x&lt;y";<br>        }<br>        return "x&gt;y";<br><br>    }<br>}<br></td>