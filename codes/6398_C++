            #include &lt;cstdio&gt;<br>#include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br><br>using namespace std;<br><br><br>typedef long long llint;<br><br>struct number {<br>   llint a;<br>   int k;<br>   number( string s ) {<br>      k = 0;<br>      sscanf( s.c_str(), "%lld", &amp;a );<br>      for( int i = 0; i &lt; s.size(); ++i )<br>         if( s[i] == '!' )<br>            ++k;<br><br>      while( k &amp;&amp; a &lt;= 15 ) {<br>         llint b = 1;<br>         for( int i = 2; i &lt;= a; ++i ) b *= i;<br>         a = b;<br>         --k;<br>      }<br>   }<br>};<br>bool operator &lt; ( const number &amp;A, const number &amp;B ) {<br>   if( A.k != B.k ) return A.k &lt; B.k;<br>   if( A.a != B.a ) return A.a &lt; B.a;<br>   return 0;<br>}<br><br>struct ExtraordinarilyLarge {<br>   string compare( string x, string y ) {<br>      number X( x ), Y( y );<br><br>      if( X &lt; Y ) return "x&lt;y";<br>      if( Y &lt; X ) return "x&gt;y";<br>      return "x=y";<br>   }<br><br><br><br><br><br><br>   <br> <br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by moj 3.0 beta 7 [modified TZTester]<br>// Powered by CodeProcessor<br></td>