            import java.util.*;<br> <br>public class ExtraordinarilyLarge {<br>    public String compare(String x, String y) {<br>        int xi = x.length() - 1;<br>        while (x.charAt(xi) == '!') {<br>            xi--;<br>        }<br>        int xn = Integer.parseInt(x.substring(0, xi + 1));<br>        xi = x.length() - xi - 1;<br>        int yi = y.length() - 1;<br>        while (y.charAt(yi) == '!') {<br>            yi--;<br>        }<br>        int yn = Integer.parseInt(y.substring(0, yi + 1));<br>        yi = y.length() - yi - 1;<br> <br>        while (xn &lt; 13 &amp;&amp; xi &gt; 0) {<br>            int r = 1;<br>            for (int i = 0; i &lt; xn; i++) {<br>                r = r * (i + 1);<br>            }<br>            xn = r;<br>            xi--;<br>        }<br>        while (yn &lt; 13 &amp;&amp; yi &gt; 0) {<br>            int r = 1;<br>            for (int i = 0; i &lt; yn; i++) {<br>                r = r * (i + 1);<br>            }<br>            yn = r;<br>            yi--;<br>        }<br> <br>        if (xi &lt; yi) {<br>            return "x&lt;y";<br>        } else if (xi &gt; yi) {<br>            return "x&gt;y";<br>        } else {<br>            if (xn &lt; yn) {<br>                return "x&lt;y";<br>            } else if (xn &gt; yn) {<br>                return "x&gt;y";<br>            } else {<br>                return "x=y";<br> <br>            }<br>        }<br>    }<br> <br> <br>}<br> <br> <br>// Powered by PopsEdit<br> <br>// Powered by CodeProcessor<br></td>