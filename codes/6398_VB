00000000 Then<br>                Return 1000000000<br>            End If<br>        Next<br>        Return res<br>    End Function<br><br>    Public Function factor(ByVal n As Integer) As Int64<br>        If n = 0 Then<br>            Return 1<br><br>        End If<br><br>        Dim i As Integer<br>        Dim res As Int64 = 1<br>        For i = 2 To n<br>            res = res * i<br>            If res &gt;= 1000000000 Then<br>                Return 1000000000<br>            End If<br>        Next<br>        Return res<br>    End Function<br>End Class<br></td>