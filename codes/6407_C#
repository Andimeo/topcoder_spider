            using System;<br><br>public class LightSwitches {<br>  public long countPossibleConfigurations(string[] switches) {<br>    int n = switches.Length;<br>    int m = switches[0].Length;<br>    bool[,] a = new bool[n, m];<br>    for (int i = 0; i &lt; n; ++i)<br>      for (int j = 0; j &lt; m; ++j)<br>        a[i, j] = switches[i][j] == 'Y';<br>    bool[] used = new bool[n];<br>    long res = 1;<br>    for (int i = 0; i &lt; m; ++i)<br>    {<br>      for (int j = 0; j &lt; n; ++j)<br>        if (!used[j] &amp;&amp; a[j, i])<br>        {<br>          used[j] = true;<br>          for (int k = 0; k &lt; n; ++k)<br>            if (a[k, i] &amp;&amp; k != j)<br>              for (int l = 0; l &lt; m; ++l)<br>                a[k, l] ^= a[j, l];<br>          res &lt;&lt;= 1;<br>          break;<br>        }<br>    }<br>    return res;<br>  }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>