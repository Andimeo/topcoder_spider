            import java.util.*;<br> <br>public class LightSwitches {<br>    public long countPossibleConfigurations(String[] switches) {<br>        int n = switches.length;<br>        int m = switches[0].length();<br>        boolean[][] a = new boolean[n][m];<br>        for (int i = 0; i &lt; n; i++) {<br>            for (int j = 0; j &lt; m; j++) {<br>                a[i][j] = switches[i].charAt(j) == 'Y';<br>            }<br>        }<br> <br>        int l = 0;<br>        for (int i = 0; i &lt; m; i++) {<br>            int k = -1;<br>            for (int j = l; j &lt; n; j++) {<br>                if (a[j][i]) {<br>                    k = j;<br>                    break;<br>                }<br>            }<br>            if (k == -1) {<br>                continue;<br>            }<br> <br>            for (int j = 0; j &lt; m; j++) {<br>                boolean t = a[l][j]; a[l][j] = a[k][j]; a[k][j] = t;<br>            }<br> <br>            for (int j = l + 1; j &lt; n; j++) {<br>                if (a[j][i]) {<br>                    for (int p = 0; p &lt; m; p++) {<br>                        a[j][p] = a[j][p] ^ a[l][p];<br>                    }<br>                }<br>            }<br> <br>            l++;<br>        }<br> <br>        return 1L &lt;&lt; l;<br>    }<br> <br> <br>}<br> <br> <br>// Powered by PopsEdit<br> <br>// Powered by CodeProcessor<br></td>