<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>  using System.Collections;<br>  <br>  public class SortMachine<br>  {<br>    public int countMoves(int[] a)<br>    {        <br>        int res = 100;<br>        <br>          for (int j=0;j&lt;a.Length; j++)            <br>        {<br>            bool s = true;<br>            int last = -2000;<br>            int cur = 0;<br>            for (int k=0; k&lt;a.Length; k++)<br>                if (a[k] &gt; a[j]) cur++;<br>            else<br>            {<br>                if (a[k] &lt; last)<br>                {<br>                    s = false;<br>                    break;<br>                }<br>                last = a[k];<br>            }<br>            if (s &amp;&amp; cur &lt; res) res = cur;<br>        }<br>        if (res == 100) res = a.Length-1;<br>      return res;<br>    }<br>  }<br></td>