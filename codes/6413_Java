            import java.util.*;<br>public class SortMachine<br>{<br> public int countMoves(int[] a)<br> {<br>  int ret=0;<br>  boolean[] move = new boolean[a.length];Arrays.fill(move,false);<br>  for(int i=0;i&lt;a.length;i++)<br>  {<br>   boolean b = false;<br>   for(int j=0;j&lt;i;j++)<br>   {<br>    if(move[j]&amp;&amp;a[j]&lt;a[i]){b=true;break;}<br>   }<br>   for(int j=i+1;j&lt;a.length;j++)<br>   {<br>    if(a[j]&lt;a[i]){b=true;break;}<br>   }<br>   if(b)move[i]=true;<br>  }<br>  for(int i=0;i&lt;a.length;i++){if(move[i])ret++;}<br>  return ret;<br> }<br>}<br></td>