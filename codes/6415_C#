            using System;<br>using System.Collections.Generic;<br><br>public class BifidSortMachine {<br>  public int countMoves(int[] a) {<br>    int[] b = (int[]) a.Clone();<br>    Array.Sort(b);<br>    Dictionary&lt;int, int&gt; d = new Dictionary&lt;int, int&gt;();<br>    for (int i = 0; i &lt; b.Length; ++i)<br>      d[b[i]] = i;<br>    for (int i = 0; i &lt; a.Length; ++i)<br>      a[i] = d[a[i]];<br>    int[] mx = new int[a.Length];<br>    int r = 0;<br>    for (int i = 0; i &lt; a.Length; ++i)<br>    {<br>      int m = 0;<br>      for (int j = 0; j &lt; i; ++j)<br>        if (a[j] == a[i] - 1)<br>          m = Math.Max(m, mx[j]);<br>      mx[i] = m + 1;<br>      r = Math.Max(r, mx[i]);<br>    }<br>    return a.Length - r;<br>  }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>