            import java.util.*;<br> <br>public class BifidSortMachine {<br> <br>  <br>  public int countMoves(int[] a)<br>  {<br>    int n = a.length;<br>    int[] t = new int[n];<br>    for(int i = 0; i &lt; n; i++)<br>      t[i] = i;<br>    for(int i = 0; i &lt; n; i++)<br>      for(int j = i+1; j &lt; n; j++)<br>        if (a[i] &gt; a[j])<br>        {<br>          int k = a[i];<br>          a[i] = a[j];<br>          a[j] = k;<br>          k = t[i];<br>          t[i] = t[j];<br>          t[j] = k;<br>        }<br>    int max = 0;<br>    int l = 0;<br>    for(int i = 0; i &lt; n; i++)<br>      if (l &gt; 0 &amp;&amp; t[i-1] &gt; t[i])<br>        l = 1;<br>      else<br>      {<br>        l++;<br>        max = Math.max(max, l);      <br>      }<br>    max = Math.max(max, l);<br>    return n - max;<br>  }<br>  <br>  public static void main(String[] args) {<br>    // TODO Auto-generated method stub<br> <br>  }<br> <br>}<br></td>