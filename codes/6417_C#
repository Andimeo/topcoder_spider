            using System;<br>  using System.Collections;<br>  public class BlockDistance<br>  {<br>      int[,] memo;<br>      string s1, s2;<br>    public int minDist(string[] oldText, string[] newText)<br>    {        <br>        s1 = "";<br>        s2 = "";<br>        for (int i = 0; i&lt;oldText.Length; i++) s1 = s1 + oldText[i];<br>        for (int i = 0; i&lt;newText.Length; i++) s2 = s2 + newText[i];<br>        <br>        if (s1.Length &gt; s2.Length)<br>        {<br>            string tmp = s1;<br>            s1 = s2;<br>            s2 = tmp;<br>        }<br>        memo = new int[s1.Length, s2.Length];<br>        for (int i = 0; i&lt;s1.Length; i++)<br>        for (int j = 0; j&lt;s2.Length; j++)<br>            memo[i,j] = 2000;<br>        <br>          <br>        return GetMin(0,0);<br>    }<br>    <br>    public int GetMin(int p1, int p2)<br>    {<br>        <br>        if (p1 &gt;= s1.Length)<br>            if (p2 &gt;= s2.Length) return 0;<br>            else return 1;<br>        if (p2 &gt;= s2.Length) return -1;<br>            <br>            if (memo[p1,p2] != 2000) return memo[p1,p2];<br>        <br>            int res = 2000;<br>            for (int i = p2; i&lt;s2.Length; i++)<br>            {                <br>                if (s2[i] == s1[p1])<br>                {<br>                    int j = i+1;<br>                    while (j&lt;s2.Length &amp;&amp; j-i+p1&lt;s1.Length &amp;&amp; s2[j] == s1[j-i+p1]) j++;<br>                    int tmp = GetMin(j-i+p1, j);                    <br>                    if (tmp != -1 &amp;&amp; i!=p2) tmp++;<br>                    if (tmp != -1 &amp;&amp; res &gt; tmp) res = tmp;<br>                }<br>            }<br>            if (res == 2000) res = -1;<br>            memo[p1,p2] = res;<br>            return res;                <br>    }<br>  }<br></td>