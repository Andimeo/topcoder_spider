            #include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cmath&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>using namespace std;<br><br>class WinningTrick {<br>public:<br>    double minimumSpeed( vector &lt;int&gt; speed, int yourSpeed ) {<br>        int mx = -1;<br>        for( int i = 0; i &lt; ( int )speed.size(); ++i ) <br>            mx = max( mx, speed[i] );<br>        if ( mx &lt; yourSpeed )<br>            return 0.0;<br>        return ( double( mx - yourSpeed ) / 2 );<br>    <br>    }<br>    <br><br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>