            import java.util.*;<br>public class WinningTrick {<br>    public double minimumSpeed(int[] speed, int yourSpeed) {<br>        double res;<br>        long currtime; <br>        currtime=System.currentTimeMillis();<br>        int val=0;<br>        for(int i:speed)<br>        {<br>          val=Math.max(val,i);<br>          <br>        }<br>        res=(double)(val-yourSpeed)/2;<br>        if(res&lt;0) return 0;<br><br><br>        // time(currtime);<br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>