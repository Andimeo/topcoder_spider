            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class CompositionTimeSignature<br>{<br>  public int parsechar (char n){<br>    if (n=='W')<br>      return 16;<br>    if (n=='H')<br>      return 8;<br>    if (n=='Q')<br>      return 4;<br>    if (n=='E')<br>      return 2;<br>    if (n=='S')<br>      return 1;<br>    return 0;<br>  }<br>  <br>  public int sum(String in){<br>    if (in == null || in == "" || in.length() == 0)<br>      return 0;<br>    return parsechar(in.charAt(0)) + sum(in.substring(1,in.length()));<br>  }<br>  <br>  public int measurecount (String in, int modamt){<br>    int cnt=0;<br>    int meas=0;<br>    int raw=0;<br>    while (in.length() &gt;0){<br>      int i = parsechar(in.charAt(0));<br>      if (i + raw &gt; meas + modamt){<br>        cnt ++;<br>      }<br>      raw+=i;<br>      meas = modamt*(raw/modamt);      <br>      in = in.substring(1,in.length());<br>        <br>    }<br>    return cnt;<br>  }<br>  <br>  public String getTimeSignature(String duration)<br>  {<br>    String [] a = {"3/8","2/4","3/4","4/4"}; <br>    int[] v = {-1,-1,-1,-1}; <br>    int [] c = {6,8,12,16};<br>    int s = sum(duration);<br>    boolean bset = false;<br>    for (int i=0;i&lt;v.length;i++){<br>      if (s % c[i] == 0){<br>        v[i]= measurecount(duration,c[i]);<br>        bset = true;<br>      }<br>    }<br>    <br>    if (!bset)<br>      return "?/?";<br>    <br>    int bestidx=0;<br>    int bestdst=9999999;<br>    for (int i=0;i&lt;v.length;i++){<br>      if (v[i]&lt;bestdst &amp;&amp; v[i]!=-1){<br>        bestdst = v[i];<br>        bestidx = i;      <br>      }<br>    }<br>    return a[bestidx];  <br>  }<br> <br>}<br></td>