            using System;<br><br>public class SumThemAll {<br>  long sumFor(long max)<br>  {<br>    if (max &lt;= 0)<br>      return 0;<br>    long p10 = 1;<br>    int pow = 0;<br>    while (max &gt;= p10)<br>    {<br>      p10 *= 10;<br>      ++pow;<br>    }<br>    p10 /= 10;<br>    --pow;<br>    long d1 = max / p10;<br>    long res = 0;<br>    for (int i = 0; i &lt; d1; ++i)<br>      res += i * p10 + pow * p10 * 45 / 10;<br>    return res + (max - d1 * p10 + 1) * d1 + sumFor(max - d1 * p10);<br>  }<br><br>  public long getSum(int lowerBound, int upperBound)<br>  {<br>    return sumFor(upperBound) - sumFor(lowerBound - 1);<br>  }<br><br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>