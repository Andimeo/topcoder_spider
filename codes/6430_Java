            public class SumThemAll {<br>  public long find(int q) {<br>    if (q&lt;1) return 0;<br>    String s=q+"";<br>    int n=s.length();<br>    long a[][][]=new long[n+1][100][2];<br>    a[0][0][0]=1;<br>    for(int i=0;i&lt;n;i++) {<br>      for(int j=0;j&lt;2;j++) {<br>        for(int z=0;z&lt;=n*9;z++) {<br>          for(int k=0;k&lt;((j==1)?10:(s.charAt(i)-'0'+1));k++) {      <br>            a[i+1][z+k][j|(k&lt;(s.charAt(i)-'0')?1:0)]+=a[i][z][j];<br>          }<br>        }<br>      }<br>    }    <br>    long sum=0;<br>    for(int i=0;i&lt;=n*9;i++)<br>      sum+=(a[n][i][0]+a[n][i][1])*i;<br>    return sum;<br>  }<br>  public long getSum(int l, int u) {<br>    return find(u)-find(l-1);<br>  }<br>}<br></td>