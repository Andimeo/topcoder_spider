            imports System.collections<br>public class SumThemAll<br>Public Function getSum(ByVal lowerBound As Integer, ByVal upperBound As Integer) As Long<br>        ht = New Hashtable<br>        ht(0) = CType(0, Long)<br>        If (lowerBound &gt; 0) Then<br>            Return sumx(upperBound) - sumx(lowerBound - 1)<br>        Else<br>            Return sumx(upperBound)<br>        End If<br><br>    End Function<br><br>    Public Function splitx(ByVal x As Integer) As ArrayList<br>        Dim t As ArrayList<br>        t = New ArrayList<br>        While (x &gt; 0)<br>            t.Add(x Mod 10)<br>            x = Math.Floor(x / 10)<br>        End While<br>        Return t<br>    End Function<br><br>    Public ht As Hashtable<br><br>    Public Function sumx(ByVal x As Integer) As Long<br>        If (ht.ContainsKey(x)) Then<br>            Return CType(ht(x), Long)<br>        End If<br><br>        Dim al As ArrayList<br>        al = splitx(x)<br>        Dim t, t1, t2, i, s As Long<br>        t = 0<br>        If al.Count = 1 Then<br>            t = 0<br>            For i = 1 To CInt(al(0))<br>                t += i<br>            Next<br>            ht(x) = t<br>            Return t<br>        End If<br><br>        t = 0<br>        t1 = CInt(al(al.Count - 1))<br>        t2 = t1 - 1<br>        For i = al.Count - 2 To 0 Step -1<br>            t = t * 10 + CInt(al(i))<br>            t1 = t1 * 10<br>            t2 = t2 * 10<br>        Next<br><br>        If t = 0 Then<br>            s = sumx(t2) + sumx(t1 - t2 - 1) + (t1 - t2) * (CInt(al(al.Count - 1)) - 1) + 1<br>        Else<br>            s = sumx(t1) + sumx(t) + t * CInt(al(al.Count - 1))<br>        End If<br>        ht(x) = s<br>        Return s<br>    End Function<br>    end class<br></td>