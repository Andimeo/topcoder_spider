            using System;<br><br>public class MatrixTransforming {<br>  public int minPushes(string[] a, string[] b) {<br>    int height = a.Length;<br>    int width = a[0].Length;<br>    bool[,] m = new bool[height, width];<br>    bool[,] r = new bool[height, width];<br>    for (int i = 0; i &lt; height; ++i)<br>      for (int j = 0; j &lt; width; ++j)<br>      {<br>        m[i, j] = a[i][j] == '1';<br>        r[i, j] = b[i][j] == '1';<br>      }<br>    int res = 0;<br>    for (int i = 0; i &lt; height - 2; ++i)<br>      for (int j = 0; j &lt; width - 2; ++j)<br>      {<br>        if (m[i, j] != r[i, j])<br>        {<br>          ++res;<br>          for (int ii = 0; ii &lt; 3; ++ii)<br>            for (int jj = 0; jj &lt; 3; ++jj)<br>              m[i + ii, j + jj] = !m[i + ii, j + jj];<br>        }<br>      }<br>    for (int i = 0; i &lt; height; ++i)<br>      for (int j = 0; j &lt; width; ++j)<br>        if (m[i, j] != r[i, j])<br>          return -1;<br>    return res;<br>  }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>