            import java.util.*;<br> <br>public class MatrixTransforming {<br>    public int minPushes(String[] a, String[] b) {<br>        int n = a.length;<br>        int m = a[0].length();<br>        int[][] x = new int[n][m];<br>        int[][] y = new int[n][m];<br>        for (int i = 0; i &lt; n; i++) {<br>            for (int j = 0; j &lt; m; j++) {<br>                x[i][j] = a[i].charAt(j) - '0';<br>                y[i][j] = b[i].charAt(j) - '0';<br>            }<br>        }<br> <br>        int ans = 0;<br>        for (int i = 0; i &lt; n - 2; i++) {<br>            for (int j = 0; j &lt; m - 2; j++) {<br>                if (x[i][j] != y[i][j]) {<br>                    ans++;<br>                    for (int k = 0; k &lt; 3; k++) {<br>                        for (int l = 0; l &lt; 3; l++) {<br>                            x[i + k][j + l] = 1 - x[i + k][j + l];<br>                        }<br>                    }<br>                }<br>            }<br>        }<br> <br>        for (int i = 0; i &lt; n; i++) {<br>            for (int j = 0; j &lt; m; j++) {<br>                if (x[i][j] != y[i][j]) {<br>                    return -1;<br>                }<br>            }<br>        }<br> <br>        return ans;<br>    }<br> <br> <br>}<br> <br> <br>// Powered by PopsEdit<br> <br>// Powered by CodeProcessor<br></td>