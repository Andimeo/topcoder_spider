            using System;<br>using System.Collections.Generic;<br>using System.Text;<br>using System.Collections;<br> <br> <br>    public class ApocalypseSomeday<br>    {    <br>        public static int getNth(int n)<br>        {<br>            int num=666;<br>            int count=1;<br> <br>            while (true)<br>            {<br>                if (n==count)<br>                {<br>                    break;<br>                }<br> <br>                num++;<br> <br>                int index=Convert.ToString(num).IndexOf("666");<br> <br>                if (index&gt;=0)<br>                {<br>                    count++;<br>                }<br>            }<br> <br>            return num;<br>        }<br>    <br>        static void Main(string[] args)<br>        {            <br>            Console.WriteLine(getNth(10000).ToString());<br>        }<br> <br>    }<br></td>