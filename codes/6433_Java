            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class ApocalypseSomeday<br>{<br>  public boolean isG(int x)<br>  {<br>    String s = new Integer(x).toString();<br>    return s.contains("666");<br>  }<br>  public int getNth(int n)<br>  {<br>    int nf = 0;<br>    for(int i =666; ; i++)<br>    {<br>      if(isG(i)) nf++;<br>      if(nf==n) return i;<br>    }<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>