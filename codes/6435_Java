            // Be the MAN!!!!<br>// Challenge it!!!!<br>import java.util.*;<br><br>public class InterleavePal {<br>  public int longestPal(String s, String t) {<br>        int n = s.length();<br>        int m = t.length();<br>        int res = 0;<br>        boolean[][][][] a = new boolean[n+1][n+1][m+1][m+1];<br>        for (int l = 0; l &lt;= n + m; l++) {<br>            for (int l1 = 0; l1 &lt;= l; l1++) {<br>                int l2 = l - l1;<br>                for (int i = 0; i &lt;= n; i++) {<br>                    int j = i + l1;<br>                    if (j &gt; n) continue;<br>                    for (int ii = 0; ii &lt;= m; ii++) {<br>                        int jj = ii + l2;<br>                        if (jj &gt; m) continue;<br><br>                        if (l &lt; 2) a[i][j][ii][jj] = true;<br><br>                        if (a[i][j][ii][jj]) {<br>                            char sl = (i &gt; 0) ? s.charAt(i - 1) : '1';<br>                            char sr = (j &lt; n) ? s.charAt(j) : '2';<br>                            char tl = (ii &gt; 0) ? t.charAt(ii - 1) : '3';<br>                            char tr = (jj &lt; m) ? t.charAt(jj) : '4';<br>                            if (sl == sr) {<br>                                a[i - 1][j + 1][ii][jj] = true;<br>                            }<br>                            if (sl == tr) {<br>                                a[i - 1][j][ii][jj + 1] = true;<br>                            }<br>                            if (tl == sr) {<br>                                a[i][j + 1][ii - 1][jj] = true;<br>                            }<br>                            if (tl == tr) {<br>                                a[i][j][ii - 1][jj + 1] = true;<br>                            }<br>                            if (l &gt; res) res = l;<br>                        }<br>                    }<br>                }<br>            }<br>        }<br>    return res;<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>