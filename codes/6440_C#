<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>using System.Collections.Generic;<br>using System.Text;<br> <br>public class BestSeller<br>{<br>    public static string findBestSeller(string[] items)<br>    {<br>        string Res="";<br>        int BestRes = -1;<br>        for (int i = 0; i &lt; items.Length; i++)<br>            if (items[i] != Res) <br>            {<br>                int C = 1;<br>                for (int j = i + 1; j &lt; items.Length; j++)<br>                    if (items[j] == items[i]) C++;<br>                if (C &gt; BestRes || (C == BestRes &amp;&amp; items[i].CompareTo(Res) &lt; 0))<br>                {<br>                    Res = items[i];<br>                    BestRes = C;<br>                }<br>            }<br>        return Res;<br>    }<br>}<br></td>