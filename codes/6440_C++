            // Cry as challange phase is passing by<br><br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br><br>#include &lt;cmath&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br><br>#include &lt;map&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br><br>using namespace std;<br><br>#define FORC(it,v) for( __typeof((v).begin()) it = (v).begin(); it != (v).end(); ++it )<br><br>map&lt; string, int &gt; mapper;<br><br>class BestSeller {<br>public:<br>    string findBestSeller( vector &lt;string&gt; items ) {<br>        sort( items.begin(), items.end() );<br>        mapper.clear();<br><br>        int maks = 0;<br><br>        FORC( it, items ) maks &gt;?= ++mapper[ *it ];<br>        FORC( it, items ) if( mapper[ *it ] == maks ) return *it;<br><br>        return "CROATIA";<br>    }<br>};<br><br><br><br><br><br><br><br><br><br>// Powered by FileEdit<br>// Powered by moj 3.0 beta 11 [modified TZTester]<br>// Powered by CodeProcessor<br></td>