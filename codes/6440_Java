            import java.util.*;<br>public class BestSeller {<br>  public String findBestSeller(String[] items) {<br>    int [] times = new int [items.length];<br>    int max = Integer.MIN_VALUE;<br>    Arrays.fill(times, 0);<br>    Arrays.sort(items);<br>    for (int i = 0; i &lt; items.length; i++) {<br>      for (int j = i+1; j &lt; items.length; j++) {<br>        if (items[i].equals(items[j])) times[i]++;<br>      }<br>      max = Math.max (max, times[i]);<br>    }<br>    for (int i = 0; i &lt; times.length; i++) if (times[i]==max) return items[i];<br>    return "";<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>