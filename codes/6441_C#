            public class TroytownKeeper{<br> <br>int[,] qc=new int[60,60];<br>int[,] he=new int[60,60];<br> <br>public int dfs(int i, int j,int q, int c){<br>  if (qc[i,j]==0) {return 0;}<br>  if (i&gt;0) {<br>    if ((he[i-1,j]==0)&amp;&amp;(qc[i-1,j]==0)) {<br>      qc[i-1,j]=1; dfs(i-1,j,q,c);<br>      }<br>    }<br>  if (j&gt;0) {<br>    if ((he[i,j-1]==0)&amp;&amp;(qc[i,j-1]==0)) {<br>      qc[i,j-1]=1; dfs(i,j-1,q,c);<br>      }<br>    }<br>  if (i&lt;q-1) {<br>    if ((he[i+1,j]==0)&amp;&amp;(qc[i+1,j]==0)) {<br>      qc[i+1,j]=1; dfs(i+1,j,q,c);<br>      }<br>    }<br>  if (j&lt;c-1) {<br>    if ((he[i,j+1]==0)&amp;&amp;(qc[i,j+1]==0)) {<br>      qc[i,j+1]=1; dfs(i,j+1,q,c);<br>      }<br>    }<br>  return 1;<br>  <br>  // Possibly, this part of code will not work correctly, but don't challenge my code please! Thank you! :-)<br>  <br>  }<br> <br>public int limeLiters (string[] maze){<br>int i,j,k,l,m,n,q,c;<br>q=maze.Length;<br>c=maze[0].Length;<br>string s;<br>double x,y,z;<br>int res=0;<br>for (i=0;i&lt;q;i++){<br>  for(j=0;j&lt;c;j++){<br>    if (maze[i][j]=='#') {he[i+1,j+1]=1;}<br>    else {he[i+1,j+1]=0;}<br>    } <br>  }<br>q=q+2;<br>c=c+2;<br>for (i=0;i&lt;q;i++){<br>  for (j=0;j&lt;c;j++){<br>    qc[i,j]=0;<br>    }<br>  }<br>for (i=0;i&lt;q;i++){<br>  qc[i,0]=1;<br>  qc[i,c-1]=1;<br>  }<br>for (i=0;i&lt;c;i++){<br>  qc[0,i]=1;<br>  qc[q-1,i]=1;<br>  }<br>for (i=0;i&lt;q;i++){<br>  for (j=0;j&lt;c;j++){<br>    k=dfs(i,j,q,c);<br>    }<br>  }<br>for (i=0;i&lt;q-1;i++){<br>  for (j=0;j&lt;c-1;j++){<br>    if (qc[i,j]+qc[i+1,j]==1) {res++;}<br>    if (qc[i,j]+qc[i,j+1]==1) {res++;}<br>    }<br>  }<br>return res;<br>}<br>}<br></td>