            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class PartSorting <br>{<br>  public int[] process(int[] data, int nSwaps) <br>  {<br>    go(data, 0, nSwaps);<br>    return data;<br>  }<br><br>  public void go(int[] data, int start, int swaps)<br>  {<br>    if (start == data.Length || swaps == 0) return;<br>    int search = Math.Min(data.Length - 1 - start, swaps);<br>    int mindex = start;<br>    for (int i = 0; i &lt;= search; i++)<br>    {<br>      if (data[i+start] &gt; data[mindex])<br>      {<br>        mindex = i+start;<br>      }<br>    }<br><br>    while(mindex &gt; start)<br>    {<br>      swap(data, mindex, mindex - 1);<br>      mindex--;<br>      swaps--;<br>    }<br>    go(data, start + 1, swaps);<br>  }<br><br>  void swap(int[] data, int i, int j)<br>  {<br>    int temp = data[i];<br>    data[i] = data[j];<br>    data[j] = temp;<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>