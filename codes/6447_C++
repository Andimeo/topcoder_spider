            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class PartSorting {<br>public:<br>  vector &lt;int&gt; process(vector&lt;int&gt; A, int m)<br>  {<br>    int n=A.size();<br>    for (int k=0;k&lt;n;k++)<br>    {<br>      if (m==0)<br>        break;<br>      int maxv=-100000000,maxd,i;<br>      for (i=k;i&lt;n;i++)<br>        if (A[i]&gt;maxv &amp;&amp; i-k&lt;=m)<br>        {<br>          maxv=A[i];<br>          maxd=i;<br>        }<br>      for (i=maxd;i&gt;k;i--)<br>      {<br>        m--;<br>        swap(A[i],A[i-1]);<br>      }<br>    }<br>    return A;<br>  }<br>};<br></td>