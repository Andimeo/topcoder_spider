            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class MooingCows {<br>    public int dissatisfaction(string[] farmland) {<br>        int res = Int32.MaxValue;<br><br>        for (int m = 0; m &lt; farmland.Length; m++)<br>            for (int n = 0; n &lt; farmland[0].Length; n++)<br>            {<br>                if (farmland[m][n] != 'C') continue;<br>                int sum = 0;<br>                for (int i = 0; i &lt; farmland.Length; i++)<br>                    for (int j = 0; j &lt; farmland[0].Length; j++)<br>                    {<br>                        if (i == m &amp;&amp; j == n) continue;<br>                        if (farmland[i][j] == 'C')<br>                        {<br>                            sum +=(m - i) * (m - i) + (n - j) * (n - j);<br>                        }<br>                    }<br>                res = Math.Min(sum, res);<br>            }<br>        return res;<br>    }<br>    <br>    }<br></td>