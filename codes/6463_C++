            #line 5 "MooingCows.cpp"<br>#include &lt;stdio.h&gt;<br>#include &lt;math.h&gt;<br>#include &lt;string&gt;<br>#include &lt;stdlib.h&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;map&gt;<br>#include &lt;list&gt;<br>#include &lt;set&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br><br><br>using namespace std;<br>class MooingCows {<br>  public:<br>  int dissatisfaction(vector &lt;string&gt; f) {<br>    int i,j,ii,jj,n=f.size();<br>    int m = f[0].size();<br>    int res = 1000000000;<br>    int buf;<br>    for (i = 0; i &lt; n; i++) {<br>      for (j = 0; j &lt; m; j++) {<br>        if (f[i][j] == 'C') {<br>          buf = 0;<br>          for (ii = 0; ii &lt; n; ii++) {<br>            for (jj = 0; jj &lt; m; jj++) {<br>              if (f[ii][jj] == 'C') {<br>                buf = buf + (i-ii)*(i-ii) + (j - jj) * (j-jj);<br>              }<br>            }<br>          }<br>          if (buf &lt; res) res = buf;<br>        }<br>      }<br>    }<br>    return res;<br>  }<br>  <br><br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>