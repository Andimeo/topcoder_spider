            import java.util.*;<br>import java.math.*;<br>public class MooingCows {<br>  public static int ipi(String s){return Integer.parseInt(s);}<br>  public static void sop(String s){System.out.println(s); }<br>  public static void sop2(String s){System.out.print(s);}<br>  public int dissatisfaction(String[] farm) {<br>      char[][] map =new char[farm.length][farm[0].length()]; <br>      for(int j=0; j&lt;map.length; j++)<br>    for(int k=0; k&lt;map[0].length; k++)<br>        map[j][k] = farm[j].charAt(k); <br> <br>      int min = Integer.MAX_VALUE; <br>      for(int j=0; j&lt;map.length; j++)<br>    for(int k=0; k&lt;map[0].length; k++)<br>        if(map[j][k]=='C')<br>      min = Math.min(comp(map,j,k), min);<br> <br>      return min; <br>  }<br>    public static int comp(char[][] map, int r, int c){<br>  int sum = 0; <br>  for(int j=0; j&lt;map.length; j++)<br>      for(int k=0; k&lt;map[0].length; k++)<br>    if(map[j][k]=='C')<br>        sum+=Math.abs(r-j) * Math.abs(r-j) + Math.abs(c-k) * Math.abs(c-k); <br>  return sum; <br> <br>  <br>    }<br> <br>  public static void main(String[] args) {<br>  }<br>}<br> <br> <br>// Powered by FileEdit<br></td>