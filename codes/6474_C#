            using System;<br>using System.Collections;<br>using System.Collections.Specialized;<br>using System.Collections.Generic;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class SpeedRadar<br>{<br>  public double averageSpeed(int minLimit, int maxLimit, int[] readings)<br>  {<br>    double r = 0.0;<br>    int nf = 0, ng = 0;<br>    int n = readings.Length;<br>    for (int i = 0; i &lt; n; i++)<br>      if (readings[i] &lt; minLimit || readings[i] &gt; maxLimit) nf++;<br>      else<br>      {<br>        r += readings[i];<br>        ng++;<br>      }<br> <br>    if (nf * 10 &gt; n) return 0.0;<br>    <br>    return (r / ng);<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>