            #include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br><br>#include &lt;cmath&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br><br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br><br>using namespace std;<br><br><br>class SpeedRadar {<br>public:<br>    double averageSpeed( int mnL, int mxL, vector &lt;int&gt; read ) {<br>        int sum = 0, cnt = 0;<br>        for( int i = 0; i &lt; ( int )read.size(); ++i ) <br>            if ( read[i] &gt;= mnL &amp;&amp; read[i] &lt;= mxL ) <br>                sum += read[i];<br>            else<br>                ++cnt;<br>        if ( cnt * 10 &gt; ( int )read.size() )<br>            return 0.0;<br>        return ( double( sum ) / ( ( int )read.size() - cnt ) );<br>    }<br>};<br><br><br><br><br><br><br><br><br><br>// Powered by FileEdit<br>// Powered by moj 3.0 beta 11 [modified TZTester]<br>// Powered by CodeProcessor<br></td>