            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class SpeedRadar<br>{<br>  public double averageSpeed(int minLimit, int maxLimit, int[] readings)<br>  {<br>    int inf = 0;<br>                int noninf = 0;<br>                double avg = 0;<br>    for(int i = 0; i&lt;readings.length;i++){<br>      if(readings[i] &lt; minLimit || readings[i] &gt; maxLimit) inf++;<br>                        else{<br>                            avg += readings[i];<br>                            noninf++;<br>                        }<br>    }<br>                if(inf &gt; readings.length / 10) return 0.0;<br>                if(noninf == 0) return 0.0;<br>                else return avg / noninf;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>