            using System;<br><br>public class HuffmanDecoding {<br>  public string decode(string archive, string[] dictionary) {<br>    string res = "";<br>    int pos = 0;<br>    while (pos &lt; archive.Length)<br>    {<br>      int cur = -1;<br>      for (int i = 0; i &lt; dictionary.Length; ++i)<br>      {<br>        string item = dictionary[i];<br>        if (pos + item.Length &lt;= archive.Length)<br>          if (archive.Substring(pos, item.Length) == item)<br>          {<br>            cur = i;<br>          }<br>      }<br>      res += (char) ('A' + cur);<br>      pos += dictionary[cur].Length;<br>    }<br>    return res;<br>  }<br><br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>