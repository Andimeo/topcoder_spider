            #include&lt;iostream&gt;<br>#include&lt;sstream&gt;<br>#include&lt;cstdlib&gt;<br>#include&lt;cmath&gt;<br>#include&lt;vector&gt;<br>#include&lt;string&gt;<br>#include&lt;algorithm&gt;<br>using namespace std;<br>typedef vector&lt;int&gt;VI;typedef vector&lt;VI&gt;VVI;<br>typedef vector&lt;string&gt;VS;<br>typedef pair&lt;int,int&gt;PII;<br>#define FOR(i,n) for((i)=0;(i)&lt;(n);(i)++)<br>#define BE(a) ((a).begin()),((a).end())<br>#define SI(a) ((a).size())<br>#define PB push_back<br>#define MP make_pair<br>#define FORIT(i,a) for((i)=(a).begin();(i)!=(a).end();(i)++)<br>#define CLR(a,v) memset((a),(v),sizeof(a))<br><br>class HuffmanDecoding {<br>  public:<br>  string decode(string o, VS dict) {<br>    string ret, s;<br>    int n, i, j;<br>    ret = "";<br>    n = SI(dict);<br>    s = "";<br>    FOR (i,SI(o)) {<br>      s += o[i];<br>      FOR (j,n) if (dict[j] == s) break;<br>      if (j == n) continue;<br>      ret += (char)('A' + j);<br>      s = "";<br>    }<br>    return ret;<br>  }<br><br>  <br><br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.2 [15-Oct-2005 modified by Revenger]<br>// Powered by CodeProcessor<br></td>