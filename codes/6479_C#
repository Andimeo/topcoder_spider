<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br><br>  public class Shopping<br>  {<br>    public int minNumber(int X, int[] values)<br>    {<br>      int m = values.Length;<br>      Array.Sort(values);<br>      int max = 0;<br>      int pos = 0;<br>      int num = 0;<br>      if (values[0] != 1)<br>        return -1;<br><br>      while (max &lt; X)<br>      {<br>        while (pos &lt; m - 1 &amp;&amp; max + 1 &gt;= values[pos+1])<br>          pos++;<br>        num++;<br>        max += values[pos];<br>      }<br>      return num;<br>    }<br>  }<br></td>