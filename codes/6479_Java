            import java.util.*;<br><br>public class Shopping {<br>    public int minNumber(int X, int[] values) {<br>        int[] d = new int[X + 1];<br>        int n = values.length;<br>        for (int i = 1; i &lt;= X; i++) {<br>            d[i] = X + 1;<br>            for (int j = 0; j &lt; n; j++) {<br>                if (values[j] &lt;= i) {<br>                    int q = Math.max(values[j] - 1, i - values[j]);<br>                    d[i] = Math.min(d[i], d[q] + 1);<br>                }<br>            }<br>            if (d[i] == X + 1) return -1;<br>        }<br>        return d[X];<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>