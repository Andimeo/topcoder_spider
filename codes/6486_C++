            #include &lt;vector&gt;<br>#include &lt;sstream&gt; <br>#include &lt;iostream&gt;<br>#include &lt;string.h&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;stdlib.h&gt; <br>#include &lt;stdio.h&gt; <br>#include &lt;numeric&gt;<br>#include &lt;math.h&gt;<br>#include &lt;string&gt;<br>#include &lt;set&gt;<br>using namespace std;<br> <br>#define forn(i,n)     for(int i=0; i&lt;(n); ++i)<br>#define forv(i,v)     forn(i,v.size())<br>#define ALL(a)        (a).begin(),(a).end()<br>#define pb            push_back<br>#define sz            size()<br>#define SORT(a)       sort(ALL(a))<br>#define PERM(a)       next_permutation(ALL(a))<br>#define cs            c_str()<br> <br>typedef vector&lt; int &gt; VI; typedef vector&lt; string &gt; VS;<br> <br>class TVSize {<br>  public:<br>  vector &lt;int&gt; calcSize(int d, int h, int w) {<br>    double p = 1.0*d/sqrt(h*h+w*w)+1e-9;<br>    VI ans;<br>    ans.pb(h*p);<br>    ans.pb(w*p);<br>    return ans;<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>