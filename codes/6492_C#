            using System;<br><br>public class DigitalDisplay<br>{<br>  bool ok(int h, int m, int s)<br>  {<br>    return (h&gt;=1) &amp;&amp; h&lt;=12 &amp;&amp; m&gt;=0 &amp;&amp; m&lt;=59 &amp;&amp; s&gt;=0 &amp;&amp; s&lt;=59;<br>  }<br>  public int waysToInterpret(string display)<br>  {<br>    int Res = 0;<br>    string[] tmp = display.Split(':');<br>    int[] A = new int[3];<br>    A[0] = int.Parse(tmp[0]);<br>    A[1] = int.Parse(tmp[1]);<br>    A[2] = int.Parse(tmp[2]);<br>    if(ok(A[0], A[1], A[2]))Res++;<br>    if(ok(A[0], A[2], A[1]))Res++;<br>    if(ok(A[1], A[0], A[2]))Res++;<br>    if(ok(A[1], A[2], A[0]))Res++;<br>    if(ok(A[2], A[1], A[0]))Res++;<br>    if(ok(A[2], A[0], A[1]))Res++;<br>    <br>    return Res;<br>  }<br>}<br></td>