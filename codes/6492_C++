            // Cry as challange phase is passing by<br><br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br><br>#include &lt;cmath&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br><br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br><br>using namespace std;<br><br>#define FORC(it,v) for( __typeof((v).begin()) it = (v).begin(); it != (v).end(); ++it )<br><br>inline int range( int a, int x, int b )<br>{<br>    if( a &lt;= x &amp;&amp; x &lt;= b ) <br>        return true;<br>    return false;<br>}<br><br>class DigitalDisplay {<br>public:<br>    int waysToInterpret( string display ) {<br>        int a, b, c; sscanf( display.c_str(), "%d:%d:%d", &amp;a, &amp;b, &amp;c );<br>        if( a &gt; 59 || b &gt; 59 || c &gt; 59 ) return 0;<br><br>        int aa = range( 1, a, 12 );<br>        int bb = range( 1, b, 12 );<br>        int cc = range( 1, c, 12 );<br><br>        return ( aa + bb + cc ) * 2;<br>    }<br>};<br><br><br><br><br><br><br><br><br><br>// Powered by FileEdit<br>// Powered by moj 3.0 beta 11 [modified TZTester]<br>// Powered by CodeProcessor<br></td>