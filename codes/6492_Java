            public class DigitalDisplay<br>{<br>  public int waysToInterpret(String display)<br>  {<br>    int ret = 0;<br>    int a = Integer.parseInt(display.substring(0, 2));<br>    int b = Integer.parseInt(display.substring(3, 5));<br>    int c = Integer.parseInt(display.substring(6, 8));<br>    if(a&gt;=1 &amp;&amp; a&lt;=12 &amp;&amp; b&gt;=0 &amp;&amp; b&lt;=59 &amp;&amp; c&gt;=0 &amp;&amp; c&lt;=59)<br>      ret+=2;<br>    if(b&gt;=1 &amp;&amp; b&lt;=12 &amp;&amp; a&gt;=0 &amp;&amp; a&lt;=59 &amp;&amp; c&gt;=0 &amp;&amp; c&lt;=59)<br>      ret+=2;<br>    if(c&gt;=1 &amp;&amp; c&lt;=12 &amp;&amp; b&gt;=0 &amp;&amp; b&lt;=59 &amp;&amp; a&gt;=0 &amp;&amp; a&lt;=59)<br>      ret+=2;<br>      return ret;<br>  }<br>}<br></td>