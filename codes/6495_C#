<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br><br>public class WildCardMatch{<br>public int minimalChanges(string f,string p){<br>int i,j,m,n;<br>int res=0;<br>n=f.Length+1;<br>m=p.Length+1;<br>int[,] a=new int[62,62];<br>for (i=0;i&lt;n;i++){<br>  for (j=0;j&lt;m;j++){<br>    a[i,j]=60;<br>    }<br>  }<br>for (i=0;i&lt;60;i++){<br>  a[0,i]=a[i,0]=i;<br>  }<br>for (i=0;i&lt;n-1;i++){<br>  for (j=0;j&lt;m-1;j++){<br>    if ((p[j]=='?')||(p[j]==f[i])) {<br>      a[i+1,j+1]=a[i,j];<br>      }<br>      else {<br>      a[i+1,j+1]=Math.Min(a[i,j],Math.Min(a[i+1,j],a[i,j+1]))+1;<br>      }<br>    }<br>  }<br>res=a[n-1,m-1];<br>return res;<br>}<br>}<br></td>