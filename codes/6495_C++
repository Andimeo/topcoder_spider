            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;utility&gt;<br>#include &lt;iostream&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;sstream&gt;<br>#include &lt;algorithm&gt;<br> <br>using namespace std;<br> <br>#define foreach(i,c) for(typeof((c).begin()) i=(c).begin(); i!=(c).end();++i)<br>#define s2i(s) atoi(s.c_str())<br> <br>class WildCardMatch {<br>public:<br>  int minimalChanges(string a, string b) {<br>    int r[51][51];<br> <br>    for(int i = 0; i &lt; a.size()+1; ++i) r[i][0] = i;<br>    for(int i = 0; i &lt; b.size()+1; ++i) r[0][i] = i;<br>    for(int i = 1; i &lt;= a.size(); ++i) {<br>      for(int j = 1; j &lt;= b.size(); ++j) {<br>        int c = (a[i-1]==b[j-1] || b[j-1]=='?')?0:1;<br>        r[i][j] = r[i-1][j]+1;<br>        r[i][j] &lt;?= r[i][j-1]+1;<br>        r[i][j] &lt;?= r[i-1][j-1]+c;<br>      }<br>    }<br>    return r[a.size()][b.size()];<br>  }<br> <br>};<br> <br> <br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>