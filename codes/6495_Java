            import java.lang.*;<br>import java.util.*;<br><br>public class WildCardMatch {<br>int[][][][] cache;<br>String a, b;<br><br>int calc(int s1, int s2, int t1, int t2)<br>{<br>  if (s1&gt;s2 &amp;&amp; t1&gt;t2) return 0;<br>  if (s1&gt;s2) return t2-t1+1;<br>  if (t1&gt;t2) return s2-s1+1;<br>  if (cache[s1][s2][t1][t2]&gt;-1) return cache[s1][s2][t1][t2];<br>  int res = 100000000;<br>  if (res&gt;calc(s1+1, s2, t1, t2)+1) res = calc(s1+1, s2, t1, t2)+1;<br>  if (res&gt;calc(s1, s2-1, t1, t2)+1) res = calc(s1, s2-1, t1, t2)+1;<br>  <br>  if (res&gt;calc(s1, s2, t1+1, t2)+1) res = calc(s1, s2, t1+1, t2)+1;<br>  if (res&gt;calc(s1, s2, t1, t2-1)+1) res = calc(s1, s2, t1, t2-1)+1;<br>  <br>  if (res&gt;calc(s1+1, s2, t1+1, t2)+1) res = calc(s1+1, s2, t1+1, t2)+1;<br>  if (res&gt;calc(s1, s2-1, t1, t2-1)+1) res = calc(s1, s2-1, t1, t2-1)+1;<br>  <br>  if (a.charAt(s1)==b.charAt(t1) || b.charAt(t1)=='?')<br>    if (res&gt;calc(s1+1, s2, t1+1, t2)) res = calc(s1+1, s2, t1+1, t2);<br>  if (a.charAt(s2)==b.charAt(t2) || b.charAt(t2)=='?')<br>    if (res&gt;calc(s1, s2-1, t1, t2-1)) res = calc(s1, s2-1, t1, t2-1);<br>  cache[s1][s2][t1][t2] = res;<br>  return res;<br>}<br><br>public int minimalChanges(String file, String pattern) {<br>  a = file;<br>  b = pattern;<br>  int i, j, k, l;<br>  cache = new int[52][52][52][52];<br>  for (i=0; i&lt;52; i++) for (j=0; j&lt;52; j++) for (k=0; k&lt;52; k++) for (l=0; l&lt;52; l++) cache[i][j][k][l] = -1;<br>  return calc(0, a.length()-1, 0, b.length()-1);<br>}<br>};<br></td>