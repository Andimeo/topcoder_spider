            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class ParallelepipedUnion<br>{<br>        public int getVolume(String[] parallelepipeds)<br>        {<br>                int[][] coords = splitInts2D(parallelepipeds);<br>                int count = 0;<br>                for (int x=0; x&lt;=100; x++)<br>                        for (int y=0; y&lt;=100; y++)<br>                                for (int z=0; z&lt;=100; z++)<br>                                {<br>                                        boolean in = false;<br>                                        for (int i=0; i&lt;2; i++)<br>                                                in |= (coords[i][0] &lt;= x &amp;&amp; coords[i][3] &gt; x &amp;&amp; coords[i][1] &lt;=y &amp;&amp; coords[i][4] &gt; y &amp;&amp; coords[i][2] &lt;= z &amp;&amp; coords[i][5] &gt; z);<br>                                        if (in)<br>                                                count++;<br>                                }<br>                return count;<br>        }<br> <br>        int[][] splitInts2D(String[] arr)<br>        {<br>                int[][] ret = new int[arr.length][];<br>                for (int i=0; i&lt;ret.length; i++)<br>                {<br>                        String[] split = arr[i].split(" ");<br>                        ret[i] = new int[split.length];<br>                        for (int j=0; j&lt;split.length; j++)<br>                                ret[i][j] = Integer.parseInt(split[j]);<br>                }<br>                return ret;<br>        }<br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>