            Imports System.Math<br>Public Class ParallelepipedUnion<br>    Public Function getVolume(ByVal parallelepipeds As String()) As Integer<br>        Dim i, j As Integer<br>        Dim a(5), b(5) As String<br><br>        a = parallelepipeds(0).Split(" ")<br>        b = parallelepipeds(1).Split(" ")<br><br>        Dim ai(5), bi(5) As Integer<br>        For i = 0 To 5<br>            ai(i) = Integer.Parse(a(i))<br>            bi(i) = Integer.Parse(b(i))<br>        Next<br><br>        Dim minx, miny, minz, maxx, maxy, maxz As Integer<br>        minx = Min(ai(0), bi(0))<br>        miny = Min(ai(1), bi(1))<br>        minz = Min(ai(2), bi(2))<br>        maxx = Max(ai(3), bi(3))<br>        maxy = Max(ai(4), bi(4))<br>        maxz = Max(ai(5), bi(5))<br>        Dim k As Integer<br>        Dim vol As Integer = 0<br>        For i = minx To maxx - 1<br>            For j = miny To maxy - 1<br>                For k = minz To maxz - 1<br>                    If isIn(i, j, k, ai) OrElse isIn(i, j, k, bi) Then<br>                        vol += 1<br>                    End If<br>                Next<br>            Next<br>        Next<br>        Return vol<br><br>    End Function<br><br>    Public Function isIn(ByVal a As Integer, ByVal b As Integer, ByVal c As Integer, ByVal rec As Integer()) As Boolean<br>        If a &gt;= rec(0) AndAlso a &lt; rec(3) AndAlso b &gt;= rec(1) AndAlso b &lt; rec(4) AndAlso c &gt;= rec(2) AndAlso c &lt; rec(5) Then<br>            Return True<br>        End If<br>        Return False<br><br>    End Function<br>End Class<br></td>