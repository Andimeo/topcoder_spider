            using System;<br>using System.Collections;<br>using System.Text;<br><br>public class WallRepair<br>{<br><br>    public int n;<br><br>    public int bricksRequired(string[] wallRows)<br>    {<br>        n = wallRows.Length;<br>        int m = wallRows[0].Length;<br>        int d=0;<br>        for (int i = 0; i &lt; m; i++)<br>        {<br>            int van=-1;<br>            for (int j = 0; j &lt; n; j++)<br>            {<br>                if (wallRows[j][i] == 'X') { van = j; break; }<br>            }<br>            if (van &gt;= 0)<br>            {<br>                for (int j = van+1; j &lt; n; j++)<br>                {<br>                    if (wallRows[j][i] == '.') { d++; }<br>                }<br>            }<br><br>        }<br><br><br>        return d;<br>    }<br><br>}<br></td>