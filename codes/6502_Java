            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class WallRepair<br>{<br>  public int bricksRequired(String[] wallRows)<br>  {<br>    int numb = 0;<br>    for(int i = 0; i &lt; wallRows[0].length(); i++)<br>    {<br>      boolean start = false;<br>      for(int j = 0; j &lt; wallRows.length; j++)<br>      {<br>        if(wallRows[j].charAt(i) == 'X') start = true;<br>        if(start &amp;&amp; wallRows[j].charAt(i) == '.') numb++;<br>                <br>      }<br>    }<br>    return numb;<br>    <br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>