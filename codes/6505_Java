            import java.util.ArrayList;<br>import java.util.Collections;<br>import java.util.Scanner;<br><br>public class StudentsOrdering {<br>  class Student implements Comparable&lt;Student&gt; {<br>    String name;<br>    int h;<br>    boolean boy;<br>    <br>    public Student(String s) {<br>      Scanner in = new Scanner(s);<br>      name = in.next();<br>      h = in.nextInt();<br>      boy = in.next().equals("boy");<br>    }<br><br>    public int compareTo(Student that) {<br>      if (h != that.h)<br>        return h - that.h;<br>      return name.compareTo(that.name);<br>    }<br>  }<br>  <br>  public String findOrder(String[] students) {<br>    ArrayList&lt;Student&gt; boys = new ArrayList&lt;Student&gt;();<br>    ArrayList&lt;Student&gt; girls = new ArrayList&lt;Student&gt;();<br>    for (String ss : students) {<br>      Student s = new Student(ss);<br>      if (s.boy)<br>        boys.add(s);<br>      else<br>        girls.add(s);<br>    }<br>    Collections.sort(boys);<br>    Collections.sort(girls);<br>    String s1 = order(boys, girls);<br>    String s2 = order(girls, boys);<br>    if (s1.equals("") &amp;&amp; s2.equals(""))<br>      return "";<br>    if (s1.equals("") || s2.equals(""))<br>      return s1 + s2;<br>    if (s1.compareTo(s2) &gt; 0)<br>      s1 = s2;<br>    return s1;<br>  }<br><br>  private String order(ArrayList&lt;Student&gt; boys, ArrayList&lt;Student&gt; girls) {<br>    int n = boys.size();<br>    int m = girls.size();<br>    if (n != m &amp;&amp; n != m + 1)<br>      return "";<br>    String a = "";<br>    int ch = -1;<br>    int nh;<br>    for (int i = 0; i &lt; m; i++) {<br>      a += boys.get(i).name + "-";<br>      nh = boys.get(i).h;<br>      if (nh &lt; ch)<br>        return "";<br>      ch = nh;<br>      a += girls.get(i).name + "-";<br>      nh = girls.get(i).h;<br>      if (nh &lt; ch)<br>        return "";<br>      ch = nh;<br>    }<br>    if (n &gt; m) {<br>      a += boys.get(n - 1).name + "-";<br>      nh = boys.get(n - 1).h;<br>      if (nh &lt; ch)<br>        return "";<br>      ch = nh;<br>    }<br>    return a.substring(0, a.length() - 1);<br>  }<br>  <br>  public static void main(String[] args) {<br>    String a = new StudentsOrdering().findOrder(<br>        new String[]<br>                   {"Alex 180 boy", <br>             "Josh 181 boy", <br>             "Mary 158 girl", <br>             "An 158 girl", <br>             "Tanya 180 girl", <br>             "Ted 158 boy"}<br>                   );<br>    System.out.println(a);<br>  }<br>}<br></td>