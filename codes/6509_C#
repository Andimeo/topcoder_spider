            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class CommutePlan<br>{<br>  public int shortestRoute(int distance, int[] cutStart, int[] cutEnd, int[] cutLength)<br>  {<br>    int[] f=new int[1001];<br>    f[0]=0;<br>    for(int i=1;i&lt;=distance;++i){<br>      f[i]=f[i-1]+1;<br>      for(int k=0;k&lt;cutEnd.Length;++k) if(cutEnd[k]==i &amp;&amp; f[i]&gt;f[cutStart[k]]+cutLength[k]){<br>        f[i]=f[cutStart[k]]+cutLength[k];<br>      }<br>    }<br>//    Console.WriteLine(f[60]);<br>    return f[distance];<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>