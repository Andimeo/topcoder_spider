            public class CommutePlan{<br>  public int shortestRoute(int dist, int[] start, int[] end, int[] length){<br>    int[] shortests = new int[dist + 1];<br>    int n = start.length;<br>    for(int i = 1; i &lt;= dist; i++){<br>      shortests[i] = shortests[i - 1] + 1;<br>      for(int j = 0; j &lt; n; j++)<br>        if(end[j] == i){<br>          shortests[i] = Math.min(shortests[i], shortests[start[j]] + length[j]);<br>          System.out.println("found: " + j);<br>        }<br>      if(i%10 == 0)<br>        System.out.println(i + " " + shortests[i]);<br>    }<br>    return shortests[dist];<br>  }<br>}<br></td>