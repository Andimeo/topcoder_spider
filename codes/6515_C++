            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdio&gt;<br> <br>#define MAX(A, B) (((A) &gt;= (B)) ? (A) : (B))<br>#define MIN(A, B) (((A) &lt;= (B)) ? (A) : (B))<br>#define ABS(X) (((X) &gt;= 0) ? (X) : -(X))<br>#define SWAP(A, B, C) ((C) = (A), (A) = (B), (B) = (C))<br> <br>using namespace std;<br> <br>class IndictDates<br>{<br>  public: int getYear( int A, int B, int C)<br>  {<br>    A--, B--, C--;<br>    for (int i = 0; ; i++)<br>      if (i % 15 == A &amp;&amp; i % 28 == B &amp;&amp; i % 19 == C)<br>        return i + 1;<br>    return -1;<br>  }<br>};<br> <br>// Code by Burunduk3<br> <br> <br>// Powered by PopsEdit<br></td>