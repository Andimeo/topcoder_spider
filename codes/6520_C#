            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class FracSum<br>{<br>    public static long gcd(long a, long b)<br>    {<br>        if (a &lt; 0) return gcd(-a, b);<br>        if (b &lt; 0) return gcd(a, -b);<br>    //    Console.WriteLine(a + " " + b);<br>        if (a &lt; b) return gcd(b, a);<br>        else if (b == 0) return a;<br>        else return gcd(b, a % b);<br>    }<br>    public int decompose(int a, int b)<br>    {<br>        long aa = a; <br>        long bb = b;<br>        long cc = gcd(aa,bb);<br>        bb /= cc;<br>        int ans = int.MaxValue;<br>        Console.WriteLine(bb);<br>        for (long i = 1;i*i&lt;=bb;i++)<br>            if (bb % i == 0 &amp;&amp; gcd(i, bb/i) == 1)<br>            {<br>                int t = (int)Math.Max(bb / i ,i);<br>                if (t &lt; ans) ans = t;<br>            }<br>        return ans;<br>    }}<br></td>