            // my submission accuracy is quite low recently, don't be afraid to challenge me<br><br>#include &lt;algorithm&gt;<br>#include &lt;math.h&gt;<br>using namespace std;<br><br>typedef long long ll;<br><br>// Greatest Common Divisor<br>ll gcd(ll x, ll y) {return x ? gcd(y%x,x) : y;}<br><br>#line 3 "FracSum.cc"<br><br>class FracSum {<br>  public:<br>  int decompose(int a, int b) {<br>    // !FDI<br>    int g = gcd(a,b);<br>    a /= g;<br>    b /= g;<br>    int sq = int(sqrt(b));<br>    for(int k=sq;;k--) if(b % k == 0 &amp;&amp; gcd(k,b/k) == 1) return b / k;<br>    }<br>  };<br><br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>