            import java.util.*;<br>import java.math.*;<br> <br> <br>public class FracSum {<br> <br> <br>    public int decompose (int A, int B) {<br>        printf ("\n");<br> <br> <br>        long a = A;<br>        long b = B;<br>        <br>        if (a == 0) return 1;<br> <br>        long ggd = gcd (a, b);<br>        a /= ggd;<br>        b /= ggd;<br> <br>        long result = b;<br> <br>        for (int d = 1; d * d &lt;= b; d++) {<br>            if (b % d == 0 &amp;&amp; gcd (d, b / d) == 1) {<br>                result = Math.min (result, Math.max (d, b/d));<br>            }<br>        }<br> <br>        return (int)result;<br>    }<br> <br> <br>    /* Euclidisch Algoritme */<br>    long gcd(long a, long b) {<br>        if(a&lt;0) return gcd(-a, b);<br>        if(b&lt;0) return gcd(a, -b);<br>        if(a&lt;b) return gcd(b, a);<br>        if(b==0) return a;<br>        return gcd(b, a%b);<br>    }<br> <br> <br> <br>    static void printf (String f, Object... a) {<br>    }<br> <br> <br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>