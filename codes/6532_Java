            public class LatticeCrossword{<br>  public int crossword(String a,String b, String c, String d){<br>    String s[]=new String[5];<br>    s[0]=a;<br>    s[1]=b;<br>    s[2]=c;<br>    s[3]=d;<br>    int ile=0;<br>    int w1=-1;<br>    int w2=-2;<br>    for(int h1=0;h1&lt;4;h1++){<br>      for(int h2=0;h2&lt;4;h2++){<br>        w1=w2=-1;<br>        if(h1==h2)continue;<br>        for(int v=0;v&lt;4;v++){<br>          if(v!=h1 &amp;&amp; v!=h2){<br>            if(w1==-1)w1=v;<br>            else w2=v;<br>          }<br>        }<br>        for(int p1=0;p1&lt;s[h1].length();p1++)for(int p2=p1+2;p2&lt;s[h1].length();p2++)<br>        for(int q1=0;q1&lt;s[h2].length();q1++)for(int q2=q1+2;q2&lt;s[h2].length();q2++){<br>          if(p2-p1!=q2-q1)continue;<br>          char a1=s[h1].charAt(p1);<br>          char a2=s[h1].charAt(p2);<br>          char a3=s[h2].charAt(q1);<br>          char a4=s[h2].charAt(q2);<br>          <br>          int u=1;<br>          for(int x1=0;x1&lt;s[w1].length();x1++)for(int y1=x1+2;y1&lt;s[w1].length();y1++)for(int z1=0;z1&lt;s[w2].length();z1++){<br>            int uy=z1+(y1-x1);<br>            if(uy&gt;=s[w2].length())continue;<br>            if(s[w1].charAt(x1)==a1 &amp;&amp; s[w1].charAt(y1)==a3 &amp;&amp; s[w2].charAt(z1)==a2 &amp;&amp; s[w2].charAt(uy)==a4)ile++;<br>          }<br>          <br>        }<br>      <br>      }<br>    }    <br>  <br>  <br>  <br>  <br>  w1=-1;<br>  w2=-1;<br>    for(int h1=0;h1&lt;4;h1++){<br>      for(int h2=0;h2&lt;4;h2++){<br>        <br>        if(h1==h2)continue;<br>        w1=w2=-1;<br>        for(int v=0;v&lt;4;v++){<br>          if(v!=h1 &amp;&amp; v!=h2){<br>            if(w2==-1)w2=v;<br>            else w1=v;<br>          }<br>        }<br>        for(int p1=0;p1&lt;s[h1].length();p1++)for(int p2=p1+2;p2&lt;s[h1].length();p2++)<br>        for(int q1=0;q1&lt;s[h2].length();q1++)for(int q2=q1+2;q2&lt;s[h2].length();q2++){<br>          if(p2-p1!=q2-q1)continue;<br>          char a1=s[h1].charAt(p1);<br>          char a2=s[h1].charAt(p2);<br>          char a3=s[h2].charAt(q1);<br>          char a4=s[h2].charAt(q2);<br>          int u=1;<br>          for(int x1=0;x1&lt;s[w1].length();x1++)for(int y1=x1+2;y1&lt;s[w1].length();y1++)for(int z1=0;z1&lt;s[w2].length();z1++){<br>            int uy=z1+(y1-x1);<br>            if(uy&gt;=s[w2].length())continue;<br>            if(s[w1].charAt(x1)==a1 &amp;&amp; s[w1].charAt(y1)==a3 &amp;&amp; s[w2].charAt(z1)==a2 &amp;&amp; s[w2].charAt(uy)==a4)ile++;<br>          }<br>          <br>        }<br>      <br>      }<br>    }    <br>  <br>  <br>  <br>  return ile;<br>  }<br>  <br>  <br>  <br>  }<br></td>