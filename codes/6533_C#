            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class MatchNumbersEasy<br>{<br>  <br>    public string maxNumber(int[] matches, int n)<br>    {<br>        int lowestnz = int.MaxValue;<br>        int lowest = int.MaxValue;<br>        for (int i = 0; i &lt; matches.Length; i++)<br>        {<br>            int matche = matches[i];<br>            if (i != 0 &amp;&amp; lowestnz &gt; matche) lowestnz = matche;<br>            if (lowest &gt; matche) lowest = matche;<br>        }<br>        <br>        int digits = ((n - lowestnz)/lowest) + 1;<br>        if(n - lowestnz &lt; 0) return "0";<br> <br>        String res = "";<br>        int rem = n;<br>        for (int i = 0; i &lt; digits; i++)<br>        {<br>            for(int j = matches.Length - 1; j &gt;= 0; j--)<br>            {<br>                if((rem - matches[j]) &gt;= lowest * (digits - i - 1))<br>                {<br>                     res = res + j;<br>                     rem -= matches[j];<br>                     break;<br>                }<br>            }    <br>        }<br>        return res;<br>    }<br> <br> <br>}<br>//Perseph0nedited<br></td>