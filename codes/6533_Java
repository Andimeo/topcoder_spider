            public class  MatchNumbersEasy{<br>  public String maxNumber(int[] matches, int n){<br>    int la=matches.length;<br>    String tab[]=new String[100];<br>      String tab0[]=new String[100];<br>    for(int a=0;a&lt;=99;a++){tab[a]="";tab0[a]="";}<br>    String best="";<br>    int bestlen;<br>    int bestch=0;<br>    for(int a=1;a&lt;=50;a++){<br>      bestlen=0;<br>      best="";<br>      bestch=1000;<br>      for(int b=0;b&lt;=la-1;b++){<br>        if(a&lt;matches[b])continue;<br>        if(tab[a-matches[b]].length()&gt;=bestlen){<br>          bestch=b;<br>          bestlen=tab[a-matches[b]].length();<br>          best=tab[a-matches[b]];<br>          }<br>        }<br>      if(bestch&lt;1000 &amp;&amp; a-matches[bestch]&gt;=0){<br>        tab[a]=Integer.toString(bestch);<br>        tab[a]+=best;<br>        }<br>      }<br>    for(int a=1;a&lt;=50;a++){<br>      bestlen=0;<br>      best="";<br>      bestch=1000;<br>      for(int b=1;b&lt;=la-1;b++){<br>        if(a&lt;matches[b])continue;<br>        if(tab[a-matches[b]].length()&gt;=bestlen){<br>          bestch=b;<br>          bestlen=tab[a-matches[b]].length();<br>          best=tab[a-matches[b]];<br>          }<br>        }<br>      <br>      if(bestch&lt;1000 &amp;&amp; a-matches[bestch]&gt;=0){<br>        tab0[a]=Integer.toString(bestch);<br>        tab0[a]+=best;<br>        }<br>      }<br>    if(tab0[n].length()==0)tab0[n]="0";<br>    return tab0[n];<br>    }<br>  }<br></td>