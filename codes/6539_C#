            using System;<br>using System.Collections.Generic;<br> <br>public class RPSTournament<br>{<br>  bool possible (int[] x, int kto, int rr)<br>  {<br>    int n = x.Length;<br>    int[] vyr = new int[n];<br>    vyr[kto] = 1;<br>    <br>    for (int i = 2; i &lt;= rr + 1; ++i)<br>    {<br>      List&lt;int&gt; w = new List&lt;int&gt; ();<br>      <br>      for (int j = 0; j &lt; n; ++j)<br>        if (vyr[j] != 0)<br>          w.Add (x[j]);<br>      <br>      int som = 0;<br>      <br>      foreach (int ww in w)<br>      {<br>        if (som &lt; ww)<br>          som = ww;<br>        <br>        while (som &lt; n &amp;&amp; vyr[som] != 0)<br>          ++som;<br>        <br>        if (som == n)<br>          return false;<br>        <br>        vyr[som] = i;<br>      }<br>    }<br>    <br>    <br>    return true;<br>  }<br>  <br>  <br>  public int greatestSeed(int rounds, int C)<br>  {<br>    int n = 1 &lt;&lt; rounds;<br>    <br>    int[] x = new int[n + 1];<br>    <br>    for (int i = 1; i &lt;= n; ++i)<br>    {<br>      x[i] = i - (int) Math.Floor (Math.Sqrt (i * C));<br>      if (x[i] &lt; 1)<br>        x[i] = 1;<br>    }<br>    <br>    int ask = 0;<br>    <br>    for (int i = rounds - 1 ; i &gt;= 0; --i)<br>    {<br>      int nask = ask | (1 &lt;&lt; i);<br>      <br>      if (possible (x, nask + 1, rounds))<br>        ask = nask;<br>    }<br>    <br>    return ask + 1;<br>  }<br>}<br> <br> <br>// Powered by FileEdit<br></td>