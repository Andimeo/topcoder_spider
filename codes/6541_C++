            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;cmath&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstdio&gt;<br>using namespace std;<br> <br>class CircuitBoard <br>{<br>  public:<br>  vector&lt;string&gt; s;<br>  int t[57][57];<br>  <br>  int maxDataLines(vector &lt;string&gt; ss) <br>  {<br>    s=ss;<br>    int ret=0,i,j;<br>    while(1)<br>    {<br>      for(i=0;i&lt;s.size();i++) for(j=0;j&lt;s[i].size();j++) t[i][j]=0;<br>      for(i=0;i&lt;s[0].size();i++) if(s[s.size()-1][i]=='.') t[s.size()-1][i]=1;<br>      for(i=s.size()-2;i&gt;=0;i--) for(j=0;j&lt;s[i].size();j++) <br>      if(s[i][j]=='.' &amp;&amp; (t[i+1][j] || (j &amp;&amp; t[i+1][j-1]) || t[i+1][j+1])) t[i][j]=1;<br>      for(i=0;!t[0][i] &amp;&amp; i&lt;s[0].size();i++);<br>      if(i==s[0].size()) break;<br>      ret++;<br>      for(j=0;j+1&lt;s.size();j++) <br>      {<br>        s[j][i]='r';<br>        if(i &amp;&amp; t[j+1][i-1]) i--;<br>        else if(!t[j+1][i]) i++;<br>      }<br>      s[j][i]='r';<br>    }<br>    return ret;<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>