            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class CircuitBoard<br>{<br>  public int maxDataLines(String[] slayout)<br>  {<br>    int rmax = slayout.length;<br>    int cmax = slayout[0].length();<br>    boolean layout[][] = new boolean[rmax][cmax];<br>    for(int r=0; r&lt;rmax; r++)<br>      for(int c=0; c&lt;cmax; c++)<br>        layout[r][c] = (slayout[r].charAt(c)=='.');<br>        <br>    int ans = 0;<br>    boolean[][] dp = new boolean[rmax][cmax];<br>    for(int c=0; c&lt;cmax; c++)<br>    {<br>      if(!layout[0][c]) continue;<br> <br>      for(int r=0; r&lt;rmax; r++)<br>      for(int c2=0; c2&lt;cmax; c2++)<br>        dp[r][c2] = false;<br>      dp[0][c] = true;<br>      for(int r=0; r&lt;rmax-1; r++)<br>        for(int c2=0; c2&lt;cmax; c2++)<br>          if(dp[r][c2])<br>            for(int n=-1; n&lt;=1; n++)<br>              if(c2+n&gt;=0 &amp;&amp; c2+n&lt;cmax)<br>                dp[r+1][c2+n] = layout[r+1][c2+n];<br>      for(int c2=0; c2&lt;cmax; c2++)<br>        if(dp[rmax-1][c2])<br>        {<br>          layout[rmax-1][c2] = false;<br>          int cur = c2;<br>          ans++;<br>          for(int r=rmax-2; r&gt;=0; r--)<br>          {<br>            boolean done = false;<br>            for(int c3=Math.max(0,cur-1); c3&lt;=cur+1&amp;&amp;!done; c3++)<br>              if(dp[r][c3])<br>              {<br>                layout[r][c3] = false;<br>                cur = c3;<br>                done = true;<br>              }<br>          }<br>          break;<br>        }<br>        <br>        <br>    }<br>    <br>    return ans;<br>  }<br>}<br></td>