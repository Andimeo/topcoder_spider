<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>using System.Text;<br><br>public class InputBoxChecker<br>{<br>  bool check(long smallest, long largest, long num)<br>  {<br>    long lo = num;<br>    long hi = num;<br>    while (lo &lt;= largest)<br>    {<br>      long intersectLo = Math.Max(smallest, lo);<br>      long intersectHi = Math.Min(largest, hi);<br>      if (intersectLo &lt;= intersectHi)<br>        return true;<br>      lo = lo * 10;<br>      hi = hi * 10 + 9;<br>    }<br>    return false;<br>  }<br><br>  public string[] checkPrefix(int smallest, int largest, int[] numbers)<br>  {<br>    string[] result = new string[numbers.Length];<br>    for (int i = 0; i &lt; numbers.Length; i++)<br>    {<br>      int number = numbers[i];<br>      if (check(smallest, largest, number))<br>        result[i] = "VALID";<br>      else<br>        result[i] = "INVALID";<br>    }<br>    return result;<br>  }<br>}<br></td>