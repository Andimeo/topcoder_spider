            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class InputBoxChecker {<br>public:<br>  vector &lt;string&gt; checkPrefix(int, int, vector &lt;int&gt;);<br>};<br> <br>vector &lt;string&gt; InputBoxChecker::checkPrefix(int s, int b, vector &lt;int&gt; n) {<br>  vector&lt;string&gt; r;<br>  <br>  for(int i = 0; i &lt; n.size(); i++) {<br>    if(n[i] &gt; b)<br>      r.push_back("INVALID");<br>    else if(n[i] &gt;= s)<br>      r.push_back("VALID");<br>    else { //smaller than s<br>      int ns = s/10;<br>      int nb = b/10;<br>      while(!(ns &lt;= n[i])) { ns/=10; nb/=10; }<br>      if(n[i] &gt; nb)<br>        r.push_back("INVALID");<br>      else<br>        r.push_back("VALID");<br>    }<br>  }<br>  <br>  return r;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>