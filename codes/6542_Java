            import java.util.*;<br> <br>public class InputBoxChecker{<br>public String[] checkPrefix(int smallest, int largest, int[] numbers){<br>ArrayList&lt;String&gt; ary = new ArrayList&lt;String&gt;();<br>String[] temp = new String[0];<br>for(int i = 0; i &lt; numbers.length; i++){<br>    long num = numbers[i];<br>    long s = smallest;<br>    long l = largest;<br>    while(num &lt; s){<br>        s /= 10;<br>        l /= 10;<br>    }<br>    if(num &gt;= s &amp;&amp; num &lt;= l){<br>        ary.add("VALID");<br>    }else{<br>        ary.add("INVALID");<br>    }<br>}<br>return ary.toArray(temp);<br>}<br>}<br></td>