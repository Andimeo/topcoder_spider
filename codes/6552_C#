            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class Fractile<br>{<br>  public int fractile(int[] x, int p)<br>  {<br>    Array.Sort(x);<br>    for(int i = 0 ; i &lt; x.Length; i++){<br>      int num = 0;<br>      for(int j = 0; j &lt; x.Length; j++){<br>        if(x[i] &gt;= x[j]) num++;<br>      }<br>      if(num &gt;= x.Length * (p / 100.0)){<br>        return x[i];<br>      }<br>    }<br>    return x[x.Length - 1];<br>  }<br> <br> <br>}<br>//Perseph0nedited<br></td>