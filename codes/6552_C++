            // Cry as challange phase is passing by<br><br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br><br>#include &lt;cmath&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br><br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br><br>using namespace std;<br><br>#define FORC(it,v) for( __typeof((v).begin()) it = (v).begin(); it != (v).end(); ++it )<br><br>class Fractile {<br>public:<br>    int fractile( vector &lt;int&gt; x, int p ) {<br>        sort( x.begin(), x.end() );<br>        int n = x.size();<br><br>        FORC( it, x ) {<br>            int cnt = 0;<br>            FORC( a, x ) cnt += ( *a &lt;= * it );<br>            if( cnt*100 &gt;= n*p ) return *it;<br>        }      <br>        return -1;<br>    }<br>};<br><br><br><br><br><br><br><br><br><br>// Powered by FileEdit<br>// Powered by moj 3.0 beta 11 [modified TZTester]<br>// Powered by CodeProcessor<br></td>