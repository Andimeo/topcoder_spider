            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class Fractile<br>{<br>  public int fractile(int[] x, int p)<br>  {<br>    int n=x.length;<br>    int min=10000000;<br>    for (int i=0;i&lt;n;i++)<br>    {<br>      int dem=0;<br>      for (int j=0;j&lt;n;j++)<br>        if (x[j]&lt;=x[i]) dem++;<br>      if ((double)dem*100/n&gt;=p)<br>      {<br>        if (x[i]&lt;min) min=x[i];<br>      }<br>    }<br>    return  min;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>