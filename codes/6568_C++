            #include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cmath&gt;<br>#include &lt;queue&gt;<br> <br>using namespace std;<br> <br>class SuperSort<br>{<br>public:<br>string wordSort(string sen)<br>{<br>  int i,j,b1;<br>  vector&lt;string&gt; word;<br>  string s;<br>  s="";<br>  word.clear();<br>  for (i=0;i&lt;sen.length();i++)<br>  {<br>    if (((sen[i]&gt;='A')&amp;&amp;(sen[i]&lt;='Z'))||((sen[i]&gt;='a')&amp;&amp;(sen[i]&lt;='z')))<br>    {<br>      s+=sen[i];<br>    }<br>    else<br>    {<br>      if (s!="") word.push_back(s);<br>      s="";<br>    }<br>  }<br>  if (s!="") word.push_back(s);<br>  s="";<br>  sort(word.begin(),word.end());<br>  j=0;<br>  b1=1;<br>  for (i=0;i&lt;sen.length();i++)<br>  {<br>    if (((sen[i]&gt;='A')&amp;&amp;(sen[i]&lt;='Z'))||((sen[i]&gt;='a')&amp;&amp;(sen[i]&lt;='z')))<br>    {<br>      if (b1==1)<br>      {<br>        s+=word[j];j++;<br>      }<br>      b1=0;<br>    }<br>    else<br>    {<br>      s+=sen[i];<br>      b1=1;<br>    }<br>  }<br>  return s;<br>}<br>};<br></td>