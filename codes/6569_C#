            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class PrefixFreeSets<br>{<br>  public int maxElements(string[] words)<br>  {<br>    Array.Sort( words );<br>    int c = 0;<br>    for ( int i = 0; i &lt; words.Length; i++ ) {<br>      bool a = false;<br>      for ( int j = i + 1; j &lt; words.Length; j++ ) {<br>        if ( words[j].StartsWith( words[i] ) ) a = true;<br>      }<br>      if ( !a ) c++;<br>    }<br>    return c;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>