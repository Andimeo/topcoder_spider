            #include&lt;iostream&gt;<br>#include&lt;sstream&gt;<br>#include&lt;cstdlib&gt;<br>#include&lt;cmath&gt;<br>#include&lt;vector&gt;<br>#include&lt;string&gt;<br>#include&lt;algorithm&gt;<br>using namespace std;<br>typedef vector&lt;int&gt;VI;typedef vector&lt;VI&gt;VVI;<br>typedef vector&lt;string&gt;VS;<br>typedef pair&lt;int,int&gt;PII;<br>#define FOR(i,n) for((i)=0;(i)&lt;(n);(i)++)<br>#define BE(a) ((a).begin()),((a).end())<br>#define SI(a) ((a).size())<br>#define PB push_back<br>#define MP make_pair<br>#define FORIT(i,a) for((i)=(a).begin();(i)!=(a).end();(i)++)<br>#define CLR(a,v) memset((a),(v),sizeof(a))<br><br>struct PrefixFreeSets {<br>  int maxElements(VS a) {<br>    int ret, n, i, j;<br>    n = SI(a);<br>    ret = n;<br>    sort(BE(a));<br>    FOR (i,n-1) if (a[i+1].substr(0, SI(a[i])) == a[i]) ret--;<br>    return ret;<br>  }<br><br>  <br><br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.2 [15-Oct-2005 modified by Revenger]<br>// Powered by CodeProcessor<br></td>