            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class PrefixFreeSets<br>{<br>        public int maxElements(String[] words)<br>        {<br>                Arrays.sort(words);<br>                int count = 0;<br>                for (int i=0; i&lt;words.length; i++)<br>                {<br>                        boolean found = false;<br>                        for (int j=i+1; j&lt;words.length; j++)<br>                                if (words[j].startsWith(words[i]))<br>                                        found = true;<br>                        if (!found)<br>                                count++;<br>                }<br>                return count++;<br>        }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>