<td class="problemText" colspan="8" valign="middle" align="left">
            public class WordMath{<br>public int maximumSum(string[] s){<br>int i,j,k,l,m,n;<br>int res=0;<br>n=s.Length;<br>m=0;<br>bool b;<br>char[] x=new char[11];<br>for (i=0;i&lt;n;i++){<br>  for (j=0;j&lt;s[i].Length;j++){<br>    b=false;<br>    for (k=0;k&lt;m;k++){  <br>      if (x[k]==s[i][j]) {b=true;}<br>      }<br>    if (!b) {x[m]=s[i][j]; m++;}<br>    }<br>  }<br>int[] a=new int[m];<br>for (l=0;l&lt;m;l++){<br>  a[l]=0;<br>  for (i=0;i&lt;n;i++){<br>    k=1;<br>    for (j=s[i].Length-1;j&gt;=0;j--){<br>      if (s[i][j]==x[l]){<br>        a[l]+=k;<br>        }<br>      k=k*10;<br>      }<br>    }<br>  }<br>for (i=0;i&lt;m;i++){<br>  for (j=i+1;j&lt;m;j++){<br>    if (a[i]&lt;a[j]) {<br>      k=a[i]; a[i]=a[j]; a[j]=k;<br>      }<br>    }<br>  }<br>k=9;<br>for (i=0;i&lt;m;i++){<br>  res+=k*a[i];<br>  k--;<br>  }<br>return res;<br>}<br>}<br></td>