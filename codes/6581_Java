            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class DessertMaker<br>{<br>  public int countBananaSplits(String[] ingredients) {<br>    int numIceCream = 0;<br>    int numBananas = 0;<br>    Map&lt;String,Integer&gt; numToppings = new HashMap&lt;String,Integer&gt;();<br>    int prodToppings;<br>    for (int i = 0; i &lt; ingredients.length; i++) {<br>      if (ingredients[i].equals("ice cream")) {<br>        numIceCream++;<br>      } else if (ingredients[i].equals("banana")) {<br>        numBananas++;<br>      } else {<br>        if (numToppings.containsKey(ingredients[i])) {<br>          numToppings.put(ingredients[i], numToppings.get(ingredients[i]) + 1);<br>        } else {<br>          numToppings.put(ingredients[i], 1);<br>        }<br>      }<br>    }<br>    // How many configurations of ice cream and banana can we have?<br>    int iceCreamBananaCombos = numIceCream * numBananas;<br>    // How many configurations of toppings can we have?<br>    int toppingCombos = 1;<br>    for (String topping : numToppings.keySet()) {<br>      // We can go from 0 to the number of toppings<br>      toppingCombos *= numToppings.get(topping) + 1;<br>    }<br>    // One of the topping combos is none, which is invalid.  Remove it.<br>    toppingCombos--;<br>    // Multiply!<br>    return iceCreamBananaCombos * toppingCombos;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>