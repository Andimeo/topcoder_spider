<td class="problemText" colspan="8" valign="middle" align="left">
            public class FiveStarPatterns<br>{<br>  int m,n;<br>  String[] a =  new String[5];<br>  int[] v = new int[20];<br>  boolean[][] b = new boolean[10][20];<br>  <br>  int res;<br>  <br>  void process()<br>  {<br>    int i,j,k;<br>    int s = 0;<br>    int le,ri;<br>    <br>    for(i=0; i&lt;m; i++) for(j=0; j&lt;n; j++) b[i][j] = true;<br>    for(j=0; j&lt;n; j++) if (v[j]==1)<br>    {<br>      for(i=0; i&lt;m; i++) { if (a[i].charAt(j) != '*') return; b[i][j]=false;}<br>      s++;<br>    }<br>        <br>    for(i=0; i&lt;m; i++) for(j=0; j&lt;n; j++) if (a[i].charAt(j)=='*' &amp;&amp; b[i][j])<br>    {<br>      ri = j; while (ri&lt;n-1 &amp;&amp; a[i].charAt(ri+1)=='*') ri++;<br>      le = j; while (le&gt;0   &amp;&amp; a[i].charAt(le-1)=='*') le--;<br>      <br>      System.out.println(i + " " + j +" " +le +" " +ri);<br>      //if (s==2) System.out.println(i + " " + j +" " +le +" " +ri);<br>      <br>      if (ri-le+1&lt;5) return;<br>      for(k=j; k&lt;=Math.min(ri,j+4);k++) b[i][k] = false;<br>      s++;<br>    }<br>    <br>    if (res&gt;s) res = s;<br>  }<br>  <br>  void go(int t)<br>  {<br>    if (t==n) {process(); return;}<br>    v[t] = 1;<br>    go(t+1);<br>    v[t] = 0;<br>    go(t+1);<br>  }<br>  <br>  public<br>  int leastLines(String[] layout)<br>  {<br>    int i;<br>    m = layout.length; n = layout[0].length();<br>    for(i=0; i&lt;m; i++) a[i] = layout[i];  <br>    <br>    res = Integer.MAX_VALUE;<br>    for(i=0; i&lt;n; i++) v[i]=0;<br>    if (m&lt;5) process(); else go(0);<br>    <br>    return (res==Integer.MAX_VALUE)? -1: res;<br>  }<br>}<br></td>