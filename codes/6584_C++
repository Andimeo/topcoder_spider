            #include&lt;vector&gt;<br>#include&lt;string&gt;<br>using namespace std;<br>class EsperantoNumbers<br>{ <br>  public:<br>  string nameThisNumber(int a)<br>  { <br>    string s[]={"","unu", "du", "tri", "kvar", "kvin", "ses", "sep", "ok", "nau"};<br>    int p=a/10;<br>    int q=a%10;<br>    if(p==0)<br>    { return s[a%10];}<br>    string str="dek";<br>    if(p!=1)<br>    {str=s[p]+str;}<br>    str+=" ";<br>    str+=s[q]; <br>    int n=str.size(); <br>    if(str[n-1]==' ')<br>    {str.erase(n-1,1);}<br>    return str;<br>  }<br>};<br></td>