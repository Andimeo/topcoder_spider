<td class="problemText" colspan="8" valign="middle" align="left">
            Imports Microsoft.VisualBasic<br>Public Class EsperantoNumbers<br>    Public Function nameThisNumber(x As Integer) As String<br>        Dim EN() As String = {"zero", "unu", "du", "tri", "kvar", "kvin", "ses", "sep", "ok", "nau", "dek"}<br>        If x &lt;= 0 Then<br>            Return ""<br>        ElseIf x &lt;= 10 Then<br>            Return EN(x)<br>        ElseIf x &lt;= 19 Then<br>            Return EN(10) + " " + EN(x - 10)<br>        Else<br>            Dim i As Integer<br>            While x &gt; 9<br>                x -= 10<br>                i += 1<br>            End While<br>            If x = 0 Then<br>                Return EN(i) + EN(10)<br>            Else<br>                Return EN(i) + EN(10) + " " + EN(x)<br>            End If<br>        End If<br>    End Function<br>End Class<br></td>