            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;cstdio&gt;<br>using namespace std;<br> <br>class LuckyFives <br>{<br>  public:<br>  double probability(int n, int m) <br>  {<br>    double w=1,t[25][25];<br>    w/=m;<br>    int i,j;<br>    for(i=0;i&lt;25;i++) for(j=0;j&lt;25;j++) t[i][j]=0;<br>    t[0][0]=1;<br>    for(i=0;i&lt;n;i++) for(j=0;j&lt;=i;j++) t[i+1][j]+=(1.0-w)*t[i][j],t[i+1][j+1]+=w*t[i][j];<br>    w=0;<br>    for(i=n/5+1;i&lt;=n;i++) w+=t[n][i];<br>    return w;<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>