<td class="problemText" colspan="8" valign="middle" align="left">
            public class LuckyFives<br>{<br>  long choose(int from, int num)<br>  {<br>    long result = 1;<br>    for (int i = from; i &gt; from-num; i--)<br>    {<br>      result *= i;<br>    }<br>    for (int i = 1; i &lt;= num; i++)<br>    {<br>      result /= i;<br>    }<br>    return result;<br>  }<br>  <br>  public double probability(int dice, int sides)<br>  {<br>    int numNes = dice / 5 + 1;<br>    double result = 0.0;<br>    for (int i = numNes; i &lt;= dice; i++)<br>    {<br>      result += Math.pow(1.0/sides, i) * Math.pow(1-1.0/sides, dice-i) * choose(dice, i);<br>    }<br>    return result;<br>  }<br>}<br></td>