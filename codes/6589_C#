<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>using System.Collections;<br>using System.Text;<br><br>    public class Cannons<br>    {<br><br>        public int[] xx;<br>        public int[] yy;<br>        public int n;<br><br>        public char get(int c)<br>        {<br>            bool d,l,r,u;<br>            d = false;<br>            l = false;<br>            r = false;<br>            u = false;<br>            for (int i = 0; i &lt; n; i++)<br>            {<br>                if (xx[c] == xx[i] &amp;&amp; yy[c] &lt; yy[i]) u = true;<br>                if (xx[c] == xx[i] &amp;&amp; yy[c] &gt; yy[i]) d = true;<br>                if (yy[c] == yy[i] &amp;&amp; xx[c] &lt; xx[i]) r = true;<br>                if (yy[c] == yy[i] &amp;&amp; xx[c] &gt; xx[i]) l = true;<br>            }<br>            if (d == false) return 'D';<br>            if (l == false) return 'L';<br>            if (r == false) return 'R';<br>            if (u == false) return 'U';<br>            return 'X';<br>        }<br><br>        public String getDirections(int[] x, int[] y)<br>        {<br>            n = x.Length;<br>            xx = x;<br>            yy = y;<br>            StringBuilder sb = new StringBuilder("");<br>            for (int i = 0; i &lt; n; i++)<br>            {<br>                char c = get(i);<br>                if (c == 'X')<br>                {<br>                    return "";<br>                }<br>                else sb.Append(c);<br><br>            }<br><br>            return sb.ToString();<br><br>        }<br><br>    }<br></td>