            #include &lt;iostream&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;vector&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;string&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;sstream&gt;<br>#include &lt;functional&gt;<br>#include &lt;utility&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br><br>using namespace std; <br> <br> class Cannons { <br> public: <br>    string getDirections( vector &lt;int&gt; x, vector &lt;int&gt; y ) { <br>     string ret;<br>     int n = (int)x.size();<br><br>     for( int i = 0; i &lt; n; ++i ) {<br>        bool left = 1, right = 1, down = 1, up = 1;<br><br>        for( int j = 0; j &lt; n; ++j ) {<br>            if( i == j ) continue;<br><br>            if( x[j] &lt; x[i] &amp;&amp; y[j] == y[i] ) left = 0;<br>            if( x[j] &gt; x[i] &amp;&amp; y[j] == y[i] ) right = 0;<br><br>            if( x[j] == x[i] &amp;&amp; y[j] &lt; y[i] ) down = 0;<br>            if( x[j] == x[i] &amp;&amp; y[j] &gt; y[i] ) up = 0;<br>        }<br><br>        if( !left &amp;&amp; !right &amp;&amp; !down &amp;&amp; !up ) return "";<br>        <br>        if( down ) ret += "D";<br>        else if( left ) ret += "L";<br>        else if( right ) ret += "R";<br>        else ret += "U";<br>     }<br><br>    return ret;<br>   } <br>     <br>   <br>  <br> }; <br> <br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>