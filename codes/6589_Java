            import java.util.*;<br>import java.lang.*;<br><br>public class Cannons {<br><br>int x[];<br>int y[];<br>int n;<br><br>boolean candown(int v)<br>{<br>  int i;<br>  for (i=0; i&lt;n; i++) if (i!=v)<br>    if (x[i]==x[v] &amp;&amp; y[i]&lt;y[v]) return false;<br>  return true;<br>}<br><br>boolean canup(int v)<br>{<br>  int i;<br>  for (i=0; i&lt;n; i++) if (i!=v)<br>    if (x[i]==x[v] &amp;&amp; y[i]&gt;y[v]) return false;<br>  return true;<br>}<br><br>boolean canleft(int v)<br>{<br>  int i;<br>  for (i=0; i&lt;n; i++) if (i!=v)<br>    if (y[i]==y[v] &amp;&amp; x[i]&lt;x[v]) return false;<br>  return true;<br>}<br><br>boolean canright(int v)<br>{<br>  int i;<br>  for (i=0; i&lt;n; i++) if (i!=v)<br>    if (y[i]==y[v] &amp;&amp; x[i]&gt;x[v]) return false;<br>  return true;<br>}<br><br>public String getDirections(int[] xx, int[] yy) {<br>  x = xx; y = yy; n = x.length;<br>  int i;<br>  String ans = "";<br>  for (i=0; i&lt;n; i++)<br>  {<br>//DLRU<br><br>    if (candown(i)) { ans += "D"; continue; }<br>    if (canleft(i)) { ans += "L"; continue; }<br>    if (canright(i)) { ans += "R"; continue; }<br>    if (canup(i)) { ans += "U"; continue; }<br>    return "";<br>  }<br>  return ans;<br>}};<br></td>