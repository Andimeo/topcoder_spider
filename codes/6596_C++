<td class="problemText" colspan="8" valign="middle" align="left">
            #include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cmath&gt;<br>#include &lt;queue&gt;<br> <br>using namespace std;<br><br>struct rec<br>{<br>  int x1,y1,x2,y2,x3,y3;<br>};<br><br>class SetOfBoxes<br>{<br>public:<br>vector&lt;rec&gt; a;<br>double ss(double x1,double y1,double x2,double y2,double x3,double y3)<br>{<br>  return 0.5*fabs(x1*y2+x2*y3+x3*y1-x1*y3-x2*y1-x3*y2);<br>}<br><br>int isin(double x,double y,double x1,double y1,double x2,double y2,double x3,double y3)<br>{<br>  double r;<br>  r=ss(x,y,x1,y1,x2,y2)+ss(x,y,x1,y1,x3,y3)+ss(x,y,x2,y2,x3,y3)-ss(x1,y1,x2,y2,x3,y3);<br>  if (r&lt;1e-9) return 1;<br>  else return 0;<br>}<br><br>double countThrow(vector &lt;string&gt; boxes, int inBox)<br>{<br>  int i,j,n,p,t;<br>  double ans1,ans2;<br>  rec q;<br>  string s,s1;<br>  n=boxes.size();<br>  a.clear();<br>  for (i=0;i&lt;n;i++)<br>  {<br>    s=boxes[i];<br>    s+=" ";<br>    p=s.find_first_of('.');<br>    s1=s.substr(0,p);<br>    s.erase(0,p+1);<br>    q.x1=atoi(s1.c_str());<br>    p=s.find_first_of(' ');<br>    s1=s.substr(0,p);<br>    s.erase(0,p+1);<br>    q.y1=atoi(s1.c_str());<br><br>    p=s.find_first_of('.');<br>    s1=s.substr(0,p);<br>    s.erase(0,p+1);<br>    q.x2=atoi(s1.c_str());<br>    p=s.find_first_of(' ');<br>    s1=s.substr(0,p);<br>    s.erase(0,p+1);<br>    q.y2=atoi(s1.c_str());<br><br>    p=s.find_first_of('.');<br>    s1=s.substr(0,p);<br>    s.erase(0,p+1);<br>    q.x3=atoi(s1.c_str());<br>    p=s.find_first_of(' ');<br>    s1=s.substr(0,p);<br>    s.erase(0,p+1);<br>    q.y3=atoi(s1.c_str());<br>    a.push_back(q);<br>  }<br>  if (inBox==0) ans1=10000;<br>  else<br>  {<br>    ans1=0;<br>    for (i=0;i&lt;n;i++)<br>    {<br>      t=0;<br>      for (j=0;j&lt;n;j++)<br>        if (j!=i)<br>        {<br>          if (isin(a[i].x1,a[i].y1,a[j].x1,a[j].y1,a[j].x2,a[j].y2,a[j].x3,a[j].y3)==1) t++;<br>        }<br>      if (t==inBox-1) ans1+=ss(a[i].x1,a[i].y1,a[i].x2,a[i].y2,a[i].x3,a[i].y3);<br>    }<br>  }<br>  ans2=0;<br>  for (i=0;i&lt;n;i++)<br>  {<br>    t=0;<br>    for (j=0;j&lt;n;j++)<br>      if (j!=i)<br>      {<br>        if (isin(a[i].x1,a[i].y1,a[j].x1,a[j].y1,a[j].x2,a[j].y2,a[j].x3,a[j].y3)==1) t++;<br>      }<br>    if (t==inBox) ans2+=ss(a[i].x1,a[i].y1,a[i].x2,a[i].y2,a[i].x3,a[i].y3);<br>  }<br>  return (ans1-ans2)/10000;<br>}<br>};<br></td>