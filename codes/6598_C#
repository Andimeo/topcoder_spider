            using System;<br><br>public class Chocolate {<br>  public int minSplitNumber(int width, int height, int nTiles) {<br>    long w = width;<br>    long h = height;<br>    long t = nTiles;<br>    if (w * h == t)<br>      return 0;<br>    if (w * h &lt; t)<br>      return -1;<br>    if (t % w == 0 || t % h == 0)<br>      return 1;<br>    for (long i = 1; i * i &lt;= t; ++i)<br>      if (t % i == 0)<br>      {<br>        long j = t / i;<br>        if (i &lt; w &amp;&amp; j &lt; h)<br>          return 2;<br>        if (i &lt; h &amp;&amp; j &lt; w)<br>          return 2;<br>      }<br>    return -1;<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>