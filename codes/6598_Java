            import java.util.*;<br><br>public class Chocolate {<br>  public int minSplitNumber(int width, int height, int nTiles) {<br>    int res = Integer.MAX_VALUE;<br>    for (int i=1; i&lt;=100000; i++)<br>      if (nTiles % i == 0) {<br>        int c = 0;<br>        if (width != i)<br>          c++;<br>        if (height != nTiles/i)<br>          c++;<br>        if (c&lt;res &amp;&amp; width &gt;= i &amp;&amp; height &gt;= nTiles/i)<br>          res = c;<br><br>        c = 0;<br>        if (height != i)<br>          c++;<br>        if (width != nTiles/i)<br>          c++;<br>        if (c&lt;res &amp;&amp; height &gt;= i &amp;&amp; width &gt;= nTiles/i)<br>          res = c;<br>      }<br>    if (res == Integer.MAX_VALUE)<br>      res = -1;<br>    return res;<br>  }<br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>