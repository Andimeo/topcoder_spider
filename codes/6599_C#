            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class SimpleCompressor<br>{<br>    public string uncompress(string toUncompress)<br>  {<br>        string ans = "";<br> <br>        for(int i = 0; i &lt; toUncompress.Length; i++)<br>        {<br>            if(toUncompress[i] == '[')<br>            {<br>                int cur = i + 1;<br>                string rpt = "";<br>                int num = toUncompress[cur] - '0';<br>                int bal = 1;<br>                int balEnd = cur;<br>                for (; balEnd &lt; toUncompress.Length &amp;&amp; bal != 0; balEnd++)<br>                {<br>                    if (toUncompress[balEnd] == '[') bal++;<br>                    else if (toUncompress[balEnd] == ']') bal--;<br>                    if(bal == 0) break;<br>                }<br> <br>                rpt = uncompress(toUncompress.Substring(cur + 1, balEnd - cur - 1));<br>                i = balEnd;<br>                for (int j = 0; j &lt; num; j++)<br>                {<br>                    ans += rpt;<br>                }<br>            }<br>            else<br>            {<br>                ans += toUncompress[i];<br>            }<br>        }<br>        <br>        return ans;<br>  }<br> <br> <br>}<br>//Perseph0nedited<br></td>