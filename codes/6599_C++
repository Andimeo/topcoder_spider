            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;sstream&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;iostream&gt;<br> <br>using namespace std;<br> <br>typedef long long llong;<br>typedef long double ldbl;<br> <br>class SimpleCompressor<br>{<br>public:  string uncompress( string S )<br>  {<br>    string Res = "";<br>    for (int i = 0; i &lt; (int)S.length(); i++)<br>      if (S[i] != '[')<br>        Res += S[i];<br>      else<br>      {<br>        string Temp = "";<br>        int Cnt = 0, Rep = -1;<br>        for (i++; S[i] != ']' || Cnt; i++)<br>        {<br>          if (S[i] == '[')<br>            Cnt++;<br>          else if (S[i] == ']')<br>            Cnt--;<br>          if (Rep + 1)<br>            Temp += S[i];<br>          else<br>            Rep = S[i] - '0';<br>        }<br>        Temp = uncompress(Temp);<br>        for (int j = 0; j &lt; Rep; j++)<br>          Res += Temp;<br>      }<br>    return Res;<br>  }<br>};<br> <br> <br>// Powered by PopsEdit<br></td>