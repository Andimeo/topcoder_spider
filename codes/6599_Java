<td class="problemText" colspan="8" valign="middle" align="left">
            public class SimpleCompressor<br>{<br>public String uncompress(String in)<br>{<br>StringBuffer out = new StringBuffer();<br>for (int i=0;i&lt;in.length();i++)<br>{<br>char c = in.charAt(i);<br>if (c&gt;='A' &amp;&amp; c&lt;='Z')<br>{<br>out.append (c);<br>}<br>else if (c=='[')<br>{<br>int n = in.charAt(i+1)-'0';<br>int en = i+1;<br>int br = 1;<br>for (;br&gt;0;en++)<br>{<br>if (in.charAt(en)=='[')<br>{<br>br++;<br>}<br>else if (in.charAt(en)==']')<br>{<br>br--;<br>}<br>}<br>String sb = in.substring(i+1,en);<br>String op = uncompress(sb);<br>for (int z=0;z&lt;n;z++)<br>  out.append (op);<br>i = en-1;<br>}<br>}<br>return out.toString();<br>}<br>}<br></td>