            using System;<br><br>public class InboxCleanup {<br>  public int fewestClicks(string messages, int low, int high) {<br>    int res = int.MaxValue;<br>    for (int k = low; k &lt;= high; ++k)<br>    {<br>      int num = 0;<br>      for (int i = 0; i &lt;= (messages.Length - 1) / k; ++i)<br>      {<br>        ++num;<br>        int na = 0;<br>        int nb = 0;<br>        for (int j = 0; j &lt; k &amp;&amp; i * k + j &lt; messages.Length; ++j)<br>          if (messages[i * k + j] == 'D')<br>            ++na;<br>          else<br>            ++nb;<br>        if (na &gt; 0)<br>        {<br>          num += Math.Min(na, nb + 1) + 1;<br>        }<br>      }<br>      --num;<br>      res = Math.Min(res, num);<br>    }<br>    return res;<br>  }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>