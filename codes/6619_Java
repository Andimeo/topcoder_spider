            import java.util.*;<br><br>public class InboxCleanup {<br>    public int fewestClicks(String messages, int low, int high) {<br>        int res = Integer.MAX_VALUE;<br>        <br>        for (int c = low; c &lt;= high; c++) {<br>          int i = 0;<br>          int r = 0;<br>          while (i &lt; messages.length()) {<br>            if (i &gt; 0)<br>              r++;<br>            <br>            int d = 0;<br>            int s = 0;<br>            for (int j = 0; j &lt; c; j++) {<br>              if (messages.charAt(i) == 'D')<br>                d++;<br>              s++;<br>              <br>              i++;<br>              if (i == messages.length())<br>                break;<br>            }<br>            if (d == 0)<br>              continue;<br>            r += Math.min(d + 1, s - d + 1 + 1);<br>          }<br>          res = Math.min(res, r);<br>        }<br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>