            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class KidsGame<br>{<br>  public int kthKid(int n, int m, int k)<br>  {<br>    k--;<br>    int retval = 1;<br>    int offset = -1;<br>    while (true){<br>      offset = (m+offset)%n;<br>      if (k==offset)<br>        return retval;<br>      retval++;<br>      if (k&gt;offset)<br>        k--;<br>      n--;<br>      offset--;<br>    }<br>    <br>  }<br>  <br>}<br></td>