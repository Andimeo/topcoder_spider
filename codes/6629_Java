            import java.util.*;<br> <br>public class SillySudoku {<br>    int[][] x;<br>    int[][] y;<br> <br>    int[][] b;<br> <br>    boolean isCorrect() {<br>        for (int i = 0; i &lt; 4; i++) {<br>            for (int j = 0; j &lt; 4; j++) {<br>                if (b[i][j] != 0) {<br>                    for (int k = 0; k &lt; 4; k++) {<br>                        for (int l = 0; l &lt; 4; l++) {<br>                            if (k == i || j == l || ((k / 2) == (i / 2) &amp;&amp; (j / 2 == l / 2))) {<br>                                if (!(k == i &amp;&amp; j == l) &amp;&amp; (b[i][j] == b[k][l])) {<br>                                    return false;<br>                                }<br>                            }<br>                        }<br>                    }<br>                }<br>            }<br>        }<br>        return true;<br>    }<br> <br>    int bt(int x, int y) {<br>        if (x == 4) {<br>            return isCorrect() ? 1 : 0;<br>        } else if (y == 4) {<br>            return bt(x + 1, 0);<br>        } else if (b[x][y] != 0) {<br>            return bt(x, y + 1);<br>        }<br> <br>        int r = 0;<br>        for (int i = 1; i &lt;= 4; i++) {<br>            b[x][y] = i;<br>            if (isCorrect()) {<br>                r += bt(x, y + 1);<br>            }<br>        }<br>        b[x][y]  = 0;<br>        return r;<br>    }<br> <br>    public int countWays(String[] board) {<br> <br>        b = new int[4][4];<br>        for (int i = 0; i &lt; 4; i++) {<br>            for (int j = 0; j &lt; 4; j++) {<br>                if (board[i].charAt(j) == '-') {<br>                    b[i][j] = 0;<br>                } else {<br>                    b[i][j] = board[i].charAt(j) - '0';<br>                }<br>            }<br>        }<br> <br>        return bt(0, 0);<br>    }<br> <br>}<br> <br> <br>// Powered by PopsEdit<br> <br>// Powered by CodeProcessor<br></td>