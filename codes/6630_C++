            #include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;cmath&gt;<br><br>using namespace std;<br><br>class DigitsSum  { <br>public: <br><br>  int lastDigit(int n) { <br>    if (n == 0) return 0;<br>    int a = (n % 9);<br>    if (a == 0) a = 9;<br>    return a;<br>  } <br><br>    <br> <br>}; <br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.2 [15-Oct-2005 modified by Revenger]<br>// Powered by CodeProcessor<br></td>