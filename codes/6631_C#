            using System;<br><br>public class StandInLine {<br>  public int[] reconstruct(int[] left) {<br>    int[] res = new int[left.Length];<br>    for (int i = 0; i &lt; left.Length; ++i)<br>    {<br>      int k = -1;<br>      for (int j = 0; j &lt;= left[i]; ++j)<br>      {<br>        do<br>        {<br>          ++k;<br>        } while (res[k] &gt; 0);<br>      }<br>      res[k] = i + 1;<br>    }<br>    return res;<br>  }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>