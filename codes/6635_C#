            using System;<br>using System.Collections;<br>using System.Text;<br><br>public class RosePetals <br>{<br><br>    public static void Main(String[] args)<br>    {<br>        int[] t = new int[5] { 2, 2, 2, 2, 2 };<br>        Console.Write(getScore (t));<br>    }<br><br>    public static int getScore(int[] dice)<br>    {<br>        int[] map = new int[6] { 0,0,2,0,4,0};<br>        int i;<br>        int ret = 0;<br>        for (i = 0; i &lt; dice.Length; i++)<br>            ret += map[dice[i]-1];<br>        return ret;<br>    }<br><br>}<br></td>