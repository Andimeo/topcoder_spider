<td class="problemText" colspan="8" valign="middle" align="left">
            public class Divisibility<br>{<br>  long GCD(long a, long b)<br>  {<br>    if ( a == 0 )<br>      return b;<br>    return GCD(b % a, a);<br>  }<br><br>  public int numDivisible(int L, int R, int[] a)<br>  {<br>    int N = a.length;<br>    long res = 0;<br>    --L;<br>    for ( int mask = 1; mask &lt; (1 &lt;&lt; N); ++mask )<br>    {<br>      long d = 1;<br>      for ( int i = 0; i &lt; N; ++i )<br>        if ( (mask &amp; (1 &lt;&lt; i)) &gt; 0 )<br>        {<br>          long t = GCD(d, a[i]);<br>          d *= a[i] / t;<br>          if ( d &gt; R )<br>            break;<br>        }<br><br>      int s = -1;<br>      if ( Integer.bitCount(mask) % 2 == 1 )<br>        s = 1;<br><br>      res += s * ((R / d) - (L / d));<br>    }<br>    return (int) res;<br>  }<br>}<br></td>