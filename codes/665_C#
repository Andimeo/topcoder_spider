            #include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cmath&gt;<br>#include &lt;queue&gt;<br>using namespace std;<br>typedef vector&lt;int&gt; VI;  typedef vector&lt;vector&lt;int&gt; &gt; VVI;<br>typedef vector&lt;string&gt; VS;  typedef vector&lt;vector&lt;string&gt; &gt; VVS;<br>typedef signed long long i64;  typedef unsigned long long u64;<br><br>class Bullets {<br>public:<br>int match(vector &lt;string&gt; a, string b) {<br>  int i, j, k, x, y, z, n;<br>  int ret;<br><br>  b = b+b;<br>  for( i = 0; i &lt; a.size(); i++ ) {<br>    for( j = 0; j &lt; a[i].size(); j++ )<br>      if( a[i] == b.substr(j, a[i].size()) ) break;<br>    if( j &lt; a[i].size() ) return i;<br>  }<br>  return -1;<br>}<br>};<br><br><br>// Powered by FileEdit<br></td>