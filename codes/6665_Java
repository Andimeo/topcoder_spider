            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class DerivativeSequence<br>{<br> <br>  public int[] derSeq(int[] a, int n)<br>  {<br>    if (n==0)<br>    return a;<br>    int[] b = new int[a.length-1];<br>    for (int i=0; i&lt;a.length-1;i++)<br>    {<br>      b[i]=a[i+1]-a[i];<br>    }    <br>    return derSeq(b,n-1);<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>