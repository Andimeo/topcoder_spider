            public class RoadsAndFools {<br>  String result = "NO SOLUTION ";<br>  int L;<br>  int[] FS;<br>  public String determineOrientation(int _L, int[] _FS) {<br>    L = _L; FS = _FS;<br>    go(-1,0,"");<br>    return result.substring(0,result.length()-1);<br>  }<br>  public void go(int pos, int ind, String cur) {<br>    if (ind == FS.length) {<br>      if (result.equals("NO SOLUTION ")) result = cur;  <br>      else if (!result.equals(cur)) result = "MULTIPLE SOLUTIONS ";<br>    } else {<br>      if (FS[ind] &gt; pos) go(FS[ind],ind+1,cur+FS[ind]+" ");<br>      if (L - FS[ind] &gt; pos) go (L-FS[ind],ind+1,cur+(L-FS[ind])+" ");<br>    }<br>  }<br>}<br></td>