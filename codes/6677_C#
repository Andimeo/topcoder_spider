            using System;<br>using System.Collections;<br>using System.Text;<br><br>public class BiggestRectangleEasy <br>{<br><br><br>    public static int findArea(int N)<br>    {<br>        int i;<br>        int ret = 0;<br>        for (i = 1; i &lt;= N; i++)<br>        {<br>            int a = i;<br>            int b = (N - 2 * i) / 2;<br>            if (a == 0 || b == 0)<br>                continue;<br>            if (a * b &gt; ret)<br>                ret = a * b;<br>        }<br>        return ret;<br>    }<br>}<br></td>