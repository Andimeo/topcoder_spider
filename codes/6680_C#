            using System;<br>using System.Collections;<br>using System.Text;<br><br>public class    RGBStreet <br>{<br>    public static int estimateCost(String[] houses)<br>    {<br>        int total = houses.Length;<br>        int[,] cost = new int[total, 3];<br>        int i;<br>        for (i = 0; i &lt; total; i++)<br>        {<br>            string[] t = houses[i].Split(' ');<br>            cost[i, 0] = Convert.ToInt32(t[0]);<br>            cost[i, 1] = Convert.ToInt32(t[1]);<br>            cost[i, 2] = Convert.ToInt32(t[2]);<br>        }<br>        int ret = 9999999;<br>        int[] c = new int[total];<br>        for (i = 0; i &lt; total; i++)<br>            c[i] = -1;<br>        int k = 0;<br>        while (k &gt;= 0)<br>        {<br>            while (true)<br>            {<br>                c[k]++;<br>                if (c[k] &gt; 2)<br>                    break;<br>                if (k &gt; 0 &amp;&amp; c[k] == c[k - 1])<br>                    continue;<br>                if (k == total - 1)<br>                {<br>                    int t=0;<br>                    for (i = 0; i &lt; total; i++)<br>                        t += cost[i, c[i]];<br>                    if (t &lt; ret)<br>                        ret = t;<br>                }<br>                if (k &lt; total - 1)<br>                    k++;<br>            }<br>            c[k] = -1;<br>            k--;<br>        }<br>        return ret;<br>    }<br>}<br></td>