            using System;<br><br>public class ReturnToHome {<br>  public double goHome(int X, int Y, int D, int T) {<br>    double dist = Math.Sqrt((double) X * X + (double) Y * Y);<br>    int full = (int) (dist / D) - 3;<br>    while (full * D &lt; dist)<br>      ++full;<br>    --full;<br>    double res = Math.Min(dist, full * T + (dist - full * D));<br>    if (full == 0)<br>    {<br>      res = Math.Min(Math.Min(res, T + (D - dist)), 2 * T);<br>    }<br>    else<br>      res = Math.Min(res, (full + 1) * T);<br>    return res;<br>  }<br><br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>