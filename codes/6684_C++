            #line 2 "ReturnToHome.cc"<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;stdio.h&gt;<br>#include &lt;math.h&gt;<br>#include &lt;string.h&gt;<br>#include &lt;ctype.h&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;stdlib.h&gt;<br>using namespace std;<br> <br>class ReturnToHome {<br>public:<br>    double goHome(int X, int Y, int D, int T) {<br>  double best = 1E10;<br>  double R = hypot(X, Y);<br>  for (int njumps = 0; njumps &lt;= 2000; njumps++) {<br>      double option;<br>      option = njumps * T;<br>      if (njumps == 1) {<br>    option += fabsl(R - D);<br>      } else {<br>    option += 0 &gt;? (R - D * njumps);<br>      }<br>      best &lt;?= option;<br>  }<br>  return best;<br>    }<br>};<br> <br> <br> <br>// Powered by FileEdit<br></td>