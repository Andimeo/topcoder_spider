            // Be the MAN!!!!<br>// Challenge it!!!!<br>import java.util.*;<br><br>public class ReturnToHome {<br>  public double goHome(int X, int Y, int D, int T) {<br>        double l = Math.hypot(X, Y);<br>        double d = D;<br>        double t = T;<br>        double res = l;<br>        for (int i = 1; i &lt;= 5000; i++) {<br>            double ll = d * i;<br>            double r;<br>            if (ll &lt; l) {<br>                r = t * i + (l - ll);<br>            } else if (ll &gt; l &amp;&amp; i == 1) {<br>                r = t * i + (ll - l);<br>            } else {<br>                r = t * i;<br>            }<br>            res = Math.min(res, r);<br>        }<br>        return res;<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>