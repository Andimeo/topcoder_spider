            using System;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class SimplifiedDarts<br>{<br>    static double[,] PWin;<br>    static double PP1, PP2;<br><br>    public static double getPWin(int W, int N)<br>    {<br>        if (W &lt;= 0) return 1;<br>        if (PWin[W, N] &gt;= 0) return PWin[W, N];<br>        double Res = 0;<br>        if (W &gt; N * 3) goto Finish;<br>        if (N == 1)<br>        {<br>            if (W == 3) Res = PP2;<br>            else Res = Math.Max(PP1, PP2);<br>        }<br>        else<br>        {<br>            Res = Math.Max(PP1 * getPWin(W - 2, N - 1) + (1 - PP1) * getPWin(W, N - 1),<br>                PP2 * getPWin(W - 3, N - 1) + (1 - PP2) * getPWin(W, N - 1));<br>        }<br>    Finish:<br>        PWin[W, N] = Res;<br>        return Res;<br>    }<br><br>    public static double tryToWin(int W, int N, int P1, int P2)<br>    {<br>        PWin = new double[W+1, N+1];<br>        for (int i = 0; i &lt;= W; i++)<br>            for (int j = 0; j &lt;= N; j++)<br>                PWin[i, j] = -1;<br>        PP1 = P1 / 100.0; PP2 = P2 / 100.0;<br>        return getPWin(W, N) * 100;<br>    }<br>}<br></td>