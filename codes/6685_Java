<td class="problemText" colspan="8" valign="middle" align="left">
            public class SimplifiedDarts {<br>  double[][] x;<br>  <br>  private double get(int i, int j) {<br>    if (j &lt; 0)<br>      return x[i][0];<br>    else<br>      return x[i][j];<br>  }<br><br>  public double tryToWin(int W, int N, int P1, int P2) {<br>    x = new double[N+1][W+1];<br>    double p1 = 0.01 * P1;<br>    double p2 = 0.01 * P2;<br>    <br>    for (int i=0; i&lt;=N; i++)<br>      x[i][0] = 1;<br>    for (int i=1; i&lt;=W; i++)<br>      x[0][i] = 0;<br>    for (int i=1; i&lt;=N; i++)<br>      for (int j=1; j&lt;=W; j++) {<br>        x[i][j] = Math.max(p1*get(i-1,j-2) + (1-p1)*get(i-1,j),<br>                  p2*get(i-1,j-3) + (1-p2)*get(i-1,j));<br>      }<br>    <br>    return x[N][W]*100;<br>  }<br>}<br></td>