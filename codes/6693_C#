            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class Repeat<br>{<br>  public int minLength(string text)<br>  {<br>        int MAX = 1000000000;<br>        int n = text.Length;<br>        int[,] mas = new int[n, n+1];<br> <br>        for(int i = 0; i &lt; n; i++)<br>            for(int j = 0; j &lt;= n; j++)<br>                mas[i, j] = MAX;<br>        mas[0, 0] = 1;<br>        for (int i = 0; i &lt; n-1; i++)<br>            for (int j = 0; j &lt; n; j++)<br>            {<br>                mas[i, i + 1] = Math.Min(mas[i, i + 1], mas[i, j] + 1);<br>                mas[i + 1, j] = Math.Min(mas[i + 1, j], mas[i, j] + 1);<br>                if (i &gt; j &amp;&amp; i + i - j + 1 &lt; n &amp;&amp; text.Substring(j, i - j + 1) == text.Substring(i+1, i - j + 1))<br>                    mas[i + i - j + 1, j] = Math.Min(mas[i + i - j + 1, j], mas[i, j] + 1);<br>            }<br>        int res = MAX;<br>        for (int i = 0; i &lt; n; i++)<br>            res = Math.Min(res, mas[n - 1, i]);<br>        return res;<br>    }<br> <br>    <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>