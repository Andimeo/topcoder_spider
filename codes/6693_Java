<td class="problemText" colspan="8" valign="middle" align="left">
            import java.util.*;<br><br>public class Repeat {<br>  private char[] s;<br>  private int[][] a;<br>  private int n;<br>  private int inf = 1000;<br>  <br>    public int minLength(String text){<br>      n = text.length();<br>      s = new char[n];<br>      for (int i = 0; i &lt; n; i++) {<br>        s[i] = text.charAt(i);<br>      }<br>      <br>      a = new int[n][n + 1];<br>      for (int i = 0; i &lt; n; i++) {<br>        for (int j = 0; j &lt;= n; j++) {<br>          a[i][j] = inf ;<br>        }<br>      }<br>      a[0][0] = 1;<br>      a[0][1] = 2;<br>      for (int i = 0; i &lt; n; i++) {<br>        for (int j = 0; j &lt;= i + 1; j++) {<br>          if (i + 1 &lt; n)<br>            a[i + 1][j] = Math.min(a[i + 1][j], a[i][j] + 1);<br>          <br>        if (i + 1 &lt; n)<br>          a[i][i + 1] = Math.min(a[i][i + 1], a[i][j] + 1);<br>        <br>        int t = i - j + 1;<br>        if (i + t &gt;= n)<br>          continue;<br>        <br>        boolean bad = false;<br>        for (int k = i + 1; k &lt;= i + t; k++) {<br>          if (s[k] != s[k - i + j - 1]) {<br>            bad = true;<br>            break;<br>          }<br>        }<br>        if (!bad) {<br>          a[i + t][j] = Math.min(a[i + t][j], a[i][j] + 1);<br>        }<br>        }<br>      }<br>      <br>      int result = inf;<br>      for (int j = 0; j &lt;= n; j++) {<br>        result = Math.min(result, a[n - 1][j]);<br>      }<br>      return result;<br>    }<br>}<br></td>