            public class Dragon {<br>  public double winFight(int h,int k,int pDragon,int pKnight) {<br>//    if (h &gt; k) return 0.0;<br>    double mat[][]=new double[101][101];<br>    double pD=(double)pDragon / 100.0, pK = (double)pKnight / 100.0;<br>    int i,j;<br>    for (i=0; i &lt;= 100; ++i) <br>      mat[ 0 ][ i ]=1.0;<br>    for (i=1; i &lt;= 100; ++i)<br>      for (j=1; j &lt;= 100; ++j) {<br>        mat[ i ][ j ] = mat[i-1][j-1] * pK + mat[i][j-1] * (1.0 - pK);<br>//        System.out.println(i + " " + j + " "+ mat[ i ][ j ] );<br>      }<br>//    System.out.println( mat[ h ][ k ] );<br>    for (i=1; i &lt;= 100; ++i)<br>      for (j=1; j &lt;= 100; ++j) <br>        mat[ i ][ j ] = Math.min( mat[ i ][ j ],mat[i][j-1] * pD + mat[i-1][j] * (1.0 - pD));<br>    return mat[ h ][ k ];<br>  }<br>}<br>    <br> <br>// Powered by PopsEdit<br></td>