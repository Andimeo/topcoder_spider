<td class="problemText" colspan="8" valign="middle" align="left">
            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br><br>using namespace std;<br><br>//typedef long long __int64;<br><br>class ContestSchedule<br>{<br>public:<br>  int n;<br>  int S[100],T[100],P[100],f[100];<br>  double expectedWinnings(vector &lt;string&gt; contests)<br>  {<br>    int i,j;<br>    n=contests.size();<br>    for (i=0;i&lt;n;i++)<br>      sscanf(contests[i].c_str(),"%d%d%d",&amp;S[i],&amp;T[i],&amp;P[i]);<br>    for (i=0;i&lt;n;i++)<br>      for (j=i+1;j&lt;n;j++)<br>        if (T[j]&lt;T[i])<br>        {<br>          swap(S[i],S[j]);<br>          swap(T[i],T[j]);<br>          swap(P[i],P[j]);<br>        }<br>    int result=0;<br>    for (i=0;i&lt;n;i++)<br>    {<br>      f[i]=P[i];<br>      for (j=0;j&lt;i;j++)<br>        if (S[i]&gt;=T[j] &amp;&amp; f[j]+P[i]&gt;f[i])<br>          f[i]=f[j]+P[i];<br>      if (f[i]&gt;result)<br>        result=f[i];<br>    }<br>    return double(result)/100.0;<br>  }<br>};<br></td>