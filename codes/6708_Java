            import java.util.*;<br> <br>public class ContestSchedule {<br>    public double expectedWinnings(String[] contests) {<br>        int n = contests.length;<br>        int[] s = new int[n];<br>        int[] t = new int[n];<br>        int[] p = new int[n];<br> <br>        for (int i = 0; i &lt; n; i++) {<br>            Scanner in = new Scanner(contests[i]);<br>            s[i] = in.nextInt();<br>            t[i] = in.nextInt();<br>            p[i] = in.nextInt();<br>        }<br> <br>        double[][] a = new double[n + 1][n];<br>        double best = 0;<br>        for (int i = 0; i &lt; n; i++) {<br>            a[0][i] = p[i] / 100.0;<br>            if (best &lt; a[0][i]) {<br>                best = a[0][i];<br>            }<br>        }<br> <br>        for (int i = 1; i &lt;= n; i++) {<br>            for (int j = 0; j &lt; n; j++) {<br>                for (int k = 0; k &lt; n; k++) {<br>                    if (t[k] &lt;= s[j]) {<br>                        a[i][j] = Math.max(a[i][j], a[i - 1][k] + p[j] / 100.0);<br>                        if (a[i][j] &gt; best) {<br>                            best = a[i][j];<br>                        }<br>                    }<br>                }<br>            }<br>        }<br>        <br>        return best;<br>    }<br> <br> <br>}<br> <br> <br>// Powered by PopsEdit<br> <br>// Powered by CodeProcessor<br></td>