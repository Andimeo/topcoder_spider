            using System;<br><br>public class ConstructBST {<br>  public long numInputs(int[] tree) {<br>    Array.Sort(tree);<br>    <br>    int dummy;<br>    long res;<br><br>    evaluate(tree, 1, out res, out dummy);<br>    return res;<br>  }<br><br>  private void evaluate(int[] tree, int root, out long amount, out int num)<br>  {<br>    if (Array.IndexOf(tree, root) &lt; 0)<br>    {<br>      amount = 1;<br>      num = 0;<br>    }<br>    else<br>    {<br>      long la;<br>      int ln;<br>      long ra;<br>      int rn;<br>      evaluate(tree, 2 * root, out la, out ln);<br>      evaluate(tree, 2 * root + 1, out ra, out rn);<br>      amount = la * ra * c(ln + rn, ln);<br>      num = ln + rn + 1;<br>    }<br>  }<br>  <br>  long c(long n, long k)<br>  {<br>    if (k &gt; n - k)<br>      k = n - k;<br>    long res = 1;<br>    for (int i = 0; i &lt; k; ++i)<br>      res = (res * (n - i)) / (i + 1);<br>    return res;<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>