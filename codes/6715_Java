            import java.util.*;<br>public class BusSeating {<br>  double d(double x0, double y0, double x1, double y1) {<br>    return Math.sqrt((x0-x1)*(x0-x1)+(y0-y1)*(y0-y1));<br>  }<br>  <br>  public double getArrangement(String leftRow, String rightRow) {<br>    double result = 1000;<br>    boolean[] occ = new boolean[20];<br>    for (int i=0; i&lt;10; i++) occ[i] = leftRow.charAt(i)=='X';<br>    for (int i=0; i&lt;10; i++) occ[i+10] = rightRow.charAt(i)=='X';<br>    double[] x = new double[20];<br>    double[] y = new double[20];<br>    for (int i=0; i&lt;20; i++) x[i] = i%10;<br>    for (int i=0; i&lt;20; i++) y[i] = 2*(i/10);<br>    <br>    for (int i=0; i&lt;20; i++)<br>    if (!occ[i])<br>    for (int j=i+1; j&lt;20; j++)<br>    if (!occ[j])<br>    for (int k=j+1; k&lt;20; k++)<br>    if (!occ[k])<br>    result = Math.min(result, d(x[i], y[i], x[j], y[j])+d(x[i], y[i], x[k], y[k])+d(x[k], y[k], x[j], y[j]));<br>    return result;    <br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>