            using System;<br>using System.Collections.Generic;<br><br>public class StringSquares {<br>  public int count(string s) {<br>    Dictionary&lt;string, object&gt; all = new Dictionary&lt;string, object&gt;();<br>    for (int i = 0; i &lt; s.Length; ++i)<br>      for (int j = 1; j &lt;= s.Length - i; ++j)<br>      {<br>        string t = s.Substring(i, j);<br>        if (isSquare(t))<br>          all[t] = null;<br>      }<br>    return all.Count;<br>  }<br><br>  bool isSquare(string s)<br>  {<br>    if (s.Length % 2 == 1)<br>      return false;<br>    for (int i = 0; i &lt; s.Length / 2; ++i)<br>      if (s[i] != s[i + s.Length / 2])<br>        return false;<br>    return true;<br>  }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>