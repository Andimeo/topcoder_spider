<td class="problemText" colspan="8" valign="middle" align="left">
            #include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;map&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;algorithm&gt;<br><br>using namespace std;<br><br>class StringSquares{<br>public:<br>  int count(string s)<br>  {<br>    string ans[100],a,b;<br>    int n=s.size(),i,j,t=0,k;<br>    for(i=0;i&lt;n;i++)for(j=1;i+j*2&lt;=n;j++){<br>      a=s.substr(i,j);<br>      b=s.substr(i+j,j);<br>      if(a==b){<br>        for(k=0;k&lt;t;k++)if(a==ans[k]) break;<br>        if(k==t) ans[t++]=a;<br>      }<br>    }<br>    return t;<br>  }<br>};<br></td>