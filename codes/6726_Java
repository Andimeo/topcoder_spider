<td class="problemText" colspan="8" valign="middle" align="left">
            import java.util.HashSet;<br><br>public class StringSquares {<br>  public int count(String s) {<br>    HashSet&lt;String&gt; sq = new HashSet&lt;String&gt;();<br>    int n = s.length();<br>    for (int i = 0; i &lt; n; i++) {<br>      for (int j = i + 2; j &lt;= n; j += 2) {<br>        String ss = s.substring(i, j);<br>        int l = (j - i) / 2;<br>        if (ss.startsWith(ss.substring(l))) {<br>          sq.add(ss);<br>        }<br>      }<br>    }<br>    return sq.size();<br>  }<br>}<br></td>