            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class NumbersLine <br>{<br><br>  public int getLeast(string line, int givenNumber) <br>  {<br>        string[] s = line.Split(' ');<br>        List&lt;int&gt; a = new List&lt;int&gt;();<br>        for (int i = 0; i &lt; s.Length; i++)<br>        {<br>            if (s[i].Length == 0) continue;<br>            a.Add(int.Parse(s[i]));<br>        }<br>        a.Sort();<br>        for (int i = 0; i &lt; a.Count; i++)<br>        {<br>            if (a[i] &gt; givenNumber) return a[i];<br>        }<br>        return -1;<br>  }<br><br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>