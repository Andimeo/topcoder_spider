            import java.util.*;<br>import java.lang.*;<br><br>public class NumbersLine {<br>public int getLeast(String line, int givenNumber) {<br>  int[] l = DL.tokenInt(line);<br>  int ans = INF;<br>  int i;<br>  for (i=0; i&lt;l.length; i++)<br>    if (l[i] &gt; givenNumber) ans = Math.min(ans, l[i]);<br>  if (ans==INF) return -1;<br>  return ans;<br>}<br><br>public int s2i(String s) {return Integer.parseInt(s);}<br><br>final static int INF = 1001001001;<br>DataLib DL = new DataLib();<br>class DataLib {  <br>  public String[] tokenString(String s, String delim) {<br>  StringTokenizer st = new StringTokenizer(s, delim);<br>  String[] r = new String[st.countTokens()];<br>  for (int i=0; i&lt;r.length; i++) r[i] = st.nextToken();<br>  return r;<br>  }<br>  public String[] tokenString(String s) {return tokenString(s," ");}    <br>  public int[] tokenInt(String s, String delim) {<br>  String[] sa = tokenString(s, delim);<br>  int[] r = new int[sa.length];<br>  for (int i=0; i&lt;r.length; i++) r[i] = s2i(sa[i]);<br>  return r;<br>  }<br>  public int[] tokenInt(String s) {return tokenInt(s, " ");}<br>}<br>};<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>