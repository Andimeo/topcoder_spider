            #include &lt;algorithm&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;queue&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cmath&gt;<br><br>using namespace std;<br><br>#define ii pair&lt;int,int&gt;<br><br>#ifdef __GNUC__<br>typedef long long int64;<br>#else <br>typedef __int64 int64;<br>#endif<br><br>class PoolFiller<br>{<br>public:<br>  PoolFiller()<br>  {<br>  }<br><br>  ~PoolFiller()<br>  {<br>  }<br><br>  int getCapacity(vector &lt;string&gt; layout)<br>  {<br>    vector&lt;vector&lt;int&gt; &gt; h;<br>    vector&lt;vector&lt;int&gt; &gt; w;<br>    int i,j,k;<br>    int N=layout.size();<br>    int M=layout[0].length();<br>    h.resize(N);<br>    w.resize(N);<br>    for(i=0;i&lt;N;i++)<br>    {<br>      h[i].resize(M);<br>      w[i].resize(M);<br>      for(j=0;j&lt;M;j++)<br>      {<br>        h[i][j]=layout[i][j]-'0';<br>        if (i&gt;0 &amp;&amp; j&gt;0 &amp;&amp; i&lt;N-1 &amp;&amp; j&lt;M-1)<br>          w[i][j]=10;<br>        else<br>          w[i][j]=0;<br>      }<br><br>    }<br>    int dx[4]={-1,+0,+1,+0};<br>    int dy[4]={+0,-1,+0,+1};<br><br>    bool change=true;<br>    while (change)<br>    {<br>      change=false;<br>      for(i=1;i&lt;N-1;i++)<br>        for(j=1;j&lt;M-1;j++)<br>          for(k=0;k&lt;4;k++)<br>            if((w[i][j]+h[i][j]&gt;w[i+dx[k]][j+dy[k]]+h[i+dx[k]][j+dy[k]]) &amp;&amp; (w[i][j]&gt;0))<br>            {<br>              change=true;<br>              w[i][j]=w[i+dx[k]][j+dy[k]]+h[i+dx[k]][j+dy[k]]-h[i][j];<br>              if (w[i][j]&lt;0)<br>                w[i][j]=0;<br>            }<br><br><br><br>    }<br>    int res=0;<br>    for(i=0;i&lt;N;i++)<br>      for(j=0;j&lt;M;j++)<br>        res+=w[i][j];<br>    return res;<br>  }<br>};<br></td>