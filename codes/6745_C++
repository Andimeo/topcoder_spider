            #include &lt;cstdio&gt;<br>#include &lt;ctime&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br><br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;iostream&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;numeric&gt;<br>#include &lt;set&gt;<br>#include &lt;sstream&gt;<br>#include &lt;string&gt;<br>#include &lt;utility&gt;<br>#include &lt;vector&gt;<br><br>using namespace std; <br> <br> class ThirdBuyDiscount { <br> public: <br>    double minCost( vector &lt;int&gt; prices, int discount ) { <br>        multiset&lt;int&gt; M( prices.begin(), prices.end() );<br>         double ret = 0., d = 1.*discount/100.;<br><br>    <br>        for( int i = 0; M.size(); ++i ) {<br>            if( i % 3 == 2 ) {<br>                double a = *M.rbegin();<br>                M.erase( M.find( *M.rbegin() ) );<br>                ret += a - d * a;<br>            } else {<br>                ret += *M.begin();<br>                M.erase( M.find( *M.begin() ) );<br>            }<br>        }   <br>        return ret;<br>   } <br>     <br>   <br>  <br> }; <br> <br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>