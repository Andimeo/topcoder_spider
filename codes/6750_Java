            public class FriendlySequences {<br>  public int count(int[] array) {<br>    int c = 0;<br>    for (int i=0; i&lt;array.length; i++) {<br>      for (int j=i+1; j&lt;array.length; j++) {<br>        if (fr(""+array[i],""+array[j])) {<br>          c++;<br>        } else break;<br>      }<br>    }<br>    return c;<br>  }<br>  boolean fr(String a, String b) {<br>    for (int i=0; i&lt;10; i++) {<br>      if (a.indexOf(""+i)&gt;=0 != b.indexOf(""+i)&gt;=0) return false;<br>    }<br>    return true;<br>    <br>  }<br>}<br></td>