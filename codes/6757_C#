            using System;<br>using System.Collections;<br>using System.Text;<br><br>public class KDoubleSubstrings<br>{<br>    public int n;<br>    public string s;<br><br>    public int knum(string str)<br>    {<br>        int m = str.Length / 2;<br>        int kul = 0;<br>        for (int i = 0; i &lt; m; i++)<br>            if (str[i] != str[m + i]) kul++;<br>        return kul;<br>    }<br>    <br>    public int howMuch(String[] str, int k)<br>    {<br>        n = str.Length;<br>        for (int i = 0; i &lt; n; i++)<br>            s += str[i];<br>        n = s.Length;<br>        int db=0;<br>        for (int i = 0; i &lt; n; i++)<br>        {<br>            for (int j = i; j &lt; n; j++)<br>            {<br>                string ss = s.Substring(i, j - i + 1);<br>                if (ss.Length % 2 == 0) if (knum(ss) &lt;= k) db++;<br>            }<br>        }<br>        return db;<br>    }<br>}<br><br>// --- END CODE ---<br>// code by agysejt<br></td>