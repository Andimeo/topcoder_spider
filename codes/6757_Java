            import java.util.*;<br>public class KDoubleSubstrings {<br>  public int howMuch(String[] str, int k) {<br>    int res = 0;String ss = "";<br>    for(String s : str)ss+=s;<br>    for(int len=2;len&lt;=ss.length();len+=2)<br>    {<br>     for(int i=0;i&lt;=ss.length()-len;i++)<br>     {<br>      String tmp = ss.substring(i,i+len);<br>      if(bb(tmp,k))res++;<br>     }<br>    }<br>    return res;<br>  }<br>  public boolean bb(String s,int k)<br>  {<br>   String a = s.substring(0,s.length()/2);<br>   String b = s.substring(s.length()/2,s.length());int cnt=0;<br>   for(int i=0;i&lt;a.length();i++)if(a.charAt(i)!=b.charAt(i))cnt++;<br>   return cnt&lt;=k;<br>  }<br>  public static void main(String[] args) {<br>    KDoubleSubstrings temp = new KDoubleSubstrings();<br>    //System.out.println(temp.howMuch(String[] str, int k));<br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>