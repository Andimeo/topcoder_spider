            using System;<br>public class ConstitutiveNumbers<br>{<br>    public int count(int a, int b)<br>    {<br>        int count = 0;<br>        for (int i = a; i &lt;= b; i++)<br>            if (check(i))<br>                count++;<br>        return count;<br>    }<br>    public bool check(int n)<br>    {<br>        n *= 2;<br>        for (int i = 3; i &lt;= Math.Sqrt(n); i++){<br>            if (n % i == 0) {<br>                int m = n / i + 1 - i;<br>                if (m % 2 == 0 &amp;&amp; m &gt; 0){<br>                    return true;<br>                }<br>            }<br>        }<br>        return false;<br>    }<br>}<br></td>