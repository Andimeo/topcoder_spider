            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class ConstitutiveNumbers {<br>public:<br>  int count(int, int);<br>};<br> <br>#define llong long long<br> <br>bool conseq(llong a) {<br>  for(llong j=3;j*j-j&lt;2*a;j++)<br>    {<br>      llong num=2*a-j*j-j;<br>      llong den=2*j;<br>      if(num%den==0)<br>        return true;<br>    }<br>  return false;<br>}<br> <br>int ConstitutiveNumbers::count(int A, int B) {<br>  int ret=0;<br>  for(int i=A;i&lt;=B;i++)<br>    if(conseq(i))<br>      ret++;<br>  return ret;<br>}<br></td>