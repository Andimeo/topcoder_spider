            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections.Generic;<br><br>public class RoomNumber<br>{<br>    public int numberOfSets(int roomNumber)<br>    {<br>        int[] f = new int[10];<br>        if (roomNumber == 0) f[0]++;<br>        else<br>        {<br>            while (roomNumber &gt; 0)<br>            {<br>                f[roomNumber % 10]++;<br>                roomNumber /= 10;<br>            }<br>        }<br>        int res = (f[6] + f[9] + 1) / 2;<br>        for (int i = 0; i &lt; 10; i++)<br>        {<br>            if (i != 6 &amp;&amp; i != 9) res = Math.Max(res, f[i]);<br>        }<br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>