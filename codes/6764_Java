            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class RoomNumber<br>{<br>  public int numberOfSets(int r)<br>  {<br>    String s=""+r;<br>      int c[]=new int[10];<br>      for(int i=0;i&lt;s.length();i++)<br>      {<br>          c[s.charAt(i)-'0']++;<br>      }<br>      int a=0,b=0;<br>      for(int i=0;i&lt;10;i++)<br>      {<br>             if(i==6 | i==9) continue;<br>            if(c[i]&gt;a)<br>            a=c[i];<br>       }<br>        b=(c[6]+c[9]);<br>       if(b%2==1) b=b/2+1;<br>       else b=b/2;<br>       return Math.max(a,b);<br>  }<br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>