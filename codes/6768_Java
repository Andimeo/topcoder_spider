            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class WarehouseJob<br>{<br>  String[] pr;<br>  int[] w;<br>  int[] next=new int[1&lt;&lt;17];<br>  int[] val=new int[1&lt;&lt;17];<br>  int n;<br>  <br>  int fans(int x,int h)<br>  {<br>    if(x==(1&lt;&lt;n)-1)return 0;<br>    if(val[x]&gt;0)return val[x];<br>    int i,j,v,mv=-1,r=0;<br>    for(i=0;i&lt;n;i++)<br>    {<br>      if((x&amp;(1&lt;&lt;i))!=0)continue;<br>      for(j=0;j&lt;n;j++)<br>      {<br>        if(pr[i].charAt(j)=='1'&amp;&amp;(x&amp;(1&lt;&lt;j))==0)break;<br>      }<br>      if(j&lt;n)continue;<br>      v=h*w[i]+fans(x|(1&lt;&lt;i),h+1);<br>      if(v&lt;mv||mv&lt;0)<br>      {<br>        mv=v;<br>        r=i;<br>      }<br>    }<br>    next[x]=r;<br>    val[x]=mv;<br>    return mv;<br>  }<br>  <br>  public int[] stackBoxes(int[] weight, String[] above)<br>  {<br>    n=weight.length;<br>    w=weight;<br>    pr=above;<br>    fans(0,1);<br>    int i,v=0;<br>    int[] ar=new int[n];<br>    for(i=0;i&lt;n;i++){<br>      ar[i]=next[v];<br>      v|=(1&lt;&lt;ar[i]);<br>    }<br>    return ar;  <br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>