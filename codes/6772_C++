            #include &lt;algorithm&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>using namespace std;<br>const int MAXL = 1000;<br>char buff[MAXL+1];<br><br><br>class ApplePie {<br>public:<br>    int getApples( vector &lt;int&gt; x, vector &lt;int&gt; y, vector &lt;int&gt; day, int howMany ) {<br>        int jos = howMany;<br>        for( int i = 0; i &lt; ( int )x.size(); ++i ) {<br>            if ( x[i] &gt;= 0 &amp;&amp; x[i] &lt;= 100 &amp;&amp; y[i] &gt;= 0 &amp;&amp; y[i] &lt;= 100 )<br>                --jos;<br>            if ( jos == 0 ) <br>                return day[i];<br>        }<br>        return -1;<br>    }<br>};<br><br><br><br><br><br><br><br><br><br>// Powered by FileEdit<br>// Powered by moj 3.0 beta 11 [modified TZTester]<br>// Powered by CodeProcessor<br></td>