            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class BComputation<br>{<br>    public static long gcd(long a, long b)<br>    {<br>        if (a &lt; 0) return gcd(-a, b);<br>        if (b &lt; 0) return gcd(a, -b);<br>        Console.WriteLine(a + " " + b);<br>        if (a &lt; b) return gcd(b, a);<br>        else if (b == 0) return a;<br>        else return gcd(b, a % b);<br>    }<br>    class pp<br>    {<br>        public long x, y;<br> <br>        public static pp minus(pp p1, pp p2)<br>        {<br>            pp t = new pp();<br>            long xx = p1.x * p2.y - p1.y * p2.x;<br>            long yy = p1.y * p2.y;<br>            long c = gcd(xx, yy);<br>            xx /= c;<br>            yy /= c;<br>            t.x = xx;<br>            t.y = yy;<br>            return t;<br>        }<br>        public static pp mul(pp p1, pp p2)<br>        {<br>            pp t = new pp();<br> <br>            long xx = p1.x * p2.x;<br>            long yy = p1.y * p2.y;<br>            long c = gcd(xx, yy);<br>            xx /= c;<br>            yy /= c;<br>            t.x = xx;<br>            t.y = yy;<br>            return t;<br>        }<br>        public static pp div(pp p1, pp p2)<br>        {<br>            pp t = new pp();<br> <br>            long xx = p1.x * p2.y;<br>            long yy = p1.y * p2.x;<br>            long c = gcd(xx, yy);<br>            xx /= c;<br>            yy /= c;<br>            t.x = xx;<br>            t.y = yy;<br>            return t;<br>        }<br> <br>    }<br>    public string getValue(int B0, int pos)<br>    {<br>        long[,] C = new long[20, 20];<br>        int i, j;<br>        C[0, 0] = 1;<br>        for (i = 1; i &lt; 20; i++)<br>        {<br>            C[i, 0] = 1;<br>            for (j = 1; j &lt;= i; j++)<br>                C[i, j] = C[i - 1, j] + C[i - 1, j - 1];<br>        }<br> <br>        pp[] a = new pp[30];<br>        a[0] = new pp();<br>        a[0].x = B0;<br>        a[0].y = 1;<br>        for (i = 1; i &lt;= pos; i++)<br>        {<br>            Console.WriteLine(i);<br>            pp t = new pp();<br>            t.x = 0; t.y = 1;<br> <br>            pp t2 = new pp();<br> <br> <br>            for (j = 0; j &lt; i; j++)<br>            {<br>                t2.x = C[i + 1, j];<br>                t2.y = 1;<br>                t = pp.minus(t, pp.mul(a[j], t2));<br>            }<br> <br>            t2.x = C[i + 1, i];<br>            t2.y = 1;<br> <br> <br>            a[i] = pp.div(t, t2);<br>            Console.WriteLine(a[i].x + " " + a[i].y);<br> <br>        }<br>        return a[pos].x + "/" + a[pos].y;<br>    }<br>}<br></td>