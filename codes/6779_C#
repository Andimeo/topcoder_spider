            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class ExtendedDominoes<br>{<br>    int[] mas;<br> <br>  public long countCycles(string[] pieces)<br>  {<br>        mas = new int[10];<br>        int m = pieces.Length;<br>        for (int i = 0; i &lt; m; i++)<br>        {<br>            mas[pieces[i][0] - '0']++;<br>            mas[pieces[i][1] - '0']++;<br>        }<br>        for (int i = 0; i &lt; 10; i++)<br>            if (mas[i] % 2 == 1)<br>                return 0;<br>        long[] mm = new long[15];<br>        mm[0] = 1;<br>        mm[2] = 1;<br>        for(int i = 4; i &lt; 15; i++)<br>            mm[i] = mm[i-2] * (i - 1);<br>        long res = 1;<br>        for (int i = 0; i &lt; 10; i++)<br>            res *= mm[mas[i]];<br>        return res;<br>  }<br> <br>    <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>