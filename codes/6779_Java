            // Be the MAN!!!!<br>// Challenge it!!!!<br>import java.util.*;<br><br>public class ExtendedDominoes {<br>  public long countCycles(String[] pieces) {<br>        int[] d = new int[10];<br>        for (String s : pieces) {<br>            d[s.charAt(0) - '0']++;<br>            d[s.charAt(1) - '0']++;<br>        }<br>        long res = 1;<br>        for (int i : d) {<br>            if (i % 2 == 1) return 0;<br>            while (i &gt; 0) {<br>                res *= (i - 1);<br>                i -= 2;<br>            }<br>        }<br>    return res;<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>