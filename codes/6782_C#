            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br><br>public class Survived {<br><br><br><br>    public double minTime(int x, int y, int V, int U) {<br>        double res;<br>        if (x == 0 &amp;&amp; y == 0) return 0;<br>        double a = x * x + y * y;<br>        double b = -2 * U * x;<br>        double c = U * U - V * V;<br>        double s = b * b - 4 * a * c;<br>        if (s &lt; 0) return -1;<br>        res = (-b + Math.Sqrt(s)) / (2 * a);<br>        Console.WriteLine(a + " " + b + " " + c);<br>        if (res &lt;= 0) return -1;<br>        return 1 / res;<br>    }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>