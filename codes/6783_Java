            import java.util.*;<br>public class MissedCall {<br>  int ans;<br>  public int waitingTime(int N, int songDuration, int delay) {<br>    boolean[] a = new boolean[10000];<br>    int u = 0;<br>    for (int i = 0; i &lt; N; i++) {<br>      for (int j = 0; j &lt; songDuration; j++) {<br>        a[u++] = false;<br>      }<br>      for (int j = 0; j &lt; 5; j++) {<br>        a[u++] = true;<br>      }<br>    }<br>    for (int i = u; i &lt; 10000; i++) {<br>      a[i] = true;<br>    }<br>    int v = 0;<br>    for (int i = 0; i &lt; 10000; i++) {<br>      if (a[v]) return v;<br>      v += delay;<br>    }<br>    return ans;<br>  }<br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>