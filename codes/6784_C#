            using System;<br><br>public class RefactorableNumber {<br>  public int count(int low, int high) {<br>    int[] numFactors = new int[high + 1];<br>    int[] factor = new int[high + 1];<br>    for (int i = 2; i &lt;= high; ++i)<br>    {<br>      if (factor[i] == 0)<br>      {<br>        factor[i] = i;<br>        for (long j = (long) i * i; j &lt;= high; j += i)<br>          if (factor[j] == 0)<br>            factor[j] = i;<br>      }<br>    }<br>    numFactors[1] = 1;<br>    for (int i = 2; i &lt;= high; ++i)<br>    {<br>      int k = factor[i];<br>      int z = i;<br>      int cnt = 1;<br>      while (z % k == 0)<br>      {<br>        z /= k;<br>        ++cnt;<br>      }<br>      numFactors[i] = cnt * numFactors[z];<br>    }<br>    int res = 0;<br>    for (int i = low; i &lt;= high; ++i)<br>      if (i % numFactors[i] == 0)<br>        ++res;<br>    return res;<br>  }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>