            import java.util.*;<br><br>public class RefactorableNumber {<br>  private int[] d;<br>  <br>    public int count(int low, int high){<br>      d = new int[2000001];<br>      for (int i = 1; i &lt; d.length; i++) {<br>        int j = i;<br>        while (j &lt; d.length) {<br>          d[j]++;<br>          j += i;<br>        }<br>      }<br>      <br>      int result = 0;<br>      for (int i = low; i &lt;= high; i++) {<br>        if (i % d[i] == 0) {<br>          result++;<br>        }<br>      }<br>      <br>      return result;<br>    }<br>}<br></td>