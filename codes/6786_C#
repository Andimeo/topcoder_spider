            using System;<br><br>public class DartThrow<br>{<br><br>    public int n;<br><br>    public double bestScore(int[] payout, int prob)<br>    {<br>        n = payout.Length;<br>        double p = prob / 100.0;<br>        double pp = (1.0 - p) / 2;<br>        double max = 0.0;<br>        double e = 0.0;<br>        e = (double)payout[0] * p + (double)payout[1] * pp + (double)payout[n - 1] * pp;<br>        if (e &gt; max) max = e;<br>        e = (double)payout[n-1] * p + (double)payout[n-2] * pp + (double)payout[0] * pp;<br>        if (e &gt; max) max = e;<br>        for (int i = 1; i &lt; n-1; i++)<br>        {<br>            e = (double)payout[i] * p + (double)payout[i-1] * pp + (double)payout[i+1] * pp;<br>            if (e &gt; max) max = e;<br>        }<br>        return max;<br>    }<br><br>}<br></td>