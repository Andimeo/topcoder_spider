            public class Diamonds {<br>  public int maxRadiusSum(String[] grid) {<br>    int hei = grid.length;<br>    int wid = grid[0].length();<br>    int[][] s = new int[hei][wid];<br>    for (int i = 0; i &lt; hei; i++) {<br>      for (int j = 0; j &lt; wid; j++) {<br>        if (grid[i].charAt(j) == '.')<br>          continue;<br>        int size = Math.min(i, j);<br>        size = Math.min(size, hei - 1 - i);<br>        size = Math.min(size, wid - 1 - j);<br>        size++;<br>        for (int ii = 0; ii &lt; hei; ii++) {<br>          for (int jj = 0; jj &lt; wid; jj++) {<br>            if (grid[ii].charAt(jj) == '.') {<br>              size = Math.min(size, Math.abs(i - ii) + Math.abs(j - jj));<br>            }<br>          }<br>        }<br>        s[i][j] = size;<br>      }<br>    }<br>    int maxs = 0;<br>    for (int i = 0; i &lt; hei; i++) {<br>      for (int j = 0; j &lt; wid; j++) {<br>        if (grid[i].charAt(j) == '.')<br>          continue;<br>        maxs = Math.max(maxs, s[i][j]);<br>        for (int ii = 0; ii &lt; hei; ii++) {<br>          for (int jj = 0; jj &lt; wid; jj++) {<br>            if (grid[ii].charAt(jj) == '.')<br>              continue;<br>            if (ii == i &amp;&amp; jj == j)<br>              continue;<br>            int cur = s[i][j] + s[ii][jj];<br>            cur = Math.min(cur, Math.abs(i - ii) + Math.abs(j - jj) + 1);<br>            maxs = Math.max(maxs, cur);<br>          }<br>        }<br>      }<br>    }<br>    return maxs;<br>  }<br>  <br>  public static void main(String[] args) {<br>    int a =new Diamonds().maxRadiusSum(<br>        new String[]<br><br>                   {".#"}<br>    );<br>    System.out.println(a);<br>  }<br>}<br></td>