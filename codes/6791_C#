            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class BisquareSums {<br>    public int getSums(int low, int high) {<br>        bool[] a = new bool[200];<br>        for (int i = 0; i &lt;= 11; i++) for (int j = 0; j &lt;= 11; j++) if (i * i + j * j &lt; 200)<br>                    a[i * i + j * j] = true;<br>        int res = 0;<br>        if (low &gt; high) {<br>            int s = low;<br>            low = high;<br>            high = s;<br>        }<br>        for (int i = low; i &lt;= high; i++) if (a[i]) res++;<br>        <br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>