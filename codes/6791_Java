            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class BisquareSums<br>{<br>  public int getSums(int low, int high)<br>  {<br>    int ret = 0;<br>    for(int i = low; i &lt;= high; i++)<br>    {<br>      boolean b = false;<br>      for(int j = 0; j &lt;= i; j++)<br>        for(int k = 0; k &lt;= i; k++)<br>          if(j*j + k*k == i) b = true;<br>      if(b) ret++;<br>    }<br>    return ret;<br>    <br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>