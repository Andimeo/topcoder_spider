            public class PostOffice<br>    {<br>        public int matchAddress(string address1, string address2)<br>        {<br>            string add1 = address1.Replace(" ", "").ToUpper() ;<br>            string add2 = address2.Replace(" ", "").ToUpper();<br><br>            if (add1 == add2)<br>                return -1;<br>            else<br>            {<br>                int i;<br>                for ( i = 0; i &lt; add1.Length &amp;&amp; i &lt; add2.Length; i++)<br>                {<br>                    if (add1[i] != add2[i])<br>                        return i;<br>                }<br>                return i;<br>            }<br>        }<br>    }<br></td>