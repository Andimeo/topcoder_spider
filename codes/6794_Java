<td class="problemText" colspan="8" valign="middle" align="left">
            public class PostOffice<br>{<br>  public int matchAddress (String inA, String inB)<br>  {<br>    String[] sp = inA.split(" ");<br>    String a = "";<br>    for (int i = 0; i &lt; sp.length; i++)<br>      a += sp[i];<br>    <br>    sp = inB.split(" ");<br>    String b = "";<br>    for (int i = 0; i &lt; sp.length; i++)<br>      b += sp[i];<br>    <br>    a = a.toLowerCase();<br>    b = b.toLowerCase();<br>    if (a.equals(b))<br>      return -1;<br>    <br>    for (int i = 0; i &lt; Math.min(a.length(), b.length()); i++)<br>      if (a.charAt(i) != b.charAt(i))<br>        return i;<br>    return Math.min(a.length(), b.length());<br>  }<br>}<br></td>