            Public Class PostOffice<br>  Public Function matchAddress(address1 As String, address2 As String) As Integer<br>    Dim RAdd1 As String, RAdd2 As String, I As Integer<br>    RAdd1 = Replace(address1," ","")<br>    RAdd2 = Replace(address2," ","")<br>    Radd1 = LCase(RAdd1): Radd2 = LCase(RAdd2)<br>    If Radd1 = RAdd2 Then Return -1: Exit Function<br>    If Len(RAdd1) &gt;= Len(Radd2) Then<br>      For I = 0 To Len(Radd2) Step 1<br>        If Mid(Radd1,I+1,1) &lt;&gt; Mid(Radd2,I+1,1) Then Return I: Exit Function<br>      Next<br>    Else<br>      For I = 0 To Len(Radd1) Step 1<br>        If Mid(Radd2,I+1,1) &lt;&gt; Mid(Radd1,I+1,1) Then Return I: Exit Function<br>      Next<br>    End If<br>  End Function<br>End Class<br></td>