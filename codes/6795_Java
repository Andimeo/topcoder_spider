            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class FixedPoint<br>{<br>  public double[] find(double s, double[] t, double r)<br>  {<br>    double t1, t0, a1, b1, c1, a2, b2, c2, x, y;<br>    t0 = t [0]; t1 = t [1];<br>    <br>    a1 = s * Math.cos (r) - 1;<br>    b1 = - s * Math.sin (r);<br>    c1 = - t0 * Math.cos (r) + t1 * Math.sin (r);<br>    a2 = s * Math.sin (r);<br>    b2 = s * Math.cos (r) - 1;<br>    c2 = - t1 * Math.cos (r) - t0 * Math.sin (r);<br>    <br>    y = (c1 * a2 - c2 * a1) / (a2 * b1 - a1 * b2);<br>    x = (c1 * b2 - c2 * b1) / (a1 * b2 - a2 * b1);<br>    double [] d = new double [2];<br>    d [0] = x; d [1] = y;<br>    return d;<br>    <br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>