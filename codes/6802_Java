            import java.util.*;<br>public class MoneyGame<br>{<br>  public int play(int[] lenny, int[] fred, int[] values)<br>  {<br>    for(int i=0;i&lt;3;i++)<br>      for(int j=i+1;j&lt;3;j++)<br>        if(values[i]&gt;values[j])<br>        {<br>          { int t = values[i]; values[i] = values[j]; values[j] = t; }<br>          { int t = lenny[i]; lenny[i] = lenny[j]; lenny[j] = t; }<br>          { int t = fred[i]; fred[i] = fred[j]; fred[j] = t; }<br>        }<br>    <br>    total = new int[3]; this.values = values;<br>    for(int i=0;i&lt;3;i++)<br>      total[i] = lenny[i]+fred[i];<br>    memo = new int[11][11][11][11][11][11];<br>    for(int[][][][][] arr:memo)<br>    for(int[][][][] arr2:arr)<br>    for(int[][][] arr3:arr2)<br>    for(int[][] arr4:arr3)<br>    for(int[] arr5:arr4)<br>      Arrays.fill(arr5, -1);<br>    return solve(lenny[0],lenny[1],lenny[2],0,0,0);<br>  }<br>  int[][][][][][] memo;<br>  int[] values,total;<br>  int solve(int a, int b, int c, int aa, int bb, int cc)<br>  {<br>    if(memo[a][b][c][aa][bb][cc]==-1)<br>    {<br>      int res = -1000000;<br>      int oa = total[0]-a-aa;<br>      int ob = total[1]-b-bb;<br>      int oc = total[2]-c-cc;<br>      if(a&gt;0)<br>        res = Math.max(res, -solve(oa,ob,oc,aa+1,bb,cc));<br>      if(b&gt;0)<br>      {<br>        for(int i=0;i*values[0]&lt;values[1] &amp;&amp; i&lt;=aa;i++)<br>          res = Math.max(res, -solve(oa,ob,oc,aa-i,bb+1,cc));<br>      }<br>      if(c&gt;0)<br>      {<br>        for(int i=0;i*values[0]&lt;values[2] &amp;&amp; i&lt;=aa;i++)<br>          for(int j=0;i*values[0]+j*values[1]&lt;values[2] &amp;&amp; j&lt;=bb;j++)<br>            res = Math.max(res, -solve(oa,ob,oc,aa-i,bb-j,cc+1));<br>      }<br>      if(a==0 &amp;&amp; b==0 &amp;&amp; c==0)<br>        res = -(oa*values[0]+ob*values[1]+oc*values[2]);<br>      //A.spr(a+":"+b+":"+c+":"+aa+":"+bb+":"+cc+":"+res);<br>      memo[a][b][c][aa][bb][cc] = res;<br>    }<br>    return memo[a][b][c][aa][bb][cc];<br>  }<br>}<br></td>