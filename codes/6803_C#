            using System;<br>using System.Collections.Generic;<br><br>public class PositiveID {<br>  public int maximumFacts(string[] suspects) {<br>    int n = suspects.Length;<br>    if (n == 1)<br>      return 0;<br>    Dictionary&lt;string, object&gt;[] data = new Dictionary&lt;string, object&gt;[n];<br>    for (int i = 0; i &lt; n; ++i)<br>    {<br>      string[] p = suspects[i].Split(',');<br>      data[i] = new Dictionary&lt;string, object&gt;();<br>      foreach (string t in p)<br>        data[i][t] = null;<br>    }<br>    int res = 0;<br>    for (int i = 0; i &lt; n; ++i)<br>      for (int j = i + 1; j &lt; n; ++j)<br>      {<br>        int cur = 0;<br>        foreach (string s in data[i].Keys)<br>          if (data[j].ContainsKey(s))<br>            ++cur;<br>        res = Math.Max(res, cur);<br>      }<br>    return res;<br>  }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>