            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class PositiveID<br>{<br>  <br>  public static int isti(String s1, String s2){<br>    String []niz1 = s1.split(",");<br>    String []niz2 = s2.split(",");<br>    int broj = 0;<br>    for (int i = 0; i &lt; niz1.length; i++) <br>      for (int j = 0; j &lt; niz2.length; j++)<br>        if (niz1[i].equals(niz2[j]))<br>          broj++;<br>    <br>    return broj;<br>  }<br>  public int maximumFacts(String[] s)<br>  {<br>    int max = 0, tren;<br>    for(int i = 0; i&lt;s.length; i++)<br>      for (int j = i+1; j&lt;s.length; j++) {<br>        tren = isti(s[i], s[j]);<br>        if (max &lt; tren)<br>          max = tren;<br>      }<br>    return max;<br>  }<br>}<br></td>