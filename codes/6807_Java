            import java.util.*;<br>import java.math.*;<br> <br> <br>public class TournamentPlan {<br> <br> <br>    public int getTravelDistance (int[] street, int[] avenue) {<br>        printf ("\n");<br> <br>        int n = street.length;<br>        Arrays.sort (street);<br>        Arrays.sort (avenue);<br> <br>        int y = street[n/2];<br>        int x = avenue[n/2];<br> <br>        int result = 0;<br> <br>        for (int i = 0; i &lt; n; i++) {<br>            result += Math.abs (street[i] - y) + Math.abs (avenue[i] - x);<br>        }<br> <br>        return result;<br> <br> <br>    }<br> <br> <br> <br>    static void printf (String f, Object... a) {<br>    }<br> <br> <br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>