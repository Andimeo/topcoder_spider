            Imports System.math<br>Imports System.Collections<br>Public Class TournamentPlan<br>    Public Function getTravelDistance(ByVal street As Integer(), ByVal avenue As Integer()) As Integer<br>        Dim N As Integer = street.Length<br>        Dim i, j As Integer<br>        Dim row As New ArrayList<br>        Dim col As New ArrayList<br>        Dim flags(1000000), flaga(1000000) As Boolean<br>        For i = 0 To N - 1<br>            Dim tmp As Integer = street(i)<br>            If Not flags(tmp) Then<br>                flags(tmp) = True<br>                row.Add(tmp)<br>            End If<br>            tmp = avenue(i)<br>            If Not flaga(tmp) Then<br>                flaga(tmp) = True<br>                col.Add(tmp)<br>            End If<br>        Next<br><br>        Dim res As Integer = Integer.MaxValue<br>        Dim k As Integer<br>        For i = 0 To row.Count - 1<br>            For j = 0 To col.Count - 1<br>                Dim curstr As Integer = CInt(row(i))<br>                Dim curave As Integer = CInt(col(j))<br>                Dim triplen As Integer = 0<br>                For k = 0 To N - 1<br>                    triplen += Abs(street(k) - curstr) + Abs(avenue(k) - curave)<br>                Next<br>                If triplen &lt; res Then<br>                    res = triplen<br>                End If<br>            Next<br>        Next<br>        Return res<br>    End Function<br>End Class<br></td>