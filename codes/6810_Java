            // Have fun challenging me!<br>import java.io.*;<br>import java.util.*;<br><br>public class CodeSet {<br>  public int numSets(int n, String favorite) {<br>    for (int i = 0; i &lt; 100; i++) {<br>      Arrays.fill(a[i], -1);<br>    }<br>    int l = favorite.length();<br>    n -= l + 1;<br>    int ans = (int)aa(l, n);<br>    if (ans == inf)<br>      ans = -1;<br>    return ans;<br>  }<br>  <br>  int ans = 0;<br>  long[][] a = new long[100][100];<br>  long inf = 1000000001;<br>  <br>  public long aa(int l, int n) {<br>    //System.out.println(l + " " + n);<br>    if (l &lt;= 0 || n &lt; 0)<br>      return 0;<br>    if (a[l][n] &gt;= 0)<br>      return a[l][n];<br>    if (n == 0) {<br>      a[l][n] = 1;<br>      return 1;<br>    }<br>    a[l][n] = aa(l - 1,n) + aa(l + 1,n - 1);<br>    if (a[l][n] &gt; inf)<br>      a[l][n] = inf;<br>    return a[l][n];<br>  }<br><br>  public static void main() {<br>    //String a = new CodeSet().numSets();<br>    //System.out.println(a);<br>  }<br>}<br></td>