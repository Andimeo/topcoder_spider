            using System;<br>using System.Collections.Generic;<br>using System.Text;<br>using System.Collections;<br><br><br>public class ProductBundling<br>{<br>    public int howManyBundles(string[] data)<br>    {<br>        bool[] visit = new bool[data[0].Length];<br>        for (int i = 0; i &lt; visit.Length; ++i)<br>        {<br>            visit[i] = false;<br>        }<br>        int cnt = 0;<br>        for (int i = 0; i &lt; data[0].Length; ++i)<br>        {<br>            if (visit[i])<br>            {<br>                continue;<br>            }<br>            ++cnt;<br>            for (int j = i + 1; j &lt; data[0].Length; ++j)<br>            {<br>                bool flag = true;<br>                for (int k = 0; k &lt; data.Length; ++k)<br>                {<br>                    if (data[k][i] != data[k][j])<br>                    {<br>                        flag = false;<br>                        break;<br>                    }<br>                }<br>                if (flag)<br>                {<br>                    visit[j] = true;<br>                }<br>            }<br>        }<br>        return cnt;<br>    }<br>}<br></td>