<td class="problemText" colspan="8" valign="middle" align="left">
            #include&lt;iostream&gt;<br>#include&lt;vector&gt;<br>#include&lt;set&gt;<br>using namespace std;<br><br>class ProductBundling<br>{<br>public:<br>  int howManyBundles(vector &lt;string&gt; data)<br>  {<br>    int i,j,len1,len2;<br>    <br>    set &lt;string&gt; s;<br>    string t;<br>    <br>    len1=data.size();<br>    len2=data[0].size();<br>    <br>    for(i=0;i&lt;len2;i++)<br>    {<br>      t="";<br>      for(j=0;j&lt;len1;j++)<br>      {<br>        t+=data[j][i];<br>      }<br>      s.insert(t);<br>    }<br>    <br>    return s.size();<br>  }<br>};<br></td>