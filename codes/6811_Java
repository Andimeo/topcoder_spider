            import java.util.*;<br>public class ProductBundling{<br>  public int howManyBundles(String[] data){<br>    String[] s=new String[data[0].length()];<br>    Arrays.fill(s,"");<br>    for(int i=0;i&lt;s.length;i++){<br>      for(int j=0;j&lt;data.length;j++)  s[i]+=data[j].charAt(i);<br>    }<br>    TreeSet&lt;String&gt; t=new TreeSet&lt;String&gt;();<br>    for(int i=0;i&lt;s.length;i++)  t.add(s[i]);<br>    return t.size();<br>  }<br>}<br></td>