<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br><br>public class IrreducibleNumber<br>{<br><br>    public int getIrreducible(int[] A)<br>    {<br>        int n = A.Length;<br>        int[] pp = new int[400];<br>        if (n == 1) pp[A[0]] = 1;<br>        if (n == 2)<br>        {<br>            pp[A[0]] = 1;<br>            pp[A[1]] = 1;<br>            pp[A[0]+A[1]] = 1;<br>        }<br>        if (n == 3)<br>        {<br>            pp[A[0]] = 1;<br>            pp[A[1]] = 1;<br>            pp[A[2]] = 1;<br>            pp[A[0] + A[1]] = 1;<br>            pp[A[2] + A[1]] = 1;<br>            pp[A[0] + A[2]] = 1;<br>            pp[A[0] + A[1]+A[2]] = 1;<br>        }<br>        for (int i = 1; i &lt; 400; i++)<br>        {<br>            if (pp[i] == 0) return i;<br>        }<br>        return 0;<br>    }<br><br>}<br></td>