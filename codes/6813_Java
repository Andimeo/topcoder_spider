            import java.util.*;<br> <br>public class IrreducibleNumber<br>{<br>   public int getIrreducible(int[] A)<br>   {<br>      boolean[] b = new boolean[50000];<br>      for(int i = 0; i &lt; A.length; i++)<br>      {<br>         b[A[i]] = true;<br>         for(int j = i+1; j &lt; A.length; j++)<br>         {<br>            b[A[i]+A[j]] = true;<br>            for(int k = j+1; k &lt; A.length; k++)<br>            {<br>               b[A[i]+A[j]+A[k]] = true;<br>            }<br>         }<br>      }<br>      <br>      for(int i = 1; i &lt; b.length; i++)<br>         if(!b[i])<br>            return i;<br>            <br>      return 0;<br>   }<br>}<br></td>