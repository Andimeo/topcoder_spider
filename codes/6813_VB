<td class="problemText" colspan="8" valign="middle" align="left">
            Public Class IrreducibleNumber<br>        Public Function getIrreducible(ByVal A As Integer()) As Integer<br>            Dim ret, i As Integer<br>            Dim len As Integer = A.Length<br>            Dim aa, bb, cc As Integer<br>            aa = A(0)<br>            If len &gt; 1 Then<br>                bb = A(1)<br>            Else<br>                bb = 0<br>            End If<br>            If len &gt; 2 Then<br>                cc = A(2)<br>            Else<br>                cc = 0<br>            End If<br>            For i = 1 To 400<br>                If i = aa Or i = bb Or i = cc Or i = aa + bb Or i = aa + cc Or i = bb + cc Or i = aa + bb + cc Then<br>                Else<br>                    Return i<br>                End If<br>            Next<br>            Return ret<br>        End Function<br>    End Class<br></td>