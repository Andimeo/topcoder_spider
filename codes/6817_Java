            import java.util.*;<br>public class BrokenElevator<br>{<br>  <br>  public int wayTime(String[] plan)<br>  {<br>    int res=0;<br>    int sx=0,sy=0,tx=0,ty=0;<br>    int m=plan.length;<br>    int n=plan[0].length();<br>    for(int i=0;i&lt;m;i++)<br>      for(int j=0;j&lt;n;j++)<br>      {<br>        if(plan[i].charAt(j)=='S')<br>        {<br>          sx=i;sy=j;<br>        }<br>        if(plan[i].charAt(j)=='F')<br>        {<br>          tx=i;ty=j;<br>        }<br>      }<br>    int down=1;<br>    if(sx&gt;tx)<br>    {<br>      int tmp=sx;sx=tx;tx=tmp;<br>      tmp=sy;sy=ty;ty=tmp;<br>      down=3;<br>    }<br>    <br>    for(int i=sx;i&lt;tx;i+=2)<br>    {<br>      int si=0;<br>      for(int j=0;j&lt;n;j++)<br>        if(plan[i+1].charAt(j)=='|')<br>          si=j;<br>      res+=down+2*Math.abs(sy-si);<br>      sy=si;<br>    }<br>    res+=2*Math.abs(sy-ty);<br>    return res;<br>  }<br>  <br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>