            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;stdio.h&gt;<br>#include &lt;math.h&gt;<br>#include &lt;stdlib.h&gt;<br>#include &lt;string.h&gt;<br>#include &lt;ctype.h&gt;<br>#include &lt;algorithm&gt;<br>using namespace std;<br><br><br>class AlmostBipartiteMatching {<br>    public:<br>    int maxMatching(int nA, int nB, vector &lt;int&gt; edgesA, vector &lt;int&gt; edgesB) {<br>  if ((nA &amp; 1) == 0 &amp;&amp; (nB &amp; 1) == 0)<br>      return nA / 2 + nB / 2;<br>  if ((nA &amp; 1) == 0 || (nB &amp; 1) == 0)<br>      return nA / 2 + nB / 2;<br>  for (int i = 0; i &lt; nA; i += 2)<br>      for (int j = 0; j &lt; nB; j += 2)<br>    for (int k = 0; k &lt; edgesA.size(); k++)<br>        if (edgesA[k] == i &amp;&amp; edgesB[k] == j)<br>      return (nA + nB) / 2;<br>  return nA / 2 + nB / 2;<br>    }<br>};<br><br><br>#line 92 "AlmostBipartiteMatching.cpp"<br><br><br>// Powered by FileEdit<br></td>