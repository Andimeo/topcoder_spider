            using System;<br>using System.Collections; <br>using System.Collections.Generic;<br><br>public class LicensePlate {<br>    public double Count(string st, int pos)<br>    {<br>        double cnt = 1;<br>        for (int i = pos+1; i &lt; st.Length; i++)<br>        {<br>            if (st[i] == 'u')<br>                cnt *= 26;<br>            if (st[i] == 'n')<br>                cnt *= 10;<br>            if (st[i] == 'a')<br>                cnt *= 36;<br>        }<br><br>        return cnt;<br>    }<br>    public string getPlate(string format, int n) <br>  {<br>        double eps = 1e-5;<br>    string res="",add="";<br>        for (int i = 0; i &lt; format.Length; i++)<br>        {<br>            add = format[i].ToString();<br>            bool find = false;<br>            if (format[i] == 'u' || format[i] == 'a' || format[i] == 'n')<br>            {<br>                if (format[i] == 'a' || format[i] == 'n')<br>                {<br>                    int k;<br>                    for(k=0;k&lt;10;k++)<br>                    {<br>                        double cnt = Count(format, i);<br>                        if (cnt - eps &lt; n)<br>                        {<br>                            n -= Convert.ToInt32(cnt);<br>                            continue;<br>                        }<br><br>                        find = true;<br>                        res += k.ToString();<br>                        break;<br>                    }<br><br>                    if (k &gt;= 10 &amp;&amp; format[i]=='n')<br>                        return "";<br>                }<br><br>                if (!find &amp;&amp; (format[i] == 'u' || format[i] == 'a'))<br>                {<br>                    int k;<br>                    for (k = 0; k &lt; 26; k++)<br>                    {<br>                        double cnt = Count(format, i);<br>                        if (cnt - eps &lt; n)<br>                        {<br>                            n -= Convert.ToInt32(cnt);<br>                            continue;<br>                        }<br><br>                        find = true;<br>                        char r=Convert.ToChar(Convert.ToInt32('A')+k);<br>                        res += r.ToString();<br>                        break;<br>                    }<br><br>                    if (k &gt;= 26)<br>                        return "";<br>                    }<br>            }<br>            if(!find)<br>                res += add;<br>        }<br>        if (n == 0)<br>            return res;<br>        else return "";<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>