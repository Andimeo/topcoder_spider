<td class="problemText" colspan="8" valign="middle" align="left">
            #include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cmath&gt;<br>#include &lt;queue&gt;<br> <br>using namespace std;<br><br>#define _int64 long long<br><br>class LicensePlate<br>{<br>public:<br>char next(char c)<br>{<br>  if (c=='9') return 'A';<br>  return c+1;<br>}<br><br>string getPlate(string format, int n)<br>{<br>  _int64 remain,tot;<br>  string s;<br>  int i,j;<br>  s=format;<br>  tot=1;<br>  for (i=0;i&lt;s.length();i++)<br>    if ((s[i]=='a')||(s[i]=='n')||(s[i]=='u'))<br>    {<br>      if (s[i]=='a') tot*=36;<br>      if (s[i]=='n') tot*=10;<br>      if (s[i]=='u') tot*=26;<br>      if (tot&gt;n) break;<br>    }<br>  if (tot&lt;=n) return "";<br>  for (i=0;i&lt;s.length();i++)<br>    if ((s[i]=='a')||(s[i]=='n')||(s[i]=='u'))<br>    {<br>      remain=1;<br>      for (j=i+1;j&lt;s.length();j++)<br>        if ((s[j]=='a')||(s[j]=='n')||(s[j]=='u'))<br>        {<br>          if (s[j]=='a') remain*=36;<br>          if (s[j]=='n') remain*=10;<br>          if (s[j]=='u') remain*=26;<br>          if (remain&gt;n) break;<br>        }<br>      if ((s[i]=='a')||(s[i]=='n')) s[i]='0';<br>      else s[i]='A';<br>      while (remain&lt;=n)<br>      {<br>        n-=remain;<br>        s[i]=next(s[i]);<br>      }<br>    }<br>  return s;<br>}<br>};<br></td>