            public class LicensePlate {<br><br>  /**<br>   * @param args<br>   */<br>  public static void main(String[] args) {<br>    // TODO Auto-generated method stub<br><br>  }<br>  <br>  public String getPlate(String format, int n){<br>    int i,j,k,len;<br>    int pos[]=new int[format.length()];<br>    int type[]=new int[format.length()];<br>    char ch[]=new char[format.length()];<br>    int size[]={26,10,36};<br>    String chs[]={"ABCDEFGHIJKLMNOPQRSTUVWXYZ","0123456789","0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZ"};<br>    String r=new String(format);<br>    len=0;<br>    for(i=0;i&lt;format.length();i++){<br>      char c=format.charAt(i);<br>      if(c=='u'){<br>        pos[len]=i;<br>        type[len]=0;<br>        len++;<br>      }<br>      if(c=='n'){<br>        pos[len]=i;<br>        type[len]=1;<br>        len++;<br>      }<br>      if(c=='a'){<br>        pos[len]=i;<br>        type[len]=2;<br>        len++;<br>      }<br>    }<br>    int last=n;<br>    for(i=len-1;i&gt;=0;i--){<br>      ch[i]=chs[type[i]].charAt(last%size[type[i]]);<br>      last=last/size[type[i]];<br>    }<br>    if(last&gt;0)<br>      return "";<br>    else{<br>      char rr[]=format.toCharArray();<br>      for(i=len-1;i&gt;=0;i--){<br>        rr[pos[i]]=ch[i];<br>      }<br>      r="";<br>      for(i=0;i&lt;format.length();i++)<br>        r=r+rr[i];<br>      return r;<br>    }<br>  }<br><br>}<br></td>