            import java.util.*;<br>public class SalePitch<br>{<br>  public String buyXGetY(String regular, String sale)<br>  {<br>    String res="";<br>    int rg=Integer.parseInt(regular.replaceAll("\\$|\\.", ""));<br>    int now=Integer.parseInt(sale.replaceAll("\\$|\\.", ""));<br>    int fac=rg-now;<br>    int gd=gcd(fac,now);<br>    res="BUY "+(now/gd)+" GET "+(fac/gd)+" FREE";<br>    return res;<br>  }<br>  public int gcd(int a, int b)<br>  {<br>    if (b == 0)<br>      return a;<br>    return gcd(b, a % b);<br>  }<br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>