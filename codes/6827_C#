            using System;<br>using System.Text;<br><br>public class FenceRepairing {<br>  public double calculateCost(string[] boards) {<br>    StringBuilder b = new StringBuilder();<br>    foreach (string bb in boards)<br>      b.Append(bb);<br>    string s = b.ToString();<br>    int n = s.Length;<br>    <br>    double[] cost = new double[n + 1];<br>    for (int i = 0; i &lt; cost.Length; ++i)<br>      cost[i] = Math.Sqrt(i);<br>    <br>    double[] best = new double[n + 1];<br>    for (int i = 1; i &lt;= n; ++i)<br>    {<br>      double cur = s[i - 1] == 'X' ? double.MaxValue : best[i - 1];<br>      for (int j = 1; j &lt;= i; ++j)<br>        cur = Math.Min(cur, best[i - j] + cost[j]);<br>      best[i] = cur;<br>    }<br>    return best[n];<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>