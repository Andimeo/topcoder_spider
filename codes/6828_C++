            #include &lt;algorithm&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cmath&gt;<br>#include &lt;iostream&gt;<br>#include &lt;map&gt;<br>#include &lt;sstream&gt;<br>#include &lt;string&gt;<br>#include &lt;utility&gt;<br>#include &lt;vector&gt;<br>using namespace std;<br>typedef long long LL;<br>long long s2ll(string s) { istringstream i(s); long long x; i&gt;&gt;x; return x; }<br> <br>map&lt;pair&lt;LL,int&gt;, LL&gt; cache;<br> <br>LL f(LL n, int k) {<br>  if(n==0) return 0;<br>  if(k==1) return n;<br>  LL &amp;res = cache[make_pair(n,k)];<br>  if(res!=0) return res;<br>  res = n;<br>  res = min(res, n%2 + f(n/2, k-1));<br>  res = min(res, n%3 + f(n/3, k-1));<br>  res = min(res, n%4 + f(n/4, k-1));<br>  res = min(res, n%5 + f(n/5, k-1));<br>  return res;<br>}<br> <br>struct NewMoneySystem {<br> <br>  // MAIN<br>  long long chooseBanknotes(string N, int K) {<br>    LL n = s2ll(N);<br>    return f(n,K);<br>  }<br> <br>};<br> <br>// Powered by FileEdit<br> <br>// Powered by TomekAI<br>// Powered by TZTester 1.01 [25-Feb-2003]<br> <br>// Powered by CodeProcessor<br></td>