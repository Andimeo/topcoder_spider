            import java.util.*;<br>import java.math.*;<br>import java.io.*;<br>import java.text.*;<br>import java.util.regex.*;<br>import static java.util.Arrays.*;<br>import static java.lang.Math.*;<br>import static java.lang.Integer.parseInt;<br>public class NewMoneySystem {<br>  public long chooseBanknotes(String N, int k) {<br>    long n = Long.parseLong(N);<br>    return doit(n,k);<br>  }<br>  HashMap&lt;State, Long&gt; memo = new HashMap&lt;State, Long&gt;();<br>  long doit(long n, int k){<br>    if(k == 1) return n;<br>    if(n == 0) return 0;<br>    State key = new State(n,k);<br>    if(memo.containsKey(key)) return memo.get(key);<br>    long ret = Long.MAX_VALUE;<br>    for(int i = 2; i &lt;= 5; i++) ret = min(ret, n%i + doit(n/i, k-1));<br>    memo.put(key, ret);<br>    return ret;<br>  }<br>  class State{<br>    long n;<br>    int k;<br>    State(long nn, int kk){ n = nn; k = kk;}<br>    public boolean equals(Object o){<br>      State s = (State)o;<br>      return s.n == n &amp;&amp; s.k == k;<br>    }<br>    public int hashCode(){<br>      return 17*k + (int)((n&lt;&lt;32) ^ n);<br>    }<br>  }<br> <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>