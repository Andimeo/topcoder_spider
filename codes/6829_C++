            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>typedef vector&lt;int&gt; vi;<br>typedef vector&lt;string&gt; vs;<br> <br>class FrogDerby {<br>public:<br>  int getPoints(vector&lt;int&gt;, vector&lt;int&gt;);<br>};<br> <br>int FrogDerby::getPoints(vector&lt;int&gt; x, vector&lt;int&gt; y) {<br>  int ans=1;<br>  for(int i=0;i&lt;x.size();i++)<br>  {<br>    int v=0;<br>    for(int j=0;j&lt;x.size();j++)<br>    {<br>      if(x[j]==0 &amp;&amp; y[j]==0)<br>      v++;<br>      else if(x[i]!=0 &amp;&amp; x[j]!=0)<br>      {<br>      long long int t1=(long long)x[i]*(long long)y[j];<br>      long long int t2=(long long)x[j]*(long long)y[i];<br>      long long int t3=(long long)x[i]*(long long)x[j];<br>      v+=(t1==t2);<br>      if(t1==t2 &amp;&amp; t3&lt;0) v--;<br>      }<br>      else if(x[i]==0 &amp;&amp; x[j]==0)<br>      {<br>        long long int t3=(long long)y[i]*(long long)y[j];<br>        v+=(t3&gt;0);<br>      }<br>    }<br>    ans&gt;?=v;<br>  }  <br>  return ans;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>