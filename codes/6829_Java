            public class FrogDerby<br>{<br>  public int getPoints(int[] x, int[] y)<br>  {<br>    int N = x.length, res = 0;<br>    for ( int i = 0; i &lt; N; ++i )<br>    {<br>      int d;<br>      <br>      if ( x[i] == 0 &amp;&amp; y[i] == 0 ) d = 1;<br>      else d = GCD(Math.abs(x[i]), Math.abs( y[i]));<br>      <br>      long x0 = x[i] / d;<br>      long y0 = y[i] / d;<br>      <br>      int m = 0; <br>      for ( int j = 0; j &lt; N; ++j )<br>      {<br>        if ( check(x0, y0, x[j], y[j]) ) ++m;      <br>      }<br>      res = Math.max(res, m);<br>    }<br>    return res;<br>  }<br><br>  private boolean check(long x0, long y0, long x1, long y1)<br>  {    <br>    if ( x0 == 0 )<br>    {<br>      if ( y0 == 0 )<br>        return x1 == 0 &amp;&amp; y1 == 0;<br>      return x1 == 0 &amp;&amp; test(y1, y0);      <br>    }<br>    if ( y0 == 0 )<br>      return y1 == 0 &amp;&amp; test(x1, x0);<br>    return test(y1, y0) &amp;&amp; test(x1, x0) &amp;&amp; y1 / y0 == x1 / x0;<br>  }<br><br>  private boolean test(long y1, long y0)<br>  {<br>    return y1 % y0 == 0 &amp;&amp; Long.signum(y1 / y0) &gt;= 0;<br>  }<br><br>  private int GCD(int a, int b)<br>  {<br>    if ( a == 0 )<br>      return b;<br>    return GCD(b % a, a);<br>  }<br>}<br></td>