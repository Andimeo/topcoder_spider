            using System;<br><br>public class Multifactorial {<br>  long INF = 1000000000000000000L;<br>  <br>  public string calcMultiFact(int n, int k) {<br>    long res = 1;<br>    for (int z = n; z &gt; 0; z -= k)<br>    {<br>      if (INF / z &lt; res)<br>        return "overflow";<br>      res *= z;<br>    }<br>    return res.ToString();<br>  }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>