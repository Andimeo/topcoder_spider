            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class Multifactorial<br>{<br>  public String calcMultiFact(int n, int k)<br>  {<br>    BigInteger prod = BigInteger.valueOf(n);<br>    while ((n = n-k) &gt; 0)<br>    {<br>      prod = prod.multiply(BigInteger.valueOf(n));<br>      if (prod.compareTo(BigInteger.valueOf(1000000000000000000L)) &gt; 0)<br>        return "overflow";<br>    }<br>    return prod.toString();<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>