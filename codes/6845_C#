            using System;<br><br>public class RPSChamps {<br>  public double numberOfMoves(int N) {<br>    double[] res = new double[N + 1];<br>    double[,] c = new double[N + 1, N + 1];<br>    c[0, 0] = 1;<br>    for (int i = 1; i &lt;= N; ++i)<br>      for (int j = 0; j &lt;= N; ++j)<br>      {<br>        if (j == 0)<br>          c[i, j] = 1;<br>        else<br>          c[i, j] = c[i - 1, j - 1] + c[i - 1, j];<br>      }<br>    <br>    res[1] = 0;<br>    double p3 = 3;<br>    for (int n = 2; n &lt;= N; ++n)<br>    {<br>      p3 *= 3;<br>      double rem = 0;<br>      double sum = 0;<br>      for (int k = 1; k &lt; n; ++k)<br>      {<br>        double cur = (c[n, k] * 3) / p3;<br>        sum += cur * (res[k] + res[n - k]);<br>        rem += cur;<br>      }<br>      res[n] = (1 + sum) / rem;<br>    }<br>    return res[N];<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>