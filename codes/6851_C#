            using System;<br><br>public class Chomp {<br>  public int moves(int N) {<br>        int[, ,] moves = new int[N + 1, N + 1, N + 1];<br>        for (int a = 0; a &lt;= N; ++a)<br>            for (int b = a; b &lt;= N; ++b)<br>                for (int c = b; c &lt;= N; ++c)<br>                {<br>                    if (c == 0)<br>                        continue;<br>                    if (a == 0 &amp;&amp; b == 0 &amp;&amp; c == 1)<br>                    {<br>                        moves[a, b, c] = -1;<br>                    }<br>                    else<br>                    {<br>                        int mxn = int.MinValue;<br>                        int mxp = int.MinValue;<br>                        for (int k = 2; k &lt;= c; ++k)<br>                        {<br>                            int z = moves[Math.Min(a, k - 1), Math.Min(b, k - 1), Math.Min(c, k - 1)];<br>                            if (z &gt; 0)<br>                                mxp = Math.Max(mxp, z);<br>                            else<br>                                mxn = Math.Max(mxn, z);<br>                        }<br>                        for (int k = 1; k &lt;= b; ++k)<br>                        {<br>                            int z = moves[Math.Min(a, k - 1), Math.Min(b, k - 1), c];<br>                            if (z &gt; 0)<br>                                mxp = Math.Max(mxp, z);<br>                            else<br>                                mxn = Math.Max(mxn, z);<br>                        }<br>                        for (int k = 1; k &lt;= a; ++k)<br>                        {<br>                            int z = moves[Math.Min(a, k - 1), b, c];<br>                            if (z &gt; 0)<br>                                mxp = Math.Max(mxp, z);<br>                            else<br>                                mxn = Math.Max(mxn, z);<br>                        }<br>                        int mx;<br>                        if (mxn &gt; int.MinValue)<br>                            mx = mxn;<br>                        else<br>                            mx = mxp;<br>                        mx = -mx;<br>                        if (mx &gt; 0)<br>                            ++mx;<br>                        else<br>                            --mx;<br>                        moves[a, b, c] = mx;<br>                    }<br>                }<br>        return moves[N, N, N];<br>  }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>