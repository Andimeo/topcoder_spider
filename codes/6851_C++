            #include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>using namespace std;<br><br>typedef vector &lt;int&gt; VI;<br>typedef vector &lt;string&gt; VS;<br>typedef double LD;<br><br>#define Abs(a) ((a)&gt;0?(a):-(a))<br>#define Sqr(a) ((a)*(a))<br>#define Min(a,b) ((a)&lt;(b)?(a):(b))<br><br>#define INF 1000000000<br>#define MAX 108<br>int a[MAX][MAX][MAX];<br><br>class Chomp<br>{<br>  void Update(int tec, int &amp;bw, int &amp;bl)<br>  {<br>    if ( tec&lt;0 )<br>    {<br>      if ( bw==INF || bw&gt;-tec+1 ) bw = -tec+1;<br>    }<br>    else<br>    {<br>      if ( bl==INF || bl&lt;tec+1 ) bl = tec+1;<br>    }<br>  }<br><br>  int Solve(int r1, int r2, int r3)<br>  {<br>    if ( r1==0 &amp;&amp; r2==0 &amp;&amp; r3==0 ) return 0;<br><br>    if ( a[r1][r2][r3]!=INF )<br>      return a[r1][r2][r3];<br><br>    int bw = INF, bl = INF;<br>    int i;<br>    for (i=1;i&lt;=r1;i++)<br>    {<br>      int tec = Solve(i-1, r2, r3);<br>      Update(tec, bw, bl);<br>    }<br><br>    for (i=1;i&lt;=r2;i++)<br>    {<br>      int tec = Solve(Min(i-1, r1), i-1, r3);<br>      Update(tec, bw, bl);<br>    }<br><br>    for (i=1;i&lt;=r3;i++)<br>    {<br>      int tec = Solve(Min(i-1, r1), Min(i-1, r2), i-1);<br>      Update(tec, bw, bl);<br>    }<br><br><br>    if ( bw!=INF )<br>      a[r1][r2][r3] = bw;<br>    else if ( bl!=INF )<br>      a[r1][r2][r3] = -bl;<br>    else printf("FU");<br><br>    return a[r1][r2][r3];<br>  }<br><br>public:<br>  int moves(int n)<br>  {<br>    int i, j, k;<br>    for (i=0;i&lt;=n;i++)<br>      for (j=0;j&lt;=n;j++)<br>        for (k=0;k&lt;=n;k++)<br>          a[i][j][k] = INF;<br><br>    return Solve(n, n, n);<br>  }<br><br>};<br></td>