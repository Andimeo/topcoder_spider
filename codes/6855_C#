            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class MarbleNecklace<br>{<br>    public string normalize(string necklace)<br>    {<br>        string neck = necklace + necklace;<br>        string ans = necklace;<br>        int n = necklace.Length;<br>        for (int i = 0; i &lt; n; i++)<br>        {<br>            string cur = "";<br>            for (int j = 0; j &lt; n; j++)<br>            {<br>                cur += neck[i + j];<br>            }<br>            if (cur.CompareTo(ans) == -1)<br>            {<br>                ans = cur;<br>            }<br> <br>            cur = "";<br>            for (int j = n - 1; j &gt;= 0; j--)<br>            {<br>                cur += neck[i + j];<br>            }<br>            if (cur.CompareTo(ans) == -1)<br>            {<br>                ans = cur;<br>            }<br> <br>        }<br> <br>        return ans;<br>    }<br> <br>}<br></td>