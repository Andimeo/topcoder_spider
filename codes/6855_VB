            Public Class MarbleNecklace<br>        Public Function normalize(ByVal necklace As String) As String<br>            Dim ret As String = necklace<br>            Dim len As Integer = necklace.Length - 1<br>            Dim i As Integer<br>            Dim str As String<br>            Dim revnecklace As String = ""<br>            Dim cc() As Char = necklace.ToCharArray<br>            For i = 0 To Math.Floor((cc.Length - 1) / 2)<br>                Dim ct As Char = cc(i)<br>                cc(i) = cc(cc.Length - 1 - i)<br>                cc(cc.Length - 1 - i) = ct<br>            Next<br>            revnecklace = "" + cc<br>            For i = 0 To len<br>                str = necklace.Substring(i) &amp; necklace.Substring(0, i)<br>                If str &lt; ret Then<br>                    ret = str<br>                End If<br>                str = revnecklace.Substring(i) &amp; revnecklace.Substring(0, i)<br>                If str &lt; ret Then<br>                    ret = str<br>                End If<br>            Next<br>            Return ret<br>        End Function<br>    End Class<br></td>