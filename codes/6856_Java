            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class LongLongNim<br>{<br>  public int numberOfWins(int maxn, int[] a)<br>  {<br>    int n = a.length;<br>    int flen = (1&lt;&lt;a[n-1])+50;<br>    byte[] f = new byte[(1&lt;&lt;a[n-1])+50];<br>    Arrays.fill(f, (byte)0);<br>    int[] s = new int[flen];<br>    Arrays.fill(s, 0);<br>    <br>    int i,j,k;<br>    f[0] = 0;<br>    int ans = 0;<br>    int l1 = a[n-1];<br>    int kkk = (1&lt;&lt;l1) - 1; <br>    int k1 = 0;<br>    for(i=1;i&lt;flen &amp;&amp; i&lt;=maxn;i++){<br>      for(j=0;j&lt;n;j++){<br>        k = i - a[j];<br>        if(k&gt;=0 &amp;&amp; f[k]==0) { f[i]=1; break; }<br>      }<br>      if(f[i]==0) ans++;<br>      <br>      k1 = (k1&lt;&lt;1) | f[i];<br>      k1 = k1 &amp; kkk;<br>      s[i] = k1;<br>      //System.out.println(s[i]);<br>    }<br>    if(i&gt;maxn) return ans;<br>    int i1 = i-1;<br>    <br>    int ans1 = 0;<br>    for(i=i-2;i&gt;=0;i--){<br>      if(f[i]==0) ans1++;<br>      if(s[i]==s[i1]) break;<br>    }<br>    <br>    int d1 = (maxn-i1)/(i1-i);<br>    ans += (d1)*ans1;<br>    int left = maxn - i1 - d1*(i1-i);<br>    for(i=i+1;left&gt;0;i++,left--){<br>      if(f[i]==0) ans++;<br>    }<br>    return ans;<br>  }<br>  <br>  public static void main(String[] args)<br>  {}<br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>