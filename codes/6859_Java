            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class Postnet<br>{<br>  public String barcode(String zipCode)<br>  {<br>    int sum = 0;<br>    String output = "H";<br>    String[] chars = { "HHLLL" ,"LLLHH" ,"LLHLH" ,"LLHHL" ,"LHLLH" ,"LHLHL" ,"LHHLL" ,"HLLLH" ,"HLLHL" ,"HLHLL" };<br>    for(int i = 0; i &lt; zipCode.length();i++){<br>      sum+= Integer.valueOf(String.valueOf(zipCode.charAt(i)));<br>      output += chars[Integer.valueOf(String.valueOf(zipCode.charAt(i)))].trim();<br>    }<br> <br>    output+= chars[(10-sum%10)%10]+"H";<br> <br> <br>    return output;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>