            using System;<br>using System.Collections.Generic;<br><br>public class ChessTraining {<br>  public string game(int[] x, int[] y) {<br>        int[,] g = new int[100, 100];<br>        for (int i = 0; i &lt; 100; ++i)<br>            for (int j = 0; j &lt; 100; ++j)<br>                if (i &gt; 0 &amp;&amp; j &gt; 0 &amp;&amp; i != j)<br>                {<br>                    bool[] w = new bool[200];<br>                    for (int z = 1; z &lt; 100; ++z)<br>                    {<br>                        if (z &lt;= i &amp;&amp; ok(i - z, j))<br>                            w[g[i - z, j]] = true;<br>                        if (z &lt;= j &amp;&amp; ok(i, j - z))<br>                            w[g[i, j - z]] = true;<br>                        if (z &lt;= i &amp;&amp; z &lt;= j &amp;&amp; ok(i - z, j - z))<br>                            w[g[i - z, j - z]] = true;<br>                    }<br>                    for (int k = 0; k &lt; 200; ++k)<br>                        if (!w[k])<br>                        {<br>                            g[i, j] = k;<br>                            break;<br>                        }<br>                }<br>        int r = 0;<br>        for (int i = 0; i &lt; x.Length; ++i)<br>        {<br>            r ^= g[x[i], y[i]];<br>            if (!ok(x[i], y[i]))<br>                return "Alice will win";<br>        }<br>        if (r == 0)<br>            return "Bob will win";<br>        else<br>            return "Alice will win";<br>  }<br><br>    bool ok(int a, int b)<br>    {<br>        return (a &gt; 0 &amp;&amp; b &gt; 0 &amp;&amp; a != b);<br>    }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>