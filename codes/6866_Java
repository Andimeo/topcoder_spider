            import java.util.*;<br> <br>public class ChessTraining {<br> <br>  public String game(int[] x, int[] y) {<br>    int[][] grundy = new int[100][100];<br>    for (int i = 1; i &lt; 100; i++) {<br>      for (int j = 1; j &lt; 100; j++) {<br>        BitSet bset = new BitSet();<br>        for (int k = 1; k &lt; i; k++) {<br>          if (i - k != j) bset.set(grundy[i - k][j]);<br>        }<br>        for (int k = 1; k &lt; j; k++) {<br>          if (i != j - k) bset.set(grundy[i][j - k]);<br>        }<br>        for (int k = 1; k &lt; i &amp;&amp; k &lt; j; k++) {<br>          if (i - k != j - k) bset.set(grundy[i - k][j - k]);<br>        }<br>        grundy[i][j] = bset.nextClearBit(0);<br>      }<br>    }<br>    int a = 0, n = x.length;<br>    for (int i = 0; i &lt; n; i++) {<br>      if (x[i] == 0 || y[i] == 0 || x[i] == y[i]) return "Alice will win";<br>      a ^= grundy[x[i]][y[i]];<br>    }<br>    if (a == 0) return "Bob will win";<br>    else return "Alice will win";<br>  }<br> <br>}<br></td>