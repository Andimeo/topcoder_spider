            import java.util.*;<br> <br>public class NiceOrUgly {<br>    public String describe(String s) {<br>        int n = s.length();<br>        boolean[][][] a = new boolean[n + 1][4][6];<br>        a[0][0][0] = true;<br> <br>        boolean ugly = false;<br>        for (int i = 0; i &lt; n; i++) {<br>            for (int j = 0; j &lt; 3; j++) {<br>                for (int k = 0; k &lt; 5; k++) {<br>                    if (a[i][j][k]) {<br>                        if (s.charAt(i) == 'A' || s.charAt(i) == 'E' || s.charAt(i) == 'I' || s.charAt(i) == 'O' || s.charAt(i) == 'U') {<br>                            a[i + 1][j + 1][0] = true;<br>                        } else if (s.charAt(i) != '?') {<br>                            a[i + 1][0][k + 1] = true;<br>                        } else {<br>                            a[i + 1][j + 1][0] = true;<br>                            a[i + 1][0][k + 1] = true;<br>                        }<br>                    }<br>                }<br>            }<br>            for (int j = 0; j &lt; 4; j++) {<br>                if (a[i + 1][j][5]) {<br>                    ugly = true;<br>                }<br>            }<br>            for (int k = 0; k &lt; 6; k++) {<br>                if (a[i + 1][3][k]) {<br>                    ugly = true;<br>                }<br>            }<br>        }<br> <br>        boolean nice = false;<br>        for (int j = 0; j &lt; 3; j++) {<br>            for (int k = 0; k &lt; 5; k++) {<br>                if (a[n][j][k]) {<br>                    nice = true;<br>                }<br>            }<br>        }<br> <br>        if (ugly &amp;&amp; nice) {<br>            return "42";<br>        } else if (ugly) {<br>            return "UGLY";<br>        } else {<br>            return "NICE";<br>        }<br> <br>    }<br> <br> <br>}<br> <br> <br>// Powered by PopsEdit<br> <br>// Powered by CodeProcessor<br></td>