            /*<br>*/<br>import java.util.*;<br>public class FunnyFence {<br>    public int getLength(String s) {<br>        int res=0;<br>      int count=1;<br>      for (int i=1; i&lt;s.length(); i++)<br>        if (s.charAt(i)==s.charAt(i-1))<br>       {<br>         if (count &gt; res) res = count;<br>        count=1;<br>       }<br>       else count++;<br>      if (count &gt; res) res = count;<br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>