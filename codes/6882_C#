            using System;<br>using System.Collections;<br>using System.Text;<br><br>  public class SwappingMarbles <br>  {<br>    public int swaptions(string c1, string c2) <br>    {<br>      int cnt = 0;<br><br>      if(c1.Length&gt;c2.Length)<br>      {<br>        for(int i=0;i&lt;c1.Length-2;i++)<br>        {<br>          if(c1[i]==c1[i+2])<br>          {<br>            for(int j=0;j&lt;c2.Length-2;j++)<br>            {<br>              if(c2[j]==c2[j+2] &amp;&amp; c2[j+1]==c1[i] &amp;&amp; c2[j]==c1[i+1])<br>                cnt++;<br>            }<br>          }<br>        }<br>      }<br>      else<br>      {<br>        for(int i=0;i&lt;c2.Length-2;i++)<br>        {<br>          if(c2[i]==c2[i+2])<br>          {<br>            for(int j=0;j&lt;c1.Length-2;j++)<br>            {<br>              if(c1[j]==c1[j+2] &amp;&amp; c1[j+1]==c2[i] &amp;&amp; c1[j]==c2[i+1])<br>                cnt++;<br>            }<br>          }<br>        }<br>      }<br>      return cnt;<br><br>    }<br><br><br>  }<br><br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>