<td class="problemText" colspan="8" valign="middle" align="left">
            public class ForestGarbage<br>{<br>  int LM = 10000;<br>  int[] xx = {-1, 0, 1, 0};<br>  int[] yy = { 0, 1, 0,-1};<br>  <br>  int m,n;<br>  String[] a = new String[50];<br>  <br>  int sx,sy,ex,ey;<br>  int[][] b = new int[50][50];<br>  <br>  int[][] d = new int[50][50];<br>  boolean[][] dx = new boolean[50][50];<br>  <br>  int[] res = new int[2];<br>  <br>  void input(String[] f)<br>  {<br>    int i;<br>    m = f.length; n = f[0].length();<br>    for(i=0; i&lt;m; i++) a[i] = f[i];<br>  }<br>  <br>  void init()<br>  {<br>    int i,j,k;<br>    int x,y;<br>    <br>    for(i=0; i&lt;m; i++) for(j=0; j&lt;n; j++) <br>    {<br>      b[i][j] = (a[i].charAt(j)=='g')? LM: 0;<br>      if (a[i].charAt(j)=='S') { sx = i; sy = j; }<br>      if (a[i].charAt(j)=='F') { ex = i; ey = j; }<br>    }<br>    for(i=0; i&lt;m; i++) for(j=0; j&lt;n; j++) if (b[i][j]==LM)<br>    {<br>      for(k=0; k&lt;4; k++) <br>      {<br>        x = i+xx[k]; y = j+yy[k]; if (x&lt;0 || y&lt;0 || x&gt;=m || y&gt;=n) continue;<br>        if (b[x][y]!=LM) b[x][y] = 1;<br>      }<br>    }<br>    b[sx][sy] = b[ex][ey] = 0;<br>  }<br>  <br>  void process()<br>  {<br>    int i,j,k;<br>    int x,y;<br>    int mm,si,sj;<br>    <br>    //System.out.println(" " + sx + " " + sy);<br>    <br>    for(i=0; i&lt;m; i++) for(j=0; j&lt;n; j++) { d[i][j] = Integer.MAX_VALUE; dx[i][j] = true; }<br>    d[sx][sy] = 0;<br>    <br>    while (true)<br>    {<br>      mm = Integer.MAX_VALUE; si = sj = 0;<br>      for(i=0; i&lt;m; i++) for(j=0; j&lt;n; j++) if (dx[i][j] &amp;&amp; mm&gt;d[i][j]) { mm = d[i][j]; si=i; sj=j; }<br>      <br>      dx[si][sj] = false;<br>      if (si==ex &amp;&amp; sj==ey)<br>      {<br>        res[0] = mm/LM; res[1] = mm%LM;<br>        return;<br>      }<br>      <br>      for(k=0; k&lt;4; k++) <br>      {<br>        x = si+xx[k]; y = sj+yy[k]; if (x&lt;0 || y&lt;0 || x&gt;=m || y&gt;=n) continue;<br>        if (d[x][y]&gt;d[si][sj]+b[x][y]) d[x][y] = d[si][sj] + b[x][y];<br>      }<br>    }<br>  }<br>  <br>  public<br>  int[] bestWay(String[] forest)<br>  {<br>    input(forest);<br>    init();<br>    process();<br>    <br>    return res;<br>  }<br>}<br></td>