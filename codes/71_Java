            public class Area {<br>   int U[][];<br>   int R[][];<br>   int fill[][];<br>   int visit[][];<br>   public int totalEnclosed(int startX, int startY, String commands) {<br>     int i;<br>     int j;<br>     U = new int[21][21];<br>     R = new int[21][21];<br>     fill = new int[21][21];<br>     visit = new int[21][21];<br>     for (i=0; i &lt;= 20; i++) { <br>       for (j=0;j&lt;=20; j++) {<br>         U[i][j] = 0;<br>         R[i][j] = 0;<br>         fill[i][j] = 0;<br>       }<br>     }<br>     int x = startX, y = startY;<br>     for (i=0;i&lt;commands.length();i++) {<br>       char c = commands.charAt(i);<br>       if (c=='U') { <br>         if (y + 10&gt;=20) return 0;<br>         U[x + 10][y + 10] = 1;<br>         y++;<br>       } else if (c=='D') { <br>         if (y+10&lt;=0) return 0;<br>         U[x+10][y-1+10] = 1;<br>         y--;<br>       } else<br>       if (c=='R') { <br>         if (x+10&gt;=20) return 0;<br>         R[x+10][y+10] = 1;<br>         x++;<br>       } else if (c=='L') { <br>         if (x+10&lt;=0) return 0;<br>         R[x-1+10][y+10] = 1;<br>         x--;<br>       }<br>     }<br>     int count = 0;<br>     for (x = 0; x &lt;= 20; x++) {<br>       for (y = 0; y&lt;=20; y++) {         <br>         for (i=0; i &lt;= 20; i++) for (j=0;j&lt;=20; j++)  visit[i][j] = 0;<br>         if (search(x, y)==0) {<br>           count++;<br>         }<br>       }<br>     }<br>     return count;<br>   }<br>   private int search(int x, int y) {<br>     if (x &lt; 0 || y &lt;0 || x &gt;= 20 || y &gt;= 20) {<br>       return 1;<br>     }<br>     if (visit[x][y]==1) return 0;<br>     visit[x][y] = 1;<br>     int d =0;<br>     if (U[x+1][y] == 0)<br>       d += search(x + 1, y);<br>     if (d &gt;0) return d;<br>     if (U[x][y] == 0)<br>       d += search(x - 1, y);<br>     if (d &gt;0) return d;<br>     if (R[x][y] == 0)<br>       d += search(x, y-1);<br>     if (d &gt;0) return d;<br>     if (R[x][y+1] == 0)<br>       d += search(x, y + 1);<br>     if (d &gt;0) return d;<br>     return d;<br>   }<br>}<br></td>