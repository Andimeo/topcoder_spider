            using System;<br>public class BoulderRace<br>    {<br>        public int winner(String[] boulders, int distance)<br>        {<br>            int ret = -1;<br>            int i = -1;<br>            int sm = int.MaxValue;<br>            foreach (string bold in boulders)<br>            {<br>                i++;<br>                int t = GetTime(bold, distance);<br>                if (t &lt; sm)<br>                {<br>                    ret = i;<br>                    sm = t;<br>                }<br>            }<br>            return ret;<br>        }<br><br>        public int GetTime(string bold, int dist)<br>        {<br>            int ret = 0;<br>            int i = -1;<br>            while (true)<br>            {<br>                i++;<br>                if (i &gt; bold.Length - 1) i = 0;<br>                dist -= Convert.ToInt32(bold[i].ToString());<br>                ret++;<br>                if (dist &lt;= 0) return ret;<br>            }<br>        }<br>    }<br></td>