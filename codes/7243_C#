            using System;<br><br>public class RailroadSeatNumeration {<br>  string badData = "BAD DATA";<br>  string ambiguous = "AMBIGUOUS";<br>  public string getInternational(int[] tickets) {<br>    string s1 = tryLocal(tickets);<br>    string s2 = tryIntern(tickets);<br>    if (s1 == badData)<br>      return s2;<br>    else if (s2 == badData)<br>      return s1;<br>    else if (s1 == s2)<br>      return s1;<br>    else<br>      return ambiguous;<br>  }<br><br>  private string tryIntern(int[] tickets)<br>  {<br>    string res = "";<br>    foreach (int x in tickets)<br>    {<br>      int a = x / 10;<br>      int b = x % 10;<br>      if (a &lt; 1 || a &gt; 9 || (b != 1 &amp;&amp; b != 3 &amp;&amp; b != 4 &amp;&amp; b != 6))<br>        return badData;<br>      res += " ";<br>      res += x;<br>    }<br>    if (res.Length &gt; 0)<br>      return res.Substring(1);<br>    else<br>      return res;<br>  }<br><br>  private string tryLocal(int[] tickets)<br>  {<br>    string res = "";<br>    foreach (int x in tickets)<br>    {<br>      if (x &lt; 1 || x &gt; 36)<br>        return badData;<br>      res += " ";<br>      res += localToIntern(x);<br>    }<br>    if (res.Length &gt; 0)<br>      return res.Substring(1);<br>    else<br>      return res;<br>  }<br><br>  private int localToIntern(int x)<br>  {<br>    int a = (x - 1) / 4 + 1;<br>    int b = (x - 1) % 4 + 1;<br>    if (b == 1)<br>      b = 1;<br>    else if (b == 2)<br>      b = 3;<br>    else if (b == 3)<br>      b = 4;<br>    else<br>      b = 6;<br>    return a * 10 + b;<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>