            import java.util.*;<br><br>public class RailroadSeatNumeration {<br>  <br>  String getDomestic(int[] a) {<br>    int[] c = new int[]{1, 3, 4, 6};<br>    String res = "";<br>    for (int i : a) {<br>      if (i &lt;= 0 || i &gt; 36) {<br>        return null;<br>      }<br>      res += ((i - 1) / 4) + 1;<br>      res += c[(i - 1) % 4];<br>      res += " ";<br>    }<br>    return res.trim();<br>  }<br>  <br>  String getInter(int[] a) {<br>    String res = "";<br>    for (int i : a) {<br>      int d = i / 10;<br>      int o = i % 10;<br>      if (!(1 &lt;= d &amp;&amp; d &lt;= 9)) {<br>        return null;<br>      }<br>      if (o != 1 &amp;&amp; o != 3 &amp;&amp; o != 4 &amp;&amp; o != 6) {<br>        return null;<br>      }<br>      res += i;<br>      res += " ";<br>    }<br>    return res.trim();<br>  }<br>  <br>    public String getInternational(int[] tickets){<br>      String s1 = getDomestic(tickets);<br>      String s2 = getInter(tickets);<br>      if (s1 == null &amp;&amp; s2 == null) {<br>        return "BAD DATA";<br>      }<br>      if (s1 != null &amp;&amp; s2 != null) {<br>        return "AMBIGUOUS";<br>      }<br>      if (s1 != null) {<br>        return s1;<br>      } else {<br>        return s2;<br>      }<br>    }<br><br>}<br></td>