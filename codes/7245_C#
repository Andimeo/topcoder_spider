            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class KnightTour <br>{<br>  public string checkTour(string[] cells) <br>  {<br>        int k = Math.Abs(cells[0][0] - cells[35][0]);<br>        int j = Math.Abs(cells[0][1] - cells[35][1]);<br>        if (k == 1 &amp;&amp; j == 2 || k == 2 &amp;&amp; j == 1)<br>        {<br>        }<br>        else<br>        {<br>            return "Invalid";<br>        }<br>        for (int i = 1; i &lt; cells.Length; i++)<br>        {<br>            k = Math.Abs(cells[i-1][0] - cells[i][0]);<br>            j = Math.Abs(cells[i-1][1] - cells[i][1]);<br>            if ((k == 1 &amp;&amp; j == 2) || (k == 2 &amp;&amp; j == 1))<br>            {<br>            }<br>            else<br>            {<br>                return "Invalid";<br>            }<br>        }<br>        for (int i = 0; i &lt; cells.Length-1; i++)<br>        {<br>            for (int h = i+1; h &lt; cells.Length; h++)<br>            {<br>                if (cells[h]==cells[i])<br>                {<br>                    return "Invalid";<br>                }<br><br>            }<br>        }<br>        return "Valid";<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by r1cs1]<br>// Powered by CodeProcessor<br></td>