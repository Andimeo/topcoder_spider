            public class KnightTour {<br><br>  boolean vis[][];<br>  <br>  boolean valid(String a, String b) {<br>    int la = a.charAt(0) - 'A';<br>    int na = a.charAt(1) - '1';<br>    int lb = b.charAt(0) - 'A';<br>    int nb = b.charAt(1) - '1';<br>    if (vis[la][na]) return false;<br>    vis[la][na] = true;<br>    int dl = Math.abs(la - lb);<br>    int dn = Math.abs(na - nb);<br>    return (dl == 1 &amp;&amp; dn == 2) || (dl == 2 &amp;&amp; dn == 1);<br>  }<br><br>  public String checkTour(String[] a) {<br>    vis = new boolean[6][6];<br>    boolean ok = true;<br>    for (int i = 0; i &lt; a.length; i++) {<br>      ok &amp;= valid(a[i], a[(i+1) % a.length]);<br>      System.out.println("Round " + i + ": " + ok);<br>    }<br>    return ok ? "Valid" : "Invalid";<br>  }<br>}<br></td>