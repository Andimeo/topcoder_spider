            import java.util.*;<br>import java.math.*;<br><br>public class VolleyballTournament{<br>  public String reconstructResults(int tt, int tthua, int st, int sthua){<br>    int[] x = new int[tt+tthua];<br>    int[] y = new int[tt+tthua];    <br>    for (int i = 0; i &lt; tthua; i++) y[i] = 3;<br>    for (int i = 0; i &lt; tt; i++) x[i+tthua] = 3;<br>    st -= tt*3;<br>    sthua -= tthua*3;<br>    if (st != tthua*2 &amp;&amp; st != tthua*2-1 &amp;&amp; st != 0 &amp;&amp; st != 1) return "AMBIGUITY";<br>    if (sthua != tt*2 &amp;&amp; sthua != tt*2-1 &amp;&amp; sthua != 0 &amp;&amp; sthua != 1) return "AMBIGUITY";<br>    if (st == 1) x[tthua-1] = 1;<br>    else if (st == tthua*2-1){<br>      for (int i = 1; i &lt; tthua; i++) x[i]=  2;<br>      x[0] = 1;<br>    }<br>    else if (st == tthua*2)<br>      for (int i = 0; i &lt; tthua; i++) x[i]=  2;<br>      <br>    if (sthua == 1) y[tthua+tt-1] = 1;<br>    else if (sthua == tt*2-1){<br>      for (int i = 1; i &lt; tt; i++) y[i+tthua]=  2;<br>      y[tthua] = 1;<br>    }<br>    else if (sthua == tt*2)<br>      for (int i = 0; i &lt; tt; i++) y[i+tthua]=  2;  <br>      <br>    String ans = "";<br>    for (int i = 0; i &lt; tt+tthua; i++){<br>      if (i != 0) ans += ",";<br>      ans += x[i]+"-"+y[i];<br>    }<br>    return ans;<br>  }<br>}<br></td>