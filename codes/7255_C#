            using System;<br><br>public class PrefixFreeSubsets {<br>  public long cantPrefFreeSubsets(string[] words) {<br>    Array.Sort(words, delegate(string a, string b)<br>                        {<br>                return a.Length.CompareTo(b.Length);<br>                        });<br>    int n = words.Length;<br>    bool[] mark = new bool[n];<br>    return getAmount("", words, mark) - 1;<br>  }<br><br>  private long getAmount(string p, string[] words, bool[] mark)<br>  {<br>    long am = 1;<br>    for (int i = 0; i &lt; words.Length; ++i)<br>      if (!mark[i] &amp;&amp; words[i].StartsWith(p))<br>      {<br>        mark[i] = true;<br>        am *= getAmount(words[i], words, mark);<br>      }<br>    return am + 1;<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>