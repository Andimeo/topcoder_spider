            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class ChessboardPattern {<br>public:<br>  vector &lt;string&gt; makeChessboard(int, int);<br>};<br> <br>vector &lt;string&gt; ChessboardPattern::makeChessboard(int rows, int columns) {<br>  vector&lt;string&gt; res;<br>  for (int i = 0; i &lt; rows; i++) {<br>    string cur = "";<br>    for (int j = 0; j &lt; columns; j++) {<br>      if ((i + j) % 2 == (rows - 1) % 2) cur += '.';<br>      else cur += 'X';<br>    }<br>    res.push_back(cur);<br>  }<br>  return res;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>