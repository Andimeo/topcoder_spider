            /*<br>*/<br>import java.util.*;<br>public class FibonacciSequence {<br>    public int numFibs(int a, int b) {<br>        int res = 0;<br>      int[] F = new int[1000];<br>      F[0] = 1;<br>      F[1] = 1;<br>      for (int i=2; i&lt;1000; i++)<br>      {<br>        F[i] = F[i-1] + F[i-2];<br>      if (F[i] &gt;= a &amp;&amp; F[i] &lt;= b) res++;<br>      if (F[i] &gt; b) break;<br>      }<br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>