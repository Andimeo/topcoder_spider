            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class Sortness<br>{<br>  public double getSortness(int[] a)<br>  {<br>    int sortness = 0;<br>    <br>    for (int i = 0; i &lt; a.Length; ++i) {<br>      for(int j = 0; j &lt; i; ++j) {<br>        if (a[i] &lt; a[j]) ++sortness;<br>      }<br>      for(int j = i+1; j &lt; a.Length; ++j) {<br>        if (a[i] &gt; a[j]) ++sortness;<br>      }<br>    }<br>    return (double)sortness/(double)a.Length;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>