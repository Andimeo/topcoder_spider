            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class Sortness<br>{<br>  public double getSortness(int[] a)<br>  {<br>    int total = 0;<br>    for(int i = 0; i &lt; a.length; i++) {<br>      for(int j = 0; j &lt; a.length; j++) if (j != i) {<br>        if ( j &lt; i &amp; a[j]&gt;a[i])<br>          total++;<br>        if (j &gt; i &amp; a[j] &lt; a[i])<br>          total++;<br>      }<br>    }<br>    return total*1.0/a.length;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>