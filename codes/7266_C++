            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class SantaGifts {<br>public:<br>  vector &lt;string&gt; distribute(vector &lt;string&gt;, int);<br>};<br> <br>vector &lt;string&gt; SantaGifts::distribute(vector &lt;string&gt; gifts, int N) {<br>  int round = 0;<br>  int cp = 0;<br>  vector&lt;string&gt; res(N, "");<br>  for (int i=0; i&lt;gifts.size() &amp;&amp; round&lt;4; ++i) {<br>    if (round) res[cp] += " ";<br>    res[cp] += gifts[i];<br>    ++cp;<br>    if (cp == N) ++round, cp=0;<br>  }<br>  return res;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>