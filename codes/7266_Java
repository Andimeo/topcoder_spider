            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class SantaGifts<br>{<br>  public String[] distribute(String[] gifts, int N)<br>  {<br>    String[]  res = new String[N];<br>    int n = gifts.length;<br>    for (int i=0; i&lt;N; i++)<br>      {<br>        res[i] = "";<br>        int j=i;<br>        int k =1 ;<br>        <br>        while (j&lt;n)<br>          {<br>            if (k&gt;4) break;<br>             if (k&gt;1) res[i] = res[i]+' ';<br>             res[i] = res[i]+gifts[j];<br>             k++;<br>             j+=N;<br>          }<br>      }<br>      return res;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>