            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class AnagramCompletion<br>{<br>    public string[] complete(string word1, string word2)<br>    {<br>        int[,] chs = new int[2, 27];<br>        for (int i = 0; i &lt; word1.Length; i++)<br>        {<br>            if (word1[i] == '.')<br>            {<br>                chs[0, 0]++;<br>            }<br>            else<br>            {<br>                chs[0, (int)(word1[i] - 'A' + 1)]++;<br>            }<br>            if (word2[i] == '.')<br>            {<br>                chs[1, 0]++;<br>            }<br>            else<br>            {<br>                chs[1, (int)(word2[i] - 'A' + 1)]++;<br>            }<br>        }<br><br>        for (int i = 1; i &lt;= 26; i++)<br>        {<br>            int n = chs[0, i] - chs[1, i];<br>            if (n &gt; 0)<br>            {<br>                if (chs[1, 0] &lt; n)<br>                {<br>                    return new string[0];<br>                }<br>                else<br>                {<br>                    chs[1, 0] -= n;<br>                    chs[1, i] += n;<br>                }<br>            }<br>            else if (n &lt; 0)<br>            {<br>                if (chs[0, 0] &lt; -n)<br>                {<br>                    return new string[0];<br>                }<br>                else<br>                {<br>                    chs[0, 0] -= -n;<br>                    chs[0, i] += -n;<br>                }<br>            }<br>        }<br>        chs[0, 1] += chs[0, 0];<br>        chs[1, 1] += chs[1, 0];<br><br>        char[] ch1 = word1.ToCharArray();<br>        for (int i = 0; i &lt; ch1.Length; i++)<br>        {<br>            if (ch1[i] != '.')<br>            {<br>                chs[0, (int)(ch1[i] - 'A' + 1)]--;<br>            }<br>        }<br><br>        for (int i = 0; i &lt; ch1.Length; i++)<br>        {<br>            if (ch1[i] == '.')<br>            {<br>                for (int j = 1; j &lt;= 26; j++)<br>                {<br>                    if (chs[0, j] &gt; 0)<br>                    {<br>                        chs[0, j]--;<br>                        ch1[i] = (char)(j - 1 + 'A');<br>                        break;<br>                    }<br>                }<br>            }<br>        }<br><br>        char[] ch2 = word2.ToCharArray();<br>        for (int i = 0; i &lt; ch2.Length; i++)<br>        {<br>            if (ch2[i] != '.')<br>            {<br>                chs[1, (int)(ch2[i] - 'A' + 1)]--;<br>            }<br>        }<br><br>        for (int i = 0; i &lt; ch2.Length; i++)<br>        {<br>            if (ch2[i] == '.')<br>            {<br>                for (int j = 1; j &lt;= 26; j++)<br>                {<br>                    if (chs[1, j] &gt; 0)<br>                    {<br>                        chs[1, j]--;<br>                        ch2[i] = (char)(j - 1 + 'A');<br>                        break;<br>                    }<br>                }<br>            }<br>        }<br><br>        return new string[] { new string(ch1), new string(ch2) };<br><br><br>    }<br>}<br></td>