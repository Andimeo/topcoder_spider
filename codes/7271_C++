<td class="problemText" colspan="8" valign="middle" align="left">
            #include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cmath&gt;<br>#include &lt;queue&gt;<br> <br>using namespace std;<br><br>class AnagramCompletion<br>{<br>public:<br>vector &lt;string&gt; complete(string word1, string word2)<br>{<br>  vector&lt;string&gt; ans;<br>  string s;<br>  int num1[30],num2[30];<br>  int num[30];<br>  int i,j,n,tot;<br>  n=word1.length();<br>  memset(num1,0,sizeof(num1));<br>  memset(num2,0,sizeof(num2));<br>  for (i=0;i&lt;word1.length();i++)<br>    if (word1[i]!='.')<br>      num1[word1[i]-'A']++;<br>  for (i=0;i&lt;word2.length();i++)<br>    if (word2[i]!='.')<br>      num2[word2[i]-'A']++;<br>  tot=0;<br>  for (i=0;i&lt;26;i++)<br>  {<br>    if (num1[i]&gt;num2[i]) num[i]=num1[i];<br>    else num[i]=num2[i];<br>    tot+=num[i];<br>  }<br>  ans.clear();<br>  if (tot&gt;n) return ans;<br>  num[0]+=n-tot;<br>  s="";<br>  for (i=0;i&lt;26;i++)<br>    for (j=0;j&lt;num[i]-num1[i];j++)<br>      s+=(char)('A'+i);<br>  j=0;<br>  for (i=0;i&lt;word1.length();i++)<br>    if (word1[i]=='.')<br>    {<br>      word1[i]=s[j];<br>      j++;<br>    }<br>  s="";<br>  for (i=0;i&lt;26;i++)<br>    for (j=0;j&lt;num[i]-num2[i];j++)<br>      s+=(char)('A'+i);<br>  j=0;<br>  for (i=0;i&lt;word2.length();i++)<br>    if (word2[i]=='.')<br>    {<br>      word2[i]=s[j];<br>      j++;<br>    }<br>  ans.push_back(word1);<br>  ans.push_back(word2);<br>  return ans;<br>}<br>};<br></td>