            public class AnagramCompletion{<br>  StringBuffer s1,s2;<br>  int n;<br>  public String[] complete(String word1, String word2){<br>    s1 = new StringBuffer(word1);<br>    s2 = new StringBuffer(word2);<br>    n=word1.length();<br>    for(char c='Z';c&gt;='A';--c){<br>      int t=0;<br>      for(int i=0;i&lt;n;++i) if(s2.charAt(i)==c) ++t;<br>      for(int i=0;i&lt;n;++i) if(s1.charAt(i)==c) --t;<br>      for(int i=n-1;i&gt;=0;--i) if((s1.charAt(i)=='.')&amp;&amp;(t&gt;0)){<br>        s1.setCharAt(i,c);<br>        --t;<br>      }<br>      if(t&gt;0) return new String[0];<br>    }<br>    <br><br>    for(char c='Z';c&gt;='A';--c){<br>      int t=0;<br>      for(int i=0;i&lt;n;++i) if(s1.charAt(i)==c) ++t;<br>      for(int i=0;i&lt;n;++i) if(s2.charAt(i)==c) --t;<br>      for(int i=n-1;i&gt;=0;--i) if((s2.charAt(i)=='.')&amp;&amp;(t&gt;0)){<br>        s2.setCharAt(i,c);<br>        --t;<br>      }<br>      if(t&gt;0) return new String[0];<br>    }<br><br>    for(int i=0;i&lt;n;++i) if(s1.charAt(i)=='.') s1.setCharAt(i,'A');<br>    for(int i=0;i&lt;n;++i) if(s2.charAt(i)=='.') s2.setCharAt(i,'A');<br>    <br>    String res[] = new String[2];<br>    res[0] = new String(s1);<br>    res[1] = new String(s2);<br>    return res;<br>  }<br>}<br></td>