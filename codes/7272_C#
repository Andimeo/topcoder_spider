            using System;<br>using System.Collections;<br>using System.Text;<br><br>public class AnswerEvaluation<br>{<br><br>  public int res;<br>  public int n,m;<br><br>  public int getScore(string[] keywords, int[] scores, string answer)<br>  {<br>    n=keywords.Length;<br>        string[] ans = answer.Split(' ');<br>    m=ans.Length;<br>        bool[] was = new bool[n];<br>        res = 0;<br>        for (int i = 0; i &lt; m; i++)<br>        {<br>            for (int j = 0; j &lt; n; j++)<br>            if(was[j]==false)<br>            {<br>                if (ans[i] == keywords[j])<br>                {<br>                    res += scores[j];<br>                    was[j] = true;<br>                }<br>            }<br>        }<br>    return res;<br>  }<br><br>}<br></td>