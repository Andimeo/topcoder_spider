            // Cry as challenge phase is passing by<br><br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br><br>#include &lt;cmath&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br><br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br><br>using namespace std;<br><br>#define FORC(it,v) for( __typeof((v).begin()) it = (v).begin(); it != (v).end(); ++it )<br><br>template&lt; typename T &gt; inline T myabs( const T &amp;x ) { if( x &lt; 0 ) return -x; return x; }<br><br>class ManyNumbers {<br>public:<br>    int getSums( vector &lt;int&gt; numbers, int K ) {<br>        int a = 0;<br>        int b = 0;<br><br>        FORC( it, numbers ) {<br>            if( *it % K == 0 ) a += *it;<br>            else b += *it;<br>        }<br><br>        return myabs( a - b );<br>    }<br>};<br><br><br><br><br><br><br><br><br><br>// Powered by FileEdit<br>// Powered by moj 3.0 beta 11 [modified TZTester]<br>// Powered by CodeProcessor<br></td>