            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class GoodExhibition {<br>    public int numberOfPictures(string[] labels, int K) {<br>        int res = 0;<br>        Dictionary&lt;string, int&gt; d = new Dictionary&lt;string, int&gt;();<br>        for (int i = 0; i &lt; labels.Length; ++i)<br>            d[labels[i]]=0;<br>        for (int i = 0; i &lt; labels.Length; ++i)<br>            d[labels[i]]++;<br>        for (int i = 0; i &lt; labels.Length; ++i)<br>        {<br>            res += Math.Min(d[labels[i]], K - 1);<br>            d[labels[i]] = 0;<br>        }<br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>