            import java.util.*;<br>public class GoodExhibition {<br>    public int numberOfPictures(String[] labels, int K) {<br>        int res=0;<br>      for (int i=0;i&lt;labels.length;i++) <br>          if (labels[i].length()&gt;0) {<br>           int t = 1;<br>            for (int j=i+1;j&lt;labels.length;j++)<br>            if (labels[j].length()&gt;0) <br>              if (labels[i].compareTo(labels[j])==0) {<br>                labels[j] ="";<br>                t++;<br>              }<br>          res+=Math.min(t,K-1);<br>        }<br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>