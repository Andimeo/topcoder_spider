            using System;<br><br>public class CarolsSinging {<br>  public int choose(string[] lyrics) {<br>    int n = lyrics[0].Length;<br>    int am = lyrics.Length;<br>    int res = n;<br>    int[] m = new int[am];<br>    for (int i = 0; i &lt; am; ++i)<br>    {<br>      int mask = 0;<br>      for (int j = 0; j &lt; n; ++j)<br>      {<br>        mask &lt;&lt;= 1;<br>        if (lyrics[i][j] == 'Y')<br>          mask |= 1;<br>      }<br>      m[i] = mask;<br>    }<br>    for (int mask = 0; mask &lt; (1 &lt;&lt; n); ++mask)<br>    {<br>      int cnt = 0;<br>      int a = mask;<br>      while (a &gt; 0)<br>      {<br>        ++cnt;<br>        a &amp;= (a - 1);<br>      }<br>      bool ok = true;<br>      for (int i = 0; i &lt; am; ++i)<br>        if ((m[i] &amp; mask) == 0)<br>          ok = false;<br>      if (ok)<br>        res = Math.Min(res, cnt);<br>    }<br>    return res;<br>  }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>