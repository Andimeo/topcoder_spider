            public class CarolsSinging{<br>    public int choose(String[] lyrics) {<br>        int[] masks=new int[lyrics.length];<br>        for (int i=0;i&lt;lyrics.length;i++)<br>            masks[i]=Integer.valueOf(lyrics[i].replaceAll("Y","1").replaceAll("N","0"),2);<br>        int best=lyrics.length;<br>        bigLoop:<br>        for (int i=1;i&lt;(1&lt;&lt;lyrics[0].length())+1;i++){<br>            for (int j=0;j&lt;masks.length;j++){<br>                if ((i&amp;masks[j])==0) continue bigLoop;<br>            }<br>            best=Math.min(best,Integer.bitCount(i));<br>        }<br>        return best;<br>    }<br>    }<br></td>