            using System;<br>using System.Collections;<br><br>public class GoodAndBadPostmen<br>  {<br>    public int[] whoIsWho(String[] white, String[] black)<br>    {<br>      ArrayList pms = new ArrayList();<br>      ArrayList good = new ArrayList();<br>      ArrayList bad = new ArrayList();<br>      foreach(string pmlist in white)<br>      {<br>        string[] tmps = pmlist.Split(' ');<br>        foreach(string pm in tmps) <br>        {<br>          if (pms.IndexOf(pm)==-1) <br>          {<br>            pms.Add(pm);<br>            good.Add(pm);<br>          }<br>          <br>        }<br>        <br>      }<br>      <br>      foreach(string pmlist in black)<br>      {<br>        string[] tmps = pmlist.Split(' ');<br>        ArrayList thebad = new ArrayList();<br>        foreach(string pm in tmps) <br>        {<br>          if (pms.IndexOf(pm)==-1) <br>          {<br>            pms.Add(pm);<br>          }<br>          if (good.IndexOf(pm)==-1) thebad.Add(pm);<br>        }<br>        if (thebad.Count==1)<br>        {<br>          string pm = (string)thebad[0];<br>          if (bad.IndexOf(pm)==-1) bad.Add(pm);<br>                    <br>        }<br>      }<br>      <br>      int[] ret = new int[3];<br>      ret[0] = good.Count;<br>      ret[1] = bad.Count;<br>      ret[2] = (pms.Count-good.Count)-bad.Count;<br>      return ret;<br>    }<br>  }<br></td>