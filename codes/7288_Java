            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class GoodAndBadPostmen<br>{<br>  public int[] whoIsWho(String[] white, String[] black)<br>  {<br>    Set&lt;String&gt; ss=new TreeSet&lt;String&gt;();<br>    Set&lt;String&gt; s2=new TreeSet&lt;String&gt;();<br>    Set&lt;String&gt; s3=new TreeSet&lt;String&gt;();<br>    for(String s:white) {<br>      String[] tam=s.split(" ");<br>      for(String x:tam) ss.add(x);<br>    }<br>    for(String s:black) {<br>      String[] tam=s.split(" ");<br>      int dem=0;<br>      for(String x:tam) if(!ss.contains(x)) ++dem;<br>      if(dem==1) {<br>        for(String x:tam) if(!ss.contains(x)) s2.add(x);<br>      }<br>    }<br>    for(String s:black) {<br>      String[] tam=s.split(" ");<br>      int dem=0;<br>      for(String x:tam) if(!ss.contains(x)) ++dem;<br>      if(dem!=1) {<br>        for(String x:tam) if((!ss.contains(x)) &amp;&amp; !s2.contains(x)) s3.add(x);<br>      }<br>    }<br>    return new int[]{ss.size(),s2.size(),s3.size()};<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>