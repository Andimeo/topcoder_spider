            import java.io.*;<br>import java.util.*;<br><br>public class RandomSwaps {<br>  public double getProbability(int n, int swapCount, int a, int b) {<br>    int m = swapCount;<br>    int ps = n * (n - 1) / 2;<br>    double[] ph = new double[m + 1];<br>    double[] pt = new double[m + 1];<br>    ph[0] = 1d;<br>    for (int i = 0; i &lt; m; i++) {<br>      pt[i + 1] = ph[i] * (n - 1) / ps + pt[i] * (ps - 1) / ps;<br>      ph[i + 1] = ph[i] * (ps - n + 1) / ps + pt[i] / ps;<br>    }<br>    if (a == b)<br>      return ph[m];<br>    return pt[m] / (n - 1);<br>  }<br>}<br></td>