            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class VowelEncryptor {<br>    public string[] encrypt(string[] text)<br>    {<br>        string[] res = new string[text.Length];<br>        int t = 0;<br>        for (int i =0; i &lt; text.Length; ++i)<br>        {<br>            res[i] = text[i];<br>            while ((t = res[i].IndexOf('a')) &gt; -1)<br>            {<br>                res[i] = res[i].Remove(t, 1);<br>            }<br>            while ((t = res[i].IndexOf('e')) &gt; -1)<br>            {<br>                res[i] = res[i].Remove(t, 1);<br>            }<br>            while ((t = res[i].IndexOf('o')) &gt; -1)<br>            {<br>                res[i] = res[i].Remove(t, 1);<br>            }<br>            while ((t = res[i].IndexOf('u')) &gt; -1)<br>            {<br>                res[i] = res[i].Remove(t, 1);<br>            }<br>            while ((t = res[i].IndexOf('i')) &gt; -1)<br>            {<br>                res[i] = res[i].Remove(t, 1);<br>            }<br><br>            if (res[i].Length == 0)<br>                res[i] = text[i];<br>        }<br>        return res;<br>    }<br><br>    #region test_region<br>    #endregion  test_region<br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>