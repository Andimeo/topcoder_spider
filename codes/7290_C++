            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br>#define FOR(i, n) for(int i=0; i&lt;(n); i++)<br>#define FORAB(i, a, b) for(int i=(a); i&lt;(b); i++)<br> <br> <br>class VowelEncryptor {<br>public:<br>  vector &lt;string&gt; encrypt(vector &lt;string&gt; text) {<br>    string v = "aeiou";<br>    FOR(i, text.size()) {<br>      string s ;<br>      FOR(j, text[i].length()) if(v.find_first_of( string(1, text[i][j]) ) == string::npos) s += text[i][j];<br>      if (s == "") s = text[i];<br>      text[i] = s;<br>    }<br>    return text;<br>  }<br>};<br> <br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>