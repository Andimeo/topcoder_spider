            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br><br>public class VowelEncryptor {<br><br>    public String[] encrypt(String[] text) {<br>        String[] res = new String[text.length];<br>        for (int i=0; i&lt;text.length; i++) {<br>          res[i] = text[i].replaceAll("[aeiou]", "");<br>          if (res[i].length()==0) {<br>            res[i] = text[i];<br>          }<br>        }<br>        return res;<br>    }<br><br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>