            using System;<br>using System.Collections.Generic;<br><br>public class BirthNumbersValidator {<br>  int[] daysInMonth = new int[] { 31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31 };<br>  <br>  public string[] validate(string[] test) {<br>    List&lt;string&gt; res = new List&lt;string&gt;();<br>    foreach (string s in test)<br>    {<br>      long v = long.Parse(s);<br>      bool ok = true;<br>      if (v % 11 != 0)<br>        ok = false;<br>      else<br>      {<br>        long c = v % 10000;<br>        v /= 10000;<br>        long d = v % 100;<br>        v /= 100;<br>        long m = v % 100;<br>        v /= 100;<br>        long y = v;<br>        if (m &lt; 1 || m &gt; 12 &amp;&amp; m &lt; 51 || m &gt; 62)<br>          ok = false;<br>        else<br>        {<br>          if (m &gt; 50)<br>            m -= 50;<br>          int days = daysInMonth[m - 1];<br>          if (y % 4 == 0 &amp;&amp; m == 2)<br>            ++days;<br>          if (d &lt; 1 || d &gt; days)<br>            ok = false;<br>        }<br>      }<br>      if (ok)<br>        res.Add("YES");<br>      else<br>        res.Add("NO");<br>    }<br>    return res.ToArray();<br>  }<br><br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>