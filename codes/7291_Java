            import java.util.*;<br>import java.math.*;<br>import java.io.*;<br>import java.text.*;<br>import java.util.regex.*;<br>import static java.util.Arrays.*;<br>import static java.lang.Math.*;<br>import static java.lang.Integer.parseInt;<br>public class BirthNumbersValidator {<br>  public String[] validate(String[] test) {<br>    String[] ret = new String[test.length];<br>    for(int i = 0; i &lt; ret.length; i++){<br>      ret[i] = ok(test[i]) ? "YES" : "NO";<br>    }<br>    return ret;<br>  }<br>  boolean ok(String s){<br>    int y = parseInt(s.substring(0,2));<br>    int m = parseInt(s.substring(2,4))%50;<br>    int d = parseInt(s.substring(4,6));<br>    boolean leap = y%4 == 0;<br>    if(Long.parseLong(s)%11 != 0) return false;<br>    if(m &lt; 1 || m &gt; 12) return false;<br>    int[] max = {31,28,31,30,31,30,31,31,30,31,30,31};<br>    if(leap) max[1]++;<br>    if(d &lt; 1 || d &gt; max[m-1]) return false;<br>    return true;<br>  }<br> <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>