            using System;<br>using System.Collections;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class DifDif<br>{<br>  public int predict(int[] seq)<br>  {<br>      int n = seq.Length;<br>        int[,] d = new int[n+10,n+10];<br>        for (int i = 0; i &lt; n; i++) d[0, i] = seq[i];<br>      int lastsum = seq[n-1];<br>        for (int j=1; j&lt;n; j++)<br>        {<br>            for (int i = 0; i &lt; n - j; i++) d[j, i] = d[j - 1, i + 1] - d[j - 1, i];<br>            lastsum += d[j, n - j - 1];<br>        }<br>      return lastsum;<br>  }<br>}<br> <br>//Powered by [Zigger]<br></td>