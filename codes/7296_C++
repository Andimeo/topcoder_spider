            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include&lt;sstream&gt;<br>#include&lt;iostream&gt;<br>#include&lt;cstdio&gt;<br>#include&lt;queue&gt;<br>#include&lt;stack&gt;<br>using namespace std;<br> <br>class DifDif {<br>  public:<br>  int predict(vector &lt;int&gt; seq) {<br>    if (seq.size() == 1) return seq[0];<br>    vector&lt;int&gt;diff;<br>    for (int i=0;i&lt;seq.size()-1;i++) diff.push_back(seq[i+1]-seq[i]);<br>    return seq.back()+predict(diff);<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>