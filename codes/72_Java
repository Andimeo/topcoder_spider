            public class Camper<br>{<br>  private static int [][] data = { { 10, 100, 170, 40, 68, 92, 220, 30, 60, 85, 92, 109, 230, 60, 65, 72, 80, 82, 120, 130, 180, 222, 400, 800 }, //calories<br>                                   {  5,  20,  10, 12, 20, 40,  70, 30, 40, 30, 30,  60, 100, 51, 52, 53, 54, 70,  20,  40,  40, 90,  200, 600 }, //weight<br>                                   {  3,  20,  90, 90, 80, 12,  50,  1, 70, 20, 20,  30, 120, 40, 40, 40, 20, 80,  90, 100,  60, 30,  200,  50 } }; //volume<br>  public int maxCalories(int maxw, int maxv) {<br>     boolean taken[] = new boolean[30];<br>     return mch(0, maxw, maxv);<br>  }<br>  public int mch(int at, int maxw, int maxv) {<br>     if (at &gt;= 24) return 0;<br>     int best = mch(at+1, maxw, maxv);<br>     if (data[1][at] &lt;= maxw &amp;&amp; data[2][at] &lt;= maxv) {<br>        int fg = mch(at+1, maxw - data[1][at], maxv - data[2][at]) + data[0][at];<br>        if (fg &gt; best) best = fg;<br>     }<br>     return best;<br>  }<br>}<br></td>