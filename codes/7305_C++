            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class DirPattern {<br>public:<br>  string matchFiles(vector &lt;string&gt;);<br>};<br> <br>string DirPattern::matchFiles(vector &lt;string&gt; filenames) {<br>  int n=filenames.size();<br>  if (n==1) return filenames[0];<br>  string ans=filenames[0];<br>  for (int i=0; i&lt;n; i++)<br>    for (int j=0; j&lt;n; ++j)<br>      if (i!=j)<br>  {<br>    for (int k=0; k&lt;filenames[i].size(); ++k)<br>    if (filenames[i][k]!=filenames[j][k]) ans[k]='?';<br>  }<br>  return ans;<br> <br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>