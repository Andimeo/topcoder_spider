            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class DirPattern<br>{<br>  public String matchFiles(String[] f)<br>  {<br>    char[] ch = f[0].toCharArray();<br>    for(int i = 0; i &lt; f[0].length(); ++i)<br>    {<br>      boolean ft = true;<br>      for(int j = 1; j &lt; f.length; ++j)<br>        if(f[0].charAt(i) != f[j].charAt(i))<br>          ft = false;<br>      if(!ft) ch[i] = '?';<br>    }<br>    return new String(ch);<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>