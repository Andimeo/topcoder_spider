            import java.util.ArrayList;<br>import java.util.Arrays;<br>import java.util.Comparator;<br>import java.util.StringTokenizer;<br><br><br>public class TooManyBugs {<br><br>  /**<br>   * @param args<br>   */<br>  public static void main(String[] args) {<br>    // TODO [\u33258][\u21160][\u29983][\u25104][\u26041][\u27861][\u23384][\u26681]<br>    String ss[]={"5 3", "20 20", "25", "15", "6 15"};<br>    System.out.println(new TooManyBugs().bestBugFixing(ss));<br>  }<br>  public int bestBugFixing(String[] info){<br>    ArrayList&lt;bug&gt; bb=new ArrayList&lt;bug&gt;();<br>    ArrayList&lt;Integer&gt; work=new ArrayList&lt;Integer&gt;();<br>    for(int i=0;i&lt;info.length;++i){<br>      String s=info[i];<br>      int index=s.indexOf(' ');<br>      if(index&gt;=0){<br>        StringTokenizer st=new StringTokenizer(s);<br>        bb.add(new bug(Integer.parseInt(st.nextToken()),Integer.parseInt(st.nextToken())));<br>      }<br>      else{<br>        work.add(Integer.parseInt(s));<br>      }<br>    }<br>    <br>    bug bbb[]=bb.toArray(new bug[0]);<br>    Arrays.sort(bbb,0,bbb.length,new Comparator&lt;bug&gt;(){<br>      public int compare(bug o1, bug o2) {<br>        // TODO [\u33258][\u21160][\u29983][\u25104][\u26041][\u27861][\u23384][\u26681]<br>        return o2.prio-o1.prio;<br>      }<br>    });<br>    <br>    Integer www[]=work.toArray(new Integer[0]);<br>    Arrays.sort(www,0,www.length,new Comparator&lt;Integer&gt;(){<br>      public int compare(Integer o1, Integer o2) {<br>        // TODO [\u33258][\u21160][\u29983][\u25104][\u26041][\u27861][\u23384][\u26681]<br>        return o1-o2;<br>      }<br>    });<br>    int ans=0;<br>    boolean fff[]=new boolean[www.length];<br>    for(int i=0;i&lt;bbb.length;++i){<br>      for(int j=0;j&lt;www.length;++j){<br>        if(!fff[j]&amp;&amp;www[j]&gt;=bbb[i].fix){<br>          fff[j]=true;<br>          ans+=bbb[i].prio;<br>          break;<br>        }<br>      }<br>    }<br>    return ans;<br>  }<br>}<br>class bug{<br>  int prio;<br>  int fix;<br>  public bug(int p,int f) {<br>    // TODO [\u33258][\u21160][\u29983][\u25104][\u26500][\u36896][\u20989][\u25968][\u23384][\u26681]<br>    prio=p;<br>    fix=f;<br>  }<br>}<br></td>