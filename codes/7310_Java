            import java.util.HashMap;<br><br><br>public class KingMovement {<br><br>  /**<br>   * @param args<br>   */<br>  public static void main(String[] args) {<br>    // TODO [\u33258][\u21160][\u29983][\u25104][\u26041][\u27861][\u23384][\u26681]<br>    String s1="A1";<br>    String s2="A2";<br>    String ss[]={"T"};<br>    System.out.println(new KingMovement().finalPosition(s1, s2, ss));<br>  }<br>  public String finalPosition(String startPosition, String stonePosition, String[] moves){<br>    HashMap&lt;String, point&gt; map=new HashMap&lt;String, point&gt;();<br>    map.put("R", new point(1,0));<br>    map.put("L", new point(-1,0));<br>    map.put("T", new point(0,1));<br>    map.put("B", new point(0,-1));<br>    map.put("RT", new point(1,1));<br>    map.put("LT", new point(-1,1));<br>    map.put("RB", new point(1,-1));<br>    map.put("LB", new point(-1,-1));<br>    <br>    int kx=startPosition.charAt(0)-'A';<br>    int ky=startPosition.charAt(1)-'1';<br>    int sx=stonePosition.charAt(0)-'A';<br>    int sy=stonePosition.charAt(1)-'1';<br>    <br>    for(int i=0;i&lt;moves.length;++i){<br>      point p=map.get(moves[i]);<br>      int x=kx+p.x;<br>      int y=ky+p.y;<br>      <br>      if(x&gt;=0&amp;&amp;x&lt;8&amp;&amp;y&gt;=0&amp;&amp;y&lt;8){<br>        if(x==sx&amp;&amp;y==sy){<br>          int xx=sx+p.x;<br>          int yy=sy+p.y;<br>          if(xx&gt;=0&amp;&amp;xx&lt;8&amp;&amp;yy&gt;=0&amp;&amp;yy&lt;8){<br>            kx=x;<br>            ky=y;<br>            sx=xx;<br>            sy=yy;<br>          }<br>        }<br>        else{<br>          kx=x;<br>          ky=y;<br>        }<br>      }<br>    }<br>    <br>    StringBuffer sb=new StringBuffer();<br>    sb.append((char)(kx+'A'));<br>    sb.append((char)(ky+'1'));<br>    sb.append(",");<br>    sb.append((char)(sx+'A'));<br>    sb.append((char)(sy+'1'));<br>    <br>    return sb.toString();<br>  }<br>}<br>class point{<br>  int x;<b