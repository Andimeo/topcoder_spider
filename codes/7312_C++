            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;queue&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cctype&gt;<br>#include &lt;string&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br> <br>class Prefixes<br>{<br>public:<br>  vector &lt;string&gt; prefixList(vector &lt;string&gt; A)<br>  {<br>    vector&lt;string&gt; R;<br>    int n=A.size();<br>    bool vis[60];<br>    memset(vis,false,sizeof(vis));<br>    sort(A.begin(),A.end());<br>    while (1)<br>    {<br>      int left=0;<br>      for (int i=0;i&lt;n;i++) left+=(int)(!vis[i]);<br>      if (left==0) break;<br>      string prefix="";<br>      for (int i=0;i&lt;n;i++) if (!vis[i])<br>        for (int j=i+1;j&lt;n;j++) if (!vis[j])<br>        {<br>          int L;<br>          for (L=0;L&lt;A[i].length() &amp;&amp; L&lt;A[j].length() &amp;&amp; A[i][L]==A[j][L];L++);<br>          string s=A[i].substr(0,L);<br>          if (s.length()&gt;prefix.length() || s.length()==prefix.length() &amp;&amp; s&lt;prefix)<br>            prefix=s;<br>        }<br>      for (int i=0;i&lt;n;i++) if (!vis[i] &amp;&amp; A[i].find(prefix)==0)<br>      {<br>        R.push_back(A[i]);<br>        vis[i]=true;<br>      }<br>      string s="";<br>      for (int i=0;i&lt;prefix.length();i++) s+="-";<br>      R.push_back(s);<br>    }<br>    return R;<br>  }<br>};<br> <br> <br> <br>// Powered by PopsEdit<br></td>