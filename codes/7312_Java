            import java.util.*;<br><br>public class Prefixes<br>{<br>  public String[]  prefixList (String[] p)<br>  {<br>    Arrays.sort(p);<br>    LinkedList&lt;String&gt; out = new LinkedList&lt;String&gt;();<br>    int num = 0;<br>    while (p.length - num &gt; 1)<br>    {<br>      String pre = "";<br>      for (int i = 0; i &lt; p.length; i++)<br>      {<br>        if (p[i].length() == 0)<br>          continue;<br>        for (int j = p[i].length(); j &gt; pre.length(); j--)<br>        {<br>          String s = p[i].substring(0, j);<br>          boolean works = false;<br>          for (int k = 0; k &lt; p.length; k++)<br>          {<br>            if (p[k].length() == 0 || i == k || p[k].length() &lt; j)<br>              continue;<br>            if (p[k].substring(0, j).equals(s))<br>            {<br>              works = true;<br>              break;<br>            }<br>          }<br>          if (works)<br>          {<br>            pre = s;<br>            break;<br>          }<br>        }<br>      }<br>      <br>      for (int i = 0; i &lt; p.length; i++)<br>        if (p[i].length() &gt; 0 &amp;&amp; p[i].length() &gt;= pre.length() &amp;&amp; p[i].substring(0, pre.length()).equals(pre))<br>        {<br>          out.add(p[i]);<br>          p[i] = "";<br>          num++;<br>        }<br>      out.add("------------------------------------------------------------------------------------------".substring(0, pre.length()));<br>    }<br>    if (p.length != num)<br>    {<br>      for (int i = 0; i &lt; p.length; i++)<br>        if (p[i].length() &gt; 0)<br>        {<br>          out.add(p[i]);<br>          out.add("");<br>          break;<br>        }<br>    }<br>    String[] out2 = new String [out.size()];<br>    ListIterator&lt;String&gt; li = out.listIterator();<br>    for (int i = 0; i &lt; out2.length; i++)<br>      out2[i] = li.next();<br>    return out2;<br>  }<br>}<br></td>