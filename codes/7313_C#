            using System;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class ServiceNames {<br>  public string[] makeList(string[] services) {<br>    Array.Sort(services, StringComparer.Ordinal);<br>    Dictionary&lt;string, List&lt;string&gt;&gt; all = new Dictionary&lt;string, List&lt;string&gt;&gt;();<br>    List&lt;string&gt; keys = new List&lt;string&gt;();<br>    foreach (string s in services)<br>    {<br>      string[] p = s.Split(' ');<br>      for (int i = 1; i &lt; p.Length; ++i)<br>      {<br>        if (!all.ContainsKey(p[i]))<br>        {<br>          all[p[i]] = new List&lt;string&gt;();<br>          keys.Add(p[i]);<br>        }<br>        all[p[i]].Add(p[0]);<br>      }<br>    }<br>    keys.Sort(StringComparer.Ordinal);<br>    List&lt;string&gt; res = new List&lt;string&gt;();<br>    foreach (string k in keys)<br>    {<br>      StringBuilder b = new StringBuilder();<br>      b.Append(k);<br>      b.Append(" ==&gt; ");<br>      bool first = true;<br>      foreach (string v in all[k])<br>      {<br>        if (first)<br>          first = false;<br>        else<br>          b.Append(", ");<br>        b.Append(v);<br>      }<br>      res.Add(b.ToString());<br>    }<br>    return res.ToArray();<br>  }<br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>