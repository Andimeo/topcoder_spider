            import java.util.*;<br> <br>public class ServiceNames {<br>    public String[] makeList(String[] services) {<br>        Map&lt;String, Set&lt;String&gt;&gt; a = new TreeMap&lt;String, Set&lt;String&gt;&gt;();<br>        for (String s : services) {<br>            StringTokenizer st = new StringTokenizer(s);<br>            String b = st.nextToken();<br>            while (st.hasMoreTokens()) {<br>                String z = st.nextToken();<br>                if (!a.containsKey(z)) {<br>                    a.put(z, new TreeSet&lt;String&gt;());<br>                }<br>                a.get(z).add(b);<br>            }<br>        }<br> <br>        String[] r = new String[a.size()];<br>        int i = 0;<br>        for (String s : a.keySet()) {<br>            r[i] = s;<br>            int j = 0;<br>            for (String z : a.get(s)) {<br>                if (j == 0) {<br>                    r[i] = r[i] + " ==&gt; ";<br>                } else {<br>                    r[i] = r[i] + ", ";<br>                }<br>                r[i] += z;<br>                j++;<br>            }<br>            i++;<br>        }<br> <br>        return r;<br>    }<br> <br>}<br> <br> <br>// Powered by PopsEdit<br> <br>// Powered by CodeProcessor<br></td>