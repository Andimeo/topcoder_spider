            import java.util.*;<br>public class Destruction {<br>  long table[][] = new long[1000][1000];<br>  private long canFind(int t, int d) {<br>    if (table[t][d] != 0) return table[t][d];<br>    if (t==0 || d==0) return 1;<br>    return table[t][d] = canFind(t-1, d-1) + canFind(t-1, d);<br>  }<br>    public double minError(int numTests, int numDestroyed) {<br>      return 50. / (canFind(numTests, numDestroyed) - 1);<br>    }<br> <br>}<br> <br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>