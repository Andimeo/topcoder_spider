            using System;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class ProdCalc<br>{<br> <br>    public long highest(int d, int op)<br>    {<br>        if (Math.Pow(2, op) &gt;= Math.Pow(10, d))<br>        {<br>            return -1;<br>        }<br>        else<br>        {<br>            build(1, d, op,2);<br>        }<br>        return calcMax;<br>    }<br>    long calcMax = 1;<br><br>    public void build(long current, int d, int op,int prev)<br>    {<br>        if (current &lt; Math.Pow(10, d))<br>        {<br>            if (op == 0)<br>            {<br><br>                calcMax = Math.Max(calcMax, current);<br><br>            }<br>            else<br>            {<br>                for (int i = 2; i &lt; 10; i++)<br>                {<br>                    if (prev &lt;= i)<br>                    {<br>                        build(current * i, d, op - 1,i);<br>                    }<br>                }<br>            }<br>        }<br>    }<br><br><br>    public static void Main()<br>    {<br>       <br>    }<br>}<br></td>