            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;cmath&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>using namespace std;<br> <br>class ProdCalc <br>{<br>  public:<br>  long long highest(int d, int op) <br>  {<br>  <br>    set&lt;int&gt; a,b;<br>    int i,W=1,j;<br>    while(d--) W*=10;<br>    set&lt;int&gt;::iterator q;<br>    a.insert(1);<br>    for(i=0;i&lt;op;i++)<br>    {<br>      b.clear();<br>      for(q=a.begin();q!=a.end();++q) for(j=2;j&lt;10;j++) if((*q)*j&lt;W) b.insert((*q)*j); else break;<br>      a=b;<br>    }<br>    if(a.size()==0) return -1;<br>    return *(a.rbegin());<br>  }<br> <br>};<br> <br>// Powered by PopsEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>