<td class="problemText" colspan="8" valign="middle" align="left">
            public class ProdCalc<br>{<br>  private long result;<br>  private long limit;<br>  <br>  private void search(int curop, int op, int lastNum, long curProd)<br>  {<br>    if (curop == op)<br>    {<br>      if (curProd &gt; result &amp;&amp; curProd &lt;= limit)<br>        result = curProd;<br>      return;<br>    }<br>    if (curProd &gt; limit)<br>      return;<br>    for (int i = lastNum; i &lt;= 9; i++)<br>    {<br>      search(curop+1, op, i, curProd * i);<br>    }<br>  }<br>  <br>  public long highest(int d, int op)<br>  {<br>    limit = (int) (Math.pow(10, d)) - 1;<br>    result = -1;<br>    search(0, op, 2, 1);<br>    return result;<br>  }<br>}<br></td>