            using System;<br><br>public class CompressedString<br>{<br>    public int rec(string s)<br>    {<br>        if (s.Contains("("))<br>        {<br>            int res = 0;<br>            while (s.Length &gt; 0 &amp;&amp; s.Contains("("))<br>            {<br>                int start = s.IndexOf("(");<br>                int end = 0;<br>                int d1 = 0; int d2 = 0;<br>                for (int i = 0; i &lt; s.Length; i++)<br>                {<br>                    if (s[i] == '(') d1++;<br>                    if (s[i] == ')')<br>                    {<br>                        d2++;<br>                        if (d1 == d2) { end = i; break; }<br>                    }<br><br>                }<br>                string elotte = s.Substring(0, start - 1);<br>                int e = elotte.Length;<br>                res+=e + ((int)s[start - 1] - (int)'0') * rec(s.Substring(start + 1, end - start - 1));<br>                s = s.Substring(end+1);<br>            }<br>            return res+s.Length;<br>        }<br>        else return s.Length;<br>    }<br><br><br>    public int getLength(string s)<br>    {<br>        int res=rec(s);<br>        return res;<br>    }<br><br>}<br></td>