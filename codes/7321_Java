            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class CompressedString<br>{<br>  public int getLength(String s)<br>  {<br>    int p1 = s.indexOf("(");<br>    if (p1 == -1) return s.length();<br>    int p2;<br>    int k = 1;<br>    for (p2 = p1+1; p2 &lt; s.length(); ++p2) {<br>    char ch = s.charAt(p2);<br>    if (ch == '(') k++;<br>    if (ch == ')')k--;<br>    if (k==0) break;<br>    }<br>    int l = Integer.parseInt(s.substring(p1-1, p1));<br>    String s1 = s.substring(p1+1, p2);<br>    String s2 = s.substring(p2+1);<br>    return p1 - 1 + l  *getLength(s1) + getLength(s2);<br>    <br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>