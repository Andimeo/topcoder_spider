            import java.util.*;<br><br>public class Armies {<br>    public String[] findOrder(String[] army1, String[] army2) {<br>        String[] res;<br>        int tiep=1;<br>        res=new String[army1.length+army2.length];<br>        int cs=0;<br>        for(int i=999;i&gt;=1;){<br>          <br>          int x1=-1, x2=-1;<br>          <br>          for(x1=0;x1&lt;army1.length;++x1){<br>            String[] tam=army1[x1].split(" ");<br>            int t=Integer.parseInt(tam[1]);<br>            if(t==i) break;<br>          }<br>            <br>          for(x2=0;x2&lt;army2.length;++x2){<br>            String[] tam=army2[x2].split(" ");<br>            int t=Integer.parseInt(tam[1]);<br>            if(t==i) break;<br>          }<br>          <br>          if(x1&lt;army1.length &amp;&amp; x2&lt;army2.length){<br>            if(tiep==1){<br>              String[] tam=army1[x1].split(" ");<br>              res[cs++]=tam[0];<br>              army1[x1] = tam[0]+" 0";<br>            }<br>            else{<br>              String[] tam=army2[x2].split(" ");<br>              res[cs++]=tam[0];<br>              army2[x2] = tam[0]+" 0";              <br>            }<br>            tiep = 3-tiep;<br>          }<br>          <br>          else if(x1&lt;army1.length){<br>              String[] tam=army1[x1].split(" ");<br>              res[cs++]=tam[0];<br>              army1[x1] = tam[0]+" 0";            <br>              tiep=2;<br>          }<br>          else if(x2&lt;army2.length){<br>              String[] tam=army2[x2].split(" ");<br>              res[cs++]=tam[0];<br>              army2[x2] = tam[0]+" 0";            <br>              tiep=1;<br>          }<br>          else --i;<br><br>          <br>        }<br>        return res;<br>    }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>