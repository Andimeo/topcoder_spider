            #include &lt;algorithm&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>using namespace std;<br>const int MAXL = 1000;<br>char buff[MAXL+1];<br><br><br>class BadExam {<br>public:<br>    double newAverage( vector &lt;int&gt; V ) {<br>        int mx = -1;<br>        for( int i = 0; i &lt; ( int )V.size(); ++i ) <br>            mx = max( mx, V[i] );<br>        double sum = 0;<br>        for( int i = 0; i &lt; ( int )V.size(); ++i ) <br>            sum += double( V[i] ) / mx * 100.;<br>        return sum / ( int )V.size();<br>    }<br>};<br><br><br><br><br><br><br><br><br><br>// Powered by FileEdit<br>// Powered by moj 3.0 beta 11 [modified TZTester]<br>// Powered by CodeProcessor<br></td>