            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br><br>public class ZenoDivision<br>{<br>  public String cycle(String a, String b)<br>  {<br>    long aa = Long.parseLong(a);<br>    long bb = Long.parseLong(b);<br>    if (aa == 0 &amp;&amp; bb == 1)<br>    {<br>      return "-";<br>    }<br>    if (aa == 1 &amp;&amp; bb == 1)<br>    {<br>      return "*";<br>    }<br>    String ans = "";<br>    long cura = aa;<br>    for (int i = 0; i &lt; 60; i++)<br>    {<br>      cura = cura * 2;<br>      if (cura &gt;= bb || cura &lt; 0)<br>      {<br>        ans = ans + "*";<br>        cura -= bb;<br>      } else<br>      {<br>        ans = ans + "-";<br>      }<br>      if (cura == aa)<br>      {<br>        return ans;<br>      }<br>    }<br>    return "impossible";<br>  }<br>}<br></td>