<td class="problemText" colspan="8" valign="middle" align="left">
            import java.util.*;<br><br>public class MostLikely {<br>  public int likelyRank(int[] sc, int low, int high) {<br>    Arrays.sort(sc);<br>    int[] rank = new int[sc.length + 1];<br>    for (int i = 0; i &lt; sc.length; ++i) {<br>      if (low &lt; sc[i] &amp;&amp; low &lt;= high) {<br>        if (high &lt; sc[i]) {<br>          rank[i] = high - low + 1;<br>          low = high + 1;<br>        } else {<br>          rank[i] = sc[i] - low;<br>          low = sc[i];<br>        }<br>      }<br>    }<br>    if (low &lt;= high) {<br>      rank[sc.length] = high - low + 1;<br>    }<br>    int max = Integer.MIN_VALUE;<br>    int h = -1;<br>    for (int i = 0; i &lt; rank.length; ++i) {<br>      //System.out.println(rank[i]);<br>      if (rank[i] &gt; max) {<br>        max = rank[i];<br>        h = i;<br>      }<br>    }<br>    for (int i = 0; i &lt; rank.length; ++i) {<br>      //System.out.println(max);<br>      if (rank[i] == max &amp;&amp; i != h) {<br>        return -1;<br>      }<br>    }<br>    return sc.length - h + 1;<br>  }<br>}<br></td>