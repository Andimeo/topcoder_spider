            using System;<br><br>public class LikelyWord {<br>  long numWords(string before, int k)<br>  {<br>    while (before.Length &lt; k)<br>      before += 'a';<br>    long num = 0;<br>    for (int i = 0; i &lt; k; ++i)<br>      num = num * 26 + (before[i] - 'a');<br>    if (before.Length &gt; k)<br>      ++num;<br>    return num;<br>  }<br><br>  public int likely(string[] dictionary, int k)<br>  {<br>    long best = -1;<br>    int bestAt = -1;<br>    long pnum = 0;<br>    for (int i = 0; i &lt;= dictionary.Length; ++i)<br>    {<br>      long cnum = i &lt; dictionary.Length ? numWords(dictionary[i], k) : pow(26, k);<br>      long num = cnum - pnum;<br>      if (i &gt; 0 &amp;&amp; dictionary[i - 1].Length == k)<br>        --num;<br>      if (num &gt; best)<br>      {<br>        best = num;<br>        bestAt = i;<br>      } <br>      else if (num == best)<br>      {<br>        bestAt = -1;<br>      }<br>      pnum = cnum;<br>    }<br>    return bestAt;<br>  }<br><br>  private long pow(int a, int b)<br>  {<br>    long res = 1;<br>    for (int i = 0; i &lt; b; ++i)<br>      res *= a;<br>    return res;<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>