            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class HighwayJam<br>{<br>  public double minTime(int[] lanes, int laneChange, int dist)<br>  {<br>    double best = Double.POSITIVE_INFINITY;<br>    for(int i = 0; i &lt; lanes.length; i++)<br>    {<br>      int toGo = dist;<br>      int lane = 0;<br>      double curr = 0;<br>      if(lanes[i]==0) continue;<br>      while(i&gt;lane)<br>      {<br>        curr += 2*(laneChange);<br>        toGo -= 2*(laneChange*lanes[lane]);<br>        lane++;<br>      }<br>      if(toGo/lanes[i] &lt; laneChange &amp;&amp; i!=0) continue;<br>      curr += 1.*toGo/lanes[i];<br>      if(curr&lt;best) best=curr;<br>    }<br>    return best;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>