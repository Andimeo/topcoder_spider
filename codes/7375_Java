            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br><br>public class YankeeSwap<br>{<br>  public String sequenceOfSwaps(String[] preferences)<br>  {<br>    boolean[] used = new boolean[preferences.length];<br>    <br>    String f = "";<br>    <br>    for(int c1=preferences.length-1; c1&gt;=0; c1--){<br>      for(int c2=0; c2&lt;preferences.length; c2++){<br>        if(!used[preferences[c1].charAt(c2) - 'A']){<br>          f = preferences[c1].charAt(c2) + f;<br>          used[preferences[c1].charAt(c2) - 'A'] = true;<br>          c2 = preferences.length;<br>        }<br>      }<br>    }<br>    System.out.println(f);<br>    char[] ca = f.toCharArray();<br>    String ret = "";<br>    <br>    for(int c1=preferences.length-1; c1&gt;=0; c1--){<br>      for(int c2=0; c2&lt;f.length(); c2++){<br>        if(ca[c2] == 'A' + c1){<br>          if(c1 == c2){<br>            ret = '-' + ret;<br>          } else{<br>            ret = (char)('a' + c2) + ret;<br>          }<br>          <br>          char t = ca[c2];<br>          ca[c2] = ca[c1];<br>          ca[c1] = t;<br>          c2 = f.length();<br>        }<br>      }<br>    }<br>    <br>    return ret;<br>  }<br>  <br>  //&lt;%:testing-code%&gt;<br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>