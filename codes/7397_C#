            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class KLastNonZeroDigits<br>{<br>  public string getKDigits(int N, int K)<br>  {<br>    long s = 1;<br>    for ( long i = 2; i &lt;= N; i++ ) {<br>      s *= i;<br>      while ( s % 10 == 0 ) s /= 10;<br>    }<br>    <br>    string k = "" + s;<br>    <br>    if ( K &gt;= k.Length ) return k;<br>    return k.Substring( k.Length - K );<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>