            import java.math.BigInteger;<br>import java.util.*;<br><br>public class KLastNonZeroDigits {<br>  public String getKDigits(int N, int K) {<br>    BigInteger r = BigInteger.ONE;<br>    for (int i=1; i&lt;=N; i++)<br>      r = r.multiply(BigInteger.valueOf(i));<br>    String s = r.toString();<br>    while (s.charAt(s.length() - 1) == '0')<br>      s = s.substring(0, s.length() - 1);<br>    if (s.length() &gt; K)<br>      s = s.substring(s.length() - K, s.length());<br>    return s;<br>  }<br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>