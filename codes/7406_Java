            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class Palindromize2<br>{<br>  public String minChanges(String s)<br>  {<br>    StringBuffer str=new StringBuffer(s);<br>    for (int i=0, j=s.length()-1;i&lt;j;i++,j--)<br>    {<br>      char a=str.charAt(i), b=str.charAt(j);<br>      if (a&lt;b)<br>      {<br>        str.setCharAt(j,a);<br>      }<br>      else<br>        str.setCharAt(i,b);<br>    }<br>    <br>    return str.toString();<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>