            import java.util.*;<br>import java.math.*;<br><br>public class AnagramList{<br>  long[] w = new long[21];<br>  int[] a = new int[26];  <br>  int ll;<br>  <br>  String count(int k){<br>    long p = 0;<br>    ll--;<br>    for (int i = 0; i &lt; 26; i++)<br>      if (a[i] &gt; 0){<br>        a[i]--;<br>        p = soluong(a, ll);<br>        if (p &gt;= k) return (char)(i+'a') + count(k);<br>        k -= p;<br>        a[i]++;<br>      }<br>    return "";<br>  }<br>  <br>  long soluong(int[] b, int len){<br>    long p = w[len];<br>    for (int i = 0; i &lt; 26; i++) <br>      p /= w[b[i]];<br>    return p;<br>  }<br>  <br>  public String getAnagram(String s, int k){<br>    w[0] = 1;<br>    for (int i = 1; i &lt;= 20; i++){<br>      w[i] = 1;<br>      for (int j = 1; j &lt;= i; j++) w[i] *= j;<br>    }<br>    Arrays.fill(a, 0);<br>    for (int i = 0; i &lt; s.length(); i++) a[s.charAt(i)-'a']++;<br>    k++;<br>    if (soluong(a, s.length()) &lt; k) return "";<br>    ll = s.length();<br>    return count(k);<br>  }<br>}<br></td>