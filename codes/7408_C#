            using System;<br><br>public class CommonMultiples {<br>  public int countCommMult(int[] a, int lower, int upper) {<br>      long z = 1;<br>        foreach (int x in a)<br>        {<br>            z = (z/gcd(z, x))*x;<br>            if (z &gt; upper)<br>                return 0;<br>        }<br>      return (int) ((upper/z) - (lower - 1)/z);<br>  }<br><br>    long gcd(long a, long b)<br>    {<br>        while (b &gt; 0)<br>        {<br>            long t = a % b;<br>            a = b;<br>            b = t;<br>        }<br>        return a;<br>    }<br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>