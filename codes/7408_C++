            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br>#include &lt;queue&gt;<br> <br>using namespace std;<br> <br>typedef long long LL;<br>typedef vector &lt;int&gt; vi;<br>typedef vector &lt;LL&gt; vll;<br>typedef vector &lt;string&gt; vs;<br>typedef vector &lt;double&gt; vd;<br> <br>class CommonMultiples {<br>public:<br>  int countCommMult(vector &lt;int&gt;, int, int);<br>};<br> <br>LL gcd(LL a, LL b)<br>{<br>  if (b == 0) return a;<br>  return gcd(b,a%b);<br>}<br> <br>int CommonMultiples::countCommMult(vector &lt;int&gt; a, int lower, int upper) {<br>  LL lcm;<br>  int i;<br>  for (i=0; i&lt;a.size(); i++) <br>  {<br>    if (i == 0) lcm = a[i];<br>    else<br>    {<br>      lcm = (lcm * a[i])/gcd(lcm,a[i]);<br>    }<br>    if (lcm &gt; upper) return 0;<br>  }<br>  <br>  return upper/lcm - (lower-1)/lcm;<br>  <br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>