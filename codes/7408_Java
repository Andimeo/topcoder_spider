            import java.util.*;<br>import java.math.*;<br>import java.io.*;<br>import java.text.*;<br>import java.util.regex.*;<br>import static java.util.Arrays.*;<br>import static java.lang.Math.*;<br>import static java.lang.Integer.parseInt;<br>public class CommonMultiples {<br>  public int countCommMult(int[] a, int lower, int upper) {<br>    long lcm = 1;<br>    for(int i = 0; i &lt; a.length; i++){<br>      lcm = lcm(lcm, a[i]);<br>      if(lcm &gt; upper) return 0;<br>    }<br>    return (int)(upper/lcm - (lower-1)/lcm);<br>  }<br>  long lcm(long a, long b){<br>    return a/gcd(a,b)*b;<br>  }<br>  long gcd(long a, long b){<br>    return a == 0 ? b : gcd(b%a, a);<br>  }<br> <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>