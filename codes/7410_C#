            using System;<br>using System.Collections; <br>using System.Collections.Generic;<br><br>public class DegreesToRadians {<br>  public double convertToRadians(int degrees, int minutes, int seconds) <br>   {<br>        return (((double)degrees + (double)minutes / 60 + (double)seconds / 3600) * Math.PI) / 180;<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>