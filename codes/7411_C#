            using System;<br>using System.Collections;<br>using System.Collections.Specialized;<br> <br>public class TagalogDictionary{<br> string order ="abkdeghilmnzoprstuwy";<br> public string[] sortWords(string[] s){<br>  for (int i = 0; i &lt;s.Length;i++) s[i]=s[i].Replace("ng","z");<br>  for (int i = 0; i &lt;s.Length;i++)<br>   for (int j=i+1;j&lt;s.Length;j++)<br>    if (compare(s[i],s[j]) &gt; 0) {<br>     string t=s[i]; s[i]=s[j]; s[j]=t;<br>    }<br>  for (int i = 0; i &lt;s.Length;i++) s[i]=s[i].Replace("z","ng");<br>  return s;<br> }<br> int compare(string a, string b) {<br>  for (int i=0;i&lt;Math.Min(a.Length, b.Length);i++) {<br>   int x=order.IndexOf(a[i]);<br>   int y=order.IndexOf(b[i]);<br>   if (x&lt;y) return -1; <br>   if (x&gt;y) return 1;<br>  }<br>  if (a.Length==b.Length) return 0;<br>  if (a.Length&gt;b.Length) return 1;<br>  return -1;<br> }<br>}<br>//hilfiger's editor<br></td>