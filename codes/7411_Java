            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class TagalogDictionary<br>{<br>  public String[] sortWords(String[] words)<br>  {<br>    for(int i = 0; i &lt; words.length; i++)<br>      words[i] = words[i].replaceAll("k","c").replaceAll("ng","n"+rep(55,"z"));<br>    Arrays.sort(words);<br>    for(int i = 0; i &lt; words.length; i++)<br>      words[i] = words[i].replaceAll("c","k").replaceAll("n"+rep(55,"z"),"ng");<br>    return words;<br>  }<br>  public String rep(int ct, String let)<br>  {<br>    String ret = let;<br>    for(;ct&gt;0;ct--)<br>      ret+=let;<br>    return ret;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>