            using System;<br>using System.Collections;<br> <br>public class BusTrip<br>{<br>  public int returnTime(int N, string[] buses)<br>  {<br>    int q = buses.Length;<br>    int[][] s = new int[q][];<br>    <br>    for (int i = 0; i &lt; q; ++i)<br>    {<br>      string[] ss = buses[i].Split (' ');<br>      <br>      int j = ss.Length;<br>      s[i] = new int[j];<br>      <br>      for (int k = 0; k &lt; j; ++k)<br>        s[i][k] = int.Parse (ss[k]);<br>    }<br>    <br>    int jat = 0;<br>    int jak = 0;<br>    <br>    int[] x = new int[q];<br>    int[] y = new int[q];<br>    <br>    for (int t = 0; t &lt;= 1000; ++t)<br>    {<br>      if (jat == 0)<br>      {<br>        for (int i = 0; i &lt; q; ++i)<br>        {<br>          if (s[i][x[i]] == jak &amp;&amp; y[i] == 0)<br>          {<br>            jat = 1;<br>            jak = i;<br>            <br>            break;<br>          }<br>        }<br>      }<br>       else<br>      {<br>        if (y[jak] == 0)<br>        {<br>          jat = 0;<br>          jak = s[jak][x[jak]];<br>          <br>          if (jak == 0)<br>            return t;<br>        }<br>      }<br>      <br>      for (int i = 0; i &lt; q; ++i)<br>      {<br>        y[i]++;<br>        <br>        int now = x[i];<br>        int next = (x[i] + 1) % (s[i].Length);<br>        int dist = Math.Abs ((s[i][now] - s[i][next]));<br>        <br>        if (y[i] == dist)<br>        {<br>          y[i] = 0;<br>          x[i] = next;<br>        }<br>      }<br>    }<br>    <br>    return -1;<br>  }<br>}<br> <br> <br>// Powered by FileEdit<br></td>