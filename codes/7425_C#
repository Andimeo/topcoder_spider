            using System;<br>using System.Collections; <br>using System.Collections.Generic;<br><br>public class TrafficReport {<br>  public int bestRoute(string[] route, string[] report) <br>  {  <br>    int res=0;<br><br>        Dictionary&lt;string, int&gt; x = new Dictionary&lt;string, int&gt;();<br>        for (int i = 0; i &lt; report.Length;i++ )<br>        {<br>            string[] t = report[i].Split(' ');<br>            x[t[1]] = Convert.ToInt32(t[0]);<br>        }<br><br>        for (int i = 0; i &lt; route.Length; i++)<br>        {<br>            string[] t = route[i].Split(' ');<br>            res += Convert.ToInt32(t[0]);<br><br>            if (x.ContainsKey(t[1]))<br>                res += x[t[1]];<br>        }<br><br>    return res;<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>