            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class TrafficReport<br>{<br>  public int bestRoute(String[] route, String[] report)<br>  {<br>    Map&lt;String,Integer&gt; ma=new TreeMap();<br>    for(String s:report) {<br>      String[] tam=s.split(" ");<br>      ma.put(tam[1], Integer.parseInt(tam[0]));<br>    }<br>    int res=0;<br>    for(String s:route) {<br>      String[] tam=s.split(" ");<br>      res+=Integer.parseInt(tam[0]);<br>      if(ma.containsKey(tam[1])) res+=ma.get(tam[1]);<br>    }<br>    return res;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>