            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class HowManyBirthdays<br>{<br>  public string[] getList(string[] friends, string today)<br>  {<br>        List&lt;string&gt; res = new List&lt;string&gt;();<br>        string day, month;<br>        string[] strs = today.Split(' ');<br>        day = strs[0];<br>        month= strs[1];<br>        foreach (string str in friends)<br>        {<br>            string[] all = str.Split(' ');<br>            if (day == all[1] &amp;&amp; month == all[2]) res.Add(all[0]);<br>        }<br>        res.Sort();<br>        return res.ToArray();<br>  }<br>}<br></td>