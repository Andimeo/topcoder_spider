            #include &lt;sstream&gt; <br>#include &lt;iostream&gt;<br>#include &lt;string.h&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;stdlib.h&gt; <br>#include &lt;stdio.h&gt; <br>#include &lt;numeric&gt;<br>#include &lt;math.h&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;stack&gt;<br>#include &lt;queue&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>using namespace std;<br> <br>#define forn(i,n)     for(int i=0; i&lt;n; ++i)<br>#define forv(i,v)     forn(i,int(v.size()))<br>#define ALL(a)        (a).begin(),(a).end()<br>#define pb            push_back<br>#define sz            size()<br>#define SORT(a)       sort(ALL(a))<br>#define iss           istringstream<br> <br>typedef vector&lt; int &gt; vi; typedef vector&lt; string &gt; vs;<br> <br>class HowManyBirthdays {<br>public:<br>    vector &lt;string&gt; getList(vector &lt;string&gt; friends, string today) {<br>      vector &lt;string&gt; res;<br>      <br>      int t1,t2;<br>      iss in2(today);<br>      in2&gt;&gt;t1&gt;&gt;t2;<br>      <br>      forv (i,friends)<br>      {<br>        iss in(friends[i]);<br>        string a;<br>        int b,c;<br>        in&gt;&gt;a&gt;&gt;b&gt;&gt;c;<br>        if (b==t1&amp;&amp;c==t2)<br>        res.pb(a);<br>      }<br>      <br>      SORT(res);<br>      <br>         return res;<br>    }<br> <br>};<br> <br> <br> <br> <br>// Powered by PopsEdit<br></td>