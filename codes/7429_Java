            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class HowManyBirthdays<br>{<br>  public String[] getList(String[] friends, String today)<br>  {<br>    Set&lt;String&gt; ss = new TreeSet&lt;String&gt;();<br>    for(String s:friends) {<br>      String [] tam = s.split(" ");<br>      if(today.equals(tam[1]+' '+tam[2])) {<br>        ss.add(tam[0]);<br>      }<br>    }<br>    String[] res=new String[ss.size()];<br>    int i=0;<br>    for(String s:ss) {<br>      res[i++] = s;<br>    }<br>    return res;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>