            #include &lt;algorithm&gt;<br>#include &lt;iostream&gt;<br>#include &lt;stack&gt;<br>#include &lt;string&gt;<br>#include &lt;utility&gt;<br>#include &lt;vector&gt;<br><br>using namespace std;<br><br><br>class BuildingAdvertise {<br>public:<br>   long long getMaxArea( vector &lt;int&gt; h, int n ) { // caret here<br>      vector&lt;int&gt; histo(n);<br>      <br>      int j = 0;<br>      int m = h.size();<br>      for ( int i=0; i&lt;n; ++i ) {<br>         histo[i] = h[j];<br>         int s = (j+1)%m;<br>         h[j] = ( ( h[j] ^ h[s] ) + 13 ) % 835454957;<br>         j = s;<br>      }<br>      histo.push_back( 0 );<br>      <br>      stack&lt;pair&lt;long long, long long&gt; &gt; st;<br>      long long best = 0;<br>      <br>      for ( int j=0; j&lt;=n; ++j ) {<br>         int dokle = j;<br>         while ( !st.empty() &amp;&amp; histo[j] &lt;= st.top().first ) {<br>            best = max( best, st.top().first * (j-st.top().second) );<br>            dokle = st.top().second;<br>            st.pop();<br>         }<br>         st.push( make_pair( histo[j], dokle ) );<br>      }<br><br>      return best;<br>   }<br>};<br><br><br> <br><br><br><br>// Powered by FileEdit<br>// Powered by moj 3.0 beta 11 [modified TZTester]<br>// Powered by CodeProcessor<br></td>