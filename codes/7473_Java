            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>class MaxArmy {<br>  public long getMax(long[] a) {<br>    int[] l, r;<br>    int n = a.length;<br>    l=new int[n];<br>    r=new int[n];<br>    for(int i=0;i&lt;n;++i) {<br>      int x = i-1;<br>      while( (x&gt;=0) &amp;&amp; (a[x]&gt;=a[i]) ) x = l[x] ;<br>      l[i] = x;<br>    }<br>    for(int i=n-1;i&gt;=0;--i) {<br>      int x = i+1;<br>      while( (x&lt;n) &amp;&amp; (a[x]&gt;=a[i]) ) x = r[x] ;<br>      r[i] = x;<br>    }<br>    long max=-1;<br>    for(int i=0;i&lt;n;++i) {<br>      long x = a[i]*(r[i]-l[i]-1);<br>      if(x&gt;max)<br>        max=x;<br>    }<br>    return max;<br>  }<br>}<br> <br> <br>public class BuildingAdvertise<br>{<br>  long[] a;<br>  long[] create(int[] h,int n) {<br>    long[] R=new long[n];<br>    int j = 0 ;<br>    int m = h.length;<br>    for ( int i = 0; i&lt;n; ++i) {<br>      R[i] = h[j] ;<br>      int s = (j+1)%m ;<br>      h[j] = ( ( h[j] ^ h[s] ) + 13 ) % 835454957 ;<br>      j = s ;<br>    }<br>    return R;<br>  }<br>  public long getMaxArea(int[] h, int n)<br>  {<br>    a = create(h, n);<br>    return new MaxArmy().getMax(a);<br>  }<br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>