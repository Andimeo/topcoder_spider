            using System;<br>using System.Text;<br><br>public class CDPlayer {<br>  public int isRandom(string[] songlist, int n) {<br>    StringBuilder builder = new StringBuilder();<br>    foreach (string line in songlist)<br>      builder.Append(line);<br>    string input = builder.ToString();<br>    for (int i = 0; i &lt; n; ++i)<br>    {<br>      bool[] was = new bool[n];<br>      int toGo = i == 0 ? n : i;<br>      bool ok = true;<br>      foreach (char ch in input)<br>      {<br>        if (was[ch - 'A'])<br>        {<br>          ok = false;<br>          break;<br>        }<br>        was[ch - 'A'] = true;<br>        --toGo;<br>        if (toGo == 0)<br>        {<br>          toGo = n;<br>          for (int j = 0; j &lt; n; ++j)<br>            was[j] = false;<br>        }<br>      }<br>      if (ok)<br>        return i;<br>    }<br>    return -1;<br>  }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>