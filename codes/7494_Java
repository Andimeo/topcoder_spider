            public class TwoTrains<br>{<br>  public int passengerNewVasyuki(int t1, int t2, int[] times)<br>  {<br>    int counter = 0;<br>    for (int i = 0; i &lt; times.length; i++)<br>    {<br>      int at1, at2;<br>      int rt1, rt2;<br>      rt1 = times[i] % t1;<br>      rt2 = times[i] % t2;<br>      if (rt1 != 0) rt1 = t1;<br>      if (rt2 != 0) rt2 = t2;<br>      at1 = times[i] / t1 * t1 + rt1;<br>      at2 = times[i] / t2 * t2 + rt2;<br>      if (at1 &lt;= at2)<br>        counter++;<br>    }<br>    return counter;<br>  }<br>}<br></td>