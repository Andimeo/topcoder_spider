            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class Provinces<br>{<br>    public int maximalNumber(int n, String[] roads)<br>  {<br>    boolean[][] tab = new boolean[n][n];<br>        String s = "";<br>        for (int i = 0; i &lt; roads.length; i++) {<br>            s += roads[i];<br>        }<br>        for (int i = 0; i &lt; n-1; i++) {<br>            for (int j = i + 1; j &lt; n; j++)<br>                tab[i][j] = true;<br>        }<br>        StringTokenizer st = new StringTokenizer(s, " ,");<br>        while(st.hasMoreTokens()) {<br>            tab[Integer.parseInt(st.nextToken()) - 1][Integer.parseInt(st.nextToken()) - 1] = true;<br>        }<br>        for (int i = 0; i &lt; n; i++) {<br>            int j = n - 1;<br>            while (j &gt; i &amp;&amp; !tab[j][i]) {<br>                j--;<br>            }<br>            for (int t = i + 1; t &lt;= i; t++) {<br>                tab[t][i] = true;<br>            }<br>        }<br>        /*for (int i = 0; i &lt; n; i++) {<br>            for (int j = 0; j &lt; n; j++) {<br>                System.out.print(tab[i][j] + " ");<br>            }<br>            System.out.println();<br>        }*/<br>        int[] col = new int[n];<br>        int ncol = 0;<br>        for (int i = 0; i &lt; n; i++) {<br>            int j = n - 1;<br>            while (j &gt; i &amp;&amp; tab[j][i] == false)<br>                j--;<br>            if (col[i] == 0) {<br>                ncol++;<br>                col[i] = ncol;<br>            }<br>            for (int t = i + 1; t &lt;= j; t++)<br>                col[t] = col[i];<br>        }<br>        int ans;<br>        for (ans = n; ans &gt; 1; ans--) {<br>            if (n % ans == 0) {<br>                int r = n / ans;<br>                boolean fl = true;<br>                for (int i = r ; i &lt; n; i += r) {<br>                    if (col[i] == col[i - 1])<br>                        fl = false;<br>                }<br>                if (fl)<br>                   break;<br>            }<br>        }<br>        return ans;<br>    }<br> <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>