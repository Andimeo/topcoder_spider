            #include &lt;algorithm&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;string&gt;<br>#include &lt;utility&gt;<br>#include &lt;vector&gt;<br> <br>using namespace std;<br> <br>typedef long long Long;<br>typedef double dbl;<br>typedef vector &lt;int&gt; VI;<br>typedef pair &lt;int, int&gt; PII;<br> <br>#define FI first<br>#define MP make_pair<br>#define PB push_back<br>#define SE second<br> <br>class SortingPhotos<br>{<br>  public:<br>    vector &lt;int&gt; sortPhotos( vector &lt;int&gt; W, vector &lt;int&gt; H )<br>    {<br>      VI Ans(3, 0);<br>      int N = (int)W.size();<br>      for (int i = 0; i &lt; N; i++)<br>      {<br>        int f1 = 0, f2 = 0, g1 = 1, g2 = 1;<br>        for (int j = 0; j &lt; N; j++)<br>          if (W[j] &lt; W[i] &amp;&amp; H[j] &lt; H[i])<br>            f2 = 1, g1 = 0;<br>          else if (W[j] &gt; W[i] &amp;&amp; H[j] &gt; H[i])<br>            f1 = 1, g2 = 0;<br>        if (f1 &amp;&amp; g1)<br>          Ans[0]++;<br>        else if (f2 &amp;&amp; g2)<br>          Ans[2]++;<br>        else<br>          Ans[1]++;<br>      }<br>      return Ans;<br>    }<br>};<br> <br>// Code by Burunduk3 =)<br> <br> <br>// Powered by PopsEdit<br></td>