            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class SortingPhotos<br>{<br>  public int[] sortPhotos(int[] W, int[] H)<br>  {<br>    int n = W.length;<br>    boolean isS, isL;<br>    int[] res = {0, 0, 0};<br>    for (int i = 0; i &lt; n; i++)<br>    {<br>      isS = false; isL = false;<br>      for (int j = 0; j &lt; n; j++)<br>      {<br>        if (W[i] &lt; W[j] &amp;&amp; H[i] &lt; H[j])<br>        {<br>          isS = true;<br>        }<br>        if (W[i] &gt; W[j] &amp;&amp; H[i] &gt; H[j])<br>        {<br>          isL = true;<br>        }<br>      }<br>      if (isS &amp;&amp; !isL) res[0]++; else<br>      if (isL &amp;&amp; !isS) res[2]++; else res[1]++;<br>    }<br>    return res;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>