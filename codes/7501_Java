            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class LongestSubanagramChain<br>{<br>  public int getLength(String[] B)<br>  {<br>    int[][] vals = new int[B.length][27];<br>    for(int i = 0;i&lt;B.length;i++) {<br>      for(int j = 0;j&lt;B[i].length();j++) {<br>        vals[i][B[i].charAt(j)-'a']++;<br>      }<br>    }<br>    int[][] max = new int[B.length][27];<br>    <br>    for(int j = 25;j&gt;=0;j--) {<br>      for(int i = B.length-1;i&gt;=0;i--) {<br>        max[i][j] = vals[i][j] + max[i][j+1];<br>        if(i==B.length-1)continue;<br>        max[i][j] = Math.min(max[i][j], max[i+1][j]);<br>      }<br>    }<br>    //for(int i = 0;i&lt;max.length;i++)System.out.println(Arrays.toString(max[i]));<br>    return max[0][0];<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>