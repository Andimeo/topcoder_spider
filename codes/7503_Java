            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class CssPropertyConverter<br>{<br>  public String getCamelized(String css)<br>  {<br>    String ret="";<br>    for(int i=0;i&lt;css.length();i++){<br>      char c = css.charAt(i);<br>      if(c=='-'){<br>        c= css.charAt(++i);<br>        ret+=(char)(c-32);<br>      }<br>      else<br>        ret+=c;<br>    }<br>    return ret;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>