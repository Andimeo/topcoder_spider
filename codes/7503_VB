<td class="problemText" colspan="8" valign="middle" align="left">
            Public Class CssPropertyConverter<br>    Public Function getCamelized(ByVal cssPropertyName As String) As String<br>        Dim words() As String = cssPropertyName.Split(New Char() {"-"c})<br>        Dim r As String = words(0).ToLower<br><br>        For i As Integer = 1 To words.Length - 1<br>            Dim str As String = words(i)<br>            r &amp;= str.Substring(0, 1).ToUpper &amp; str.Substring(1).ToLower<br>        Next<br><br>        Return r<br>    End Function<br>End Class<br></td>