            public class FamilyTravel<br>{<br>    static final int MAXN = 60;<br>    <br>    public int shortest(String[] G)<br>    {<br>        int N = G.length;<br>        <br>        int D[][] = new int[N][MAXN];        <br>        for ( int a = 0; a &lt; N; ++a )<br>            for ( int b = 0; b &lt; MAXN; ++b )<br>                D[a][b] = 12345689;        <br>        <br>        boolean Mark[][] = new boolean[N][MAXN];<br>        D[0][MAXN - 1] = 0;<br>        for ( int i = 0; i &lt; N * MAXN; ++i )<br>        {<br>            int a0 = -1, b0 = -1, m = 1234567890;<br>            for ( int a = 0; a &lt; N; ++a )<br>                for ( int b = 0; b &lt; MAXN; ++b )<br>                    if ( !Mark[a][b] )<br>                    {<br>                        if ( m &gt; D[a][b] )<br>                        {<br>                            m = D[a][b];<br>                            a0 = a;<br>                            b0 = b;<br>                        }<br>                    }<br>            Mark[a0][b0] = true;<br>            for ( int a = 0; a &lt; N; ++a )<br>                for ( int b = 0; b &lt; MAXN; ++b )<br>                    if ( !Mark[a][b] )<br>                    {<br>                        int len = getLen(G[a0].charAt(a)); <br>                        if ( len == 0 ) continue;<br>                        if ( len &lt;= b0 &amp;&amp; b == len )<br>                            D[a][b] = Math.min(D[a][b], D[a0][b0] + len);<br>                    }<br>                            <br>        }<br>        int res = 123456789;<br>        for ( int b = 0; b &lt; MAXN; ++b )<br>            res = Math.min( res, D[1][b] );<br>        if ( res &gt;= 12345678 ) return -1;<br>        return res;<br>    }<br><br>    private int getLen(char ch)<br>    {<br>        if ( ch == '0' ) return 0;<br>        if ( 'a' &lt;= ch &amp;&amp; ch &lt;= 'z' ) return ch - 'a' + 1;<br>        return ch - 'A' + 27;<br>    }<br>}<br></td>