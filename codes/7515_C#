            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class CountingCrowds <br>{<br>  public int minimalInterpretation(string[] sensorData) <br>  {<br>        bool[] b = new bool[26];<br>        for (int i = 0; i &lt; 26; i++)<br>        {<br>            b[i] = false;<br>        }<br>        string s = "";<br>        int ans = 0;<br>        foreach (string qwe in sensorData)<br>        {<br>            s += qwe;<br>        }<br>        s += '.';<br>        foreach (char c in s)<br>        {<br>            if (c == '.')<br>            {<br>                for (int i = 0; i &lt; 26; i++)<br>                {<br>                    if (b[i])<br>                    {<br>                        ans++;<br>                        b[i] = false;<br>                    }<br>                }<br>            }<br>            else<br>            {<br>                int r = c - 'A';<br>                b[r] = true;<br>                for (int i = r-1; i &gt;-1; i--)<br>                {<br>                    if (b[i])<br>                    {<br>                        ans++;<br>                        b[i] = false;<br>                    }<br>                }<br>            }<br>        }<br>        return ans;<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by r1cs1]<br>// Powered by CodeProcessor<br></td>