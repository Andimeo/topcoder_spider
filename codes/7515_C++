            #include &lt;algorithm&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br><br>using namespace std;<br><br><br>class CountingCrowds {<br>public:<br>    int minimalInterpretation( vector &lt;string&gt; sensorData ) {<br>    string S;<br><br>    for( int i = 0; i &lt; sensorData.size(); ++i ){<br>      S += sensorData[i];<br>    }<br><br>    int sol = 0;<br><br>    for( int i = 0; i &lt; S.size(); ++i ){<br>      if( S[i] != '.' ){<br>        for( int k = i+1; k &lt; S.size(); ++k ){<br>          if( S[k] == '.' ) break;<br>          if( S[k] &gt; S[i] ) break;<br>          if( S[k] == S[i] ) S[k] = '.';<br>        }<br>        S[i] = '.';<br>        ++sol;<br>      }<br>    }<br>       <br>    return sol;<br>    }<br>};<br><br><br><br><br><br><br><br>// Powered by FileEdit<br>// Powered by moj 3.0 beta 11 [modified TZTester]<br>// Powered by CodeProcessor<br></td>