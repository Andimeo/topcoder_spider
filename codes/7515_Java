<td class="problemText" colspan="8" valign="middle" align="left">
            public class CountingCrowds<br>{<br>    public int minimalInterpretation(String[] A)<br>    {<br>        String s = "";<br>        for ( String s1 : A ) s += s1;<br>        s += ".";<br>        <br>        int N = s.length(), res = 0;        <br>        for ( char ch = 'A'; ch &lt;= 'Z'; ++ch )<br>        {<br>            int beg = -1;<br>            for ( int i = 0; i &lt; N; ++i  )<br>                if ( s.charAt(i) == ch )<br>                {<br>                    if ( beg == -1 ) beg = i;<br>                }<br>                else<br>                {<br>                    if ( beg != -1 )<br>                        if ( s.charAt(i) == '.' || (s.charAt(i) &gt; ch) )<br>                        {<br>                            ++res;<br>                            beg = -1;<br>                        }<br>                }<br>        }<br>        return res;<br>    }<br>}<br></td>