            #include &lt;iostream&gt;<br>#include &lt;fstream&gt;<br>#include &lt;math.h&gt;<br>#include &lt;stdlib.h&gt;<br>#include &lt;stdio.h&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;iterator&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;map&gt;<br>#include &lt;queue&gt;<br>#include &lt;set&gt;<br>#include &lt;stack&gt;<br>#include &lt;sstream&gt;<br>#include &lt;cctype&gt;<br>#include &lt;assert.h&gt;<br>#include &lt;list&gt;<br>#include &lt;ext/hash_set&gt;<br>#include &lt;ext/hash_map&gt;<br><br>using namespace __gnu_cxx;<br>using namespace std;<br><br>#define MP(a,b) make_pair(a,b)<br>#define i64 long long<br>#define pb push_back<br>#define For(i,a,b) for(typeof(a) i=(a);i&lt;(b);i++)<br>#define Rev(i,a,b) for(typeof(a) i=(a);i&gt;=(b);i--)<br>#define FOREACH(V,it) for(typeof(V.begin()) it=V.begin();it!=V.end();it++)<br>#define CLR(a,x) memset(a,x,sizeof(a))<br>#define ALL(x) x.begin(),x.end()<br><br>/**********************************************************************************/<br><br>int temp( char c){<br>  if (c&gt;='A' &amp;&amp; c&lt;='Z') return -1*(c-'A'+1);<br>  if (c=='0') return 0;<br>  if (c&gt;='a' &amp;&amp; c&lt;='z') return c-'a'+1;<br>}<br><br>vector&lt;int&gt; V;<br>int SUM(int x, int y){<br>  if (x) return V[y]-V[x-1];<br>  return V[y];<br>}<br><br><br>struct CrazyCompetition{<br>public:<br>  double differenceTemperature(int S, int W, vector &lt;string&gt; te){<br>    string s;<br>    For(i,0,te.size())<br>      s+=te[i];<br>    For(i,0,s.size())<br>      V.pb( temp(s[i]));<br>    For(i,1,V.size()) V[i]+=V[i-1];<br><br>    double ret=-1e100;<br>    For(i,0,V.size()-S+1){<br>      double r1= SUM(i,i+S-1)/(double)S;<br>    //  cout &lt;&lt; r1 &lt;&lt; endl;<br>      For(j,0,i-W+1){<br>        double r2= SUM(j,j+W-1)/(double)W;<br>        ret&gt;?=r1-r2;<br>      }<br>      For(j,i+S,V.size()-W+1){<br>        double r2= SUM(j,j+W-1)/(double)W;<br>        ret&gt;?=r1-r2;<br>      }<br>    }<br>    return ret;<br>  }<br><br>};<br></td>