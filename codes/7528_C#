            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class StringOfNumbers<br>{<br>  public static int getLeastN(string[] a)<br>  {<br>    string s = "";<br>        int i;<br>    for (i = 0; i &lt; a.Length; i++)<br>      s = s + a [i];<br>    int n = s.Length;<br>    int sol = 1;<br>    i = 0;<br>    while (i &lt; n)<br>    {<br>            string tmp = sol.ToString();<br>            while (i &lt; n)<br>            {<br>                if (tmp.Contains(s[i].ToString()))<br>                    tmp = tmp.Substring(tmp.IndexOf(s[i].ToString()) + 1);<br>                else<br>                    break;<br>                i++;<br>            }<br>            if (i == n)<br>                break;<br>            sol++;<br>    }<br>    return sol;<br>  }<br>}<br></td>