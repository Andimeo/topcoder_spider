            #include &lt;cstdio&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br>#include &lt;climits&gt;<br>#include &lt;cmath&gt;<br><br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;iostream&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;queue&gt;<br>#include &lt;set&gt;<br>#include &lt;sstream&gt;<br>#include &lt;stack&gt;<br>#include &lt;vector&gt;<br>using namespace std;<br><br><br>class StringOfNumbers {<br>  public:<br>  int getLeastN(vector &lt;string&gt; leftDigits) {<br>    string s; for( int i = 0; i &lt; leftDigits.size(); ++i ) s += leftDigits[i];<br><br>        int i = 0;<br>        for( int ret = 1;; ++ret ) {<br>            ostringstream O; O &lt;&lt; ret;<br>            string a = O.str();<br>            for( int j = 0; j &lt; a.size(); ++j ) {<br>                if( a[j] == s[i] ) ++i;<br>                if( i == s.size() ) return ret;<br>            }<br>        }<br>        return -1;<br>  }<br>  <br><br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>