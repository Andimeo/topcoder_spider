            #include &lt;algorithm&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>using namespace std;<br>#define FORC( V, it ) for( __typeof( (V).begin() ) it = (V).begin(); it != (V).end(); ++it )<br>const int MAXL = 1000;<br>const int MAXN = 50;<br>char buff[MAXL+1];<br><br><br>class LargestCountry {<br>public:<br>    string getLargest( vector &lt;string&gt; V ) {<br>        vector&lt; int &gt; cnt( 26, 0 );<br>        for( int i = 0; i &lt; ( int )V.size(); ++i ) <br>            for( int j = 0; j &lt; ( int )V[i].size(); ++j ) <br>                if ( V[i][j] != ' ' )<br>                    ++cnt[V[i][j]-'A'];<br>        int mx = 0;<br>        for( int i = 0; i &lt; ( int )cnt.size(); ++i ) <br>            if ( cnt[i] &gt; cnt[mx] )<br>                mx = i;<br>        return string( 1, 'A' + mx );<br>    }<br>};<br><br><br><br><br><br><br><br><br><br>// Powered by FileEdit<br>// Powered by moj 3.0 beta 11 [modified TZTester]<br>// Powered by CodeProcessor<br></td>