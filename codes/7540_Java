            import java.io.*;<br>import java.util.*;<br><br>public class QuantumAlchemy {<br>  public int minSteps(String initial, String[] reactions) {<br>    int[] init = new int[26];<br>    int inn = initial.length();<br>    for (int i = 0; i &lt; inn; i++) {<br>      init[initial.charAt(i) - 'A']++;<br>    }<br>    int[] need = new int[26];<br>    need[23] = 1;<br>    int needn = 1;<br>    int ans = 0;<br>    while (true) {<br>      int get = -1;<br>      for (int i = 0; i &lt; 26; i++) {<br>        if (need[i] &gt; init[i]) {<br>          get = i;<br>          break;<br>        }<br>      }<br>      if (get &lt; 0)<br>        return ans;<br>      if (needn &gt; inn)<br>        return -1;<br>      boolean ch = false;<br>      for (String s : reactions) {<br>        if (s.charAt(s.length() - 1) == ('A' + get)) {<br>          for (int i = 0;; i++) {<br>            if (s.charAt(i) == '-')<br>              break;<br>            int v = s.charAt(i) - 'A';<br>            need[v]++;<br>            needn++;<br>          }<br>          need[get]--;<br>          needn--;<br>          ch = true;<br>          break;<br>        }<br>      }<br>      if (!ch)<br>        return -1;<br>      ans++;<br>    }<br>  }<br>}<br></td>