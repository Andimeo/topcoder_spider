            using System;<br><br>public class SortMaterials<br>{<br>  public int totalVolume(string[] data, string[] requirements)<br>  {<br>    int total = 0;<br>    foreach (string s in data)<br>    {<br>      string[] split = s.Split(new string[] {" "}, StringSplitOptions.RemoveEmptyEntries);<br>      int edge = int.Parse(split[0]);<br>      int quality = int.Parse(split[1]);<br>      string color = split[2];<br>      if (Fit(requirements, edge, quality, color))<br>        total += edge * edge * edge;<br>    }<br>    return total;<br>  }<br><br>  private bool Fit(string[] requirements, int edge, int quality, string color)<br>  {<br>    foreach (string req in requirements)<br>    {<br>      string[] split = req.Split('=');<br>      if (split[0] == "EDGE")<br>      {<br>        if (edge != int.Parse(split[1])) return false;<br>      }<br>      else if (split[0] == "QUALITY")<br>      {<br>        if (quality &lt; int.Parse(split[1])) return false;<br>      }<br>      else if (color != split[1]) return false;<br>    }<br>    return true;<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>