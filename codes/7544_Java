            import java.util.*;<br>import java.util.regex.*;<br>public class SortMaterials {<br>    public int totalVolume(String[] data, String[] requirements) {<br>      int vol = 0;<br>        MAT:<br>        for(String dt : data) {<br>          String xs[] = dt.split(" ");<br>          for(String r0 : requirements) {<br>            String r[] = r0.split("=");<br>            if(r[0].equals("EDGE") &amp;&amp; Integer.parseInt(xs[0]) != Integer.parseInt(r[1])) continue MAT;<br>            if(r[0].equals("QUALITY") &amp;&amp; Integer.parseInt(xs[1]) &lt; Integer.parseInt(r[1])) continue MAT;<br>            if(r[0].equals("COLOR") &amp;&amp; ! xs[2].equals(r[1])) continue MAT;<br>          }<br>          int t = Integer.parseInt(xs[0]);<br>      vol += t*t*t;<br>        }<br>        return vol;<br>    }<br> <br>}<br> <br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>