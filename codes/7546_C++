            #include &lt;algorithm&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;iostream&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;sstream&gt;<br>#include &lt;string&gt;<br>#include &lt;utility&gt;<br>#include &lt;vector&gt;<br> <br>using namespace std;<br> <br>class CellularPhoneTarif<br>{<br>  public:<br>  int calculatePrice(int seconds)<br>  {<br>    int minutes = (seconds + 60) / 60;<br>    if(minutes &lt;= 5) return 5 + 10 * minutes;<br>    return 5 + 10 * 5 + 3 * (minutes - 5);<br>  }<br>};<br></td>