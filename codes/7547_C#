            public class PulseDial<br>{<br>  public string toDigits(string pulses)<br>  {<br>    char[] sc = new char[] { '*' };<br>    string[] nums = pulses.Split(sc);<br>    string final = string.Empty;<br>    foreach(string num in nums)<br>      if(num != string.Empty)<br>        if(num.Length == 10)<br>          final += "0";<br>        else<br>          final += string.Empty + num.Length;<br>    if(final.Length == 11)<br>      final = "+" + final;<br>    else<br>    {<br>      final = final.Substring(0,3) + "-" + final.Substring(3, 2) + "-" + final.Substring(5);<br>    }<br>    return final;<br>  }<br>}<br></td>