            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class PulseDial<br>{<br>  public:<br>  string toDigits (string raw)<br>  {<br>      int len = raw.length ();<br>      <br>      vector &lt;int&gt; nums;<br>      <br>      int currtot = 0;<br>      <br>      for (int i = 0; i &lt; len; i++)<br>      {<br>          if (raw [i] == '-')<br>              currtot++;<br>          else<br>          {<br>              if (currtot)<br>                  nums.push_back (currtot == 10 ? 0 : currtot);<br>              currtot = 0;<br>          }<br>      }<br>      <br>      if (currtot)<br>          nums.push_back (currtot == 10 ? 0 : currtot);<br>      <br>      stringstream s;<br>      <br>      if (nums.size () == 7)<br>      {<br>          s &lt;&lt; nums [0] &lt;&lt; nums [1] &lt;&lt; nums [2] &lt;&lt; '-' &lt;&lt; nums [3] &lt;&lt; nums [4] &lt;&lt; '-' &lt;&lt; nums [5] &lt;&lt; nums [6];<br>          return s.str ();<br>      }<br>      else<br>      {<br>          s &lt;&lt; '+';<br>          for (int i = 0; i &lt; nums.size (); i++)<br>              s &lt;&lt; nums [i];<br>          <br>          return s.str ();<br>      }<br>  }<br>};<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>