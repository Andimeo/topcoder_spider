            using System;<br>using System.Collections.Generic;<br>using System.Text;<br><br>class PowerOfInteger<br>{<br>  public int greatestPower(string left, string right)<br>  {<br>    long lnum = long.Parse(left);<br>    long rnum = long.Parse(right);<br><br>    int range = (int)(Math.Sqrt(rnum) + 1);<br>    int max = 1;<br>    for (int i = 2; i &lt;= range; ++i)<br>    {<br>      long r = i;<br>      for (int j = 2; ; ++j)<br>      {<br>        r *= i;<br>        if (r &gt;= lnum &amp;&amp; r &lt;= rnum)<br>        {<br>          if (max &lt; j)<br>          {<br>            max = j;<br>          }<br>        }<br>        if (r &gt; rnum)<br>        {<br>          break;<br>        }<br>      }<br>    }<br><br>    return max;<br>  }<br>}<br></td>