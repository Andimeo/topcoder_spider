            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;sstream&gt;<br>#include &lt;utility&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cmath&gt;<br> <br>using namespace std;<br> <br>#define PB push_back<br>#define MP make_pair<br>#define FI first<br>#define SE second<br>#define sqr(x) ((x) * (x))<br> <br>typedef long long llong;<br>typedef long double ldbl;<br> <br>typedef vector &lt;int&gt; VI;<br>typedef vector &lt;string&gt; VS;<br>typedef pair &lt;int, int&gt; PII;<br> <br>#define m 1010<br> <br>class ColorfulDisks<br>{<br>public:<br>  vector &lt;string&gt; singleColor(vector &lt;string&gt; d)<br>  {<br>    set &lt;string&gt; a, b[m];<br>    for (int i = 0; i &lt; d.size(); i++)<br>    {<br>      char buf[99];<br>      int n;<br>      sscanf(d[i].c_str(), "%s%d", buf, &amp;n);<br>      a.insert(string(buf));<br>      b[n].insert(string(buf));<br>    }<br>    vector &lt;string&gt; ans;<br>    for (set &lt;string&gt; :: iterator i = a.begin(); i != a.end(); i++)<br>    {<br>      int f = 1;<br>      for (int j = 0; j &lt; m; j++)<br>        if (b[j].size() &amp;&amp; !b[j].count(*i))<br>          f = 0;<br>      if (f)<br>        ans.PB(*i);<br>    }<br>    sort(ans.begin(), ans.end());<br>    return ans;<br>  }<br>};<br> <br> <br>// Powered by PopsEdit<br></td>