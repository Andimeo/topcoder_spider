            #include &lt;algorithm&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;string&gt;<br>#include &lt;utility&gt;<br>#include &lt;vector&gt;<br> <br>using namespace std;<br> <br>typedef long long Long;<br>typedef double dbl;<br>typedef vector &lt;int&gt; VI;<br>typedef pair &lt;int, int&gt; PII;<br> <br>#define FI first<br>#define MP make_pair<br>#define PB push_back<br>#define SE second<br> <br>#define eps 1e-9<br> <br>int Count;<br>double Value;<br>VI A, B, R;<br> <br>void dfs( int p, int i )<br>{<br>  if (i == 5)<br>  {<br>    Count++;<br>    int Temp = 0;<br>    VI X = R;<br>    sort(X.begin(), X.end());<br>    set &lt;int&gt; S;<br>    for (int j = 0; j &lt; 5; j++)<br>      S.insert(X[j]);<br>    if (X[0] == X[1] &amp;&amp; X[0] == X[2] &amp;&amp; X[0] == X[3] &amp;&amp; X[0] == X[4])<br>      Temp = max(Temp, B[0]);<br>    if (S.count(2) &amp;&amp; S.count(3) &amp;&amp; S.count(4) &amp;&amp; S.count(5) &amp;&amp; (S.count(1) || S.count(6)))<br>      Temp = max(Temp, B[1]);<br>    if (S.count(3) &amp;&amp; S.count(4) &amp;&amp; ((S.count(1) &amp;&amp; S.count(2)) || (S.count(2) &amp;&amp; S.count(5)) || (S.count(5) &amp;&amp; S.count(6))))<br>      Temp = max(Temp, B[2]);<br>    if (X[0] == X[1] &amp;&amp; X[3] == X[4] &amp;&amp; (X[2] == X[1] || X[2] == X[3]))<br>      Temp = max(Temp, B[3]);<br>    Value += Temp;<br>    return;<br>  }<br>  if (p &amp; (1 &lt;&lt; i))<br>    for (int k = 1; k &lt;= 6; k++)<br>      R[i] = k, dfs(p, i + 1);<br>  else<br>    R[i] = A[i], dfs(p, i + 1);<br>}<br> <br>class YahtzeeAdversary<br>{<br>  public:<br>    vector &lt;int&gt; reRollDice( vector &lt;int&gt; scores, vector &lt;int&gt; dice )<br>    {<br>      R.resize(5);<br>      A = dice;<br>      B = scores;<br>      VI Ans;<br>      double AnsVal = 1e9;<br>      for (int p = 0; p &lt; (1 &lt;&lt; 5); p++)<br>      {<br>        int t = 0;<br>        VI T(0);<br>        for (int i = 0; i &lt; 5; i++)<br>          if (p &amp; (1 &lt;&lt; i))<br>            t++, T.PB(i);<br>        sort(T.begin(), T.end());<br>        if (t &lt; 2)<br>          continue;<br>        Value = 0;<br>        Count = 0;<br>        dfs(p, 0);<br>        Value /= Count;<br>        if (Value &lt; AnsVal - eps || Value &lt;= AnsVal + eps &amp;&amp; T &lt; Ans)<br>          AnsVal = Value, Ans = T;<br>      }<br>      return Ans;<br>    }<br>};<br> <br>// Code by Burunduk3 =)<br> <br> <br>// Powered by PopsEdit<br></td>