            #include &lt;algorithm&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;string&gt;<br>#include &lt;utility&gt;<br>#include &lt;vector&gt;<br> <br>using namespace std;<br> <br>typedef long long Long;<br>typedef double dbl;<br>typedef vector &lt;int&gt; VI;<br>typedef pair &lt;int, int&gt; PII;<br> <br>#define FI first<br>#define MP make_pair<br>#define PB push_back<br>#define SE second<br> <br>class FractionPoints<br>{<br>  public:<br>    double findPoint( vector &lt;int&gt; A, int b, int c )<br>    {<br>      sort(A.begin(), A.end());<br>      while (b &amp; 1 == 0)<br>        b &gt;&gt;= 1, c &gt;&gt;= 1;<br>      double M;<br>      if (A.size() &amp; 1)<br>        M = A[A.size() / 2];<br>      else<br>        M = 0.5 * (A[A.size() / 2] + A[A.size() / 2 - 1]);<br>      if (b == 1 &amp;&amp; c == 2)<br>        return M;<br>      VI T;<br>      if (b &lt; c / 2)<br>        for (int i = 0; i &lt; (int)A.size() / 2; i++)<br>          T.PB(A[i]);<br>      else<br>      {<br>        b -= c / 2;<br>        for (int i = A.size() / 2 - 1; i &gt;= 0; i--)<br>          T.PB(A[A.size() - 1 - i]);<br>      }<br>      return findPoint(T, b, c / 2);<br>    }<br>};<br> <br>// Code by Burunduk3 =)<br> <br> <br>// Powered by PopsEdit<br></td>