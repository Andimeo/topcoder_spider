            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class FractionPoints<br>{<br>  public double findPoint(int[] data, int numerator, int denominator)<br>  {<br>    Arrays.sort(data);<br>    int l = 0;<br>    int r = data.length-1;<br>    int t1 = numerator;<br>    int t2 = denominator;<br>    for (int i = 2; i &lt;= t1; i++) <br>      if (t1%i == 0)<br>        if (t2%i == 0) {<br>          t1 = t1/i;<br>          t2 = t2/i;<br>        }<br>    int nt2;<br>    while (t2 != 2) {<br>      nt2 = t2/2;<br>      int tl,tr;<br>      if ((r-l)%2 == 0) { tl = (l+r)/2; tr = tl; } <br>      else { <br>        tl = (l+r)/2;<br>        tr = tl+1;<br>      }<br>      if (tl == tr) {<br>        if (t1 &lt; nt2) {<br>          r = tl-1;<br>        } else {<br>          t1 = t1-nt2;<br>          l = tr+1;<br>        }<br>      } else {<br>        if (t1 &lt; nt2) {<br>          r = tl;<br>        } else {<br>          t1 = t1-nt2;<br>          l = tr;<br>        }<br>      }<br>      t2 = nt2;<br>    }<br>    double res;<br>    if ((r-l)%2 == 0) res = data[(l+r)/2]; <br>      else { <br>        res = data[(l+r)/2]+data[(l+r)/2+1];<br>        res = res/2;<br>      }<br>    return res;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>