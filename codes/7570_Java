            import java.util.*;<br> <br>public class ItemizedList {<br>  <br>  public String[] generateList(String[] items) {<br>    TreeMap&lt;String, Integer&gt; m = new TreeMap&lt;String, Integer&gt;();<br>    for (String s : items) {<br>      int k = (m.containsKey(s)) ? m.get(s) : 0;<br>      m.put(s, k+1);<br>    }<br>    String res[] = new String[m.keySet().size()];<br>    int pos = 0;<br>    for (String s : m.keySet())<br>      res[pos++] = s + " - " + m.get(s);<br>    return res;<br>  }<br>  <br>  public static void main(String[] args) {<br>    String[] items = {"b","b","a"};<br>    String[] ret = new ItemizedList().generateList(items);<br>    for (String s : ret)<br>      System.out.println(s);<br>  }<br>  <br>}<br> <br> <br>// Powered by PopsEdit<br></td>