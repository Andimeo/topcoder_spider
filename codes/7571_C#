            using System;<br>using System.Collections.Generic;<br><br>public class Turnpike<br>{<br>  public int unserviced(int pikeLength, int n, int[] plazas)<br>  {<br>    List&lt;int&gt; a = new List&lt;int&gt;(plazas);<br>    a.Add(0);<br>    a.Add(pikeLength);<br>    a.Sort();<br>    int[] b = a.ToArray();<br><br>    int l = 1;<br>    int r = pikeLength;<br>    while (l &lt; r)<br>    {<br>      int c = (l + r) / 2;<br>      if (canConstuct(n, b, c))<br>        r = c;<br>      else<br>        l = c + 1;<br>    }<br>    return l;<br>  }<br><br>  private bool canConstuct(int n, int[] b, int len)<br>  {<br>    int m = b.Length;<br>    int cnt = 0;<br>    for (int i = 1; i &lt; m; i++)<br>      cnt += (b[i] - b[i - 1] + len - 1) / len - 1;<br>    return cnt &lt;= n;<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>