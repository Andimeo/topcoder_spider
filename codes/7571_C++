            #include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;stdlib.h&gt;<br>#include &lt;math.h&gt;<br>#include &lt;iostream&gt;<br>#include &lt;fstream&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;sstream&gt;<br>#include &lt;map&gt;<br><br>using namespace std;<br><br><br><br>class Turnpike<br>{<br>  public:<br>  int unserviced(int len, int m, vector &lt;int&gt; lis)<br>  {<br>    lis.push_back(0);<br>    lis.push_back(len);<br>    sort(lis.begin(),lis.end());<br>    int ans=1000;<br>    int i,j,k,x,y,z;<br>    for(i=1; i&lt;=len; i++)<br>    {<br>      j=0;<br>      for(k=0; k&lt;lis.size()-1; k++)<br>      {<br>        x=lis[k+1]-lis[k];<br>        if(x%i==0)<br>        {<br>          j+=(x/i)-1;<br>        }<br>        else<br>          j+=x/i;<br>        <br>      }<br>      if(j&lt;=m)<br>        return i;<br>    }<br>    return ans;<br>  }<br><br><br><br><br><br><br><br><br><br><br>};<br></td>