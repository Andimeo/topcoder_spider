            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br><br>public class Turnpike<br>{<br>  public int unserviced(int pikeLength, int n, int[] plazas)<br>  {<br>    int N = plazas.length;<br>    int[] pl = new int[N+2];<br>    for (int i=0; i&lt;N; i++)<br>      pl[i] = plazas[i];<br>    pl[N] = 0;<br>    pl[N+1] = pikeLength;<br>    Arrays.sort(pl);<br>    N+=2;<br><br>    <br>    int l = 0;<br>    int h = 1000;<br>    while (l+1&lt;h)<br>    {<br>      int m = (l+h+1)/2;<br>      <br>      int k = 0;<br>      for (int i=1; i&lt;N; i++)<br>        k+=(pl[i]-pl[i-1]-1)/m;<br>      if (k&gt;n)<br>        l = m;<br>      else h = m;<br>    }<br>    return h;<br>  }<br>  <br><br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>