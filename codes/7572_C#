            using System;<br>using System.Collections;<br><br> public class FrequentPairs {<br>   public int mostFrequent(string[] words) {<br>     ArrayList a = new ArrayList();<br>     string stemp;<br>       for (int ctr1 = 0; ctr1 &lt; words.Length; ++ctr1)<br>         for (int ctr2 = 0; ctr2 &lt; words[ctr1].Length - 1; ++ctr2) {<br>           stemp = words[ctr1].Substring(ctr2, 2);<br>             if (stemp[0] &gt; stemp[1])<br>               stemp = stemp[1].ToString() + stemp[0].ToString();<br>           a.Add(stemp);<br>           }<br>     a.Sort();<br>     string s = string.Empty;<br>     int res = 0;<br>     int counttemp = 0;<br>       for (int ctr = 0; ctr &lt; a.Count; ++ctr)<br>           if (s == ((string) a[ctr]))<br>             ++counttemp;<br>           else {<br>               if (counttemp &gt; res)<br>                 res = counttemp;<br>             s = (string) a[ctr];<br>             counttemp = 1;<br>           }<br>               if (counttemp &gt; res)<br>                 res = counttemp;<br>     return res;<br>   }<br>  }<br></td>