<td class="problemText" colspan="8" valign="middle" align="left">
            #include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;string.h&gt;<br><br>using namespace std;<br><br>class FrequentPairs {<br>  public:<br>  <br>  int temp[26][26];<br>  <br>  int mostFrequent(vector &lt;string&gt; words) {<br>    int i,j;<br>    int a,b,s;<br>    <br>    s = 0;<br>    memset(temp,0,sizeof(temp));<br>    for (i = 0; i &lt; words.size(); i ++)<br>      for (j = 0; j &lt; words[i].size() - 1; j ++) {<br>        a = words[i][j] - 'a';<br>        b = words[i][j + 1] - 'a';<br>        if (a &gt; b) {<br>          a ^= b;<br>          b ^= a;<br>          a ^= b;<br>          }<br>        temp[a][b] ++;<br>        s &gt;?= temp[a][b];<br>        }<br>    return s;<br>    }<br>  };<br></td>