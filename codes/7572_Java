            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class FrequentPairs<br>{<br>  public void sop(String s){System.out.print(s);}<br>  public void sopl(String s){System.out.println(s);}<br>  public int ipi(String s){return Integer.parseInt(s);}<br>  public int gcd(int x, int y){if (y==0) return x; return gcd(y,x%y);}<br>  int INF=2147483647;<br>  <br>  int cnt(String s, char ch1, char ch2)<br>  {<br>    int ret=0;<br>    for (int i=0;i&lt;s.length()-1;i++)<br>    {<br>      if (s.charAt(i)==ch1 &amp;&amp; s.charAt(i+1)==ch2) ret++;<br>      else if (s.charAt(i)==ch2 &amp;&amp; s.charAt(i+1)==ch1) ret++;<br>    }<br>    return ret;<br>  }<br>  public int mostFrequent(String[] w)<br>  {<br>    int ret=0;<br>    for (char i='a';i&lt;='z';i++)<br>      for (char j=i;j&lt;='z';j++)<br>      {<br>        int dem=0;<br>        for (int k=0;k&lt;w.length;k++)<br>          dem+=cnt(w[k],i,j);<br>        ret=Math.max(ret,dem);<br>      }<br>    return ret;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>