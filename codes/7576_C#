            using System;<br><br>public class Chessboard<br>{<br>  public string changeNotation(string cell)<br>  {<br>    if (Char.IsDigit(cell[0]))<br>    {<br>      int x = int.Parse(cell) - 1;<br>      return new string("abcdefgh"[x % 8], 1) + new string("12345678"[x / 8], 1);<br>    }<br>    else<br>    {<br>      int x = (int)(cell[1]) - (int)'1';<br>      int y = (int)(cell[0]) - (int)'a';<br>      return (x * 8 + y + 1).ToString();<br>    }<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>