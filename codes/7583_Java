            public class ImageTraders {<br>  public int maxOwners(String[] price) {<br>    int am = price.length;<br>        int[][] prices = new int[am][am];<br>        for (int i = 0; i &lt; am; i++) {<br>            for (int j = 0; j &lt; am; j++) {<br>                prices[i][j] = price[i].charAt(j) - '0';<br>            }<br>        }<br> <br>        int[][] dn = new int[1 &lt;&lt; am][am];<br>        for (int m = 0; m &lt; 1 &lt;&lt; am; m++) {<br>            for (int j = 0; j &lt; am; j++) {<br>                dn[m][j] = 16;<br>            }<br>        }<br>        dn[1][0] = 0;<br>        for (int m = 0; m &lt; (1 &lt;&lt; am); m++) {<br>            for (int j = 0; j &lt; am; j++) {<br>                if (dn[m][j] != 16) {<br>                    for (int k = 0; k &lt; am; k++) {<br>                        if ((m &amp; (1 &lt;&lt; k)) == 0) {<br>                            if ((prices[j][k] &gt;= dn[m][j]) &amp;&amp; (prices[j][k] &lt; dn[m | (1 &lt;&lt; k)][k])) {<br>                                dn[m | (1 &lt;&lt; k)][k] = prices[j][k];<br>                            }<br>                        }<br>                    }<br>                }<br>            }<br>        }<br>        int res = 0;<br>        for (int m = 0; m &lt; 1 &lt;&lt; am; m++) {<br>            for (int j = 0; j &lt; am; j++) {<br>                if (dn[m][j] != 16) {<br>                    if (res &lt; Integer.bitCount(m)) {<br>                        res = Integer.bitCount(m);<br>                    }<br>                }<br>            }<br>        }<br>        return res;<br>    }<br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>