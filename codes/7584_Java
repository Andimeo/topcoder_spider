            import java.util.*;<br> <br>public class FlightScheduler {<br>    double hm(int distance, int K, int emptyMass, int takeoffFuel, long i) {<br>        double to = 1.0 * i * takeoffFuel;<br>        double seg = 1.0 * distance / i;<br>        double fl = 1.0 * emptyMass * (Math.exp(seg / K) - 1) * i;<br>        return to + fl;<br>    }<br> <br>    public double minimizeFuel(int distance, int K, int emptyMass, int takeoffFuel) {<br>        long l = 1;<br>        long r = 1000000000000000L;<br>        while (l &lt; r - 2) {<br>            long m1 = (2 * l + r) / 3;<br>            long m2 = (l + 2 * r) / 3;<br> <br>            double v1 = hm(distance, K, emptyMass, takeoffFuel, m1);<br>            double v2 = hm(distance, K, emptyMass, takeoffFuel, m2);<br> <br>            if (v1 &lt;= v2) {<br>                r = m2;<br>            } else {<br>                l = m1;<br>            }<br>        }<br>        double best = Double.MAX_VALUE;<br>        for (long i = -1000; i &lt; 1000; i++) {<br>            if (l + i &gt; 0) {<br>                best = Math.min(best, hm(distance, K, emptyMass, takeoffFuel, l + i));<br>            }<br>        }<br>        return best;<br>    }<br> <br>}<br> <br> <br>// Powered by PopsEdit<br> <br>// Powered by CodeProcessor<br></td>