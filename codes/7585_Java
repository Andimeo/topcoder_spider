            import java.util.*;<br>public class ChangingString<br>{<br>  public int distance(String A, String B, int K)<br>  {<br>    int res = 0;<br>    int [] p=new int[A.length()];<br>    for(int i=0;i&lt;p.length;i++)<br>      p[i]=Math.abs(A.charAt(i)-B.charAt(i));<br>    Arrays.sort(p);<br>    for(int i=p.length-1,j=0;i&gt;=0&amp;&amp;j&lt;K;j++,i--)<br>    {<br>      if(p[i]!=0)<br>        p[i]=0;<br>      else p[i]=1;<br>    }<br>    for(int d:p)<br>      res+=d;<br>    return res;<br>  }<br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>