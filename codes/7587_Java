            import java.util.*;<br><br>public class BagsQuiz {<br>  public int checkIfProper(int n, String[] actions) {<br>    int[] bags = new int[n];<br>    Arrays.fill(bags, -1);<br>    for (String a : actions) {<br>      String[] p = a.split(" ");<br>      int b1 = Integer.valueOf(p[1]) - 1;<br>      if (bags[b1] != -1)<br>        return -1;<br>      if (a.startsWith("PUT")) {<br>        int b2 = Integer.valueOf(p[3]) - 1;<br>        if (bags[b2] != -1)<br>          return -1;<br>        bags[b1] = b2;<br>      }<br>      else if (a.startsWith("SET")) {<br>        for (int i = 0; i &lt; n; i++)<br>          if (bags[i] == b1)<br>            bags[i] = -1;<br>      }<br>      else if (a.startsWith("SWAP")) {<br>        int b2 = Integer.valueOf(p[3]) - 1;<br>        if (bags[b2] != -1)<br>          return -1;<br>        for (int i = 0; i &lt; n; i++) {<br>          if (bags[i] == b1)<br>            bags[i] = b2;<br>          else if (bags[i] == b2)<br>            bags[i] = b1;<br>        }<br>      }<br>    }<br>    int numOnFloor = 0;<br>    for (int i = 0; i &lt; n; i++) {<br>      if (bags[i] == -1)<br>        numOnFloor++;<br>      else if (bags[i] &lt; i)<br>        return -1;<br>    }<br>    return numOnFloor;<br>  }<br>}<br></td>