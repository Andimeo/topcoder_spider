<td class="problemText" colspan="8" valign="middle" align="left">
            public class QuoteContest<br>{<br>  public string bestQuote(string[] quotes)<br>  {<br>    int maxVotes = -1;<br>    int curVotes = 0;<br>    string maxQuote = "";<br>    string[] holder = new string[3];<br>    <br>    for(int i=0;i&lt;quotes.Length;i++)<br>    {<br>      holder = quotes[i].Split(' ');<br>      curVotes = System.Convert.ToInt32(holder[1]) + System.Convert.ToInt32(holder[0]);<br>      if (curVotes &gt; maxVotes)<br>      {<br>        maxVotes = curVotes;<br>        maxQuote = holder[2];<br>      }<br>    }<br>    <br>    return maxQuote;<br>  }<br>}<br></td>