            #include &lt;iostream&gt;<br>#include &lt;vector&gt;<br>#include &lt;queue&gt;<br>#include &lt;string&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cmath&gt;<br>#include &lt;list&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;sstream&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>using namespace std;<br>class QuoteContest {<br> public:<br>  string bestQuote(vector &lt;string&gt; quotes)<br>  {<br>   int ans=0; string ret="";<br>   for (int i=0;i&lt;quotes.size();i++)  <br>   {<br>    int a,b; char t[100];<br>    sscanf(quotes[i].c_str(),"%d %d %s",&amp;a,&amp;b,t);<br>    if (a+b&gt;ans) { ans=a+b; ret=t; }<br>   }<br>   return ret;<br>  }<br>};<br></td>