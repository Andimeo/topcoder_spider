            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class QuoteContest<br>{<br>  public String bestQuote(String[] quotes)<br>  {<br>    int best = -1;<br>    String ret = "";<br>    for(String s : quotes)<br>    {<br>      String[] temp = s.split(" ");<br>      int x = Integer.parseInt(temp[0]) + Integer.parseInt(temp[1]);<br>      if(x &gt; best)<br>      {<br>        best = x;<br>        ret = temp[2];<br>      }<br>    }<br>    return ret;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>