<td class="problemText" colspan="8" valign="middle" align="left">
            public class TournamentsAmbiguityNumber {<br>  public int scrutinizeTable(string[] table) {<br>    int n = table.Length;<br>    int ans = 0;<br>    for (int i = 0; i &lt; n; i++)<br>    {<br>      for (int j = 0; j &lt; n; j++)<br>      {<br>        for (int k = 0; k &lt; n; k++)<br>        {<br>          if (table[i][j] == '1' &amp;&amp; table[j][k] == '1' &amp;&amp; table[k][i] == '1')<br>          {<br>            ans++;<br>          }<br>        }<br>      }<br>    }<br>    return ans;<br>  }<br>}<br></td>