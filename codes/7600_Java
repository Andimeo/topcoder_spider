            import java.util.*;<br>import java.math.*;<br>import java.lang.Math.*;<br><br>public class TournamentsAmbiguityNumber {<br><br>  public int scrutinizeTable(String[] table) {<br>    int n = table.length;<br>    int ans = 0;<br>    for (int i = 0; i &lt; n; i++)<br>      for (int j = 0; j &lt; n; j++)<br>        for (int k = 0; k &lt; n; k++)<br>          if (table[i].charAt(j) == '1' &amp;&amp; table[j].charAt(k) == '1' &amp;&amp; table[k].charAt(i) == '1') ans++;<br>    return ans;<br>  }<br><br>}<br></td>