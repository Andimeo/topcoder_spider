            import java.util.*;<br> <br>public class DiceGames {<br>    public long countFormations(int[] sides) {<br>        Arrays.sort(sides);<br>        int n = sides.length;<br>        long[][] a = new long[n + 1][33];<br>        a[0][0] = 1;<br>        for (int i = 0; i &lt; n; i++) {<br>            for (int j = 1; j &lt;= sides[i]; j++) {<br>                for (int k = 0; k &lt;= j; k++) {<br>                    a[i + 1][j] += a[i][k];<br>                }<br>            }<br>        }<br>        long r = 0;<br>        for (int i = 0; i &lt;= sides[n - 1]; i++) {<br>            r += a[n][i];<br>        }<br>        return r; <br>    }<br> <br> <br>}<br> <br> <br>// Powered by PopsEdit<br> <br>// Powered by CodeProcessor<br></td>