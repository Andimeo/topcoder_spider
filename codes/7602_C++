            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br><br>using namespace std;<br><br>class BooksNumbering {<br>public:<br>  int numberOfBooks(int d) {<br>    long long b = 9, c = 1, u = 0;<br>    while(d &gt; b * c) {<br>      d -= b * c;<br>      u += b;<br>      b *= 10;<br>      c++;<br>    }<br>    if(d % c != 0)<br>      return -1;<br>    return u + d / c;<br>  }<br>};<br><br><br>// Powered by FileEdit<br></td>