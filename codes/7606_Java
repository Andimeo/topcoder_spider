            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class DistanceBetweenStrings<br>{<br>  public int getDistance(String a, String b, String letterSet)<br>  {<br>    int res = 0;<br>    <br>    a = a.toLowerCase();<br>    b = b.toLowerCase();<br>    letterSet = letterSet.toLowerCase();<br>    <br>    for (char c : letterSet.toCharArray())  {<br>      int n1 = count(a, c);<br>      int n2 = count(b, c);<br>      <br>      res += (n1 - n2) * (n1 - n2);<br>    }<br>    <br>    return res;<br>  }<br>  <br>  private int count(String s, char c)  {<br>    int res = 0;<br>    <br>    for (char t : s.toCharArray())<br>      if (t == c)<br>        res++;<br>        <br>    return res;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>