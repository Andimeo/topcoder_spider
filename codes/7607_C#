            using System.Collections.Generic;<br>using System;<br><br>public class Kings<br>{<br>  public string[] enumerate(string[] names)<br>  {<br>    for(int i = names.Length - 1; i &gt;= 0; i--)<br>    {<br>      int nc = 1;<br>      for(int j = i - 1; j &gt;= 0; j--)<br>        if(names[j] == names[i]) nc++;<br>      if(nc &gt; 1)  names[i] = names[i] + " " + nc;<br>      else<br>      {<br>        <br>        for(int j = i + 1; j &lt; names.Length; j++)<br>        {<br>          try{<br>          if(names[j].Substring(0,names[i].Length) == names[i] &amp;&amp; names[j][names[i].Length]==' ') nc++;<br>          }catch{}<br>        }<br>        if(nc &gt; 1)<br>          names[i] = names[i] + " 1";<br>      }<br>    }<br>    return names;<br>  }<br>}<br></td>