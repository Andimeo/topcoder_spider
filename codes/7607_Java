            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class Kings<br>{<br>  public String[] enumerate(String[] names)<br>  {<br>    <br>    for(int i=0; i&lt;names.length-1; i++){<br>      String king1 = names[i];<br>      int pos = 2;<br>      boolean is = false;<br>      for(int j=i+1; j&lt;names.length; j++){<br>        String king2 = names[j];<br>        if(king2.equals(king1)){<br>          names[j]+= " " + pos;<br>          pos++;<br>          is=true;<br>        }<br>      }<br>      <br>      if(is)<br>        names[i]+=" " +1;<br>    }<br>    <br>    return names;<br>        <br>    <br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>