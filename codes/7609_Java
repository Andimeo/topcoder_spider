            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class DividingRectangle<br>{<br>  public long maxProduct(String[] rectangle)<br>  {<br>    int rmax = rectangle.length;<br>    int cmax = rectangle[0].length();<br>    int[][] rect = new int[rmax][cmax];<br>    long totalsum = 0;<br>    for(int r=0; r&lt;rmax; r++) for(int c=0; c&lt;cmax; c++) {rect[r][c] = rectangle[r].charAt(c)-'0'; totalsum+=rect[r][c];}<br>    long ans = 0;<br>    for(int rcut=1; rcut&lt;rmax; rcut++)<br>    {<br>      long sum1 = 0;<br>      for(int r=0; r&lt;rcut; r++) for(int c=0; c&lt;cmax; c++) sum1+=rect[r][c];<br>      for(int ccut=1; ccut&lt;cmax; ccut++)<br>      {<br>        long sum2 = 0;<br>        for(int r=rcut; r&lt;rmax; r++) for(int c=0; c&lt;ccut; c++)<br>          sum2+=rect[r][c];<br>        long sum3 = totalsum-sum1-sum2;<br>        if(sum1*sum2*sum3&gt;ans) ans = sum1*sum2*sum3;<br>      }<br>    }<br>    for(int rcut=1; rcut&lt;rmax; rcut++)<br>    {<br>      long sum1 = 0;<br>      for(int r=rcut; r&lt;rmax; r++) for(int c=0; c&lt;cmax; c++) sum1+=rect[r][c];<br>      for(int ccut=1; ccut&lt;cmax; ccut++)<br>      {<br>        long sum2 = 0;<br>        for(int r=0; r&lt;rcut; r++) for(int c=0; c&lt;ccut; c++)<br>          sum2+=rect[r][c];<br>        long sum3 = totalsum-sum1-sum2;<br>        if(sum1*sum2*sum3&gt;ans) ans = sum1*sum2*sum3;<br>      }<br>    }<br>    for(int ccut=1; ccut&lt;cmax; ccut++)<br>    {<br>      long sum1 = 0;<br>      for(int c=0; c&lt;ccut; c++) for(int r=0; r&lt;rmax; r++) sum1+=rect[r][c];<br>      for(int rcut=1; rcut&lt;rmax; rcut++)<br>      {<br>        long sum2 = 0;<br>        for(int c=ccut; c&lt;cmax; c++) for(int r=0; r&lt;rcut; r++)<br>          sum2+=rect[r][c];<br>        long sum3 = totalsum-sum1-sum2;<br>        if(sum1*sum2*sum3&gt;ans) ans = sum1*sum2*sum3;<br>      }<br>    }<br>    for(int ccut=1; ccut&lt;cmax; ccut++)<br>    {<br>      long sum1 = 0;<br>      for(int c=ccut; c&lt;cmax; c++) for(int r=0; r&lt;rmax; r++) sum1+=rect[r][c];<br>      for(int rcut=1; rcut&lt;rmax; rcut++)<br>      {<br>        long sum2 = 0;<br>        for(int c=0; c&lt;ccut; c++) for(int r=0; r&lt;rcut; r++)<br>          sum2+=rect[r][c];<br>        long sum3 = totalsum-sum1-sum2;<br>        if(sum1*sum2*sum3&gt;ans) ans = sum1*sum2*sum3;<br>      }<br>    }<br>    for(int rcut=1; rcut&lt;rmax-1; rcut++)<br>    {<br>      long sum1 = 0;<br>      for(int r=0; r&lt;rcut; r++) for(int c=0; c&lt;cmax; c++) sum1+=rect[r][c];<br>      for(int rcut2=rcut+1; rcut2&lt;rmax; rcut2++)<br>      {<br>        long sum2 = 0;<br>        for(int r=rcut; r&lt;rcut2; r++) for(int c=0; c&lt;cmax; c++)<br>          sum2+=rect[r][c];<br>        long sum3 = totalsum-sum1-sum2;<br>        if(sum1*sum2*sum3&gt;ans) ans = sum1*sum2*sum3;<br>      }<br>    }<br>    for(int ccut=1; ccut&lt;cmax-1; ccut++)<br>    {<br>      long sum1 = 0;<br>      for(int c=0; c&lt;ccut; c++) for(int r=0; r&lt;rmax; r++) sum1+=rect[r][c];<br>      for(int ccut2=ccut+1; ccut2&lt;cmax; ccut2++)<br>      {<br>        long sum2 = 0;<br>        for(int c=ccut; c&lt;ccut2; c++) for(int r=0; r&lt;rmax; r++)<br>          sum2+=rect[r][c];<br>        long sum3 = totalsum-sum1-sum2;<br>        if(sum1*sum2*sum3&gt;ans) ans = sum1*sum2*sum3;<br>      }<br>    }<br>    return ans;<br>  }<br>}<br></td>