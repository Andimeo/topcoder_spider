            using System;<br>using System.Collections.Generic;<br><br>public class SumsOfPerfectPowers {<br>  public int howMany(int lowerBound, int upperBound) {<br>    Dictionary&lt;int, object&gt; powers = new Dictionary&lt;int, object&gt;();<br>    powers[1] = null;<br>    powers[0] = null;<br>    for (int i = 2; i * i &lt;= upperBound; ++i)<br>    {<br>      long z = i * i;<br>      while (z &lt;= upperBound)<br>      {<br>        powers[(int) z] = null;<br>        z *= i;<br>      }<br>    }<br>    bool[] pow = new bool[2 * upperBound + 100];<br>    foreach (int x1 in powers.Keys)<br>      foreach (int x2 in powers.Keys)<br>        pow[x1 + x2] = true;<br>    int res = 0;<br>    for (int x = lowerBound; x &lt;= upperBound; ++x)<br>      if (pow[x])<br>        ++res;<br>    return res;<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>