            import java.util.*;<br><br>public class SumsOfPerfectPowers {<br>  public int howMany(int lowerBound, int upperBound) {<br>    boolean[] ok = new boolean[upperBound+1];<br>    Vector&lt;Integer&gt; pows = new Vector&lt;Integer&gt;();<br>    pows.add(0);<br>    pows.add(1);<br>    for (int i=2; i&lt;=2236; i++) {<br>      for (long j=i*i; j&lt;=5000000; j*=i) {<br>        pows.add((int)j);<br>      }<br>    }<br>    Collections.sort(pows);<br>    for (int i:pows) {<br>      for (int j : pows) {<br>        if (i+j&gt;upperBound) break;<br>        ok[i+j]=true;<br>      }<br>    }<br>    int ans = 0;<br>    for (int i=lowerBound; i&lt;=upperBound; i++) if (ok[i]) ans++;<br>    return ans;<br>    <br>  }<br>}<br></td>