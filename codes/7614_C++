            #include &lt;algorithm&gt;<br>#include &lt;cmath&gt;<br>#include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>using namespace std;<br><br>class BusyTime {<br>public:<br>double busy(int k, vector &lt;int&gt; need) {<br>  int i, j, x, y, z;<br><br>  double lo = 0.0, hi = 1e9, mid;<br>  for (i = 0; i &lt; 200; i++) {<br>    mid = (lo+hi)/2;<br>    int np = k;<br>    double sum = 0.0;<br>    for (j = 0; j &lt; need.size(); j++)<br>      if (need[j] &gt;= mid) {<br>        np--;<br>      } else {<br>        sum += need[j];<br>      }<br>    if (np &lt;= 0) {lo = mid; continue;}<br>    sum /= np;<br>    if (sum &lt; mid) hi = mid; else lo = mid;<br>  }<br>  return mid;<br>}<br>};<br><br><br>// Powered by FileEdit<br></td>