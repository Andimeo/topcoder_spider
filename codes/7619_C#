            using System;<br>using System.Collections;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class Bigital<br>{<br>    int cn(int a)<br>    {<br>        int ret = 0;<br>        int x = a / 10;<br>        while (x &gt; 0)<br>        {<br>            ret += x % 2;<br>            x &gt;&gt;= 1;<br>        }<br>        x = a % 10;<br>        while (x &gt; 0)<br>        {<br>            ret += x % 2;<br>            x &gt;&gt;= 1;<br>        }<br>        return ret;<br>    }<br> <br>    public double energy(string tStart, string tEnd)<br>    {<br>        string[] s = tStart.Split(':');<br>        int sh = int.Parse(s[0]);<br>        int sm = int.Parse(s[1]);<br>        int ss = int.Parse(s[2]);<br>        s = tEnd.Split(':');<br>        int eh = int.Parse(s[0]);<br>        int em = int.Parse(s[1]);<br>        int es = int.Parse(s[2]);<br> <br>        double wm = 0;<br>        while (sh != eh || sm != em || ss != es)<br>        {<br>            wm += cn(sh) + cn(sm) + cn(ss);<br>            ss++;<br> <br>            if (ss &gt; 59)<br>            {<br>                ss = 0;<br>                sm++;<br>            }<br> <br>            if (sm &gt; 59)<br>            {<br>                sm = 0;<br>                sh++;<br>            }<br> <br>            if (sh &gt; 12)<br>            {<br>                sh = 1;<br>            }<br>        }<br>        wm += cn(sh) + cn(sm) + cn(ss);<br> <br>        return wm / 3600000;<br>    }<br>}<br> <br>//Powered by [KawigiEdit]<br></td>