            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;string&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br>#include &lt;algorithm&gt;<br>using namespace std;<br><br>class Bigital<br>{<br>public:<br>  int c(int t)<br>  {<br>    int ans=0;<br>    while (t)<br>    {<br>      ans+=t%2;<br>      t/=2;<br>    }<br>    return ans;<br>  }<br>  int calc(int t)<br>  {<br>    return c(t/10)+c(t%10);<br>  }<br>  double energy(string tStart, string tEnd)<br>  {<br>    string t1=tStart,t2=tEnd;<br>    int s1=(t1[0]-'0')*10+t1[1]-'0';<br>    int s2=(t1[3]-'0')*10+t1[4]-'0';<br>    int s3=(t1[6]-'0')*10+t1[7]-'0';<br>    int p1=(t2[0]-'0')*10+t2[1]-'0';<br>    int p2=(t2[3]-'0')*10+t2[4]-'0';<br>    int p3=(t2[6]-'0')*10+t2[7]-'0';<br>    cout&lt;&lt;s1&lt;&lt;s2&lt;&lt;s3&lt;&lt;p1&lt;&lt;p2&lt;&lt;p3&lt;&lt;endl;<br>    long ans=0;<br>    while (1)<br>    {<br>      ans+=calc(s1)+calc(s2)+calc(s3);<br>      if (s1==p1 &amp;&amp; s2==p2 &amp;&amp; s3==p3)<br>        break;<br>      s3++;<br>      if (s3==60)<br>      {<br>        s3=0;s2++;<br>      }<br>      if (s2==60)<br>      {<br>        s2=0;s1++;<br>      }<br>      if (s1==13)<br>        s1=1;<br>    }<br>    return (double)ans/3600/1000;<br>  }<br>};<br></td>