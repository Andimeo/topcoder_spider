            import java.util.*;<br> <br>public class Bigital<br>{<br>  int memo[] = new int[10];<br>    public double energy(String tStart, String tEnd)<br>    {<br>      for (int i = 0; i &lt; memo.length; i++) {<br>        String s = Integer.toBinaryString(i);<br>        for (int j = 0; j &lt; s.length(); j++) {<br>          if (s.charAt(j)=='1') memo[i]++;<br>      }<br>    }<br>      <br>      int s = c(tStart);<br>      int e = c(tEnd);<br>      if (e&lt;s) e+= 12*3600;<br>      int r = 0; <br>        for (int i=s; i &lt;= e; i++) {<br>          int v = h(i);<br>          r+=v;<br>        }<br>        <br>        return r/3600000.0;<br>    }<br> <br>    int c(String s) {<br>      String v[] = s.split(":");<br>      return Integer.parseInt(v[0])*3600+Integer.parseInt(v[1])*60+Integer.parseInt(v[2]);<br>    }<br>    <br>    int h(int v) {<br>      int s = v%60;<br>      v/=60;<br>      int m = v%60;<br>      v/=60;<br>      int h = v;<br>      while (h&gt;12) h-=12;<br>      if (h==0) h=12;<br>      return k(s)+k(m)+k(h);<br>    }<br>    <br>    int k(int v) {<br>      return memo[v%10]+memo[v/10];<br>    }<br>    <br>}<br></td>