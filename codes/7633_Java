            import java.util.*;<br>public class TaxiManager {<br>  int mm[][]=new int[1&lt;&lt;12+4][55];<br>  int g[][];<br>  int m;<br>  int cust[][];<br>  int n;<br>  public int schedule(String[] roads, String[] customers) {<br>    for(int i=0;i&lt;mm.length;i++)<br>    Arrays.fill(mm[i],-1);<br>    m=roads.length;<br>    g=new int[m][m];<br>    for(int i=0;i&lt;m;i++)<br>    for(int j=0;j&lt;m;j++)<br>    {<br>      if(i==j)continue;<br>      g[i][j]=roads[i].charAt(j)-'0';<br>      if(g[i][j]==0)<br>      g[i][j]=Integer.MAX_VALUE/2;<br>    }<br>    for(int k=0;k&lt;m;k++)for(int i=0;i&lt;m;i++)for(int j=0;j&lt;m;j++)<br>    g[i][j]=Math.min(g[i][j],g[i][k]+g[k][j]);<br>    n=customers.length;<br>    cust=new int[n][2];<br>    for(int i=0;i&lt;n;i++)<br>    {<br>      String ss[]=customers[i].split(" ");<br>      cust[i][0]=Integer.parseInt(ss[0]);<br>      cust[i][1]=Integer.parseInt(ss[1]);<br>    }<br>    int ret=Integer.MAX_VALUE;<br>    for(int i=0;i&lt;(1&lt;&lt;n);i++)<br>    {<br>      ret=Math.min(ret,Math.max(find(i,0),find(((1&lt;&lt;n)-1)^i,0)));<br>    }<br>    return ret;<br>  }<br>  int find(int state,int loc)<br>  {<br>    if(state==0)return g[loc][0];<br>    if(mm[state][loc]!=-1)return mm[state][loc];<br>    int ret=Integer.MAX_VALUE/2;<br>    for(int i=0;i&lt;n;i++)<br>    {<br>      if((state&amp;(1&lt;&lt;i))==0)continue;<br>      ret=Math.min(ret,g[loc][cust[i][0]]+g[cust[i][0]][cust[i][1]]+find(state^(1&lt;&lt;i),cust[i][1]));<br>    }<br>    return mm[state][loc]=ret;<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>