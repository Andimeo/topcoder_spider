            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class Snaky<br>{<br>  public int longest(string[] snake)<br>  {<br>    int res = 0;<br>    foreach (string s in snake)<br>    {<br>      foreach (string s2 in s.Split('.'))<br>        res = Math.Max(res, s2.Length);<br>    }<br>    for (int i = 0; i &lt; snake[0].Length; i++)<br>    {<br>      string s = "";<br>      foreach (string s2 in snake)<br>        s += s2[i];<br>      foreach (string s2 in s.Split('.'))<br>        res = Math.Max(res, s2.Length);<br>    }<br>    return res;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>