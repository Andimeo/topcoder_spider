            Public Class Snaky<br>        Public Function longest(ByVal snake As String()) As Integer<br>            Dim ret As Integer = 0<br>            Dim t As Integer = 0<br>            Dim i As Integer<br>            Dim j As Integer<br>            For i = 0 To snake.Length - 1<br>                For j = 0 To snake(i).Length - 1<br>                    If j = 0 Then<br>                        t = 0<br>                    End If<br>                    If Not snake(i)(j) = "." Then<br>                        t += 1<br>                        If t &gt; ret Then<br>                            ret = t<br>                        End If<br>                    Else : t = 0<br>                    End If<br>                Next<br>            Next<br>            For j = 0 To snake(0).Length - 1<br>                For i = 0 To snake.Length - 1<br>                    If i = 0 Then<br>                        t = 0<br>                    End If<br>                    If Not snake(i)(j) = "." Then<br>                        t += 1<br>                        If t &gt; ret Then<br>                            ret = t<br>                        End If<br>                    Else : t = 0<br>                    End If<br>                Next<br>            Next<br>            Return ret<br>        End Function<br>    End Class<br></td>