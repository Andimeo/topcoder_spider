            using System;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class PacketRepack<br>{<br>  public int[] StringToArray(string s, int b, int n)<br>  {<br>    List&lt;int&gt; ans = new List&lt;int&gt;();<br>    int pos = 0;<br>    for (int i = 0; i &lt; n; i++)<br>    {<br>      int x = 0;<br>      for (int j = 0; j &lt; b; j++)<br>      {<br>        x += (pos &lt; s.Length &amp;&amp; s[pos] == '1' ? 1 : 0) * (1 &lt;&lt; j);<br>        pos++;<br>      }<br>      ans.Add(x);<br>    }<br>    return ans.ToArray();<br>  }<br><br>  public string ArrayToString(int[] input, int b)<br>  {<br>    StringBuilder sb = new StringBuilder();<br>    foreach (int i in input)<br>    {<br>      int k = i;<br>      for (int j = 0; j &lt; b; j++)<br>      {<br>        sb.Append(k % 2);<br>        k /= 2;<br>      }<br>    }<br>    return sb.ToString();<br>  }<br><br>  public int[] output(int[] input, int b, int num_fields, int field_size)<br>  {<br>    string str = ArrayToString(input, b);<br>    int[] data = StringToArray(str, field_size, num_fields);<br>    Array.Reverse(data);<br>    string str2 = ArrayToString(data, field_size);<br>    return StringToArray(str2, b, input.Length);<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>