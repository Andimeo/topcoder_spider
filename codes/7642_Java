            import java.util.*;<br>public class PacketRepack {<br>    public int[] output(int[] input, int ws, int num_fields, int field_size) {<br>        int[] res = new int[input.length];<br>        for(int f=num_fields; f--&gt;0; ) {<br>          for(int b=field_size; b--&gt;0; ) {<br>            int ip = f*field_size + b;<br>            int op = (num_fields-1-f)*field_size + b;<br>            int bit = (input[ip / ws] &gt;&gt; (ip%ws)) &amp; 1;<br>            res[op / ws] ^= bit &lt;&lt; (op%ws);<br>          }<br>        }<br>        return res;<br>    }<br> <br>}<br> <br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>