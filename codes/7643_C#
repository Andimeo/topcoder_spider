            using System;<br>using System.Collections;<br>public class CableDonation {<br> public int cable(string[] s) {<br>   int n = s.Length;<br>   int[,] a = new int[n,n];<br>   int ss=0;<br>   for (int i=0;i&lt;n;i++)<br>   for (int j=0;j&lt;n;j++) {<br>     if (s[i][j]&gt;='a' &amp;&amp;s[i][j]&lt;='z') a[i,j]=s[i][j]-'a' + 1; else<br>     if (s[i][j]&gt;='A' &amp;&amp;s[i][j]&lt;='Z') a[i,j]=s[i][j]-'A' + 27;<br>     ss+=a[i,j];<br>   }   <br>   bool[] b = new bool[n]; b[0]=true;<br>   int xx=0;<br>   do {<br>    int min = 100, ii= -1, jj = -1;<br>    for (int i=0;i&lt;n;i++) if (b[i])<br>    for (int j=0;j&lt;n;j++) if (!b[j])<br>    if (a[i,j]&lt;min &amp;&amp; a[i,j]&gt;0) {<br>      ii = i; jj = j; min = a[i,j];<br>    }<br>    for (int i=0;i&lt;n;i++) if (b[i])<br>    for (int j=0;j&lt;n;j++) if (!b[j])<br>    if (a[j,i]&lt;min &amp;&amp; a[j,i]&gt;0) {<br>      ii = i; jj = j; min = a[j,i];<br>    }<br>    if (min == 100) break;<br>    b[jj] = true; xx+=min;<br>  } while (true);<br>  for (int i = 0; i &lt;n;i++) if (!b[i]) return -1;<br>  return ss-xx;<br> }<br>}<br> <br>// Powered by PopsEdit<br></td>