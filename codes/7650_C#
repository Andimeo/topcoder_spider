            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class Trekking<br>{<br>  public int findCamps(string trail, string[] plans)<br>  {<br>    int ans = -1;<br>    foreach (string plan in plans)<br>    {<br>        bool valid = true;<br>        int count = 0;<br>        for (int i = 0; i &lt; plan.Length; ++i)<br>        {<br>            if (plan[i] == 'C' &amp;&amp; trail[i] == '^')<br>            {<br>               valid = false;<br>            }<br>            if (plan[i] == 'C')<br>            ++count;<br>        }<br>         if (valid &amp;&amp; (ans == -1 || count &lt; ans))<br>         {<br>            ans = count;<br>         }<br>    }<br>    return ans;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>