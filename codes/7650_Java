            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class Trekking<br>{<br>  public int findCamps(String ss, String[] st)<br>  {<br>    int res = -1;<br>    <br>    for (String s: st) {<br>      int cnt = 0;<br>      boolean ok = true;<br>      for (int i=0; i&lt;ss.length(); i++)<br>        if (s.charAt(i)=='C') {<br>          if (ss.charAt(i)=='^') ok = false;<br>          cnt++;<br>        }<br>      if (ok)<br>        if (res ==-1 || cnt &lt; res) res = cnt;<br>    }<br>    <br>    return res;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>