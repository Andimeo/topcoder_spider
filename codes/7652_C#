            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class CarBuyer<br>{<br>  public double lowestCost(string[] cars, int fuelPrice, int annualDistance, int years)<br>  {<br>    double min = 1e30;<br>    foreach(string car in cars) {<br>      string[] c = car.Split(' ');<br>      double purchase = double.Parse(c[0]), tax = double.Parse(c[1]), fuelEff = double.Parse(c[2]);<br>      <br>      double res = purchase + tax * years + fuelPrice * annualDistance * years/fuelEff;<br>      min = Math.Min(res, min);<br>    }<br>    return min;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>