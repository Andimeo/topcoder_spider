            import java.util.*;<br>public class CarBuyer {<br>    public double lowestCost(String[] cars, int fuelPrice, int annualDistance, int years) {<br>        double res = Double.MAX_VALUE;<br>        for(String s : cars)<br>        {<br>            String [] ary = s.split(" ");<br>            double cost = Double.parseDouble(ary[0]);<br>            double tax = Double.parseDouble(ary[1]);<br>            double gas  = Double.parseDouble(ary[2]);<br>            double totalCost = cost + tax * (double)years + (double)fuelPrice * (double)years * annualDistance / gas;<br>            res = Math.min(res, totalCost);<br>        }<br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>