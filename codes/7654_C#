            using System;<br>using System.Text;<br><br>public class DateFormat {<br>  public string fromEuropeanToUs(string[] dateList) {<br>    StringBuilder builder = new StringBuilder();<br>    foreach (string line in dateList)<br>      builder.Append(line);<br>    string[] dates = builder.ToString().Split(' ');<br>    int n = dates.Length;<br>    bool[,] can = new bool[n, 12 * 40];<br>    for (int i = n - 1; i &gt;= 0; --i)<br>    {<br>      string[] p = dates[i].Split('/');<br>      int a = int.Parse(p[0]);<br>      int b = int.Parse(p[1]);<br>      int last = -1;<br>      if (i == n - 1)<br>        last = 12 * 40;<br>      else<br>        for (int x = 12 * 40 - 1; x &gt;= 0; --x)<br>          if (can[i + 1, x])<br>          {<br>            last = x;<br>            break;<br>          }<br>      if (valid(a, b) &amp;&amp; (a - 1) * 40 + b - 1 &lt; last)<br>      {<br>        can[i, (a - 1) * 40 + b - 1] = true;<br>      }<br>      if (valid(b, a) &amp;&amp; (b - 1) * 40 + a - 1 &lt; last)<br>      {<br>        can[i, (b - 1) * 40 + a - 1] = true;<br>      }<br>    }<br>    StringBuilder res = new StringBuilder();<br>    int at = -1;<br>    for (int i = 0; i &lt; n; ++i)<br>    {<br>      bool ok = false;<br>      for (int x = at + 1; x &lt; 12 * 40; ++x)<br>        if (can[i, x])<br>        {<br>          ok = true;<br>          if (res.Length &gt; 0)<br>            res.Append(' ');<br>          res.Append(formatDate(x / 40 + 1, x % 40 + 1));<br>          at = x;<br>          break;<br>        }<br>      if (!ok)<br>        return "";<br>    }<br>    return res.ToString();<br>  }<br><br>  private string formatDate(int month, int day)<br>  {<br>    string s1 = month.ToString();<br>    while (s1.Length &lt; 2)<br>      s1 = '0' + s1;<br>    string s2 = day.ToString();<br>    while (s2.Length &lt; 2)<br>      s2 = '0' + s2;<br>    return s1 + "/" + s2;<br>  }<br><br>  readonly int[ ] daysInMonth = new int[ ] { 31, 29, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31 };<br>  <br>  bool valid(int month, int day)<br>  {<br>    if (month &lt; 1 || month &gt; 12)<br>      return false;<br>    if (day &lt; 1 || day &gt; daysInMonth[month - 1])<br>      return false;<br>    return true;<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>