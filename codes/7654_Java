            import java.util.*;<br> <br>public class DateFormat {<br>    int[] m = new int[]{<br>            31, 29, 31, 30, 31, 30,<br>            31, 31, 30, 31, 30, 31};<br> <br>    public String fromEuropeanToUs(String[] dateList) {<br>        String s = "";<br>        for (String t : dateList) {<br>            s += t;<br>        }<br>        StringTokenizer st = new StringTokenizer(s);<br>        int n = st.countTokens();<br>        int[] a = new int[n];<br>        int[] b = new int[n];<br>        for (int i = 0; i &lt; n; i++) {<br>            String t = st.nextToken();<br>            StringTokenizer sst = new StringTokenizer(t, "/");<br>            a[i] = Integer.parseInt(sst.nextToken()) - 1;<br>            b[i] = Integer.parseInt(sst.nextToken()) - 1;<br>        }<br> <br>        for (int i = 0; i &lt; n; i++) {<br>            if (a[i] &gt;= 12 || b[i] &gt;= m[a[i]]) {<br>                int t = a[i]; a[i] = b[i]; b[i] = t;<br>            }<br>            if (b[i] &lt; 12 &amp;&amp; a[i] &lt; m[b[i]] &amp;&amp; b[i] &lt; a[i] &amp;&amp;<br>                    ((i == 0) || (b[i] &gt; a[i - 1] || b[i] == a[i - 1] &amp;&amp; a[i] &gt; b[i - 1]))<br>                    ) {<br>                int t = a[i]; a[i] = b[i]; b[i] = t;<br>            }<br>            if (a[i] &gt;= 12 || b[i] &gt;= m[a[i]]) {<br>                return "";<br>            }<br>            if (i &gt; 0 &amp;&amp; (a[i] &lt; a[i - 1] || (a[i] == a[i - 1] &amp;&amp; b[i] &lt;= b[i - 1]))) {<br>                int t = a[i]; a[i] = b[i]; b[i] = t;<br>            }<br>            if (a[i] &gt;= 12 || b[i] &gt;= m[a[i]]) {<br>                return "";<br>            }<br>            if (i &gt; 0 &amp;&amp; (a[i] &lt; a[i - 1] || (a[i] == a[i - 1] &amp;&amp; b[i] &lt;= b[i - 1]))) {<br>                return "";<br>            }<br>        }<br>        for (int i = 0; i &lt; n; i++) {<br>            a[i]++;<br>            b[i]++;<br>        }<br>        String r = "";<br>        for (int i = 0; i &lt; n; i++) {<br>            r = r + (a[i] / 10) + (a[i] % 10) + "/" + (b[i] / 10) + (b[i] % 10);<br>            if (i &lt; n - 1) {<br>                r += " ";<br>            }<br>        }<br>        return r;<br>    }<br> <br> <br>}<br> <br> <br>// Powered by PopsEdit<br> <br>// Powered by CodeProcessor<br></td>