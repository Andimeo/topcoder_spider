<td class="problemText" colspan="8" valign="middle" align="left">
            public class SumoTournament<br>{<br>public int maxWeight(int[] w, int avrg)<br> {<br> int i,j,k;<br> int N=w.length;<br><br> boolean[][] viz;<br> viz=new boolean[10010][64];<br> int[][] maximu;<br> maximu = new int[10010][64];<br> <br> viz[0][0]=true;<br> maximu[0][0]=0;<br> <br> for(i=0;i&lt;N;i++)<br>   {<br>   for(j=10000;j&gt;=0;j--)<br>      if(w[i] + j &lt;= 10000)<br>         {<br>         for(k=0;k&lt;50;k++)<br>            if(viz[j][k])<br>              {<br>              viz[ w[i] + j][k+1] = true;<br>              maximu[w[i] + j][k+1] = Math.max(maximu[w[i] + j][k+1] , Math.max(maximu[j][k], w[i]));<br>              }<br>         }<br>   }<br>   <br> int sol=-1;<br> for(i=1; i*avrg&lt;=10000 &amp;&amp; i&lt;=50 ; i++)<br>     if(viz[i*avrg][i] &amp;&amp; maximu[i*avrg][i] &gt; sol)<br>        sol=maximu[i*avrg][i];<br> <br> return sol;<br> }<br><br>}<br></td>