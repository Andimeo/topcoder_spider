            public class FourSubstrings {<br>  <br>  boolean[][] z;<br>  String[] e;<br>  String s;<br>  int[][][] m;<br>  int[][][] M;<br>  boolean[][][] g;<br>  <br>  public int[] count(int p, int o, int b){<br>    if (b==15) return new int[]{0,0};<br>    if (p==s.length()) return new int[]{1000000000,-1000000000};<br>    if (g[p][o][b])<br>      return new int[]{m[p][o][b],M[p][o][b]};<br>    g[p][o][b] = true;<br>    int[] cur = count(p+1,Math.max(o-1,0),b);<br>    for(int i=0;i&lt;4;i++){<br>      if ((b &amp; (1&lt;&lt;i))==0 &amp;&amp; z[p][i]){<br>        int[] t = count(p,Math.max(o,e[i].length()),b+(1&lt;&lt;i));<br>        if (e[i].length()&gt;o){<br>          t[0]+=e[i].length()-o;<br>          t[1]+=e[i].length()-o;<br>        }<br>        if (t[0]&lt;cur[0]) cur[0] = t[0];<br>        if (t[1]&gt;cur[1]) cur[1] = t[1];<br>      }<br>    }<br>    m[p][o][b] = cur[0];<br>    M[p][o][b] = cur[1];<br>    return cur;<br>  }<br>  public int[] getCoverageCount(String[] q, String a, String b, String c, String d){<br>    s = "";<br>    for(int i=0;i&lt;q.length;i++)<br>      s+=q[i];<br>    e = new String[]{a,b,c,d};<br>    z = new boolean[s.length()][4];<br>    for(int i=0;i&lt;s.length();i++){<br>      String qq = s.substring(i,s.length());<br>      for(int j=0;j&lt;4;j++){<br>        z[i][j] = qq.startsWith(e[j]);<br>      }<br>    }<br>    m = new int[s.length()][51][16];<br>    M = new int[s.length()][51][16];    <br>    g = new boolean[s.length()][51][16];<br>    return count(0,0,0);<br>  }<br>}<br></td>