            using System;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class AmbiguousWatch<br>{<br>    private double d1(int A1, int A2)<br>    {<br>        return (A1 + 12 * A2) / 143.0;<br>    }<br><br>    int calc(int h, int m)<br>    {<br>        int res = 0;<br>        for(int AH=0;AH&lt;=h;AH++)<br>            for (int AM = 0; AM &lt; 12; AM++)<br>            {<br>                if (AM == AH)<br>                    continue;<br>                double dh = d1(AH, AM);<br>                double dm = d1(AM, AH);<br>                if (AH==h &amp;&amp; dh * 60 &gt;= m)<br>                    return res;<br>                res++;<br>            }<br>        return res;<br>    }<br><br>    public int howMany(string startTime, string finishTime)<br>    {<br>        DateTime dt1 = DateTime.Parse(startTime);<br>        DateTime dt2 = DateTime.Parse(finishTime);<br><br>        int h1 = dt1.Hour;<br>        int m1 = dt1.Minute;<br><br>        int h2 = dt2.Hour;<br>        int m2 = dt2.Minute;<br><br>        return calc(h2, m2) - calc(h1, m1);<br><br>    }<br>}<br></td>