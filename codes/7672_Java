            import java.util.*;<br>public class AmbiguousWatch {<br>  public int howMany(String start, String end) {<br>    int one = Integer.parseInt(start.substring(0,2))*60+Integer.parseInt(start.substring(3,5));<br>    int two = Integer.parseInt(end.substring(0,2))*60+Integer.parseInt(end.substring(3,5));<br><br>    int count = 0;<br>    for (int i=143*one; i&lt;=143*two; i++) {<br>       if (i%720==0) count++;<br>    }<br>    for (int i=11*one; i&lt;=11*two; i++) {<br>      if (i%720==0) count--;<br>    }<br><br>    return count;<br>  }<br>}<br></td>