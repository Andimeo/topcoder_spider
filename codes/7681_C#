            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class DiamondHunt<br>{<br>  public int countDiamonds(string mine)<br>  {<br>      int initLength = mine.Length;<br>    while(mine.IndexOf("&lt;&gt;") &gt;= 0)<br>    {<br>      mine = mine.Replace("&lt;&gt;", "");<br>    }<br>    <br>    return (initLength - mine.Length) / 2;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>