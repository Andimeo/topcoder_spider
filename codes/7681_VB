            Public Class DiamondHunt<br><br>    Public Function countDiamonds(ByVal mine As String) As Integer<br><br>        Dim chr01 As Char<br>        Dim intStack As Integer<br>        Dim intResult As Integer<br><br>        intStack = 0<br>        For Each chr01 In mine.ToCharArray<br>            If chr01 = "&lt;" Then<br>                intStack += 1<br>            Else<br>                If chr01 = "&gt;" Then<br>                    If intStack &gt; 0 Then<br>                        intStack -= 1<br>                        intResult += 1<br>                    End If<br>                End If<br>            End If<br>        Next<br><br>        Return intResult<br><br>    End Function<br><br>End Class<br></td>