            using System;<br>using System.Collections.Generic;<br><br>public class HeightRound {<br>  public int[] getBestRound(int[] heights) {<br>        Array.Sort(heights);<br>      int answer = 0;<br>        for (int i = 0; i &lt; heights.Length - 2; ++i)<br>            answer = Math.Max(answer, heights[i + 2] - heights[i]);<br>        List&lt;int&gt; l1 = new List&lt;int&gt;();<br>        List&lt;int&gt; l2 = new List&lt;int&gt;();<br>      int cur = heights[0];<br>      int at = 0;<br>        while (cur + answer &lt; heights[heights.Length - 1])<br>        {<br>            while (cur + answer &gt;= heights[at])<br>            {<br>                l1.Add(heights[at]);<br>                ++at;<br>            }<br>            --at;<br>            l1.RemoveAt(l1.Count - 1);<br>            l2.Add(heights[at]);<br>            cur = heights[at];<br>            ++at;<br>        }<br>        while (at &lt; heights.Length)<br>        {<br>            l1.Add(heights[at]);<br>            ++at;<br>        }<br><br>        List&lt;int&gt; res = new List&lt;int&gt;();<br>        foreach (int x in l1)<br>            res.Add(x);<br>        l2.Reverse();<br>        foreach (int x in l2)<br>            res.Add(x);<br>      return res.ToArray();<br>    }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>