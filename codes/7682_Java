            import java.util.*;<br>public class HeightRound {<br>  int n;<br>  int[] b;<br>  <br>  public boolean may(int d,int[] a){<br>    b[0] = a[0];<br>    int l = 1;<br>    int r = n-1;<br>    int pr = a[0];<br>    for(int i=1;i&lt;n;i++){<br>      int j = i;<br>      while (j&lt;n &amp;&amp; a[j]&lt;=pr+d) j++;<br>      j--;<br>      if (j&lt;i) return false;<br>      for(int k=i;k&lt;j;k++){<br>        b[l] = a[k]; l++;<br>      }<br>      b[r] = a[j]; pr = b[r]; r--; <br>      i = j;<br>    }<br>    for(int i=0;i&lt;n-1;i++)<br>      if (Math.abs(b[i]-b[i+1])&gt;d) return false;<br>    if (Math.abs(b[0]-b[n-1])&gt;d) return false;<br>    return true;<br>  }<br>  public int[] getBestRound(int[] a){    <br>    Arrays.sort(a);<br>    n = a.length;<br>    b = new int[n];<br>    int l = 0;<br>    int r = a[n-1];<br>    while (r-l&gt;1){<br>      int xx = (r+l)&gt;&gt;1;<br>      if (may(xx,a)) r = xx;<br>      else l = xx;<br>    }<br>    may(r,a);<br>    return b;<br>    <br>  }<br>}<br></td>