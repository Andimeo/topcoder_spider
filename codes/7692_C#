            using System;<br>using System.Text;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class SquareCypher<br>{<br>    public string decrypt(string cryptogram)<br>    {<br>        int n = cryptogram.Length;<br>        char[] ans = new char[n];<br>        char[] a = cryptogram.ToCharArray();<br><br>        for (int i = 0; i &lt; n; i++) ans[i] = '?';<br><br>        int p = 0;<br>        for (int i = 0; i*i &lt; n; i++)<br>        {<br>            ans[i * i] = a[p++];<br>        }<br><br>        for (int i = 0; i &lt; n; i++) if (ans[i] == '?') ans[i] = a[p++];<br>        return new string(ans);<br>    }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>