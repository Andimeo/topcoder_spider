            import java.util.*;<br><br>public class SquareCypher {<br>  public String decrypt(String cryptogram) {<br>    StringBuilder sb = new StringBuilder();<br>    <br>    String s = cryptogram;<br>    int n = s.length();<br>    int i = 0;<br>    while ((i+1) * (i+1) &lt; n)<br>      i++;<br>    for (int j=i+1; j&lt;n; j++)<br>      sb.append(s.charAt(j));<br>    for (int j=0; j&lt;=i; j++)<br>      sb.insert(j*j, s.charAt(j));<br>    <br>    return sb.toString();<br>  }<br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>