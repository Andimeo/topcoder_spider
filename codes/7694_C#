            using System;<br>using System.Collections;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections.Generic;<br> <br>public class CyclicWords<br>{<br>    bool same(string a, string b)<br>    {<br>        for (int i = 0; i &lt; a.Length; i++)<br>        {<br>            if (a.Substring(i) + a.Substring(0, i) == b)<br>            {<br>                return true;<br>            }<br>        }<br>        return false;<br>    }<br> <br>    public int differentCW(string[] words)<br>    {<br>        List&lt;string&gt; ar = new List&lt;string&gt;();<br>        for (int i = 0; i &lt; words.Length; i++)<br>        {<br>            bool exists = false;<br>            for (int j = 0; j &lt; ar.Count; j++)<br>            {<br>                if (ar[j].Length != words[i].Length) continue;<br>                if (same(ar[j], words[i]))<br>                {<br>                    exists = true;<br>                    break;<br>                }<br>            }<br> <br>            if (!exists) ar.Add(words[i]);<br>        }<br> <br>        return ar.Count;<br>    }<br>}<br> <br>//Powered by [KawigiEdit]<br></td>