            #include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;queue&gt;<br>#include &lt;set&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;ctype.h&gt;<br>#include &lt;map&gt;<br>#include &lt;cmath&gt;<br><br>using namespace std;<br><br>#define PI 3.1415926535897932384<br>#define inf 123456789<br>#define tr(c,i) for(typeof((c).begin()) i = (c).begin(); i != (c).end(); i++)<br>#define all(c) c.begin(), c.end()<br><br>typedef long long ll;<br>typedef vector&lt;int&gt; vi;<br><br>void rev(string&amp; a) {<br>  char t = a[a.size() - 1];<br>  for (int i = a.size() -1;i &gt;= 0;i--)<br>    a[i] = a[i - 1];<br>  a[0] = t;<br>}<br><br>bool same(string a,string b) {<br>  for (int i = 0;i &lt; b.size();i++) {<br>    if (a == b)<br>      return true;<br>    else<br>    rev(b);<br>  }<br>  <br>  return false;<br>}<br><br>class CyclicWords {<br>  public:<br>  int differentCW(vector &lt;string&gt; a) {<br>    vi v(a.size());<br>    <br>    int res = 0;<br>    for (int i = 0;i &lt; a.size();i++)<br>      if (!v[i]) {<br>        res++;<br>        for (int j = i + 1;j &lt; a.size();j++) <br>          if (same(a[i],a[j]))<br>            v[j] = 1;  <br>      }<br>      <br>    return res;<br>  }<br>};<br><br><br>// Powered by FileEdit<br><br><br>// Powered by FileEdit<br><br><br>// Powered by FileEdit<br></td>