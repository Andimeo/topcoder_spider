            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class CyclicWords<br>{<br>  public int differentCW(String[] words)<br>  {<br>    HashSet&lt;String&gt; set = new HashSet&lt;String&gt;();<br>    Big:<br>    for (String word: words) {<br>      for (int i=0; i&lt;word.length(); i++) {<br>        String w = word.substring(i) + word.substring(0, i);<br>        if (set.contains(w))<br>          continue Big;<br>      }<br>      set.add(word);<br>    }<br>    return set.size();<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>