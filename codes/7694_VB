<td class="problemText" colspan="8" valign="middle" align="left">
            Public Class CyclicWords<br>    Public Function differentCW(words As String()) As Integer<br>        Dim data As New System.Collections.ArrayList()<br>        Dim i As Integer <br>        Dim j As Integer<br>        Dim ret As Integer<br>        ret = 0         <br>        For i = 0 To Ubound(words)<br>            If Not data.Contains(words(i)) Then<br>               ret = ret + 1                         <br>            End If<br>            For j = 0 To Len(words(i))<br>                 data.add(Right(words(i), j) + Left(words(i), Len(words(i)) - j))<br>      Next j<br>        Next i<br>        Return ret<br>    End Function<br>End Class<br></td>