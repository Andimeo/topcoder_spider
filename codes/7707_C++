            #include &lt;iostream&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>using namespace std;<br><br>class CricketScores {<br>  public:<br>  vector&lt;int&gt; totalRuns(vector&lt;int&gt; runs){<br>    int a = 0;<br>    int b = 0;<br>    int i = 0;<br>    int j = 0;<br>    int l = runs.size();<br>    for (int k = 0; k &lt; l; k++){<br>      j++; <br>      if (i == 0){<br>        a += runs[k];<br>      } else {<br>        b += runs[k];<br>      }<br>      if (runs[k] % 2 == 1){<br>        i = 1 - i;<br>      }<br>      if (j == 6){<br>        j = 0;<br>        i = 1-i;<br>      }<br>    }<br>    vector&lt;int&gt; ans;<br>    ans.push_back(a);<br>    ans.push_back(b);<br>    return ans;<br>  }<br>};<br></td>