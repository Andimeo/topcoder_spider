            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class CricketScores<br>{<br>  public int[] totalRuns(int[] runs)<br>  {<br>    int A[] = new int[2];<br>    int player = 1;<br>    for(int i=0;i&lt;runs.length;i++){<br>      if(i%6 == 0)<br>        player = 1-player;<br>      A[player] += runs[i];<br>      if(runs[i]%2 == 1)<br>        player = 1-player;<br>    }<br>    return A;<br>  }<br>  <br>  <br> <br> <br>// Utilities <br>private void sout(String str){System.out.println(str);}<br> <br>private void sout(Object a[]){for(int i=0;i&lt;a.length;i++)System.out.print(a[i]+" ");System.out.println();}<br> <br>}<br> <br>//Powered by [KawigiEdit] 2.0!<br></td>