            public class Factorer{<br><br>  public String valq(long q){<br>    if (q&gt;1) return ""+q;<br>    if (q==1) return "";<br>    if (q==-1) return "-";<br>    return ""+q;<br>  }<br>  <br>  public String valp(long p){<br>    if (p&lt;0) return ""+p;<br>    return "+"+p;<br>  }<br>  <br>  public long gcd(long a, long b){<br>    long c = a%b;<br>    while (c!=0){<br>      a = b; b = c; c = a%b;<br>    }<br>    return b;<br>  }<br>  <br>  public String factor(int _a, int _b, int _c){<br>    long a = _a;<br>    long b = _b;<br>    long c = _c;<br>    long dd = b*b-4*a*c;<br>    double d = b*b-4*a*c;<br>    if (d&lt;0) return "NONE";<br>    long v = (long)Math.sqrt(d);<br>    long w = -1;<br>    for(int i=-100;i&lt;100;i++){<br>      long ww = v+i;<br>      if (ww&lt;0) continue;<br>      if (ww*ww==dd){<br>        w = ww; break;<br>      }<br>    }<br>    System.out.println(w);<br>    if (w&lt;0) return "NONE";<br>    long p1 = -(-b+w);<br>    long p2 = -(-b-w);<br>    long q1 = 2*a;<br>    long q2 = 2*a;<br>    if (q1&lt;0){<br>      q1 = -q1; p1 = -p1;<br>      q2 = -q2; p2 = -p2;<br>    }<br>    long t = gcd(Math.abs(p1),Math.abs(q1)); p1/=t; q1/=t;<br>    t = gcd(Math.abs(p2),Math.abs(q2)); p2/=t; q2/=t;<br>    if (a%(q2*q1)!=0) return "NONE";<br>    a/=(q2*q1);<br>    System.out.println(q1+" "+q2);<br>    if (q2&gt;q1){<br>      t = q2; q2 = q1; q1 = t;<br>      t = p2; p2 = p1; p1 = t;<br>    } else {<br>      if (q2==q1){<br>        if (p2&gt;p1){<br>          t = q2; q2 = q1; q1 = t;<br>          t = p2; p2 = p1; p1 = t;<br>        }<br>      }<br>    }<br>    if (a&lt;0){<br>       a = -a;<br>       p2 = -p2;<br>       q2 = -q2;<br>    }<br>    String res = "("+valq(q1*a)+"x"+valp(p1*a)+")("+valq(q2)+"x"+valp(p2)+")";<br>    return res;<br>  }<br>}<br></td>