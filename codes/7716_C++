            #include &lt;algorithm&gt;<br>#include &lt;vector&gt;<br>#include &lt;sstream&gt;<br>#include &lt;cmath&gt;<br>#include &lt;iostream&gt;<br>#include &lt;map&gt;<br>using namespace std;<br> <br>struct BrokenButtons {<br>  int minPresses(int page, vector&lt;int&gt; broken) {<br>    bool ok[20];<br>    for (int i = 0; i &lt; 10; i++) ok[i] = true;<br>    for (int i = 0; i &lt; (int)broken.size(); i++) ok[broken[i]] = false;<br> <br>    int best = abs(page - 100);<br>    for (int p = 0; p &lt;= 1000000; p++) {<br>      char s[10];<br>      sprintf(s, "%d", p);<br> <br>      for (int i = 0; s[i]; i++)<br>        if (!ok[s[i] - '0']) goto skip;<br> <br>      best &lt;?= strlen(s) + abs(p - page);<br>skip:;<br>    }<br> <br>    return best;<br>  }<br>};<br></td>