            import java.util.*;<br><br>public class BrokenButtons {<br>    public int minPresses(int page, int[] broken){<br>      int ans = Math.abs(page - 100);<br>      boolean[] can = new boolean[10];<br>      Arrays.fill(can, true);<br>      for (int x : broken) {<br>        can[x] = false;<br>      }<br>      for (int i = 0; i &lt;= 2000000; i++) {<br>        boolean good = true;<br>        String s = Integer.toString(i);<br>        for (char ch : s.toCharArray()) {<br>          good &amp;= can[ch - '0'];<br>          if (!good) {<br>            break;<br>          }<br>        }<br>        if (good) {<br>          ans = Math.min(ans, Math.abs(page - i) + s.length());<br>        }<br>      }<br>      return ans;<br>    }<br><br>}<br></td>