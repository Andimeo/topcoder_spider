            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class ChessMatchup <br>{<br>  const int INF = 987654321;<br><br>  public int maximumScore(int[] us, int[] them) <br>  {<br>        int n = us.Length;<br>        Array.Sort(us);<br>        Array.Sort(them);<br><br>        int res = 0;<br>        bool[] aus = new bool[n];<br>        bool[] athem = new bool[n];<br><br>        for (int i = n-1; i &gt;= 0; i--)<br>        {<br>            for (int j = 0; j &lt; n; ++j)<br>            {<br>                if (aus[j]) continue;<br><br>                if (us[j] &gt; them[i])<br>                {<br>                    aus[j] = true;<br>                    athem[i] = true;<br>                    res += 2;<br>                    break;<br>                }<br>            }<br>        }<br><br>        for (int i = n - 1; i &gt;= 0; i--)<br>        {<br>            if (athem[i]) continue;<br><br>            for (int j = 0; j &lt; n; ++j)<br>            {<br>                if (aus[j]) continue;<br><br>                if (us[j] == them[i])<br>                {<br>                    aus[j] = true;<br>                    athem[i] = true;<br>                    res += 1;<br>                    break;<br>                }<br>            }<br>        }<br><br>        return res;<br>  }<br><br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>