            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class TeamSelection<br>{<br>  public int selectBestTeam(int[] rating, String[] compatibility)<br>  {<br>    int ret = -1;<br>    int l = rating.length;<br>    for(int i = 0 ; i &lt; l ; i++)<br>      for(int j = i + 1 ; j &lt; l ; j++)<br>        for(int k = j + 1 ; k &lt; l ; k++)<br>        {<br>          int tmp = rating[i] + rating[j] + rating[k];<br>          if(ret &lt; tmp)<br>          {<br>            if(compatibility[i].charAt(j) == 'Y' &amp;&amp;<br>               compatibility[i].charAt(k) == 'Y' &amp;&amp;<br>               compatibility[j].charAt(k) == 'Y')<br>            {<br>              ret = tmp;<br>            }<br>          }<br>        }<br>    return ret;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>