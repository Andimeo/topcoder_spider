            import java.util.*;<br>public class PolishNotation<br>{<br>  public long waysToDecode(String expression)<br>  {<br>    ch = expression.toCharArray();<br>    n = ch.length;<br>    memo = new long[n][n];<br>    for(int i=0;i&lt;memo.length;i++)<br>      Arrays.fill(memo[i], -1L);<br>    <br>    return solve(0,n-1);<br>  }<br>  char[] ch; int n;<br>  long[][] memo;<br>  String ops = "-+*/";<br>  long solve(int start, int end)<br>  {<br>    if(memo[start][end]==-1)<br>    {<br>      long res = 0;<br>      if(isNumber(start,end))<br>        res++;<br>      if(ops.indexOf(ch[start])!=-1)<br>      {<br>        for(int i=start+1;i&lt;end;i++)<br>        {<br>          res += solve(start+1,i)*solve(i+1,end);<br>        }<br>      }<br>      memo[start][end] = res;<br>    }<br>    return memo[start][end];<br>  }<br>  boolean isNumber(int start, int end)<br>  {<br>    if(ch[start]=='-') start++;<br>    if(start&gt;end) return false;<br>    for(int i=start;i&lt;=end;i++)<br>      if(ch[i]&lt;'0' || ch[i]&gt;'9')<br>        return false;<br>    return true;<br>  }<br>}<br></td>