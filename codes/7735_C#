            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections.Generic;<br><br>public class MaximizeSquares<br>{<br>    public int squareCount(int N)<br>    {<br>        int res = 0;<br>        for (int i = 1; i * i &lt;= N; i++)<br>        {<br>            int j = N / i;<br>            int cnt = 0;<br>            for (int t = 1; t &lt;= i; t++)<br>            {<br>                cnt += (i - t) * (j - t);<br>            }<br>            int k = N % i;<br>            if (k &gt; 1)<br>            {<br>                cnt += k * (k - 1) / 2;<br>            }<br>            res = Math.Max(res, cnt);<br>        }<br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>