            using System;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class Aircraft {<br>  public string nearMiss(int[] p1, int[] v1, int[] p2, int[] v2, int R) {<br>        long[] x = new long[3];<br>        for (int i = 0; i &lt; 3; ++i)<br>            x[i] = p2[i] - p1[i];<br>        long[] v = new long[3];<br>        for (int i = 0; i &lt; 3; ++i)<br>            v[i] = v2[i] - v1[i];<br>        long[] prod = new long[3];<br>        for (int i = 0; i &lt; 3; ++i)<br>            prod[i] = x[(i + 1) % 3] * v[(i + 2) % 3] - v[(i + 1) % 3] * x[(i + 2) % 3];<br>        long p = 0;<br>        for (int i = 0; i &lt; 3; ++i)<br>            p += prod[i] * prod[i];<br>        long q = 0;<br>        for (int i = 0; i &lt; 3; ++i)<br>            q += v[i] * v[i];<br>        q *= (long) R * R;<br>        if (p &gt; q)<br>            return "NO";<br><br>        p = 0;<br>        for (int i = 0; i &lt; 3; ++i)<br>            p += x[i] * x[i];<br>        q = (long)R * R;<br>        if (p &lt;= q)<br>            return "YES";<br><br>        long z = 0;<br>        for (int i = 0; i &lt; 3; ++i)<br>            z += x[i] * v[i];<br>        if (z &gt;= 0)<br>            return "NO";<br>        else<br>            return "YES";<br>    }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>