            /*<br>* @author nik<br>*/<br><br>public class Aircraft {<br><br>  public String nearMiss(int[] p1, int[] v1, int[] p2, int[] v2, int R) {<br>    long a = (v1[0]-v2[0])*(v1[0]-v2[0]) + (v1[1]-v2[1])*(v1[1]-v2[1]) +(v1[2]-v2[2])*(v1[2]-v2[2]);<br>    long b = (v1[0]-v2[0])*(p1[0]-p2[0]) + (v1[1]-v2[1])*(p1[1]-p2[1]) +(v1[2]-v2[2])*(p1[2]-p2[2]);<br>    long c = (p1[0]-p2[0])*(p1[0]-p2[0]) + (p1[1]-p2[1])*(p1[1]-p2[1]) +(p1[2]-p2[2])*(p1[2]-p2[2]);<br>    long R2 = R*R;<br>    if (a == 0) {<br>      return c &lt;= R2 ? "YES" : "NO";<br>    }<br><br>    if (b &gt;= 0) {<br>      return c &lt;= R2 ? "YES" : "NO"; <br>    }<br>    return -b * b  + c*a &lt;= R2*a ? "YES" : "NO"; <br>  }<br><br>}<br></td>