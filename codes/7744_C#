            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class NotationLength <br>{<br>    private double calc(int sz, int tern)<br>    {<br>        double ret = Math.Log(sz) / Math.Log(tern)+1;<br>        return (int)ret / ((double)sz.ToString().Length);<br>    }<br>  public double avgRatio(int radix, int low, int high) <br>  {<br>        double ret = 0;<br>        int o = 0;<br>        for (int i = low; i &lt;= high; i++)<br>        {<br>            ret += calc(i,radix);<br>            o++;<br>        }<br>        ret /= o;<br>        return ret;<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by r1cs1]<br>// Powered by CodeProcessor<br></td>