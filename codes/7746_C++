            #include&lt;string&gt;<br>#include&lt;vector&gt;<br>#include&lt;cmath&gt;<br>#include&lt;sstream&gt;<br>#include&lt;iostream&gt;<br>#include&lt;set&gt;<br>#include&lt;map&gt;<br>#include&lt;algorithm&gt;<br>using namespace std;<br>string key[12][2]={{"A","A"},{"A#","Bb"},{"B","B"},{"C","C"},{"C#","Db"},{"D","D"},{"D#","Eb"},{"E","E"},{"F","F"},{"F#","Gb"},{"G","G"},{"G#","Ab"}};<br>class TransposeMyMusic{<br>public:<br>  int resolve(string kk)<br>  {<br>    int i,j;<br>    string tmp="";<br>    for (i=0;i&lt;kk.size();++i)<br>      if (kk[i]!='m') <br>        tmp+=kk[i];<br>    for (i=0;i&lt;12;++i)<br>      for (j=0;j&lt;2;++j)<br>        if (key[i][j]==tmp) return i;<br>  }<br>  string transposeMusic(vector &lt;string&gt; sheetMusic, string newKey){<br>    string tmp="";<br>    string tt;<br>    for (int i=0;i&lt;sheetMusic.size();++i)<br>      tmp+=sheetMusic[i];<br>    istringstream in(tmp);<br>    bool first=0;<br>    in&gt;&gt;tt;<br>    int p;<br>    if (newKey[0]=='F' &amp;&amp; (newKey.size()==1 || newKey[1]!='#')|| newKey.size()&gt;1 &amp;&amp; newKey[1]=='b') p=1;<br>    else p=0;<br>    int i=resolve(tt);<br>    int j=resolve(newKey);<br>    string t1=key[(resolve(tt)+12+j-i)%12][p];<br>    if (tt[tt.size()-1]=='m') t1+="m";<br>    string ans=t1;<br><br>    while (in&gt;&gt;tt)<br>    {<br>      if (tt=="|") ans+=" |";<br>      else<br>      {<br>        string t1=key[(resolve(tt)+12+j-i)%12][p];<br>        if (tt[tt.size()-1]=='m') t1+="m";<br>        ans+=" "+t1;<br>      }<br>    }<br>    return ans;  <br>  }<br><br>};<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>