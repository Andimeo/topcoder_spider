            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text; <br><br>public class GuitarConcert<br>{<br>    int bestS, bestG;<br>    int n, m;<br>    string[] ans;<br>    string[] gn;<br>    string[] makeAns(int g, int mask)<br>    {<br>        string[] ret = new string[g];<br>        int i, k=0;<br>        for (i = 0; i &lt; n; i++) if ((mask &amp; (1 &lt;&lt; i)) &gt; 0) ret[k++] = gn[i];<br>        Array.Sort(ret);<br>        return ret;<br>    }<br>    bool better(string[] x, string[] y)<br>    {<br>        int i;<br>        for (i=0; i&lt;x.Length; i++)<br>        {<br>            int v = x[i].CompareTo(y[i]);<br>            if (v &lt; 0) return true;<br>            if (v &gt; 0) return false;<br>        }<br>        return false;<br>    }<br>    public string[] buyGuitars(string[] guitarNames, string[] guitarSongs)<br>    {<br>        gn = guitarNames;<br>        bestS = -1;<br>        bestG = -1;<br>        n = guitarNames.Length;<br>        m = guitarSongs[0].Length;<br>        int mask, i, j;<br>        for (mask=0; mask&lt;(1&lt;&lt;n); mask++)<br>        {<br>            int s = 0;<br>            int g = 0;<br>            bool[] ex = new bool[m];<br>            for (i=0; i&lt;n; i++) if ((mask &amp; (1&lt;&lt;i))&gt;0)<br>            {<br>                g++;<br>                for (j = 0; j &lt; m; j++) if (!ex[j] &amp;&amp; guitarSongs[i][j] == 'Y') { ex[j] = true; s++; }<br>            }<br>            if (bestS == -1 || bestS &lt; s || (bestS == s &amp;&amp; bestG &gt; g))<br>            {<br>                bestS = s;<br>                bestG = g;<br>                ans = makeAns(g, mask);<br>                continue;<br>            }<br>            if (bestS == s &amp;&amp; bestG == g)<br>            {<br>                string[] res = makeAns(g, mask);<br>                if (better(res, ans)) ans = res;<br>            }<br>        }<br>        return ans;<br>    }<br><br>    // helper<br>    void cout(object o) { System.Console.WriteLine(o); } <br><br>};<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>