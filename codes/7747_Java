            import java.util.*;<br> <br>public class GuitarConcert {<br>  public String[] buyGuitars(String[] guitarNames, String[] guitarSongs) {<br>        String[] res = guitarNames.clone();<br>        Arrays.sort(res);<br>        boolean[] any = new boolean[guitarSongs[0].length()];<br>        for (String s : guitarSongs)<br>            for (int i = 0; i &lt; s.length(); ++i)<br>                if (s.charAt(i) == 'Y')<br>                    any[i] = true;<br>        for (int take = 0; take &lt; (1 &lt;&lt; guitarNames.length); ++take) {<br>            List&lt;String&gt; curList = new ArrayList&lt;String&gt;();<br>            boolean[] cAny = new boolean[guitarSongs[0].length()];<br>            for (int i = 0; i &lt; guitarNames.length; ++i)<br>                if (((take &gt;&gt; i) &amp; 1) != 0) {<br>                    curList.add(guitarNames[i]);<br>                    String s = guitarSongs[i];<br>                    for (int j = 0; j &lt; s.length(); ++j)<br>                        if (s.charAt(j) == 'Y')<br>                            cAny[j] = true;<br>                }<br>            boolean ok = true;<br>            for (int j = 0; j &lt; guitarSongs[0].length(); ++j)<br>                if (any[j] &amp; !cAny[j])<br>                    ok = false;<br>            if (!ok)<br>                continue;<br>            String[] cur = curList.toArray(new String[0]);<br>            Arrays.sort(cur);<br>            if (less(cur, res))<br>                res = cur;<br>        }<br>        return res;<br>  }<br> <br>    private boolean less(String[] a, String[] b) {<br>        if (a.length != b.length)<br>            return a.length &lt; b.length;<br>        for (int i = 0; i &lt; a.length; ++i) {<br>            int z = a[i].compareTo(b[i]);<br>            if (z != 0)<br>                return z &lt; 0;<br>        }<br>        return false;<br>    }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by 