            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class BrokenStrings {<br>public:<br>  int buyStrings(int, vector &lt;string&gt;);<br>};<br> <br>int BrokenStrings::buyStrings(int n, vector &lt;string&gt; stringCosts) {<br>  int a=10000,b=10000;<br>  for (int i=0; i&lt;stringCosts.size(); ++i)<br>  {<br>    istringstream In(stringCosts[i]);<br>    int x,y;<br>    In&gt;&gt;x;<br>    In&gt;&gt;y;<br>    if (x&lt;a) a=x;<br>    if (y&lt;b) b=y;<br>  }<br>  if (b*6&lt;=a) return b*n;<br>  int ans=n/6*a;<br>  if (b*(n%6)&lt;=a) ans+=b*(n%6);<br>  else<br>    ans+=a;<br>  return ans;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>