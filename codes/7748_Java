            import java.util.*;<br><br>public class BrokenStrings {<br>  public int buyStrings(int n, String[] stringCosts) {<br>    int[] opt = new int[200];<br>    Arrays.fill(opt, 100000000);<br>    opt[0] = 0;<br>    try {<br>      for (int j = 0; j &lt; stringCosts.length; j++) {<br>        String[] s = stringCosts[j].split(" ");<br>        int a = Integer.parseInt(s[0]), b = Integer.parseInt(s[1]);<br>        for (int i = 0; i &lt; 200 - 6; i++) {<br>          if (opt[i] + a &lt; opt[i + 6])<br>            opt[i + 6] = opt[i] + a;<br>          if (opt[i] + b &lt; opt[i + 1])<br>            opt[i + 1] = opt[i] + b;<br>        }<br>      }<br>      int res = 100000000;<br>      for (int i = n; i &lt; 200; i++)<br>        if (opt[i] &lt; res)<br>          res = opt[i];<br>      return res;<br>    } catch (Exception e) {<br>      return -1;<br>    }<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>