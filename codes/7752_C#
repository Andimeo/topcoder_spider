            using System;<br>using System.Collections.Generic;<br>using System.Text;<br>using System.Text.RegularExpressions;<br><br>public class OptimalList<br>{<br>    public string optimize(string inst)<br>    {<br>        int x = 0, y = 0;<br>        foreach (char c in inst)<br>        {<br>            if (c == 'E') x++;<br>            if (c == 'W') x--;<br>            if (c == 'N') y--;<br>            if (c == 'S') y++;<br>        }<br>        string ret = "";<br>        if (x &gt; 0)<br>            for (int i = 0; i &lt; x; i++) ret += "E";<br>        if (y &lt; 0)<br>            for (int i = 0; i &lt; Math.Abs(y); i++) ret += "N";<br>        if (y &gt; 0)<br>            for (int i = 0; i &lt; y; i++) ret += "S";<br>        if (x &lt; 0)<br>            for (int i = 0; i &lt; Math.Abs(x); i++) ret += "W";<br>        return ret;<br>    }<br>}<br></td>