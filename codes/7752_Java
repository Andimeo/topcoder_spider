            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class OptimalList<br>{<br>  public String optimize(String inst)<br>  {<br>    int n=0,e=0;<br>    for (int i = 0; i &lt; inst.length(); ++i) {<br>      char c =inst.charAt(i);<br>      if (c == 'N') n++;<br>      if (c == 'S') n--;<br>      if (c == 'E') e++;<br>      if (c == 'W') e--;<br>    }<br>    String out = "";<br>    if (e &gt; 0)<br>      for (int i = 0; i &lt; e; ++i) out +='E';<br>    if (n &gt; 0)<br>      for (int i = 0; i &lt; n; ++i) out +='N';<br>    if (n &lt; 0)<br>      for (int i = 0; i &lt; (-n); ++i) out +='S';<br>    if (e &lt; 0)<br>      for (int i = 0; i &lt; (-e); ++i) out +='W';<br>    return out;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>