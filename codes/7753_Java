            public class IncreasingSubsequences {<br>    public long count(int[] a) {<br>        long f[] = new long[50];<br>        f[0] = 1;<br>        for (int i = 1; i &lt; a.length; i++) {<br>            long sum = 0;<br><br>            boolean mm = true;<br>            for (int j = 0; j &lt; i; j++) {<br>                if (a[j] &lt; a[i]) {<br>                    mm = false;<br>                    break;<br>                }<br>            }<br>            if (mm) {<br>                sum = 1;<br>            }<br><br>            for (int j = 0; j &lt; i; j++) {<br>                if (a[i] &lt; a[j]) {<br>                    continue;<br>                }<br>                boolean ok = true;<br>                for (int k = j + 1; k &lt; i; k++) {<br>                    if (a[k] &gt; a[j] &amp;&amp; a[k] &lt; a[i]) {<br>                        ok = false;<br>                        break;<br>                    }<br>                }<br>                if (ok) {<br>                    sum += f[j];<br>                }<br>            }<br>            f[i] = sum;<br><br>        }<br>        long sum = 0;<br>        long max = Long.MIN_VALUE;<br>        for (int i = a.length - 1; i &gt;= 0; i--) {<br>            if (a[i] &gt; max) {<br>                sum += f[i];<br>                max = a[i];<br>            }<br>        }<br>        return sum;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>