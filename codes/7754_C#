            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br> <br>public class LostParentheses<br>{<br>  int cc (string s)<br>  {<br>    string[] q = s.Split ('+');<br>    <br>    int r = 0;<br>    <br>    foreach (string ss in q)<br>      r += int.Parse (ss);<br>    <br>    return r;<br>  }<br>  <br>  public int minResult (string e)<br>  {<br>    string[] q = e.Split ('-');<br>    <br>    <br>    int num = cc (q[0]);<br>    <br>    for (int i = 1; i &lt; q.Length; ++i)<br>      num -= cc (q[i]);<br>    <br>    return num;<br>  }<br>  <br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>