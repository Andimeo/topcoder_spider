            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class LostParentheses<br>{<br>  public int minResult(String e)<br>  {<br>    String ee = e.replaceAll("[+-]"," ");<br>    String[] nums1 = ee.split(" ");<br>    ee = e.replaceAll("[^+-]","");<br>    int[] nums = new int[nums1.length];<br>    for(int i = 0; i &lt; nums1.length; i++)<br>      nums[i] = Integer.parseInt(nums1[i]);<br>    int sum = 0;<br>    boolean b = false;<br>    for(int i = 0; i &lt; nums.length; i++)<br>    {<br>      if(!b) sum += nums[i];<br>      else sum -= nums[i];<br>      if(i &lt; ee.length() &amp;&amp; ee.charAt(i) == '-')<br>        b = true;<br>    }<br>    return sum;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>