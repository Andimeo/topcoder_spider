            #include&lt;stdio.h&gt;<br>#include&lt;vector&gt;<br>#include&lt;string&gt;<br>#include&lt;map&gt;<br>#include&lt;algorithm&gt;<br>using namespace std;<br>const int oo=0X10101010;<br>int in[10000+4],ax[10000+4],z[10000+4];<br>int tt(int n,int f) // 1 [\u22823] [\u24555] <br>{<br>  int now,i,d;<br>  if(f)<br>  {<br>    if(in[n]!=-1)<br>      return in[n];<br>    now=oo;<br>    for(i=1;i*i&lt;=n;i++)<br>    {<br>      if(z[n-i*i])<br>        continue;<br>      d=tt(n-i*i,!f);<br>      if(d&lt;now)<br>        now=d;<br>    }<br>    return in[n]=now+1;<br>  }<br>  else<br>  {<br>    if(ax[n]!=-1)<br>      return ax[n];<br>    now=0;<br>    for(i=1;i*i&lt;=n;i++)<br>    {<br>      if(!z[n-i*i])<br>        continue;<br>      d=tt(n-i*i,!f);<br>      if(d&gt;now)<br>        now=d;<br>    }<br>    return ax[n]=now+1;<br>  }<br>}<br>class PowerGame<br>{<br>public:<br>  string winner(int n, int m)<br>  {<br>    int i,j,d1,d2;<br>    string ans;<br>    bool u[1024];<br>    z[0]=0;<br>    for(i=1;i&lt;=10000;i++)<br>    {<br>      memset(u,0,sizeof(u));<br>      for(j=1;j*j&lt;=i;j++)<br>        u[z[i-j*j]]=true;<br>      for(j=0;;j++)<br>        if(!u[j])<br>          break;<br>      z[i]=j;<br>    }<br>    memset(in,0XFF,sizeof(in));<br>    memset(ax,0XFF,sizeof(ax));<br>    in[0]=ax[0]=0;<br>    if(z[n])<br>      d1=tt(n,1);<br>    else<br>      d1=tt(n,0);<br>    if(z[m])<br>      d2=tt(m,1);<br>    else<br>      d2=tt(m,0);<br>    if(z[n]&amp;&amp;z[m])<br>    {<br>      ans="Alan will win after ";<br>      char str[16];<br>      if(d1&gt;d2)<br>        d1=d2;<br>      sprintf(str,"%d",d1);<br>      ans+=str;<br>      ans+=" moves";<br>      return ans;<br>    }<br>    if(!z[n]&amp;&amp;!z[m])<br>    {<br>      ans="Bob will win after ";<br>      char str[16];<br>      if(d1&gt;d2)<br>        d1=d2;<br>      sprintf(str,"%d",d1);<br>      ans+=str;<br>      ans+=" moves";<br>      return ans;<br>    }<br>    if(z[n]&amp;&amp;!z[m])<br>    {<br>      if(d1&gt;d2)<br>      {<br>        ans="Bob will win after ";<br>        char str[16];<br>        sprintf(str,"%d",d2);<br>        ans+=str;<br>        ans+=" moves";<br>        return ans;<br>      }<br>      else<br>      {<br>        ans="Alan will win after ";<br>        char str[16];<br>        sprintf(str,"%d",d1);<br>        ans+=str;<br>        ans+=" moves";<br>        return ans;<br>      }<br>    }<br>    if(d1&lt;=d2)<br>    {<br>      ans="Bob will win after ";<br>      char str[16];<br>      sprintf(str,"%d",d1);<br>      ans+=str;<br>      ans+=" moves";<br>      return ans;<br>    }<br>    else<br>    {<br>      ans="Alan will win after ";<br>      char str[16];<br>      sprintf(str,"%d",d2);<br>      ans+=str;<br>      ans+=" moves";<br>      return ans;<br>    }<br>  }<br>};<br></td>