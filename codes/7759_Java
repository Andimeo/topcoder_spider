            import java.util.*;<br> <br>public class NoEights<br>{<br>  public int smallestAmount(int low, int high)<br>  {<br>    String hs = ""+high;<br>    int n = hs.length();<br>    String ls = ""+low;  <br>    for (int i=ls.length(); i&lt;n; i++)<br>      ls = "0"+ls;<br>    int sa = 0;<br>    for (int i=0; i&lt;n; i++)<br>    {<br>      if (hs.charAt(i) != ls.charAt(i))<br>        break;<br>      else if (hs.charAt(i) == '8')<br>        sa++;<br>    }<br>    return sa;<br>  }<br>}<br> <br> <br>// Powered by FileEdit<br></td>