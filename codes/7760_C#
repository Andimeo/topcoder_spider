            using System;<br>using System.Collections; <br>using System.Collections.Generic;<br><br>public class RailwayTickets {<br>  public int minRejects(string[] travel, int seats) <br>  {<br>        List&lt;int&gt; x = new List&lt;int&gt;();<br>        List&lt;int&gt; y = new List&lt;int&gt;();<br>        for (int i = 0; i &lt; travel.Length; i++)<br>        {<br>            string[] t = travel[i].Split(' ');<br>            for (int j = 0; j &lt; t.Length; j++)<br>            {<br>                string[] w = t[j].Split('-');<br>                x.Add(Convert.ToInt32(w[0]));<br>                y.Add(Convert.ToInt32(w[1]));<br>            }<br>        }<br><br>        int[] x1 = x.ToArray();<br>        int[] y1 = y.ToArray();<br><br>        Array.Sort(y1, x1);<br>        <br>        int res=0;<br><br>        int[] v = new int[11111];<br>        for (int i = 0; i &lt; y1.Length; i++)<br>        {<br>            int mx = 0;<br>            for (int j = x1[i]; j &lt; y1[i]; j++)<br>                if (v[j] &gt; mx)<br>                    mx = v[j];<br>            if (mx &lt; seats)<br>            {<br>                for (int j = x1[i]; j &lt; y1[i]; j++)<br>                    v[j]++;<br>         <br>            }<br>            else res++;<br>        }    <br><br>        return res;<br>  }  <br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>