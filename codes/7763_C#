            using System;<br>using System.Collections;<br>using System.Text.RegularExpressions;<br>public class DocumentSearch {<br>  public int nonIntersecting(string[] doc, string search) {<br>    string s="";<br>    for(int i=0;i&lt;doc.Length;i++)<br>    {<br>      s+=doc[i];<br>    }<br>    int c=0;<br>    while(s.IndexOf(search)!=-1)<br>    {<br>      int ind=s.IndexOf(search);<br>      s=s.Substring(ind+search.Length);<br>      c++;<br>    }<br>    return c;<br>  }<br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>