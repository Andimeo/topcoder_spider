            import java.util.*;<br><br>public class RadarFinder {<br>  public int possibleLocations(int x1, int y1, int r1, int x2, int y2, int r2) {<br>    if (x1 == x2 &amp;&amp; y1 == y2 &amp;&amp; r1 == r2)<br>      return -1;<br>    if (x1 == x2 &amp;&amp; y1 == y2)<br>      return 0;<br>    long d = ((long)x1-x2)*(x1-x2) + ((long)y1-y2)*(y1-y2);<br>    if (d &gt; ((long)r1+r2)*((long)r1+r2))<br>      return 0;<br>    if (d &lt; ((long)r2-r1)*((long)r2-r1))<br>      return 0;<br>    if (d == ((long)r1+r2)*((long)r1+r2))<br>      return 1;<br>    if (d == ((long)r1-r2)*((long)r1-r2))<br>      return 1;<br>    return 2;<br>  }<br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>