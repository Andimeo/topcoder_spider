            using System;<br><br>public class CoinsExchange {<br>  public int countExchanges(int G1, int S1, int B1, int G2, int S2, int B2) {<br>    int res = 0;<br>    while (G2 &gt; G1)<br>    {<br>      --G2;<br>      S2 += 11;<br>      ++res;<br>    }<br>    while (S2 &gt; S1)<br>    {<br>      if (G1 &gt; G2)<br>      {<br>        --G1;<br>        S1 += 9;<br>        ++res;<br>      } <br>      else<br>      {<br>        --S2;<br>        B2 += 11;<br>        ++res;<br>      }<br>    }<br>    while (B2 &gt; B1)<br>    {<br>      if (S1 &gt; S2)<br>      {<br>        --S1;<br>        B1 += 9;<br>        ++res;<br>      }<br>      else if (G1 &gt; G2)<br>      {<br>        --G1;<br>        S1 += 9;<br>        ++res;<br>      }<br>      else return -1;<br>    }<br>    return res;<br>  }<br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>