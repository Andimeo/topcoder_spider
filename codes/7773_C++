            #include &lt;algorithm&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;string&gt;<br>#include &lt;utility&gt;<br>#include &lt;vector&gt;<br> <br>using namespace std;<br> <br>typedef long long Long;<br>typedef double dbl;<br>typedef vector &lt;int&gt; VI;<br>typedef pair &lt;int, int&gt; PII;<br> <br>#define FI first<br>#define MP make_pair<br>#define PB push_back<br>#define SE second<br> <br>class CoinsExchange<br>{<br>  public:<br>    int countExchanges( int G1, int S1, int B1, int G2, int S2, int B2 )<br>    {<br>      int ans = 0;<br>      while (G1 &lt; G2)<br>      {<br>        while (S1 &lt; 11)<br>        {<br>          if (B1 &lt; 11)<br>            return -1;<br>          B1 -= 11, S1++;<br>          ans++;<br>        }<br>        S1 -= 11, G1++;<br>        ans++;<br>      }<br>      while (S1 &lt; S2)<br>      {<br>        if (G1 &gt; G2)<br>        {<br>          S1 += 9, G1--;<br>          ans++;<br>        }<br>        else<br>        {<br>          if (B1 &lt; 11)<br>            return -1;<br>          B1 -= 11, S1++;<br>          ans++;<br>        }<br>      }<br>      while (B1 &lt; B2)<br>      {<br>        while (S1 &lt;= S2)<br>        {<br>          if (G1 &lt;= G2)<br>            return -1;<br>          S1 += 9, G1--;<br>          ans++;<br>        }<br>        B1 += 9, S1--;<br>        ans++;<br>      }<br>      return ans;<br>    }<br>};<br> <br>// Code by Burunduk3 =)<br> <br> <br>// Powered by PopsEdit<br></td>