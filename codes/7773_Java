            public class CoinsExchange {<br>    public int countExchanges(int G1, int S1, int B1, int G2, int S2, int B2) {<br>        int g = G1;<br>        int s = S1;<br>        int b = B1;<br>        int res = 0;<br>        while (b &lt; B2) {<br>            res++;<br>            b += 9;<br>            s--;<br>        }<br>        while (g &lt; G2) {<br>            res++;<br>            g++;<br>            s -= 11;<br>        }<br>        while (s &lt; S2) {<br>            res++;<br>            if (g &gt; G2) {<br>                g--;<br>                s += 9;<br>            } else if (b &gt;= B2 + 11) {<br>                s++;<br>                b -= 11;<br>            } else {<br>                return -1;<br>            }<br>        }<br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>