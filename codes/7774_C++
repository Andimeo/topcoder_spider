            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class Thimbles {<br>public:<br>  int thimbleWithBall(vector &lt;string&gt;);<br>};<br> <br>int Thimbles::thimbleWithBall(vector &lt;string&gt; s) {<br>  int cur = 1;<br>  for(int i = 0; i &lt; s.size(); i ++)<br>  {<br>    int x, y;<br>    sscanf(s[i].c_str(), "%d-%d", &amp;x, &amp;y);<br>    if(y == cur)cur = x;<br>    else if(x == cur) cur = y;<br>  }<br>  return cur;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>