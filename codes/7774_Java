            public class Thimbles<br>{<br>  public int thimbleWithBall(String[] swaps)<br>  {<br>    int pt=1;<br>    for(int i=0; i&lt;swaps.length;i++)<br>    {<br>    String s1= "";<br>    String s2 ="";<br>    int fa=0;<br>      for(int j=0; j&lt;swaps[i].length();j++)<br>      {<br>        if(swaps[i].charAt(j)!='-'&amp;&amp;fa==0)<br>        {<br>          s1 = s1+swaps[i].charAt(j);<br>        }<br>        if(swaps[i].charAt(j)!='-'&amp;&amp;fa==1)<br>        {<br>          s2 = s2+swaps[i].charAt(j);<br>        }<br>        if(swaps[i].charAt(j)=='-')<br>        fa=1;<br>      }<br>      if (Integer.parseInt(s1) == pt)<br>      {<br>        pt = Integer.parseInt(s2);<br>        continue;<br>      }<br>      if (Integer.parseInt(s2) == pt)<br>      pt = Integer.parseInt(s1);<br>    }<br>    return pt;<br>  }<br>}<br></td>