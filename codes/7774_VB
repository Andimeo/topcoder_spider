            Public Class Thimbles<br>        Public Function thimbleWithBall(ByVal swaps As String()) As Integer<br>            Dim tab(4) As Integer<br>            tab(0) = 0 : tab(1) = 1 : tab(2) = 0 : tab(3) = 0<br>            Dim i As Integer<br>            For i = 0 To swaps.Length - 1<br>                Dim temp As Integer<br>                temp = tab(Integer.Parse(swaps(i).Substring(0, 1)))<br>                tab(Integer.Parse(swaps(i).Substring(0, 1))) = tab(Integer.Parse(swaps(i).Substring(2, 1)))<br>                tab(Integer.Parse(swaps(i).Substring(2, 1))) = temp<br>            Next<br>            For i = 0 To 3<br>                If tab(i) = 1 Then<br>                    Return i<br>                End If<br>            Next<br>        End Function<br>    End Class<br></td>