            #include &lt;vector&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;queue&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br>#include &lt;ctime&gt;<br>using namespace std;<br> <br> <br>class AttendanceShort {<br>public:<br>  vector &lt;string&gt; shortList(vector &lt;string&gt;, vector &lt;string&gt;);<br>};<br> <br>vector &lt;string&gt; AttendanceShort::shortList(vector &lt;string&gt; names, vector &lt;string&gt; attendance) {<br>  vector&lt;string&gt; ans;<br>  for(int i=0;i&lt;names.size();i++){<br>    int all=0;<br>    int here=0;<br>    for(int j=0;j&lt;attendance[i].size();j++){<br>      if(attendance[i][j]=='M')continue;<br> <br>      all++;<br>      if(attendance[i][j]=='P')here++;<br>    }<br>    if(here*4/3&lt;all)ans.push_back(names[i]);<br>  }<br> <br>  return ans;<br>}<br> <br>//<br>// All My Achievements Are Dedicated To Lala<br>// Powered by [KawigiEdit] 3.0!<br></td>