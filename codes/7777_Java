            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class AttendanceShort<br>{<br>  public String[] shortList(String[] a, String[] b)<br>  {<br>    ArrayList&lt;String&gt;p=new ArrayList&lt;String&gt;();<br> <br>    for(int i=0;i&lt;a.length;i++)<br>    {<br>          String t=a[i];<br>          String s=b[i];<br>          double P=0,A=0,M=0;<br>          for(int j=0;j&lt;s.length();j++)<br>          {<br>                if(s.charAt(j)=='M') continue;<br>                if(s.charAt(j)=='A') A++;<br>                if(s.charAt(j)=='P') P++;<br>          }<br>            double total=P+A;<br>          double per=P*100/total;  <br>          if(per&lt;75.0)<br>            p.add(t);<br>    }<br>    String ret[]=new String[p.size()];<br>    for(int i=0;i&lt;p.size();i++)<br>    ret[i]=p.get(i);<br> <br>    return ret;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>