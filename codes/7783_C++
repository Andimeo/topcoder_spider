            #include &lt;stdio.h&gt;<br>#include &lt;string&gt;<br>#include &lt;iostream&gt;<br>#include &lt;vector&gt;<br>#include &lt;stack&gt;<br>#include &lt;queue&gt;<br>#include &lt;math.h&gt;<br>#include &lt;ctype.h&gt;<br>#include &lt;map&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;stdlib.h&gt;<br>#include &lt;sstream&gt;<br><br>using  namespace  std;<br><br>#define  inf 1000000000<br>#define  eps 1e-9<br><br>#define  ss 100<br>#define  st 999999<br>#define  ss1 0x7fffffff<br>int   res[ss][ss];<br>bool  vi[ss];<br><br>class FreeGuitars<br>{<br>public:<br>  int minimumCosts(int n, vector &lt;string&gt; t)<br>  {<br>    vector&lt;int&gt; my;<br>    int sum;<br>    int trace,mmin;<br>    int size=t.size();<br>    int i,j,k,l;<br><br>    for(i=0;i&lt;ss;i++)<br>      for(j=0;j&lt;ss;j++)<br>        res[i][j]=st;<br>    <br>    for(i=0;i&lt;size;i++)<br>    {<br>      stringstream iss(t[i]);<br>            int  x,y,z;<br>      iss&gt;&gt;x;<br>      iss&gt;&gt;y;<br>      iss&gt;&gt;z;<br><br>      res[x][y]=z;<br>      res[y][x]=z;<br>    }<br>    <br>      my.push_back(1);<br>        sum=0;<br><br>    memset(vi,0,sizeof(vi));<br>    vi[1]=true;<br>      <br>    for(i=1;i&lt;n;i++)<br>    {<br>      mmin=ss1;<br>      for(j=1;j&lt;=n;j++)<br>        if(!vi[j])<br>          for(k=0;k&lt;my.size();k++)<br>          {<br>            if(res[my[k]][j]&lt;mmin)<br>            {<br>              mmin=res[my[k]][j];<br>              trace=j;<br>            }<br>          }<br>      if(mmin==st)<br>        return -1;<br><br>      my.push_back(trace);<br>      vi[trace]=true;<br>      <br>      sum+=mmin;<br>    }<br>    if(sum&lt;=st)<br>      return  sum;<br>    return -1;<br>  }<br>};<br></td>