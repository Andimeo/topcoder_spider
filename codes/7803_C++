            #include&lt;cstdio&gt;<br>#include&lt;iostream&gt;<br>#include&lt;sstream&gt;<br>#include&lt;cmath&gt;<br>#include&lt;algorithm&gt;<br>#include&lt;map&gt;<br>#include&lt;set&gt;<br>#include&lt;list&gt;<br>#include&lt;vector&gt;<br>#include&lt;stack&gt;<br>#include&lt;queue&gt;<br>#include&lt;string&gt;<br>using namespace std;<br>#define FOR(i,a,b) for(int i=(a);i&lt;=(b);++i)<br>#define FORD(i,a,b) for(int i=(a);i&gt;=(b);--i)<br>#define REP(i,n) FOR(i,0,(n)-1)<br>#define FS(i,v) for(__typeof((v).begin())i=(v).begin();i!=v.end();++(i))<br>#define ALL(a) (a).begin(),(a).end()<br>#define SZ(a) (int)((a).size())<br>#define MK make_pair<br>#define FI first<br>#define SE second<br>typedef long long ll;<br>typedef long double ldouble;<br>class SortInsideNumber {<br>  public:<br>  int sort(int x) {<br>    stringstream ww;<br>    ww &lt;&lt; x;<br>    string ret;<br>    ww &gt;&gt; ret;<br>    std::sort(ALL(ret));<br>    reverse(ALL(ret));<br>    stringstream wr;<br>    wr &lt;&lt; ret;<br>    int bl;<br>    wr &gt;&gt; bl;<br>    return bl;<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>