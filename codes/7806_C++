            #include &lt;algorithm&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br> <br>using namespace std;<br> <br>typedef long long Long;<br>typedef pair &lt;int, int&gt; pii;<br>typedef vector &lt;int&gt; vi;<br> <br>#define fi first<br>#define mp make_pair<br>#define pb push_back<br>#define se second<br> <br>#define eps 1e-9<br>#define pi 3.14159265358979<br> <br>class ZeroesAndOnesGrid<br>{<br>  public:<br>    int minMovesCount( vector &lt;string&gt; table )<br>    {<br>      int ans = 0;<br>      for (int i = (int)table.size() - 1; i &gt;= 0; i--)<br>        for (int j = (int)table[0].length() - 1; j &gt;= 0; j--)<br>          if (table[i][j] == '1')<br>          {<br>            ans++;<br>            for (int ii = 0; ii &lt;= i; ii++)<br>              for (int jj = 0; jj &lt;= j; jj++)<br>                table[ii][jj] ^= 1;<br>          }<br>      return ans;<br>    }<br>};<br> <br> <br>// Powered by PopsEdit<br></td>