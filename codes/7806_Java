            public class ZeroesAndOnesGrid<br>{<br>  public int minMovesCount (String[] in)<br>  {<br>    boolean[][] b = new boolean [in.length][in[0].length()];<br>    for (int i = 0; i &lt; in.length; i++)<br>      for (int j = 0; j &lt; in[0].length(); j++)<br>        b[i][j] = in[i].charAt(j) == '1';<br>    <br>    int out = 0;<br>    for (int i = b.length-1; i &gt;= 0; i--)<br>      for (int j = b[i].length-1; j &gt;= 0; j--)<br>        if (b[i][j])<br>        {<br>          out++;<br>          for (int k = 0; k &lt;= i; k++)<br>            for (int m = 0; m &lt;= j; m++)<br>              b[k][m] = !b[k][m];<br>        }<br>    return out;<br>  }<br>}<br></td>