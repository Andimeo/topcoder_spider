            import java.util.*;<br>public class WSNParentsAssignment{<br>  <br>  int n;  <br>  int[][] c;<br>  int[][] f;<br>  boolean[] gone;<br>  <br>  public boolean poisk(int v){<br>    if (v==2*n+2) return true;<br>    gone[v] = true;<br>    for(int i=0;i&lt;2*n+4;i++) if (!gone[i] &amp;&amp; f[v][i]&lt;c[v][i] &amp;&amp; poisk(i)){<br>      f[v][i]++;<br>      f[i][v]--;<br>      return true;<br>    }<br>    return false;<br>  }<br>  <br>  public boolean go(int v, int d){<br>    for(int i=0;i&lt;2*n+4;i++)<br>      Arrays.fill(f[i],0);<br>    for(int i=0;i&lt;d;i++){<br>      Arrays.fill(gone,false);<br>      if (!poisk(v)) return false;<br>    }<br>    return true;<br>  }<br>  <br>  public int[] minNetworkBurdenLevel(String[] _a, String _b){<br>    n = _a.length+1;<br>    c = new int[2*n+4][2*n+4];<br>    f = new int[2*n+4][2*n+4];<br>    gone = new boolean[2*n+4];<br>    for(int i=0;i&lt;n-1;i++)<br>      for(int j=0;j&lt;n-1;j++)<br>        if (_a[i].charAt(j)=='Y')<br>          c[2*i+1][2*j] = 1;<br>    for(int i=0;i&lt;n-1;i++)<br>      if (_b.charAt(i)=='Y')<br>        c[2*i+1][2*n-2] = 1;<br>    for(int i=0;i&lt;n;i++){<br>      c[2*n+1][2*i+1] = 1;<br>    }<br>    for(int i=0;i&lt;n;i++){<br>      for(int j=0;j&lt;n-1;j++)<br>        c[2*j][2*n+2] = i;<br>      c[2*n-2][2*n+2] = n;<br>      if (go(2*n+1,n-1)){<br>        int[] res = new int[n-1];<br>        for(int j=0;j&lt;n-1;j++){<br>          for(int k=0;k&lt;=n;k++) if (c[2*j+1][2*k]&gt;0 &amp;&amp; c[2*k][2*n+2]&gt;0){<br>            //System.out.println(i);<br>            c[2*k][2*n+2]--;<br>            c[2*n+1][2*j+1]--;<br>            if (go(2*n+1,n-j-2)){res[j] = k; break;}<br>            c[2*k][2*n+2]++;<br>            c[2*n+1][2*j+1]++;<br>          }<br>        }<br>        return res;<br>      }<br>    }<br>    return new int[]{};<br>  }<br>}<br></td>