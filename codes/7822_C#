<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>class RaceManagement<br>{<br>  public double getPayoutFactor(int[] probability, int[] amounts, int minimumMoney)<br>  {<br>    int i,j,len;<br>    double nall=0,na=0;<br>    len=probability.Length;<br>    double[] pp=new double[len];<br>    for(i=0;i&lt;len;i++)<br>    {<br>      pp[i]=probability[i]/100.0;<br>      for(j=0;j&lt;len;j++)<br>      {<br>        if(j!=i)<br>        {<br>          pp[i]=pp[i]*(100.0-probability[j])/100.0;<br>        }<br>      }<br>    }<br>    for(i=0;i&lt;len;i++)<br>    {<br>      nall+=(1.0-pp[i])*(double)amounts[i];<br>      na+=pp[i]*(double)amounts[i];<br>    }<br>    if(nall&lt;minimumMoney)<br>    {<br>      return -1.0;<br>    }<br>    if(na==0)<br>    {<br>      return -2.0;<br>    }<br>    nall=nall-minimumMoney;<br>    nall=nall/na;<br>    return nall;<br>  }<br>}<br></td>