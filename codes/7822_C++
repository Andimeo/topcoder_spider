<td class="problemText" colspan="8" valign="middle" align="left">
            #include &lt;vector&gt;<br>#include &lt;string&gt;<br>using namespace std;<br> <br>struct RaceManagement {<br> <br>double getPayoutFactor(vector &lt;int&gt; probability, vector &lt;int&gt; amounts, int minimumMoney)<br>{<br>    double exp_money_0 = 0, exp_money_1 = 0;<br>    vector&lt;double&gt; true_prob;<br>    <br>    for (int i = 0; i &lt; probability.size(); ++ i)<br>    {<br>        double p = probability[i] / 100.0;<br>        for (int j = 0; j &lt; probability.size(); ++ j)<br>        {<br>            if (i != j) p *= 1 - probability[j] / 100.0;<br>        }<br>        true_prob.push_back(p);<br>    }<br>    <br>    for (int i = 0; i &lt; probability.size(); ++ i)<br>    {<br>        exp_money_0 += amounts[i];<br>        exp_money_1 -= amounts[i] * true_prob[i];<br>    }<br>    <br>    if (exp_money_1 == 0) return exp_money_0 &gt;= minimumMoney ? -2 : -1;<br>    <br>    double s = (minimumMoney - exp_money_0) / exp_money_1 - 1;<br>    if (s &lt; 0) return -1;<br>    return s;<br>}<br> <br>};<br></td>