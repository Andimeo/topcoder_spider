            public class RaceManagement{<br>public double getPayoutFactor(int[] p,int[] a,int min)<br>{<br>  double f=0;<br>  int n=p.length;<br>  double[] win=new double[n];<br>  for(int i=0;i&lt;n;i++)<br>  {<br>    win[i]=1;<br>    for(int j=0;j&lt;n;j++)<br>    {<br>      if(i==j)<br>        win[i]*=p[j]/100.0;<br>      else<br>        win[i]*=(100-p[j])/100.0;<br>    }<br>  }<br>  double sum=0;<br>  for(int i=0;i&lt;n;i++)<br>    sum+=win[i];<br>  sum=1-sum;<br>  double s=0;<br>  for(int i=0;i&lt;n;i++)<br>    s+=a[i];<br>  double su=0;<br>  su+=sum*s;<br>  for(int i=0;i&lt;n;i++)<br>    su+=win[i]*(s-a[i]);<br>  double pr=0;<br>  su-=min;<br>  for(int i=0;i&lt;n;i++)<br>    pr+=win[i]*a[i];<br>  if(pr==0)<br>  {<br>    if(su&gt;=0)<br>      return -2;<br>    else<br>      return -1;<br>  }<br>  f=su/pr;<br>  if(f&lt;0)<br>    return -1;<br>  return f;<br>}}<br></td>