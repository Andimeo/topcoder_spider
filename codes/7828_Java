            public class BigWheels{<br><br><br>  double eps = 1e-12;<br>  public int[] enough(String[] w){<br>    int n = w.length;<br>    int[][] u = new int[n][];<br>    double[][] a = new double[n+1][111];<br>    double[][] b = new double[n+1][111];<br>    boolean[][][] z = new boolean[n+1][111][51];<br>    int[] l = new int[n];<br>    int[] best = new int[n];<br>    for(int i=0;i&lt;n;i++){<br>      String[] t = w[i].split(" ");<br>      l[i] = t.length;<br>      u[i] = new int[l[i]];<br>      for(int j=0;j&lt;l[i];j++){<br>        u[i][j] = Integer.parseInt(t[j]);<br>        if (u[i][j]&gt;best[i])<br>          best[i] = u[i][j];<br>      }<br>    }<br>    for(int i=n-1;i&gt;=0;i--){<br>      for(int j=0;j&lt;=100;j++){<br>        double r = 0;<br>        double r2 = 0;<br>        for(int k=0;k&lt;l[i];k++) {<br>          double p1 = 0;<br>          double q1 = 0;<br>          if (u[i][k]&gt;=j){<br>            p1 = 1.0/l[i]*(1-b[i+1][u[i][k]+1]);<br>            q1 = 1.0/l[i];            <br>          } else {<br>            q1 = 1.0/l[i]*b[i+1][j];<br>          }<br><br>          double p2 = 0;<br>          double q2 = 0;<br>          for(int m=0;m&lt;l[i];m++) if (u[i][k]+u[i][m]&gt;=j &amp;&amp; u[i][k]+u[i][m]&lt;=best[i]){<br>            p2+=1.0/l[i]/l[i]*(1-b[i+1][u[i][k]+u[i][m]+1]);<br>            q2 += 1.0/l[i]/l[i];<br>          } else {<br>            q2 += 1.0/l[i]/l[i]*b[i+1][j];<br>          }<br>          if (p2&gt;p1 || p1&lt;eps){<br>            r+=p2;<br>            r2+=q2;<br>          } else {<br>            z[i][j][k] = true;<br>            r+=p1;<br>            r2+=q1;<br>          }<br>        }<br>        a[i][j] = r;<br>        b[i][j] = r2;        <br>      }<br>    }<br>    boolean[] e1 = new boolean[101];<br>    for(int i=0;i&lt;=l[0];i++){<br>      if (z[0][0][i]){<br>        e1[u[0][i]] = true;<br>      }<br>    }<br>    int cc = 0;<br>    for(int i=0;i&lt;=100;i++){<br>      if (e1[i]) cc++;<br>    }<br>    int[] res = new int[cc];<br>    cc = 0;<br>    for(int i=0;i&lt;=100;i++) if (e1[i]) res[cc++] = i;<br>    return res;<br>  }<br>}<br></td>