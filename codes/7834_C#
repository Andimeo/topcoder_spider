            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br> <br>public class SharksDinner<br>{<br>  int n, nn;<br>  bool[,] caneat;<br>  int?[] f, b;<br>  bool[] bol;<br>  <br>  <br>  bool tryeat (int kam)<br>  {<br>    if (bol[kam])<br>      return false;<br>    <br>    bol[kam] = true;<br>    <br>    for (int i = 0; i &lt; n; ++i)<br>      if (caneat[kam, i] &amp;&amp; b[i] == null)<br>    {<br>      f[kam] = i;<br>      b[i] = kam;<br>      <br>      return true;<br>    }<br>    <br>    for (int i = 0; i &lt; n; ++i)<br>      if (caneat[kam, i])<br>    {<br>      int q = (int) b[i];<br>      <br>      if (bol[q])<br>        continue;<br>      <br>      b[i] = kam;<br>      <br>      if (tryeat (q))<br>      {<br>        f[kam] = i;<br>        return true;<br>      }<br>      <br>      b[i] = q;<br>    }<br>    <br>    return false;<br>  }<br>  <br>  public int minSurvivors (int[] size, int[] speed, int[] intelligence)<br>  {<br>    n = size.Length;<br>    nn = 2 * n;<br>    <br>    caneat = new bool[nn, n];<br>    <br>    for (int i = 0; i &lt; n; ++i)<br>      for (int j = 0; j &lt; n; ++j)<br>        if (i != j)<br>    {<br>      if (size[i] &gt;= size[j] &amp;&amp; speed[i] &gt;= speed[j] &amp;&amp; intelligence[i] &gt;= intelligence[j])<br>      {<br>        caneat[2 * i, j] = true;<br>        caneat[2 * i + 1, j] = true;<br>      }<br>      <br>      if (size[i] == size[j] &amp;&amp; speed[i] == speed[j] &amp;&amp; intelligence[i] == intelligence[j] &amp;&amp; i &lt; j)<br>      {<br>        caneat[2 * i, j] = false;<br>        caneat[2 * i + 1, j] = false;<br>      }<br>    }<br>    <br>    f = new int?[nn];<br>    b = new int?[n];<br>    <br>    for (int i = 0; i &lt; nn; ++i)<br>    {<br>      bol = new bool[nn];<br>      tryeat (i);<br>    }<br>    <br>    int res = 0;<br>    <br>    foreach (int? x in b)<br>      if (x == null)<br>        ++res;<br>    <br>    return res;<br>  }<br>  <br> <br>}<br> <br> <br>// Powered