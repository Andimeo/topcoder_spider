            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class EllipseCoverage<br>{<br>  public int calculateCoverage(int x1, int y1, int x2, int y2, int d)<br>  {<br>    int minx = Math.Min(x1,x2);<br>    int maxx = Math.Max(x1,x2);<br>    int miny = Math.Min(y1,y2);<br>    int maxy = Math.Max(y1,y2);<br>    int count =0;<br>    for (int i =minx - d + 1; i &lt; maxx + d; ++i){<br>      for (int j =miny - d + 1; j &lt; maxy + d; ++j){<br>        if (Math.Sqrt((i - x1) * (i - x1) + (j - y1) * (j - y1)) + Math.Sqrt((i - x2) * (i - x2) + (j - y2) * (j - y2)) &lt; d) {<br>          ++count;<br>        }<br>      }<br>    }<br>    return count;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>