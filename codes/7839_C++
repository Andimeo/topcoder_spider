            #include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>using namespace std;<br><br>double dist(double a, double b, double c, double d) {<br>  return sqrt((a-c)*(a-c)+(b-d)*(b-d));<br>}<br><br>class EllipseCoverage {<br>  public:<br>    int calculateCoverage(int x1, int y1, int x2, int y2, int d) {<br>      int ret=0;<br>      for(double i=-1000; i&lt;1000; ++i)<br>        for(double j=-1000; j&lt;1000; ++j)<br>          if(dist(i, j, x1, y1)+dist(i, j, x2, y2)+1e-10&lt;d)<br>            ++ret;<br>      return ret;<br>    }<br>    <br><br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>