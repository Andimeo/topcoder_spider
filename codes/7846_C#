            using System;<br><br>public class MarbleTop {<br>    private const int INF = 1000000;<br><br>  public int minCuts(int k, int[] stock, int[] orders) {<br>      int nk = 0;<br>      Array.Sort(orders);<br>        Array.Sort(stock);<br>      int res = 0;<br>        for (int i = orders.Length - 1; i &gt;= 0; --i)<br>        {<br>            if (orders[i] == k)<br>                ++nk;<br>            else<br>            {<br>                bool ok = false;<br>                for (int j = 0; j &lt; stock.Length; ++j)<br>                    if (stock[j] &gt;= orders[i] &amp;&amp; (stock[j] - orders[i]) % k == 0)<br>                    {<br>                        int cnt = (stock[j] - orders[i])/k;<br>                        nk -= cnt;<br>                        res += cnt;<br>                        ok = true;<br>                        stock[j] = 0;<br>                        Array.Sort(stock);<br>                        break;<br>                    }<br>                if (!ok)<br>                    return -1;<br>            }<br>        }<br>        if (nk &gt; 0)<br>        {<br>            int[] best = new int[100];<br>            for (int i = 0; i &lt; best.Length; ++i)<br>                best[i] = INF;<br>            best[0] = 0;<br>            foreach (int x in stock)<br>            {<br>                for (int i = best.Length - 1; i &gt;= 0; --i)<br>                    for (int am = 1; am &lt;= Math.Min(x / k, i); ++am)<br>                    {<br>                        int by = x == am*k ? (am - 1) : am;<br>                        best[i] = Math.Min(best[i], best[i - am] + by);<br>                    }<br>            }<br>            if (best[nk] &lt; INF)<br>                return res + best[nk];<br>            else<br>                return -1;<br>        } else return res;<br>  }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>