<td class="problemText" colspan="8" valign="middle" align="left">
            public class BestHotel<br>{<br>  public int numberOfDisadvantageous(int[] price, int[] quality)<br>  {<br>    int numDis = 0;<br>    for(int i=0;i&lt;price.Length;i++)<br>    {<br>      for(int j=0;j&lt;price.Length;j++)<br>      {<br>        if(i==j) { continue; }<br>        if(price[j] &lt;= price[i] &amp;&amp; quality[j] &gt; quality[i]) { numDis++; break; }<br>        if(price[j] &lt; price[i] &amp;&amp; quality[j] &gt;= quality[i]) { numDis++; break; }<br>      }<br>    }<br>    <br>    return numDis;<br>  }<br>}<br></td>