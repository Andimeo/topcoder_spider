            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br>#include &lt;queue&gt;<br>using namespace std;<br> <br>class BestHotel<br>{<br>  public:<br>  int numberOfDisadvantageous (vector &lt;int&gt; price, vector &lt;int&gt; quality)<br>  {<br>    int ans = 0;<br>    <br>    int size = price.size ();<br>    <br>    for (int i = 0; i &lt; size; i++)<br>    {<br>      bool bad = false;<br>      <br>      for (int j = 0; j &lt; size; j++)<br>      {<br>        if (j == i) continue;<br>        <br>        if (price [j] &lt;= price [i] &amp;&amp; quality [j] &gt;= quality [i])<br>        {<br>          if (price [j] &lt; price [i] || quality [j] &gt; quality [i])<br>            bad = true;<br>        }<br>      }<br>      <br>      if (bad)<br>        ans++;<br>    }<br>    <br>    return ans;<br>  }<br>};<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>