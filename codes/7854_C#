            using System;<br>using System.Collections.Generic;<br><br>public class BubbleSortIterations {<br>  public int numIterations(int[] X, int[] Y, int L0, int M, int n) {<br>    long[] seq = new long[n];<br>    long[] sortedSeq = new long[n];<br>    seq[0] = L0;<br>    for (int i = 1; i &lt; n; i++)<br>      seq[i] = (seq[i - 1]*X[i%X.Length] + Y[i%Y.Length])%M;<br>    Dictionary&lt;long, int&gt; originalPosition = new Dictionary&lt;long, int&gt;();<br>    for (int i = 0; i &lt; n; i++)<br>    {<br>      seq[i] = seq[i]*100000 + i;<br>      sortedSeq[i] = seq[i];<br>      originalPosition[sortedSeq[i]] = i;<br>    }<br><br>    Array.Sort(sortedSeq);<br><br>    int max = 0;<br>    for (int i = 0; i &lt; n; i++)<br>    {<br>      int x = originalPosition[sortedSeq[i]];<br>      if (x &gt; i)<br>        max = Math.Max(max, x - i);<br>    }<br>    return max + 1;<br>  }<br><br>}<br><br><br><br><br>// Powered by FileEdit<br></td>