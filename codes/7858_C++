            // Ja Sprite, Ty pragnienie<br> <br>#include &lt;algorithm&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;iostream&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;queue&gt;<br>#include &lt;set&gt;<br>#include &lt;sstream&gt;<br>#include &lt;stack&gt;<br>#include &lt;string&gt;<br>#include &lt;utility&gt;<br>#include &lt;vector&gt;<br>using namespace std;<br>#define REP(i,n) for(int i=0; i&lt;(n); ++i)<br>#define ST first<br>#define ND second<br>#define MP make_pair<br>#define ALL(a) (a).begin(),(a).end()<br>template&lt;typename Q&gt; inline int size(Q a) { return a.size(); }<br>typedef vector&lt;string&gt; VS;<br> <br>map&lt;string,pair&lt;int,string&gt; &gt; m;<br> <br>class AuctionHouse {<br>  public:<br>  vector &lt;string&gt; getList(vector &lt;string&gt; bids) {<br>    m.clear();<br>    VS v;<br>    REP(i,size(bids)) {<br>      string who,what;<br>      int c;<br>      char whoa[50],whata[50];<br>      sscanf(bids[i].c_str(),"%s%s%d",whoa,whata,&amp;c);<br>      who=whoa;<br>      what=whata;<br>      if(m[what].ND=="" || m[what].ST&lt;c) m[what]=MP(c,who);<br>      if(find(ALL(v),what)==v.end()) v.push_back(what);<br>    }<br>    VS res;<br>    REP(i,size(v)) {<br>      char cur[100];<br>      sprintf(cur,"%s got %s",m[v[i]].ND.c_str(),v[i].c_str());<br>      res.push_back(cur);<br>    }<br>    return res;<br>  }<br>};<br> <br> <br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>