            public class AuctionHouse{<br>  public String[] getList(String[] bid){<br>    String[] items = new String[50];<br>    int[] win = new int[50];<br>    String[] name = new String[50];<br>    int num = 0;<br>    <br>    String[] tmp;<br>    boolean done;<br>    <br>    for(int i = 0; i &lt; bid.length; i++){<br>      done = false;<br>      tmp = bid[i].split(" ");<br>      for(int j = 0; j &lt; num; j++){<br>        if(items[j].equals(tmp[1])){<br>          done = true;<br>          if(Integer.parseInt(tmp[2]) &gt; win[j]){<br>            win[j] = Integer.parseInt(tmp[2]);<br>            name[j] = tmp[0];<br>          }<br>          break;<br>        }<br>      }<br>      if(!done){<br>        items[num] = tmp[1];<br>        name[num] = tmp[0];<br>        win[num] = Integer.parseInt(tmp[2]);<br>        num++;<br>      }<br><br>    }<br>    <br>    String[] ret = new String[num];<br>    <br>    for(int i = 0; i &lt; num; i++){<br>      ret[i] = (name[i] + " got " + items[i]);<br>    }<br>    <br>    return ret;<br>    <br>  }<br>}<br></td>