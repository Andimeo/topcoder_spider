            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections.Generic;<br><br>public class RelabelingOfGraph {<br>    public int[] renumberVertices(string[] m) {<br>        int[] res = new int[m.Length];<br><br>        int x = m.Length - 1;<br><br>        while (x &gt;= 0) {<br>            int i = m.Length-1;<br>            for (; i &gt;= 0; i--) {<br>                if (res[i] != 0) continue;<br>                bool flag = true;<br>                for (int j = 0; j &lt; m.Length; j++) {<br>                    flag = flag &amp;&amp; (m[i][j] == '0' || res[j] &gt; 0);<br>                }<br>                if (flag) {<br>                    res[i] = x;<br>                    break;<br>                }<br>            }<br>            if (i &lt;0) return new int[0];<br>            x--;<br>        }<br><br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>