            #include &lt;string&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;cmath&gt;<br>using namespace std;<br><br>unsigned char prime[10000001];<br>class AlmostPrime<br>{<br>public:<br>  int count(string a, string b)<br>  {<br>    memset(prime, -1, sizeof(prime));<br>    prime[0] = false;<br>    prime[1] = false;<br>    for(int i = 2; i&lt;=3162; i++)<br>      if(prime[i])<br>        for(int j = i*i; j&lt;=10000000; j+=i)<br>          prime[j] = false;<br>    long long A, B;<br>    istringstream sin(a);<br>    sin &gt;&gt; A;<br>    sin.clear();<br>    sin.str(b);<br>    sin &gt;&gt; B;<br>    int count = 0;<br>    for(long long i = 2; i&lt;=10000000 &amp;&amp; i*i&lt;=B; i++)<br>      if(prime[i])<br>      {<br>        for(long long j = i*i; true;)<br>        {<br>          if(j&gt;=A)<br>            count++;<br>          if(j&lt;=B/i)<br>            j*=i;<br>          else<br>            break;<br>        }<br>          <br>      }<br>    return count;<br>  }<br>};<br></td>