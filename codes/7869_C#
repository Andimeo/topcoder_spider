            using System;<br><br>public class MirrorNumber {<br>  int[] digLeft = new int[]{0, 1, 8, 2, 5};<br>  <br>  public int count(string A, string B) {<br>    long a = long.Parse(A);<br>    long b = long.Parse(B);<br>    int res = 0;<br>    if (a == 0)<br>      ++res;<br>    int cnt = 1;<br>    for (int len = 1; len &lt;= 18; ++len)<br>    {<br>      if (len % 2 != 0)<br>        cnt *= 5;<br>      for (int cur = 0; cur &lt; cnt; ++cur)<br>        if (cur % 5 != 0)<br>        {<br>          long x = 0;<br>          long tmp = cur;<br>          for (int i = 0; i &lt; (len + 1) / 2; ++i)<br>          {<br>            x = x * 10 + digLeft[tmp % 5];<br>            tmp /= 5;<br>          }<br>          if (len % 2 != 0 &amp;&amp; (x % 10 == 2 || x % 10 == 5))<br>            continue;<br>          tmp = x;<br>          if (len % 2 != 0)<br>            tmp /= 10;<br>          for (int i = 0; i &lt; len / 2; ++i)<br>          {<br>            long dig = tmp % 10;<br>            if (dig == 2)<br>              dig = 5;<br>            else if (dig == 5)<br>              dig = 2;<br>            x = x * 10 + dig;<br>            tmp /= 10;<br>          }<br>          if (x &gt;= a &amp;&amp; x &lt;= b)<br>            ++res;<br>        }<br>    }<br>    return res;<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>