            import java.util.*;<br><br>public class MirrorNumber {<br>  public int count (String x, String y) {<br>    Scanner in = new Scanner (x);<br>    long A = in.nextLong ();<br>    in = new Scanner (y);<br>    long B = in.nextLong ();<br><br>    int R [] = new int [10];<br><br>    for (int i = 0; i &lt;= 9; i++)<br>      R[i] = i;<br>    R[2] = 5;<br>    R[5] = 2;<br>    R[3] = -1;<br>    R[4] = -1;<br>    R[6] = -1;<br>    R[7] = -1;<br><br>    int ans = 0;<br><br><br>    for (int i = 0; i &lt;= 9; i++) {<br>      int Q [] = new int [i];<br><br>      if (i &gt; 0) Q[0] = 1;<br><br>      int l;<br>      long P = 1;<br>      for (int j = 0; j &lt; i; j++) P *= 10;<br><br>      do {<br>        long c1 = 0, c2 = 0;<br>        for (int j = 0; j &lt; i; j++) c1 = c1 * 10 + Q[j];<br>        for (int j = i - 1; j &gt;= 0; j--) c2 = c2 * 10 + R[Q[j]];<br>        //System.out.println (c1 + " " + c2);<br>        long cur = c1 * P + c2;<br>        if (i &gt; 0 &amp;&amp; cur &gt;= A &amp;&amp; cur &lt;= B) ++ans;<br>        for (int k = 0; k &lt;= 8; k++)<br>          if (R[k] == k) {<br>            cur = c1 * P * 10 + k * P + c2;<br>            //System.out.println (k + " " + cur);<br>            if (cur &gt;= A &amp;&amp; cur &lt;= B) ++ans;<br>          };<br>        for (l = i - 1; l &gt;= 0; l--) {<br>          do {<br>            ++Q[l];<br>          } while (R[Q[l]] == -1);<br>          if (Q[l] == 9) Q[l] = 0; else break;<br>        };<br>      } while (l &gt;= 0);<br>    };<br><br>    return ans;<br>  };<br>};<br></td>