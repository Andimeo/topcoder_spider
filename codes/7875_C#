            using System;<br>using System.Collections;<br>public class SumOfSelectedCells {<br>  public string hypothesis(string[] table) {<br>    int[,] tab=new int[50,50];<br>    int n=table.Length;<br>    int m=0;<br>    for(int i=0;i&lt;n;i++)<br>    {<br>      string[] temp=table[i].Split();<br>      m=temp.Length;<br>      for(int j=0;j&lt;m;j++)<br>        tab[i,j]=int.Parse(temp[j]);<br>    }<br><br>    if(n&gt;m)<br>    {<br>      for(int i=0;i&lt;m;i++)<br>        for(int j=1;j&lt;n;j++)<br>          if(tab[j,i]!=tab[0,i])<br>            return "INCORRECT";<br>      return "CORRECT";<br>    }<br>    if(m&gt;n)<br>    {<br>      for(int i=0;i&lt;n;i++)<br>        for(int j=1;j&lt;m;j++)<br>          if(tab[i,j]!=tab[i,0])<br>            return "INCORRECT";<br>      return "CORRECT";<br>    }<br>    for(int i=0;i&lt;n;i++)<br>      for(int j=0;j&lt;m;j++)<br>        for(int ii=0;ii&lt;n;ii++)<br>          for(int jj=0;jj&lt;m;jj++)<br>            if(i!=ii &amp;&amp; j!=jj &amp;&amp; tab[i,j]+tab[ii,jj]!=tab[i,jj]+tab[ii,j])<br>              return "INCORRECT";<br>    return "CORRECT";<br>  }<br><br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003], modified to support C#<br>// Powered by CodeProcessor<br></td>