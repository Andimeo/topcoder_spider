            import java.util.*;<br><br>public class SumOfSelectedCells {<br>        public String hypothesis(String[] table) {<br>            int height = table.length;<br>            int width = table[0].split(" ").length;<br>            int[][] data = new int[height][width];<br>            for (int i = 0; i &lt; height; ++i) {<br>                String[] p = table[i].split(" ");<br>                for (int j = 0; j &lt; width; ++j) {<br>                    data[i][j] = Integer.parseInt(p[j]);<br>                }<br>            }<br>            boolean res = true;<br>            if (height == width) {<br>                for (int a = 0; a &lt; 1; ++a)<br>                    for (int b = 0; b &lt; height; ++b)<br>                        for (int c = 0; c &lt; 1; ++c)<br>                            for (int d = 0; d &lt; width; ++d)<br>                                if (data[a][c] + data[b][d] != data[a][d] + data[b][c])<br>                                    res = false;<br>            } else if (height &lt; width) {<br>                for (int a = 0; a &lt; 1; ++a)<br>                    for (int b = 0; b &lt; height; ++b)<br>                        for (int c = 0; c &lt; 1; ++c)<br>                            for (int d = 0; d &lt; width; ++d)<br>                                if (data[b][c] != data[b][d])<br>                                    res = false;<br>            } else {<br>                for (int a = 0; a &lt; 1; ++a)<br>                    for (int b = 0; b &lt; height; ++b)<br>                        for (int c = 0; c &lt; 1; ++c)<br>                            for (int d = 0; d &lt; width; ++d)<br>                                if (data[a][d] != data[b][d])<br>                                    res = false;<br>            }<br>            return res ? "CORRECT" : "INCORRECT";<br>        }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>