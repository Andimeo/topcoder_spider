            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class ValueAddedTax<br>{<br>  public double calculateFinalPrice(string product, int price, string[] food)<br>  {<br>    foreach(string item in food)<br>    {<br>      if (item == product)<br>        return price * 1.1;<br>    }<br>    return price * 1.18;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>