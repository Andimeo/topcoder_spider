            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class ValueAddedTax<br>{<br>  public double calculateFinalPrice(String product, int price, String[] food)<br>  {<br>    int n = -1;<br>    for(int i=0; i&lt;food.length; i++){<br>      if(product.equals(food[i])){<br>        n=i;<br>        break;<br>      }<br>    }<br>    <br>    if(n==-1){<br>      return ((double)price)*1.18;<br>    }  <br>    else<br>      return ((double)price)*1.1;<br>      <br>      <br>    <br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>