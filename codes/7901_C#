            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class AverageProblem<br>{<br>  public bool go( int[] m, int x ) {<br>    for ( int i = 0; i &lt; m.Length; i++ ) {<br>      int s = m[i] * x;<br> <br>      if ( s % 1000 == 0 ) continue;<br>      else {<br>        s += ( 1000 - ( s % 1000 ) );<br>        if ( s / x == m[i] &amp;&amp; s / x &gt;= 0 &amp;&amp; s / x &lt;= 10000 ) continue;<br>        return false;<br>      }<br>    }<br>  <br>    return true;<br>  }<br> <br>  public int numberOfParticipants(string[] marks)<br>  {<br>    ArrayList a = new ArrayList();<br>    for ( int i = 0; i &lt; marks.Length; i++ ) {<br>      string[] k = marks[i].Split(' ');<br>      foreach ( string s in k ) {<br>        string d = s.Replace( ".", "" );<br>        a.Add( int.Parse( d ) );<br>      }<br>    }<br>  <br>    int[] m = new int[a.Count];<br>    for ( int i = 0; i &lt; m.Length; i++ ) m[i] = (int) a[i];<br>    for ( int i = 1; i &lt;= 100000; i++ ) {<br>      if ( go( m, i ) ) return i;<br>    }<br>    <br>    return 100000;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>