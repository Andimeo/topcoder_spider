            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;queue&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br> <br>class SortingWithPermutation {<br>public:<br>  vector &lt;int&gt; getPermutation(vector &lt;int&gt; ques) {<br>  <br>  vector &lt;int&gt; :: iterator q;<br>  int target=0;<br>  vector &lt;int&gt; ans(ques.size());<br>  for(int j=0;j&lt;ques.size();j++)<br>  {<br>    q=min_element(ques.begin(),ques.end());<br>    *q=1111;<br>    ans[q-ques.begin()]=target;<br>    target++;<br>  }<br>    return ans;<br>  }<br>};<br> <br> <br> <br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>