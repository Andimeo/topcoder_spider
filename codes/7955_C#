<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>using System.Collections.Generic;<br>using System.Text;<br><br><br>  class NumbersGame<br>    {<br>        public int winningNumber(int[] numbers)<br>        {<br>            int max=0;<br>            List&lt;int&gt; pom=new List&lt;int&gt;();<br>            int b,j,st,i;<br>            for ( i = 0; i &lt; numbers.Length; i++)<br>            {<br>                pom.Clear();<br>                while (numbers[i]&gt;0)<br>                {<br>                    if (numbers[i] % 10 != 0)<br>                        pom.Add(numbers[i] % 10);<br>                    numbers[i] /= 10;<br>                }<br>                b = 0;<br>                pom.Sort();<br>                pom.Reverse();<br>                for ( j = 0,st=1; j &lt; pom.Count; j++,st*=10)<br>                {<br>                    b += st * pom[j];<br>                }<br>                if (b &gt; max) max = b;<br>            }<br><br>            return max;<br>        }<br>    }<br></td>