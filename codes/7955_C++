            // Ja Sprite, Ty pragnienie<br> <br>#include &lt;algorithm&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;iostream&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;queue&gt;<br>#include &lt;set&gt;<br>#include &lt;sstream&gt;<br>#include &lt;stack&gt;<br>#include &lt;string&gt;<br>#include &lt;utility&gt;<br>#include &lt;vector&gt;<br>using namespace std;<br>#define REP(i, n) for(int i=0; i&lt;(n); ++i)<br>template&lt;typename Q&gt; inline int size(Q a) {return a.size();}<br>typedef vector&lt;int&gt; VI;<br>typedef vector&lt;string&gt; VS;<br>typedef unsigned long long ULL;<br>typedef long long LL;<br>const int INF=1000000000;<br> <br>/////////////////////////////////////////////////<br> <br>class NumbersGame {<br>  public:<br>  int winningNumber(vector &lt;int&gt; numbers) {<br>    VI v;<br>    REP(i,size(numbers)) {<br>      string s;<br>      char buf[20];<br>      sprintf(buf,"%d",numbers[i]);<br>      s=buf;<br>      sort(s.begin(),s.end());<br>      int x;<br>      sscanf(s.c_str(),"%d",&amp;x);<br>      v.push_back(x);<br>    }<br>    sort(v.begin(),v.end());<br>    return v[size(v)-1];<br>  }<br>};<br> <br> <br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>