            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class NumbersGame<br>{<br>  public int winningNumber(int[] numbers)<br>  {<br>    int best = -1;<br>    for(int s : numbers)<br>    {<br>      int x = sortDig(s);<br>      if(x &gt; best)<br>        best = x;<br>    }<br>    return best;<br>  }<br>  public int sortDig(int x)<br>  {<br>    int[] dig = new int[10];<br>    String s = x+"";<br>    for(char c : s.toCharArray())<br>      dig[c-'0']++;<br>    String t = "";<br>    for(int i = 0; i &lt; 10; i++)<br>      while(dig[i] &gt; 0)<br>      {<br>        t += i+"";<br>        dig[i]--;<br>      }<br>    return Integer.parseInt(t);<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>