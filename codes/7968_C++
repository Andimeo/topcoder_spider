            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br>#include &lt;queue&gt;<br>using namespace std;<br> <br>typedef long long ll;<br> <br>class DogAndRabbit<br>{<br>  public:<br>  string willCatch (int rab1, int home1, int speed1, int accel1)<br>  {<br>    ll rab = rab1, home = home1, speed = speed1, accel = accel1;<br>    <br>    if (rab == 0)<br>      return "YES";<br>    <br>    home += rab;<br>    <br>    if (home == 0)<br>      return "NO";<br>    <br>    if (speed == 0)<br>    {<br>      if (accel != 0)<br>        return "YES";<br>      else<br>        return "NO";<br>    }<br>    <br>    if (accel == 0)<br>      return "NO";<br>    <br>    ll a = accel, b = -2 * speed, c = -2 * rab;<br>    <br>    if (b * b &lt; 4 * a * c) return "NO";<br>    <br>    if (accel * (home - rab) * (home - rab) &gt;= 2 * speed * speed * home)<br>      return "YES";<br>    else<br>      return "NO";<br>  }<br>};<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>