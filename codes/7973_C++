            #include &lt;stdio.h&gt;<br>#include &lt;string.h&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;algorithm&gt;<br><br>using namespace std;<br><br>typedef long long lint;<br>typedef vector&lt;int&gt; vi;<br>typedef pair&lt;int, int&gt; pii;<br>typedef set&lt;int&gt; si;<br><br>#define REP(i,n) for (int i = 0; i &lt; (n); i++)<br><br>bool dp[64][1024];<br><br>class ChangingSounds {<br>public:<br>  int maxFinal(vector&lt;int&gt; nums, int beginLevel, int maxLevel) {<br>    int i, j, k;<br>    memset(dp, 0, sizeof(dp));<br>    dp[0][beginLevel] = 1;<br>    for (i = 0; i &lt; nums.size(); i++) {<br>      for (j = 0; j &lt;= maxLevel; j++) {<br>        if (!dp[i][j]) continue;<br>        if (j &gt;= nums[i]) dp[i + 1][j - nums[i]] = true;<br>        if (j + nums[i] &lt;= maxLevel) dp[i + 1][j + nums[i]] = true;<br>      }<br>    }<br>    for (j = maxLevel; j &gt;= 0; j--) if (dp[i][j]) return j;<br>    return -1;<br>  }<br>};<br></td>