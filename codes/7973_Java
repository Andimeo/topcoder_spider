            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class ChangingSounds<br>{<br>  public int maxFinal(int[] cI, int beginLevel, int mL)<br>  {<br>    boolean[] can = new boolean[mL + 1];<br>    can[beginLevel] = true;<br>    for (int l : cI) {<br>      boolean[] can1 = new boolean[mL + 1];<br>      for (int i = 0; i &lt;= mL; i++) {<br>        if (can[i]) {<br>          int nL = i + l;<br>          if (nL &lt;= mL)<br>            can1[nL] = true;<br>          nL = i - l;<br>          if (nL &gt;= 0)<br>            can1[nL] = true;<br>        }<br>      }<br>      can = can1;<br>    }<br>    for (int i = mL; i &gt;= 0; i--)<br>      if (can[i])<br>        return i;<br>    return -1;<br>  }<br>  <br>  <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>