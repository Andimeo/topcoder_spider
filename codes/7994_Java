            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class CoolNumber<br>{<br>  public int count(int A, int B)<br>  {<br>    int ct = 0;<br>    A = Math.max(11, A);<br>    for(; A &lt;= B; A++)<br>      ct += isCool(A)?1:0;<br>    return ct;<br>  }<br>  int[] arr = new int[10];<br>  HashMap&lt;Integer, Boolean&gt; qwerty = new HashMap&lt;Integer, Boolean&gt;();<br>  public boolean isCool(int A)<br>  {<br>    int len = (int)(Math.log10(A+.1)+1);<br>    for(int i = 0; i &lt; 10; i++)<br>      arr[i] = 0;<br>    for(int i = 0; i &lt; len; i++)<br>    {<br>      arr[i] = A%10;<br>      if(arr[i]==0)<br>      {<br>        len--;<br>        i--;<br>      }<br>      A/=10;<br>    }<br>    Arrays.sort(arr);<br>    int wag = 0;<br>    for(int i = 0; i &lt; arr.length; i++)<br>    {<br>      wag *= 10;<br>      wag += arr[i];<br>    }<br>    if(qwerty.containsKey(wag))<br>      return qwerty.get(wag);<br>    int sum = 0;<br>    for(int x : arr)<br>      sum += x;<br>    if((sum&amp;1) == 1)<br>      return false;<br>    for(int i = 1; i &lt; (1&lt;&lt;(len-1)); i++)<br>    {<br>      int guiA = i;<br>      int setA = 0;<br>      for(int j = 0; j &lt; len-1; j++)<br>      {<br>        if((guiA&amp;1) == 1)<br>          setA += arr[9-j];<br>        guiA &gt;&gt;= 1;<br>      }<br>      if(setA == (sum&gt;&gt;1))<br>      {<br>        qwerty.put(wag, true);<br>        return true;<br>      }<br>    }<br>    qwerty.put(wag, false);<br>    return false;<br>  }<br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>