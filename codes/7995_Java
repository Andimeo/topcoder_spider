            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br><br>public class GameOfEight<br>{<br>  String target = "12345678.";<br>  class State{<br>    String now;<br>    int cnt;<br>    State(String s, int cc)<br>    {<br>      now = s;<br>      cnt = cc;<br>    }<br>  }<br>  HashSet&lt;String&gt; dict = new HashSet&lt;String&gt;();<br>  char[] ch = new char[9];<br>  char[][] tmp = new char[3][3];<br>  int[][] d = new int[][]{{-1,0},{1,0},{0,1},{0,-1}};<br>  public int fewestMoves(String[] board)<br>  {<br>    char[][] now= new char[3][];<br>    for(int i = 0; i &lt; 3; ++i)<br>      now[i] = board[i].toCharArray();<br>    LinkedList&lt;State&gt; q = new LinkedList&lt;State&gt;();<br>    String bt = board[0] + board[1] + board[2];<br>    q.add(new State(bt, 0));<br>    dict.add(bt);<br>    while(!q.isEmpty())<br>    {<br>      State s = q.remove();<br>      if(s.now.equals(target)) return s.cnt;<br>      int x = 0, y = 0;<br>      for(int i = 0; i &lt; 9; ++i)<br>      {<br>        if(s.now.charAt(i) == '.')<br>        {<br>          x = i / 3; y = i % 3;<br>          break;<br>        }<br>      }<br>      for(int i = 0; i &lt; 4; ++i)<br>      {<br>        int tx = x + d[i][0];<br>        int ty = y + d[i][1];<br>        if(tx &gt;= 0 &amp;&amp; tx &lt; 3 &amp;&amp; ty &gt;= 0 &amp;&amp; ty &lt; 3)<br>        {<br>          for(int j = 0; j &lt; 3; j++) for(int k = 0; k &lt; 3; k++) tmp[j][k] = s.now.charAt(j * 3 + k);<br>          tmp[x][y] = tmp[tx][ty]; tmp[tx][ty] = '.';<br>          for(int j = 0; j &lt; 3; j++) for(int k = 0; k &lt; 3; k++) ch[j*3+k] = tmp[j][k];<br>          String st = new String(ch);<br>          if(!dict.contains(st))<br>          {<br>            dict.add(st);<br>            q.add(new State(st, s.cnt + 1));<br>          }<br>        }<br>      }<br>    }<br>    return -1;<br>  }<br>  <br>  <br>}