            public class DecreasingNumbers {<br><br>    public long getNth(int n) {<br>        long[] C = new long[2008];<br>        int p = 1, u = 0;<br>        for (; u &lt;= 9; u++) {<br>            C[u] = u;<br>        }<br>        while (p &lt; u) {<br>            long x = C[p++];<br>            for (int c = 0; c &lt; x % 10; c++) {<br>                C[u++] = (long) x * 10 + c;<br>            }<br>        }<br>        if (n &lt;= 1022) {<br>            return C[n];<br>        }<br>        return -1;<br>    }<br><br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by moj 4.11 [modified TZTester]<br>// Powered by CodeProcessor<br></td>