            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class SquareFreeNumbers<br>{<br>  public int getCount(long min, long max)<br>  {<br>    long n = max - min + 1;<br>    bool[] b = new bool[n];<br>    int c = 0;<br>    for(long k=2; k*k&lt;=max; k++)<br>    {<br>      long v = (min -1)/(k*k) + 1;<br>      for(long j=v;j*k*k &lt;= max; j++)<br>      {<br>        b[j * k *k - min] = true;<br>      }<br>    }<br>    for(long i=0; i&lt;n; i++)<br>    {<br>      if(!b[i])<br>      {<br>        c++;<br>      }<br>    }<br>    return c;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>