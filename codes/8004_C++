            #include &lt;map&gt; <br>#include &lt;set&gt; <br>#include &lt;deque&gt; <br>#include &lt;vector&gt; <br>#include &lt;string&gt; <br>#include &lt;cstdio&gt; <br>#include &lt;cstring&gt; <br>#include &lt;sstream&gt; <br>#include &lt;iostream&gt; <br>#include &lt;algorithm&gt; <br>using namespace std; <br>template &lt;class T&gt; void out(T A[], int n) {for (int i = 0; i&lt;n; i++) cout &lt;&lt; A[i] &lt;&lt; ' '; cout &lt;&lt; endl;}  <br>template &lt;class T&gt; void out(vector&lt;T&gt; A, int n) {for (int i = 0; i&lt;n; i++) cout &lt;&lt; A[i] &lt;&lt; ' '; cout &lt;&lt; endl;}  <br><br>int a[2000000];<br><br>class SquareFreeNumbers {  <br>public:  <br>     int getCount(long long min, long long max) {  <br>        long long i, j, ans = 0;<br>        memset(a, 0, sizeof(a));<br>        for (i=2; i*i&lt;=max; i++) {<br>            j = min/i/i*i*i;<br>            if (j&lt;min) j += i*i;<br>            for (; j&lt;=max; j+=i*i) a[j-min] = 1;<br>        }<br>//        out(a, max-min+1);<br>        for (i=min; i&lt;=max; i++) if (!a[i-min]) ans++;<br>        return ans;<br>     }  <br>     <br>   <br>}; <br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>