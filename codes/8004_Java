<td class="problemText" colspan="8" valign="middle" align="left">
            import java.util.*;<br>public class SquareFreeNumbers<br>{<br>  public int getCount(long min, long max)<br>  {<br>    int n = (int)(max - min + 1);<br>    boolean[] ok = new boolean[n];<br>    Arrays.fill(ok, true);<br>    for(long i=2;i*i&lt;=max;i++)<br>    {<br>      long sq = i*i;<br>      long start = min;<br>      long rem = min%sq;<br>      if(rem!=0)<br>        start += sq-rem;<br>      //A.spr(sq+":"+start);<br>      while(start&lt;=max)<br>      {<br>        ok[(int)(start-min)] = false;<br>        start += sq;<br>      }<br>    }<br>    int res = 0;<br>    for(int i=0;i&lt;n;i++)<br>      if(ok[i])<br>        res++;<br>    return res;<br>  }<br>}<br></td>