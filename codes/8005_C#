            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class RoundOfEleven<br>{<br>  long[,,] dp = new long[12,15,505];<br>  string a;<br>  int N;<br>  public long go( int mod, int ind, int left ) {<br>    if ( dp[mod,ind,left] &gt;= 0 ) return dp[mod,ind,left];<br>    if ( mod == 0 &amp;&amp; ind == N ) return left;<br>    if ( ind == N ) return 0;<br>    <br>    long res = 0;<br>    for ( int i = 0; i &lt;= 9; i++ ) {<br>      int cost = Math.Abs( (int) ( a[ind] - '0' ) - i );<br>      <br>      if ( left &gt;= cost ) {<br>        res += go( ( mod * 10 + i ) % 11, ind + 1, left - cost );<br>      }<br>    }    <br>    <br>//    Console.WriteLine( mod + " " + ind + " " + left + " " + res );<br>    return dp[mod,ind,left] = res;<br>  }<br>    <br>  public long maxIncome(int n, int money)<br>  {<br>    for ( int i = 0; i &lt; 12; i++ ) for ( int j = 0; j &lt; 15; j++ ) for ( int k = 0; k &lt; 505; k++ ) dp[i,j,k] = -1;<br>    a = "" + n;<br>    N = a.Length;<br>    <br>    return go( 0, 0, money );<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>