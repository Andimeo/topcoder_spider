            public class RoundOfEleven {<br>  public long maxIncome(int n, int money) {<br>    String s = "" + n;<br>    long[][] num = new long[11][money + 1];<br>    num[0][0] = 1;<br>    for (int i = 0; i &lt; s.length(); ++i) {<br>      long[][] nnum = new long[11][money + 1];<br>      for (int prev = 0; prev &lt; 11; ++prev)<br>        for (int digit = 0; digit &lt; 10; ++digit) {<br>          int next = (prev * 10 + digit) % 11;<br>          int by = Math.abs(s.charAt(i) - '0' - digit);<br>          for (int m = 0; m &lt;= money - by; ++m)<br>            nnum[next][m + by] += num[prev][m];<br>        }<br>      num = nnum;<br>    }<br>    long res = 0;<br>    for (int m = 0; m &lt;= money; ++m) {<br>      res += (money - m) * num[0][m]; <br>    }<br>    return res;<br>  }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>