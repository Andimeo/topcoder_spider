            imports System<br>imports Microsoft.VisualBasic<br>imports System.Collections<br>imports System.Text<br>imports System.Collections.Generic<br> <br> <br>Public Class RoundOfEleven<br> <br>    Dim n, m As Integer<br>    Dim memo(15, 11, 500) As Long<br> <br>    Public Function maxIncome(ByVal nn As Integer, ByVal mm As Integer) As Long<br>        n = nn<br>        m = mm<br> <br>        Dim i1, i2, i3 As Integer<br>        For i1 = 0 To 15<br>            For i2 = 0 To 11<br>                For i3 = 0 To 500<br>                    memo(i1, i2, i3) = -1<br>                Next<br>            Next<br>        Next<br> <br>        Return tWin(n, 0, 0, mm)<br>    End Function<br> <br>    Private Function tWin(ByVal v As Integer, ByVal u As Integer, ByVal m11 As Integer, ByVal money As Integer) As Long<br>        Dim nv As Integer<br> <br>        If money &lt;= 0 Then Return 0<br>        If v = 0 Then<br>            If m11 = 0 Then<br>                Return money<br>            End If<br>            Return 0<br>        End If<br> <br>        If memo(u, m11, money) &lt;&gt; -1 Then Return memo(u, m11, money)<br> <br>        Dim tt As Long<br>        Dim uuu As Long<br>        uuu = 10 ^ u<br>        For nv = 0 To 9<br>            tt += tWin(v \ 10, u + 1, (m11 + (nv * uuu)) Mod 11, money - Math.Abs((v Mod 10) - nv))<br>        Next<br> <br>        memo(u, m11, money) = tt<br>        Return tt<br>    End Function<br> <br>End Class<br></td>