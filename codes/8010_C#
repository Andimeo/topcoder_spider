            using System;<br>using System.Collections.Generic;<br><br>public class WhiteHats {<br>  public int whiteNumber(int[] count) {<br>    int n = count.Length;<br>    List&lt;int&gt; c = new List&lt;int&gt;(count);<br>    c.Sort();<br>    for (int i = 0; i &lt;= n; ++i)<br>    {<br>      List&lt;int&gt; l = new List&lt;int&gt;();<br>      for (int j = 0; j &lt; i; ++j)<br>      {<br>        l.Add(i - 1);<br>      }<br>      for (int j = i; j &lt; n; ++j)<br>      {<br>        l.Add(i);<br>      }<br>      bool ok = true;<br>      for (int j = 0; j &lt; n; ++j)<br>        if (c[j] != l[j])<br>          ok = false;<br>      if (ok)<br>        return i;<br>    }<br>    return -1;<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>