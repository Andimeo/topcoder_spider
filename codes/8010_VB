            imports System<br>imports Microsoft.VisualBasic<br>imports System.Collections<br>imports System.Text<br>imports System.Collections.Generic<br> <br>Public Class WhiteHats<br> <br>    Public Function whiteNumber(ByVal count() As Integer) As Integer<br>        Dim n, i As Integer<br>        Dim bh(n - 1) As Integer<br>        Dim l, h As Integer<br>        n = count.Length<br> <br>        l = 50<br>        h = 0<br>        For i = 0 To count.Length - 1<br>            If count(i) &lt; l Then l = count(i)<br>            If count(i) &gt; h Then h = count(i)<br>        Next<br> <br>        If l = 0 And h = 0 Then Return 0<br>        If l = n - 1 And h = n - 1 Then Return n<br>        If l + 1 &lt;&gt; h Then Return -1<br> <br>        Dim hc As Integer<br>        Dim nh As Integer<br>        For i = 0 To count.Length - 1<br>            If count(i) = l Then hc += 1<br>            If count(i) = h Then nh += 1<br>        Next<br> <br>        If hc + nh &lt;&gt; n Then Return -1<br>        If h &lt;&gt; hc Then Return -1<br>        If l &lt;&gt; hc - 1 Then Return -1<br> <br>        Return hc<br> <br>    End Function<br> <br>End Class<br></td>