            using System;<br><br>public class GridColoring {<br>    public double area(int K, int rows, int cols) {<br>        double res = 0.0;<br>        long total = rows * (long) cols * (long)rows * (long)cols;<br>        for (int r = 0; r &lt; rows; ++r)<br>            for (int c = 0; c &lt; cols; ++c)<br>            {<br>                long over = 2 * r * (rows - 1 - r) + 2 * rows - 1;<br>                over *= 2 * c * (cols - 1 - c) + 2 * cols - 1;<br>                double free = (total - over) / (double)total;<br>                res += (1 - Math.Pow(free, K));<br>            }<br>        return res;<br>    }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>