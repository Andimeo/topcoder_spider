            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;queue&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cctype&gt;<br>#include &lt;string&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br> <br>class GridColoring<br>{<br>public:<br>  double area(int K, int n, int m)<br>  {<br>    double result=0;<br>    for (int x=1;x&lt;=n;x++) for (int y=1;y&lt;=m;y++)<br>    {<br>      double C[3][3];<br>      for (int i=0;i&lt;3;i++) for (int j=0;j&lt;3;j++)<br>      {<br>        int lx=(i==0)?(x-1):((i==1)?1:(n-x));<br>        int ly=(j==0)?(y-1):((j==1)?1:(m-y));<br>        C[i][j]=lx*ly;<br>      }<br>      double total=0;<br>      for (int px1=0;px1&lt;3;px1++) for (int py1=0;py1&lt;3;py1++)<br>      for (int px2=0;px2&lt;3;px2++) for (int py2=0;py2&lt;3;py2++)<br>      {<br>        int x1=min(px1,px2),x2=max(px1,px2);<br>        int y1=min(py1,py2),y2=max(py1,py2);<br>        if (x1&lt;=1 &amp;&amp; x2&gt;=1 &amp;&amp; y1&lt;=1 &amp;&amp; y2&gt;=1) total+=C[px1][py1]*C[px2][py2];<br>      }<br>      total/=n*m;<br>      total/=n*m;<br>      result+=1.0-pow(1.0-total,K);<br>    }<br>    return result;<br>  }<br>};<br> <br> <br> <br>// Powered by PopsEdit<br></td>