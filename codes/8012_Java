            import java.util.*;<br>import java.math.*;<br>import static java.lang.Math.*;<br>public class GridColoring {<br>    public static double area(int K, int rows, int cols) {<br>        double[] rowTable = table(rows);<br>        double[] colTable = table(cols);<br>        double res = 0.0;<br>        for(int i = 0; i &lt; rows; i++){<br>          for(int j = 0; j &lt; cols; j++){<br>            double p = rowTable[i] * colTable[j];<br>            double q = 1.0 - Math.pow(1.0 - p, K);<br>            res += q;<br>          }<br>        }<br>        return res;<br>    }<br>    public static double[] table(int n){<br>      double[] res = new double[n];<br>      for(int i = 0; i &lt; n; i++){<br>        res[i] = 2.0 * (i + 1) * (n - i) - 1.0;<br>        res[i] /= n * n;<br>      }<br>      return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>