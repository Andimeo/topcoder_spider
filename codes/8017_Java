            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class CastleGuards<br>{<br>  public int missing(String[] castle)<br>  {<br>    int rows = 0, cols = 0;<br>    for(int i = 0; i &lt; castle.length; i++)<br>      if(!castle[i].contains("X"))<br>        rows++;<br>    for(int i = 0; i &lt; castle[0].length(); i++)<br>    {<br>      String s = "";<br>      for(int j = 0; j &lt; castle.length; j++)<br>        s += castle[j].charAt(i)+"";<br>      if(!s.contains("X"))<br>        cols++;<br>    }<br>    return Math.max(rows, cols);<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>