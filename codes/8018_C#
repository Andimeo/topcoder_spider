            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class EvenOnes<br>{<br>  public int minOperations(string[] matrix)<br>  {<br>    int n = matrix.Length;<br>    int m = matrix[0].Length;<br>    <br>    int[] a = new int[n];<br>    int sa = 0;<br>    for (int i = 0; i &lt; n; i++) {<br>      a[i] = 0;<br>      for (int j = 0; j &lt; m; j++) a[i] ^= matrix[i][j] - '0';<br>      sa += a[i];<br>    }<br>    int[] b = new int[m];<br>    int sb = 0;<br>    for(int j = 0; j &lt; m; j++) {<br>      b[j] = 0;<br>      for(int i = 0; i &lt; n; i++) b[j] ^= matrix[i][j] - '0';<br>      sb += b[j];<br>    }<br>    <br>    int ret1 = sa + (sa % 2 == 1 ? m - sb : sb);<br>    int ret2 = n - sa + ((n - sa) % 2 == 1 ? m - sb : sb);<br>    <br>    return Math.Min(ret1, ret2);<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>