            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class EvenOnes {<br>public:<br>  int minOperations(vector &lt;string&gt;);<br>};<br> <br>int EvenOnes::minOperations(vector &lt;string&gt; mat) {<br>  int r = mat.size(), c = mat[0].size();<br>  int row[50] = {0}, col[50] = {0};<br>  for (int i = 0; i &lt; r; ++i)<br>    for (int j = 0; j &lt; c; ++j) {<br>      row[i] += mat[i][j] - '0';<br>      col[j] += mat[i][j] - '0';<br>    }<br>  const int kInf = 0x7fffffff;<br>  int ans = kInf;<br>  for (int rr = 0; rr &lt;= r; ++rr)<br>    for (int cc = 0; cc &lt;= c; ++cc) {<br>      int rrr = 0, ccc = 0;<br>      for (int i = 0; i &lt; r; ++i)<br>        if ((row[i] + cc) % 2 == 1)<br>          ++rrr;<br>      for (int i = 0; i &lt; c; ++i)<br>        if ((col[i] + rr) % 2 == 1)<br>          ++ccc;<br>      if (rr == rrr &amp;&amp; cc == ccc &amp;&amp; rr + cc &lt; ans)<br>        ans = rr + cc;<br>    }<br>  return ans == kInf ? -1 : ans;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>