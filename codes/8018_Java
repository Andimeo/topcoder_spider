            public class EvenOnes <br>{<br> <br>  public static int minOperations(String[] matrix)<br>  {<br>    int n = matrix.length;<br>    int m = matrix [0].length();<br>    <br>    int h = 0;<br>    for (int x = 0; x &lt; n; x++)<br>    {<br>      int tmp = 0;<br>      for (int y = 0; y &lt; m; y++)<br>        tmp += (matrix [x].charAt(y) - '0');<br>      if (tmp % 2 == 1)<br>        h++;<br>    }<br>    <br>    int v = 0;<br>    for (int y = 0; y &lt; m; y++)<br>    {<br>      int tmp = 0;<br>      for (int x = 0; x &lt; n; x++)<br>        tmp += (matrix [x].charAt(y) - '0');<br>      if (tmp % 2 == 1)<br>        v++;<br>    }<br>    <br>    if ((v + h) % 2 == 1)<br>      return -1;<br>    <br>    if ((v % 2 == 0) &amp;&amp; (h % 2 == 0))<br>    {<br>      int s1 = h + v;<br>      int s2 = n + m - h - v;<br>      return Math.min (s1, s2);<br>    }<br>    <br>    int s1 = n - h + v;<br>    int s2 = h + m - v;<br>    return Math.min (s1, s2);<br>  }<br>  <br>  public static void main(String[] args) <br>  {<br>    String [] s =<br>      {<br>        "10101",<br>        "01010",<br>        "10101",<br>        "01010",<br>        "10101"<br>      };<br>    System.out.println(minOperations(s));<br>  }<br> <br>}<br></td>