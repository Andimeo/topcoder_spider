            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections.Generic;<br><br>public class VectorMatching<br>{<br>    public double minimumLength(int[] x, int[] y)<br>    {<br><br>        double res = 1000000000.0;<br>        int n = x.Length;<br>        for (int i = 0; i &lt; (1 &lt;&lt; n); i++)<br>        {<br>            bool[] f = new bool[n];<br>            int x1 = 0, x2 = 0, y1 = 0, y2 = 0;<br>            int cnt = 0;<br>            for (int j = 0; j &lt; n; j++)<br>                if (((i &gt;&gt; j) &amp; 1) == 1)<br>                {<br>                    x1 += x[j];<br>                    y1 += y[j];<br>                    cnt++;<br>                }<br>                else<br>                {<br>                    x2 += x[j];<br>                    y2 += y[j];<br>                }<br>            if (cnt == n / 2)<br>            {<br>                double dx = Math.Abs(x1 - x2);<br>                double dy = Math.Abs(y1 - y2);<br>                double t = 0.0 + dx * dx + dy * dy;<br>                res = Math.Min(res, Math.Sqrt(t));<br>            }<br>        }<br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>