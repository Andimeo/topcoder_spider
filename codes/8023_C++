            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>#define For(i,a,b) for(int i=(a),lll=(b);i&lt;=lll;++i)<br>#define Rep(i,n) for(int i=0,lll=(n);i&lt;lll;++i)<br>#define Ford(i,a,b) for(int i=(a),lll=(b);i&gt;=lll;--i)<br>#define FIT(i,t) for(__typeof(t.begin()) i=t.begin();i!=t.end();++i)<br>#define ALL(a) (a).begin(),(a).end()<br>#define Size(a) ((int)((a).size()))<br> <br>#define co cout&lt;&lt;<br>#define el &lt;&lt;endl;<br>#define sp &lt;&lt;" "&lt;&lt;<br> <br> <br>class VectorMatching {<br>public:<br> <br>  vector&lt;int&gt; X, Y;<br>  int n;<br>  bool b[50];<br>  double xr, yr, rr, res;<br>  <br>  void duyet(int i) {<br>    if(i==n) {<br>      int count=0;<br>      Rep(t,n) if(b[t]) ++count;<br>      if(count != (n/2)) return;<br>      xr=0;<br>      yr=0;<br>      Rep(t,n) if(b[t]) {<br>        xr += X[t];<br>        yr += Y[t];<br>      }<br>      else {<br>        xr -= X[t];<br>        yr -= Y[t];<br>      }<br>      <br>      rr = sqrt(xr*xr + yr*yr);<br>      <br>      //co rr el<br>      <br>      if( (res&lt;0) || (res&gt;rr)) res=rr;<br>      <br>      return;<br>    }<br>    b[i]=false;<br>    duyet(i+1);<br>    b[i]=true;<br>    duyet(i+1);<br>  }<br> <br>  double minimumLength(vector &lt;int&gt; _x, vector &lt;int&gt; _y) {<br>    X=_x;<br>    Y=_y;<br>    n=X.size();<br>    res=-1;<br>    duyet(0);<br>    return res;<br>  }<br>  <br>};<br> <br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>