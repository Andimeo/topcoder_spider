            import java.util.*;<br>public class VectorMatching {<br>  public double minimumLength(int[] x, int[] y) {<br>    long res = Long.MAX_VALUE;<br>    int n = x.length;<br>    for (int i = 0; i &lt; 1 &lt;&lt; n; i++) {<br>      int k = 0;<br>      long sx = 0;<br>      long sy = 0;<br>      for (int j = 0; j &lt; n; j++)<br>        if ((i &gt;&gt; j &amp; 1) == 1) {<br>          k++;<br>          sx += x[j];<br>          sy += y[j];<br>        } else {<br>          sx -= x[j];<br>          sy -= y[j];<br>        }<br>      if (2 * k != n) continue;<br>      res = Math.min(res, sx * sx + sy * sy);<br>    }<br>    return Math.sqrt(res);<br>  }<br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>