<td class="problemText" colspan="8" valign="middle" align="left">
            #include &lt;stdio.h&gt;<br>using namespace std;<br>class TakeSubstringGame{<br>  public:<br>  int win[1000001];<br>  int powerof10[9];<br>  int winningMove(int n){<br>    int i;<br>    int winning=99999999;<br>    powerof10[0]=1;<br>    for(i=1;i&lt;=8;i++)powerof10[i]=powerof10[i-1]*10;<br>    for(i=1;i&lt;=9;i++)win[i]=0;<br>    for(i=10;i&lt;=n;i++){<br>      int digit;<br>      int d1,d2;<br>      int dec;<br>      int tmp;<br>      win[i]=0;<br>      digit=0;<br>      tmp=i;<br>      for(;;){<br>        if(tmp==0)break;<br>        tmp/=10;digit++;<br>      }<br>      for(d1=0;d1&lt;digit;d1++){<br>        for(d2=1;d2+d1&lt;=digit;d2++){<br>          dec=(i/powerof10[d1])%powerof10[d2];<br>    //      if(dec!=0)printf("move:%d-%d\n",i,dec);<br>          if(dec!=0 &amp;&amp; dec!=i &amp;&amp; win[i-dec]==0){<br>            win[i]=1;<br>            if(i==n &amp;&amp; winning&gt;dec)winning=dec;<br>          }<br>        }<br>      }<br>    }<br>    if(win[n]==0)return -1;<br>    else return winning;<br>  }<br>};<br></td>