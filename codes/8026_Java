            public class TakeSubstringGame<br>{<br>  public int winningMove(int n)<br>  {<br>    int[] X = new int[2000001];<br>    int i, j, k, t, b;<br>    int[] T = new int[8];<br>    <br>    for(i=0; i&lt;X.length; i++)<br>      X[i] = 0;<br>    <br>    for(i=1; i&lt;X.length; i++)<br>    {<br>      if(i &lt; 10)<br>      {<br>        X[i] = -1;<br>        continue;<br>      }<br>      t = i;<br>      <br>      for(b=0; t&gt;0; b++)<br>      {<br>        T[b] = t%10;<br>        t /= 10;<br>      }<br>      <br>      for(j=b-1; j&gt;=0; j--)<br>      {<br>        t = 0;<br>        for(k=j; k&gt;=0; k--)<br>        {<br>          t *= 10;<br>          t += T[k];<br>        <br>          if(t == 0)<br>            continue;<br>          if(X[i-t] == -1 &amp;&amp; (X[i] == 0 || t &lt; X[i] || X[i] == -1))<br>            X[i] = t;<br>          else if(X[i-t] &gt; 0 &amp;&amp; X[i] == 0)<br>            X[i] = -1;<br>        }<br>      }<br>    }<br>    <br>    return X[n];<br>  }<br>}<br></td>