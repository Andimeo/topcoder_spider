            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class AzimuthMonitoring {<br>public:<br>  int getAzimuth(vector &lt;string&gt;);<br>};<br> <br>int AzimuthMonitoring::getAzimuth(vector &lt;string&gt; a) {<br>  int degree=0;<br>  <br>  for(int i = 0; i &lt; a.size(); i++) {<br>    if(a[i][0]=='H') break;<br>    else if(a[i][0]=='T') degree+=180;<br>        <br>    else if(a[i].size()==4||a[i].size()==5) {<br>      if(a[i][0]=='L')<br>        degree-=90;<br>      else<br>        degree+=90;<br>    }<br>    <br>    else {<br>      istringstream in(a[i]);<br>      <br>      string s;<br>      int c;<br>      in &gt;&gt; s &gt;&gt; c;<br>      if(s[0]=='L') <br>        degree-=c;<br>      else<br>        degree+=c;<br>    }<br>  }<br>  while(degree&lt;0)<br>    degree+=360;<br>  return degree%360;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>