            import java.util.*;<br>public class AzimuthMonitoring<br>{<br>    public int getAzimuth(String[] instructions)<br>    {<br>        int res = 0;<br>        for(String s : instructions)<br>        {<br>            if(s.equals("RIGHT"))<br>                res += 90;<br>            else if(s.equals("TURN AROUND"))<br>                res += 180;<br>            else if(s.equals("HALT"))<br>                break;<br>            else if(s.equals("LEFT"))<br>                res -= 90;<br>            else if(s.contains("RIGHT"))<br>            {<br>                String [] ary = s.split(" ");<br>                res += Integer.parseInt(ary[1]);<br>            }<br>            else if(s.contains("LEFT"))<br>            {<br>                String [] ary = s.split(" ");<br>                res -= Integer.parseInt(ary[1]);<br>            }<br>        }<br>        res += 3600000;<br>        return res % 360;<br>    }<br>    <br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>