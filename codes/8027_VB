            Public Class AzimuthMonitoring<br>    Public Function getAzimuth(ByVal instructions As String()) As Integer<br>        Dim tmp As String<br>        Dim i%<br>        Dim cntr As Long = 0<br><br>        For i = instructions.GetLowerBound(0) To instructions.GetUpperBound(0)<br>            tmp = instructions(i)<br>            Dim str() As String = Split(tmp)<br>            If str.Length = 1 Or tmp = "TURN AROUND" Then<br>                Select Case tmp<br>                    Case "LEFT"<br>                        cntr = cntr + 270<br>                    Case "RIGHT"<br>                        cntr = cntr + 90<br>                    Case "TURN AROUND"<br>                        cntr = cntr + 180<br>                    Case "HALT"<br>                        Return cntr Mod 360<br>                        Exit Function<br>                End Select<br><br>            ElseIf str.Length = 2 And Not (tmp = "TURN AROUND") Then<br>                If str(0) = "LEFT" Then<br>                    cntr = cntr + (360 - CLng(str(1)))<br>                ElseIf str(0) = "RIGHT" Then<br>                    cntr = cntr + CLng(str(1))<br>                End If<br>            End If<br>        Next<br><br>        Return CInt(cntr Mod 360)<br><br>    End Function<br>     End Class<br></td>