            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>    public class PaperGame<br>    {<br>        public int mostMoves(int width, int height, int forbidden)<br>        {<br>            if (width == 1 &amp;&amp; height == 1) return 0;<br>            if (forbidden == 2) return -1;<br>            if (width == forbidden || height == forbidden) return -1;<br>            int ret = 0;<br>            while (!(width == 1 &amp;&amp; height == 1))<br>            {<br>                if (width &gt; height)<br>                {<br>                    if (width - 1 == forbidden) width = width - 2;<br>                    else width--;<br>                }<br>                else<br>                {<br>                    if (height - 1 == forbidden) height = height - 2;<br>                    else height--;<br>                }<br>                ret++;<br>            }<br>            return ret;<br>        }<br>    }<br></td>