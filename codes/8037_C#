            using System;<br>using System.Collections;<br>using System.Collections.Specialized;<br>using System.Collections.Generic;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class NamesList<br>{<br>  public string popularInitial(string[] names, int k)<br>  {<br>    int[] a = new int[26];<br>    for (int i = 0; i &lt; names.Length; i++)<br>      if (names[i].Length &gt;= k) a[names[i][k - 1] - 'A']++;<br>    int best = 0;<br>    string ans = "";<br>    for (int i = 0; i &lt; 26; i++)<br>      if (best &lt; a[i])<br>      {<br>        best = a[i];<br>        ans = ((char) ('A' + i)) + "";<br>      }<br>    return ans;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>