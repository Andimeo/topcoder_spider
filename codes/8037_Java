            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br>import static java.lang.Math.*;<br> <br> <br>public class NamesList<br>{<br>  public void sop(String s){System.out.print(s);}<br>  public void sopl(String s){System.out.println(s);}<br>  public int ipi(String s){return Integer.parseInt(s);}<br>  public int gcd(int x, int y){if (y==0) return x; return gcd(y,x%y);}<br>  int INF=2147483647;<br>  <br>  public String popularInitial(String[] n, int k)<br>  {<br>    int[] a=new int[50];<br>    for (int i=0;i&lt;n.length;i++)<br>    {<br>      String s=n[i];<br>      if (s.length()&gt;=k) a[s.charAt(k-1)-'A']++;<br>    }<br>    int max=0;<br>    char ret='A';<br>    for (int i=0;i&lt;26;i++)<br>      if (a[i]&gt;max)<br>      {<br>        max=a[i];<br>        ret=(char)('A'+i);<br>      }<br>    return ret+"";<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>