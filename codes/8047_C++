            #include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;cmath&gt;<br>#include &lt;queue&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;cfloat&gt;<br>#include &lt;climits&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>using namespace std;<br><br>class Caketown<br>{<br>private:<br>  int m;<br>public:<br>  int howManyBites(vector &lt;int&gt; c, int n)<br>  {<br>    m=c.size();<br>    sort(c.begin(),c.end());<br>    int ret=0;<br>    for(int i=m-1;i&gt;=0;i-=n+1)ret+=c[i];<br>    return ret;<br>  }<br>};<br></td>