            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br>import static java.lang.Integer.*;<br>import static java.lang.Long.*;<br>import static java.util.Collections.*;<br>import static java.util.Arrays.*;<br>import static java.lang.Math.*;<br>public class Caketown{<br>  public int howManyBites(int[] cakeSize,int numFriends){<br>    int len=cakeSize.length;<br>    int ans=0,num,flag;<br>    int[] visit=new int[len];<br>    int max,i,j,k;<br>    for(i=0;i&lt;len;i++) visit[i]=0;<br>    flag=0;<br>    for(i=0;i&lt;len;i++){<br>      for(k=0;k&lt;numFriends+1;k++){<br>        max=0;num=0;<br>        for(j=0;j&lt;len;j++){<br>          if(max&lt;cakeSize[j]&amp;&amp;visit[j]==0) {max=cakeSize[j];num=j;}<br>        }<br>        if(k==0) ans+=max;<br>        visit[num]=1;<br>        if(max==0) {flag=1;break;}<br>      }<br>      if(flag==1) break;<br>      <br>    }<br>    return ans;<br>  }<br>}<br></td>