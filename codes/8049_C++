t.str();<br>}<br>string toString(int v)<br>{<br>  ostringstream sout;<br>  sout&lt;&lt;v;<br>  return sout.str();<br>}<br> <br> <br>class SongsList <br>{<br>public:<br>  vector &lt;int&gt; getPositions(vector &lt;string&gt; songs) <br>  {<br>    int n=songs.size();<br>    string A[60][3];<br>    for (int i=0;i&lt;n;i++)<br>    {<br>      istringstream sin(songs[i]);<br>      sin&gt;&gt;A[i][0]&gt;&gt;A[i][1]&gt;&gt;A[i][2];<br>      sort(A[i]+0,A[i]+3);<br>    }<br>    set&lt;int&gt; M;<br>    int P[3];<br>    for (int i=0;i&lt;3;i++) P[i]=i;<br>    do{<br>      string s="";<br>      for (int i=0;i&lt;3;i++) s+=" "+A[0][P[i]];<br>      s=s.substr(1);<br>      int C1=0,C2=0,C3=0;<br>      for (int k=1;k&lt;n;k++)<br>      {<br>        bool f1=false,f2=false;<br>        int P2[3];<br>        for (int i=0;i&lt;3;i++) P2[i]=i;<br>        do{<br>          string s2="";<br>          for (int v=0;v&lt;3;v++) s2+=" "+A[k][P2[v]];<br>          s2=s2.substr(1);<br>          if (s2&lt;s) f1=true;<br>          if (s2&gt;s) f2=true;<br>        }while (next_permutation(P2,P2+3)!=0);<br>        if (f1 &amp;&amp; !f2) C1++;<br>        else if (!f1 &amp;&amp; f2) C3++;<br>        else C2++;<br>      }<br>      printf("%d %d %d %d %d %d\n",P[0],P[1],P[2],C1,C2,C3);<br>      for (int i=C1;i&lt;=C1+C2;i++) M.insert(i);<br>    }while (next_permutation(P,P+3)!=0);<br>    vector&lt;int&gt; R;<br>    for (set&lt;int&gt;::iterator it=M.begin();it!=M.end();++it) R.push_back(*it);<br>    return R;<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>