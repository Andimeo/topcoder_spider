            import java.io.*;<br>import java.util.*;<br><br>public class SongsList {<br>  public int[] getPositions(String[] songs) {<br>    int[][] t = new int[][]{<br>      new int[]{0, 1, 2},<br>      new int[]{0, 2, 1},<br>      new int[]{1, 0, 2},<br>      new int[]{1, 2, 0},<br>      new int[]{2, 1, 0},<br>      new int[]{2, 0, 1},<br>    };<br>    int n = songs.length;<br>    String[][] s = new String[n][];<br>    for (int i = 0; i &lt; n; i++) {<br>      s[i] = songs[i].split(" ");<br>    }<br>    TreeSet&lt;Integer&gt; ans = new TreeSet&lt;Integer&gt;();<br>    for (int ms = 0; ms &lt; 6; ms++) {<br>      String main = s[0][t[ms][0]] + " " + s[0][t[ms][1]] + " " + s[0][t[ms][2]];<br>      int fir = 0;<br>      int sec = 1;<br>      for (int i = 1; i &lt; n; i++) {<br>        boolean pb = false;<br>        boolean pa = false;<br>        for (int sh = 0; sh &lt; 6; sh++) {<br>          String song = s[i][t[sh][0]] + " " + s[i][t[sh][1]] + " " + s[i][t[sh][2]];<br>          if (song.compareTo(main) &lt; 0) {<br>            pb = true;<br>          } else {<br>            pa = true;<br>          }<br>        }<br>        if (!pa)<br>          fir++;<br>        if (pb)<br>          sec++;<br>      }<br>      for (int i = fir; i &lt; sec; i++)<br>        ans.add(i);<br>    }<br>    int[] a = new int[ans.size()];<br>    int m = 0;<br>    for (int x : ans)<br>      a[m++] = x;<br>    return a;<br>  }<br>}<br></td>