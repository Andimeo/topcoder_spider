            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class MirroredClock<br>{<br>  public String whatTimeIsIt(String t)<br>  {<br>    String time[]=t.split(":");<br>    int minute=0,hour=0;<br>    <br>    minute=60-Integer.parseInt(time[1]);<br>    hour=11-Integer.parseInt(time[0]);<br>    <br>    if(minute==60)<br>    {<br>       minute=0;;<br>       hour=hour+1;<br>       hour=hour%12;<br>    }<br>    <br>    String m="",h="";<br>    h+=hour;<br>    m+=minute;<br>    <br>    if(h.length()==1)<br>    h="0"+h;<br>    if(m.length()==1)<br>    m="0"+m;<br>    <br>    return h+":"+m;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>