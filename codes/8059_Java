            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br>import static java.lang.Math.*;<br> <br> <br>public class VegetableField<br>{<br>  public void sop(String s){System.out.print(s);}<br>  public void sopl(String s){System.out.println(s);}<br>  public int ipi(String s){return Integer.parseInt(s);}<br>  public int gcd(int x, int y){if (y==0) return x; return gcd(y,x%y);}<br>  int INF=2147483647;<br>  <br>  boolean[][] b;<br>  public int biggestPlot(String[] f)<br>  {<br>    int n=f.length, m=f[0].length();<br>    char[][] a=new char[n][m];<br>    for (int i=0;i&lt;n;i++)<br>      a[i]=f[i].toCharArray();<br>    b=new boolean[n][m];<br>    int ret=0;<br>    for (char x='A';x&lt;='Z';x++)<br>      for (char y='A';y&lt;='Z';y++)<br>      {<br>        for (int i=0;i&lt;n;i++)<br>          for (int j=0;j&lt;m;j++)<br>            if (a[i][j]==x || a[i][j]==y) b[i][j]=true;<br>            else b[i][j]=false;<br>        for (int i=0;i&lt;n;i++)<br>          for (int j=0;j&lt;m;j++)<br>            if (b[i][j])<br>            {<br>              int bx=i, by=j;<br>              while (bx&lt;n &amp;&amp; b[bx][j]) bx++;<br>              while (by&lt;m &amp;&amp; b[i][by]) by++;<br>              for (int h=i;h&lt;=bx;h++)<br>                for (int k=j;k&lt;=by;k++)<br>                {<br>                  boolean kt=true;<br>                  for (int ii=i;ii&lt;h;ii++)<br>                  {<br>                    for (int jj=j;jj&lt;k;jj++)<br>                      if (!b[ii][jj])<br>                      {<br>                        kt=false;<br>                        break;<br>                      }<br>                    if (!kt) break;<br>                  }<br>                  if (kt) ret=Math.max(ret,(h-i)*(k-j));<br>                }<br>            }<br>      }<br>    return ret;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>