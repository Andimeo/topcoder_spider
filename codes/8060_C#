            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class Paintball <br>{<br>  class Player : IComparable&lt;Player&gt;<br>  {<br>    public int score;<br>    public string name;<br>    public Team team;<br>    public int CompareTo(Player rhs)<br>    {<br>      int res = -score + rhs.score;<br>      if (res != 0) return res;<br>      return name.CompareTo(rhs.name);<br>    }<br>  }<br>  class Team : IComparable&lt;Team&gt;<br>  {<br>    public string name;<br>    public List&lt;Player&gt; players = new List&lt;Player&gt;();<br><br>    public int CompareTo(Team rhs)<br>    {<br>      int res = -Score() + rhs.Score();<br>      if (res != 0) return res;<br>      return name.CompareTo(rhs.name);<br>    }<br><br>    public int Score()<br>    {<br>      int score = 0;<br>      foreach (Player player in players) score += player.score;<br>      return score;<br>    }<br><br>  }<br>  <br><br>  public string[] getLeaderboard(string[] players, string[] messages) <br>  {<br>    Dictionary&lt;string, Player&gt; playerLut = new Dictionary&lt;string, Player&gt;();<br>    List&lt;Team&gt; teams = new List&lt;Team&gt;();<br>    foreach (string p in players)<br>    {<br>      string[] s = p.Split(' ');<br>      string name = s[0];<br>      string team = s[1];<br>      Team theTeam = null;<br>      foreach (Team t in teams)<br>      {<br>        if (t.name == team)<br>        {<br>          theTeam = t;<br>        }<br>      }<br>      if (theTeam == null)<br>      {<br>        theTeam = new Team();<br>        theTeam.name = team;<br>        teams.Add(theTeam);<br>      }<br>      Player player = new Player();<br>      player.name = name;<br>      player.team = theTeam;<br>      playerLut[name] = player;<br>      theTeam.players.Add(player);<br>    }<br>    foreach (string message in messages)<br>    {<br>      string[] m = message.Split(' ');<br>      Player source = playerLut[m[0]];<br>      Player dest = playerLut[m[2]];<br>      if (source.team == dest.team)<br>      {<br>        source.score--;<br>      }<br>      else<br>      {<br>        source.score++;<br>        dest.score--;<br>      }<br>    }<br><br>    teams.Sort();<br>    List&lt;string&gt; ret = new List&lt;string&gt;();<br>    foreach (Team team in teams)<br>    {<br>      string r = team.name + " " + team.Score();<br>      ret.Add(r);<br>      team.players.Sort();<br>      foreach (Player player in team.players)<br>      {<br>        r = "  " + player.name + " " + player.score;<br>        ret.Add(r);<br>      }<br>    }<br><br>    return ret.ToArray();<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>