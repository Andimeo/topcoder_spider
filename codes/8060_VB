            imports System<br>imports Microsoft.VisualBasic<br>imports System.Collections<br>imports System.Text<br>imports System.Collections.Generic<br> <br>Public Class Paintball<br> <br>    Dim np, nt As Integer<br>    Dim teams As New List(Of String)<br>    Dim plays As New List(Of String)<br>    Dim pTeam() As Integer<br>    Dim pScore() As Integer<br>    Dim tScore(200) As Integer<br> <br>    Dim rr As New List(Of String)<br> <br>    Public Function getLeaderboard(ByVal players() As String, ByVal messages() As String) As String()<br>        np = players.Length<br>        ReDim pTeam(np - 1)<br>        ReDim pScore(np - 1)<br> <br>        Dim i As Integer<br>        Dim sp() As String<br>        For i = 0 To players.Length - 1<br>            sp = Split(players(i), " ")<br>            If Not teams.Contains(sp(1)) Then teams.Add(sp(1))<br>            If Not plays.Contains(sp(0)) Then plays.Add(sp(0))<br>            pTeam(plays.IndexOf(sp(0))) = teams.IndexOf(sp(1))<br>        Next<br>        nt = teams.Count<br> <br>        For i = 0 To messages.Length - 1<br>            sp = Split(messages(i), " ")<br>            Dim t1, t2 As Integer<br>            Dim p1, p2 As Integer<br>            p1 = plays.IndexOf(sp(0))<br>            p2 = plays.IndexOf(sp(2))<br> <br>            t1 = pTeam(p1)<br>            t2 = pTeam(p2)<br> <br>            If t1 = t2 Then<br>                pScore(p1) -= 1<br>                tScore(t1) -= 1<br>            Else<br>                pScore(p1) += 1<br>                tScore(t1) += 1<br>                pScore(p2) -= 1<br>                tScore(t2) -= 1<br>            End If<br>        Next<br> <br>        Dim sortTnum(nt - 1) As Integer<br>        Dim sortTscore(nt - 1) As Integer<br>        Dim sortTname(nt - 1) As String<br>        For i = 0 To nt - 1<br>            sortTnum(i) = i<br>            sortTscore(i) = tScore(i)<br>            sortTname(i) = teams(i)<br>        Next<br> <br>        Dim x1, x2 As Integer<br>        For x1 = 0 To nt - 1<br>            For x2 = x1 + 1 To nt - 1<br>                If sortTscore(x2) &gt; sortTscore(x1) OrElse (sortTscore(x2) = sortTscore(x1) And sortTname(x2) &lt; sortTname(x1)) Then<br>                    swap(sortTscore(x1), sortTscore(x2))<br>                    swap(sortTname(x1), sortTname(x2))<br>                    swap(sortTnum(x1), sortTnum(x2))<br>                End If<br>            Next<br>        Next<br> <br>        Dim p, ct As Integer<br>        For i = 0 To nt - 1<br>            ct = sortTnum(i)<br> <br>            rr.Add(teams(ct) &amp; " " &amp; tScore(ct))<br> <br>            Dim bGo As Boolean<br>            Dim best As Integer<br>            bGo = True<br> <br>            While bGo<br>                bGo = False<br>                best = -1<br>                For p = 0 To np - 1<br>                    If pTeam(p) = ct Then<br>                        If best = -1 OrElse (pScore(best) &lt; pScore(p) Or (pScore(best) = pScore(p) And plays(p) &lt; plays(best))) Then<br>                            best = p<br>                        End If<br>                    End If<br>                Next<br>                If best &lt;&gt; -1 Then<br>                    bGo = True<br>                    rr.Add("  " &amp; plays(best) &amp; " " &amp; pScore(best))<br>                    pTeam(best) = -1<br>                End If<br>            End While<br>        Next<br> <br>        Dim ret(rr.Count - 1) As String<br>        ret = rr.ToArray()<br>        Return ret<br>    End Function<br> <br>    Private Sub Swap(ByRef v1 As Object, ByRef v2 As Object)<br>        Dim t As Object<br>        t = v1<br>        v1 = v2<br>        v2 = t<br>    End Sub<br> <br>End Class<br></td>