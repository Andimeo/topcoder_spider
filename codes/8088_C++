            #include &lt;string&gt;<br>#include &lt;vector&gt;<br><br>using namespace std;<br><br>class PaperUnfolding {<br>  public:<br>  <br>  string check (string s) {<br>    if (s.size() == 0)<br>      return "YES";<br>    string b = "";<br>    for (int i = 0; i &lt; s.size() / 2; i ++) {<br>      if (s[i] == s[s.size() - i - 1])<br>        return "NO";<br>      b += s[i];<br>      }<br>        <br>    return check(b);<br>    }<br>  <br>  string isValidUnfolding(vector &lt;string&gt; c) {<br>    string f = "";<br>    for (int i = 0; i &lt; c.size(); i ++)<br>      f += c[i];<br>    return check(f);<br>    }<br>  };<br></td>