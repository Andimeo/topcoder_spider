            //Written by Sergey Melnikov<br>import java.util.*;<br>import java.math.*;<br>public class PaperUnfolding {<br>    public String isValidUnfolding(String[] code) {<br>      String s = "";<br>      for (int i = 0; i &lt; code.length; i++) {<br>      s += code[i];<br>    }<br>//      System.err.println(s);<br>      char[] c = s.toCharArray();<br>      int j = (c.length)/2;<br>      for (; j &gt; 0 ; j/=2) {<br>//        System.err.println(j);<br>        for (int i = 1; i &lt;= j; i++) {<br>        if (c[j-i]==c[j+i]) {<br>//          System.err.println(i+" == "+(j+i+1));<br>          return "NO";<br>        }<br>      }<br>      <br>    }<br>        return "YES";<br>    }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>