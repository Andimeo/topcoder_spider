<td class="problemText" colspan="8" valign="middle" align="left">
            Public Class PaperUnfolding<br>        Dim s As String<br>        Function isval(ByVal len As Integer) As Boolean<br>            If len = 0 Then Return True<br>            If len Mod 2 = 0 Then Return False<br>            If Not isval(len \ 2) Then Return False<br>            For i As Integer = 0 To len \ 2 - 1<br>                If s.Chars(i) = "0" Then<br>                    If s.Chars(len - i - 1) &lt;&gt; "1" Then Return False<br>                Else<br>                    If s.Chars(len - i - 1) &lt;&gt; "0" Then Return False<br>                End If<br>            Next<br>            Return True<br>        End Function<br>        Public Function isValidUnfolding(ByVal code As String()) As String<br>            s = ""<br>            For Each ts As String In code<br>                s = s &amp; ts<br>            Next<br>            If isval(s.Length) Then Return "YES" Else Return "NO"<br>        End Function<br>    End Class<br></td>