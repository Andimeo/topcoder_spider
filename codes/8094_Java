            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class JoshString<br>{<br>  public String isJoshString(String s)<br>  {<br>    int num = 0;<br>    for(char c : s.toCharArray())<br>      num += c - 'a' + 1;<br>    boolean f = true;<br>    for(int i = 2; i &lt; num; i++)<br>      if(num % i == 0)<br>        f = false;<br>    if(f)<br>      return "YES";<br>    else<br>      return "NO";<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>