            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class SquareConstruction<br>{<br>  public string[] construct(int N, int a, int b, int c, int d)<br>  {<br>    int[,] m = new int[N,N];<br>    <br>    int x = 0, y = 0;<br>    <br>    for ( int i = 0; i &lt; N; i++ ) for ( int j = 0; j &lt; N; j++ ) m[i,j] = -1;<br>    <br>    bool done = false;<br>    int count = 1;<br>    while ( !done ) {<br>      if ( m[x,y] == -1 ) {<br>        m[x,y] = count++;<br>        continue;<br>      }<br>      x += a;<br>      y += b;<br>      x %= N;<br>      y %= N;<br>      if ( m[x,y] == -1 ) {<br>        m[x,y] = count++;<br>        continue;<br>      }<br>      <br>      x += c;<br>      y += d;<br>      x %= N;<br>      y %= N;<br>      <br>      if ( m[x,y] == -1 ) {<br>        m[x,y] = count++;<br>        continue;<br>      }<br>      <br>      done = true;<br>    }<br>    <br>    string[] r = new string[N];<br>    <br>    for ( int i = 0; i &lt; r.Length; i++ ) {<br>      r[i] = "";<br>      for ( int j = 0; j &lt; N; j++ ) {<br>        r[i] += " " + m[i,j];<br>      }<br>      r[i] = r[i].Trim();<br>    }<br>    return r;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>