            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class SquareConstruction<br>{<br>  public String[] construct(int N, int a, int b, int c, int d)<br>  {<br>    int i,j,x,y;<br>    int v[][]=new int[N][N];<br>    x=0; y=0;<br>    for (i=0;i&lt;N;i++) for (j=0;j&lt;N;j++)<br>      v[i][j]=-1;<br>    v[0][0]=1;<br>    for (i=2,x=0,y=0;;i++){<br>      x=(x+a)%N; y=(y+b)%N;<br>      if (v[x][y]==-1) v[x][y]=i;<br>      else{<br>        x=(x+c)%N; y=(y+d)%N;<br>        if (v[x][y]==-1) v[x][y]=i;<br>        else break;<br>      }<br>    }<br>    String Res[]=new String[N];<br>    for (i=0;i&lt;N;i++){<br>      Res[i]=""+v[i][0];<br>      for (j=1;j&lt;N;j++) Res[i]=Res[i]+" "+v[i][j];<br>    }<br>    return Res;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>