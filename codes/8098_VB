            Class SquareConstruction<br>    Public Function construct(ByVal N As Integer, ByVal a As Integer, ByVal b As Integer, ByVal c As Integer, ByVal d As Integer) As String()<br>        Dim cur As Integer = 1<br>        Dim col As Integer = 0, row As Integer = 0<br>        Dim board(N - 1, N - 1) As Integer<br>        For i As Integer = 0 To N - 1<br>            For j As Integer = 0 To N - 1<br>                board(i, j) = -1<br>            Next<br>        Next<br><br>        While True<br>            If board(col, row) &lt;&gt; -1 Then Exit While<br>            board(col, row) = cur<br>            cur += 1<br>            row = (row + a) Mod N<br>            col = (col + b) Mod N<br>            If board(col, row) = -1 Then Continue While<br>            row = (row + c) Mod N<br>            col = (col + d) Mod N<br><br>        End While<br>        Dim ret(N - 1) As String<br>        For row = 0 To N - 1<br>            ret(row) = ""<br>            For col = 0 To N - 1<br>                ret(row) += " " + board(col, row).ToString<br>            Next<br>            ret(row) = ret(row).Trim(" ")<br>        Next<br>        Return ret<br>    End Function<br>End Class<br></td>