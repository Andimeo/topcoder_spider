            using System;<br>using System.Collections.Generic;<br>using System.Text;<br> <br>public class OptimalGroupMovement<br>{<br>    public static int minimumCost(string board)<br>    {<br>        int N = int.MaxValue;<br>        List&lt;int&gt; D = new List&lt;int&gt;();<br>        bool X = false; int Last = 0;<br>        for (int i = 0; i &lt; board.Length; i++)<br>            if (board[i] == '.')<br>            {<br>                if (X)<br>                {<br>                    D.Add(Last);<br>                    X = false;<br>                    Last = 0;<br>                    D.Add(0);<br>                }<br>                else<br>                {<br>                    D.Add(0);<br>                }<br>            }<br>            else<br>            {<br>                if (X)<br>                {<br>                    Last++;<br>                }<br>                else<br>                {<br>                    X = true;<br>                    Last++;<br>                }<br>            }<br>        if (X) D.Add(Last);<br>        for (int i = 0; i &lt; D.Count; i++)<br>            if (D[i] != 0)<br>            {<br>                int S = 0;<br>                for (int j = 0; j &lt; D.Count; j++)<br>                    if (D[j] != 0 &amp;&amp; i != j)<br>                    {<br>                        int ZN = 0;<br>                        for (int k = Math.Min(i, j); k &lt;= Math.Max(i, j); k++)<br>                            if (D[k] == 0) ZN++;<br>                        S += D[j] * D[j] * ZN;<br>                    }<br>                if (S &lt; N) N = S;<br>            }<br>        return N;<br>    }<br>}<br></td>