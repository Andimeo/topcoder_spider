            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class Pawns<br>{<br>  public int pawnsAttack(String[] pawns)<br>  {<br>    boolean[][] hp = new boolean[8][8];<br>    for(int i = 0; i &lt; pawns.length; i++)<br>      hp[pawns[i].charAt(0)-'a'][pawns[i].charAt(1)-'1'] = true;<br>    int ct = 0;<br>    for(int i = 0; i &lt; 8; i++)<br>      for(int j = 1; j &lt; 8; j++)<br>        if(!hp[i][j])<br>          if((i&gt;0&amp;&amp;hp[i-1][j-1])||(i&lt;7&amp;&amp;hp[i+1][j-1]))<br>            ct++;<br>    return ct;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>