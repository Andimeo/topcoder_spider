            Public Class Pawns<br><br><br>        Public Function pawnsAttack(ByVal pawns As String()) As Integer<br>            Dim l As Integer, d As Integer<br>            Dim hui(9, 9) As Integer<br>            For Each p As String In pawns<br>                l = Asc(p.Chars(0)) - Asc("a") + 1<br>                d = Asc(p.Chars(1)) - Asc("1") + 1<br>                hui(l - 1, d + 1) = 2<br>                hui(l + 1, d + 1) = 2<br><br>            Next<br>            For Each p As String In pawns<br>                l = Asc(p.Chars(0)) - Asc("a") + 1<br>                d = Asc(p.Chars(1)) - Asc("1") + 1<br>                hui(l, d) = 1<br>            Next<br>            Dim an As Integer = 0<br>            For i As Integer = 1 To 8<br>                For j As Integer = 1 To 8<br>                    If hui(i, j) = 2 Then an += 1<br>                Next<br>            Next<br>            Return an<br>        End Function<br><br>    End Class<br></td>