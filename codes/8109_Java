            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class KingsTour<br>{<br>  int[][] mob = new int[8][8];<br>  public int attackPawns(String k, String a, String b)<br>  {<br>    int kx = k.charAt(0)-'a';<br>    int ky = k.charAt(1)-'1';<br>    int ax = a.charAt(0)-'a';<br>    int ay = a.charAt(1)-'1';<br>    int bx = b.charAt(0)-'a';<br>    int by = b.charAt(1)-'1';<br>    for(int i = 0; i &lt; 8; i++)<br>      for(int j = 0; j &lt; 8; j++)<br>        mob[i][j] = 1000000;<br>    try{mob[ax+1][ay+1] = -1;}catch(Throwable bb){}<br>    try{mob[ax-1][ay+1] = -1;}catch(Throwable bb){}<br>    try{mob[bx+1][by+1] = -1;}catch(Throwable bb){}<br>    try{mob[bx-1][by+1] = -1;}catch(Throwable bb){}<br>    int soln = 1000000;<br>    if(mob[ax][ay] != -1)<br>    {<br>      dfs(kx,ky,0);<br>      soln = mob[ax][ay];<br>    }<br>    <br>    for(int i = 0; i &lt; 8; i++)<br>      for(int j = 0; j &lt; 8; j++)<br>        mob[i][j] = 1000000;<br>    try{mob[ax+1][ay+1] = -1;}catch(Throwable bb){}<br>    try{mob[ax-1][ay+1] = -1;}catch(Throwable bb){}<br>    try{mob[bx+1][by+1] = -1;}catch(Throwable bb){}<br>    try{mob[bx-1][by+1] = -1;}catch(Throwable bb){}<br>    if(mob[bx][by] != -1)<br>    {<br>      int temp = 0;<br>      dfs(kx,ky,0);<br>      temp += mob[bx][by];<br>      for(int i = 0; i &lt; 8; i++)<br>        for(int j = 0; j &lt; 8; j++)<br>          mob[i][j] = 1000000;<br>      dfs(bx,by,0);<br>      soln = Math.min(soln, temp+mob[ax][ay]);<br>    }<br>    return soln;<br>  }<br>  void dfs(int x, int y, int d)<br>  {<br>    if(x&lt;0||x&gt;7||y&lt;0||y&gt;7||mob[x][y]&lt;=d)return;<br>    mob[x][y] = d;<br>    dfs(x-1,y-1,d+1);<br>    dfs(x,y-1,d+1);<br>    dfs(x+1,y-1,d+1);<br>    dfs(x-1,y,d+1);<br>    dfs(x+1,y,d+1);<br>    dfs(x-1,y+1,d+1);<br>    dfs(x,y+1,d+1);<br>    dfs(x+1,y+1,d+1);<br>  }<br>  <br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>