            #include&lt;iostream&gt;<br>#include&lt;vector&gt;<br>#include&lt;string&gt;<br><br>using namespace std;<br><br>class LetterInterchange<br>{<br>      public:<br>            vector &lt;int&gt; interchangeWhich(vector &lt;string&gt; s1, vector &lt;string&gt; s2)<br>            {<br>                   vector &lt;int&gt; ans;<br>                   int n = s1.size(), m = s2.size();<br>                   string s;<br>                   for(int i=0;i&lt;n;i++) s += s1[i];<br>                   for(int i=0;i&lt;m;i++) s += s2[i];<br>                   int len = s.size();<br>                   for(int i=0;i&lt;len;i++)<br>                   {<br>                      int k = i;<br>                      for(int j = len-1;j&gt;i;j--)<br>                        if(s[j] &lt; s[k]) k=j;<br>                      if(k != i)<br>                      {<br>                           ans.push_back(i);<br>                           ans.push_back(k);<br>                           return ans;<br>                      }<br>                   }<br>                   for(int i=0;i&lt;len-1;i++)<br>                   {<br>                       for(int j=i+1;j&lt;len;j++)<br>                         if(s[i] == s[j])<br>                         {<br>                           ans.push_back(i);<br>                           ans.push_back(j);<br>                           return ans; <br>                         }<br>                   }<br>                   ans.push_back(len-2);<br>                   ans.push_back(len-1);<br>                   return ans;<br>            }<br>};<br></td>