            import java.util.*;<br>import java.math.*;<br>import java.io.*;<br>import java.text.*;<br>import java.util.regex.*;<br>import static java.util.Arrays.*;<br>import static java.lang.Math.*;<br>import static java.lang.Integer.parseInt;<br>public class LetterInterchange {<br>  public int[] interchangeWhich(String[] s1, String[] s2) {<br>    String s = "";<br>    for(int i = 0; i &lt; s1.length; i++){<br>      s += s1[i];<br>    }<br>    for(int i = 0; i &lt; s2.length; i++){<br>      s += s2[i];<br>    }<br>    boolean asc = true;<br>    for(int i = 1; i &lt; s.length(); i++){<br>      if(s.charAt(i) &lt; s.charAt(i-1)) asc = false;<br>    }<br>    if(asc){<br>      for(int i = 1; i &lt; s.length(); i++){<br>        if(s.charAt(i) == s.charAt(i-1)) return new int[] {i-1,i};<br>      }<br>      return new int[] {s.length()-2, s.length()-1};<br>    }<br>    int rem = 0;<br>    for(char c = 'a';;c++){<br>      int to = 0;<br>      for(to = 0; to &lt; s.length(); to++){<br>        if(s.charAt(to) != c) break;<br>      }<br>      s = s.substring(to);<br>      rem += to;<br>      int ind = s.lastIndexOf(c);<br>      if(ind == -1) continue;<br>      return new int[] {rem, rem+ind};<br>    }<br>  }<br> <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>