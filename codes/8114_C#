            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class ComplementaryDNAChains {<br>  public int minReplaces(string first, string second) {<br>        int a = 0;<br>        for (int i = 0; i &lt; first.Length; i++)<br>        {<br>            if (first[i] == 'A' &amp;&amp; second[i] != 'T')<br>                a++;<br>            if (first[i] == 'T' &amp;&amp; second[i] != 'A')<br>                a++;<br>            if (first[i] == 'C' &amp;&amp; second[i] != 'G')<br>                a++;<br>            if (first[i] == 'G' &amp;&amp; second[i] != 'C')<br>                a++;<br>        }<br><br>        return a;<br>    <br>  }<br><br>  <br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>