            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class ComplementaryDNAChains<br>{<br>  public int minReplaces(String f, String s)<br>  {<br>    int total=0;<br>    int n=f.length();<br>    int i=0;<br>    while(i&lt;n)<br>    {<br>      <br>      if(f.charAt(i)=='T' &amp;&amp; s.charAt(i)=='A' ||   f.charAt(i)=='A' &amp;&amp; s.charAt(i)=='T' )<br>      { i++; continue; }<br>      <br>      if(f.charAt(i)=='G' &amp;&amp; s.charAt(i)=='C' ||   f.charAt(i)=='C' &amp;&amp; s.charAt(i)=='G' )<br>      { i++; continue; }<br>      <br>      total++;<br>      i++;<br>    }<br>    return total;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>