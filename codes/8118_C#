            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class MovieRating<br>{<br>  public double calculate(int[] marks, int lowCount, int highCount)<br>  {<br>    Array.Sort(marks);<br>    int sum = 0;<br>    for (int i = lowCount; i &lt; marks.Length - highCount; i++)<br>      sum += marks[i];<br>    return sum / (double)(marks.Length - lowCount - highCount);<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>