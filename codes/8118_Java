            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class MovieRating<br>{<br>  public double calculate(int[] marks, int lowCount, int highCount)<br>  {<br>    double sum = 0;<br>    double tot = 0;<br>    <br>    Arrays.sort(marks);<br>    <br>    for(int i=lowCount; i&lt;marks.length-highCount; i++){<br>      sum+=marks[i]*1.0;<br>      tot++;<br>    }<br>    <br>    return sum/tot;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>