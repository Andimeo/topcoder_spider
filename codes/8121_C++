            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>using namespace std;<br>class WebBrowser {<br>  public:<br>    vector &lt;string&gt; getSequence(vector &lt;string&gt; action) {<br>      string st[55];<br>      int t;<br>      int i, j;<br>      vector &lt;string&gt; ret;<br>      t = j = 0;<br>      for (i = 0; i &lt; action.size(); i ++)<br>        if (action[i] == "BACK") {<br>          if (j &gt; 1) {<br>            j --;<br>            ret.push_back(st[j]);<br>          }<br>        }<br>        else<br>          if (action[i] == "FORWARD") {<br>            if (j &lt; t) {<br>              j ++;<br>              ret.push_back(st[j]);<br>            }<br>          }<br>          else {<br>            if (st[j] == action[i])<br>              continue;<br>            ret.push_back(action[i]);<br>            st[++ j] = action[i];<br>            t = j;<br>          }<br>      return ret;<br>    }<br>};<br></td>