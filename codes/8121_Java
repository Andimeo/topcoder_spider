            public class WebBrowser{<br>  public String[] getSequence(String[] actions){<br>    String [] forwardQueue=new String[actions.length];<br>    String [] backwardQueue=new String[actions.length];<br>    String [] order=new String[actions.length];<br>    int fPos=0;<br>    int bPos=0;<br>    int i;<br>    int o=0;<br>    for(i=0;i&lt;actions.length;i++){<br>      if(actions[i].equals("BACK") &amp;&amp; bPos&gt;0){<br>        order[o++]=backwardQueue[--bPos];<br>        forwardQueue[fPos++]=order[o-2];<br>        }<br>      if(actions[i].equals("FORWARD") &amp;&amp; fPos&gt;0){<br>        order[o++]=forwardQueue[--fPos];<br>        backwardQueue[bPos++]=order[o-2];<br>        }<br>      if(!actions[i].equals("FORWARD") &amp;&amp; !actions[i].equals("BACK")){<br>        if(o==0 || !actions[i].equals(order[o-1]))<br>        {order[o++]=actions[i];<br>        fPos=0;<br>        if(o&gt;1){<br>          backwardQueue[bPos++]=order[o-2];<br>          }}<br>        }<br>      }<br>    String[] retvals=new String[o];<br>    for(i=0;i&lt;o;i++){<br>      retvals[i]=order[i];<br>      }<br>    System.out.println(retvals.length+" ");<br>    return retvals;<br>    }<br>  }<br></td>