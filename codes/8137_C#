            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class MajorityElement <br>{<br>  public int findMajorityElement(int[] a) <br>  {<br>        int len = a.Length;<br>        for (int i = 0; i &lt; a.Length; i++)<br>        {<br>            int el = 0;<br>            for (int j = 0; j &lt; a.Length; j++)<br>            {<br><br>                if (a[i]==a[j])<br>                    el++;<br>            }<br>            double ret=(double)el/(double)len;<br>            if (ret &gt; 0.5)<br>                return a[i];<br>        }<br>        return -1;<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by r1cs1]<br>// Powered by CodeProcessor<br></td>