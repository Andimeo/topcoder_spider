            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class MajorityElement<br>{<br>  public int findMajorityElement(int[] a)<br>  {<br>    int majEl = -1;<br>    for(int i = 0; i &lt; 1001; i++)<br>    {<br>      int ct = 0;<br>      for(int j = 0; j &lt; a.length; j++)<br>        if(a[j] == i)<br>          ct++;<br>      if(ct &gt; a.length/2)<br>        majEl = i;<br>    }<br>    return majEl;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>