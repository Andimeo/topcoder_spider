            #include&lt;iostream&gt;<br>#include&lt;cstdio&gt;<br>#include&lt;cctype&gt;<br>#include&lt;cmath&gt;<br>#include&lt;cstdlib&gt;<br>#include&lt;algorithm&gt;<br>#include&lt;vector&gt;<br>#include&lt;string&gt;<br>#include&lt;list&gt;<br>#include&lt;deque&gt;<br>#include&lt;map&gt;<br>#include&lt;set&gt;<br>#include&lt;queue&gt;<br>#include&lt;stack&gt;<br>#include&lt;utility&gt;<br>#include&lt;sstream&gt;<br>#include&lt;cstring&gt;<br>#define fup(i,a,b) for(int i=a;i&lt;=b;i++)<br>#define fdo(i,a,b) for(int i=a;i&gt;=b;i--)<br>#define abso(a) ((a)&lt;0?(-(a)):(a))<br>#define maxi(a,b) ((a)&gt;(b)?(a):(b))<br>#define mini(a,b) ((a)&lt;(b)?(a):(b))<br>#define MP make_pair<br>#define PB push_back<br>#define FI first<br>#define SE second<br>#define siz(a) (int)a.size()<br>#define inf 1000000000<br>#define SQR(a) ((a)*(a))<br>using namespace std;<br>typedef long long int64;<br> <br>class CommonFactors <br>{<br>  public:<br>  int mostCommon(vector &lt;int&gt; n)<br>  {<br>    vector&lt;int&gt; t;<br>    fup(i,0,siz(n)-1){<br>    int x=n[i];<br>    int64 act=1;<br>    while(act*act&lt;=x){<br>      if(x%act==0){<br>        t.PB(act);<br>        t.PB(x/act);<br>      }<br>      act++;<br>    }<br>    }<br>    int best=0;<br>    int kto=-1;<br>    fup(i,0,siz(t)-1){<br>    int z=t[i];<br>    if(z==1)continue;<br>    int il=0;<br>    fup(j,0,siz(n)-1)if(n[j]%z==0)il++;<br>    if(il&gt;best||(il==best&amp;&amp;z&gt;kto)){<br>      best=il;<br>      kto=z;<br>    }<br>    }<br>return kto;<br>  }<br>  <br> <br>};<br> <br> <br> <br> <br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>