            //Written by Sergey Melnikov<br>import java.util.*;<br>import java.math.*;<br>public class CommonFactors {<br>    public int mostCommon(int[] n) {<br>        Set&lt;Integer&gt; s = new TreeSet&lt;Integer&gt;();<br>        for (int i = 0; i &lt; n.length; i++) {<br>      int r = n[i];<br>      for (int j = 2; j*j &lt;= r; j++) {<br>        if (r%j==0) {<br>          r/=j;<br>          s.add(j);<br>          j--;<br>        }<br>      }<br>      if (r&gt;1) {<br>        s.add(r);<br>      }<br>    }<br>        int max = 0;<br>        for (int i:s) {<br>      int c = 0;<br>      for (int j = 0; j &lt; n.length; j++) {<br>        if (n[j]%i==0) {<br>          c++;<br>        }<br>      }<br>      if (c&gt;max) {<br>        max = c;<br>      }<br>    }<br>        int res = 1;<br>        fori:for (int i:s) {<br>//      while (true) {<br>        int c = 0;<br>        for (int j = 0; j &lt; n.length; j++) {<br>          if (n[j]%i==0) {<br>            c++;<br>          }<br>        }<br>        if (c!=max) {<br>          continue fori;<br>        }<br>        BigInteger bi  = null;<br>        for (int j = 0; j &lt; n.length; j++) {<br>          if (n[j]%i==0) {<br>            if (bi==null) {<br>              bi = BigInteger.valueOf(n[j]);<br>            }else {<br>              bi = bi.gcd(BigInteger.valueOf(n[j]));<br>            }<br>          }<br>        }<br>        int q = bi.intValue();<br>        if (q&gt;res) {<br>          res = q;<br>        }<br>//      }      <br>    }<br>        return res;<br>    }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>