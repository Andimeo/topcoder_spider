            #include &lt;algorithm&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;queue&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cmath&gt;<br>using namespace std;<br> <br>#define forn(i,n)     for(int i=0; i&lt;int((n)); ++i)<br>#define forv(i,v)     forn(i,int(v.size()))<br>#define ALL(a)        (a).begin(),(a).end()<br>#define pb            push_back<br>#define sz            size()<br>#define SORT(a)       sort(ALL(a))<br>#define iss           istringstream<br> <br>typedef vector&lt; int &gt; vi; typedef vector&lt; string &gt; vs;<br> <br>class SolvingEquation {<br>public:<br>    int solve(int A, int B, int C, int W) {<br>      int ans;<br>      <br>      int x,y,z;<br>      ans = (1&lt;&lt;30);<br>      <br>      for (x=0;x&lt;=100;x++)<br>      for (y=0;y&lt;=100;y++)<br>      for (z=0;z&lt;=100;z++)<br>      if ( A*x + B*y + C*z == W )<br>      ans &lt;?= x+y+z;<br>      <br>      if ( ans == (1&lt;&lt;30) ) return -1;<br>        <br>        return ans;<br>}<br> <br>};<br> <br> <br>// Powered by PopsEdit<br></td>