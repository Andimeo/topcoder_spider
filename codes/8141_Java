            //Written by Sergey Melnikov<br>import java.util.*;<br>import java.math.*;<br>public class SolvingEquation {<br>    public int solve(int A, int B, int C, int W) {<br>        int res = Integer.MAX_VALUE/2;<br>        for (int i = 0; i &lt; 200; i++) {<br>          for (int j = 0; j &lt; 200; j++) {<br>        for (int k = 0; k &lt; 200; k++) {<br>          if (A*i+B*j+C*k==W &amp;&amp; i+j+k&lt;res) {<br>            res = i+j+k; <br>          }<br>        }<br>      }<br>    }<br>        if (res==Integer.MAX_VALUE/2) {<br>          res = -1;<br>        }<br>        return res;<br>    }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>