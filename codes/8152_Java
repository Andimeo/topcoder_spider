            public class MorselikeCode {<br>  public String decrypt(String[] library, String message){<br>    String[] code = message.split(" ");<br>    String res = "";<br>    String[] mo = new String[library.length];<br>    <br>    for(int m=0;m&lt;library.length;m++){<br>      mo[m]=library[m].substring(2);<br>    }<br>    <br>    for(int i=0;i&lt;code.length;i++){<br>      boolean f = false;<br>      for(int j=0;j&lt;library.length;j++){<br>        if(code[i].compareTo(mo[j]) == 0){<br>          res+=library[j].charAt(0);<br>          f = true;<br>          break;<br>        }<br>      }<br>      if(f == false)<br>        res+='?';<br>    }<br>    return res;<br>  }<br>}<br></td>