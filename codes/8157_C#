            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class CountExpressions<br>{<br>  public int c = 0;<br>  bool[] d = new bool[4];<br>  long[] a;<br>  long v;<br>  <br>  public void go( int ind, long sum ) {<br>    if ( ind == 4 ) {<br>      if ( sum == v ) c++;<br>      return;<br>    }<br>    <br>    for ( int i = 0; i &lt; 4; i++ ) {<br>      if ( !d[i] ) {<br>        d[i] = true;<br>        go( ind + 1, sum + a[i] );<br>        go( ind + 1, sum - a[i] );<br>        go( ind + 1, sum * a[i] );<br>        d[i] = false;<br>      }<br>    }<br>  }<br>  <br>  public int calcExpressions(int x, int y, int val)<br>  {<br>    a = new long[4];<br>    a[0] = x;<br>    a[1] = x;<br>    a[2] = y;<br>    a[3] = y;<br>    c = 0;<br>    v = val;<br>    <br>    for ( int i = 0; i &lt; 4; i++ ) {<br>      d[i] = true;<br>      go( 1, a[i] );<br>      d[i] = false;<br>    }<br>    <br>    return c / 4;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>