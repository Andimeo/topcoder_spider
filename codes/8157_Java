            import java.util.*;<br> <br>public class CountExpressions {<br>  public int calcExpressions(int x, int y, int val) {<br>    return am(x, y, val, 1, 2, x) + am(x, y, val, 2, 1, y);<br>  }<br> <br>    private int am(int x, int y, int val, int nx, int ny, int cur) {<br>        int res = 0;<br>        if (nx == 0 &amp;&amp; ny == 0) {<br>            if (cur == val)<br>                ++res;<br>        }<br>        for (int op = 1; op &lt;= 3; ++op) {<br>            for (int kind = 1; kind &lt;= 2; ++kind) {<br>                int z;<br>                int mx = nx;<br>                int my = ny;<br>                if (kind == 1) {<br>                    z = x;<br>                    --mx;<br>                } else {<br>                    z = y;<br>                    --my;<br>                }<br>                if (mx &gt;= 0 &amp;&amp; my &gt;= 0) {<br>                    if (op == 1) {<br>                        z = cur + z;<br>                    } else if (op == 2) {<br>                        z = cur - z;<br>                    } else {<br>                        z = cur * z;<br>                    }<br>                    res += am(x, y, val, mx, my, z);<br>                }<br>            }<br>        }<br>        return res;<br>    }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>