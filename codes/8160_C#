            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class MatchString<br>{<br>  public int placeWords(string matchString, string[] matchWords)<br>  {<br>    int min=-1;<br>    for (int p=0; p&lt;50; ++p) {<br>      int cost=0;<br>      for (int i=0; i&lt;matchString.Length; ++i) {<br>        int j=matchWords[i].PadRight(50).Substring(0,p+1).LastIndexOf(matchString[i]);<br>        if (j==-1)<br>          goto next_p;<br>        cost+=p-j;<br>      }<br>      if (min==-1 || cost&lt;min)<br>        min=cost;<br>next_p:;<br>    }<br>    return min;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>