            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class MatchString {<br>public:<br>  int placeWords(string, vector &lt;string&gt;);<br>};<br> <br>int MatchString::placeWords(string matchString, vector &lt;string&gt; matchWords) {<br>  int i, j, k, res=0x7fffffff;<br>  <br>  for(i=0; i&lt;=100; i++)<br>  {<br>    int p=1, val=0;<br>    for(j=0; j&lt;matchWords.size(); j++)<br>    {<br>      int st=i;<br>      if(st&gt;matchWords[j].size()) st=matchWords[j].size();<br>      for(k=st; k&gt;=0; k--) if(matchWords[j][k]==matchString[j]) break;<br>      if(k==-1) {p=0; break; }<br>      else val+=i-k;<br>    }<br>    <br>    if(p &amp;&amp; res&gt;val) res=val;<br>  }<br>  <br>  if(res==0x7fffffff) return -1;<br>  return res;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>