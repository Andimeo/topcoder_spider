            import java.util.*;<br>public class MatchString {<br><br><br>  public int placeWords(String matchString, String[] matchWords) {<br>    char[] cc = matchString.toCharArray();<br>    int best = Integer.MAX_VALUE;<br>    int maxLen = 0;<br>    for (int i = 0; i &lt; matchWords.length; i++) {<br>      maxLen = Math.max(maxLen, matchWords[i].length());<br>    }<br><br>    for (int i = 0; i &lt; maxLen; i++) {<br>      int cur = 0;<br>      for (int j = 0; j &lt; cc.length; j++) {<br>        int ind = findNearestIndex(i, cc[j], matchWords[j]);<br>        if (ind == -1) {<br>          cur = -1;<br>          break;<br>        }<br>        cur += i - ind;<br>      }<br>      if (cur != -1)<br>        best = Math.min(cur, best);<br>    }<br>    return best == Integer.MAX_VALUE ? -1 : best; <br>  }<br><br>  private int findNearestIndex(int i, char c, String matchWord) {<br>    for (int j = Math.min(i, matchWord.length() - 1); j &gt;= 0; j--)<br>      if (matchWord.charAt(j) == c)<br>        return j;<br>    return -1;<br>  }<br><br>  public static void main(String[] args) {<br>    MatchString temp = new MatchString();<br>    System.out.println(temp.placeWords("TOP", new String[] {"TIK",<br> "PPPO",<br> "OP"}));<br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>