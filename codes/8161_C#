            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class MinDifference<br>{<br>  public int closestElements(int A0, int X, int Y, int M, int n)<br>  {<br>    int[] A=new int[n];<br>    A[0]=A0;<br>    for (int i=1; i&lt;n;++i)<br>      A[i]=(A[i - 1] * X + Y) % M;<br>    int min=int.MaxValue;<br>    for (int i=1; i&lt;n; ++i)<br>      for (int j=0; j&lt;i; ++j)<br>        min=Math.Min(min,Math.Abs(A[i]-A[j]));<br>    return min;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>