            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class SerialNumbers<br>{<br>  int digSum(String s) {<br>    int  ans = 0;<br>    for (char c : s.toCharArray()) {<br>      if (c &gt;= '0' &amp;&amp; c &lt;= '9')<br>        ans += c - '0';<br>    }<br>    return ans;<br>  }<br>  <br>  class Cmp implements Comparator&lt;String&gt; {<br>    public int compare(String a, String b) {<br>      if (a.length() != b.length())<br>        return new Integer(a.length()).compareTo(new Integer(b.length()));<br>      int d1 = digSum(a);<br>      int d2 = digSum(b);<br>      if (d1 != d2)<br>        return new Integer(d1).compareTo(new Integer(d2));<br>      return a.compareTo(b);<br>    }<br>  }<br>  <br>  public String[] sortSerials(String[] serialNumbers)<br>  {<br>    Arrays.sort(serialNumbers, new Cmp());<br>    return serialNumbers;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>