            using System;<br><br>public class IsomorphicWords {<br>  public int countPairs(string[] words) {<br>        int n = words.Length;<br>        string[] w = new string[n];<br>        for (int i = 0; i &lt; n; ++i)<br>            w[i] = norm(words[i]);<br>        int res = 0;<br>        foreach (string x in w)<br>            foreach (string y in w)<br>                if (x == y)<br>                    ++res;<br>        return (res - n) / 2;<br>  }<br><br>    string norm(string x)<br>    {<br>        string map = "";<br>        string res = "";<br>        foreach (char ch in x)<br>        {<br>            if (map.IndexOf(ch) &lt; 0)<br>                map += ch;<br>            res += (char) ('a' + map.IndexOf(ch));<br>        }<br>        return res;<br>    }<br><br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>