            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br>#include &lt;complex&gt;<br>#include &lt;setjmp.h&gt;<br>using namespace std;<br> <br>class IsomorphicWords {<br>public:<br>  int countPairs(vector &lt;string&gt;);<br>};<br>#define FR(i,a,b) for(int i=a;i&lt;b;i++)<br>#define FOR(i,n) FR(i,0,n)<br>#define FORI(i,v) FOR(i,v.size())<br>#define BEND(v) v.begin(),v.end()<br>typedef long long ll; typedef long double ld;<br>int IsomorphicWords::countPairs(vector &lt;string&gt; words) {<br>  int ans = 0;<br>  FORI(i,words) FORI(j,words) if (i&lt;j) {<br>    char m[256]={};<br>    bool used[256]={};<br>    FORI(k,words[i]) {<br>      char a=words[i][k],b=words[j][k];<br>      if (m[a] == 0) {<br>        if (used[b]) goto xx;<br>        used[b] = true;<br>        m[a] = b;<br>      }<br>      if (m[a] != b) goto xx;<br>    }<br>    ans++;<br>    xx:;<br>  }<br>  return ans;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>