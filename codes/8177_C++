<td class="problemText" colspan="8" valign="middle" align="left">
            #include &lt;vector&gt;<br><br>using namespace std;<br><br>class PickGuitars {<br>public:<br>int maxValue(vector&lt;int&gt; v) {<br>  int n=v.size();<br>  int a[60][60];<br>  int i,j,k;<br>  for(i=0;i&lt;n;i++)<br>    a[i][0]=0;<br>  for(i=0;i&lt;n;i++)<br>    a[i][1]=v[i];<br>  for(i=2;i&lt;n;i++) {<br>    for(j=0;j&lt;n;j++) {<br>      a[j][i]=-1000000000;<br>      for(k=0;k&lt;i;k++) {<br>        int t=-a[j][k]+v[(j+k)%n]-a[(j+k+1)%n][i-k-1];<br>        if (t&gt;a[j][i]) a[j][i]=t;<br>      }<br>    }<br>  }<br>  int best=-1000000000;<br>  for(i=0;i&lt;n;i++) {<br>    int t=v[i]-a[(i+1)%n][n-1];<br>    if (t&gt;best) best=t;<br>  }<br>  for(i=0;i&lt;n;i++)<br>    best+=v[i];<br>  return best/2;<br>}<br>};<br></td>