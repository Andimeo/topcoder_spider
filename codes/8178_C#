            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br> <br>public class AbsSequence<br>{<br>  public string[] getElements (string first, string second, string[] indices)<br>  {<br>    int n = indices.Length;<br>    <br>    long a0 = long.Parse (first);<br>    long a1 = long.Parse (second);<br>    long[] ind = new long[n];<br>    long[] res = new long[n];<br>    <br>    for (int i = 0; i &lt; n; ++i)<br>      ind[i] = long.Parse (indices[i]);<br>    <br>    long q = 0;<br>    <br>    while (a1 != 0)<br>    {<br>      if (a0 &lt;= a1)<br>      {<br>        for (int i = 0; i &lt; n; ++i)<br>          if (ind[i] == q)<br>            res[i] = a0;<br>        <br>        long na0 = a1;<br>        long na1 = a1 - a0;<br>        <br>        a0 = na0;<br>        a1 = na1;<br>        ++q;<br>        <br>        continue;<br>      }<br>      <br>      if (a0 &lt; 3 * a1)<br>      {<br>        for (int i = 0; i &lt; n; ++i)<br>          if (ind[i] == q)<br>            res[i] = a0;<br>        <br>        long na0 = a1;<br>        long na1 = a0 - a1;<br>        <br>        a0 = na0;<br>        a1 = na1;<br>        ++q;<br>        <br>        continue;<br>      }<br>      <br>      <br>      long steps = (a0 - a1) / a1 / 2;<br>      <br>      for (int i = 0; i &lt; n; ++i)<br>        if (ind[i] &gt;= q &amp;&amp; ind[i] &lt; q + steps * 3)<br>      {<br>        long round = (ind[i] - q) / 3;<br>        <br>        switch ((ind[i] - q) % 3)<br>        {<br>          case 0:<br>            res[i] = a0 - 2 * round * a1;<br>            break;<br>          <br>          case 1:<br>            res[i] = a1;<br>            break;<br>          <br>          case 2:<br>            res[i] = a0 - 2 * round * a1 - a1;<br>            break;<br>        }<br>      }<br>      <br>      a0 -= 2 * steps * a1;<br>      q += steps * 3;<br>    }<br>    <br>    for (int i = 0; i &lt; n; ++i)<br>      if (ind[i] &gt;= q)<br>    {<br>      switch ((ind[i] - q) % 3)<br>      {<br>        case 0:<br>        case 2:<br>          res[i] = a0;<br>          break;<br>        <br>        case 1:<br>          res[i] = a1;<br>          break;<br>      }<br>    }<br>    <br>    string[] r = new string[n];<br>    <br>    for (int i = 0; i &lt; n; ++i)<br>      r[i] = res[i].ToString ();<br>    <br>    return r;<br>  }<br>  <br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>