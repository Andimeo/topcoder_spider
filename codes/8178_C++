<td class="problemText" colspan="8" valign="middle" align="left">
            #include &lt;vector&gt;<br>#include &lt;string&gt;<br><br>using namespace std;<br><br>class AbsSequence {<br>  public:<br>  <br>  vector &lt;string&gt; getElements(string first, string second, vector &lt;string&gt; ind) {<br>    long long a, b, c[50];<br>    long long aa, bb, cc;<br>    int n, i;<br>    long long j;<br>    <br>    vector&lt;string&gt; re;<br>    <br>    sscanf(first.c_str(),"%lld",&amp;a);<br>    sscanf(second.c_str(),"%lld",&amp;b);<br>    n = ind.size();<br>    <br>    for (i = 0; i &lt; n; i ++)<br>      sscanf(ind[i].c_str(),"%lld",c+i);<br>    <br>    for (i = 0; i &lt; n; i ++) {<br>      aa = a; bb = b;<br>      for (j = 0; j &lt; c[i];) {<br>        cc = aa - bb &gt;? bb - aa;<br>        <br>        long long dd, ee, ff;<br>        <br>        dd = 120000000000000000LL;<br>        ee = 150000000000000000LL;<br>        ff = 100000000000000000LL;<br>        while (dd &gt;= 12) {<br>            while (aa &gt; bb &amp;&amp; bb / dd &gt; cc &amp;&amp; j + ee &lt; c[i]) {<br>            aa -= cc * ff;<br>            bb -= cc * ff;<br>            j += ee;<br>            cc = aa - bb &gt;? bb - aa;<br>            }<br>          dd /= 10;<br>          ee /= 10;<br>          ff /= 10;<br>          }<br>        if (aa == bb &amp;&amp; j &lt; c[i]) {<br>          j += (c[i] - j - 1) / 3 * 3;<br>          cc = aa - bb &gt;? bb - aa;<br>          }<br>        if (j &lt; c[i]) {<br>          aa = bb;<br>          bb = cc;<br>          j ++;<br>          }<br>        }<br>      char tmp[100];<br>      sprintf(tmp, "%lld", aa);<br>      re.push_back(tmp);<br>      }<br>    return re;<br>    }<br>  };<br></td>