            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class AbsSequence<br>{<br>  long get(long x, long f, long s)<br>  {<br>    while (x &gt; 0)<br>    {<br>      if (x &lt; 3 || f &lt; 2 * s)<br>      {<br>        x--;<br>        long temp = s;<br>        s = Math.abs(s - f);<br>        f = temp;<br>      }<br>      else<br>      {<br>        long maxCount = x / 3;<br>        if (s &gt; 0)<br>          maxCount = Math.min(f / 2 / s, maxCount); <br>        x -= maxCount * 3;<br>        f -= maxCount * 2 * s;<br>      }<br>    }<br>    return f;  <br>  }<br>  <br>  public String[] getElements(String first, String second, String[] indices)<br>  {<br>    long f = Long.parseLong(first);<br>    long s = Long.parseLong(second);<br>    String[] result = new String[indices.length];<br>    for(int i = 0; i &lt; indices.length; i++)<br>      result[i] = Long.toString(get(Long.parseLong(indices[i]), f, s));<br>    return result;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>