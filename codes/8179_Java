<td class="problemText" colspan="8" valign="middle" align="left">
            public class IsoscelesTriangulations<br>{<br>  public final static int m=123456789;<br>  int[][] table;<br>  public int getCount(int n,int k)<br>  {<br>    table=new int[60][60];<br>    table[1][0]=1;<br>    for (int kk=1;kk&lt;=k;kk++)<br>      table[1][kk]=0;<br>    for (int nn=2;nn&lt;=n;nn++)<br>    {<br>      for (int kk=0;kk&lt;=k;kk++)<br>      {<br>        int res=0;<br>        for (int i=1;i&lt;=nn-1;i++)<br>        {<br>          int kk1=kk;<br>          if (2*i==nn || i==n-nn || nn-i==n-nn)<br>          {<br>            kk1--;<br>            if (kk1&lt;0)<br>              continue;<br>          }<br>          for (int l=0;l&lt;=kk1;l++)<br>          {<br>            res=(int)(res+((long)table[i][l]*(long)table[nn-i][kk1-l])%m);<br>            res=res%m;<br>          }<br>            <br>        }<br>        table[nn][kk]=res%m;<br>      }<br>    }<br>    return table[n-1][k];<br>  }<br>}<br></td>