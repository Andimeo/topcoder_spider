            #include &lt;cctype&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cmath&gt;<br><br>#include &lt;vector&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br><br>#define MP make_pair<br>#define PB push_back<br>typedef long long LL;<br><br>#define FOR(i, n) for (int i = 0; i &lt; (n); i++)<br>#define FORIT(i, a) for (__typeof((a).begin()) i = (a).begin(); i != (a).end(); i++)<br>#define SZ(a) ((int)(a).size())<br><br>using namespace std;<br><br>class Containers<br>{<br>public:<br>  int wastedSpace(vector &lt;int&gt; a, vector &lt;int&gt; b)<br>  {<br>    int sum = 0;<br>    FORIT(i, a)<br>      sum += *i;<br>    FORIT(i, b)<br>      sum -= *i;<br>    return sum;<br>  }<br>};<br></td>