            #include &lt;algorithm&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;queue&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cmath&gt;<br>using namespace std;<br> <br>#define forn(i,n)     for(int i=0; i&lt;int((n)); ++i)<br>#define forv(i,v)     forn(i,int(v.size()))<br>#define ALL(a)        (a).begin(),(a).end()<br>#define pb            push_back<br>#define sz            size()<br>#define SORT(a)       sort(ALL(a))<br>#define iss           istringstream<br> <br>typedef vector&lt; int &gt; vi; typedef vector&lt; string &gt; vs;<br> <br>int a[64][64];<br> <br>class LinearCity {<br>public:<br>    vector &lt;string&gt; getReference(vector &lt;int&gt; rs, vector &lt;int&gt; rd, string rdir, int N, vector &lt;int&gt; q1, vector &lt;int&gt; q2) {<br>        vector &lt;string&gt; ans;<br>  <br>  forv (i,rs)<br>  {<br>    if ( rdir[i] == 'R' )<br>    {<br>      //2 dqsno<br>      //1 lqvo<br>      <br>      a[rs[i]][rd[i]] = 2;<br>      a[rd[i]][rs[i]] = 1;<br>    }<br>    else<br>    {<br>      a[rs[i]][rd[i]] = 1;<br>      a[rd[i]][rs[i]] = 2;<br>    }<br>  }<br>  <br>  int i,j,k;<br>  int n = N;<br>  <br>  for (k=0;k&lt;n;k++)<br>  {<br>    for (i=0;i&lt;n;i++)<br>    {<br>      if (k!=i)<br>      {<br>        for (j=0;j&lt;n;j++)<br>        {<br>          if ( k!=j &amp;&amp; i != j )<br>          {<br>            if ( a[i][k] == 2 &amp;&amp; a[k][j] == 2 )<br>            {<br>              a[i][j] = 2;<br>            }<br>            if ( a[i][k] == 1 &amp;&amp; a[k][j] == 1 )<br>            {<br>              a[i][j] = 1;<br>            }<br>          }<br>        }<br>      }<br>    }<br>  }<br>  <br>  forv ( i,q1 )<br>  {<br>    if ( a[q1[i]][q2[i]] == 0 )<br>    {<br>      ans.pb("UNKNOWN");<br>    }<br>    else<br>    if ( a[q1[i]][q2[i]] == 1 )<br>    {<br>      ans.pb("LEFT");<br>    }<br>    else ans.pb("RIGHT");<br>  }<br>  <br>        return ans;<br>}<br> <br>};<br> <br> <br>// Powered by PopsEdit<br></td>