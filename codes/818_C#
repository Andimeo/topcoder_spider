            using System;<br>using System.Collections;<br>public class Average {<br>  public int belowAvg(int[] math, int[] verbal) {<br>    // Begin Your Code Here...<br>    double avg = 0.0;<br>    for (int i = 0; i &lt; math.Length; i++)<br>      avg += (math[i] + verbal[i]);<br>    avg /= (double)math.Length;<br>    int ret = 0;<br>    for (int i = 0; i &lt; math.Length; i++)<br>      if (math[i] + verbal[i] &lt; avg)<br>        ret++;<br>    return ret;<br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>