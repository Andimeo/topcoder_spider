<td class="problemText" colspan="8" valign="middle" align="left">
            public class StudentEnrollment<br>{<br>  public string[] checkClasses(string[] names, int[] spaces, string[] queries)<br>  {<br>    string[] res = new string[queries.Length];<br>    bool taken = false;<br>    for(int i=0;i&lt;queries.Length;i++)<br>    {<br>      for(int j=0;j&lt;names.Length;j++)<br>      {<br>        if(names[j] == queries[i])<br>        {<br>          taken = true;<br>          if(spaces[j]&gt;0)<br>          {<br>            res[i]="GOOD";<br>            spaces[j]--;<br>          }<br>          else<br>          {<br>            res[i]="NOT GOOD";<br>          }<br>        }<br>      }<br>      if(taken == false)<br>      {<br>        res[i]="DOES NOT EXIST";<br>      }<br>      taken = false;<br>    }<br>    return res;<br>  }<br>}<br></td>