            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class StudentEnrollment<br>{<br>  public String[] checkClasses(String[] names, int[] f, String[] q)<br>  {<br>    String[] sol = new String[q.length];<br>    for(int i=0;i&lt;q.length;i++){<br>      int j=0;<br>      while(j&lt;names.length &amp;&amp; names[j].compareTo(q[i])!=0) j++;<br>      if(j==names.length)<br>        sol[i] = "DOES NOT EXIST";<br>      else<br>      if(f[j] &gt; 0){<br>        sol[i]="GOOD";<br>        f[j]--;<br>      }<br>      else<br>        sol[i]="NOT GOOD";<br>    }<br>    <br>    return sol;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>