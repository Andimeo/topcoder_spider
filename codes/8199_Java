            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class Prank<br>{<br>  public int[] realWeight(int apparentGain)<br>  {<br>    ArrayList&lt;Integer&gt; arl=new ArrayList&lt;Integer&gt;();<br>    int i=0;<br>    for(;i&lt;100000;i++)if(Math.pow(i, 2)&gt;=apparentGain)break;<br>    for(;i&lt;100000;i++){<br>      double pp=Math.pow(i, 2);<br>    for(int j=i;j&gt;0;j--){<br>      if(pp-Math.pow(j, 2)==apparentGain){<br>        arl.add(i);<br>        break;<br>      }<br>      if(pp-Math.pow(j, 2)&gt;apparentGain)break;<br>    }<br>    }<br>    Object[] result=arl.toArray();<br>    int[] re=new int[result.length];<br>    for(int m=0;m&lt;result.length;m++){<br>      re[m]=arl.get(m);<br>    }<br>    return re;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>