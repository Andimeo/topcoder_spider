            using System;<br><br>public class KeysInBoxes {<br>  public string getAllKeys(int N, int M) {<br>        long[,] am = new long[N + 1, M + 1];<br>        am[0, 0] = 1;<br>        for (int i = 1; i &lt;= N; ++i)<br>            for (int j = 1; j &lt;= M; ++j)<br>            {<br>                long s = 0;<br>                long pr = 1;<br>                for (int u = 1; u &lt;= i; ++u)<br>                {<br>                    s += am[i - u, j - 1] * pr;<br>                    pr *= (i - u);<br>                }<br>                am[i, j] = s;<br>            }<br>        long p = 0;<br>        for (int j = 1; j &lt;= M; ++j)<br>            p += am[N, j];<br>        long q = 1;<br>        for (int i = 1; i &lt;= N; ++i)<br>            q *= i;<br>        long pp = p;<br>        long qq = q;<br>        while (qq &gt; 0)<br>        {<br>            long t = pp % qq;<br>            pp = qq;<br>            qq = t;<br>        }<br>        p /= pp;<br>        q /= pp;<br>        return p + "/" + q;<br>  }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>