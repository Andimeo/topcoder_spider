<td class="problemText" colspan="8" valign="middle" align="left">
            public class SausagesProductionScheduler{<br>  public int maxCount(String[] lp, String[] up, int[] limits){<br>    int n = lp.length;<br>    int[] a = new int[n];<br>    int[] b = new int[n];<br>    for(int i=0;i&lt;n;i++){<br>      String[] s = lp[i].split(" ");<br>      String[] S = up[i].split(" ");<br>      int x = Integer.parseInt(s[0]);<br>      int X = Integer.parseInt(S[0]);<br>      int y = Integer.parseInt(s[1]);<br>      int Y = Integer.parseInt(S[1]);<br>      a[i] = Math.max(x,100-Y);<br>      b[i] = Math.min(X,100-y);<br>    }<br>    int aa = limits[0];<br>    int bb = limits[1];<br>    boolean[][] res = new boolean[n+1][aa+1];<br>    res[0][0] = true;<br>    int best = 0;<br>    //System.out.println(a[0]+" "+b[0]);<br>    for(int i=0;i&lt;n;i++){<br>      for(int x=i;x&gt;=0;x--)<br>        for(int z=0;z&lt;=aa &amp;&amp; z&lt;=x*100;z++) if (res[x][z]){<br>          if (z+a[i]&lt;=aa){<br>            for(int j=a[i];j&lt;=b[i];j++){<br>              if (z+j&gt;aa) break;<br>              res[x+1][z+j] = true;<br>              if (best&lt;x+1 &amp;&amp; bb&gt;=(x+1)*100-(z+j)){<br>                best = x+1;<br>              }<br>            }<br>          }<br>        }<br><br>    }<br>    return best;<br>  }<br>}<br></td>