            // Cry as challenge phase is passing by<br><br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br><br>#include &lt;cmath&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br><br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br><br>using namespace std;<br><br>#define FORC(it,v) for( __typeof((v).begin()) it = (v).begin(); it != (v).end(); ++it )<br><br>const int mod = 10000003;<br><br>vector&lt; int &gt; V;<br><br>int n;<br>int dp[ 51 ][ 100010 ];<br><br>int gcd( int a, int b ) { return a &amp;&amp; b ? gcd( b, a%b ) : a|b; }<br><br>int solve( int x, int sum )<br>{<br>    if( x == n ) return ( sum == 1 );<br>    if( dp[x][sum] != -1 ) return dp[x][sum];<br><br>    return dp[x][sum] = ( solve( x+1, sum ) + solve( x+1, gcd( sum, V[x] ) ) ) % mod;<br>}<br><br><br>class DecorationDay {<br>public:<br>    int howMany( vector &lt;int&gt; groups ) {<br>        memset( dp, -1, sizeof dp );<br>        V = groups; n = ( int )V.size();<br>        return solve( 0, 0 );<br>    }<br>};<br><br><br><br><br><br><br><br><br><br>// Powered by FileEdit<br>// Powered by moj 3.0 beta 11 [modified TZTester]<br>// Powered by CodeProcessor<br></td>