            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class DecorationDay<br>{<br>  int gcd(int a,int b){<br>    if(b==0)<br>      return a;<br>    return gcd(b,a%b);<br>  }<br>  <br>  public int howMany(int[] g)<br>  {<br>    int[] ant = new int[100010], crt = new int[100010];<br>    int base=10000003;<br>    <br>    int N=g.length;<br>    for(int i=0;i&lt;N;i++){<br>      crt[g[i]] ++;<br>      crt[g[i]] %= base;<br>      <br>      for(int j=1;j&lt;=100000;j++){<br>        int cmmdc = gcd(g[i],j);<br>        crt[cmmdc] += ant[j];<br>        crt[cmmdc] %= base;      <br>      }<br>      <br>      for(int j=1;j&lt;=100000;j++){<br>        ant[j] += crt[j];<br>        ant[j] %= base;<br>        crt[j] = 0;<br>      }<br>        <br>    }<br>    <br>    return ant[1];<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>