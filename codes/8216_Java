            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class ReadingBooks<br>{<br>  public int countBooks(String[] readParts)<br>  {<br>    int ans = 0;<br>    for(int i = 0; i&lt; readParts.length-2; i++)<br>    {<br>      if(readParts[i].compareTo(readParts[i+1]) != 0 &amp;&amp; readParts[i].compareTo(readParts[i+2]) != 0 &amp;&amp; readParts[i+2].compareTo(readParts[i+1]) != 0 )<br>      {<br>        ans++;<br>        i+=2;<br>      }<br>      <br>      <br>    }<br>    return ans;<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>