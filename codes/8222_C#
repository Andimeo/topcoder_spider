            using System;          <br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br>using System.Diagnostics;<br><br>public class DietPlan {<br>  public string chooseDinner(string diet, string breakfast, string lunch) {<br>        int[] ile = new int[256];<br>        add(diet, ile, 1);<br>        add(breakfast, ile, -1);<br>        add(lunch, ile, -1);<br>        for (int i = 0; i &lt; 256; i++) if (ile[i] &lt; 0) return "CHEATER";<br>        string ret = "";<br>        for (int i = 0; i &lt; 256; i++) while (ile[i] &gt; 0) {<br>                ile[i]--;<br>                ret = ret + ((char)i);<br>        }<br><br>        return ret;<br>  }<br><br>    private void add(string diet, int[] ile, int p) {<br>        foreach (char item in diet) ile[item] += p;<br>    }      <br>#region TESTCODE     <br>  <br>     <br>#endregion    <br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>