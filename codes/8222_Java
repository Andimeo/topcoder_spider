            import java.util.*;<br><br>public class DietPlan {<br>  <br>  public String chooseDinner(String diet, String breakfast, String lunch) {<br>    StringBuilder mydin = new StringBuilder("");<br>    int [] arr = new int [26];<br>    int i;<br>    for (i = 0; i &lt; 26; i++) arr[i] = 0;<br>    for (i = 0; i &lt; diet.length(); i++)<br>      arr[diet.charAt(i)-'A']++;<br>    for (i = 0; i &lt; breakfast.length(); i++)<br>      arr[breakfast.charAt(i)-'A']--;<br>    for (i = 0; i &lt; lunch.length(); i++)<br>      arr[lunch.charAt(i)-'A']--;<br>    for (i = 0; i &lt; 26; i++) {<br>      for (int j = 0; j &lt; arr[i]; j++)<br>        mydin.append((char)('A' + i));<br>      if (arr[i] &lt; 0)<br>        return "CHEATER";<br>    }<br>    <br>    return mydin.toString();<br>    <br>  }<br><br>}<br></td>