            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br>using System.Diagnostics;<br><br>public class RoadConstruction {<br>    public int getExitTime(string[] A) {<br>        int ret = 0;<br>        bool[] yield = new bool[A.Length];<br>        while (true) {<br>            bool[] abouttoexit = new bool[A.Length];<br>            bool ok = true;<br>            for (int i = 0; i &lt; A.Length; i++) if(A[i].Length&gt;0){<br>                for (int j = 0; j &lt; i; j++) if (abouttoexit[j]) goto hell;<br>                bool needtoyield = false;<br>                for (int j = i + 1; j &lt; A.Length; j++) if (A[j].Length &gt; 0) {<br>                        needtoyield = true;<br>                }<br>                if (yield[i] || !needtoyield) {<br>                    abouttoexit[i] = true;<br>                } else<br>                    yield[i] = true;<br>            hell: ;<br>            }<br><br>            for (int i = 0; i &lt; A.Length; i++) if (abouttoexit[i]) {<br>                    if (A[i][0] == 'D') return ret;<br>                    ok = false;<br>                    abouttoexit[i] = false;<br>                    A[i] = A[i].Substring(1);<br>                    yield[i] = false;<br>                    ret++;<br>            }<br><br><br>            if (ok) break;<br>        }<br>        return ret;<br>    }<br>    #region TESTCODE<br><br><br>    #endregion<br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>