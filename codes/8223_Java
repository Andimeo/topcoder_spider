            public class RoadConstruction{<br>boolean[] yield=new boolean[60]; <br>int res=0;<br>char e='-';<br>void f(int i,String[] currentLanes){<br>    e=currentLanes[i].charAt(0);<br>    currentLanes[i]=currentLanes[i].substring(1);<br>    yield[i]=false;<br>    i=500;<br>    res++;<br>    return;<br>    <br>};<br>public int getExitTime(String[] currentLanes){<br><br>for (int i=0;i&lt;60;i++){yield[i]=false;};<br>while (e!='D'){<br>  e='-';<br><br>  for (int i=0;i&lt;currentLanes.length;i++){<br>    if (currentLanes[i].length()&gt;0){<br>    if (!yield[i]){yield[i]=true;}else{<br>    f(i,currentLanes);<br>    i=100;<br>    };<br>    };<br>  };<br>  if (e=='-'){<br>  for (int i=currentLanes.length-1;i&gt;=0;i--){<br>    if (currentLanes[i].length()&gt;0){<br>    f(i,currentLanes);<br>    i=-100;<br>    };<br>  };  <br>  };<br><br>};<br>return res-1;<br>};<br><br>};<br></td>