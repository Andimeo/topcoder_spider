            using System;<br>using System.Collections.Generic;<br>using System.Text;<br><br>class HockeyFault<br>{<br>    bool isInsideSqare(int x, int y, int w, int h, int a, int b)<br>    {<br>        return ((a &lt;= x + w &amp;&amp; a &gt;= x) &amp;&amp; (b &lt;= y + h &amp;&amp; b &gt;= y));<br>    }<br>    bool isInsideCircle(int xc, int yc, int r, int a, int b)<br>    {<br>        return ((a - xc) * (a - xc) + (b - yc) * (b - yc)) &lt;= r * r;<br>    }<br><br>    public int numPlayers(int width, int height, int x, int y, int[] px, int[] py)<br>    {<br>        int ctr = 0;<br>        int xc1, xc2, yc1, yc2;<br>        int r = height / 2;<br>        xc1 = x; yc1 = y + height / 2;<br>        xc2 = x + width; yc2 = y + height / 2;<br>        for (int i = 0; i &lt; px.Length; ++i)<br>        {<br>            if (isInsideCircle(xc1, yc1, r, px[i], py[i]) ||<br>                isInsideCircle(xc2, yc2, r, px[i], py[i]) ||<br>                isInsideSqare(x, y, width, height, px[i], py[i])) ++ctr;<br>        }<br>        return ctr;<br>    }<br><br>    static void Main(string[] args)<br>    {<br>    }<br>}<br></td>