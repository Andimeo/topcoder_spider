            #include &lt;algorithm&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;vector&gt;<br>#include &lt;cmath&gt;<br>#include &lt;map&gt;<br>using namespace std;<br><br>int sqr(int x) { return x * x; }<br><br>struct HockeyFault {<br>  int numPlayers(int width, int height, int x0, int y0, vector &lt;int&gt; px, vector &lt;int&gt; py) {<br>    int res = 0;<br>    for (int i = 0; i &lt; (int)px.size(); i++) {<br>      int x = px[i] - x0;<br>      int y = py[i] - y0;<br><br>      int inside = 0;<br>      if (0 &lt;= x &amp;&amp; x &lt;= width &amp;&amp; 0 &lt;= y &amp;&amp; y &lt;= height) inside = 1;<br>      if (4*sqr(x) + sqr(2*y - height) &lt;= sqr(height)) inside = 1;<br>      if (4*sqr(x - width) + sqr(2*y - height) &lt;= sqr(height)) inside = 1;<br><br>      res += inside;<br>    }<br>    return res;    <br>  }<br>};<br><br><br><br>// Powered by FileEdit<br></td>