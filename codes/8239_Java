            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class FoodCollecting<br>{<br>  public int gather(int neededFood, int n, int price)<br>  {<br>    int best = (neededFood+n-1)/n;<br>    for(int i = n; i &lt;= neededFood; i++)<br>    {<br>      int food = 0, turns = 0, nn = n;<br>      while(nn &lt; i)<br>      {<br>        while(food &lt; price)<br>        {<br>          turns++;<br>          food += nn;<br>        }<br>        food -= price;<br>        nn++;<br>      }<br>      turns += (neededFood+nn-1-food)/nn;<br>      best = Math.min(best, turns);<br>    }<br>    return best;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>