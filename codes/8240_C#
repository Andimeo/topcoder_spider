            public class CondorcetVoting<br>  {<br>    public bool prefer(int a, int b, string[] votes)<br>    {<br>      int acount = 0;<br>      foreach (string s in votes)<br>      {<br>        acount += prefer(a, b, s);<br>      }<br>      if (acount &gt; 0)<br>      {<br>        return true;<br>      }<br>      else<br>      {<br>        return false;<br>      }<br>    }<br><br>    public int prefer(int a, int b, string vote)<br>    {<br>      return -(vote[a]+"").CompareTo(vote[b]+"");<br>    }<br><br>    public int winner(string[] votes)<br>    {<br>      bool[] candidates = new bool[votes[0].Length];<br>      for (int i = 0; i &lt; candidates.Length; i++)<br>      {<br>        bool good = true;<br>        for (int j = 0; j &lt; candidates.Length; j++)<br>        {<br>          if (i == j)<br>          {<br>            continue;<br>          }<br>          if (prefer(i, j, votes))<br>          {<br>            candidates[j] = true;<br>          }<br>          else<br>          {<br>            good = false;<br>            break;<br>          }<br>        }<br>        if (good)<br>        {<br>          return i;<br>        }<br>      }<br>      return -1;<br>    }<br>  }<br></td>