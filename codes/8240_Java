            public class CondorcetVoting<br>{<br><br><br>  public int winner( String[] votes )<br>  {<br>    int win = -1;<br>    for( int i = 0; (i &lt; votes[0].length()) &amp;&amp; (win==-1); ++i)<br>    {<br>      //check if canidate i won<br>      boolean iWins = true;<br>      for( int j = 0;( j &lt; votes[0].length() ) &amp;&amp; iWins; ++j)<br>      {<br>        //check if candi beat candj<br>        int numberPreferiToj = 0;<br>        int numberPreferjToi = 0;<br>        if( i != j)<br>        {<br>          for( int k = 0; (k &lt; votes.length ); ++k)<br>          {<br>            //check if voter k likes i better<br>            System.out.println( votes[k].charAt( i )+ " " + votes[k].charAt( j ));<br>            System.out.println(  votes[k].charAt( i ) &lt; votes[k].charAt(j) );<br>            if(( votes[k].charAt( i ) &lt; votes[k].charAt(j) ) )<br>            {<br>            System.out.println( k );<br>              numberPreferiToj++;<br>            }<br>            if(( votes[k].charAt( j ) &lt; votes[k].charAt(i) ) )<br>            {<br>            System.out.println( k );<br>              numberPreferjToi++;<br>            }<br>            <br>          }<br>        <br>          System.out.println((votes.length /2 +1)+ "numberPrefer " + i + " " + j + " " +numberPreferiToj);<br>          if( numberPreferiToj &lt;= numberPreferjToi) // if i not preferred<br>          {<br>          System.out.println( "Setting iwins false for " +i);<br>            iWins = false;<br>          }<br>        }<br>      }<br>      if( iWins )<br>      {<br>        win = i;<br>      }<br>    }<br>    return win;<br>  }<br>}<br></td>