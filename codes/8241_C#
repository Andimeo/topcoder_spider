<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>using System.Text;<br>using System.Collections.Generic;<br><br>class PirateTreasure<br>{<br>  public static double getDistance(int[] steps, string[] directions)<br>  {<br>    double x = 0.0, y = 0.0;<br>    for (int i = 0; i &lt; steps.Length; i++)<br>    {<br>      double z = (double)steps[i]/Math.Sqrt(2.0);<br>      switch(directions[i])<br>      {<br>        case "NORTH": y += steps[i];break;<br>        case "SOUTH": y -= steps[i];break;<br>        case "EAST": x -= steps[i];break;<br>        case "WEST": x += steps[i];break;<br>        case "NORTHWEST": {x += z; y+= z;  break;}<br>        case "NORTHEAST": {x -= z; y+= z;  break;}<br>        case "SOUTHWEST": {x += z; y-= z;  break;}<br>        case "SOUTHEAST": {x -= z; y-= z;  break;}<br>      }<br>    }<br>    return Math.Sqrt(x*x + y*y);<br>  }  <br>}<br></td>