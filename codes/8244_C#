            using System;<br>using System.Text;<br>using System.Collections.Generic;<br><br>class TurningLightOn<br>{<br>  public static int minFlips(string[] board)<br>  {  <br>    int n = board.Length;<br>    int m = board[0].Length;  <br>    StringBuilder[] a = new StringBuilder[board.Length];    <br>    for (int i = 0; i &lt; board.Length; i++)<br>    {<br>      a[i] = new StringBuilder();<br>      a[i].Append(board[i]);<br>    }<br>    char x = board[n-1][m-1];<br>    int count = 0;<br>    for (int i = n - 1; i &gt;= 0; i--)<br>    {<br>      for (int j = m - 1; j&gt;=0; j--)<br>      {<br>        if (a[i][j] != x)<br>        {<br>          count++;<br>          for (int k = 0; k &lt;= i; k++)<br>            for (int t = 0; t &lt;= j; t++)<br>              if (a[k][t] == '0')  <br>                a[k][t] = '1';<br>              else<br>                a[k][t] ='0';<br>        }<br>      }<br>    }  <br>    if (x == '0') return count + 1;<br>    return count;<br>  }  <br>}<br></td>