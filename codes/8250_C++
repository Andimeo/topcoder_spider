            // By mirosuaf<br>#include &lt;algorithm&gt; <br>#include &lt;cstdio&gt; <br>#include &lt;cstdlib&gt; <br>#include &lt;cctype&gt; <br>#include &lt;cmath&gt; <br>#include &lt;iostream&gt; <br>#include &lt;sstream&gt; <br>#include &lt;string&gt; <br>#include &lt;utility&gt; <br>#include &lt;vector&gt; <br>using namespace std; <br>#define VAR(a,b) typeof(b) a=(b) <br>#define REP(i,n) for(int _n=n, i=0;i&lt;_n;++i) <br>#define FOR(i,a,b) for(int i=(a),_b=(b);i&lt;=_b;++i) <br>#define FORD(i,a,b) for(int i=(a),_b=(b);i&gt;=_b;--i) <br>#define FOREACH(it,c) for(VAR(it,(c).begin());it!=(c).end();++it) <br>#define ALL(f,w) ({ bool _ok=true; f _ok=_ok &amp;&amp; (w); _ok; }) <br>#define EXISTS(f,w) (!ALL(f,!(w))) <br>typedef long long LL; <br>const int INF = 1000000000; <br>typedef vector&lt;int&gt; VI; <br> <br>    class DriveCar<br>        { <br>        public: <br>        int minNumberOfDrifts(vector &lt;string&gt; road) <br>            { <br> <br>int tab[100][100]; REP(i,100) REP(j,100) tab[i][j]=INF;<br> <br>tab[1][0]=0;<br>VI r;<br>int v;<br>REP(i,road[0].size()-1) {<br>    REP(j,3) <br>  FOR(zmien,-1,1) if (j+zmien&gt;=0 &amp;&amp; j+zmien&lt;3) {<br>    if (zmien==0) v=tab[j][i]; else v=tab[j][i]+1;<br>    if (road[j+zmien][i+1]!='#') tab[j+zmien][i+1]=min(v,tab[j+zmien][i+1]);<br>    }<br>  }<br>  <br>  <br>  <br>  int w=INF;<br>REP(i,3) if (tab[i][road[0].size()-1]&lt;w) w=tab[i][road[0].size()-1];<br>if (w==INF) return -1; else return w;<br> <br> <br> <br>            } <br>        <br> <br>        }; <br> <br>    <br> <br> <br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>