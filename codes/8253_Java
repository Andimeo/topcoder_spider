            public class Police{<br>  public int minimalCost(int[] c, String[] a){<br>    int n = c.length;<br>    boolean[][] e = new boolean[n][n];<br>    for(int i=0;i&lt;n;i++)<br>      for(int j=0;j&lt;n;j++)<br>        e[i][j] = a[i].charAt(j)=='Y';<br>    for(int k=0;k&lt;n;k++)<br>      for(int i=0;i&lt;n;i++)<br>        for(int j=0;j&lt;n;j++)<br>          if (e[i][k] &amp;&amp; e[k][j]) e[i][j] = true;<br>    boolean[] g = new boolean[n];<br>    int res = 0;<br>    for(int i=0;i&lt;n;i++) if (!g[i]){<br>      g[i] = true;<br>      int cc = c[i];<br>      for(int j=0;j&lt;n;j++) if (e[i][j] &amp;&amp; e[j][i]){<br>        g[j] = true;<br>        if (c[j]&lt;cc) cc = c[j];<br>      }<br>      res+=cc;<br>    }<br>    return res;<br>  }<br>}<br></td>