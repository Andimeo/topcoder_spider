            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class CardsAndSlots<br>{<br>  int n;<br>  boolean[][] mas;<br>  int[] s;<br>  boolean[] f;<br>  <br>  private final boolean rec(int x)<br>  {<br>    f[x] = true;<br>    for(int i = 0; i &lt; n; i++)<br>      if (mas[x][i])<br>      {<br>        if (s[i] == -1)<br>        {<br>          s[i] = x;<br>          return true;<br>        }<br>        else if (!f[s[i]])<br>        {<br>          if (rec(s[i]))<br>          {<br>            s[i] = x;<br>            return true;<br>          }<br>        }        <br>      }<br>    return false;<br>  }<br> <br>  public String firstValid(int[] values, String letters, int[] required)<br>  {<br>    char[] let = letters.toCharArray();<br>    n = values.length;<br>    mas = new boolean[n][n];<br>    <br>    for(int i = 0; i &lt; n; i++)<br>      for(int j = 0; j &lt; n; j++)<br>        mas[i][j] = (values[i] &gt;= required[j]);<br>      <br>    s = new int[n];<br>    f = new boolean[n];<br> <br>    for(int k = 0; k &lt; n; k++)<br>    {<br>      boolean ok = false;<br>      for(int j = 0; j &lt; 26; j++)<br>      {<br>        for(int l = 0; l &lt; n; l++)<br>          mas[l][k] = (let[l] == j + 'A') &amp;&amp; (values[l] &gt;= required[k]);<br>                  <br>        ok = true;<br>        Arrays.fill(s, -1);<br>        for(int i = 0; i &lt; n; i++)<br>        {<br>          Arrays.fill(f, false);<br>          if (!rec(i))<br>          {<br>            ok = false;<br>            break;<br>          }<br>        }<br>        if (ok)<br>        {<br>          System.out.println(k + " " + j);<br>          break;          <br>        }<br>      }<br>      if (!ok)<br>        return "";<br>    }<br>    <br>    String res = "";<br>    for(int i = 0; i &lt; n; i++)<br>      res += let[s[i]];<br>    return res;<br>  }<br>}<br>//Powered by [KawigiEditNonTest] modified by pivanof!<br></td