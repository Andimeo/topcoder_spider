            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class TraditionalMarriage<br>{<br>  public int getLuckyItems(String[] p, int[] we)<br>  {<br>    int val = 100000000;<br>    Item[] r = new Item[p.length+3];<br>    int i;<br>    for(i = 0; i &lt; p.length; i++)<br>      r[i] = new Item(p[i], we[i]);<br>    r[i++] = new Item("", 0);<br>    r[i++] = new Item("", 0);<br>    r[i++] = new Item("", 0);<br>    for(int a = 0; a &lt; r.length; a++)<br>    for(int b = 0; b &lt; r.length; b++)<br>    for(int c = 0; c &lt; r.length; c++)<br>    for(int d = 0; d &lt; r.length; d++)<br>    {<br>      if(r[a].A || r[b].A || r[c].A || r[d].A)<br>      if(r[a].B || r[b].B || r[c].B || r[d].B)<br>      if(r[a].C || r[b].C || r[c].C || r[d].C)<br>      if(r[a].D || r[b].D || r[c].D || r[d].D)<br>        val = Math.min(val, r[a].w+r[b].w+r[c].w+r[d].w);<br>    }<br>    return val==100000000?-1:val;<br>  }<br>  class Item<br>  {<br>    public boolean A, B, C, D;<br>    public int w;<br>    public Item(String ss, int o)<br>    {<br>      w = o;<br>      A = B = C = D = false;<br>      String[] temp = ss.split(",");<br>      for(String s : temp)<br>      {<br>        if(s.equals("blue"))<br>          A = true;<br>        if(s.equals("old"))<br>          B = true;<br>        if(s.equals("borrowed"))<br>          C = true;<br>        if(s.equals("new"))<br>          D = true;<br>      }<br>    }<br>  }<br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>