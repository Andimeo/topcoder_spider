<td class="problemText" colspan="8" valign="middle" align="left">
            public class OddGraph{<br>  <br>  <br>  public int col(int a, int b){<br>    b--;<br>    long rr =1;<br>    for(int i=1;i&lt;=a;i++){<br>      rr*=(b+i);<br>      rr/=i;<br>    }<br>    return (int)rr;<br>  }<br>  <br>  int[] r;<br>  <br>  public int poisk(int n){<br>    int[][] res = new int[n+1][n+1];<br>    res[0][0] = 1;<br>    for(int i=1;i&lt;=n;i++){<br>      res[i][0] = 0;<br>      res[0][i] = 1;<br>    }<br>    for(int i=1;i&lt;=n;i++)<br>      for(int j=1;j&lt;=n;j++){<br>        if (j%2==0) {<br>          res[i][j] = res[i][j-1];<br>          continue;<br>        }<br>        for(int t=0;t*j&lt;=i;t++){<br>          res[i][j]+=col(t,r[j-1])*res[i-t*j][j-1];<br>        } <br>      }<br>    return res[n][n];<br>  }<br>  <br>  <br>  public int count(int n){<br>    r = new int[n+1];<br>    r[0]= 1;<br>    for(int i=1;i&lt;=n;i++)<br>      r[i] = poisk(i);<br>    return r[n];<br>  }<br>  <br>}<br></td>