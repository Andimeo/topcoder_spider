            import java.util.*;<br>import static java.lang.Math.*; <br>import static java.util.Arrays.*;<br>import static java.lang.Integer.*;<br>import static java.lang.Character.*;<br>import static java.lang.Double.*;<br>import java.awt.geom.*;<br>public class DateFieldCorrection {<br>    int n=0;<br>    /**                        DON'T  PANIC                      */<br>    String[]Mon = new String[]{"January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November" , "December"};<br>    int[]Days = new int[]{31, 29, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31};<br>    <br>    int[][]D = new int[300][300];<br>    <br>    void add(int a, int b){D[a][b]=D[b][a]=1;}<br>    <br>    int diff(String s1, String s2){<br>        int r =0;<br>        for(int i=0;i&lt;s1.length();i++){<br>            r += D[s1.charAt(i)][s2.charAt(i)];<br>        }<br>        return r;<br>    }<br>    <br>    public String correctDate(String input) {<br>        input = input.toLowerCase();<br>        String res="";<br>        int i=0,j=0;<br>        int best = 100000000;<br>        String[]M = new String[]{<br>                "1234567890",<br>                "qwertyuiop",<br>                "asdfghjkl",<br>                "zxcvbnm",<br>        };<br>        for(int[]DD:D)fill(DD,1000);<br>        for(int x=0;x&lt;D.length;x++)D[x][x]=0;<br>        for(i=0;i&lt;M.length;i++)for(j=0;j&lt;M[i].length();j++){<br>            char c = M[i].charAt(j);<br>            if(i&gt;0){<br>                char d =M[i-1].charAt(j);<br>                add(c,d);<br>                if(j+1&lt;M[i-1].length()){<br>                    d = M[i-1].charAt(j+1);<br>                    add(c,d);<br>                }<br>            }<br>            if(j&gt;0){<br>                char d = M[i].charAt(j-1);<br>                add(c,d);<br>            }<br>            if(j+1&lt;M[i].length()){<br>                char d = M[i].charAt(j+1);<br>                add(c,d);<br>            }<br>        }<br>        String sp = "xcvbnm";<br>        for(char c:sp.toCharArray()){<br>            D[c][' ']=D[' '][c]= 3;<br>        }<br>        for(int k=0;k&lt;D.length;k++)for(i=0;i&lt;D.length;i++)for(j=0;j&lt;D.length;j++){<br>            if(D[i][j]&gt;D[i][k]+D[k][j])D[i][j]=D[i][k]+D[k][j];<br>        }<br>        for(i=0;i&lt;Mon.length;i++)for(j=1;j&lt;=Days[i];j++){<br>            String s = Mon[i]+" "+j;<br>            if(s.length()!=input.length())continue;<br>            int dif = diff(input,s.toLowerCase());<br>            if(dif&lt;best){<br>                res = s;<br>                best =dif;<br>            }<br>        }<br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>