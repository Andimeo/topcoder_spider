            using System;<br>using System.Collections;<br>using System.Text;<br>public class MixtureDensity {<br>  public double getDensity(string[] ing) {<br>    double mas=0;<br>    double vol=0;<br>    for (int i=0;i&lt;ing.Length;i++)<br>    {<br>      int j=0;<br>      string temp="";<br>      while (ing[i][j]!=' ')<br>      {<br>        <br>        temp+=ing[i][j];<br>        j++;<br>      }<br>      j++;<br>      while ((ing[i][j]!=','))<br>      j++;<br>      j++;<br>      while ((ing[i][j]!=' '))<br>      j++;<br>      j++;<br>            while ((ing[i][j]!=' '))<br>      j++;<br>      j++;<br> <br>      Console.WriteLine("!!! "+temp);<br>      int curvol=Convert.ToInt32(temp);<br>      temp="";<br>      <br>      while (ing[i][j]!=' ')<br>      {<br>        temp+=ing[i][j];<br>        j++;<br>      }<br>      <br>      Console.WriteLine("!!! "+temp);<br>      int curmas=Convert.ToInt32(temp);<br>      mas+=curmas;<br>      vol+=curvol;<br>    }<br>    return ((double)(mas))/((double)(vol));<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>