            public class MixtureDensity {<br>  public double getDensity (String [] ing) {<br>    int v = 0;<br>    int m = 0;<br>    for (int i = 0; i&lt;ing.length; ++i) {<br>      String [] b = ing[i].split("\\s");<br>      m += Integer.parseInt(b[b.length-2]);<br>      v += Integer.parseInt(b[0]);<br>    }<br>    return (double)m/v;<br>  <br>  }<br>}<br></td>