            using System;<br><br>public class Unjumpers {<br>  public int reachableTargets(string start, string[] targets) {<br>        int z = estimate(start);<br>        int res = 0;<br>        foreach (string x in targets)<br>            if (estimate(x) == z)<br>                ++res;<br>        return res;<br>    }<br><br>    int estimate(string s)<br>    {<br>        if (s.IndexOf('*') &lt; 0)<br>            return 4;<br>        int[] cnt = new int[3];<br>        for (int i = 0; i &lt; s.Length; ++i)<br>            if (s[i] == '*')<br>                ++cnt[i % 3];<br>        while (true)<br>        {<br>            for (int i = 0; i &lt; 3; ++i)<br>                while (cnt[i] &gt; 2)<br>                    cnt[i] -= 2;<br><br>            for (int i = 0; i &lt; 3; ++i)<br>            {<br>                int j = (i + 1) % 3;<br>                int k = (j + 1) % 3;<br>                if (cnt[j] == 0 &amp;&amp; cnt[k] == 0)<br>                {<br>                    if (cnt[i] == 1)<br>                        return i;<br>                    else<br>                        return 3;<br>                }<br>                else if (cnt[j] &gt; 0 &amp;&amp; cnt[k] &gt; 0)<br>                {<br>                    --cnt[j];<br>                    --cnt[k];<br>                    ++cnt[i];<br>                }<br>            }<br>        }<br>    }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>