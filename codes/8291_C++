<td class="problemText" colspan="8" valign="middle" align="left">
            #include &lt;string&gt;<br>#include &lt;vector&gt;<br><br>using namespace std;<br><br>class Transformation<br>{<br>  long long gcd(long long a, long long b)<br>  {<br>    return b ? gcd(b, a%b) : a;<br>  }<br>  long long a[64];<br>public:<br>  vector &lt;string&gt; transform(vector &lt;string&gt; v)<br>  {<br>    int n = v.size();<br>    int i, j;<br>    for (i=0; i&lt;n; i++)<br>      sscanf(v[i].c_str(), "%lld", &amp;a[i]);<br>      <br>    for (i=n-1; i&gt;=0; i--)<br>      for (j=0; j&lt;i; j++)<br>      {<br>        long long d = gcd(a[i], a[j]);<br>        long long c = d;<br>        for (;;)<br>        {<br>          long long x = gcd(d, a[j]/d);<br>          if (x == 1)<br>            break;<br>          d /= x;<br>        }<br>        a[j] /= d;<br>        a[i] /= c/d;<br>      }<br>    <br>    char s[50];<br>    vector &lt;string&gt; res;<br>    for (i=0; i&lt;n; i++)<br>    {<br>      sprintf(s, "%lld", a[i]);<br>      res.push_back(s);<br>    }<br>    return res;<br>  }<br>};<br></td>