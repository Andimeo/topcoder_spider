            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class FingerCounting <br>{<br>  public int maxNumber(int weakFinger, int maxCount) <br>  {<br>        int a = 0;<br>        bool nov = true;<br>        int ret = 0;<br>        while (maxCount &gt;= 0)<br>        {<br>            if (nov)<br>                a++;<br>            else<br>                a--;<br>            if (a == 5)<br>            {<br>                nov = false;<br>            }<br>            if (a == 1)<br>                nov = true;<br>            if (weakFinger == a)<br>                maxCount--;<br>            ret++;<br>        }<br>        return ret-1;<br>  }<br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by r1cs1]<br>// Powered by CodeProcessor<br></td>