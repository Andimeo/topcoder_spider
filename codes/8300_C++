            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class FingerCounting {<br>public:<br>  int maxNumber(int, int);<br>};<br> <br>int FingerCounting::maxNumber(int weakFinger, int maxCount) {<br>  int x=1,y=0,a=1;<br>  while(1)<br>  {<br>  if(x==weakFinger&amp;&amp;maxCount==0)break;<br> <br>  if(x==weakFinger)maxCount--;<br>  x+=a;<br>  y++;<br>  if(x==5)a=-a;<br>  if(x==1)a=-a;<br>  }<br>  return y;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>