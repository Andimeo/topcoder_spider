            using System;<br>using System.Collections.Generic;<br><br>class ConsecutiveNumbers<br>    {<br>        public int[] missingNumber(int[] numbers)<br>        {<br>            Array.Sort(numbers);<br>            List&lt;int&gt; ret = new List&lt;int&gt;();<br>            for (int i = 1; i &lt; numbers.Length; i++)<br>            {<br>                int dif = numbers[i] - numbers[i - 1];<br>                if (dif == 1) continue;<br>                else if (dif == 2) ret.Add(numbers[i - 1] + 1);<br>                else return new int[0];<br>            }<br>            if (ret.Count == 0)<br>            {<br>                if (numbers[0] != 1)<br>                    ret.Add(numbers[0] - 1);<br>                ret.Add(numbers[numbers.Length - 1] + 1);<br>            }<br>            else if (ret.Count &gt; 1) return new int[0];<br>            return ret.ToArray();<br>        }<br>    }<br></td>