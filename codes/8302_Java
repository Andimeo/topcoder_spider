<td class="problemText" colspan="8" valign="middle" align="left">
            import java.util.*;<br><br>public class ConsecutiveNumbers {<br>    public int[] missingNumber(int[] x) {<br>        int n, m = 0, y = 0;<br>        n = x.length;<br>        Arrays.sort(x);<br>        for(int i = 0; i &lt; n - 1; i++) {<br>            if(x[i+1] - x[i] != 1) {<br>                m+=x[i+1] - x[i] == 2? 1 : 5;<br>                y = x[i] + 1;<br>            }<br>        }<br>        if(m &gt; 1) return new int[]{};<br>        if(m == 1) return new int[]{y};<br>        if(x[0] == 1) return new int[]{x[n-1] + 1};<br>        return new int[]{x[0] - 1, x[n-1] + 1};<br>    }<br>}<br></td>