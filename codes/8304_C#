            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class TwoRotationCypher<br>{<br>  public string encrypt(int firstSize, int firstRotate, int secondRotate, string message)<br>  {<br>    char[] wyn =  new char[message.Length];<br>    for(int i=0; i&lt;message.Length; i++)<br>    {<br>    if(message[i]==' ')<br>    {<br>      wyn[i]=' ';<br>      continue;<br>      }<br>    if(message[i] &lt; (char)((int)'a'+firstSize))<br>    {<br>    wyn[i]=(char)(((int)message[i] - 97 + firstRotate) % firstSize + 97);<br>    <br>    <br>    }else<br>    {<br>    wyn[i]=(char)(((int)message[i] - 97 + secondRotate - firstSize) % (26-firstSize) + 97 + firstSize);<br>    <br>    }<br>      <br>    <br>    }<br>    return new string(wyn);<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>