            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class WordAbbreviation {<br>public:<br>  vector &lt;string&gt; getAbbreviations(vector &lt;string&gt;);<br>};<br> <br>vector &lt;string&gt; WordAbbreviation::getAbbreviations(vector &lt;string&gt; w) {<br>vector&lt;string&gt; a;<br>  for (int  i = 0; i&lt; w.size(); ++i) {<br>    int k = 1;<br>    for (int j = 0; j &lt; w.size(); ++j) {<br>      if (i != j ) {<br>        while (w[i].substr(0,k)==w[j].substr(0,k))<br>        ++k;<br>      }<br>    }<br>    a.push_back(w[i].substr(0,k));<br>  }<br>  return a;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>