            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import static java.lang.Math.abs;<br>import static java.lang.Math.max;<br>import static java.lang.Math.min;<br> <br>public class WordAbbreviation {<br>  static double ESP = 1e-10;<br>  public String[] getAbbreviations(String[] w) {<br>    String[] a = new String[w.length];<br>    for (int i = 0; i &lt; w.length; i++) {<br>      String abb = new String(String.valueOf(w[i].charAt(0)));<br>      int pos = 1;<br>      while (true) {<br>        boolean found = false;<br>        for (int j = 0; j &lt; w.length; j++)<br>          if (j != i)<br>            if (w[j].indexOf(abb) == 0) {<br>              found = true;<br>              break;<br>            }<br>        if (found)<br>          abb += String.valueOf(w[i].charAt(pos++));<br>        else<br>          break;<br>      }<br>      a[i] = abb;<br>    }<br>    return a;<br>  }<br>  <br>}<br></td>