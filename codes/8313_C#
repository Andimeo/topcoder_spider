            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class DepositProfit <br>{<br>  public int depositTerm(int amount, int annualInterest, int profit) <br>  {<br>        double amugynovel = (double)annualInterest / (double)1200;<br>        double mostpenz = (double)amount;<br>        int honap = 0;<br>        profit += amount;<br>        profit *= 100;<br>        mostpenz *= 100;<br>        if (mostpenz &gt;= profit)<br>            return honap;<br>        while (true)<br>        {<br>            honap++;<br>            mostpenz =mostpenz + (int)(mostpenz * amugynovel);<br>            if (mostpenz &gt;= profit)<br>            {<br>                return honap;<br>            }<br>        }<br>  }<br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by r1cs1]<br>// Powered by CodeProcessor<br></td>