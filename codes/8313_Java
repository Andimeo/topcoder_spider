            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class DepositProfit<br>{<br>  public int depositTerm(int amount, int annualInterest, int profit)<br>  {<br>    double epsilon = 0.0000001;<br>    double aI = annualInterest/(double)100;<br>    amount*=100;<br>    int nMonths=0;<br>    int curAmount=amount;<br>    while(curAmount-amount&lt;profit*100){<br>      curAmount = (int)(curAmount*(1+(double)aI/12)+epsilon);<br>      nMonths++;<br>    }<br>    return nMonths;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>