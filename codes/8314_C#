            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class DreamingAboutCarrots<br>{<br> <br>  int gcd(int m, int n)<br>  {<br>    if(n == 0) return m;<br>    if( m%n == 0) return n;<br>    return gcd(n, m%n);<br>  }<br>  <br>  public int carrotsBetweenCarrots(int x1, int y1, int x2, int y2)<br>  {<br>    return gcd(Math.Abs(x1-x2),Math.Abs(y1-y2) ) -1;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>