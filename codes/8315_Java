            import java.util.*;<br><br>public class ParticleCollision {<br>       public double[] collision(int vx, int vy, int vz, int x0, int y0, int z0) {<br>         <br>         double eps = 1e-9;<br>         <br>             long a = vx * vx + vy * vy;<br>             long b = 2 * vx * x0 + 2 * vy * y0;<br>             long c = x0 * x0 + y0* y0 - 1;<br>             long d = b * b - 4 * a * c;<br>             if (a == 0) {<br>               if (x0 * x0 + y0 * y0 == 1) {<br>                 if (vz != 0) {<br>                   return new double[] {0, 0, 0};<br>                 } else {<br>                   if (Math.abs(x0 - Math.cos(Math.PI * z0)) &lt; eps) {<br>                     return new double[] {x0, y0, z0};<br>                   } else {<br>                     return new double[] {};<br>                   }<br>                 }<br>               } else {<br>                 return new double[] {};<br>               }<br>             }<br>             <br>             if (d &lt; 0) {<br>               return new double[] {};<br>             }<br>             <br>             double t1 = (-b + Math.sqrt(d)) / (2 * a);<br>             double t2 = (-b - Math.sqrt(d)) / (2 * a);<br>             <br>             double x1 = x0 + vx * t1;<br>             double y1 = y0 + vy * t1;<br>             double z1 = z0 + vz * t1;<br>             boolean ok1 = (Math.abs(x1 - Math.cos(Math.PI * z1)) &lt; eps) &amp;&amp; (Math.abs(y1 - Math.sin(Math.PI * z1)) &lt; eps);<br>         <br>             double x2 = x0 + vx * t2;<br>             double y2 = y0 + vy * t2;<br>             double z2 = z0 + vz * t2;<br>             boolean ok2 = (Math.abs(x2 - Math.cos(Math.PI * z2)) &lt; eps) &amp;&amp; (Math.abs(y2 - Math.sin(Math.PI * z2)) &lt; eps);<br>             <br>             if (ok1 &amp;&amp; ok2 &amp;&amp; d != 0) {<br>               return new double[] {0, 0, 0};<br>             }<br>             <br>             if (ok1) {<br>               return new double[] {x1, y1, z1};<br>             }<br>             <br>             if (ok2) {<br>               return new double[] {x2, y2, z2};<br>             }<br>             <br>               return new double[] {};<br>       }<br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>