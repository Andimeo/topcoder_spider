            using System;<br><br>public class CharmingTickets {<br>    const int MOD = 999983;<br><br>  public int count(int K, string good) {<br>        int n1;<br>        int n2;<br>        if (K % 2 == 0) {<br>            n1 = K / 2;<br>            n2 = K / 2;<br>        } else {<br>            n1 = K / 2 + 1;<br>            n2 = K / 2;<br>        }<br>        int[,] num = new int[K + 1, 9 * K + 1];<br>        num[0, 0] = 1;<br>        for (int i = 0; i &lt; K; ++i)<br>            for (int j = 0; j &lt; 9 * i + 1; ++j)<br>            {<br>                foreach (char ch in good) {<br>                    num[i + 1, j + ch - '0'] += num[i, j];<br>                    if (num[i + 1, j + ch - '0'] &gt;= MOD)<br>                    {<br>                        num[i + 1, j + ch - '0'] -= MOD;<br>                    }<br>                }<br>            }<br>        int res = 0;<br>        for (int s = 0; s &lt; 9 * K + 1; ++s)<br>        {<br>            res = (int) ((res + (long) num[K, s] * num[K, s]) % MOD);<br>        }<br>        int r1 = 0;<br>        for (int s = 0; s &lt; 9 * n1 + 1; ++s)<br>        {<br>            r1 = (int) ((r1 + (long) num[n1, s] * num[n1, s]) % MOD);<br>        }<br>        int r2 = 0;<br>        for (int s = 0; s &lt; 9 * n2 + 1; ++s)<br>        {<br>            r2 = (int) ((r2 + (long) num[n2, s] * num[n2, s]) % MOD);<br>        }<br>        return (int) (((2 * res - (long) r1 * r2) % MOD + MOD) % MOD);<br>    }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>