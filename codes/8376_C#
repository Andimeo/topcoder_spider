            using System;<br>using System.Collections.Generic;<br>class CreateGroups {<br>  public int minChanges(int[] groups, int minSize, int maxSize) {<br>    int total = 0;<br>    foreach (int n in groups)<br>      total += n;<br>    if (total &lt; groups.Length * minSize || total &gt; groups.Length * maxSize)<br>      return -1;<br>    int over = 0, under = 0;<br>    foreach (int n in groups)<br>    {<br>      if (n &lt; minSize) under += minSize - n;<br>      if (n &gt; maxSize) over += n - maxSize;<br>    }<br>    return Math.Max(over, under);<br>  }<br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>