            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br>using namespace std;<br>#define FOR(i,a,b) for(int i=(a);i&lt;=(b);++i)<br>#define FORD(i,a,b) for(int i=(a);i&gt;=(b);--i)<br>#define REP(i,n) FOR(i,0,(n)-1)<br>#define FS(i,v) for(__typeof((v).begin())i=(v).begin();i!=v.end();++(i))<br>#define ALL(a) (a).begin(),(a).end()<br>#define SZ(a) (int)((a).size())<br>#define FI first<br>#define SE second<br>typedef long long ll;<br>typedef long double ldouble;<br> <br>class TrueStatements {<br>public:<br>  int numberTrue(vector &lt;int&gt; s) {<br>    for(int i = 1000; i &gt;= 0; --i) {<br>      int ile = 0;<br>      for(int j = 0; j &lt; SZ(s); ++j) if(s[j] == i) ++ile;<br>      if(ile == i) return i;<br>    }<br>    return -1;<br>  }<br>};<br></td>