            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class LuckyTicketSubstring <br>{<br>  public int maxLength(string s) <br>    {<br>        int res = 0;<br>        for (int i = 0; i &lt; s.Length; i++)<br>        {<br>            for (int j = i + 1; j &lt; s.Length; j++)<br>            {<br><br>                string str = s.Substring(i, j - i + 1);<br><br>                if(str.Length % 2 == 1)<br>                    continue;<br><br>                int left = 0, right = 0;<br>                for (int k = 0; k &lt; str.Length / 2; k++)<br>                {<br>                    left += str[k] - '0';<br>                }<br><br>                for (int k = str.Length / 2; k &lt; str.Length; k++)<br>                {<br>                    right += str[k] - '0';<br>                }<br><br>                if (left == right)<br>                {<br>                    if (str.Length &gt; res)<br>                        res = str.Length;<br>                }<br>            }<br>        }<br><br>        return res;<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>