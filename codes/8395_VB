            Public Class LuckyTicketSubstring<br>Public Function maxLength(ByVal s As String) As Integer<br>        Dim duzina, k As Integer<br>        Dim max As Integer = 0<br>        Dim l As Integer = 0<br>        Dim r As Integer = 0<br>        Dim count As Integer = 0<br><br><br>        duzina = 2<br>        For k = 0 To s.Length - 1<br>            For count = 0 To s.Length - 1<br>                If k - count &lt; 0 Or k + count + 1 &gt; s.Length - 1 Then<br>                    l = 0<br>                    r = 0<br>                    duzina = 2<br>                    Exit For<br>                End If<br><br>                l += Val(s(k - count))<br>                r += Val(s(k + count + 1))<br><br>                If l = r Then<br>                    If max &lt; duzina Then<br>                        max = duzina<br>                    End If<br>                End If<br>                duzina += 2<br>            Next<br>        Next<br><br>        Return max<br>    End Function<br>End Class<br></td>