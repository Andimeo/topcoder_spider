            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class Planks<br>{<br>    public int makeSimilar(int[] lengths, int costPerCut, int woodValue)<br>    {<br>        int max = 0;<br>        for (int i = 1; i &lt;= 10000; i++)<br>        {<br>            int deze = 0;<br>            for (int j = 0; j &lt; lengths.Length; j++)<br>            {<br>                int parts = lengths[j] / i;<br>                int cuts = parts;<br>                if (cuts * i == lengths[j]) cuts--;<br>                int win = parts * i * woodValue - costPerCut * cuts;<br>                if (win &gt; 0) deze += win;<br>            }<br>            max = Math.Max(deze, max);<br>        }<br>        return max;<br>    }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>