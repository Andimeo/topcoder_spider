<td class="problemText" colspan="8" valign="middle" align="left">
            import java.lang.Math;<br><br>public class Planks {<br>  public int makeSimilar(int[] lengths, int costPerCut, int woodValue) {<br>    int maxProfit = 0;<br>    for (int L = 1; L &lt;= 10000; L++) {<br>      int profit = 0;<br>      for (int i = 0; i &lt; lengths.length; i++) {<br>        int K = (int)Math.floor(lengths[i] / L);<br>        int cost = (lengths[i] % L == 0?(K-1):K) * costPerCut;<br>        int value = K * L * woodValue;<br>        if (value - cost &gt; 0.0) <br>          profit += value - cost;<br>      }<br>      maxProfit = Math.max(maxProfit, profit);<br>    }<br>    return maxProfit;<br>  }<br>}<br></td>