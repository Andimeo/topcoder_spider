            #include &lt;algorithm&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstring&gt;<br>#include &lt;iostream&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;sstream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>using namespace std;<br> <br>class FloorLayout {<br>public:<br>  int countBoards(vector &lt;string&gt; layout);<br>};<br> <br>int FloorLayout::countBoards(vector &lt;string&gt; l)<br>{<br>  int res=0;<br>  int n=l.size();<br>  int m=l[0].size();<br>  for(int i=0;i&lt;n;i++)<br>  for(int j=0;j&lt;m;j++)<br>  {<br>    if(l[i][j]=='-')<br>    {<br>      if(j==0 || l[i][j-1]!='-')<br>      res++;<br>    }<br>    else<br>    {<br>      if(i==0 || l[i-1][j]!='|')<br>      res++;<br>    }<br>  }<br> <br>  return res;<br>}<br> <br>// Powered by MBMagic 0.12.2<br>// Powered by PopsEdit<br></td>