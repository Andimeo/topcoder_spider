            public class DownloadingFiles<br>    {<br>        public double actualTime(string[] tasks)<br>        {<br>            int iTotalSpeed = 0, iTotalSizeLeft = 0;<br><br>            for (int i = 0; i &lt; tasks.Length; i++)<br>            {<br>                string[] sSplit = tasks[i].Split(' ');<br>                //objTaskList.Add(new int[] { int.Parse(sSplit[0]), int.Parse(sSplit[1]) });<br><br>                iTotalSpeed += int.Parse(sSplit[0]);<br>                iTotalSizeLeft += int.Parse(sSplit[0]) * int.Parse(sSplit[1]);<br>            }<br><br>            return iTotalSizeLeft / (double)iTotalSpeed;<br>        }<br>    }<br></td>