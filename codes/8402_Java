            public class GraphClique {<br>  public int[] allClique(String[] graphEdge) {<br>    int n = graphEdge.length;<br>    boolean[][] arr = new boolean[n][n];<br>    for (int i = 0; i &lt; n; i++) {<br>      for (int j = 0; j &lt; n; j++) {<br>        if (graphEdge[i].charAt(j) == '1') {<br>          arr[i][j] = true;<br>        }<br>      }<br>    }<br>    <br>    int res[] = new int[n];<br>    <br>    for (int state = 0; state &lt; (1&lt;&lt;n); state++) {<br>      int x = 0;<br>      <br>      boolean b = true;<br>      outer:<br>      for (int i = 0; i &lt; n; i++) {<br>        if ((state &amp; (1&lt;&lt;i)) &gt; 0) {<br>          x++;<br>          for (int j = 0; j &lt; n; j++) if (i!=j) {<br>            if ((state &amp; (1&lt;&lt;j)) &gt; 0) {<br>              if (!arr[i][j]) {<br>                b = false;<br>                break outer;<br>              }<br>            }<br>          }<br>        }<br>      }<br>      if (x != 0 &amp;&amp; b) {<br>        res[x-1]++;<br>      }<br>    }<br>    return res;<br>  }<br>}<br></td>