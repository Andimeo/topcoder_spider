            #include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cassert&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;utility&gt;<br>#include &lt;numeric&gt;<br>#include &lt;iterator&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;list&gt;<br>#include &lt;queue&gt;<br>#include &lt;stack&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>using namespace std;<br><br>typedef vector&lt;int&gt; VI;<br>typedef vector&lt;VI&gt; VVI;<br>typedef vector&lt;string&gt; VS;<br>typedef long long int i64;<br>typedef pair&lt;int, int&gt; PII;<br><br>#define FST first<br>#define SND second<br><br>class TheBestName {<br>  public:<br>    VS sort(VS nm);<br>};<br><br>int summa(const string &amp;ss) {<br>  int i, L=ss.length(), A=0;<br>  for (i=0; i&lt;L; i++)<br>    A+=ss[i]-'A'+1;<br>  return A;<br>}<br><br>bool myComp(const string &amp;s1, const string &amp;s2) {<br>  if (s1=="JOHN")<br>    return 1;<br>  if (s2=="JOHN")<br>    return 0;<br>  int z1=summa(s1);<br>  int z2=summa(s2);<br>  if (z1!=z2)<br>    return z1&gt;z2;<br>  return s1&lt;s2;<br>}<br><br>VS TheBestName::sort(VS nm) {<br>  std::sort(nm.begin(), nm.end(), myComp);<br>  return nm;<br>}<br></td>