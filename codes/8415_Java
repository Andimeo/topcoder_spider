            import java.util.*;<br> <br>public class TheHomework {<br>    int[][][] memo;<br> <br>    int find(int a, int b, int c) {<br>        if (a == 0 &amp;&amp; b == 0) {<br>            return 0;<br>        }<br>        if (memo[a][b][c] != 0) {<br>            return memo[a][b][c];<br>        }<br> <br>        int r = Integer.MAX_VALUE;<br>        for (int i = 1; i &lt;= b &amp;&amp; i &lt;= a + c; i++) {<br>            r = Math.min(r, 1 + find(a, b - i, c + i));<br>        }<br>        for (int i = 1; i &lt;= a &amp;&amp; 2 * i &lt;= a + c; i++) {<br>            r = Math.min(r, 1 + find(a - i, b, c));<br>        }<br>        for (int i = 1; i &lt;= a &amp;&amp; i &lt;= b &amp;&amp; 2 * i &lt;= a + c; i++) {<br>            r = Math.min(r, 1 + find(a - i, b - i, c + i));<br>        }<br>        return memo[a][b][c] = r;<br>    }<br> <br>    public int transform(int[] first, int[] second) {<br>        Arrays.sort(first);<br>        Arrays.sort(second);<br>        int m = first.length;<br>        int n = second.length;<br>        int c = 0;<br> <br>        int j = 0;<br>        for (int i = 0; i &lt; m; i++) {<br>            while (j &lt; n &amp;&amp; second[j] &lt; first[i]) {<br>                j++;<br>            }<br>            if (j &lt; n &amp;&amp; second[j] == first[i]) {<br>                c++;<br>                j++;<br>            }<br>        }<br> <br>        int a = m - c;<br>        int b = n - c;<br> <br>        memo = new int[101][101][101];<br>        return find(a, b, c);<br>    }<br> <br> <br>}<br> <br> <br>// Powered by PopsEdit<br> <br>// Powered by CodeProcessor<br></td>