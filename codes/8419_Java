            public class TheNumbersLord{<br><br>  public String create(int[] numbers, int n){<br>    int[] mas=new int[n];<br>    for (int i=0; i&lt;numbers.length; i++){<br>      mas[i]=numbers[i];<br>    }<br>    int maks=max(numbers);<br>    for (int k=numbers.length; k&lt;n; k++){<br>      mas[k]=maks;<br>    }<br>    for (int j=0; j&lt;n; j++){<br>      for (int m=0; m&lt;n-1; m++){<br>        String x=buildstring(mas,n);<br>        int swap=mas[m];<br>        mas[m]=mas[m+1];<br>        mas[m+1]=swap;<br>        String y=buildstring(mas,n);<br>        if (x.compareTo(y)&gt;0){<br>          swap=mas[m];<br>          mas[m]=mas[m+1];<br>          mas[m+1]=swap;<br>        }<br>      }<br>    }<br>    return buildstring(mas,n);<br>  }  <br>  <br>  public String buildstring(int[] from, int n){<br>    String res="";<br>    for(int i=0; i&lt;n; i++){<br>      res=res+from[i];<br>    }<br>    return res;<br>  }<br>  <br>  public int max(int[] number){<br>    int curmax=0;<br>    for(int i=0; i&lt;number.length; i++){<br>      if (number[i]&gt;curmax) curmax=number[i];<br>    }<br>    return curmax;<br>  }<br>}<br></td>