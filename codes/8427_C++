            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;string&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br>#include &lt;algorithm&gt;<br>using namespace std;<br>typedef long long LONG;<br>typedef vector &lt;int&gt; VI;<br>typedef vector &lt;double&gt; VD;<br>typedef vector &lt;string&gt; VS;<br>VI a,b;<br><br>class FactoVisors<br>{<br>public:<br>  bool Valid(int x){<br>    int i;<br>    for(i=0;i&lt;a.size();i++){<br>      if(x%a[i]){<br>        return false;<br>      }<br>    }<br>    for(i=0;i&lt;b.size();i++){<br>      if(b[i]%x){<br>        return false;<br>      }<br>    }<br>    return true;<br>  }<br>  int getNum(vector &lt;int&gt; divi, vector &lt;int&gt; mult)<br>  {<br>    int mx=0,i,j,ans;<br>    a=divi;<br>    b=mult;<br>    for(i=0;i&lt;mult.size();i++){<br>      mx=max(mx,mult[i]);<br>    }<br>    ans=0;<br>    for(i=1;i*i&lt;=mx;i++){<br>      if(mx%i==0){<br>        if(Valid(i)){<br>          ans++;<br>        }<br>        if(i!=mx/i &amp;&amp; Valid(mx/i)){<br>          ans++;<br>        }<br>      }<br>    }<br>    return ans;<br>  }<br>};<br></td>