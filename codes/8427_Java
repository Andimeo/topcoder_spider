            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class FactoVisors<br>{<br>  public int getNum(int[] d, int[] m)<br>  {<br>    <br>     Arrays.sort(m);<br>      <br>      int ans = 0;<br>    int ch = m[0];<br>    int sq = (int)Math.round(Math.sqrt(ch));<br>    double plus = 0;<br>    if (sq*sq==ch) plus = 0.5;<br>      <br>    for(int div=1;div&lt;(Math.sqrt(ch)+plus);div++)<br>    if (ch%div==0)<br>    {<br>    int wehave = div;<br>    int flag = 1;<br>      for(int i =1; i&lt;m.length;i++)<br>        if (m[i]%div!=0) { flag=0; break; }<br>      for(int i =0; i&lt;d.length;i++)<br>        if (div%d[i]!=0) { flag=0; break; }<br>        <br>      ans+=flag;<br>        <br>    wehave = ch/div;<br>    if (wehave!=div)<br>    {<br>    flag = 1;<br>      for(int i =1; i&lt;m.length;i++)<br>        if (m[i]%wehave!=0) { flag=0; break; }<br>      for(int i =0; i&lt;d.length;i++)<br>        if (wehave%d[i]!=0) { flag=0; break; }    <br>      ans+=flag;        <br>    }    <br>      }<br>    return ans;<br>        <br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>