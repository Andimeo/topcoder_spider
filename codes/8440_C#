            using System;<br><br>class SellingProducts<br>{<br><br>  struct customer:IComparable<br>  {<br>    public int price;<br>    public int cost;<br>    <br>    public int CompareTo(object o)<br>    {<br>      customer c = (customer)o;<br>      return c.price - price;<br>    }<br>  }<br><br>  public int optimalPrice(int[] price, int[] cost)<br>  {<br>    int[] p = new int[price.Length]; //profits<br>    customer[] c= new customer[price.Length];<br>    int i,j,k;<br>    <br>    for(i=0;i&lt;price.Length;i++)<br>    {<br>      c[i].price = price[i];<br>      c[i].cost = cost[i];<br>    }<br>    Array.Sort(c);<br>    <br>    int best = 0;<br>    ulong bestp = 0;<br>    int cur;<br>    ulong curp;<br>    for (i=0;i&lt;price.Length;i++)<br>    {<br>      cur = c[i].price;<br>      curp = 0;<br>      for (j=0;j&lt;=i;j++)<br>      {<br>        if (cur - c[j].cost &gt;0) curp += (ulong)(cur - c[j].cost);<br>      }<br>      if (curp&gt;=bestp)<br>      {<br>        if (curp!=0)<br><br>{        best = cur;<br>        bestp=curp;}<br>      }<br>    }<br>    return best;<br>  }<br>}<br></td>