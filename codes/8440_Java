            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class SellingProducts<br>{<br>  public int optimalPrice(int[] price, int[] cost)<br>  {<br>    int bestProfit = -1;<br>    int bestPrice = -1;<br>    for(int i = 0; i &lt; price.length;i++){<br>      int totalProfit = 0;<br>      for(int j = 0; j &lt; cost.length;j++){<br>        if(price[j] &lt; price[i]) continue;<br>        if(price[i] - cost[j] &lt;= 0) continue;<br>        //System.out.println(i+" "+j);<br>        totalProfit += price[i]-cost[j];<br>      <br>      }<br>      if(totalProfit == 0) continue;<br>      if(totalProfit &gt; bestProfit){ bestProfit = totalProfit;bestPrice=price[i];}<br>      if(totalProfit == bestProfit &amp;&amp; price[i] &lt; bestPrice){<br>        bestProfit = totalProfit; bestPrice = price[i];<br>      }<br>    }  <br>    if(bestPrice &gt; 0) return bestPrice;<br>    else return 0;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>