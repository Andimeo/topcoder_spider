            using System;          <br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br>using System.Diagnostics;<br><br>public class FillBox {<br>  public int minCubes(long  length, long  width, long height, int[] CC) {<br>        int n = CC.Length;<br>        long[] C = new long[n];<br>        long[] D = new long[n];<br>        for (int i = 0; i &lt; n; i++) C[i] = CC[i];<br>        for (int i = 0; i &lt; n; i++) D[i] = CC[i];<br><br>        long[]P = new long[n];<br>        for (int i = 0; i &lt; P.Length; i++) P[i] = 1 &lt;&lt; i;<br>        <br>        for (int j = n - 1; j &gt;= 0; j--) {<br>            long sz = 1 &lt;&lt; j;<br>            long a = length / sz;<br>            long b = width / sz;<br>            long c = height / sz;<br><br>            long rem = C[j] - a * b * c;<br>            C[j]=Math.Min(C[j],a*b*c);<br>            if (rem &gt; 0) D[j] -= rem;<br><br>            if (j &gt;= 1) {<br>                C[j - 1] = C[j - 1] + 8 * C[j];<br>                D[j - 1] = D[j - 1] + D[j];<br>            }<br>        }<br>        if (C[0] == length * width * height)<br>            return (int)D[0];<br>        else<br>            return -1;<br>  }<br>    #region TESTCODE<br><br>     <br>#endregion    <br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>