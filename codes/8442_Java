            import java.util.*;<br><br>public class FillBox {<br>  public int minCubes(int length, int width, int height, int[] cubes) {<br>    long done = 0;<br>    long l = length;<br>    long w = width;<br>    long h = height;<br>    <br>    int res = 0;<br>    <br>    for (int i = cubes.length - 1; i &gt;= 0; i--) {<br>      long len = 1L &lt;&lt; i;<br>      <br>      long amount = (l / len) * (w / len) * (h / len);<br>      amount -= done;<br>      <br>      if (amount &gt; cubes[i]) {<br>        amount = cubes[i];<br>      }<br>      <br>      done = (done + amount) * 8;<br>      <br>      res += amount;<br>    }<br>    <br>    if (done &lt; 8 * l * w * h) {<br>      return -1;<br>    }<br>    <br>    return res;<br>  }<br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>