            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class OneTwoThree<br>{<br>    private bool IsPrime(int n)<br>    {<br>        if (n &lt; 4) { if (n == 1) return false; return true; }<br>        if ((n &amp; 1) == 0) return false;<br>        int g = (int)Math.Sqrt(n) + 1;<br>        for (int i = 3; i &lt; g; i += 2)<br>        { if (n % i == 0) return false; }<br>        return true;<br>    }<br>    long m = 987654321;<br>    public int minimalNumber(int n) <br>    {<br>        long r = 1;<br>        long N=n;<br>        for (int i = 2; i &lt;= n; i++)<br>        {<br>            if (IsPrime(i))<br>            {<br>                long g = i;<br>                while (g * i &lt;= N)<br>                {<br>                    g *= i;<br>                }<br>                r*= g;<br>                r %= m;<br>            }<br>        }<br>        return (int)r;<br>    }<br>    <br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by r1cs1]<br>// Powered by CodeProcessor<br></td>