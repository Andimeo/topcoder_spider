            class OneTwoThree {<br>  public:<br>    int tmp[100001];<br>    int minimalNumber(int n) {<br>      for (int i = 1; i &lt;= 100000; i ++)<br>        tmp[i] = 0;<br>      for (int i = 1; i &lt;= n; i ++) {<br>        int k = i;<br>        for (int j = 2; j * j &lt;= n; j ++) {<br>          int s = 0;<br>          while (k % j == 0) {<br>            s ++;<br>            k /= j;<br>            }<br>          tmp[j] &gt;?= s;<br>          }<br>        if (k &gt; 1)<br>          tmp[k] &gt;?= 1;<br>        }<br>      long long re = 1;<br>      for (int i = 1; i &lt;= 100000; i ++)<br>        for (int j = 0; j &lt; tmp[i]; j ++)<br>          re = re * i % 987654321;<br>      return (int) re;<br>      }<br>  };<br></td>