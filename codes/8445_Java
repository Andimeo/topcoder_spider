            //Written by Sergey Melnikov<br>import java.util.*;<br>import java.math.*;<br> <br>public class OneTwoThree {<br>  <br>    public int minimalNumber(int n) {<br>      boolean d[] = new boolean[n+1];<br>      Arrays.fill(d, true);<br>      d[1] = false;<br>      long res = 1;<br>      for (int i = 2; i &lt;= n; i++) {<br>      if (d[i]) {<br>        long p = i;<br>        while (p*i&lt;=n) {<br>          p*=i;<br>        }<br>        res = (res*p)%987654321;<br>        for (int j = 2*i; j &lt;=n ; j+=i) {<br>          d[j] = false;<br>        }<br>      }<br>    }<br>      return (int)(res%987654321);<br>    }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>