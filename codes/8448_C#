            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class GoodHours<br>{<br>    class XTime<br>    {<br>        public XTime(string str)<br>        {<br>            string[] v = str.Split(':');<br>            h = int.Parse(v[0]);<br>            m = int.Parse(v[1]);<br>        }<br>        public override bool Equals(object obj)<br>        {<br>            XTime rhs = (XTime)obj;<br>            return this.h == rhs.h &amp;&amp; this.m == rhs.m;<br>        }<br>        XTime()<br>        {<br>        }<br>        public static XTime GetNext(XTime now)<br>        {<br>            XTime rv = new XTime();<br>            rv.h = now.h;<br>            rv.m = now.m;<br>            rv.m++;<br>            if (rv.m == 60)<br>            {<br>                rv.m = 0;<br>                rv.h++;<br>                rv.h %= 24;<br>            }<br>            return rv;<br>        }<br>        public int IsGood()<br>        {<br>            string sh;<br>            if (h &lt; 10)<br>                sh = string.Format("0{0}", h);<br>            else<br>                sh = string.Format("{0}", h);<br>            string sm;<br>            if (m &lt; 10)<br>                sm = string.Format("0{0}", m);<br>            else<br>                sm = string.Format("{0}", m);<br>            string str = string.Format("{0}{1}", sh, sm);<br>            int[] f = new int[4];<br>            for (int i = 0; i &lt; 4; ++i)<br>                f[i] = str[i] - '0';<br>            if (f[0] == f[1] * f[2] * f[3] || f[0] * f[1] == f[2] * f[3] || f[0] * f[1] * f[2] == f[3])<br>                return 1;<br>            return 0;<br>        }<br>        public int h, m;<br>    }<br>    public int howMany(string beforeTime, string afterTime)<br>    {<br>        XTime now = new XTime(beforeTime);<br>        XTime end = new XTime(afterTime);<br>        int cnt = 0;<br>        do<br>        {<br>            cnt += now.IsGood();<br>            if (now.Equals(end))<br>                break;<br>            now = XTime.GetNext(now);<br>        } while (true);<br>        return cnt;<br>    }<br>}<br> <br>// Powered by PopsEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>