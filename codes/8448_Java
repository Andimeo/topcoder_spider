            public class GoodHours {<br>  public int howMany(String a, String b) {<br>    <br>    int ans = 0;<br>    while (true) {<br>      ans += isgood(a);<br>      if (a.equals(b)) {<br>        break;<br>      }<br>      a = inc(a);<br>    }<br>    return ans;<br>  }<br>  <br>  private String inc(String s) {<br>    String[] a = s.split(":");<br>    int aa = Integer.parseInt(a[0]);<br>    int bb = Integer.parseInt(a[1]);<br>    <br>    bb++;<br>    if (bb == 60) {<br>      bb = 0;<br>      aa++;<br>    }<br>    if (aa == 24) {<br>      aa = 0;<br>    }<br>    return f(aa) + ":" + f(bb);<br>  }<br>  <br>  private String f(int a) {<br>    if (a &gt; 9) {<br>      return Integer.toString(a);<br>    }<br>    return "0" + Integer.toString(a);<br>  }<br>  <br>  private int isgood(String s) {<br>    int a = s.charAt(0) - '0';<br>    int b = s.charAt(1) - '0';<br>    int c = s.charAt(3) - '0';<br>    int d = s.charAt(4) - '0';<br>    <br>    if (a == b * c * d || a * b == c * d || a * b * c == d) {<br>      return 1;<br>    }<br>    return 0;<br>  }<br>}<br></td>