            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class ContiguousSubsequences {<br>public int[] findMaxAverage(int[] a, int K) {<br>int[] res = new int[2];<br>int n = a.length;<br>double max = 0;<br>int maxL = 0;<br>for (int i = 0; i &lt; n; i++){<br>  for (int k = K; i + k &lt;= n; k++) {<br>    double t = 0;<br>    for (int b = 0; b &lt; k; b++){<br>      t += a[i+b];<br>    }<br>    t /= k;    <br>    if (t &gt; max){<br>      max = t;<br>      maxL = k;<br>      res[0] = i;<br>      res[1] = i+k-1;<br>    }<br>    else if (t == max &amp;&amp; k &gt; maxL){<br>      res[0] = i;<br>      res[1] = i+k-1;<br>      maxL = k;<br>    }<br>  }<br>}<br>return res;<br>}<br> <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>