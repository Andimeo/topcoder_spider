            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class SendingCards<br>{<br>  public int howMany(string[] friends)<br>  {<br>     int res=0;<br>     for(int i=0;i&lt;friends.Length;i++)<br>       for(int j=0;j&lt;friends.Length;j++)<br>       {<br>         if ((friends[i][j]=='Y')&amp;(friends[j][i]=='N'))<br>          res++;<br>       }<br>       return res;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>