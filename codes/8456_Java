            public class SendingCards {<br>  public int howMany(String[] friends) {<br>    int res = 0;<br>    for (int i = 0; i &lt; friends.length; i++) {<br>      for (int j = 0; j &lt; friends[i].length(); j++) {<br>        if (friends[i].charAt(j) == 'Y' &amp;&amp; friends[j].charAt(i) == 'N') {<br>          res++;<br>        }<br>      }<br>    }<br>    return res;<br>  }<br>}<br></td>