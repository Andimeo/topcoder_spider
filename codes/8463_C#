            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class TriviaGame<br>{<br>  public int maximumScore(int[] points, int tokensNeeded, int[] bonuses)<br>  {<br>    int[,] dp=new int[51,50];<br>    for (int i=1; i&lt;=points.Length; ++i) {<br>      for (int t=0; t&lt;tokensNeeded; ++t) {<br>        int a=points[points.Length-i]+dp[i-1,(t+1)%tokensNeeded];<br>        if (t+1==tokensNeeded)<br>          a+=bonuses[points.Length-i];<br>        int b=dp[i-1,t]-points[points.Length-i];<br>        dp[i,t]=Math.Max(a, b);<br>      }<br>    }<br>    return dp[points.Length,0];<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>