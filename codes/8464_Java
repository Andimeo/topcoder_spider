            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class StreetWalking<br>{<br>  public long minTime(int X, int Y, int wt, int st)<br>  {<br>    long min = Math.min( X, Y);<br>    long res = 0;<br>    res += Math.min( min * wt * 2, min * st);<br>    long thua = X+Y - 2*min;<br>    if(thua%2==1) {<br>      res += wt;<br>      --thua;<br>    }<br>    res += Math.min( wt * thua, st * thua);<br>    return res;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>