            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class InformFriends <br>{<br>  const int INF = 987654321;<br>    int n;<br>    string[] f;<br><br>    int[] g;<br>    int[] a, b;<br><br>    int calc(int mask)<br>    {<br>        if (mask == 0) return 0;<br>        if (g[mask] &gt; -1) return g[mask];<br>        int res = 0;<br><br>        for (int i = mask; i &gt; 0; i = (i - 1) &amp; mask)<br>        {<br>            // empty set not considered<br>            if (b[i] == (1 &lt;&lt; n) - 1)<br>            {<br>                res = Math.Max(res, 1 + calc(mask ^ i));<br>            }<br>        }<br><br>        return g[mask] = res;<br>    }<br><br>  public int maximumGroups(string[] friends) <br>  {<br>        n = friends.Length;<br>        f = friends;<br><br>        a = new int[n];<br>        for (int i = 0; i &lt; n; i++)<br>        {<br>            a[i] = 1 &lt;&lt; i;<br>            for (int j = 0; j &lt; n; j++)<br>            {<br>                if (f[i][j] == 'Y') a[i] |= (1 &lt;&lt; j);<br>            }<br>        }<br><br>        b = new int[1 &lt;&lt; n];<br>        for (int i = 0; i &lt; 1&lt;&lt;n; i++)<br>        {<br>            for (int j = 0; j &lt; n; j++)<br>            {<br>                if ((i &amp; (1 &lt;&lt; j)) &gt; 0)<br>                {<br>                    b[i] |= a[j];<br>                }<br>            }<br>        }<br><br>        g = new int[1 &lt;&lt; n];<br>        for (int i = 0; i &lt; 1&lt;&lt;n; i++)<br>        {<br>            g[i] = -1;<br>        }<br><br>        return calc((1 &lt;&lt; n) - 1);<br>        <br>  }<br><br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>