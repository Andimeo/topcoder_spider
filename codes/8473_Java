            import java.util.*;<br>public class InformFriends<br>{<br>  public int maximumGroups(String[] friends)<br>  {<br>    int n = friends.length;<br>    int[] edge = new int[n];<br>    for(int i=0;i&lt;n;i++)<br>      for(int j=0;j&lt;n;j++)<br>        if(friends[i].charAt(j)=='Y')<br>          edge[i] |= 1&lt;&lt;j;<br>    <br>    boolean[] allMask = new boolean[1&lt;&lt;n];<br>    for(int i=0;i&lt;1&lt;&lt;n;i++)<br>    {<br>      int reaching = i;<br>      for(int j=0;j&lt;n;j++)<br>        if((i&amp;1&lt;&lt;j)!=0)<br>          reaching |= edge[j];<br>      allMask[i] = reaching==(1&lt;&lt;n)-1;<br>    }<br>    <br>    int[] res = new int[1&lt;&lt;n];<br>    for(int i=0;i&lt;1&lt;&lt;n;i++)<br>    {<br>      for(int j=i;j&gt;0;j--)<br>      {<br>        j &amp;= i;<br>        if(allMask[j] &amp;&amp; res[i]&lt;res[i^j]+1)<br>          res[i] = res[i^j]+1;<br>      }<br>    }<br>    return res[(1&lt;&lt;n)-1];<br>  }<br>}<br></td>