            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class SummingArithmeticProgressions<br>{<br>  private int get(int rigth, int k)<br>  {<br>    int a = k;<br>    int b = (k - 1) * k / 2;<br>    <br>    int[] mod = new int[a];<br>    for(int i = 0; i &lt; a; i++)<br>      mod[i] = rigth + 1;<br>    int begin = a + b;<br>    mod[begin % a] = begin;<br>    for(int z = 0; z &lt; 100; z++)<br>    {<br>      for(int i = 0; i &lt; a; i++)<br>      {<br>        int v = mod[i] + b;<br>        int vv = v % a;<br>        if (mod[vv] &gt; v)<br>        {<br>          mod[vv] = v;<br>        }<br>      }<br>    }<br>    int sum = 0;<br>    for(int i = 0; i &lt; a; i++)<br>      if (mod[i] &lt;= rigth)<br>        sum += (rigth - mod[i]) / a + 1;<br>    return sum;<br>  }<br>  <br>  public int howMany(int left, int right, int k)<br>  {<br>    return get(right, k) - get(left - 1, k);<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>