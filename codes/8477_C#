            using System; <br>using System.Collections; <br>using System.Collections.Generic; <br>using System.Collections.Specialized; <br>using System.Text; <br>using System.Text.RegularExpressions; <br> <br>public class TrueSpace {<br>    public long calculateSpace(int[] sizes, int clusterSize) <br>    {<br>        long ret = 0;<br>        for (int i = 0; i &lt; sizes.Length; i++)<br>        {<br>            ret += (sizes[i] + clusterSize - 1) / clusterSize;<br>        }<br>        return ret * clusterSize;<br>    }<br><br>    <br><br>} <br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>