            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class ErrantKnight<br>{<br>  public int dx = 2000;<br>  public int dy = 2000;<br>  public int[] ddx = {1, 2, 2, 1, -1, -2, -2, -1};<br>  public int[] ddy = {2, 1, -1, -2, -2, -1, 1, 2};<br>  public byte[][] a = new byte[8100][8100];<br>  <br>  public int dist(int x, int y)<br>  {<br>    return (x*x + y*y);<br>  }<br>  <br>  public boolean move(int x, int y, int q)<br>  {<br>    boolean ok = true;<br>    int tx = x+ddx[q];<br>    int ty = y+ddy[q];<br>    <br>    if (dist(x,y) &lt; dist(tx,ty)) return false;<br>    <br>    while (ok)<br>    {<br>      x = tx;<br>      y = ty;<br>      if (a[x+dx][y+dy] == 0)<br>      {<br>        byte temp = calc(x, y);<br>        a[x+dx][y+dy] = temp;<br>        if (temp == -1) return true;<br>      } else<br>      if (a[x+dx][y+dy] == -1) return true;<br>      <br>      tx = x+ddx[q];<br>      ty = y+ddy[q];<br>    <br>      if (dist(x,y) &lt; dist(tx,ty)) ok = false;  <br>    }<br>    return false;<br>  }<br>  <br>  public byte calc(int x, int y)<br>  {<br>    if (x != y &amp;&amp; x!=0 &amp;&amp; y!=0) return 1;<br>    if (a[x+dx][y+dy]!=0) return a[x+dx][y+dy];<br>    <br>    for (int i=0; i&lt;8; i++)<br>    {<br>      boolean t = move(x, y, i);<br>      if (t) return 1;<br>    }<br>    return -1;<br>  }<br>  <br>  public String whoWins(int[] x, int[] y)<br>  {<br>    a[dx][dy] = -1;<br>    <br>//    for (int i=0; i&lt;30; i++)<br>//    {<br>//      for (int j=0; j&lt;30; j++)<br>//      {<br>//        if (a[i+dx][j+dy]&gt;0) System.out.print(" ");<br>//        System.out.print(a[i+dx][j+dy]+" ");<br>//      }<br>//      System.out.println();<br>//    }<br>    <br>    <br>    String res = new String();<br>    <br>    for (int i=0; i&lt;x.length; i++)<br>    {<br>      x[i] = Math.abs(x[i]);<br>      y[i] = Math.abs(y[i]);<br>      if (x[i]!=y[i] &amp;&amp; x[i]!=0 &amp;&amp; y[i]!=0) res+="W"; else<br>      {<br>        a[x[i]+dx][y[i]+dy] = calc(x[i], y[i]);<br>        if (a[Math.abs(x[i])+dx][Math.abs(y[i])+dy] == 1) res+="W"; else res+="B";<br>      }<br>    }<br>    <br>    return res;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>