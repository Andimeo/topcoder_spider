            using System;<br><br>public class ForbiddenStrings {<br>  public long countNotForbidden(int n) {<br>        long[] n1 = new long[n + 5];<br>        long[] n2 = new long[n + 5];<br>        n1[0] = 1;<br>        n1[1] = 3;<br>        n2[1] = 0;<br>        n1[2] = 6;<br>        n2[2] = 3;<br>        for (int i = 3; i &lt;= n; ++i)<br>        {<br>            n1[i] = n1[i - 1] + n2[i - 1] * 2;<br>            n2[i] = n1[i - 1] + n2[i - 1];<br>        }<br>        return n1[n] + n2[n];<br>  }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>