            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class BirthdayReminders<br>{<br>  public String[] remind(String[] fs, int[] bds, int cd, String[] oc, int[] days, int k)<br>  {<br>    long[][] var=new long[fs.length][oc.length];<br>    for(int i=0; i&lt;fs.length; i++){<br>      for(int j=0; j&lt;oc.length; j++){<br>        var[i][j]=bds[i]+((cd-bds[i])/days[j])*days[j];<br>        if((cd-bds[i])%days[j]!=0) var[i][j]+=days[j];<br>      }<br>    }<br>    <br>    String[] ret=new String[k];<br>    <br>    int pt=0;<br>    while(true){<br>      long min=-1;<br>      for(int i=0; i&lt;fs.length; i++){<br>        for(int j=0; j&lt;oc.length; j++){<br>          if(min==-1) min=var[i][j];<br>          else min=Math.min(var[i][j],min);<br>        }<br>      }<br>      for(int j=0; j&lt;oc.length; j++){<br>        for(int i=0; i&lt;fs.length; i++){<br>          if(pt==k) return ret;<br>          if(var[i][j]==min){<br>            long ct=(min-bds[i])/days[j];<br>            ret[pt]=""+min+". "+fs[i]+" "+oc[j]+" ("+ct+")";<br>            pt++;<br>            var[i][j]+=days[j];<br>          }<br>        }<br>      }<br>      <br>    }<br>    //return new String[1];<br>    <br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>