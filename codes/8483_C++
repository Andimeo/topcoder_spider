            #include &lt;vector&gt;<br>#include &lt;iostream&gt;<br>#include &lt;fstream&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;sstream&gt;<br>#include &lt;cmath&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>using namespace std;<br><br><br>class RussianSpeedLimits<br>{<br>public:<br>  int getCurrentLimit(vector &lt;string&gt; signs)<br>  {<br>    int iscity = 1;<br>    int i;<br>    int curlim = 60;<br>    for (i = 0; i &lt; signs.size(); i++)<br>    {<br>      if (signs[i] == "default")<br>      {<br>        if (iscity == 1) curlim = 60;<br>        else curlim = 90;<br>      }<br>      else if (signs[i] == "city") <br>      {<br>        iscity = 1 - iscity;<br>        if (iscity == 1) curlim = 60;<br>        else curlim = 90;<br>      }<br>      else<br>      {<br>        istringstream iss(signs[i]);<br>        iss &gt;&gt; curlim;<br>      }<br>    }<br>    return curlim;<br>  }<br>};<br></td>