            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class RussianSpeedLimits<br>{<br>  public int getCurrentLimit(String[] signs)<br>  {<br>    int n = signs.length;<br>    boolean in = true;<br>    int speed = 60;<br>    for(int i = 0; i &lt; n; i++)<br>    {<br>      String str = signs[i];<br>      if (str.compareTo("city") == 0 || str.compareTo("default") == 0)<br>      {<br>        if (str.compareTo("city") == 0)<br>          in = !in;<br>        if (in)<br>          speed = 60;<br>        else<br>          speed  = 90;<br>      }<br>      else<br>        speed = Integer.parseInt(str);<br>    }<br>    return speed;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>