            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class TelephoneNumbers<br>{<br>  public String kthNumber(int separation, int k) {<br>    int max = 7 * 16;<br>    int[] mask = new int[max], bonus = new int[max];<br>    int n = 0;<br>      for (int i = 0; i &lt; 28; i += 4)<br>        for (int j = 0; j &lt; 16; j++) {<br>          mask[n] = ~(15 &lt;&lt; i);<br>          bonus[n] = j &lt;&lt; i;<br>          n++;<br>        }<br>    int[] use = new int[1 &lt;&lt; 23];<br>    int number = 0;<br>    int step = 1 &lt;&lt; ((separation - 1)*4);<br>    int step1 = 1 &lt;&lt; ((separation - 2)*4);<br>    main:while (true)<br>    {<br>      if ((use[number &gt;&gt; 5] &amp; (1 &lt;&lt; (number &amp; 31))) != 0)<br>      {<br>        number = (number&amp;(~(step1-1))) + step1;<br>        continue main;<br>      }<br>      if (separation==3)<br>      for (int i=0; i&lt;n; i++)<br>      {<br>        int x = (number&amp;mask[i])|bonus[i];<br>        if ((use[x&gt;&gt;5] &amp; (1&lt;&lt;(x&amp;31)))!=0)<br>        {<br>          number++;<br>          continue main;<br>        }<br>      }<br>      if (--k==0)<br>      {<br>        String s = Integer.toString(number, 16);<br>        while (s.length() &lt; 7)<br>          s = '0' + s;<br>        return s;<br>      }<br>      if (separation&gt;=2)<br>      for (int i=0; i&lt;n; i++)<br>      {<br>        int x = (number&amp;mask[i])|bonus[i];<br>        use[x&gt;&gt;5]|=1&lt;&lt;(x&amp;31);<br>      }<br>      number = (number&amp;(~(step-1))) + step;<br>    }<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>