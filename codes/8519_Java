            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class SmoothNumbers<br>{<br>  public int countSmoothNumbers(int N, int k)<br>  {<br>    int dem = 0;<br>    for(int i=1;i&lt;=N;++i) {<br>      int x = i;<br>      for(int j=2;j&lt;=k;++j) {<br>        while(x%j==0) x/= j;<br>      }<br>      if(x==1) ++dem;<br>    }<br>    return dem;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>