            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class MarblesRegroupingEasy {<br>  public int minMoves(string[] boxes) {<br>    int res = int.MaxValue;<br>      int l = boxes[0].Length;<br>      int n = boxes.Length;<br>      for (int k = 0; k &lt; n; k++)<br>      {<br>          int r = 0;<br>          bool[] m = new bool[l];<br>          for (int i = 0; i &lt; n; i++)<br>          {<br>              if (i != k)<br>              {<br>                  int count = 0;<br>                  int d = -1;<br>                  for (int j = 0; j &lt; l; j++)<br>                  {<br>                      if (boxes[i][j] != '0')<br>                      {<br>                          count++;<br>                          d = j;<br>                      }<br>                  }<br>                  if (count &gt; 1)<br>                  {<br>                      r++;<br>                  } else if (count == 1)<br>                  {<br>                      if (m[d])<br>                      {<br>                          r++;<br>                      } else<br>                      {<br>                          m[d] = true;<br>                      }<br>                  }<br>              }<br>          }<br>          res = Math.Min(res, r);<br>      }<br>      return res;<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>