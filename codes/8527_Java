            public class MarblesRegroupingEasy {<br>  public int minMoves( String[] box ) {<br>    int m = box.length;<br>    int n = box[0].length();<br>    int[][] b = new int[m][n];<br>    for ( int i = 0; i &lt; m; i ++ ) {<br>      for ( int j = 0; j &lt; n; j ++ ) {<br>        b[i][j] = box[i].charAt( j ) - '0';<br>      }<br>    }<br>    int r = m - 1;<br>    for ( int i = 0; i &lt; m; i ++ ) {<br>      boolean[] f = new boolean[n];<br>      int cr = 0;<br>      for( int j = 0; j &lt; m; j ++ ) {<br>        if ( i == j ) {<br>          continue;<br>        }<br>        int c = -1;<br>        int cc = 0;<br>        for ( int k = 0; k &lt; n; k ++ ) {<br>          if ( b[j][k] &gt; 0 ) {<br>            cc ++;<br>            c = k;<br>          }<br>        }<br>        if ( cc == 1 &amp;&amp; ! f[c] ) {<br>          f[c] = true;<br>        } else if ( cc != 0 ) {<br>          cr ++;<br>        }<br>      }<br>      r = Math.min( r, cr );<br>    }<br>    return r;<br>  }<br>}<br></td>