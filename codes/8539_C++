            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class GuessingNextElement {<br>public:<br>  int guess(vector &lt;int&gt;);<br>};<br> <br>int GuessingNextElement::guess(vector &lt;int&gt; A) <br>{<br>    int p=A[1]-A[0];<br>  bool fuck=true;<br>  for(int i=2;i&lt;A.size();i++)<br>   if(A[i]-A[i-1]!=p){fuck=false;break;}<br>    if( fuck==true )<br>        return A[A.size()-1]+p;<br>    p=A[1]/A[0];<br>    fuck=true;<br>    for(int i=2;i&lt;A.size();i++)<br>      if( A[i]/A[i-1]!=p ){fuck=false;break;}<br>     if( fuck ==true)<br>      return A[A.size()-1]*p;<br>}<br></td>