            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class TrophyShelf <br>{<br>  public int[] countVisible(int[] trophies) <br>  {<br>        int max = -1;<br>        int lat1, lat2 = 0;<br>        lat1 = 0;<br>        for (int i = 0; i &lt; trophies.Length; i++)<br>        {<br>            if (max &lt; trophies[i])<br>            {<br>                lat1++;<br>                max = trophies[i];<br>            }<br>        }<br>        int[] ret = new int[2];<br>        ret[0] = lat1;<br>        Array.Reverse(trophies);<br>        max = -1;<br>        for (int i = 0; i &lt; trophies.Length; i++)<br>        {<br>            if (max &lt; trophies[i])<br>            {<br>                lat2++;<br>                max = trophies[i];<br>            }<br>        }<br>        ret[1] = lat2;<br>        return ret;<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by r1cs1]<br>// Powered by CodeProcessor<br></td>