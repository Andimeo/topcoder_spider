            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class TrophyShelf<br>{<br>  public int[] countVisible(int[] trophies)<br>  {<br>    int[] r = new int[2];<br>  <br>    int height = 0;<br>    for(int x = 0; x &lt; trophies.length; x++) {<br>      if(trophies[x] &gt; height) {<br>        height = trophies[x];<br>        r[0] += 1;<br>      }<br>    }<br>    <br>    height = 0;<br>    for(int x = trophies.length - 1; x &gt;= 0; x--) {<br>      if(trophies[x] &gt; height) {<br>        height = trophies[x];<br>        r[1] += 1;<br>      }<br>    }<br>    <br>    return r;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>