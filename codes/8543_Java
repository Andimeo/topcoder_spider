            import java.util.*;<br> <br>public class LittleSquares {<br>    public String winner(String[] state) {<br>        int m = state.length;<br>        int n = state[0].length();<br> <br>        int[] mv = new int[2 * n + n - 1];<br>        for (int i = 0; i &lt; 2 * n; i++) {<br>            mv[i] = 1 &lt;&lt; i;<br>        }<br>        for (int j = 0; j &lt; n - 1; j++) {<br>            mv[2 * n + j] = (1 &lt;&lt; j) + (1 &lt;&lt; (j + 1)) + (1 &lt;&lt; (j + n)) + (1 &lt;&lt; (j + 1 + n));<br>        }<br> <br>        int[] g = new int[1 &lt;&lt; (2 * n)];<br>        g[(1 &lt;&lt; (2 * n)) - 1] = 0;<br>        boolean[] ban = new boolean[mv.length + 1];<br>        for (int i = (1 &lt;&lt; (2 * n)) - 2; i &gt;= 0; i--) {<br>            Arrays.fill(ban, false);<br>            for (int j : mv) {<br>                if ((i &amp; j) == 0) {<br>                    ban[g[i | j]] = true;<br>                }<br>            }<br>            for (int j = 0; true; j++) {<br>                if (!ban[j]) {<br>                    g[i] = j;<br>                    break;<br>                }<br>            }<br>        }<br> <br>        int gg = 0;<br>        for (int i = 0; i &lt; m; i += 2) {<br>            int p = 0;<br>            for (int j = 0; j &lt; n; j++) {<br>                if (state[i].charAt(j) == '#') {<br>                    p |= (1 &lt;&lt; j);<br>                }<br>                if (i == m - 1 || state[i + 1].charAt(j) == '#') {<br>                    p |= (1 &lt;&lt; (j + n));<br>                }<br>            }<br>            gg ^= g[p];<br>        }<br> <br>        return gg == 0 ? "second" : "first";<br>    }<br> <br> <br>}<br> <br> <br>// Powered by PopsEdit<br> <br>// Powered by CodeProcessor<br></td>