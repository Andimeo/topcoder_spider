            #include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;queue&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;cmath&gt;<br><br>using namespace std;<br><br>//#define GI ({int t;scanf("%d",&amp;t);t;})<br>#define dbg(x) cout &lt;&lt; #x &lt;&lt; " -&gt; " &lt;&lt; x &lt;&lt; "\t" &lt;&lt; flush;<br>#define dbge(x) cout &lt;&lt; #x &lt;&lt; " -&gt; " &lt;&lt; x &lt;&lt; "\t" &lt;&lt; endl;<br>#define LET(x,a) typeof(a) x(a)<br>#define FOR(i,a,b) for(LET(i,a);i&lt;(b);++i)<br>#define REP(i,n) FOR(i,0,n)<br>#define EACH(i,v) FOR(i,(v).begin(),(v).end())<br>#define cs c_str()<br>#define pb push_back<br>#define sz size()<br>#define INF (int)1e9+1<br><br>typedef vector&lt;int&gt; VI;<br>typedef vector&lt;VI&gt; VVI;<br>typedef vector&lt;string&gt; VS;<br>typedef long long LL;<br>typedef long double LD;<br><br>class ProperDivisors {<br>  public:<br>  int fn1(int a,int b,int x){<br>    int c=0;<br>    if(a&lt;=x &amp;&amp; b&gt;=x) c=1;<br>    a=((a-1)/x+1)*x;<br>    b=(b/x)*x;<br>    return (b-a)/x+1;<br>  }<br>  int fn(int a,int b,int x){<br>    int c=0;<br>    if(a&lt;=x &amp;&amp; b&gt;=x) c=1;<br>    a=((a-1)/x+1)*x;<br>    b=(b/x)*x;<br>    return (b-a)/x+1-c;<br>  }<br>  int analyzeInterval(int a, int b, int n){<br>    int x=int( pow(LD((a+b)*1.0),LD(1/(long double)(n))) + 0.0001); LL ret=0;<br>    //cout&lt;&lt;x&lt;&lt;" ";<br>    FOR(i,2,a+b+1){<br>      ret+= LL(fn(a,a+b,i));<br>      //cout&lt;&lt;fn(a,a+b,i)&lt;&lt; " ";<br>    }<br>    //cout&lt;&lt;ret&lt;&lt; " ";<br>    FOR(i,2,x+1){<br>      ret-= fn1(a,a+b, int(pow(LD(i),LD(n))+0.0001));<br>    }<br>    return int(ret);<br>  }<br><br><br>};<br></td>