            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class QueryFilter <br>{<br>    public string[] preprocess(string query, string[] common) <br>    {<br>        List&lt;string&gt; A = new List&lt;string&gt;(query.Split(' '));<br>        List&lt;string&gt; B = new List&lt;string&gt;(common);<br>        for (int i = 0; i &lt; A.Count; i++)<br>        {<br>            if (B.Contains(A[i]) || A.LastIndexOf(A[i]) &gt; i)<br>            {<br>                A.RemoveAt(i);<br>                i--;<br>            }<br>        }<br>        A.Sort(); <br>        return A.ToArray();<br>    }<br>    <br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by r1cs1]<br>// Powered by CodeProcessor<br></td>