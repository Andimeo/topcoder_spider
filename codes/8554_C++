            // Cry as challenge phase is passing by<br><br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br><br>#include &lt;cmath&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br><br>#include &lt;set&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br><br>using namespace std;<br><br>#define FORC(it,v) for( __typeof((v).begin()) it = (v).begin(); it != (v).end(); ++it )<br><br>class QueryFilter {<br>public:<br>    vector &lt;string&gt; preprocess( string query, vector &lt;string&gt; common ) {<br>        set&lt; string &gt; S;<br><br>        istringstream in( query );<br><br>        for( string x; in &gt;&gt; x; ) S.insert( x );<br><br>        FORC( it, common ) S.erase( *it );<br><br>        vector&lt; string &gt; sol( S.begin(), S.end() );<br>        return sol;<br>    }<br>};<br><br><br><br><br><br><br><br><br><br>// Powered by FileEdit<br>// Powered by moj 3.0 beta 11 [modified TZTester]<br>// Powered by CodeProcessor<br></td>