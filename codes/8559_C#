            using System;<br>using System.Collections;<br>using System.Text;<br>public class BoxesOfBooks {<br>  public int boxes(int[] weights, int maxWeight) {<br>        int ans = 0;<br>        int curbox = 0;<br>        if (weights.Length == 0)<br>            return 0;<br>        ans++;<br>        for (int i = 0; i &lt; weights.Length; i++)<br>        {<br>            if (curbox + weights[i]&gt; maxWeight)<br>            {<br>                ans++;<br>                curbox = 0;<br>            }<br>            curbox += weights[i];<br>        }<br>        return ans;<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>