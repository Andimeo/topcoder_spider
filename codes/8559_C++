<td class="problemText" colspan="8" valign="middle" align="left">
            #include &lt;iostream&gt;<br>#include &lt;vector&gt;<br>#include &lt;queue&gt;<br>#include &lt;string&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cmath&gt;<br>#include &lt;list&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;sstream&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>using namespace std;<br><br>class BoxesOfBooks {<br> public:<br>  int boxes(vector &lt;int&gt; weights, int maxWeight)<br>  {<br>   if (weights.size()&lt;1) return 0;<br>   int ans=1,cur=weights[0];<br>   for (int i=1;i&lt;weights.size();i++)<br>   {<br>    if (cur+weights[i]&lt;=maxWeight) { cur+=weights[i]; continue; }<br>    ans++; cur=weights[i];<br>   }<br>   return ans;<br>  }<br>};<br></td>