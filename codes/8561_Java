            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class QuasiLatinSquares<br>{<br>    int extraRow[];<br>    int extraCol[];<br>    int countInRow[][];<br>    int countInCol[][];<br>    int table[][];<br>    int n1,d1;<br>    boolean set=true;<br>    <br>    private void fillTable(int x,int y)<br>    {<br>           if(set==false)<br>           return;<br>           <br>           for(int i=0;i&lt;d1;i++)<br>           {<br>               int cr=countInRow[i][x];<br>               int cc=countInCol[i][y];<br>               if((cr==0||extraRow[x]&gt;0) &amp;&amp; (cc==0||extraCol[y]&gt;0))<br>               {<br>                   table[x][y]=i;<br>                   countInRow[i][x]++;<br>                   countInCol[i][y]++;<br>                   if(cr!=0)<br>                   extraRow[x]--;<br>                   if(cc!=0)<br>                   extraCol[y]--;<br>                   //System.out.println("filled with "+i);<br>                   if(x==n1-1 &amp;&amp; y==n1-1)<br>                   {<br>                       set=false;<br>                       return;<br>                   }<br>                   else if(y==n1-1)<br>                   fillTable(x+1,0);<br>                   else<br>                   fillTable(x,y+1);<br>                   if(set==false)<br>                   return;<br>                   if(cc!=0)<br>                   extraCol[y]++;<br>                   if(cr!=0)<br>                   extraRow[x]++;<br>                   countInRow[i][x]--;<br>                   countInCol[i][y]--;<br>                   table[x][y]=-1;<br>               }<br>          }<br>    }<br>                   <br>                         <br>                  <br>  public String[] makeSquare(int n, int d)<br>  {<br>         n1=n;<br>         d1=d;<br>         table=new int[n][n];<br>         for(int i=0;i&lt;n;i++)<br>            for(int j=0;j&lt;n;j++)<br>               table[i][j]=-1;<br>         countInRow=new int[d][n];<br>         countInCol=new int[d][n];<br>         extraRow=new int[n];<br>         extraCol=new int[n];<br>         Arrays.fill(extraRow,(n1-d1));<br>         Arrays.fill(extraCol,(n1-d1));<br>         fillTable(0,0);<br>         String ret[]=new String[n];<br>         Arrays.fill(ret,"");<br>         for(int i=0;i&lt;n;i++)<br>         {<br>            //String temp="";<br>            for(int j=0;j&lt;n;j++)<br>            {   <br>               if(j!=0)<br>               ret[i]+=" ";<br>               ret[i]+=table[i][j];<br>            }<br>         }<br>         return ret;         <br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>