            // Cry as challenge phase is passing by<br><br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br><br>#include &lt;cmath&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br><br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br><br>using namespace std;<br><br>#define FORC(it,v) for( __typeof((v).begin()) it = (v).begin(); it != (v).end(); ++it )<br><br>int solve( string A, string B )<br>{<br>    int AA[ 26 ] = { 0 };<br>    int BA[ 26 ] = { 0 };<br><br>    FORC( it, A ) ++AA[ toupper( *it ) - 'A' ];<br>    FORC( it, B ) ++BA[ toupper( *it ) - 'A' ];<br><br>    int sum = 0;<br><br>    for( int i = 0; i &lt; 26; ++i )<br>        sum += min( AA[i], BA[i] );<br>    <br>    return sum;<br>}<br><br>class SimilarWords {<br>public:<br>    int mostSimilarPair( vector &lt;string&gt; words ) {<br>        int n = words.size();<br>        int maks = 0;<br><br>        for( int i = 0; i &lt; n; ++i )<br>            for( int j = i+1; j &lt; n; ++j )<br>                maks &gt;?= solve( words[i], words[j] );<br><br>        return maks;<br>    }<br>};<br><br><br><br><br><br><br><br><br><br>// Powered by FileEdit<br>// Powered by moj 3.0 beta 11 [modified TZTester]<br>// Powered by CodeProcessor<br></td>