            //Written by Sergey Melnikov<br>import java.util.*;<br>import java.math.*;<br> <br>public class SimilarWords {<br>    public int mostSimilarPair(String[] w) {<br>        int res = 0;<br>      for (int i = 0; i &lt; w.length; i++) {<br>      w[i] =w[i].toLowerCase();<br>    }<br>      for (int i = 0; i &lt; w.length; i++) {<br>      for (int j = i+1; j &lt; w.length; j++) {<br>        int a[] = new int[26];<br>        for (int k = 0; k &lt; w[i].length(); k++) {<br>          a[w[i].charAt(k)-'a']++;<br>        }<br>        int b[] = new int[26];<br>        for (int k = 0; k &lt; w[j].length(); k++) {<br>          b[w[j].charAt(k)-'a']++;<br>        }<br>        int c = 0;<br>        for (int k = 0; k &lt; 26; k++) {<br>          c += Math.min(a[k], b[k]);<br>        }<br>        res = Math.max(res, c);<br>      }<br>    }<br>        return res;<br>    }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>