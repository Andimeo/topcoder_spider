            #include &lt;iostream&gt;<br>#include &lt;fstream&gt;<br>#include &lt;math.h&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>using namespace std;<br><br>class SmoothNumbersHard<br>{<br>public:<br>  int countSmoothNumbers(int N, int k)<br>  {<br>    vector &lt;int&gt; a(N+1);<br>    vector &lt;int&gt; b(N+1);<br>    long i,j;<br>    for(i=2;i&lt;=N;i++) a[i]=1;<br>    for(i=2;i&lt;=N;i++)<br>      if(a[i]==1)<br>      {<br>        b[i]=i;<br>        for(j=2;i*j&lt;=N;j++) <br>        {<br>          a[i*j]=0;<br>          b[i*j]=i;<br>        }<br>      }<br>    int num=1;<br>    for(i=2;i&lt;=N;i++) <br>      if(b[i]&lt;=k) num++;<br>    return num;<br>  }<br>};<br></td>