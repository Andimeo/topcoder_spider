<td class="problemText" colspan="8" valign="middle" align="left">
            public class SmoothNumbersHard<br>{<br>  public int countSmoothNumbers(int N, int k)<br>  {<br>    if(N==1)<br>      return 1;<br>    int ret = N;<br>    int i = 2;<br>    boolean prime[] = new boolean[N+1];<br>    boolean smooth[] = new boolean[N+1];<br>    <br>    while(i&lt;=N)<br>    {<br>      if(!prime[i])<br>      {<br>        for(int j=2*i; j&lt;=N; j=j+i)<br>          prime[j]=true;<br>        if(i&gt;k)<br>          for(int j=i; j&lt;=N; j=j+i)<br>            if(!smooth[j])<br>            {<br>              smooth[j]=true;<br>              //System.out.println("smooth: "+j+" (div by "+i+")");<br>              ret--;<br>            }<br>      }<br>      i++;<br>    }<br>    return ret;<br>  }<br>}<br></td>