            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class TheLuckyNumbers {<br>public:<br>  int count(int, int);<br>};<br> <br>int tmp, ans;<br> <br>void search(long long x, int lim)<br>{<br>  if (x &lt;= lim)<br>  {<br>    ++tmp;<br>  }<br>  else<br>    return ;<br>  search(x * 10 + 4, lim);<br>  search(x * 10 + 7, lim);<br>}<br> <br> <br>int TheLuckyNumbers::count(int a, int b) {<br>  tmp = 0;<br>  search(0, a - 1);<br>  ans = tmp;<br>  tmp = 0;<br>  search(0, b);<br>  ans = tmp - ans;<br>  return ans;<br>}<br> <br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>