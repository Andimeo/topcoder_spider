            import java.util.Vector;<br><br><br>public class TheLuckyNumbers {<br><br>  /**<br>   * @param args<br>   */<br>  public static void main(String[] args) {<br>    // TODO Auto-generated method stub<br><br>  }<br><br>  public static int count(int a, int b){<br>    int re=0;<br>    Vector list=new Vector();<br>    list.add(4);<br>    if(a&lt;=4&amp;&amp;b&gt;=4) re++;<br>    list.add(7);<br>    if(a&lt;=7&amp;&amp;b&gt;=7) re++;<br>    for(int i=0;i&lt;list.size();i++){<br>      String each=list.get(i).toString();<br>      //System.out.println(each);<br>      if(each.length()&gt;=9) break;<br>      else{<br>        String one=each.concat("4");<br>        list.add(one);<br>        if(a&lt;=Integer.parseInt(one)&amp;&amp;b&gt;=Integer.parseInt(one)) {<br>          //System.out.println(one);<br>          re++;<br>        }<br>        String two=each.concat("7");<br>        list.add(two);<br>        if(a&lt;=Integer.parseInt(two)&amp;&amp;b&gt;=Integer.parseInt(two)) {<br>          //System.out.println(two);<br>          re++;<br>        }<br>        <br>        if(b&lt;Integer.parseInt(two)) break;<br>        }<br>      }<br>      <br><br>    return re;<br>  }<br>}<br></td>