            import java.util.*;<br>import java.math.*;<br>import java.io.*;<br>import java.text.*;<br>import java.util.regex.*;<br>import static java.util.Arrays.*;<br>import static java.lang.Math.*;<br>import static java.lang.Integer.parseInt;<br>public class CreatureTraining {<br>  public long maximumPower(int[] count, int[] power, int D) {<br>    cs = get(count);<br>    ps = get(power);<br>    return go(0,0,D);<br>  }<br>  long[] get(int[] ar){<br>    long[] ret = new long[ar.length];<br>    for(int i = 0; i &lt; ar.length; i++){<br>      ret[i] = ar[i];<br>    }<br>    return ret;<br>  }<br>  long[] cs, ps;<br>  long[][][] memo = new long[60][110][110];<br>  long go(int at, int extra, int d){<br>    if(at == cs.length) return 0;<br>    if(memo[at][extra][d] != 0) return memo[at][extra][d]-1;<br>    long ret = 0;<br>    long cnt = cs[at]+extra;<br>    for(int i = 0; i &lt;= cnt &amp;&amp; i &lt;= d; i++){<br>      ret = max(ret, ps[at]*(cnt-i) + go(at+1, i, d-i));<br>    }<br>    memo[at][extra][d] = ret+1;<br>    return ret;<br>  }<br> <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>