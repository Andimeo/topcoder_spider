<td class="problemText" colspan="8" valign="middle" align="left">
            Public Class TriangularSubsequence<br>        Public Function longest(ByVal a As Integer()) As Integer<br>            Dim ans As Integer = 2<br>            Dim t As Integer = 2<br>            Array.Sort(a)<br>            If a.Length = 1 Then Return 1<br>            If a.Length = 2 Then Return 2<br>            If a.Length = 0 Then Return 0<br>            For i As Integer = 0 To a.Length - 1 - 2<br>                For j As Integer = i + 2 To a.Length - 1<br>                    If (a(i) + a(i + 1)) &gt; a(j) Then t = j - i + 1<br>                    If ans &lt; t Then ans = t<br>                Next<br>            Next<br>            Return ans<br>        End Function<br>    End Class<br></td>