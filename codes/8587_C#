            using System;<br>using System.Collections;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class MoreNim<br>{<br>  public long bestSet(string[] heaps)<br>  {<br>    long[] h = new long[heaps.Length];<br>        for (int i = 0; i &lt; heaps.Length; i++)<br>            h[i] = -long.Parse(heaps[i]);<br>      Array.Sort(h);<br>        for (int i = 0; i &lt; h.Length; i++) h[i] = -h[i];<br>      long ans = 0;<br>        bool[] c = new bool[h.Length];<br>        for (int i=0; i&lt;h.Length; i++)<br>        {<br>            bool flag = true;<br>            long now = h[i];<br>            for( int j=0; j&lt;i; j++)<br>                if (c[j])<br>                {<br>                    if ((h[i] ^ h[j]) &lt; h[i]) h[i] = h[i] ^ h[j];<br>                    if (h[i] == 0)<br>                    {<br>                        flag = false;<br>                        break;<br>                    }<br>                }<br>            if (flag)<br>            {<br>                c[i] = true;<br>            }<br>            else ans += now;<br>        }<br>      return ans;<br>  }<br>}<br> <br>//Powered by [Zigger]<br></td>