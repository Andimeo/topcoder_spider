            using System;<br><br>public class ConcatenateNumber {<br>  public int getSmallest(int number, int k) {<br>        int mod = number % k;<br>        long p10 = 1;<br>        while (p10 &lt;= number)<br>            p10 *= 10;<br>        bool[] was = new bool[k];<br>        int res = 1;<br>        while (true)<br>        {<br>            if (mod == 0)<br>                return res;<br>            if (was[mod])<br>                return -1;<br>            was[mod] = true;<br>            mod = (int)((p10 * mod + number) % k);<br>            ++res;<br>        }<br>  }<br><br>  <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>