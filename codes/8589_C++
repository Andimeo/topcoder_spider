            #include &lt;iostream&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;string&gt;<br>#include &lt;utility&gt;<br>#include &lt;vector&gt;<br><br>using namespace std;<br><br><br>struct ConcatenateNumber {<br>   int getSmallest( int number, int k ) { // caret here<br>      long long p10 = 1;<br>      while ( p10 &lt;= number ) p10 *= 10;<br>      <br>      long long current = number%k;<br>      for ( int iter=1; iter&lt;=k; ++iter ) {<br>         if ( current == 0 ) {<br>            return iter;<br>         }<br>         current = (current*p10+number)%k;<br>      }<br>      return -1;<br>   }<br>};<br><br><br><br><br>// Powered by FileEdit<br>// Powered by moj 4 beta 3 [modified TZTester]<br>// Powered by CodeProcessor<br></td>