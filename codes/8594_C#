            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br> <br>public class RoughStrings<br>{<br>  public int minRoughness (string s, int n)<br>  {<br>    int[] cnts = new int[26];<br>    <br>    foreach (char c in s)<br>      cnts[c - 'a']++;<br>    <br>    List&lt;int&gt; x = new List&lt;int&gt; ();<br>    <br>    foreach (int i in cnts)<br>      if (i != 0)<br>        x.Add (i);<br>    <br>    int min = 50;<br>    for (int a = 0; a &lt;= 50; ++a)<br>      for (int b = a; b &lt;= 50; ++b)<br>    {<br>      int rem = 0;<br>      <br>      foreach (int i in cnts)<br>        if (i &lt; a)<br>          rem += i;<br>      else<br>        if (i &gt; b)<br>          rem += i - b;<br>      <br>      if (rem &lt;= n)<br>        min = Math.Min (min, b - a);<br>    }<br>    <br>    return min;<br>  }<br>  <br>  <br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>