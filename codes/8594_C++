            #include &lt;vector&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;cmath&gt;<br>#include &lt;string&gt;<br>using namespace std;<br>class RoughStrings {<br>  public:<br>    int minRoughness(string s, int n) {<br>      int i, j, k, l = s.length();<br>      int count[30];<br>      memset(count, 0, sizeof(count));<br>      for (i = 0; i &lt; l; i ++)<br>        count[s[i] - 'a'] ++;<br>      int ret = 10000;<br>      for (i = 0; i &lt;= 50; i ++)<br>        for (j = i; j &lt;= 50; j ++) {<br>          int m = 0;<br>          for (k = 0; k &lt; 26; k ++)<br>            if (count[k] &lt; i)<br>              m += count[k];<br>            else<br>              if (count[k] &gt; j)<br>                m += (count[k] - j);<br>          if (m &lt;= n)<br>            ret = min(ret, (j - i));<br>        }<br>      return ret;<br>    }<br>};<br> <br>//Powered by [KawigiEdit] 2.0!<br></td>