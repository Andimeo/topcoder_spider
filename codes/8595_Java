            import java.util.*;<br>import java.math.*;<br> <br>public class MonstersAndBunnies {<br> <br>    public double survivalProbability (int M, int B) {<br>        if (M % 2 == 1) return 0.0;<br> <br>        double result = 1;<br>        while (M &gt; 0) {<br>            result *= M / (double)(M + 1);<br>            result *= (M - 1) / (double)M;<br>            M -= 2;<br>        }<br>        return result;<br>    }<br> <br>    // Library:<br> <br>}<br> <br>// Powered by FileEdit<br>// Powered by UnusedCodeRemover<br>// Powered by CodeProcessor<br></td>