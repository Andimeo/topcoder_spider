            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;queue&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;sstream&gt;<br>#include &lt;cmath&gt;<br>#include &lt;iostream&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;ctime&gt;<br>using namespace std;<br> <br>typedef vector&lt;int&gt; vi;<br>typedef vector&lt;string&gt; vs;<br>#define si size()<br>#define pb push_back<br>#define mp make_pair<br>typedef long long ii;<br> <br>bool p[1000000];<br>ii n[1000000];<br>int N;<br>int elts[50];<br>int cts[50];<br>class PrimeSums {<br>  public:<br>  long long getCount(vector &lt;int&gt; bag) {  <br>    for (int i = 0; i &lt; 1000000; i++) p[i] = 1;<br>    p[0] = p[1] = 0;<br>    for (int i = 4; i &lt; 1000000; i += 2) p[i] = 0;<br>    for (int i = 3; i &lt; 1000000; i += 2) {<br>      if (!p[i]) continue;<br>      int two = i+i;<br>      for (int j = 3 * i; j &lt; 1000000; j += two) {<br>        p[j] = 0;<br>      }<br>    }<br>    <br>    int nz = 0;<br>    <br>    N = 0;<br>    for (int i = 0; i &lt; bag.si; i++) {<br>      if (bag[i] == 0) {nz++; continue;}<br>      for (int j = 0; j &lt; N; j++) if (elts[j] == bag[i]) {cts[j]++; goto next;}<br>      elts[N] = bag[i];<br>      cts[N] = 1;<br>      N++;<br>      next:;<br>    }<br>    <br>    n[0] = 1;<br>    int max = 0;<br>    for (int i = 0; i &lt; N; i++) {<br>      for (int j = max; j &gt;= 0; j--) {<br>        for (int k = cts[i]; k &gt; 0; k--) {<br>          n[elts[i] * k + j] += n[j];<br>        }<br>      }<br>      max += elts[i] * cts[i];<br>    }<br>    ii ret = 0;<br>    for (int i = 0; i &lt; 1000000; i++) if (p[i]) ret += n[i];<br>    <br>    ret *= (nz + 1);<br>    return ret;<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>