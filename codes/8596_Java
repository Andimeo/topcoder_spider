            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class PrimeSums<br>{<br>  public long getCount(int[] bag)<br>  {<br>    int[] ma = new int[10001];<br>    for (int i = 0; i &lt; bag.length; i++) ma[bag[i]]++;<br>    <br>    int max = 50*10000+1;<br>    long[] ok = new long[max];<br>    ok[0] = ma[0]+1;<br>    for (int i = 1; i &lt; 10001; i++)<br>      if (ma[i] != 0) {<br>        for (int j = max-1; j &gt;= 0; j--)<br>          if (ok[j] != 0) <br>            for (int k = 1; k &lt;= ma[i]; k++) ok[j+i*k]+=ok[j];<br>      }<br>  //  for (int i = 0; i &lt;= 11; i++) System.out.println(i+" "+ok[i]);<br>            <br>    boolean[] isp = new boolean[max];<br>    long ans = 0;<br>    for (int i = 2; i &lt; max; i++)<br>      if (!isp[i]) {<br>        ans+=ok[i];<br>        for (int j = 2; i*j &lt; max; j++) isp[i*j] = true;<br>      }<br>    return ans;  <br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>