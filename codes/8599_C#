            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br> <br>public class OneWayStreets<br>{<br>  public string canChange (string[] roads)<br>  {<br>    int n = roads.Length;<br>    <br>    bool[,] b = new bool[n, n];<br>    <br>    for (int i = 0; i &lt; n; ++i)<br>      for (int j = 0; j &lt; n; ++j)<br>        if (roads[i][j] == 'Y' &amp;&amp; roads[j][i] != 'Y')<br>          b[i, j] = true;<br>    <br>    for (int k = 0; k &lt; n; ++k)<br>      for (int i = 0; i &lt; n; ++i)<br>        for (int j = 0; j &lt; n; ++j)<br>          b[i, j] |= b[i, k] &amp;&amp; b[k, j];<br>    <br>    for (int i = 0; i &lt; n; ++i)<br>      for (int j = 0; j &lt; n; ++j)<br>        if (i != j &amp;&amp; b[i, j] &amp;&amp; b[j, i])<br>          return "NO";<br>    <br>    return "YES";<br>  }<br>  <br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>