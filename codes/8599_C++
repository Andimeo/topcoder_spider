<td class="problemText" colspan="8" valign="middle" align="left">
            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>using namespace std;<br>struct OneWayStreets<br>{<br>bool g[50][50];<br>int n;<br>string canChange(vector &lt;string&gt; r)<br>{<br>n=r.size();<br>for (int i=0;i&lt;n;i++) for (int j=0;j&lt;n;j++) if (r[i][j]=='Y'&amp;&amp;r[j][i]=='N') g[i][j]=true;<br>for (int k=0; k&lt;n; k++) for (int i=0; i&lt;n; i++) for (int j=0; j&lt;n; j++) if (g[i][k]&amp;&amp;g[k][j]) g[i][j]=true;<br>for (int i=0;i&lt;n;i++) if (g[i][i]) return "NO";<br>return "YES";<br>}<br>};<br></td>