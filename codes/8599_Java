            import java.util.*;<br> <br>public class OneWayStreets {<br>  public String canChange(String[] roads) {<br>        int n = roads.length;<br>        boolean[][] g = new boolean[n][n];<br>        for (int i = 0; i &lt; n; ++i)<br>            for (int j = 0; j &lt; n; ++j)<br>                if (roads[i].charAt(j) == 'Y' &amp;&amp; roads[j].charAt(i) == 'N')<br>                    g[i][j] = true;<br>        for (int k = 0; k &lt; n; ++k)<br>            for (int i = 0; i &lt; n; ++i)<br>                for (int j = 0; j &lt; n; ++j)<br>                    g[i][j] |= g[i][k] &amp;&amp; g[k][j];<br>        for (int i = 0; i &lt; n; ++i)<br>            if (g[i][i])<br>                return "NO";<br>        return "YES";<br>  }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>