            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class PreciousStones<br>{<br>  public double value(int[] s, int[] g)<br>  {<br>    int n=g.Length;<br>    double[,] st=new double[n,2];<br>    int i,j,k,sum=0;<br>    double kk;<br>    for (i=0;i&lt;n;i++){<br>      st[i,0]=0.0+s[i];<br>      st[i,1]=0.0+g[i];<br>      sum+=s[i];<br>    }<br>    for (i=0;i&lt;n;i++){<br>      for(j=i+1;j&lt;n;j++){<br>        if (st[i,0]*st[j,1]&gt;st[i,1]*st[j,0]) {<br>          kk=st[i,0]; st[i,0]=st[j,0]; st[j,0]=kk;<br>          kk=st[i,1]; st[i,1]=st[j,1]; st[j,1]=kk;<br>        }<br>      }<br>    }<br>    double qq=0;<br>    double cc=0.0+(double)n;<br>    double q0;<br>    double gg=0.0,ss=0.0,pp;<br>    for(k=0;k&lt;1000;k++){<br>      q0=(qq+cc)/2;<br>      gg=0.0;<br>      ss=0.0;<br>      for (i=0;i&lt;n;i++){<br>        if ((i+1)&lt;q0) {gg+=st[i,1];} else{<br>          if (i&gt;q0) {ss+=st[i,0];} else {<br>            pp=q0-i;<br>            gg+=(st[i,1]*pp);<br>            ss+=(st[i,0]*(1-pp));<br>          }<br>        }<br>      }<br>      if (gg&gt;ss) {cc=q0;} else {qq=q0;}<br>    }<br>    return ((gg+ss)/2);<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>