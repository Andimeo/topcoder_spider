            import java.util.*;<br>public class InstantRunoffVoting<br>{<br>  public int winner(String[] voters)<br>  {<br>    int m = voters.length, n = voters[0].length();<br>    boolean[] used = new boolean[n];<br>    while(true)<br>    {<br>      int[] count = new int[n];<br>      for(int i=0;i&lt;m;i++)<br>      {<br>        for(int j=0;j&lt;n;j++)<br>        {<br>          int dig = voters[i].charAt(j)-'0';<br>          if(!used[dig])<br>          {<br>            count[dig]++;<br>            break;<br>          }<br>        }<br>      }<br>      int min = 10000;<br>      for(int i=0;i&lt;n;i++)<br>      {<br>        if(!used[i])<br>        {<br>          if(count[i]*2&gt;m)<br>            return i;<br>          else<br>            min = Math.min(min, count[i]);<br>        }<br>      }<br>      boolean ok = false;<br>      for(int i=0;i&lt;n;i++)<br>        if(!used[i])<br>          if(count[i]==min)<br>            used[i] = true;<br>          else<br>            ok = true;<br>      if(!ok)<br>        return -1;<br>    }<br>  }<br>}<br></td>