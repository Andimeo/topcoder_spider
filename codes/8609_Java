            import java.util.*;<br>import static java.lang.Math.*; <br>import static java.util.Arrays.*;<br>import static java.lang.Integer.*;<br>import static java.lang.Character.*;<br>import static java.lang.Double.*;<br>import java.awt.geom.*;<br>public class PhoneNumbers {<br>    int n=0;<br>    /**                        DON'T  PANIC                      */<br>    <br>    int good(String number){<br>        String[]s = number.split("-");<br>        int res =0;<br>        for(String x:s){<br>            int b =0;<br>            for(int i=0;i&lt;x.length();i++){<br>                int r =1;<br>                for(int j=i+1;j&lt;x.length();j++){<br>                    if(x.charAt(i)==x.charAt(j))r++;<br>                }<br>                b=max(b,r);<br>            }<br>            if(b==x.length())res+=2;<br>            else if(b==2)res++;<br>        }<br>        return res;<br>    }<br>    <br>    HashMap&lt;String , String&gt;HS = new HashMap&lt;String, String&gt;();<br>    String doit(String s){<br>        if(s.length()&lt;4)return s;<br>        if(s.length()==4)return s.substring(0,2)+"-"+s.substring(2);<br>        if(HS.containsKey(s))return HS.get(s);<br>        String res="";<br>        String a = s.substring(0,2)+"-"+doit(s.substring(2));<br>        String b = s.substring(0,3)+"-"+doit(s.substring(3));<br>        if(good(a)&gt;good(b))res=a;<br>        if(good(a)&lt;good(b))res=b;<br>        if(good(a)==good(b)){<br>            if(a.compareTo(b)&lt;0)res=a;<br>            else res=b;<br>        }<br>        HS.put(s,res);<br>        return res;<br>    }<br>    <br>    public String bestNumber(String number) {<br>   <br>        return doit(number);<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>