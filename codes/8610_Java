            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class OlympicGames<br>{<br>  <br>  class Pair {<br>    public int bac, dong;<br>    public Pair() {}<br>    public Pair(int b,int d) {<br>      bac = b;<br>      dong = d;<br>    }<br>  }<br>  <br>  int n;<br>  Pair[] a;<br>  int t0vang, t0bac, t0dong;<br>  int left;<br>  int result;<br>  <br>  void qhd() {<br>    <br>    int[][] F1 = new int[n+1][left+1];<br>    int[][] F2 = new int[n+1][left+1];<br>    <br>    for(int[] d1 : F1) Arrays.fill(d1, -1);<br>    F1[0][0] = 0;<br>    <br>    for(int i=0;i&lt;n;++i) {<br>      <br>      for(int[] d1 : F2) Arrays.fill(d1, -1);<br>      <br>      int bc=0, dc=0;<br>      <br>      if(a[i].bac&lt;=t0bac) {<br>        <br>        bc = t0bac - a[i].bac + 1;<br>        if(a[i].dong &lt;= t0dong) dc = t0dong - a[i].dong + 1;        <br>        for(int j=0;j&lt;n;++j) for(int k=0;k&lt;=left;++k) if(F1[j][k]&gt;=0) {<br>          <br>          if(F2[j][k]==-1 || F2[j][k]&gt;F1[j][k]) F2[j][k] = F1[j][k];<br>          <br>          int nb = k + bc - 1;<br>          int nd = F1[j][k] + dc;<br>          if(nb&lt;=left &amp;&amp; nd&lt;=left &amp;&amp; bc-1+dc &lt;= left) {<br>            if(F2[j+1][nb]==-1 || F2[j+1][nb]&gt;nd) F2[j+1][nb] = nd;<br>          }<br>          <br>           nb = k + bc;<br>           nd = F1[j][k];<br>          <br>          if(nb&lt;=left &amp;&amp; nd&lt;=left &amp;&amp; bc&lt;=left) {<br>            if(F2[j+1][nb]==-1 || F2[j+1][nb]&gt;nd) F2[j+1][nb] = nd;<br>          }<br>          <br>        }<br>        <br>      }<br>      <br>      int[][] tmp = F1; F1 = F2; F2 = tmp;<br>      <br>    }<br>    <br>    result = 0;<br>    for(int j=0;j&lt;=n;++j) for(int k=0;k&lt;=left;++k) if(F1[j][k]&gt;=0) result = j;<br>    <br>  }<br>  <br>  public int worstPlace(String[] medals, int left)<br>  {<br>    this.left = left;<br>    n = medals.length;<br>    String[] tam;<br>    tam = medals[0].split(" ");<br>    t0vang = Integer.parseInt(tam[0]) + left;<br>    t0bac = Integer.parseInt(tam[1]);<br>    t0dong = Integer.parseInt(tam[2]);<br>    <br>    int sl = 0;<br>    int higher = 0;<br>    <br>    for(int i=1;i&lt;n;++i) {<br>      tam = medals[i].split(" ");<br>      int vang = Integer.parseInt(tam[0]);<br>      int bac = Integer.parseInt(tam[1]);<br>      if(vang == t0vang) {<br>        if(bac&gt;t0bac) ++higher;<br>        else ++sl;<br>      }<br>      else if(vang&gt;t0vang) ++ higher;<br>    }<br>    a = new Pair[sl];<br>    int na = 0;<br>    for(int i=1;i&lt;n;++i) {<br>      tam = medals[i].split(" ");<br>      int vang = Integer.parseInt(tam[0]);<br>      int bac = Integer.parseInt(tam[1]);<br>      int dong = Integer.parseInt(tam[2]);<br>      if(t0vang == vang &amp;&amp; t0bac&gt;=bac)<br>        a[na++] = new Pair( bac, dong);<br>    }<br>    n = na;<br>    <br>    qhd();<br>    <br>    return result + higher + 1;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>