            using System;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class SnowyWinter<br>{<br>  public static int snowyHighwayLength(int[] startPoints, int[] endPoints)<br>  {<br>    bool[] d = new bool[10005];<br>    for (int i = 0; i &lt; startPoints.Length; i++)<br>    {<br>      for (int j = startPoints[i]; j &lt; endPoints[i]; j++)<br>        d[j] = true;<br>    }<br>    int res = 0;<br>    for (int i = 0; i &lt; d.Length; i++)<br>      if (d[i]) res++;<br>    return res;<br>  }<br>}<br></td>