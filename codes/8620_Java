            import java.util.*;<br>import java.math.*;<br>import static java.util.Arrays.*;<br>import static java.lang.Character.*;<br>import static java.util.Collections.*;<br>import static java.lang.Double.*;<br>import static java.lang.Integer.*;<br>import static java.lang.Long.*;<br>import static java.lang.Math.*;<br>public class SnowyWinter {<br>  public int snowyHighwayLength(int[] s, int[] e) {<br>    int[] a = new int[10001];<br>    int r = 0;<br>    int n = s.length;<br>    for(int i=0;i&lt;n;i++){<br>      for(int k=s[i];k&lt;e[i];k++) a[k] = 1;<br>    }<br>    for(int z : a) r += z;<br>    return r;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] X.O!<br>//Aug 31st, 2007<br></td>