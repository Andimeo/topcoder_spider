            import java.util.*;<br>public class MakeSquare {<br>    public int minChanges(String S) {<br>        int res=S.length();<br>        for(int x=0;x&lt;S.length();x++)<br>        {<br>          res=Math.min(res, dis(S.substring(0,x),S.substring(x)));<br>        }<br>        <br>        return res;<br>    }<br>    public int dis(String a,String b)<br>    {<br>      char[] a1=a.toCharArray();<br>      char[] b1=b.toCharArray();<br>      int[][] leven=new int[a.length()+1][b.length()+1];<br>      for(int x=0;x&lt;a.length()+1;x++)<br>        leven[x][0]=x;<br>      for(int y=0;y&lt;b.length()+1;y++)<br>        leven[0][y]=y;<br>      for(int x=1;x&lt;a.length()+1;x++)<br>        for(int y=1;y&lt;b.length()+1;y++)<br>        {<br>          if(a1[x-1]==b1[y-1])leven[x][y]=leven[x-1][y-1];<br>          else<br>          {<br>            leven[x][y]=Math.min(leven[x-1][y], Math.min(leven[x][y-1], leven[x-1][y-1]))+1;<br>          }<br>        }<br>      return leven[a.length()][b.length()];<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>