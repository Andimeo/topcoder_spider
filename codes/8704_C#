            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class JohnnysCannon<br>{<br> <br>    public double getDistance(int velocity, int distance, int[] angles)<br>    {<br>        double rv = double.MaxValue;<br>        foreach(int v in angles)<br>        {<br>            double alpha = v/180.0*Math.PI;<br>            double land = (2 * velocity * velocity * Math.Sin(alpha) * Math.Cos(alpha)) / 10.0; <br>            land = Math.Abs(land-distance);<br>            rv= Math.Min(land,rv);<br>        }<br>        return rv;<br>    }<br>}<br> <br>// Powered by PopsEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>