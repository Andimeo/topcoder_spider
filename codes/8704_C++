            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;queue&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cctype&gt;<br>#include &lt;string&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>#define SIZE(X) ((int)(X.size()))<br>const double pi=acos(-1.0);<br>template&lt;class T&gt; inline void checkmin(T &amp;a,T b){if(b&lt;a) a=b;}<br>template&lt;class T&gt; inline T sqr(T x){return x*x;}<br> <br>class JohnnysCannon<br>{<br>public:<br>  double getDistance(int v, int d, vector &lt;int&gt; p)<br>  {<br>    double result=1e100;<br>    for (int i=0;i&lt;SIZE(p);i++)<br>    {<br>      double alpha=p[i]/180.0*pi;<br>      double d0=sqr(v)*sin(alpha)*cos(alpha)/5.0;<br>      checkmin(result,fabs(d-d0));<br>    }<br>    return result;<br>  }<br>};<br> <br> <br> <br>// Powered by PopsEdit<br></td>