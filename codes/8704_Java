            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class JohnnysCannon<br>{<br>  public double getDistance(int velocity, int distance, int[] angles)<br>  {<br>        double res=1e10;<br>      for(int i=0;i&lt;angles.length;i++){<br>           res=Math.min(res,Math.abs(1.0*distance-(2*velocity*velocity*Math.sin(angles[i]/180.0*Math.PI)*Math.cos(angles[i]/180.0*Math.PI) )/10.0));<br>      }<br>      return res;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>