            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class AverageCandyLifetime<br>{<br>  public double getAverage(int[] eatenCandies)<br>  {<br>    int days[] = {31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31};<br>    int candies = 0;<br>    int numDays = 0;<br>    int ans = 0;<br>    for(int i=0;i&lt;12;i++){<br>      numDays += days[i];<br>      ans += numDays*eatenCandies[i];<br>      candies += eatenCandies[i];    <br>    }<br>    return ans*1.0/candies;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>