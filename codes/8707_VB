            Public Class AverageCandyLifetime<br>  Public Function getAverage(eatenCandies As Integer()) As Double<br>    Dim i,j As Integer<br>    Dim temp As Integer<br>    Dim can As Integer<br>    Dim ret As Double<br>    Dim a() As Integer = {31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31}<br>    <br>    For i = 0 To 11<br>      temp = 0<br>      For j = 0 To i<br>        temp = temp + a(j)<br>      Next<br>      can = can + eatenCandies(i)<br>      ret = ret + temp * eatenCandies(i)<br>    Next<br>    <br>    ret = ret /can<br>    <br>    Return ret<br>  End Function<br>End Class<br></td>