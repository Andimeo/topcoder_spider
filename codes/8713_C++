            #include &lt;stdio.h&gt;<br>#include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;queue&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;cmath&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;complex&gt;<br>#include &lt;sstream&gt;<br>using namespace std;<br> <br>#define REP(i,n) for(int i=0;i&lt;(n);i++)<br>#define FOR(i,a,b) for(int i=(a);i&lt;=(b);i++)<br>#define FORD(i,a,b) for(int i=(a);i&gt;=(b);i--)<br>#define FORE(i,x) for(__typeof((x).begin()) i=(x).begin();i != (x).end();++i)<br>#define PB push_back<br>#define SS stringstream<br>#define ALL(x) x.begin(),x.end()<br>typedef long long int lli;<br>typedef pair&lt;int, int&gt; P;<br> <br>typedef vector&lt;int&gt; vi;<br>typedef vector&lt;string&gt; vs;<br> <br>template&lt;class A,class B&gt;<br>A cast(B b){SS ss; A a; ss &lt;&lt; b; ss &gt;&gt; a; return a;}<br> <br>string dp[2][50];<br>string rev(string s){reverse(ALL(s)); return s;}<br> <br>class ReversingPrefixes {<br>  public:<br>  string getMinimal(string s) {<br>    int n=s.size();<br>    dp[0][0]=dp[1][0]=s[0];<br>    FOR(i,1,n-1){<br>      dp[0][i]=min(dp[0][i-1]+s[i],s[i]+dp[1][i-1]);<br>      dp[1][i]=min(s[i]+dp[1][i-1],dp[0][i-1]+s[i]);<br>    }<br>    return dp[0][n-1];<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>