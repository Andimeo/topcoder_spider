            #include &lt;algorithm&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br>#include &lt;ctime&gt;<br>#include &lt;functional&gt;<br>#include &lt;iostream&gt;<br>#include &lt;map&gt;<br>#include &lt;numeric&gt;<br>#include &lt;queue&gt;<br>#include &lt;set&gt;<br>#include &lt;sstream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br><br>using namespace std;<br><br>#define VAR( a, b ) typeof(b) a = (b)<br>#define FOREACH( it, c ) for( VAR( it, (c).begin()); it != (c).end(); ++it )<br><br>const int MOD = 1000000007;<br><br>int choose[51][51];<br>map&lt; pair&lt;int, int&gt;, int &gt; Mpower;<br>map&lt; pair&lt;int, int&gt;, int &gt; M;<br><br>int power( int n, int k ) {<br>    long long res = 1;<br><br>    while( k-- ) <br>        res = ( res * n ) % MOD;<br><br>    return ( int )res;<br>}<br><br>int calc( int n, int k ) {<br>    if( n == 1 )<br>        return 1;<br>    if( M.count( make_pair( n, k ) ) )<br>        return M[ make_pair( n, k ) ];<br><br>    long long res;<br><br>    if( n%2 ) {<br>        res = calc( n - 1, k );<br>        res = ( res + power( n, k ) ) % MOD;<br>    } else {<br>        res = ( calc( n / 2, k ) * 2 ) % MOD;<br>        for( int i = 1; i &lt;= k; ++i ) {<br>            long long z = ( ( long long )choose[k][i] * power( n / 2, i ) ) % MOD;<br>            long long zz = ( z * calc( n / 2, k - i ) ) % MOD;<br>            res = ( res + zz ) % MOD;<br>        }<br>    }<br><br>    return M[ make_pair( n, k ) ] = ( int )res;<br>}<br><br>class SumOfPowers {<br>public:<br>    int value(int n, int k) {<br>        for( int i = 0; i &lt;= k; ++i ) {<br>            choose[i][0] = 1;<br>            for( int j = 1; j &lt;= i; ++j )<br>                choose[i][j] = ( choose[i-1][j-1] + choose[i-1][j] ) % MOD;<br>        }<br><br>        return calc( n, k );<br>    }<br>    <br> <br>};<br><br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>