            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class CircularCells {<br>    private int Dist(int r1, int c1, int r2, int c2)<br>    {<br>        return Math.Abs(r1 - r2) + Math.Abs(c1 - c2);<br>    }<br><br>    public int minimumMovements(int N, int M, int r1, int c1, int r2, int c2) {<br>        int res = int.MaxValue;<br>        for (int i = -100; i &lt; 100; i++)<br>            for (int j = -100; j &lt; 100; j++)<br>                res = Math.Min(Dist(r1, c1, N * i + r2, M * j + c2), res);<br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>