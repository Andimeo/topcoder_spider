            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class VariableSpeedLimit<br>{<br>  public double journeyTime(int journeyLength, int[] speedLimit)<br>  {<br>    double time = 0;<br>    while(journeyLength&gt;0)<br>    {<br>      for(int i=0; i&lt;speedLimit.Length; i++)<br>      {<br>        if(journeyLength&gt;speedLimit[i]) <br>        {<br>          journeyLength-=speedLimit[i];<br>          time += 1;<br>        }<br>        else <br>        {<br>          time += (journeyLength*1.0)/speedLimit[i];<br>          journeyLength = 0;<br>        }<br>      }<br>    }<br>    return time;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>