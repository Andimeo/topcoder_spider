            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class VariableSpeedLimit {<br>public:<br>  double journeyTime(int, vector &lt;int&gt;);<br>};<br> <br>double VariableSpeedLimit::journeyTime(int dist, vector &lt;int&gt; v) {<br>  double t=0.0;<br>  while(dist&gt;0){<br>  for(int i=0;i&lt;v.size();i++){<br>    if(dist-v[i]&lt;0){<br>      t+=(1.0*dist)/(1.0*v[i]);<br>      dist=0;<br>      }<br>    else{<br>      t+=1.0;<br>      dist-=v[i];<br>      }<br>    }<br>  }<br>  return t;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>