            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class VariableSpeedLimit<br>{<br>  public double journeyTime(int j, int[] s)<br>  {<br>  double time=0;<br>  int k=0;<br>    while(j&gt;0)<br>    {<br>    if(s[k]&lt;j)<br>    {<br>      j=j-s[k];<br>      time++;<br>    }<br>    else<br>    {<br>    time=time+(double)j/s[k];<br>    j=0;<br>    }<br>    k=(k+1)%s.length;<br>    }<br>    return time;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>