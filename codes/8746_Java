            public class CollectingMarbles {<br>    public int mostMarbles(int[] marblesWeights, int bagCapacity, int numberOfBags) {<br>        v = marblesWeights;<br>        n = numberOfBags;<br>        c = bagCapacity;<br>        cc = new int[n];<br>        go(0, 0);<br>        return ans;<br>    }<br><br>    private void go(int i, int cnt) {<br>        if(ans == v.length) {<br>            return;<br>        }<br>        if(i == v.length) {<br>            if(cnt &gt; ans) {<br>                ans = cnt;<br>            }<br>            return;<br>        }<br>        boolean[] mask = new boolean[21];<br>        for(int j = 0; j &lt; n; ++j) {<br>            if(!mask[cc[j]]) {<br>                mask[cc[j]] = true;<br>                if(cc[j] + v[i] &lt;= c) {<br>                    cc[j] += v[i];<br>                    go(i + 1, cnt+1);<br>                    cc[j] -= v[i];<br>                }<br>            }<br>        }<br>        go(i + 1, cnt);<br>    }<br><br>    int ans = 0;<br><br>    int[] v;<br>    int[] cc;<br>    int c;<br>    int n;<br>}<br></td>