            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class GrabbingTaxi<br>{<br>  public int minTime(int[] tXs, int[] tYs, int gX, int gY, int walkTime, int taxiTime)<br>  {<br>        int tl = tXs.Length;<br>        int min=int.MaxValue;<br> <br>        int allw=(Math.Abs(gX) + Math.Abs(gY)) * walkTime;<br> <br>        for (int i = 0; i &lt; tl; i++)<br>        {<br>            int tt = (Math.Abs(tXs[i] - gX) + Math.Abs(tYs[i] - gY)) * taxiTime;<br>            int wt = (Math.Abs(tXs[i]) + Math.Abs(tYs[i])) * walkTime;<br> <br>            min = Math.Min(min, tt + wt);<br>        }<br> <br>        return Math.Min(min,allw);<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.1 (alpha) modified by pivanof!<br></td>