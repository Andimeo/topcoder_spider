<td class="problemText" colspan="8" valign="middle" align="left">
            #include&lt;iostream&gt;<br>#include&lt;vector&gt;<br>#include&lt;algorithm&gt;<br>#include&lt;list&gt;<br>#include&lt;map&gt;<br>using namespace std;<br>class GrabbingTaxi{<br>public:<br>    int minTime(vector &lt;int&gt; xx, vector &lt;int&gt; yy, int x, int y, int wt, int tt){<br>  int min,i,p,t;<br>  min=wt*(abs(x)+abs(y));<br>  for(i=0;i&lt;xx.size();i++){<br>      p=wt*(abs(xx[i])+abs(yy[i]));<br>      t=tt*(abs(xx[i]-x)+abs(yy[i]-y));<br>      if((p+t)&lt;min)<br>    min=p+t;<br>  }<br>  return min;<br>    }<br>};<br></td>