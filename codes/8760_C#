<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>public class MaximalProduct<br>{<br>    public static long maximalProduct(int s, int k)<br>    {<br>        long ans = 1;<br>        int low = s / k;<br>        for (int i = 0; i &lt; s % k; i++)<br>        {<br>            ans *= low + 1;<br>        }<br>        for (int i = 0; i &lt; k - s % k; i++)<br>        {<br>            ans *= low;<br>        }<br>        return ans;<br>    }<br>}<br></td>