<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class CircularLine<br>{<br><br>    public int longestTravel(int[] t)<br>    {       <br>        int max=int.MinValue;<br>        int total = 0;<br>        foreach (int i in t)<br>            total += i;<br>        for(int a=0;a&lt;t.Length;a++)<br>            for(int b=a+1;b&lt;t.Length;b++)<br>            {<br>                int ln = 0;<br>                for(int i=a;i&lt;b;i++)<br>                {<br>                    ln += t[i];<br>                }<br>                ln = ln &lt; total - ln ? ln : total - ln;<br>                if (max &lt; ln)<br>                    max = ln;                <br>            }        <br><br><br>        return max;<br>    }<br><br>}<br></td>