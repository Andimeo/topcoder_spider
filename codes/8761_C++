            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class CircularLine {<br>public:<br>  int longestTravel(vector &lt;int&gt;);<br>};<br> <br>int CircularLine::longestTravel(vector &lt;int&gt; t) {<br>  int res=-1;<br>  int tot=0;<br>  int i;<br>  for(i=0;i&lt;t.size();i++)<br>    tot+=t[i];<br>  for(i=0;i&lt;t.size();i++)<br>    for(int j=i+1;j&lt;t.size();j++)<br>    {<br>      int sum=0;<br>      for(int k=i;k&lt;j;k++)<br>        sum+=t[k];<br>      res=max(res,min(sum,tot-sum));<br>    }<br>  return res;<br>}<br></td>