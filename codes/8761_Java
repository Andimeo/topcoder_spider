            //import java.smart.programmer.*;<br>import java.awt.*;<br>import java.math.*;<br>import java.util.regex.*;<br>import static java.util.Arrays.*;<br>import static java.util.Collections.*;<br>import static java.lang.System.*;<br>import static java.lang.Math.*;<br>import java.awt.geom.*;<br>import java.util.*;<br>import java.text.*;<br> <br>//beware of bugs in the following code; I have only made it pass the examples, not proved it correct.<br> <br>public class CircularLine {<br> <br>  final boolean SUBMIT = false;<br> <br>  public int longestTravel(int[] t) {<br>    int ans = 0, n = t.length;<br>    <br>    for(int s = 0; s &lt; n; s++)<br>      for(int e = s+1; e &lt; n; e++)<br>        {<br>          int t1 = 0, t2 = 0;<br>          for(int i = s+1; i &lt;= e; i++)<br>            t1 += t[i];<br>          for(int i = e+1; i &lt; n; i++)<br>            t2 += t[i];<br>          for(int i = 0; i &lt;= s; i++)<br>            t2 += t[i];<br>          ans = max(ans, min(t1, t2));  <br>        <br>        }<br>        <br>    return ans;    <br>    <br>  }<br>  <br>  void print(Object ...O) {if(!SUBMIT)for(Object o:O)err.print(o+" ");err.println();}<br>}<br> <br>//powered by good ol' micro$oft notepad<br></td>