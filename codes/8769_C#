            using System;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class FairDiceGame {<br>    public string getPlayer(int playerCount, int turnNumber) {<br>        long[] am = new long[playerCount];<br>        long p6 = 6;<br>        long p5 = 1;<br>        char res = 'A';<br>        for (int i = 0; i &lt; turnNumber; ++i) {<br>            int j;<br>            for (j = 0; j &lt; playerCount; ++j) {<br>                long cur = am[j] + p5;<br>                if (cur * playerCount &gt; p6)<br>                    continue;<br>                am[j] = cur;<br>                break;<br>            }<br>            if (j &gt;= playerCount)<br>                return "";<br>            res = (char) ('A' + j);<br>            p5 *= 5;<br>            p6 *= 6;<br>            for (j = 0; j &lt; playerCount; ++j)<br>                am[j] *= 6;<br>        }<br>        return "" + res;<br>    }<br>}<br><br><br>// Powered by FileEdit<br></td>