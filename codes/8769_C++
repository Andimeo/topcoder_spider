            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>using namespace std;<br> <br>#define FORE(i,a) for(__typeof((a).begin()) i=(a).begin();i!=(a).end();++i)<br>#define REP(i,n) for(int i=0;i&lt;(n);i++)<br>#define FOR(i,a,b) for(int i=(a);i&lt;=(b);i++)<br>#define FORD(i,a,b) for(int i=(a);i&gt;=(b);i--)<br>typedef vector&lt;int&gt; VI;<br>typedef long double LD;<br>typedef double D;<br>typedef pair&lt;int,int&gt; PII;<br>typedef long long ll;<br>#define F first<br>#define S second<br>#define PB push_back<br>#define MP make_pair<br>#define ALL(x) (x).begin(),(x).end()<br>#define CLR(a,i) memset(a,i,sizeof(a))<br> <br>class FairDiceGame <br>{<br>  public:<br>  string getPlayer(int n, int k) <br>  {<br>    ll x=1;<br>    REP(i,k) x*=6;<br>    ll d=x/n,t[30];<br>    REP(i,30) t[i]=0;<br>    int ret=0;<br>    x/=6;<br>    REP(i,k)<br>    {<br>      int ok=0;<br>      REP(j,n) if(t[j]+x&lt;=d)<br>      {<br>        t[j]+=x;<br>        ret=j;<br>        ok=1;<br>        break;<br>      }<br>      if(!ok) return "";<br>      x/=6;<br>      x*=5;<br>    }<br>    return string("")+char(ret+'A');<br>  }<br>};<br> <br>// Powered by PopsEdit<br></td>