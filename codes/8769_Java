            import java.util.*;<br>import java.io.*;<br><br>public class FairDiceGame {<br><br>  public String getPlayer(int n, int t) {<br>    double[] p = new double[n];<br>    double q = 1;<br>    String res = "";<br>    for (int i = 0; i &lt; t; i++) {<br>      double pp = q / 6;<br>      for (int j = 0; j &lt; n; j++) {<br>        if (p[j] + pp &lt;= (1.0 / n) + 1e-10) {<br>          res = "" + (char)('A' + j);<br>          p[j] += pp;<br>          break;<br>        }<br>        if (j == n - 1) {<br>          return "";<br>        }<br>      }    <br>      q *= 5.0 / 6;      <br>    }<br>    return res;<br>  }<br><br>//  public static void main(String[] args) {<br>//    FairDiceGame temp = new FairDiceGame();<br>//    System.out.println(temp.getPlayer(int playerCount, int turnNumber));<br>//  }<br>}<br><br><br>// Powered by FileEdit<br></td>