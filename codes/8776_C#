            using System;<br><br>public class FIELDDiagrams {<br>    public long countDiagrams(int fieldOrder) {<br>        long[,] am = new long[fieldOrder + 1, fieldOrder + 1];<br>        am[0, 0] = 1;<br>        long res = 0;<br>        for (int i = 1; i &lt;= fieldOrder; ++i)<br>        {<br>            long sum = 0;<br>            for (int j = 0; j &lt;= i; ++j)<br>            {<br>                sum += am[i - 1, j];<br>                am[i, j] = sum;<br>                if (i == fieldOrder)<br>                    res += sum;<br>            }<br>        }<br>        return res - 1;<br>    }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>