            Imports Microsoft.VisualBasic<br>Imports System<br>Imports System.Collections<br>Imports System.Text<br> <br>Public Class FIELDDiagrams<br>    Dim c(42, 42) As Long<br>    Dim lm As Long<br>    Function work(ByVal l As Integer, ByVal n As Integer) as Long<br>        If c(l, n) = -1 Then<br>            If l = 1 Then<br>                If n &gt; 0 Then c(l, n) = 1 Else c(l, n) = 0<br>            Else<br>                Dim t As Long = 0<br>                For i As Long = n To lm - l + 2<br>                    t = t + work(l - 1, i)<br>                Next<br>                c(l, n) = t<br>            End If<br>        End If<br>        Return c(l, n)<br>    End Function<br>    Public Function countDiagrams(ByVal fieldOrder As Integer) As Long<br>        lm = fieldOrder<br>        For i As Integer = 0 To 40<br>            For j As Integer = 0 To 40<br>                c(i, j) = -1<br>            Next<br>        Next<br>        Return work(lm, 0) + work(lm, 1)<br>    End Function<br> <br>End Class<br></td>