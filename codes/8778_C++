            #include "iostream"<br>#include "vector"<br>#include "string"<br>#include "algorithm"<br>#include "math.h"<br>#include "map"<br>#include "sstream"<br>using namespace std;<br><br>class LightedPanels<br>{<br>public:<br>  int g[10][10];<br>  int tt[10][10];<br>  int W,H;<br>  void flip(int x, int y)<br>  {<br>    for(int i=-1;i&lt;=1;i++)<br>    {<br>      for(int j=-1;j&lt;=1;j++)<br>      {<br>        int xx=x+i;<br>        int yy=y+j;<br>        if(xx&lt;0 || xx&gt;=H || yy&lt;0 || yy&gt;=W)<br>          continue;<br>        tt[xx][yy]=1-tt[xx][yy];<br>      }<br>    }<br>  }<br>  int solve()<br>  {<br>    int steps=0;<br>    for(int i=1;i&lt;H;i++)<br>    {<br>      for(int j=1;j&lt;W;j++)<br>      {<br>        if(tt[i-1][j-1]==0)<br>        {<br>          flip(i,j);<br>          steps++;<br>        }<br>      }<br>    }<br>    for(int i=0;i&lt;H;i++)<br>    {<br>      for(int j=0;j&lt;W;j++)<br>      {<br>        if(tt[i][j]==0)<br>          return 65;<br>      }<br>    }<br>    return steps;<br>  }<br>  int minTouch(vector &lt;string&gt; board)<br>  {<br>    for(int i=0;i&lt;10;i++)<br>      for(int j=0;j&lt;10;j++)<br>        g[i][j]=0;<br>    int min=65;<br>    H=board.size();<br>    for(int i=0;i&lt;board.size();i++)<br>    {<br>      string str=board[i];<br>      W=str.size();<br>      for(int j=0;j&lt;str.size();j++)<br>      {<br>        if(str[j]=='*')<br>          g[i][j]=1;<br>        else<br>          g[i][j]=0;<br>      }<br>    }<br>    for(int i=0;i&lt;=0x7fff;i++)<br>    {<br>      for(int x=0;x&lt;10;x++)<br>        for(int y=0;y&lt;10;y++)<br>          tt[x][y]=g[x][y];<br>      int mask=0x01;<br>      int step=0;<br>      for(int j=0;j&lt;W;j++)<br>      {<br>        if(i&amp;mask)<br>        {<br>          flip(0,j);<br>          step++;<br>        }<br>        mask&lt;&lt;=1;<br>      }<br>      for(int j=1;j&lt;H;j++)<br>      {<br>        if(i&amp;mask)<br>        {<br>          flip(j,0);<br>          step++;<br>        }<br>        mask&lt;&lt;=1;<br>      }<br>      int moves=solve()+step;<br>      if(min&gt;moves)<br>        min=moves;<br>    }<br>    if(min&gt;=65)<br>      return -1;<br>    return min;<br>  }<br>};<br></td>