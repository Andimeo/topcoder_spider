            using System; <br>using System.Collections; <br>using System.Collections.Generic; <br>using System.Collections.Specialized; <br>using System.Text; <br>using System.Text.RegularExpressions; <br> <br>public class RestaurantManager {<br>    public int allocateTables(int[] tables, int[] groupSizes, int[] arrivals, int[] departures) <br>    {<br>        int cnt = 0;<br>        int N = tables.Length;<br>        int M = groupSizes.Length;<br>        int[] used = new int[N];<br>        for (int i = 0; i &lt; used.Length; i++)<br>        {<br>            used[i] = -1;<br>        }<br>        Array.Sort(arrivals, departures);<br>        Array.Sort(tables);<br><br>        for (int i = 0; i &lt; M; i++)<br>        {<br>             int curTime = arrivals[i];<br>            for (int k = 0; k &lt; used.Length; k++)<br>            {<br>                if (used[k] != -1 &amp;&amp; departures[used[k]] &lt;= curTime)<br>                    used[k] = -1;<br>            }<br>            bool flag = false;<br>            for (int j = 0; j &lt; N; j++)<br>            {<br>                if (tables[j] &gt;= groupSizes[i] &amp;&amp; used[j] == -1)<br>                {<br>                    used[j] = i;<br>                    flag = true;<br>                    break;<br>                }<br>            }<br>            if (!flag)<br>                cnt += groupSizes[i];<br>        }<br>        return cnt;<br>    }<br><br>    <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>