            #include&lt;iostream&gt;<br>#include&lt;cstdio&gt;<br>#include&lt;cmath&gt;<br>#include&lt;string&gt;<br>#include&lt;vector&gt;<br>#include&lt;cstring&gt;<br>#include&lt;stack&gt;<br>#include&lt;queue&gt;<br>#include&lt;set&gt;<br>#include&lt;map&gt;<br>#include&lt;utility&gt;<br>#include&lt;algorithm&gt;<br>#include&lt;sstream&gt;<br>#include&lt;cctype&gt;<br>#include&lt;cstdlib&gt;<br>using namespace std;<br>class RestaurantManager<br>{<br> public:<br> int allocateTables(vector &lt;int&gt; t, vector &lt;int&gt; g, vector &lt;int&gt; a, vector &lt;int&gt; d)<br> { <br>  int count=0;<br>  sort(t.begin(),t.end());<br>  vector&lt;int&gt;end(t.size(),0);<br>  for(int i=0;i&lt;a.size();i++)<br>  {<br>   bool found=false;<br>   for(int j=0;j&lt;t.size();j++)<br>   { <br>    if(g[i]&gt;t[j])<br>    continue;<br>    if(a[i]&gt;=end[j])<br>    {<br>     end[j]=d[i];<br>     found=true;<br>     break;<br>     }<br>    }<br>   if(!found)<br>   count+=g[i];<br>   }<br>  return count;<br>  }<br>};<br></td>