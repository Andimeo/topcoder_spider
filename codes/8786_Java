            import java.util.*;<br>import java.math.*;<br><br>public class RestaurantManager {<br>  public static int allocateTables(int[] tables, int[] groupSizes, int[] arrivals, int[] departures){<br>    Arrays.sort(tables);<br>    int [] allocate = new int [tables.length];<br>    Arrays.fill(allocate, -1);<br>    int ret = 0;<br>    for(int i = 0;i &lt; groupSizes.length;i++){<br>      boolean flag = true;<br>      for(int j = 0;j &lt; tables.length &amp;&amp; flag;j++){<br>        if(tables[j] &gt;= groupSizes[i] &amp;&amp; arrivals[i] &gt;= allocate[j]){<br>          flag = false;<br>          allocate[j] = departures[i];<br>        }<br>      }<br>      if(flag)<br>        ret+= groupSizes[i];<br>    }<br>    return ret;<br>  }<br>  public static void main(String[] args) {<br>//    System.out.println("Hello World");<br>  }<br>}<br></td>