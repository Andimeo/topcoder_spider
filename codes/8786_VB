            Imports Microsoft.VisualBasic<br> <br>Public Class RestaurantManager<br>  Public Function allocateTables(ByVal tables As Integer(), ByVal groupSizes As Integer(), ByVal arrivals As Integer(), ByVal departures As Integer()) As Integer<br>        Dim k As Integer = 0<br>        Dim t As Integer = tables.Length<br>        Array.Sort(tables)<br>        Dim avail(t) As Integer<br>        Dim ztime(t) As Integer<br>        Dim n As Integer = groupSizes.Length<br> <br>        'arrivals<br>        For i As Integer = 0 To n - 1<br>            'what can be free<br>            For j As Integer = 0 To t - 1<br>                If avail(j) = 1 And arrivals(i) &gt;= ztime(j) Then<br>                    avail(j) = 0<br>                End If<br>            Next<br> <br>            k += groupSizes(i)<br>            For j As Integer = 0 To t - 1<br>                If avail(j) = 0 AndAlso tables(j) &gt;= groupSizes(i) Then<br>                    avail(j) = 1<br>                    ztime(j) = departures(i)<br>                    k -= groupSizes(i)<br>                    Exit For<br>                End If<br>            Next<br>        Next<br> <br>    Return k<br>  End Function<br> <br>End Class<br></td>