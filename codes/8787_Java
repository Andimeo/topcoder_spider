            import java.util.*;<br>public class DividingIntoTeams<br>{<br>  public String findYourTeam(int[] alexFriends, int[] bobFriends, int[] charlieFriends, int k)<br>  {<br>    int visited[]=new int[alexFriends.length+1];<br>    int len = visited.length; <br>    int a [][] = new int[3][len];<br>    for(int i=0; i&lt;len-1; i++)<br>    {<br>      a[0][i] = alexFriends[i];<br>      a[1][i] = bobFriends[i];<br>      a[2][i] = charlieFriends[i];<br>    }<br>    Arrays.fill(visited, -1);<br>  <br>    for(int i=0; i&lt;visited.length; i++)<br>    {<br>      for(int j=0; j&lt;len; j++)<br>      {<br>        if(visited[a[i%3][j]] == -1)<br>        {<br>          visited[a[i%3][j]] = i%3;<br>          break;<br>        }<br>      }<br>    }<br>    if(visited[k]==0)<br>      return "Alex";<br>    else if(visited[k] == 1)<br>      return "Bob";<br>    return "Charlie";<br>  }<br>}<br></td>