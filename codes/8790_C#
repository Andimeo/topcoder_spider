            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class TimeToSay <br>{<br>  public int maximumJoy(int[] lostHealth, int[] joy) <br>    {<br>        int[] dp = new int[101];<br>        int n = lostHealth.Length;<br>        for (int i = 0; i &lt; n; i++)<br>        {<br>            for (int j = 100; j &gt;= lostHealth[i]; j--)<br>            {<br>                if(j - lostHealth[i] &gt; 0)<br>                    dp[j] = Math.Max(dp[j], dp[j - lostHealth[i]] + joy[i]);<br>            }<br>        }<br>        int ret = 0;<br>        for (int i = 0; i &lt; 101; i++)<br>        {<br>            if (dp[i] &gt; ret)<br>                ret = dp[i];<br>        }<br>        return ret;<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>