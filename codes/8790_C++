            #include&lt;stdio.h&gt;<br>#include&lt;vector&gt;<br>#include&lt;algorithm&gt;<br>#include&lt;string&gt;<br>#include&lt;math.h&gt;<br>#include&lt;map&gt;<br>using namespace std;<br><br>class TimeToSay<br>{<br>public:<br>  int maximumJoy(vector&lt;int&gt;a,vector&lt;int&gt;b)<br>  {<br>    int i,j;<br>    int ans=0;<br>    int ss,tt;<br>    int N=a.size();<br>    for(i=0;i&lt;(1&lt;&lt;N);i++)<br>    {<br>      ss=tt=0;<br>      for(j=0;j&lt;N;j++)<br>      {<br>        if(i&amp;(1&lt;&lt;j))<br>          ss+=a[j],tt+=b[j];<br>      }<br>      if(ss&lt;100&amp;&amp;tt&gt;ans)<br>        ans=tt;<br>    }<br>    return ans;<br>  }<br>};<br></td>