            import static java.lang.Math.*;<br>import static java.util.Arrays.*;<br> <br>public class TimeToSay<br>{<br>    public int maximumJoy(int[] lostHealth, int[] joyarray)<br>    {<br>        int hel = 100;<br>        int joy = 0;<br>        int n = lostHealth.length;<br>        int state[] = new int[103];<br>        for(int i = 0; i &lt; n; i++)<br>        {<br>            for(int j = lostHealth[i] + 1; j &lt; 101; j++)<br>            {<br>                state[j - lostHealth[i]] = max(state[j - lostHealth[i]], state[j] + joyarray[i]);<br>            }   <br>        }<br>        <br>        int ret = 0;<br>        for(int i = 1; i &lt; 103; i++)<br>            ret = max(ret, state[i]);<br>        return ret;<br>    }<br>    <br>    <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>