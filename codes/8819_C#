            using System;<br>using System.Text;<br>using System.Collections.Generic;<br><br>public class WhackAMole {<br>  int countBits(int x)<br>  {<br>    int cnt=0;<br>    while (x&gt;0) {<br>      if ((x&amp;1)&gt;0) cnt++;<br>      x/=2;<br>    }<br>    return cnt;    <br>  }  <br>  <br>  string GetString(int x, int len)<br>  {<br>    StringBuilder sb = new StringBuilder();<br>    for(int k=0;k&lt;len;k++)<br>      if (((1&lt;&lt;k) &amp; x) &gt; 0) sb.Append('X'); else sb.Append('O');<br>    return sb.ToString();<br>  }<br>  <br>  int rotate(int t, int len)<br>  {<br>    t=t*2;<br>    if (t&gt;=(1&lt;&lt;len)) {<br>      t-=(1&lt;&lt;len);<br>      t++;<br>    }<br>    return t;<br>  }<br>  <br>  public string placeMoles(int numHoles, int numHammers) {        <br>    int most = -1;<br>    string best = "";<br>    for(int i=0;i&lt;(1&lt;&lt;numHoles);i++) {<br>      //Console.WriteLine("Trying " + GetString(i, numHoles));<br>      bool ok = true;<br>      for(int j=0;j&lt;(1&lt;&lt;numHoles);j++) {<br>        if (countBits(j) != numHammers) continue;<br>        <br>        bool anyHits = true;<br>        int t=i;<br>        for(int k=0;k&lt;numHoles;k++) {<br>          if ((j&amp;t)==0) anyHits = false;<br>          t=rotate(t,numHoles);          <br>        }<br>        if (anyHits)<br>          ok=false;        <br>      }<br>      <br>      if (ok) {<br>        //Console.WriteLine("OK");<br>        int b=countBits(i);<br>        if (b&gt;=most) {<br>          string s = GetString(i, numHoles);<br>          if (b&gt;most || s.CompareTo(best) &lt; 0) {<br>            most=b;<br>            best=s;<br>          }            <br>        }<br>      }<br>    }<br>    return best;<br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>