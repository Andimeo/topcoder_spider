            import java.util.*;<br>public class WhackAMole {<br>  public String placeMoles(int hol, int ham) {<br>    int[] no = new int[1 &lt;&lt; 10];<br>    for (int i = 1; i &lt; (1 &lt;&lt; 10); i++) {<br>      no[i] = no[i / 2] + i % 2;<br>    }<br>    <br>    int[] ph = new int[1 &lt;&lt; hol];<br>    int m = 0;<br>    for (int i = 0; i &lt; (1 &lt;&lt; hol); i++) {<br>      if (no[i] == ham) {<br>        ph[m++] = i;<br>      }<br>    }<br>    <br>    int bhn = 0;<br>    int bh = 0;<br>    for (int i = 0; i &lt; (1 &lt;&lt; hol); i++) {<br>      if (no[i] &gt; bhn) {<br>        boolean ok = true;<br>        int t = i;<br>        for (int j = 0; j &lt; m; j++) {<br>          boolean can = false;<br>          for (int k = 0; k &lt; hol; k++) {<br>            if ((t &amp; ph[j]) == 0) {<br>              can = true;<br>              break;<br>            }<br>            t = (t &gt;&gt; 1) + ((t % 2) &lt;&lt; (hol - 1));<br>          }<br>          if (!can) {<br>            ok = false;<br>            break;<br>          }<br>        }<br>        if (ok) {<br>          bhn = no[i];<br>          bh = i;<br>        }<br>      }<br>    }<br>    <br>    String r = "";<br>    <br>    for (int i = 0; i &lt; hol; i++) {<br>      if (bh % 2 == 0) {<br>        r = "O" + r;<br>      } else {<br>        r = "X" + r;<br>      }<br>      bh /= 2;<br>    }<br>    return r;<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>