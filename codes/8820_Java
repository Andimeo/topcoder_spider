            import java.util.*;<br><br>public class OptimalPlay {<br><br>  public double winnings(double[] bp, double[] cp) {<br>    // first bet<br>    int n = bp.length;<br>    double res = 0;<br>    for (int i = 0; i &lt; n; i++) {<br>      double q = 0;<br>      for (int j = 0; j &lt; n; j++) {<br>        double p = 1.0 / n * bp[j];<br>        if (j &lt; i) q += 2 * p;<br>        if (j == i) q += 0.5 * p;<br>        if (j &gt; i) q -= p;<br>      }<br>      if (q &gt; 0) {<br>        // call<br>        res += q / n;<br>        System.out.println("" + i + " " + q);<br>      }<br>    }  <br>    <br>    System.out.println(res);<br>    <br>    // first check<br>    for (int i = 0; i &lt; n; i++) {<br>      double q = 0;<br>      double w = 0;<br>      for (int j = 0; j &lt; n; j++) {        <br>        // bet<br>        //call<br>        double p = 1.0 / n * cp[j];<br>        if (j &lt; i) q += 2 * p;<br>        if (j == i) q += 0.5 * p;<br>        if (j &gt; i) q -= p;<br>        //fold<br>        p = 1.0 / n * (1 - bp[j] - cp[j]);<br>        q += p;<br>        <br>        //check<br>        p = 1.0 / n * (1 - bp[j]);<br>        if (j &lt; i) w += p;<br>        if (j == i) w += 0.5 * p;<br>        if (j &gt; i) w += 0; //...<br>      }      <br>      if (q &gt; w) {        <br>        //bet<br>        res += q / n;<br>        System.out.println("bet " + i + " " + q);<br>      } else {<br>        //check<br>        res += w / n;<br>        System.out.println("check " + i + " " + w);<br>      }<br>    }<br>      <br>    System.out.println(res);<br>    <br>    return res;<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br></td>