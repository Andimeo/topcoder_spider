            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class EvilRobot {<br>public:<br>  string createProgram(int, int, int, int, int);<br>};<br> <br>string EvilRobot::createProgram(int N, int rx, int ry, int tx, int ty) {<br>  <br>  string NO = "I'LL GET YOU NEXT TIME";<br>  <br>  if (tx&lt;rx || ty&lt;ry)<br>  {<br>    return NO;<br>  }<br>  <br>  int RR = tx-rx;<br>  int UU = ty-ry;<br>  int TOT = RR+UU;<br>  <br>  if (TOT%N == 0)<br>  {<br>    int C = TOT/N;<br>    int R1, U1;<br>    bool ok = false;<br>    for (R1=N; R1&gt;=0; R1--)<br>    {<br>      U1 = N-R1;<br>      if (R1*C==RR &amp;&amp; U1*C==UU)<br>      {<br>        ok = true;<br>        break;<br>      }<br>    }<br>    if (ok == true)<br>    {<br>      string ans = "";<br>      while (R1--)<br>        ans += 'R';<br>      while (U1--)<br>        ans += 'U';<br>      return ans;<br>    }<br>    else{<br>      return NO;<br>    }<br>  }<br>  else{<br>    int C = TOT/N+1;<br>    int M = TOT%N;<br>    int R1, U1, R2, U2;<br>    bool ok = false;<br>    for (R1=M; R1&gt;=0; R1--)<br>    {<br>      U1 = M-R1;<br>      if (C != 1){<br>      R2 = (RR-R1*C)/(C-1);<br>      U2 = N-M-R2;<br>      if (U1*C+U2*(C-1) == UU)<br>      {<br>        if (R1&gt;=0 &amp;&amp; R1&lt;=M)<br>        if (U1&gt;=0 &amp;&amp; U1&lt;=M)<br>        if (R2&gt;=0 &amp;&amp; R2&lt;=N-M)<br>        if (U2&gt;=0 &amp;&amp; U2&lt;=N-M)<br>        {<br>          ok = true;<br>          break;<br>        }<br>      }}<br>      else{<br>        R2 = N-M;<br>        U2 = 0;<br>        if (R1*C==RR &amp;&amp; U1*C==UU)<br>        {<br>          ok = true;<br>          break;<br>        }<br>      }<br>    }<br>    if (ok == false)<br>      return NO;<br>    else{<br>      string ans = "";<br>      while (R1--)<br>        ans += 'R';<br>      while (U1--)<br>        ans += 'U';<br>      while (R2--)<br>        ans += 'R';<br>      while (U2--)<br>        ans += 'U';<br>      return ans;<br>    }<br>  }<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>