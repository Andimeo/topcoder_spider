            import static java.lang.Math.*;<br>import static java.util.Arrays.*;<br>import java.util.*;<br> <br>public class EvilRobot<br>{<br>    public String createProgram(int N, int robotX, int robotY, int trapX, int trapY)<br>    {<br>        String ret = "";  <br>        if(robotX &gt; trapX ||  robotY &gt; trapY)<br>            return "I'LL GET YOU NEXT TIME";   <br>        if(robotX == trapX &amp;&amp;  robotY == trapY)<br>        {<br>            for(int j = 0; j &lt; N; j++){<br>                ret += 'R';<br>            }<br>            return ret;<br>        }<br>        for(int i = 0; i &lt;= N; i++){<br>            int R = N - i;<br>            int U = i;              <br>            int k = 0;<br>            if(R == 0){<br>                k = (trapY - robotY) / U;<br>            }else if(U == 0)<br>            {<br>                k = (trapX - robotX) / R;<br>            }else<br>            {<br>                k = min((trapY - robotY) / U, (trapX - robotX) / R);<br>            }<br>            <br>            //System.out.println(" " + i + " " + k);<br>            int Rl = trapX - robotX - k * R;<br>            int Ul = trapY - robotY - k * U;<br>            if(Rl &lt; 0 || Ul &lt; 0 || Rl &gt; R  || Ul &gt; U)<br>                continue;<br>            String tem = "";<br>            for(int j = 0; j &lt; N; j++)<br>            {<br>                if(j &lt; Rl){<br>                    tem += 'R';<br>                }else if(j &lt; Rl + Ul){<br>                    tem += 'U';<br>                }else if(j &lt; Ul + R){<br>                    tem += 'R';<br>                }else<br>                {<br>                    tem += 'U';<br>                }<br>                    <br>            }<br>            <br>            if(ret == "")<br>            {<br>                ret = tem;<br>            }else<br>            {<br>                for(int j = 0; j &lt; N; j++){<br>                    if(tem.charAt(j) &lt; ret.charAt(j)){<br>                        ret = tem;<br>                        break;<br>                    }<br>                }<br>            }<br>                        <br>        }<br>        <br>        if(ret == "")<br>            ret = "I'LL GET YOU NEXT TIME";<br>        return ret;<br>    }<br>    <br>    <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>