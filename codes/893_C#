            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class QuadraticRoots {<br>    void aa( ArrayList al, int n ) {<br>        if ( !al.Contains(n)) al.Add(n);<br>    }<br>    void add( double a, double b, double c, ArrayList al ) {<br>        if ( a == 0 ) return;<br>        double d = b*b-4*a*c;<br>        if ( d &lt; 0 ) return;<br>        double x = ((-b+Math.Sqrt(d))/(2*a));<br>        double y = ((-b-Math.Sqrt(d))/(2*a));<br>        if ( y == Math.Floor(y)) aa(al,(int)Math.Floor(y));<br>        if ( x == Math.Floor(x)) aa(al,(int)Math.Floor(x));<br>    }<br>    public int[] findRoots(int[] a, int[] b, int[] c) {<br>        ArrayList al = new ArrayList();<br>        for ( int i = 0 ; i != a.Length; i++ ) {<br>            for ( int j = 0; j != b.Length; j++ ) {<br>                for ( int k = 0 ; k != c.Length; k++ ) {<br>                    add( a[i], b[j], c[k], al );<br>                }<br>            }<br>        }<br>        al.Sort();<br>        return (int[])al.ToArray(typeof(int));<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>