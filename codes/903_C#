            using System;<br>using System.Collections;<br><br>public class UGroupOrder <br>{<br>  static int gcd (int x, int y)<br>  {<br>    if (x == 0)<br>      return y;<br>    else<br>      return gcd(y % x, x);<br>  }<br>  static int order (int x, int N)<br>  {<br>    int y = x;<br><br>    for (int i = 1;; i++)<br>      if (x % N == 1)<br>        return i;<br>      else<br>        x = x * y % N;<br>  }<br><br>  public static int[] findOrders (int N) <br>  {<br>    ArrayList a = new ArrayList();<br><br>    for (int i = 1; i &lt; N; i++)<br>      if (gcd(i, N) == 1)<br>        a.Add(i);<br><br>    //Code.print(a);<br><br>    int[] o = new int[a.Count];<br><br>    for (int i = 0; i &lt; a.Count; i++)<br>      o[i] = order((int)a[i], N);<br><br>    return o;<br>  }<br><br>  public static void Main ()<br>  {<br>    /*<br>    Code.print(findOrders(8));<br>    Code.print(findOrders(9));<br>    Code.print(findOrders(15));<br>    Code.print(findOrders(51));<br>    Code.print(findOrders(10));<br>    Code.print(findOrders(2));<br>    */<br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>