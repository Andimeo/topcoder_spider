            #line 144 "UGroupOrder.cpp"<br> <br>using namespace std;<br> <br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>class UGroupOrder {<br>  public:<br>  vector &lt;int&gt; findOrders(int N) {<br>      vector&lt;int&gt; result;<br>      <br>      for (int i = 0; i &lt; N; i++) {<br>    int p = 1;<br>    int j;<br>    for (j = 0; j &lt; N; j++) {<br>        p *= i;<br>        p %= N;<br>        if (p == 1)<br>      break;<br>    }<br>    if (p == 1)<br>        result.push_back( j+1 );<br>      }<br>      <br>      return result;<br>  }<br>};<br> <br> <br>// Powered by FileEdit<br></td>