<td class="problemText" colspan="8" valign="middle" align="left">
            public class UGroupOrder {<br>    public int gcd(int a, int b) { // gcd(0,0)=0<br>        if (a&lt;0) a=-a;<br>        if (b&lt;0) b=-b;<br>        while (a!=0 &amp;&amp; b!=0) {<br>            a%=b;<br>            if (a!=0)<br>                b%=a;<br>        }<br>        return (a==0)?b:a;<br>    }<br> <br>int order(int k,int N) {<br>  int res=1,pow=k;<br>  while (pow!=1) {<br>    pow=(pow*k)%N;<br>    res++;<br>  }<br>  return res;<br>}<br> <br>public int[] findOrders(int N) {<br>  int[] r=new int[N];<br>  int i=0;<br>  for (int k=1;k&lt;N;k++) {<br>    if (gcd(k,N)==1) r[i++]=order(k,N);<br>  }<br>  int[] res=new int[i];<br>  System.arraycopy(r,0,res,0,i);<br>  return res;<br>}<br> <br>}<br></td>