            using System;<br>using System.Collections;<br><br>public class BankingArray <br>{<br>  public int addCost(int initialCapacity, int numAdds) <br>  {<br>    int size = 0;<br>    int cost = 0;<br>    while(numAdds &gt; 0)<br>    {<br>      if(size == initialCapacity)<br>      {<br>        initialCapacity *= 2;<br>        cost += size;<br>      }<br>      int takeaway = Math.Min(numAdds, initialCapacity - size);<br>      cost += takeaway;<br>      size += takeaway;<br>      numAdds -= takeaway;<br>    }<br>    return cost;<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>