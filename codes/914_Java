            import java.util.*;<br>import java.io.*;<br>import java.math.*;<br>import static java.lang.Math.*;<br> <br>public class BankingArray {<br>  public int addCost(int initialCapacity, int numAdds) {<br>    int ret=0;<br>    int capacity=initialCapacity;<br>    int size=0;<br>    try {<br>      while (numAdds&gt;0) {<br>        if (size != capacity) {<br>          int diff = capacity - size;<br>          diff = min(diff, numAdds);<br>          ret+=diff;<br>          size+=diff;<br>          numAdds-=diff;<br>        } else {<br>          // Full, double it<br>          ret+=capacity;<br>          capacity*=2;<br>        }<br>      }<br>    } catch (Exception e) {<br>      System.out.println(e);<br>      e.printStackTrace(System.out);<br>    }<br>    return ret;<br>  }<br> <br>  public static void println(String s) {<br>    System.out.println(s);<br>  }<br> <br> <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>