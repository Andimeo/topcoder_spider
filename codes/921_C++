            #line 4 "TCU.cpp"<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;iostream&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;map&gt;<br>#include &lt;sstream&gt;<br><br>using namespace std;<br><br>// START of code template<br><br>template&lt;class T&gt;<br>vector&lt;T&gt; split(string s, string tok, bool flag=false)<br>{<br>  s+=tok;<br>  int cur=0,i;<br>  vector&lt;T&gt; ans;<br>  while ((i=s.find(tok,cur))!=s.npos) {<br>    stringstream ss;<br>    ss &lt;&lt; string(&amp;s[cur],&amp;s[i]);<br>    T tmp;<br>    if ((ss &gt;&gt; tmp) || flag)<br>      ans.push_back(tmp);<br>    cur=i+tok.size();<br>  }<br>  return ans;<br>}<br><br>template&lt;class T&gt;<br>ostream &amp;operator&lt;&lt;(ostream &amp;os, vector&lt;T&gt; v)<br>{<br>  os &lt;&lt; "{";<br>  for(int i=0;i&lt;v.size();i++) {<br>    if (i) os &lt;&lt; ",";<br>    os &lt;&lt; v[i];<br>  }<br>  os &lt;&lt; "}";<br>  return os;<br>}<br><br>// END of code template<br><br>class TCU {<br>public:<br>  vector &lt;int&gt; majors(vector &lt;string&gt; percentages, vector &lt;int&gt; start, int years) {<br>    vector&lt;vector&lt;int&gt; &gt; p;<br>    int n=percentages.size();<br>    for(int i=0;i&lt;n;i++)<br>      p.push_back(split&lt;int&gt;(percentages[i]," "));<br>    while (years--) {<br>      vector&lt;int&gt; cnt(n,0);<br>      for(int i=0;i&lt;n;i++) {<br>  int left=start[i];<br>  for(int j=0;j&lt;n;j++) {<br>    cnt[j]+=start[i]*p[i][j]/100;<br>    left-=start[i]*p[i][j]/100;<br>  }<br>  cnt[i]+=left;<br>      }<br>      //      cout &lt;&lt; cnt &lt;&lt; endl;<br>      start=cnt;<br>    }<br>    return start;<br>  }<br>};<br><br><br>// Powered by FileEdit<br></td>