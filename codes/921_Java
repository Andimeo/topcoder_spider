            import java.util.*;<br>import java.awt.geom.*;<br>import java.math.*;<br>import java.awt.Polygon;<br> <br>public class TCU {<br>  public int[] majors(String[] percentages, int[] start, int years) {<br>    int N = percentages.length;<br>    <br>    int[][] p = new int[N][N];<br>    for (int i = 0; i &lt; N; i++) {<br>      StringTokenizer st = new StringTokenizer(percentages[i]);<br>      int j = 0;<br>      while (st.hasMoreTokens()) {<br>        p[i][j++] = Integer.parseInt(st.nextToken());<br>      }<br>    }<br>    <br>    for (int x = 0; x &lt; years; x++) {<br>      int[] end = new int[N];<br>      for (int i = 0; i &lt; N; i++) {<br>        int s = 0;<br>        for (int j = 0; j &lt; N; j++) {<br>          if (i != j) {<br>            end[j] += p[i][j] * start[i] / 100;<br>            s += p[i][j] * start[i] / 100;<br>          }<br>        }<br>        end[i] += (start[i] - s);<br>      }<br>      <br>      for (int a = 0; a &lt; N; a++) {<br>        start[a] = end[a]; end[a] = 0;<br>      }<br>    }<br>    <br>    return start;<br>  }<br>}<br> <br>// Powered by PopsEdit<br></td>