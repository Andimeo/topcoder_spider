            using System; using System.Collections; public class Inserter { public String insert(String[] com, String s) {<br>foreach (String cc in com)<br>{<br>String c = cc;<br>c = c.Substring(1);<br>int i = c.IndexOf('#');<br>String insert = c.Substring(0,i);<br>c = c.Substring(i+1);<br> <br>c = c.Substring(c.IndexOf('#') + 1);<br>i = c.IndexOf('#');<br>String before = c.Substring(0,i);<br>c = c.Substring(i+1);<br> <br>int index = Convert.ToInt32(c.Trim());<br>// Console.WriteLine("'{0}' '{1}' {2}", insert, before, index);<br> <br>i = 0;<br>while (i &lt;= s.Length - before.Length &amp;&amp; index &gt; 0)<br> if (s.Substring(i, before.Length) == before)<br>  { index--; if (index &gt; 0) i++; else break; }<br> else<br>  i++;<br> <br>if (index == 0)<br> s = s.Substring(0, i) + insert + s.Substring(i);<br>}<br> <br>return s;<br>}}<br></td>