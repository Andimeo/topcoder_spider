            import java.util.*;<br>public class Inserter {<br>  public String insert( String[] commands, String original ) {<br>    for ( int i = 0; i &lt; commands.length; i++ ) {<br>      StringTokenizer st = new StringTokenizer( commands[i], "#" );<br>      String insert = st.nextToken();<br>      String pat = st.nextToken();<br>      if ( pat.trim().length() == 0 )<br>        pat = st.nextToken();<br>      int times = Integer.parseInt( st.nextToken().trim() );<br> <br>      for ( int j = 0; j &lt; original.length(); j++ ) {<br>        if ( original.substring( j ).startsWith( pat ) )<br>          times--;<br>        if ( times == 0 ) {<br>          original = original.substring( 0, j ) + insert + original.substring( j );<br>          break;<br>        }<br>      }<br>    }<br>    return original;<br>  }<br>}<br></td>