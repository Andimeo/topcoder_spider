            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br><br>public class GameOfLife {<br>  public int alive(string[] m, string rr, int gg) {<br>    int R=m.Length,C=m[0].Length;<br>    char[][] g= new char[R][];<br>    for( int r= 0; r != R; r++)<br>      g[r]= m[r].ToCharArray();<br>    for( int gx= 0; gx != gg; gx++) {<br>      char[][] gn= new char[R][];<br>      for( int r= 0; r != R; r++) {<br>        gn[r]= new char[C];<br>        for ( int c= 0; c != C; c++) {<br>          int lv= 0;<br>          for( int dx= -1; dx &lt;= 1; dx++)<br>            for( int dy= -1; dy != 2; dy++) {<br>              if( dx != 0 || dy != 0) {<br>                int nr= (r+dx+R)%R;<br>                int nc= (C+c+dy)%C;<br>                if(nr&gt;=0&amp;&amp;nc&gt;=0&amp;&amp;nr&lt;R&amp;&amp;nc&lt;C&amp;&amp;g[nr][nc] == 'X') {<br>                  lv++;<br>                }<br>              }<br>            }<br>          switch(rr[lv]) {<br>            case 'D':<br>              gn[r][c]='.';<br>              break;<br>            case 'S':<br>              if(g[r][c]=='X')<br>                gn[r][c]='X';<br>              break;<br>            case 'B':<br>              gn[r][c]='X';<br>              break;<br>          }<br>        }<br>      }<br>      g=gn;<br>    }<br>    int res= 0;<br>    for( int r= 0; r != R; r++)<br>      for( int c= 0; c != C; c++)<br>        if(g[r][c]=='X')<br>          res++;<br>    return res;<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>