            using System;<br>        using System.Collections;<br> <br>        public class BlueMoons<br>        {<br>                public int count(string a,string b)<br>                {<br>                        DateTime d1 = new DateTime(2002,1,1);<br>                        DateTime d2 = new DateTime(2002,1,1);<br>                        d2 = d2.AddDays(29.53);<br>                        TimeSpan diff = d2 - d1;<br>                        //string a ="01/2002 to 05/2002";<br>                        //string b = "01.00/01/2002";<br>                        string[] c = a.Split(new char[]{' '});<br>                        string aa = c[0];<br>                        aa = aa.Replace("/"," ");<br>                        string[] cc = aa.Split(new char[]{' '});<br>                        int month = Int32.Parse(cc[0]);<br>                        int year = Int32.Parse(cc[1]);<br>                        DateTime begin = new DateTime(year,month,1);<br> <br>                         aa = c[2];<br>                        aa = aa.Replace("/"," ");<br>                         cc = aa.Split(new char[]{' '});<br>                         month = Int32.Parse(cc[0]);<br>                         year = Int32.Parse(cc[1]);<br>                        DateTime end = new DateTime(year,month,1);<br>                        b = b.Replace("/"," ");<br>                        cc = b.Split(new char[]{' '});<br>                        double day = Double.Parse(cc[0]);<br>                        int d = (int)day;<br>                         month = Int32.Parse(cc[1]);<br>                         year = Int32.Parse(cc[2]);<br>                        DateTime moon = new DateTime(year, month,d);<br>                        moon = moon.AddDays(day - d);<br>                        int sum =0;<br>                        while(moon.CompareTo(begin) &gt; 0)<br>                        {<br>                                moon = moon.Subtract(diff);<br>                        }<br>                        while(begin.CompareTo(end) &lt;= 0)<br>                        {<br>                                while(moon.CompareTo(begin) &lt; 0)<br>                                {<br>                                        moon = moon.Add(diff);<br>                                }<br>                                DateTime final = new DateTime(9999,12,1);<br>                                if(begin.Equals(final))<br>                                {<br>                                        int tt= 0;<br>                                        try<br>                                        {<br>                                                while(true)<br>                                                {<br>                                                        moon = moon.Add(diff);<br>                                                        tt++;<br>                                                        if(tt == 2) break;<br>                                                }<br>                                        }<br>                                        catch(Exception)<br>                                        {<br>                                                <br>                                        }<br>                                        if(tt == 2) sum++;<br>                                        break;<br>                                }<br>                                                                DateTime nextMonth = begin.AddMonths(1);<br>                                int total = 0;<br>                                while(moon.CompareTo(nextMonth) &lt; 0)<br>                                {<br>                                        total++;<br>                                        moon = moon.Add(diff);<br>                                }<br>                                if(total == 2) sum++;<br>                                begin = begin.AddMonths(1);<br>                        }<br>                        return sum;<br>                        <br>                }<br>        }<br></td>