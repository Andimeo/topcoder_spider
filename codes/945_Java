            import java.util.*;<br>public class BlueMoons {<br>  int getDay(int y, int m, int d) {<br>    y--;<br>    int r = 2 + y * 365 + y / 4 - y / 100 + y / 400 + (m - 1) * 31 + d;<br>    if (m &gt; 11) r--;<br>    if (m &gt; 9) r--;<br>    if (m &gt; 6) r--;<br>    if (m &gt; 4) r--;<br>    y++;<br>    if (m &gt; 2) r -= y % 400 == 0 || y % 4 == 0 &amp;&amp; y % 100 != 0 ? 2 : 3;<br>    return r * 100;<br>  }<br>  public int count(String in, String fm) {<br>    int m1 = Integer.parseInt(in.substring(0, 2));<br>    int y1 = Integer.parseInt(in.substring(3, 7));<br>    int m2 = Integer.parseInt(in.substring(11, 13));<br>    int y2 = Integer.parseInt(in.substring(14));<br>    int f = (getDay(Integer.parseInt(fm.substring(9)), Integer.parseInt(fm.substring(6, 8)),<br>                    Integer.parseInt(fm.substring(0, 2))) + Integer.parseInt(fm.substring(3, 5))) % 2953;<br>    int c = 0;<br>    while (y1 == y2 &amp;&amp; m1 &lt;= m2 || y1 &lt; y2) {<br>      int a = getDay(y1, m1, 1);<br>      m1++;<br>      if (m1 &gt; 12) {<br>        m1 = 1;<br>        y1++;<br>      }<br>      int b = getDay(y1, m1, 1);<br>      int x = (a - f) % 2953;<br>      x = x == 0 ? 0 : 2953 - x;<br>      if (a + x + 2953 &lt; b) c++;<br>    }<br>    return c;<br>  }<br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>