            import java.util.*;<br>public class LetterStrings {<br>    public int sum(String[] s) {<br>        int res = 0;<br>        for (int i = 0; i &lt; s.length; i++) {<br>          String sl = s[i];<br>          for (int j = 0; j&lt; sl.length(); j++) {<br>            if (sl.charAt(j)=='-'); else res++;<br>          }<br>        }<br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>