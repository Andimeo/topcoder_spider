            using System;<br>using System.Collections.Generic;<br>using System.Text;<br><br>class ConductorWalk<br>{<br>    public int[] getObserverRecords(int[] orderSeat, int[] orderTime, int inspectorSeat)<br>    {<br>        int a = inspectorSeat;<br>        int len = orderTime.Length;<br>        List&lt;int&gt; res = new List&lt;int&gt;();<br>        int last = 0;<br>        for (int i = 0; i &lt; len; i++)<br>        {<br>            if (orderSeat[i] &lt; a)<br>            {<br>                if (last &gt; orderTime[i])<br>                    last += orderSeat[i] * 2;<br>                else<br>                    last = orderTime[i] + orderSeat[i] * 2;<br>                continue;<br>            }<br><br>            if (orderSeat[i] == a)<br>            {<br>                if (last &gt; orderTime[i])<br>                {<br>                    res.Add(last + a);<br>                    last += orderSeat[i] * 2;<br>                }<br>                else<br>                {<br>                    res.Add(orderTime[i] + a);<br>                    last = orderTime[i] + a * 2;<br>                }<br>                continue;<br>            }<br><br>            if (last &gt; orderTime[i])<br>            {<br>                res.Add(last + a);<br>                res.Add(last + a + 2 * (orderSeat[i] - a));<br>                last += orderSeat[i] * 2;<br>            }<br>            else<br>            {<br>                res.Add(orderTime[i] + a);<br>                res.Add(orderTime[i] + a + 2 * (orderSeat[i] - a));<br>                last = orderTime[i] + orderSeat[i] * 2;<br>            }<br><br>        }<br>        return res.ToArray();<br>    }<br><br>    static void Main(string[] args)<br>    {<br>        ConductorWalk cw = new ConductorWalk();<br>        int[] os ={1};<br>        int[] ot = {0};<br>        int aa = 1;<br>        int[] rr=cw.getObserverRecords(os,ot, aa);<br>        foreach(int i in  rr)<br>            Console.WriteLine(i);<br>    }<br>}<br></td>