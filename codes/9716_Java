            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class ConductorWalk<br>{<br>  public int[] getObserverRecords(int[] s, int[] t, int in)<br>  {<br>    int i,j,k;<br>    int ct = 0;<br>    int n = s.length;<br>    ArrayList&lt;Integer&gt; re = new ArrayList&lt;Integer&gt;();<br>    for(i=0;i&lt;n;i++){<br>      if(ct&lt;t[i]) ct=t[i];<br>      int t2 = ct + s[i];<br>      int t3 = t2 + s[i];<br>      <br>      if(s[i]==in) re.add(t2);<br>      else if(s[i]&gt;in){<br>        re.add(ct+in);<br>        re.add(t2+s[i]-in);<br>      }<br>      ct = t3;<br>    }<br>    int[] ans = new int[re.size()];<br>    i = 0;<br>    for(int a:re)ans[i++] = a;<br>    return ans;<br>  }<br>  <br>  public static void main(String[] args)<br>  {}<br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>