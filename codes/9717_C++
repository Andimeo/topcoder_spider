            #include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;stack&gt;<br>#include &lt;queue&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;algorithm&gt;<br>using namespace std;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;ctime&gt;<br>#include &lt;cmath&gt;<br> <br> <br>class CharSwaps {<br>public:<br>  int getMinCount(string);<br>};<br>// Sort whatever needs<br>// Clear global vector/set/array...<br>int CharSwaps::getMinCount(string s) {<br>  int n=s.length();<br>  int ac=0;<br>  int bc=0;<br>  for(int i=0;i&lt;n;i++){<br>    if(s[i]=='a')ac++;<br>    if(s[i]=='b')bc++;<br>  }<br> <br>  int ans=10000000;<br>  for(int start=0;start&lt;n;start++){<br>    int count=0;<br>    for(int end=0;end&lt;ac;end++){<br>      if(s[(start+end)%n]=='a')<br>        count++;<br>    }<br>    if(ac-count&lt;ans)<br>      ans=ac-count;<br>  }<br> <br>  return ans;<br>}<br> <br> <br>// Feel Free To Challenge Me Or the Machines Will Do Your Job. <br>string Powered_by = "[KawigiEdit] 2.0! ";<br></td>