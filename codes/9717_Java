            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class CharSwaps<br>{<br>  int n;<br>  int f[];<br>  <br>  public int getMinCount(String s)<br>  {<br>    int i,j,k;<br>    int a = 0, c1 = 0;;<br>    n = s.length();<br>    for(i=0;i&lt;n;i++){<br>      a = a*2;<br>      if(s.charAt(i)=='a') { a++; c1++; }<br>    }<br>    if(c1==n || c1==0) return 0;<br>    <br>    int ans = n;<br>    for(i=0;i&lt;n;i++){<br>      k = 0;<br>      for(j=0;j&lt;c1;j++){<br>        if(s.charAt((i+j)%n)=='a') k++;<br>      }<br>      int f = c1 - k;<br>      if(f&lt;ans) ans = f;<br>    }<br>    return ans;<br>  }<br>  <br>  public static void main(String[] args)<br>  {}<br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>