            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class SubgridsCounter {<br>public:<br>  int howMany(vector &lt;int&gt;, vector &lt;int&gt;);<br>};<br> <br>int SubgridsCounter::howMany(vector &lt;int&gt; x, vector &lt;int&gt; y) {<br>  set&lt;pair&lt;int,int&gt; &gt; mm;<br>  int n=x.size();<br>  pair&lt;int,int&gt; p[64];<br>  for(int i=0;i&lt;n;i++) mm.insert(p[i]=make_pair(x[i],y[i]));<br>  sort(p,p+n);<br>  int ret=0;<br>  for(int i=0;i&lt;n;i++) <br>  for(int j=i+1;j&lt;n;j++)<br>  if (p[i]!=p[j] &amp;&amp; p[i].first==p[j].first) {<br>    int diff=p[j].second-p[i].second;<br>    bool flag=true;<br>    for(int i1=0;i1&lt;3;i1++)<br>    for(int j1=0;j1&lt;3;j1++)<br>    if (mm.find(make_pair(p[i].first+i1*diff,p[i].second+j1*diff))==mm.end()) flag=false;<br>    if (flag) ret++;<br>  }<br>  return ret;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>