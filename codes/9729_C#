<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br><br>public class ContiguousCacheEasy<br>{<br>    public int bytesRead(int n, int k, int[] addresses)<br>    {<br>        int ret = 0;<br>        int start = 0;<br>        int end = k-1;<br>        foreach (int i in addresses)<br>        {<br>            if (i &gt; end)<br>            {<br>                ret += Math.Min(k, i - end);<br>                end = i;<br>                start = end - k + 1;<br>            }<br>            if (i &lt; start)<br>            {<br>                ret += Math.Min(k, start - i);<br>                start = i;<br>                end = start + k - 1;<br>            }<br>        }<br><br>        return ret;<br>    }<br>}<br></td>