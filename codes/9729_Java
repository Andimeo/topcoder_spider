<td class="problemText" colspan="8" valign="middle" align="left">
            public class ContiguousCacheEasy<br>{<br>  public int bytesRead(int n, int k, int[] ad)<br>  {<br>    int low = 0, high = k-1, read = 0;<br>    for(int i=0; i&lt; ad.length; i++)<br>    {<br>      if( low &gt; ad[i] )<br>      {<br>        read += Math.min(k,low - ad[i]);<br>        low = ad[i];<br>        high = low + k-1;<br>      }<br>      else if( ad[i] &lt;= high )<br>      {<br>      }<br>      else<br>      {<br>        read += Math.min(k,ad[i]-high);<br>        high = ad[i];<br>        low = high -(k-1);<br>      }<br>    }<br>    return read;<br>  }<br>}<br></td>