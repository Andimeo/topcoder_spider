            #include &lt;iostream&gt;<br>#include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;sstream&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;cmath&gt;<br> <br>using namespace std;<br> <br>#define REP(x,n) for(int x=0; x&lt;(int)(n); ++x)<br>#define REB(b,x,n) for(int x=b; x&lt;(int)(n); ++x)<br>#define PB push_back<br> <br> <br>struct RRSchedule<br>{<br> vector&lt;int&gt; terminalTimes(vector&lt;int&gt; tasks)<br> {<br>  int N=tasks.size();<br>  vector&lt;int&gt; res;<br>  REP(i,N) {<br>    int cr=0;<br>    REP(j,N) cr += min(tasks[i]-1,tasks[j]);<br>    REP(j,i+1) if(tasks[j]&gt;=tasks[i]) cr++;<br>    res.PB(cr);<br>  }<br> <br>  return res;<br> }<br>};<br></td>