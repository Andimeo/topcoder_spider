            public class RRSchedule<br>{<br>  public int[] terminalTimes(int[] tasks)<br>  {<br>    int N = tasks.length;<br>    int i, at=0, A[] = new int[tasks.length], j, r, q;<br>    int x[] = new int[tasks.length], p[] = new int[tasks.length], u[] = new int[tasks.length];<br>    <br>    for(i=0; i&lt;x.length; i++)<br>    {<br>      u[i] = 0;<br>      x[i] = tasks[i];<br>    }<br>      <br>    java.util.Arrays.sort(x);<br>    <br>    r = 0;<br>    for(i=0; i&lt;x.length; i++)<br>    {<br>      for(j=0; j &lt;N; j++)<br>      {<br>        if(x[i] == tasks[j] &amp;&amp; u[j] == 0)<br>        {<br>          u[j] = 1;<br>          p[i] = j;<br>          break;<br>        }<br>      }<br>      r = p[i];<br>    }<br>    <br>    A[p[0]] = (x[0]-1)*N+p[0]+1;<br>    u[p[0]] = 2;<br>    for(i=1; i&lt;x.length; i++)<br>    {<br>      r = 0;<br>      for(j=p[i-1]; j!=p[i]; j = (j+1)%N)<br>      {<br>        if(u[j] == 1)<br>          r++;<br>      }<br>      r++;<br>      <br>      q = x[i]-x[i-1];<br>      if(p[i] &lt; p[i-1])<br>        q--;<br>      if(q &lt; 0)<br>        q = 0;<br>      <br>      System.out.println(r+" "+(q)+" "+(N-i)+" "+A[p[i-1]]);<br>      <br>      A[p[i]] = r+(q)*(N-i)+A[p[i-1]];<br>      <br>      u[p[i]] = 2;<br>    }<br>    <br>    return A;<br>  }<br>}<br></td>