            using System;<br>using System.Collections.Generic;<br><br>using System.Text;<br><br>    class TextBoard<br>    {<br>        string[] s = new string[] <br>{"#.#.#.#.",<br> ".#.#.#.#",<br> "#.#.#.#.",<br> ".#.#.#.#",<br> "#.#.#.#.",<br> ".#.#.#.#",<br> "#.#.#.#.",<br> ".#.#.#.#" };<br>        public string[] draw(string[] pieces)<br>        {<br>            StringBuilder[] cs = new StringBuilder[8];<br>            for (int i = 0; i &lt; 8; i++)<br>                cs[i] = new StringBuilder(s[i]);<br>            string ss = "";<br>            for (int i = 0; i &lt; pieces.Length; i++)<br>            {<br>                ss = pieces[i];<br>                string[] sss = ss.Split(' ');<br>                int x = sss[1][0] - 'A';<br>                int y = sss[1][1] - '0' - 1;<br>                if (sss[0].CompareTo("WHITE") == 0)<br>                {<br>                    cs[y][x] = 'W';<br>                }<br>                else<br>                {<br>                    cs[y][x] = 'B';<br>                }<br>            }<br>            string[] ccs = new string[8];<br>            for (int i = 0; i &lt; 8; i++)<br>            {<br>                ccs[i] = cs[i].ToString();<br>            }<br>            return ccs;<br>        }<br><br>    }<br></td>