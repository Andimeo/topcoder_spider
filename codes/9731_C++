            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;queue&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>char P[100], A[100];<br> <br>class TextBoard {<br>public:<br>  vector &lt;string&gt; draw(vector &lt;string&gt; p) {<br>    int N = p.size();<br>    vector &lt;string&gt; Ans;<br>    for (int i = 0; i &lt; 4; i ++)<br>      Ans.push_back("#.#.#.#."), Ans.push_back(".#.#.#.#");<br>    for (int i = 0; i &lt; N; i ++)<br>    {<br>      sscanf(p[i].c_str(), "%s%s", &amp;P, &amp;A);<br>      Ans[A[1] - '1'][A[0] - 'A'] = P[0];<br>    }<br>    return Ans;<br>  }<br>};<br> <br> <br> <br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>