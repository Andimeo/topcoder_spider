            import static java.lang.Math.*;<br>import static java.util.Arrays.*;<br>import java.util.*;<br> <br>public class TextBoard<br>{<br>    public String[] draw(String[] pieces)<br>    {<br>        int n = pieces.length;<br>        String[] ret = new String[8];           <br>        for(int i = 0; i &lt; 8; i++){<br>            if(i % 2 == 0){<br>                ret[i]= "#.#.#.#.";<br>            }else<br>            {<br>                ret[i] = ".#.#.#.#";<br>            }<br>        }<br>        <br>        for(int i = 0; i &lt; n; i++){<br>            boolean isB = (pieces[i].charAt(0) == 'B');<br>            int w = pieces[i].charAt(6) - 'A';<br>            int c = pieces[i].charAt(7) - '0' - 1;<br>            if(isB)<br>            {<br>                ret[c] = ret[c].substring(0, w) + 'B' + ret[c].substring(w + 1, 8);<br>            }else<br>            {<br>                ret[c] = ret[c].substring(0, w) + 'W' + ret[c].substring(w + 1, 8);<br>            }            <br>        }<br>        return ret;<br>    }<br>    <br>    <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>