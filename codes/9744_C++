<td class="problemText" colspan="8" valign="middle" align="left">
            #include&lt;vector&gt;<br>#include&lt;string&gt;<br>using namespace std;<br><br>#define EPS 1e-9<br><br>class ReasonableOdds{<br>  public:<br>    string check(int p1, int pd, int p2, int k){<br>      if(p1==0||p2==0) return "YES";<br>      double lo=0., hi=1.;<br>      while(true){<br>        double mi=(lo+hi)/2.;<br>        double mi1=1.-mi;<br>        vector&lt;double&gt; P(1);<br>        P[0]=1.;<br>        for(int i=1; i&lt;=k; i++){<br>          vector&lt;double&gt; Q(i+1);<br>          Q[0]=P[0]*mi;<br>          Q[i]=P[i-1]*mi1;<br>          for(int j=1; j&lt;i; j++){<br>            Q[j]=P[j]*mi+P[j-1]*mi1;<br>          }<br>          P=Q;<br>        }<br>        vector&lt;double&gt; P2(k+1);<br>        for(int i=0; i&lt;=k; i++){<br>          P2[i]=P[k-i];<br>        }<br>        double pp1=0;<br>        double pp2=0;<br>        for(int i=0; i&lt;=k; i++){<br>          for(int j=0; j&lt;=k; j++){<br>            if(i&lt;j)pp1+=P[i]*P2[j];<br>            if(i&gt;j)pp2+=P[i]*P2[j];<br>          }<br>        }<br>        pp1+=EPS;<br>        pp2+=EPS;<br>        pp1*=100;<br>        pp2*=100;<br>        if(pp1&gt;=p1&amp;&amp;pp2&gt;=p2) return "YES";<br>        if(pp1&lt;p1&amp;&amp;pp2&lt;p2) return "NO";<br>        if(pp1&gt;=p1){<br>          hi=mi;<br>        }else{<br>          lo=mi;<br>        }<br>      }<br>    }<br>};<br></td>