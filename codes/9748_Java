            public class SegmentDisplay {<br><br>    public int howManyNumbers(int n) {<br>        int[] fr = {6, 2, 5, 5, 4, 5, 6, 3, 7, 6};<br>        int i, j;<br>        int[] nr = new int[n + 10];<br>        //if(n == 6) nr[6] = 3;<br>        //else nr[6] = 2;<br>        nr[6] = 3;<br>        nr[7] = 1;<br>        nr[5] = 3;<br>        nr[4] = 1;<br>        nr[3] = 1;<br>        nr[2] = 1;<br>        for (i = 0; i &lt;= n; i++) {<br>            for (j = 0; j &lt; 10; j++) {<br>                if (i - fr[j] &gt;= 0) {<br>                    nr[i] += nr[i - fr[j]];<br>                    if(i - fr[j] == 6) nr[i]--;<br>                    nr[i] %= 1000000007;<br>                }<br>            }<br>        }<br>        return nr[n];<br>    }<br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by moj 4.11 [modified TZTester]<br>// Powered by CodeProcessor<br></td>