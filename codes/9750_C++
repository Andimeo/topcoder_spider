            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class NumberPartition {<br>public:<br>  vector &lt;int&gt; kthPartition(int, int);<br>};<br>int sum,res,l;<br>vector&lt;int&gt; v;<br>void rec(int n,int k,int p)<br>{<br>if(l==1)return;<br>if(sum==n)res++;<br>if(res==k){l=1;return;}<br>    int i;<br>    for(i=p;i&lt;=n-sum;i++)<br>    {<br>      sum+=i;<br>      v.push_back(i);<br>      rec(n,k,i);<br>      if(l==1)return;<br>      v.pop_back();<br>      sum-=i;<br>    }<br>}<br>vector &lt;int&gt; NumberPartition::kthPartition(int n, int k) {<br>  rec(n,k,1);<br>  return v;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>