            public class NumberPartition {<br>   int valP;<br>   int[] p = new int[51];<br>   int n, rez, ok, K;<br>   int[] ret;<br>   <br>    void part(int k){<br>        if(valP == n){<br>            rez++;<br>            if(rez == K){<br>                ret = new int[k-1];<br>                for(int i = 0; i &lt; k-1; i++) ret[i]  = p[i+1];<br>                ok = 1;<br>            }<br>        }else <br>            for(int i = p[k-1]; i &lt;= n - valP &amp;&amp; ok==0; i++){<br>                p[k] = i;<br>                valP += i;<br>                part(k+1);<br>                valP -= i;<br>            }<br>    }<br>    <br>    public int[] kthPartition(int nn, int k) {<br>        p[0] = 1;<br>        n = nn; K = k;<br>        part(1);<br>        if(ok ==0 )<br>            ret = new int[0];<br><br>        <br>        return ret;<br>   }<br><br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by moj 4.11 [modified TZTester]<br>// Powered by CodeProcessor<br></td>