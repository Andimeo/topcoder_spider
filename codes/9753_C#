<td class="problemText" colspan="8" valign="middle" align="left">
            using System;<br>using System.Collections.Generic;<br>using System.Text;<br><br><br>public class MaximumScoredNumber<br>{<br>    public<br>        int getNumber(int lowerBound, int upperBound)<br>    {<br><br>        int bestScore = 0;<br>        int ret = 0;<br>        for (int i = lowerBound; i &lt;= upperBound; i++)<br>        {<br>            int score = 0;<br>            for (int a = 0; a &lt;= Math.Sqrt(i); a++)<br>            {<br>                for (int b = a; b &lt;= Math.Sqrt(i); b++)<br>                {<br>                    if (a * a + b * b == i) score++;<br>                }<br>            }<br>            if (score &gt;= bestScore)<br>            {<br>                ret = i;<br>                bestScore = score;<br>            }<br>        }<br><br>        return ret;<br>    }<br>}<br></td>