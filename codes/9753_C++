            #include&lt;cstdio&gt;<br>#include&lt;iostream&gt;<br>#include&lt;cmath&gt;<br>#include&lt;stack&gt;<br>#include&lt;queue&gt;<br>#include&lt;string&gt;<br>#include&lt;cstring&gt;<br>#include&lt;sstream&gt;<br>#include&lt;vector&gt;<br>#include&lt;utility&gt;<br>#include&lt;map&gt;<br>#include&lt;cstdlib&gt;<br>#include&lt;algorithm&gt;<br>using namespace std;<br>class MaximumScoredNumber<br>{<br> public:<br> int getNumber(int l, int u)<br> { <br>  int max=-1,ans=0;<br>  for(int i=l;i&lt;=u;i++)<br>  {<br>   int count=0;<br>   for(int j=0;j*j&lt;=i;j++)<br>   {<br>    for(int k=0;k&lt;=j;k++)<br>    {  <br>     if(j*j+k*k==i)<br>     count++;<br>     }<br>    }<br>   if(count&gt;=max)<br>   {<br>    max=count;<br>    ans=i;<br>    }<br>   }<br>  return ans;<br> }<br>};<br></td>