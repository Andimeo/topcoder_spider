            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class MaximumScoredNumber<br>{<br>  public int getNumber(int lb, int ub)<br>  {<br>    int ans = 0,score = 0;<br>    for(int i = lb; i &lt;= ub; i++)<br>    {<br>      int tp = 0;<br>      for(int j = 0 ; j &lt; (int)Math.sqrt(ub)+1; j++)<br>        for(int k = j; k &lt; (int)Math.sqrt(ub)+1; k++)<br>          if(j*j + k*k == i)<br>            tp++;<br>      if(tp &gt;= score)<br>      {<br>        score = tp;<br>        ans = i;<br>      }<br>    }<br>    return ans;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>