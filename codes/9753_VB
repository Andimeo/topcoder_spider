            Public Class MaximumScoredNumber<br><br>    Public Function getNumber(ByVal lowerBound As Integer, ByVal upperBound As Integer) As Integer<br>        Dim i As Integer<br>        Dim j As Integer<br>        Dim k As Integer<br>        Dim tmax As Integer<br>        Dim tval As Integer<br>        Dim result As Integer<br><br>        result = upperBound<br>        tmax = 0<br>        For i = upperBound To lowerBound Step -1<br><br>            tval = 0<br>            For j = 0 To Math.Sqrt(i)<br>                For k = 0 To Math.Sqrt(i)<br>                    If (j &gt;= k) Then<br>                        If (j * j + k * k) = i Then<br>                            tval = tval + 1<br>                        End If<br>                    End If<br>                Next k<br>            Next j<br>            If (tval &gt; tmax) Then<br>                tmax = tval<br>                result = i<br>            End If<br><br>        Next i<br>        Return result<br><br>    End Function<br><br>End Class<br></td>