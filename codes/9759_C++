            #include &lt;cstdio&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cmath&gt;<br>#include &lt;iostream&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;sstream&gt;<br>using namespace std;<br><br>bool    mat  [52][52];<br>int    indeg  [52];<br><br>class UnknownNames<br>{<br>  public:<br>  vector &lt;string&gt; sortNames(vector &lt;string&gt; list)<br>  {<br>    memset(mat , 0 , sizeof(mat));<br><br>    int  n = list.size();<br>    int  i , j , p;<br>    int  leng = list[0].length();<br><br>    for (i = 0; i &lt; n; i ++)<br>      for (j = 0; j &lt; n; j ++)<br>      {<br>        if (i == j) continue;<br>        for (p = 0; p &lt; leng; p ++)<br>          if (list[i][p] != '?' &amp;&amp; list[j][p] != '?') {<br>            if (list[i][p] &lt; list[j][p])<br>              mat[i][j] = 1;<br>          }<br>      }<br><br>    memset(indeg , 0 , sizeof(indeg));<br><br>    for (i = 0; i &lt; n; i ++)<br>      for (j = 0; j &lt; n; j ++)<br>        if (mat[i][j]) indeg[j] ++;<br><br>    vector&lt;string&gt;  ret , empty;<br>    bool    mark[52];<br><br>    string    last = "";<br><br>    memset(mark , 0 , sizeof(mark));<br>    for (i = 0; i &lt; leng; i ++)<br>      last += "A";  <br>    for (int t = 0; t &lt; n; t ++)<br>    {<br>      int  k = -1;<br>      string  best , tmp;<br>      for (i = 0; i &lt; n; i ++)<br>        if (indeg[i] == 0 &amp;&amp; ! mark[i])<br>        {<br>          tmp = "";<br>          for (p = 0; p &lt; leng; p ++)<br>            if (list[i][p] == '?')<br>              tmp += last[p];<br>            else  tmp += list[i][p];<br><br>          if (k &lt; 0 || tmp &lt; best)<br>            best = tmp , k = i;<br>        }<br><br>      if (k &lt; 0) return empty;<br>      ret.push_back( best );<br>      mark[k] = 1;<br>      for (i = 0; i &lt; n; i ++)<br>        if (mat[k][i]) indeg[i] --;<br>      last = best;<br>    }<br><br>    return  ret;<br>  }<br><br>  <br><br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>