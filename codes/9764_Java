            //BEGIN CUT HERE<br> <br>//END CUT HERE<br>import java.util.*;<br>public class AllCycleLengths {<br>  int N;<br>  String[] arcs;<br>  boolean v(int a,int b) { return arcs[a].charAt(b)=='Y'; }<br>  Map&lt;String,Integer&gt; pp = new HashMap();<br>  public String findAll(String[] arcs) {<br>    N = arcs.length;<br>    this.arcs = arcs;<br>    int state[] = new int[N];<br>    for (int i=0;i&lt;N;i++) state[i] = 1&lt;&lt;i;<br>    String str = "";<br>    int loop, t=1;<br>    for (;;) {<br>      int ny[] = new int[N];<br>      for (int y=N;y--&gt;0;)<br>        for (int x=N;x--&gt;0;)<br>          if(v(x,y)) ny[y] |= state[x];<br>      state = ny;<br>      char flag = '0';<br>      for (int y=N;y--&gt;0;)<br>        if ((state[y] &amp; (1&lt;&lt;y)) != 0) flag = '1';<br>      str += flag;<br>      String x = Arrays.toString(state);<br>      if (pp.get(x) != null) {<br>        loop = t - pp.get(x); break;<br>      }<br>      pp.put(x, t);<br>      t++;<br>    }<br>    int lstart = t-loop;<br>    X:    <br>    for (int tl = 1; tl &lt;= loop; tl++) {<br>      if ((loop % tl)==0) {<br>        for (int j=0;j&lt;loop;j+=tl) {<br>          if (!str.substring(lstart, lstart+tl).equals(str.substring(lstart+j, lstart+j+tl))) continue X;<br>        }<br>        loop = tl;<br>        break;<br>      }<br>    }<br>    while (str.length() &gt; loop) {<br>      if (str.charAt(str.length()-1) == str.charAt(str.length()-1-loop)) str = str.substring(0, str.length()-1);<br>      else break;<br>    }<br>    this.arcs = arcs;<br>    return str.substring(0,str.length()-loop)+"("+str.substring(str.length()-loop)+")";<br>  }<br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>