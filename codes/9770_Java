            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class HappyCells<br>{<br>  public int[] getHappy(String[] grid)<br>  {<br>    int len = grid.length;<br>    int wid = grid[0].length();<br>    char[][] arr = new char[len][wid];<br>    for(int i=0;i&lt;len;i++)<br>      arr[i] = grid[i].toCharArray();<br>    int one=0,two=0,three=0;<br>    int[] dy = {-1,-1,0,1,1,1,0,-1};<br>    int[] dx = {0,-1,-1,-1,0,1,1,1};<br>    <br>    for(int i=0;i&lt;len;i++)<br>    {<br>      for(int k=0;k&lt;wid;k++)<br>      {<br>        if(arr[i][k] == 'X') continue;<br>        boolean allorgo = true,alldiag=true;<br>        for(int j=0;j&lt;8;j++)<br>        {<br>          int y = i + dy[j];<br>          int x = k + dx[j];<br>          <br>          if(y &lt; 0 || x &lt; 0 || y &gt; len - 1 || x &gt; wid - 1)<br>            continue;<br>          <br>          if(arr[y][x] != 'X')<br>          {<br>            if(j%2==0)<br>              allorgo = false;<br>            else<br>              alldiag = false;<br>          }<br>        }<br>        if(allorgo &amp;&amp; alldiag) one++;<br>        else if(allorgo) two++;<br>        else if(alldiag) three++;<br>        <br>      }<br>    }<br>    return new int[] {one,two,three};<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>