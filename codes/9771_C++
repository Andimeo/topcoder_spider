            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class PositiveArray {<br>public:<br>  int countMoves(vector &lt;string&gt;);<br>};<br> <br>int a[25][25], b[25][25];<br> <br>int PositiveArray::countMoves(vector &lt;string&gt; data) {<br>  int n = data.size(), m = data[0].length(), i, j, ko, ans = 10000, s, t;<br>  char c;<br>  for (i=0;i&lt;n;i++)<br>    for (j=0;j&lt;m;j++) {<br>      c = data[i][j];<br>      if (c &gt;= '0' &amp;&amp; c &lt;= '9') a[i][j] = c-48; else<br>      if (c &gt;= 'a' &amp;&amp; c &lt;= 'z') a[i][j] = c-96+9; else<br>      if (c &gt;= 'A' &amp;&amp; c &lt;= 'Z') a[i][j] = -(c-64);<br>    }<br>  for (t=0;t&lt;(1&lt;&lt;n);t++) {<br>    for (i=0;i&lt;n;i++)<br>      for (j=0;j&lt;m;j++) b[i][j] = a[i][j];<br>    ko = 0;<br>    for (i=0;i&lt;n;i++)<br>      if ((t &amp; (1 &lt;&lt; i)) != 0) {<br>        for (j=0;j&lt;m;j++) a[i][j] = -a[i][j];<br>        ko++;<br>      }<br>    for (j=0;j&lt;m;j++) {<br>      s = 0;<br>      for (i=0;i&lt;n;i++) s += a[i][j];<br>      if (s == 0) {<br>        ko = 10000;<br>        break;<br>      }<br>      if (s &lt; 0) {<br>        for (i=0;i&lt;n;i++) a[i][j] = -a[i][j];<br>        ko++;<br>      }<br>    }<br>    for (i=0;i&lt;n;i++) {<br>      s = 0;<br>      for (j=0;j&lt;m;j++) s += a[i][j];<br>      if (s &lt;= 0) {<br>        ko = 10000;<br>        break;<br>      }<br>    }<br>    if (ko &lt; ans) ans = ko;<br>    for (i=0;i&lt;n;i++)<br>      for (j=0;j&lt;m;j++) a[i][j] = b[i][j];<br>  }<br>  if (ans &lt; 10000) return ans;<br>  return -1;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>