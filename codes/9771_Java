            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class PositiveArray<br>{<br>  public int countMoves(String[] data)<br>  {<br>    int[][] arr = new int[data.length][data[0].length()];<br>    int[][] tmp = new int[data.length][data[0].length()];<br>    <br>    for(int i=0;i&lt;data.length;i++){<br>      for(int j=0;j&lt;data[0].length();j++){<br>        char c = data[i].charAt(j);<br>        if('0'&lt;=c&amp;&amp;c&lt;='9') arr[i][j] = c-'0';<br>        if('a'&lt;=c&amp;&amp;c&lt;='z') arr[i][j] = c-'a'+10;<br>        if('A'&lt;=c&amp;&amp;c&lt;='Z') arr[i][j] = -(c-'A'+1);<br>      }<br>    }<br>    <br>    int nSwaps = 0;<br>    int minSwaps = 1000;<br>    <br>    boolean flag;<br>    <br>    for(int i=0;i&lt;(1&lt;&lt;arr.length);i++){<br>      nSwaps = 0;<br>      flag = true;<br>      <br>      for(int j=0;j&lt;arr.length;j++){<br>        if((i&amp;(1&lt;&lt;j))&gt;0) nSwaps++;<br>        for(int k=0;k&lt;arr[0].length;k++){<br>          if((i&amp;(1&lt;&lt;j))&gt;0){<br>            tmp[j][k] = -arr[j][k];<br>          }else{<br>            tmp[j][k] = arr[j][k];<br>          }  <br>        }<br>      }<br>      <br>    //  System.out.println(i+" " +nSwaps);<br>      <br>      for(int k=0;k&lt;arr[0].length;k++){<br>        int sum = 0;<br>        for(int j=0;j&lt;arr.length;j++){<br>          sum+=tmp[j][k];<br>        }<br>        if(sum &lt; 0){<br>          nSwaps ++;<br>          for(int j=0;j&lt;arr.length;j++){<br>            tmp[j][k]*=-1;<br>          }<br>        }<br>        if(sum==0){<br>          flag = false;<br>        }<br>      }<br>      <br>      for(int j=0;j&lt;arr.length;j++){<br>        int sum = 0;<br>        for(int k=0;k&lt;arr[0].length;k++){<br>          sum += tmp[j][k];<br>        }<br>        if(sum&lt;=0) flag = false;<br>      }<br>      <br>      if(flag) if(nSwaps&lt;minSwaps) minSwaps = nSwaps;<br>    }<br>    <br>    if(minSwaps==1000) return -1;<br>    return minSwaps;<br>  }<br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>