            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class SpiralWalking<br>{<br>  public int totalPoints(string[] levelMap)<br>  {<br>    int h = levelMap.Length, w = levelMap[0].Length;<br>        int r = levelMap[0][0] - '0';<br>        int o = 1;<br>        int x = 0, y = 0;<br>        int dx=0,dy=1;<br>        bool[,] v=new bool[h,w];<br>        v[0,0]=true;<br>        while (o&lt;h*w)<br>        {<br>            if (x + dx &gt;= 0 &amp;&amp; x + dx &lt; h &amp;&amp; y + dy &gt;= 0 &amp;&amp; y + dy &lt; w &amp;&amp; !v[x + dx, y + dy])<br>            {<br>                v[x + dx, y + dy] = true;<br>                r += levelMap[x + dx][y + dy] - '0';<br>                o++;<br>                x += dx;<br>                y += dy;<br>            }<br>            else<br>            {<br>                int s = dx;<br>                dx = dy;<br>                dy = -s;<br>                r -= levelMap[x][y] - '0';<br>            }<br>        }<br>        return r;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>