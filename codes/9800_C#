            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Diagnostics;<br> <br>public class DigitPrime<br>{<br>    public int countNumbers(int a, int b)<br>    {<br>        Dictionary&lt;int, bool&gt; pd = new Dictionary&lt;int, bool&gt;();<br>        for (int i = 10; i &lt; 100; ++i)<br>        {<br>            bool ok = true;<br>            for (int j = 2; j &lt; i; ++j)<br>            {<br>                if (i % j == 0)<br>                {<br>                    ok = false;<br>                    break;<br>                }<br>            }<br>            if (ok)<br>                pd.Add(i, true);<br>        }<br>        int cnt = 0;<br>        for (int v = a; v &lt;= b; ++v)<br>        {<br>            string vs = v.ToString();<br>            int ac = 0;<br>            for(int i=0;0==ac&amp;&amp;i&lt;vs.Length;++i)<br>                for(int j=0;0==ac&amp;&amp;j&lt;vs.Length;  ++j)<br>                {<br>                    if (i == j)<br>                        continue;<br>                    char c0 = vs[i];<br>                    char c1 = vs[j];<br>                    if ('0' == c0)<br>                        continue;<br>                    int vk = int.Parse(string.Format("{0}{1}", c0, c1));<br>                    if (pd.ContainsKey(vk))<br>                        ac = 1;<br> <br>                }<br>            cnt += ac;<br>        }<br>        return cnt;<br>    }<br>}<br> <br>// Powered by PopsEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>