            #include &lt;string&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;numeric&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;queue&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;list&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cctype&gt;<br>#include &lt;cassert&gt;<br>#include &lt;cmath&gt;<br>#include &lt;ctime&gt;<br>#include &lt;complex&gt;<br>#include &lt;cfloat&gt;<br>using namespace std;<br>#define REP(i,n) for(int i=0, _n=(int)(n);i&lt;_n;++i)<br>int CheckPrime( int n ) {<br>  for (int i = 2; i * i &lt;= n; ++i ){<br>    if ( n % i == 0 ){<br>      return 0;<br>    }<br>  }<br>  return 1;<br>}<br><br>class DigitPrime {<br>  public:<br>    int countNumbers( int a, int b ) {<br><br>      int d[10];<br>      int isPri[100];<br>      for (int i = 10; i &lt; 100; ++i){<br>        if (CheckPrime(i)){<br>          isPri[i] = 1;<br>        }<br>        else{<br>          isPri[i] = 0;<br>        }<br>      }<br>      int ans = 0;<br>      for (int i = max(a, 10); i &lt;= b; ++i){<br>        int l = 0;<br>        int t = i;<br>        while (t){<br>          d[l++] = t%10;<br>          t /= 10;<br>        }<br>        int ist = 0;<br>        REP(m, l){<br>          REP(n, l){<br>            if (m != n){<br>              int cur = d[m]*10+d[n];<br>              if (cur &gt; 10 &amp;&amp; isPri[cur]){<br>                ist = 1;<br>                break;<br>              }<br>            }<br>          }<br>        }<br>        if (ist){<br>          ++ans;<br>        }<br>      }<br>      return ans;<br>    }<br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by moj 4.1 [modified TZTester]<br>// Powered by CodeProcessor<br></td>