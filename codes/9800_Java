            import java.util.Arrays;<br><br>public class DigitPrime {<br>  boolean[] isPrime = new boolean[100];<br><br>  public int countNumbers(int a, int b) {<br>    Arrays.fill(isPrime, true);<br>    isPrime[0] = isPrime[1] = false;<br>    for (int i = 2; i &lt;= 99; ++i) {<br>      if (isPrime[i] == true)<br>        for (int j = i * i; j &lt;= 99; j += i)<br>          isPrime[j] = false;<br>    }<br>    int res = 0;<br>    for (int i = a; i &lt;= b; ++i) {<br>      String str = Integer.toString(i);<br>      outer: for (int j = 0; j &lt; str.length(); ++j) {<br>        for (int k = j + 1; k &lt; str.length(); ++k) {<br>          if (str.charAt(j) != '0' &amp;&amp; isPrime[(str.charAt(j) - '0') * 10 + (str.charAt(k) - '0')]) {<br>            ++res;<br>            break outer;<br>          }<br>          if (str.charAt(k) != '0' &amp;&amp; isPrime[(str.charAt(k) - '0') * 10 + (str.charAt(j) - '0')]) {<br>            ++res;<br>            break outer;<br>          }<br>        }<br>      }<br>    }<br>    return res;<br>  }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>