            import java.lang.*;<br>import java.util.*;<br><br>public class Stick<br>{<br>  public int pieces(int x)  <br>  {<br>    int i, s, res = 0, currSum = 64, currStick, tempSum=0;<br>    <br>    if (x == 32 || x==64)<br>      return 1;<br>    <br>    Vector a = new Vector();<br>    <br>    a.add(64);<br>    <br>    while(currSum &gt; x)<br>    {<br>      Collections.sort(a);<br>      <br>      currStick = Integer.parseInt((a.firstElement()).toString());<br><br>      a.set(0,0);<br>        <br>      currStick = currStick/2;<br>      tempSum=0;        <br>      for(i = 0 ; i &lt; a.size() ; i++)<br>      {<br>        tempSum += Integer.parseInt((a.get(i)).toString());<br>      }<br>      if((currStick + tempSum) &gt;= x )<br>      {  <br>        a.set(0, currStick);<br>        currSum = tempSum + currStick;<br>      }<br>      else<br>      {<br>        a.set(0, currStick);<br>        a.add(currStick);<br>        currSum = tempSum + (2*currStick);<br>      }<br>    }<br>    res = a.size();<br>    return res;<br>  }<br>}<br></td>