            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class DNAConstruction<br>{<br>  public int maxLength(String nucleotides)<br>  {<br>    int[] pairs = new int[4];    <br>    for(int i=0;i&lt;nucleotides.length();i++)<br>    {<br>      char c = nucleotides.charAt(i);<br>      if(c=='A') pairs[0]++;<br>      if(c=='T') pairs[1]++;<br>      if(c=='C') pairs[2]++;<br>      if(c=='G') pairs[3]++;<br>    }<br>    return Math.min(pairs[0],pairs[1])+Math.min(pairs[2],pairs[3]);<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>