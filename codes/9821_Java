            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class WordFindPuzzle<br>{<br>  public int solveWordFindPuzzle(String[] words, String[] board)<br>  {<br>    String[] board2 = new String[board[0].length()];<br>    <br>    for(int i=0;i&lt;board2.length;i++){<br>      board2[i]="";<br>      for(int j=0;j&lt;board.length;j++) board2[i]+=board[j].charAt(i);<br>    }<br>    <br>    int count = 0;<br>    <br>    for(int i=0;i&lt;words.length;i++){<br>      boolean fH = false;<br>      boolean fV = false;<br>      <br>      for(int j=0;j&lt;board.length;j++){<br>        if(board[j].indexOf(words[i])&gt;=0) fH = true;<br>        if(board[j].indexOf(reverse(words[i]))&gt;=0) fH = true;<br>      }<br>      <br>      for(int j=0;j&lt;board2.length;j++){<br>        if(board2[j].indexOf(words[i])&gt;=0) fV = true;<br>        if(board2[j].indexOf(reverse(words[i]))&gt;=0) fV = true;<br>      }<br>      if(fH&amp;&amp;fV) count++;<br>    }<br>    <br>    return count;<br>    <br>  }<br>  <br>  public String reverse(String str){<br>    String ret = "";<br>    for(int i=str.length()-1;i&gt;=0;i--){<br>      ret+=str.charAt(i);<br>    }<br>    return ret;<br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>