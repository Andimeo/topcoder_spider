            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class TournamentJudging<br>{<br>  public int getPoints(int[] rawScores, int[] conversionFactor)<br>  {<br>        int score = 0;<br>        for (int i = 0; i &lt; rawScores.Length; i++)<br>        {<br>            score += (int)(((double)rawScores[i]/conversionFactor[i])+0.5);<br>        }<br>        return score;<br>  }<br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>