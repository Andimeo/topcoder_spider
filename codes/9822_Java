            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class TournamentJudging<br>{<br>  public int getPoints(int[] rawScores, int[] conversionFactor)<br>  {<br>    int sum = 0;<br>    for (int i=0; i&lt;rawScores.length; i++) {<br>      double x = Math.round(((double)rawScores[i]) / (double)conversionFactor[i]);<br>      sum += (int)x;<br>    }<br>    return sum;<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>