            using System;<br><br>public class OrderedSuperString {<br>    public int getLength(string[] words) {<br>        string super = "";<br>        int last = 0;<br>        foreach (string w in words)<br>        {<br>            while (true)<br>            {<br>                string nxt = super;<br>                if (nxt.Length &lt; last + w.Length)<br>                    nxt = nxt + w.Substring(nxt.Length - last);<br>                if (nxt.Substring(last, w.Length) == w)<br>                {<br>                    super = nxt;<br>                    break;<br>                }<br>                ++last;<br>            }<br>        }<br>        return super.Length;<br>    }<br>    <br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>