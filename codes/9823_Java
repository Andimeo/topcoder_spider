            import java.util.*;<br>import java.io.*;<br>import java.text.*;<br>import java.math.*;<br>import java.util.regex.*;<br>import static java.lang.Math.*;<br>import static java.util.Arrays.*;<br>import static java.util.Collections.*;<br>import static java.lang.Integer.*;<br>import static java.lang.Long.*;<br>public class OrderedSuperString {<br>  public int getLength(String[] words) {<br>    String ret = "";<br>    int last = 0;<br>    for(int i = 0; i &lt; words.length; i++){<br>      outer:<br>      for(;;last++){<br>        for(int j = 0; last+j &lt; ret.length() &amp;&amp; j &lt; words[i].length(); j++){<br>          if(ret.charAt(last+j) != words[i].charAt(j)) continue outer;<br>        }<br>        String tmp = ret.substring(0,last)+words[i];<br>        if(tmp.length() &gt; ret.length()) ret = tmp;<br>        break;<br>      }<br>    }<br>    return ret.length();<br>  }<br> <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>