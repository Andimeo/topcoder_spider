            using System;<br><br>public class CorporationSalary {<br>    long[] salary;<br>    string[] relations;<br><br>    public long totalSalary(string[] relations) {<br>        this.relations = relations;<br>        salary = new long[relations.Length];<br>        for (int i = 0; i &lt; relations.Length; ++i)<br>            salary[i] = -1;<br>        long res = 0;<br>        for (int i = 0; i &lt; relations.Length; ++i)<br>            res += Get(i);<br>        return res;<br>    }<br><br>    private long Get(int i)<br>    {<br>        if (salary[i] &gt;= 0)<br>            return salary[i];<br>        salary[i] = 0;<br>        for (int j = 0; j &lt; relations[i].Length; ++j)<br>            if (relations[i][j] == 'Y')<br>                salary[i] += Get(j);<br>        if (salary[i] == 0)<br>            salary[i] = 1;<br>        return salary[i];<br>    }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>