            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;queue&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cctype&gt;<br>#include &lt;string&gt;<br>#include &lt;cstring&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>#define SIZE(X) ((int)(X.size()))<br>typedef long long int64;<br> <br>class CorporationSalary<br>{<br>public:<br>  int n;<br>  vector&lt;string&gt; G;<br>  int64 f[60];<br>  int64 solve(int v)<br>  {<br>    int64 &amp;ret=f[v];<br>    if (ret!=-1) return ret;<br>    ret=0;<br>    for (int i=0;i&lt;n;i++) if (G[v][i]=='Y') ret+=solve(i);<br>    if (ret==0) ret=1;<br>    return ret;<br>  }<br>  int64 totalSalary(vector &lt;string&gt; vs)<br>  {<br>    G=vs;<br>    n=SIZE(G);<br>    memset(f,255,sizeof(f));<br>    int64 result=0;<br>    for (int i=0;i&lt;n;i++) result+=solve(i);<br>    return result;<br>  }<br>};<br> <br> <br> <br>// Powered by PopsEdit<br></td>