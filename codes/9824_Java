            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br> <br>public class CorporationSalary<br>{<br>  int n, m;<br>  boolean[][] map;<br>  void parse(String[] in, char on)<br>  {<br>    n = in.length;<br>    m = in[0].length();<br>    map = new boolean[n][m];<br>    for (int i = 0; i &lt; n; i++)<br>    {<br>      for (int j = 0; j &lt; m; j++)<br>      {<br>        map[i][j] = in[i].charAt(j) == on;<br>      }<br>    }<br>  }<br> <br>  long[] mem;<br>  boolean[] used;<br>  long doit(int u)<br>  {<br>    long res = 0;<br>    if (used[u]) return mem[u];<br>    for (int i = 0; i &lt; n; i++)<br>    {<br>      if (!map[u][i]) continue;<br>      res += doit(i);<br>    }<br>    if (res == 0) res = 1;<br>    used[u] = true;<br>    mem[u] = res;<br>    return res;<br>  }<br>  <br>  public long totalSalary(String[] relations)<br>  {<br>    parse(relations, 'Y');<br>    mem = new long[n];<br>    used = new boolean[n];<br>    long res = 0;<br>    for (int i = 0; i &lt; n; i++)<br>    {<br>      res += doit(i);<br>    }<br>    return res;<br>    <br>  }<br>  <br>  <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>