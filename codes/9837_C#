            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class InfiniteSequence<br>{<br>  Hashtable memo = new Hashtable();<br> <br>  public long calc(long n, int p, int q)<br>  {<br>    if(n == 0) return 1;<br>    if(memo.ContainsKey(n))<br>      return (long)memo[n];<br>    memo.Add(n, calc(n/p, p, q) + calc(n/q, p ,q));<br>    return (long)memo[n];<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>