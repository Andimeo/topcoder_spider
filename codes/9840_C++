            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>using namespace std;<br> <br>class Subway2 {<br>public:<br>double minTime(int length, int maxAcceleration, int maxVelocity) {<br>  double len=length,a=maxAcceleration,vmax=maxVelocity;<br>  if((double)len &gt; (double)vmax*(double)vmax/(double)a){<br>    len -= (double)vmax*(double)vmax/(double)a;<br>    return len/vmax + vmax/a*2;<br>  }else{<br>    return 2*sqrt(len/a);<br>  }<br>}<br>};<br> <br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>