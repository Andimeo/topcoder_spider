            #include &lt;vector&gt;<br>#include &lt;list&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br>#include &lt;deque&gt;<br>#include &lt;stack&gt;<br>#include &lt;bitset&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;functional&gt;<br>#include &lt;numeric&gt;<br>#include &lt;utility&gt;<br>#include &lt;sstream&gt;<br>#include &lt;iostream&gt;<br>#include &lt;iomanip&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;ctime&gt;<br> <br>#define pb push_back<br>#define mp make_pair<br>#define forn (i, n) for (int i = 0; i &lt; n; i++)<br>#define ford (i, n) for (int i = n - 1; i &gt;= 0; i--)<br> <br>typedef long long ll;<br>typedef long double ld;<br> <br>using namespace std;<br> <br>class Auxanometer {<br>public:<br>  int countIncreasingMarks(int, int);<br>};<br> <br>int cnt;<br>int ans;<br> <br>int check (int a, int b) {<br>  int ok = 1;<br>  int k = 10;<br>  while (b &gt; 0) {<br>    if (b % 10 &gt; k) ok = 0;<br>    if (!ok) break;<br>    k = b % 10;<br>    b /= 10;<br>  }<br>  b = a;<br>  while (b &gt; 0) {<br>    if (b % 10 &gt; k) ok = 0;<br>    if (!ok) break;<br>    k = b % 10;<br>    b /= 10;<br>  }<br>  return ok;<br>}<br> <br>int f;<br> <br>int calc (int x, int y) {<br>  if (x &gt; cnt) return 0;<br>  if (x &gt;= 1) {<br>    int z = f + x - 1;<br>    if (check (z, x)) ans++;<br>  }<br>  for (int i = y; i &lt; 10; i++)<br>    if ((cnt - i) / 10 &gt;= x) calc (x * 10 + i, i);<br>  return 0;<br>}<br> <br>int Auxanometer::countIncreasingMarks(int nmin, int nmax) {<br>  f = nmin;<br>  cnt = nmax - nmin + 1;<br>  ans = 0;<br>  calc (0, 1);<br>  return ans;<br>}<br> <br> <br>//Powered by [KawigiEdit] 2.0!<br></td>