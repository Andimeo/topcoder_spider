            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br> <br>public class FloorIndicator<br>{<br>    <br> <br>  public double averageFloor(int N, string[] indicator)<br>  {<br>    string[] m=new string[5]{"###..########.################", "#.#..#..#..##.##..#....##.##.#", "#.#..################..#######", "#.#..##....#..#..##.#..##.#..#", "###..#######..#######..#######"};<br>        int[,] r=new int[11,11];<br>        for (int q=0; q&lt;10; q++) r[q,0]=0;<br>        for (int i = 0; i &lt; N; i++)<br>        {<br>            for (int j = 0; j &lt;= 9; j++)<br>            {<br>                bool flag=true;<br>                for (int k = 0; k &lt; 5; k++)<br>                {<br>                    for (int l = 0; l &lt; 3; l++)<br>                    {<br>                        if (indicator[k][4 * i + l] == '#' &amp;&amp; m[k][3 * j + l] == '.') flag = false;<br>                    }<br>                }<br>                if (flag) r[i,++r[i,0]]=j;<br>            }<br>        }<br> <br>        double res = 0.0;<br> <br>        for (int i = 0; i &lt; N; i++)<br>        {<br>            if (r[i,0]== 0) return -1.0;<br>            double sum = 0.0;<br>            for (int j = 1; j &lt;= r[i,0]; j++)<br>            {<br>                sum += r[i,j];<br>            }<br>            sum /= (double)(r[i,0]);<br>            res += sum * Math.Pow(10.0, N - i - 1);<br>        }<br>        return res;<br>  }<br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>