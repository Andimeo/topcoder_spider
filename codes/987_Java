            public class GardenHose<br>{<br>  public static void main(String[] args)<br>  {<br>    int input1= 50;<br>    int input2= 50;<br>    int input3= 50;<br>    int input4= 49;<br>    System.out.println("returns "+countDead(input1,input2,input3,input4));<br>  }<br>  public static int countDead(int n, int rowDist, int colDist, int hoseDist)<br>  {<br>    int ret=0;<br>    for(int x=1;x&lt;=n;x++)<br>    {<br>      for(int y=1;y&lt;=n;y++)<br>      {<br>        if(rowDist*x&gt;hoseDist &amp;&amp; rowDist*(n-x+1)&gt;hoseDist &amp;&amp;<br>        colDist*y&gt;hoseDist &amp;&amp; colDist*(n-y+1)&gt;hoseDist)<br>        ret++;<br>      }<br>    }<br>    return ret;<br>  }<br>}<br> <br>//Made with SubEthaEdit, on a Mac<br></td>