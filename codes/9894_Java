            import java.util.*;<br> <br>public class LongStraightRoad {<br> <br>    List&lt;String&gt; ss = new ArrayList&lt;String&gt;();<br> <br>    public int distanceToDestination(String[] signs, String destination) {<br>        int n = signs.length;<br>        for (int i = 0; i &lt; n; i++) {<br>            ss.add("Sign " + i);<br>        }<br>        double[][] al = new double[200][200];<br>        for (double[] q : al) {<br>            Arrays.fill(q, -1e100);<br>        }<br>        double[][] ah = new double[200][200];<br>        for (double[] q : ah) {<br>            Arrays.fill(q, 1e100);<br>        }<br>        for (int i = 0; i &lt; n; i++) {<br>            String[] sss = signs[i].split(";");<br>            for (String s : sss) {<br>                String[] ps = s.split(" ");<br>                int j = getIndex(ps[0]);<br>                al[i][j] = Integer.parseInt(ps[1]);<br>                al[j][i] = -al[i][j];<br>                ah[i][j] = al[i][j];<br>                ah[j][i] = -ah[i][j];<br>            }<br>        }<br>        for (int i = 0; i &lt; n - 1; i++) {<br>            al[i][i + 1] = 1e-10;<br>            ah[i + 1][i] = -1e-10;<br>        }<br>        int m = ss.size();<br>        for (int i = 0; i &lt; m; i++) {<br>            for (int j = 0; j &lt; m; j++) {<br>                for (int k = 0; k &lt; m; k++) {<br>                    ah[j][k] = Math.min(ah[j][k], ah[j][i] + ah[i][k]);<br>                    al[j][k] = Math.max(al[j][k], al[j][i] + al[i][k]);<br>                    if (ah[j][k] &lt; al[j][k]) return -1;<br>                }<br>            }<br>        }<br>        int d = getIndex(destination);<br>        if (ah[n - 1][d] &gt; al[n - 1][d]) {<br>            return -1;<br>        } else {<br>            if (ah[n - 1][d] &lt; -1e-10) {<br>                return -1;<br>            } else {<br>                return (int)ah[n - 1][d];<br>            }<br>        }<br>    }<br> <br>    private int getIndex(String s) {<br>        int res = ss.indexOf(s);<br>        if (res == -1) {<br>            ss.add(s);<br>            return ss.size() - 1;<br>        } else {<br>            return res;<br>        }<br>    }<br> <br>}<br> <br> <br>// Powered by FileEdit<br>// Powered by RETester [based on ExampleBuilder]<br>// Powered by CodeProcessor<br></td>