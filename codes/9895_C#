            using System;<br><br>public class Embassy {<br>    public int visaApplication(int[] forms, int dayLength, int openTime) {<br>        int res = int.MaxValue;<br>        for (int start = 0; start &lt; dayLength; ++start)<br>        {<br>            int at = start;<br>            foreach (int x in forms)<br>            {<br>                at += x;<br>                if (at % dayLength &gt; openTime)<br>                {<br>                    at = at + dayLength - at % dayLength;<br>                }<br>            }<br>            res = Math.Min(res, at - start);<br>        }<br>        return res;<br>    }<br><br>   <br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>