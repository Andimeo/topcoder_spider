            #include &lt;iostream&gt;<br>#include &lt;vector&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;string&gt;<br>#include &lt;cmath&gt;<br><br>using namespace std;<br><br>class DancingCouples<br>{<br>  public:<br>  <br>    vector&lt;string&gt; a;<br>    int k,n,m;<br>    int b[11][(1 &lt;&lt; 10)+1][11];<br><br>    int rec(int i,int p,int t)<br>    {<br>      if (t == k) return 1;<br>      if (i == n) return 0;<br>      if (b[i][p][t] != -1) return b[i][p][t];<br>      int j,x;<br>      x = rec(i+1,p,t);<br>      for (j = 0;j&lt;m;j++)<br>        if (((p &amp; (1 &lt;&lt; j)) == 0)&amp;&amp;(a[i][j] == 'Y'))<br>        {<br>          x = x+rec(i+1,p | (1 &lt;&lt; j),t+1);<br>        }<br>      b[i][p][t] = x;<br>      return x;<br>           <br>    }<br>    <br>    int countPairs(vector &lt;string&gt; s,int k1)<br>    {<br>      a = s;<br>      n = s.size();<br>      m = s[0].length();<br>      k = k1;<br>      memset(b,-1,sizeof(b));<br>      return rec(0,0,0);    <br>    }  <br>};<br></td>