            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class MostCommonLetters <br>{<br>  public string listMostCommon(string[] text) <br>  {<br>        string s = "";<br>        foreach (string ss in text)<br>            s += ss;<br>        int[] T = new int[26];<br>        for (int i = 0; i &lt; s.Length; i++)<br>        {<br>            if (s[i] == ' ')<br>                continue;<br>            T[(s[i] - 'a')]++;<br>        }<br>        int max = -1;<br>        for (int i = 0; i &lt; T.Length; i++)<br>            if (max &lt; T[i])<br>                max = T[i];<br>        string ret = "";<br>        for (int i = 0; i &lt; T.Length; i++)<br>            if (T[i] == max)<br>                ret += ((char)(i+'a')).ToString();<br>        return ret;<br>  }<br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by r1cs1]<br>// Powered by CodeProcessor<br></td>