            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class MostCommonLetters<br>{<br>  public String listMostCommon(String[] text)<br>  {<br>    String ret = "";<br>    int[] used = new int[26];<br>    int max = 0;<br>    for(int i=0;i&lt;text.length;i++)<br>      for(int j=0;j&lt;text[i].length();j++)<br>        if(text[i].charAt(j) != ' ')<br>          used[text[i].charAt(j) - 'a'] ++;<br>    for(int i=0;i&lt;26;i++)<br>      max = Math.max(max,used[i]);<br>    for(int i=0;i&lt;26;i++)<br>      if(used[i] == max)<br>        ret += (char)('a'+i);<br>    return ret;<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>