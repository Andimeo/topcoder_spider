            Class MostCommonLetters<br>    Public Function listMostCommon(ByVal text As String()) As String<br>        Dim i As Integer<br>        Dim im As Integer<br>        im = text.Length<br><br>        Dim ia() As Integer<br>        ReDim ia(25)<br><br>        For i = 0 To 25<br>            ia(i) = 0<br>        Next<br><br>        Dim j As Integer<br>        Dim t As String<br>        For i = 0 To text.Length - 1<br>            For j = 1 To text(i).Length<br>                t = Mid(text(i), j, 1)<br>                If t &lt;&gt; " " Then<br>                    ia(Asc(t) - Asc("a")) = ia(Asc(t) - Asc("a")) + 1<br>                End If<br>            Next<br>        Next<br>        Dim ta As Integer<br>        ta = -1<br>        Dim ind As Integer<br>        ind = -1<br><br>        Dim ret As String<br>        ret = ""<br><br>        For i = 0 To 25<br>            If ia(i) &gt; ta Then<br>                ta = ia(i)<br>                ind = i<br>                ret = Chr(ind + Asc("a"))<br>            Else<br>                If ia(i) = ta Then<br>                    ret = Trim(ret + Chr(i + Asc("a")))<br>                End If<br><br>            End If<br><br>        Next<br><br><br><br>        Return ret<br><br>    End Function<br>End Class<br></td>