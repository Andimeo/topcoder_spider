            using System;<br>using System.Text;<br>using System.Text.RegularExpressions;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class DeckRearranging {<br>    public string rearrange(string deck, int[] above) {<br>        string res = "";<br>        List&lt;char&gt; final = new List&lt;char&gt;();<br><br>        for (int i = 0; i &lt; deck.Length; i++)<br>            final.Insert(above[i], deck[i]);<br><br>        foreach (char c in final)<br>            res += c;<br><br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>