            #include &lt;string&gt;<br>#include &lt;iostream&gt;<br>#include &lt;sstream&gt;<br>#include &lt;cmath&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;algorithm&gt;<br>#include &lt;cctype&gt;<br>#include &lt;iterator&gt;<br>#include &lt;vector&gt;<br>#include &lt;queue&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;utility&gt;<br>#include &lt;numeric&gt;<br>#include &lt;functional&gt;<br>#include &lt;math.h&gt;<br><br>#include &lt;cstring&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;ctype.h&gt;<br>#include &lt;iomanip&gt;<br><br>using namespace ::std;<br><br><br><br>typedef long long LL;<br><br>struct PrettyPrintingProduct <br>{<br>  string prettyPrint(int A, int B) <br>  {<br>    string res;<br>        ostringstream oss;<br>        int E=0;<br>        LL low=1;<br>        LL high=1;<br>        bool bb=false;<br>        for(int n=A; n&lt;=B; n++)<br>        {<br>            low*=n;<br>            while(low%10==0)<br>            {<br>                low/=10;<br>                E++;<br>            }<br>            if (low&gt;=1000000000000LL)<br>            {<br>                bb=true;<br>                low%=1000000000000LL;<br>            }<br>            high*=n;<br>            LL t=high/1000000000000LL;<br>            while(t)<br>            {<br>                t/=10LL;<br>                high/=10LL;<br>            }<br>        }<br>        if(bb)<br>        {<br>            oss&lt;&lt;setw(5)&lt;&lt;setfill('0')&lt;&lt;(high/10000000L);<br>            oss&lt;&lt;"...";<br>            oss&lt;&lt;setw(5)&lt;&lt;setfill('0')&lt;&lt;(low%100000L);<br>        }<br>        else<br>        {<br>            if(low&gt;=10000000000LL)<br>            {<br>                high=low;<br>                while(high/100000L)<br>                {<br>                    high/=10L;<br>                }<br>                oss&lt;&lt;(high);<br>                oss&lt;&lt;"...";<br>                oss&lt;&lt;setw(5)&lt;&lt;setfill('0')&lt;&lt;(low%100000L);<br>           }<br>           else<br>           {<br>                oss&lt;&lt;(low);<br>           }<br>            <br>        }<br>        oss&lt;&lt;" * 10^"&lt;&lt;E;<br>        res = oss.str();<br>    return res;<br>  }<br>  <br><br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>