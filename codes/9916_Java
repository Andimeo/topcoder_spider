            import java.util.*;<br><br>public class PrettyPrintingProduct {<br>    public String prettyPrint(int A, int B) {<br>      int n2 = 0, n5 = 0;<br><br>      long t = 1;<br>      boolean l10 = false;<br>      <br>      long las = 1;<br>      double fir = 1;<br>      <br>      for (int i = A; i &lt;= B; ++i) {<br>        int j = i;<br>        while (j % 10 == 0) {<br>          j /= 10;<br>          n2++;<br>          n5++;<br>        }<br>        while (j % 2 == 0) {<br>          j /= 2;<br>          n2++;<br>        }<br>        while (j % 5 == 0) {<br>          j /= 5;<br>          n5++;<br>        }<br><br>        if (!l10)<br>          t *= j;<br>        if (t &gt; 9999999999L)<br>          l10 = true;<br>        <br>        las = (las * j) % 100000L;<br>        <br>        fir *= j;<br>        while (fir &gt;= 100000)<br>          fir /= 10;<br>      }<br>      <br>      int E;<br>      <br>      if (n2 &gt;= n5) {<br>        E = n5;<br>        for (int i = n5; i &lt; n2; ++i) {<br>          int j = 2;<br>            if (!l10)<br>              t *= j;<br>            if (t &gt; 9999999999L)<br>              l10 = true;<br>            <br>            las = (las * j) % 100000L;<br>            <br>            fir *= j;<br>            while (fir &gt;= 100000)<br>              fir /= 10;<br>        }<br>      }<br>      else {<br>        E = n2;<br>        for (int i = n2; i &lt; n5; ++i) {<br>          int j = 5;<br>            if (!l10)<br>              t *= j;<br>            if (t &gt; 9999999999L)<br>              l10 = true;<br>            <br>            las = (las * j) % 100000L;<br>            <br>            fir *= j;<br>            while (fir &gt;= 100000)<br>              fir /= 10;<br>        }<br>      }<br>      <br>      if (!l10) <br>        return Long.toString(t) + " * 10^" + Integer.toString(E);<br>      <br>      int fir5 = (int)fir;<br>      <br>      String lasStr = Long.toString(las);<br>      String firStr = Integer.toString(fir5);<br>      <br>      while (lasStr.length() &lt; 5)<br>        lasStr = "0" + lasStr;<br>      while (firStr.length() &lt; 5)<br>        firStr = "0" + firStr;<br>      <br>      return firStr + "..." + lasStr + " * 10^" + Integer.toString(E);<br>    }<br><br>}<br><br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>