            using System;<br><br>public class BitwiseEquations {<br>  public long kthPlusOrSolution(int x, int k) {<br>        int[] bits = new int[100];<br>        int[] kbits = new int[100];<br>        for (int i = 0; i &lt; bits.Length; ++i)<br>        {<br>            bits[i] = x % 2;<br>            x /= 2;<br>            kbits[i] = k % 2;<br>            k /= 2;<br>        }<br>        long res = 0;<br>        int j = 0;<br>        for (int i = 0; i &lt; bits.Length; ++i)<br>            if (bits[i] == 0)<br>            {<br>                if (kbits[j] != 0)<br>                {<br>                    res += (1L &lt;&lt; i);<br>                }<br>                ++j;<br>            }<br>        return res;<br>  }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>