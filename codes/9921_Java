            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class BitwiseEquations<br>{<br>  public long kthPlusOrSolution(int x, int k)<br>  {<br>    long ret = 0;<br>    int place = -1;<br>    for(int i = 0; k &gt; 0; i++)<br>    {<br>      if((x&amp;(1L&lt;&lt;i)) == 0)<br>      {<br>        place++;<br>        if((k&amp;1) != 0)<br>          ret += (1L&lt;&lt;i);<br>        k &gt;&gt;= 1;<br>      }<br>    }<br>    return ret;<br>  }<br>  <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>