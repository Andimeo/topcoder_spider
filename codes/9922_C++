            #include &lt;algorithm&gt;<br>#include &lt;string&gt;<br>#include &lt;set&gt;<br>#include &lt;map&gt;<br>#include &lt;vector&gt;<br>#include &lt;queue&gt; <br>#include &lt;iostream&gt;<br>#include &lt;iterator&gt;<br>#include &lt;math.h&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cstdlib&gt;<br>#include &lt;sstream&gt;<br>#include &lt;bitset&gt;<br><br>#pragma comment(linker, "/STACK:60777216")<br><br>using namespace std;<br><br>#define F(i,a,b) for (int _n(b), i(a); i &lt; _n; i++) <br>#define R(i,n) F(i,0,n) <br>#define UN(v) SORT(v),v.erase(unique(v.begin(),v.end()),v.end())<br>#define SORT(c) sort((c).begin(), (c).end())<br>#define CL(a,b) memset(a,b,sizeof(a))<br>#define INF 1000000000<br>#define pb push_back<br>#define pii pair&lt;int,int&gt;<br>#define ll long long<br>#define ld long double<br><br><br>int p,q,x,y;<br>ll a[1111111];<br><br>class InfiniteSequence2 {<br>public:<br>    ll go(ll n){        <br>        if(n&lt;=0) return 1;<br>        if(n&lt;=1000000 &amp;&amp; a[n]!=-1) return a[n];<br>        ll v=go(n/p-x)+go(n/q-y);<br>        if(n&lt;=1000000) a[n]=v;<br>    return v;<br>    }<br>  ll calc(long long N, int _p, int _q, int _x, int _y) {<br>        CL(a,-1);<br>        p=_p,q=_q;x=_x;y=_y;<br>        return go(N);<br>  }<br>  <br><br>};<br><br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003]<br>// Powered by CodeProcessor<br></td>