            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class CyclicSequence<br>{<br>  public String minimalCycle(String s)<br>  {<br>    for(int i = 1; i &lt; s.length(); i++)  {<br>      if(s.length() % i == 0)  {<br>        StringBuilder sb = new StringBuilder();<br>        StringBuilder sc = new StringBuilder();<br>        for(int x = 0; x &lt; 50; x++)  {<br>          sb.append(s);<br>          sc.append(s.substring(0,i));<br>        }<br>        if(sb.indexOf(sc.toString()) != -1)<br>          return s.substring(0,i);<br>      }<br>    }<br>    return s;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>