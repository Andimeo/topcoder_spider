            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br> <br> <br>public class TournamentWinner<br>{<br>  double[][] g;<br>  public double[] winningProbabilities(int[] P)<br>  {<br>    g = new double[8][8];<br>    int c = 0;<br>    for(int i = 0; i &lt; g.length; i++)<br>      for(int j = i+1; j &lt; g.length; j++)  {<br>        g[i][j] = 1.0*P[c++]/100;<br>        g[j][i] = 1.0-g[i][j];<br>      }<br>    double[] p = new double[8];<br>    outer: for(int i = 0; i &lt; 16; i++)  {<br>      ArrayList&lt;Integer&gt; list = new ArrayList&lt;Integer&gt;();<br>      double x = 1;<br>      for(int j = 0; j &lt; 4; j++)  {<br>        if((i&amp;(1&lt;&lt;j)) != 0)  {<br>          list.add(2*j+1);<br>          x *= g[2*j+1][2*j];<br>          if(x==0)<br>            continue outer;<br>        }<br>        else  {<br>          list.add(2*j);<br>          x *= g[2*j][2*j+1];<br>          if(x==0)<br>            continue outer;<br>        }<br>      }<br>      for(int k = 0; k &lt; 4; k++)  {<br>        double xx = x;<br>        ArrayList&lt;Integer&gt; list2 = new ArrayList&lt;Integer&gt;();<br>        for(int j = 0; j &lt; 2; j++)  {<br>          if((k&amp;(1&lt;&lt;j)) != 0)  {<br>            list2.add(list.get(2*j+1));<br>            xx *= g[list.get(2*j+1)][list.get(2*j)];<br>          }<br>          else  {<br>            list2.add(list.get(2*j));<br>            xx *= g[list.get(2*j)][list.get(2*j+1)];<br>          }  <br>        }<br>        p[list2.get(0)] += xx * g[list2.get(0)][list2.get(1)];<br>        p[list2.get(1)] += xx * g[list2.get(1)][list2.get(0)];<br>      }<br>    }<br>    return p;<br>  }<br> <br> <br>}<br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>