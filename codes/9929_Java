            import java.util.*;<br>public class BankLoans  {<br>  public double expectedProfit(String[] loans, String[] riskClasses)  {<br>    double y = 0;<br>    double x = 0;<br>    for(String s: loans)  {<br>      StringTokenizer st = new StringTokenizer(s);<br>      int a = Integer.parseInt(st.nextToken());<br>      int b = Integer.parseInt(st.nextToken());<br>      x += a;<br>      String c = st.nextToken();<br>      double p = 0;<br>      for(String t: riskClasses)  {<br>        st = new StringTokenizer(t);<br>        if(st.nextToken().equals(c))<br>          p = Integer.parseInt(st.nextToken());<br>      }<br>      p = 100-p;<br>      p /= 100;<br>      y += p * (a+b);<br>    }<br>    return y-x;<br>  }<br>}<br></td>