            Imports System.Math<br>Imports System.Text<br>Imports System.Text.RegularExpressions<br>Imports System.IO<br>Imports System.Text.Encoding<br>Imports System.Collections<br><br> Class BankLoans<br><br>        Public Function expectedProfit(ByVal loans As String(), ByVal riskClasses As String()) As Double<br><br>            Dim tot As Double = 0<br><br>            Dim classHash As New Hashtable<br>            For Each rc As String In riskClasses<br>                classHash.Add(Split(rc)(0), Split(rc)(1))<br>            Next<br><br>            For Each l As String In loans<br><br>                Dim s() As String = Split(l)<br><br>                Dim wontpay As Double = 0 - (classHash(s(2)) / 100 * CType(s(0), Double))<br><br>                Dim willpay As Double = (1 - (classHash(s(2)) / 100)) * CType(s(1), Double)<br><br>                tot += willpay<br>                tot += wontpay<br><br>            Next<br><br>            Return tot<br><br>        End Function<br><br>    End Class<br></td>