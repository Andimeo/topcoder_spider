            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br><br>public class ShipLoading {<br>    public int minimumTime(int[] cranes, string[] boxes) {<br><br>        string sb = "";<br>        for (int i = 0; i &lt; boxes.Length; i++) {<br>            sb += boxes[i];<br>        }<br>        string[] sb2 = sb.Split(' ');<br>        int[] B = new int[sb2.Length];<br>        for (int i = 0; i &lt; sb2.Length; i++) {<br>            B[i] = int.Parse(sb2[i]);<br>        }<br><br>        Array.Sort(B);<br>        Array.Reverse(B);<br><br>        Array.Sort(cranes);<br>        Array.Reverse(cranes);<br>        if (cranes[0] &lt; B[0]) return -1;<br>        int ret = 0;<br>        for (int i = 0; i &lt; B.Length; i++) {<br>            int cc = 0;<br>            for (int j = 0; j &lt; cranes.Length; j++) {<br>                if (cranes[j] &gt;= B[i]) cc++;<br>            }<br>            ret = Math.Max(ret, i / cc + 1);<br>        }<br>        return ret;<br><br>    }<br>}<br></td>