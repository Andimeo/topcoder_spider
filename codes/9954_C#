            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Text;<br><br>public class ThreePhotos<br>{<br>    public int removeCubes(string[] A, string[] B, string[] C)<br>    {<br>        int n=A.Length;<br>        int[,,] cube = new int[n,n,n];<br>        for(int a=0;a&lt;n;a++)<br>            for(int b=0;b&lt;n;b++)<br>            {<br>                if (A[a][b] == 'N')<br>                    for (int c = 0; c &lt; n; c++)<br>                        cube[a, b, c] = -1;<br>                if (B[a][b] == 'N')<br>                    for (int c = 0; c &lt; n; c++)<br>                        cube[a, c, b] = -1;<br>                if (C[a][b] == 'N')<br>                    for (int c = 0; c &lt; n; c++)<br>                        cube[c, a, b] = -1;<br>            }<br>        for(int a=0;a&lt;n;a++)<br>            for (int b = 0; b &lt; n; b++)<br>            {<br>                if (A[a][b] == 'Y')<br>                {<br>                    bool f = false;<br>                    for (int c = 0; c &lt; n; c++)<br>                        if (cube[a, b, c] &gt;= 0)<br>                            f = true;<br>                    if (!f)<br>                        return -1;<br>                }<br>                if (B[a][b] == 'Y')<br>                {<br>                    bool f = false;<br>                    for (int c = 0; c &lt; n; c++)<br>                        if (cube[a, c, b] &gt;= 0)<br>                            f = true;<br>                    if (!f)<br>                        return -1;<br>                }<br>                if (C[a][b] == 'Y')<br>                {<br>                    bool f = false;<br>                    for (int c = 0; c &lt; n; c++)<br>                        if (cube[c, a, b] &gt;= 0)<br>                            f = true;<br>                    if (!f)<br>                        return -1;<br>                }<br>            }<br>        int res = 0;<br>        for (int a = 0; a &lt; n; a++)<br>            for (int b = 0; b &lt; n; b++)<br>                for (int c = 0; c &lt; n; c++)<br>                    if (cube[a, b, c] &lt; 0)<br>                        res++;<br>        return res;<br>    }<br>}<br></td>