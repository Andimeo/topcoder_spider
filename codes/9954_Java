            /*<br>*/<br>import java.util.*;<br>public class ThreePhotos {<br>  public int removeCubes(String[] A, String[] B, String[] C) {<br>    int n = A.length;<br>    int[][][] c = new int[n][n][n];<br>    for (int i=0;i&lt;n;i++)<br>      for (int j=0;j&lt;n;j++) {<br>        if (A[i].charAt(j)=='N')<br>          for (int k=0;k&lt;n;k++)<br>            c[i][j][k]=1;<br>      }<br>    for (int i=0;i&lt;n;i++)<br>      for (int j=0;j&lt;n;j++) {<br>        if (B[i].charAt(j)=='N')<br>          for (int k=0;k&lt;n;k++)<br>            c[i][k][j]=1;<br>      }<br>    for (int i=0;i&lt;n;i++)<br>      for (int j=0;j&lt;n;j++) {<br>        if (C[i].charAt(j)=='N')<br>          for (int k=0;k&lt;n;k++)<br>            c[k][i][j]=1;<br>      }<br>    for (int i=0;i&lt;n;i++)<br>      for (int j=0;j&lt;n;j++) {<br>        if (A[i].charAt(j)=='Y') {<br>          boolean fl = false;<br>          for (int k=0;k&lt;n;k++)<br>            if (c[i][j][k]==0){<br>              fl=true;<br>              break;<br>            }<br>          if (!fl) return -1;<br>        }<br>      }<br>    for (int i=0;i&lt;n;i++)<br>      for (int j=0;j&lt;n;j++) {<br>        if (B[i].charAt(j)=='Y') {<br>          boolean fl = false;<br>          for (int k=0;k&lt;n;k++)<br>            if (c[i][k][j]==0){<br>              fl=true;<br>              break;<br>            }<br>          if (!fl) return -1;<br>        }<br>      }<br>    for (int i=0;i&lt;n;i++)<br>      for (int j=0;j&lt;n;j++) {<br>        if (C[i].charAt(j)=='Y') {<br>          boolean fl = false;<br>          for (int k=0;k&lt;n;k++)<br>            if (c[k][i][j]==0){<br>              fl=true;<br>              break;<br>            }<br>          if (!fl) return -1;<br>        }<br>      }<br>    int f = 0;<br>    for (int i=0;i&lt;n;i++)<br>      for (int j=0;j&lt;n;j++)<br>        for (int k=0;k&lt;n;k++)<br>          if (c[i][j][k]==1) f++;<br>    return f;<br>  }<br>  public static void main(String[] args) {<br>    //ThreePhotos temp = new ThreePhotos();<br>    //System.out.println(temp.removeCubes(String[] A, String[] B, String[] C));<br>  }<br>}<br><br><br>// Powered by FileEdit<br></td>