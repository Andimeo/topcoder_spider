            // WARNING: Following code was written by michalbb ;-)<br><br>//*** PREWRITTEN CODE ***//<br><br>// Header files<br><br>#include &lt;algorithm&gt;<br>#include &lt;cstdio&gt;<br>#include &lt;cmath&gt;<br>#include &lt;complex&gt;<br>#include &lt;numeric&gt;<br>#include &lt;iostream&gt;<br>#include &lt;vector&gt;<br>#include &lt;string&gt;<br>#include &lt;sstream&gt;<br>#include &lt;queue&gt;<br>#include &lt;map&gt;<br>#include &lt;set&gt;<br><br>using namespace std;<br><br>// Some defines<br><br>#define FOR(q,n) for(int q=0;q&lt;n;q++)<br><br>#define ESZ(elem) (int)elem.size()<br><br>//*** SOLUTION ***//<br> <br>class VeryInterestingMovie<br>{<br>public:<br>  int maximumPupils(vector&lt;string&gt; seats)<br>  {<br>    int res = 0;<br>    FOR(i, ESZ(seats))<br>    FOR(j, ESZ(seats[0]))if(seats[i][j] == 'Y'){res++; j++;}<br>    return res;<br>  }<br>};<br></td>