            public class MagicStones  {<br>  public int maximumDays(int N)  {<br>    int p = 1;<br>    for(int a = 2; a &lt;= N; a++)  {<br>      if(a &gt; p)<br>        p = a;<br>      for(int b = a+1; a+b &lt;= N; b++)  {<br>        if(g(b,a) != 1)<br>          continue;<br>        if(a*b &gt; p)<br>          p = a*b;<br>        for(int c = b+1; a+b+c &lt;= N; c++)  {<br>          if(g(c,a)!=1||g(c,b)!=1)<br>            continue;<br>          if(a*b*c &gt; p)<br>            p = a*b*c;<br>          for(int d = c+1; a+b+c+d &lt;= N; d++)  {<br>            if(g(d,a)!=1||g(d,b)!=1||g(d,c)!=1)<br>              continue;<br>            if(a*b*c*d &gt; p)<br>              p = a*b*c*d;<br>            for(int e = d+1; a+b+c+d+e &lt;= N; e++)  {<br>              if(g(e,a)!=1||g(e,b)!=1||g(e,c)!=1||g(e,d)!=1)<br>                continue;<br>              if(a*b*c*d*e &gt; p)<br>                p = a*b*c*d*e;<br>              for(int f = e+1; a+b+c+d+e+f &lt;= N; f++)  {<br>                if(g(f,a)!=1||g(f,b)!=1||g(f,c)!=1||g(f,d)!=1||g(f,e)!=1)<br>                  continue;<br>                if(a*b*c*d*e*f &gt; p)<br>                  p = a*b*c*d*e*f;<br>              <br>              }<br>            }<br>          }<br>        }<br>      }<br>    }<br>    return p;<br>  }<br>  public int g(int a, int b)  {<br>    if(a%b==0)<br>      return b;<br>    return g(b,a%b);<br>  }<br>}<br></td>