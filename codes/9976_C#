            using System;<br><br>public class TheTower {<br>  public int[] count(int[] x, int[] y) {<br>        int[] res = new int[x.Length];<br>        for (int i = 0; i &lt; x.Length; ++i)<br>            res[i] = int.MaxValue;<br>        foreach (int ax in x)<br>            foreach (int ay in y)<br>            {<br>                int[] dist = new int[x.Length];<br>                for (int i = 0; i &lt; x.Length; ++i)<br>                    dist[i] = Math.Abs(x[i] - ax) + Math.Abs(y[i] - ay);<br>                Array.Sort(dist);<br>                int sum = 0;<br>                for (int i = 0; i &lt; x.Length; ++i)<br>                {<br>                    sum += dist[i];<br>                    res[i] = Math.Min(res[i], sum);<br>                }<br>            }<br>        return res;<br>    }<br><br><br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by TZTester 1.01 [25-Feb-2003] [modified for C# by Petr]<br>// Powered by CodeProcessor<br></td>