            import java.util.*;<br>import java.math.*;<br>import java.io.*;<br>import java.text.*;<br>import java.util.regex.*;<br>import static java.util.Arrays.*;<br>import static java.lang.Math.*;<br>import static java.lang.Integer.parseInt;<br>public class TheTower {<br>  public int[] count(int[] x, int[] y) {<br>    int[] ret = new int[x.length];<br>    fill(ret, Integer.MAX_VALUE);<br>    for(int i = 0; i &lt; x.length; i++){<br>      for(int k = 0; k &lt; x.length; k++){<br>        int[] ds = new int[x.length];<br>        for(int j = 0; j &lt; x.length; j++){<br>          ds[j] = abs(x[i]-x[j])+abs(y[k]-y[j]);<br>        }<br>        sort(ds);<br>        int tot = 0;<br>        for(int j = 0; j &lt; ds.length; j++){<br>          tot += ds[j];<br>          ret[j] = min(ret[j], tot);<br>        }        <br>      }<br>    }<br>    return ret;<br>  }<br> <br>}<br> <br>// Powered by PopsEdit<br>// Powered by CodeProcessor<br></td>