            using System;<br>using System.Collections;<br>using System.Collections.Generic;<br>using System.Collections.Specialized;<br>using System.Text;<br>using System.Text.RegularExpressions;<br> <br>public class CollectingUsualPostmarks<br>{<br>  public int numberOfPostmarks(int[] prices, int[] have)<br>  {<br>    int res=0;<br>    int ret=0;<br>    for(int i=0;i&lt;have.Length;i++)<br>      res+=prices[have[i]];<br>    Array.Sort(prices);<br>    for(int i=0;i&lt;prices.Length;i++)<br>    {<br>      if(res &gt;= prices[i])<br>      {<br>        res-=prices[i];<br>        ret++;<br>      }<br>      else break;<br>    }<br>    return ret;<br>  }<br> <br> <br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>