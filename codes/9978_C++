            #include&lt;vector&gt;<br>#include&lt;list&gt;<br>#include&lt;map&gt;<br>#include&lt;set&gt;<br>#include&lt;queue&gt;<br>#include&lt;algorithm&gt;<br>#include&lt;sstream&gt;<br>#include&lt;iostream&gt;<br>#include&lt;cstdio&gt;<br>#include&lt;cmath&gt;<br>#include&lt;cstdlib&gt;<br>using namespace std;<br>#define inf 1e8<br>#define f(i,a,b)for(int i=a;i&lt;b;i++)<br>#define rep(i,n)f(i,0,n)<br>#define all(a)a.begin(),a.end()<br> <br> <br>class CollectingUsualPostmarks {<br> public:<br>  int numberOfPostmarks(vector &lt;int&gt;, vector &lt;int&gt;);<br>};<br> <br> <br> <br>int CollectingUsualPostmarks::numberOfPostmarks(vector &lt;int&gt; prices, vector &lt;int&gt; have){<br> <br>  int money=0;<br> <br>  int N=have.size(),K=prices.size();<br>  rep(i,N){<br>    int m=have[i];<br>    money+=prices[m];<br>  }<br> <br>  sort(all(prices));<br> <br>  int res=0;<br>  while(money&gt;0){<br>    money-=prices[res];<br>    res++;<br>  }<br> <br>  if(money&lt;0) return res-1;<br>  return res;<br>}<br>    <br> <br>//Powered by KawigiEdit 2.1.4 (beta) modified by pivanof!<br></td>