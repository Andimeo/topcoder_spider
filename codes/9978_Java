            import java.util.*;<br>public class CollectingUsualPostmarks {<br>    public int numberOfPostmarks(int[] prices, int[] have) {<br>        int res = 0;<br>        int money = 0;<br>        for(int i : have) money += prices[i];<br>        Arrays.sort(prices);<br>        while(true) {<br>          if(res &gt;= prices.length || prices[res] &gt; money) break;<br>          money -= prices[res];<br>          res++;<br>        }<br>        return res;<br>    }<br><br>}<br><br><br>// Powered by FileEdit<br>// Powered by CodeProcessor<br></td>