            import java.util.*;<br><br>public class LocateTreasure {<br>  public String location (int K, int M) {<br>    int dx = 0, dy = 1;<br>    int V = 1, st = 0;<br>    long x = 0, y = 0;<br>    for (int i = 0; i &lt; Math.min (K, 24); i++) {<br>      int RV = (V == 0) ? 9 : V;<br>      x += dx * RV;<br>      y += dy * RV;<br>      int ndx = dy;<br>      int ndy = -dx;<br>      dx = ndx; dy = ndy;<br>      V = (V * M) % 9;<br>    };<br><br>    K -= 24;<br><br>    if (K &gt; 0) {<br>      long xp = 0, yp = 0;<br>      for (int i = 0; i &lt; 24; i++) {<br>        int RV = (V == 0) ? 9 : V;<br>        xp += dx * RV;<br>        yp += dy * RV;<br>        int ndx = dy;<br>        int ndy = -dx;<br>        dx = ndx; dy = ndy;<br>        V = (V * M) % 9;<br>      };<br><br>      int KS = K / 24;<br>      x += xp * KS;<br>      y += yp * KS;<br><br>      K -= KS * 24;<br><br>      for (int i = 0; i &lt; K; i++) {<br>        int RV = (V == 0) ? 9 : V;<br>        x += dx * RV;<br>        y += dy * RV;<br>        int ndx = dy;<br>        int ndy = -dx;<br>        dx = ndx; dy = ndy;<br>        V = (V * M) % 9;<br>      };<br>    };<br><br>    return x + " " + y;<br>  };<br>};<br></td>