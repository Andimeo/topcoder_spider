            #include&lt;vector&gt;<br>#include&lt;set&gt;<br>#include&lt;string&gt;<br>#include&lt;iostream&gt;<br>#include&lt;cmath&gt;<br>#include&lt;algorithm&gt;<br>#include&lt;sstream&gt;<br>using namespace std;<br><br>string a[100], b[100], ans;<br>int n, m, tot;<br>class BestJob{<br>    public:<br>    void update(string cur)<br>    {<br>         if (ans == "" || cur &lt; ans) ans = cur;<br>     }<br>    bool search(string cur, int cp)<br>    {<br>         int i, j, k;<br>         if (cp &gt; m)<br>         {<br>            for (i = 0; i &lt; n; i++)<br>                if (b[i] == cur)<br>                   return false;<br>            for (i = 0; i &lt; n; i++)<br>                if (a[i] == cur)<br>                   return false;       <br>            ans = cur; return true;<br>         }<br>         if (search(cur + "N", cp + 1)) return true;<br>         if (search(cur + "Y", cp + 1)) return true;<br>         return false;<br>    }<br>    string didIMadeMistake(int k1, int k2, vector &lt;string&gt; aa)<br>    {<br>           int i, j, k, l, s1, s2;<br>           string cur = "";<br>           n = aa.size(); m = aa[0].size();<br>           for (i = 0; i &lt; n; i++) a[i] = aa[i];<br>           for (i = 0; i &lt; n; i++)<br>           {<br>               for (j = 0; j &lt; m; j++)<br>                 if (a[i][j] == 'Y')<br>                            b[i] = b[i] + "N";<br>                         else b[i] = b[i] + "Y";<br>           }<br>           if (k1 + k2 == 0)<br>           {<br>                 search(cur, 1);<br>                 return ans;<br>           }              <br>           if (k1 &gt; 0)<br>           {<br>              for (i = 0; i &lt; n; i++)<br>              {<br>                  s1 = 0; s2 = 0;<br>                  for (j = 0; j &lt; n; j++)<br>                  {<br>                      if (a[i] == a[j]) s1++;<br>                      if (b[i] == a[j]) s2++;<br>                  }<br>                  if (s1 == k1 &amp;&amp; s2 == k2) update(a[i]);<br>              }   <br>              return ans;<br>           }<br>           if (k2 &gt; 0)<br>           {<br>              for (i = 0; i &lt; n; i++)<br>              {<br>                  s1 = 0; s2 = 0;<br>                  for (j = 0; j &lt; n; j++)<br>                  {<br>                      if (b[i] == a[j]) s1++;<br>                      if (a[i] == a[j]) s2++;<br>                  }<br>                  if (s1 == k1 &amp;&amp; s2 == k2) update(b[i]);<br>              }   <br>              return ans;<br>           }<br>           <br>    }<br>};<br></td>