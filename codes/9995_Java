            import java.util.*;<br>import java.util.regex.*;<br>import java.text.*;<br>import java.math.*;<br>import java.awt.geom.*;<br><br>public class Polygons2<br>{<br>  public long number(int[] seg, int K)<br>  {<br>    int n = seg.length;<br>    <br>    if (n &lt; K) return 0;<br>    int max = 50001;<br>    <br>    Arrays.sort(seg);<br>    <br>    long[][] kol = new long[K][max + 1];<br>    kol[0][0] = 1;<br>    long count = 0;<br>    for (int s = 0; s &lt; n - 1; s++) {<br>      for (int i = K - 2; i &gt;= 0; i--)<br>        for (int j = 0; j &lt;= max; j++)<br>          if (j + seg[s] &lt; max) kol[i + 1][seg[s] + j] += kol[i][j];<br>          else kol[i + 1][max] += kol[i][j];<br>      <br>      for (int i = seg[s + 1] + 1; i &lt;= max; i++) count += kol[K - 1][i];<br>    }<br>    <br>    return count;<br>  }<br>  <br><br>}<br>//Powered by [KawigiEdit] 2.0!<br></td>